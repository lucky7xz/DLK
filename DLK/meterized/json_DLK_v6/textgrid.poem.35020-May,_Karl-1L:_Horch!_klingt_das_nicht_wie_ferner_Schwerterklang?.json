{"textgrid.poem.35020": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch! klingt das nicht wie ferner Schwerterklang?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch! klingt das nicht wie ferner Schwerterklang?", "tokens": ["Horch", "!", "klingt", "das", "nicht", "wie", "fer\u00b7ner", "Schwer\u00b7ter\u00b7klang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Marsch bebt unter dampfenden Schwadronen", "tokens": ["Die", "Marsch", "bebt", "un\u00b7ter", "damp\u00b7fen\u00b7den", "Schwad\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es jagt der Tod den weiten Plan entlang", "tokens": ["Es", "jagt", "der", "Tod", "den", "wei\u00b7ten", "Plan", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPO"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und erntet unter br\u00fcllenden Kanonen.", "tokens": ["Und", "ern\u00b7tet", "un\u00b7ter", "br\u00fcl\u00b7len\u00b7den", "Ka\u00b7no\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Will deutsches Recht in deutschen Landen beugen;", "tokens": ["Will", "deut\u00b7sches", "Recht", "in", "deut\u00b7schen", "Lan\u00b7den", "beu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum ziehen alle kampfbegierig hin,", "tokens": ["Drum", "zie\u00b7hen", "al\u00b7le", "kampf\u00b7be\u00b7gie\u00b7rig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm deutsche Kraft und deutschen Muth zu zeigen.", "tokens": ["Ihm", "deut\u00b7sche", "Kraft", "und", "deut\u00b7schen", "Muth", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun gilt's ein Ringen um den h\u00f6chsten Preis,", "tokens": ["Nun", "gilt's", "ein", "Rin\u00b7gen", "um", "den", "h\u00f6chs\u00b7ten", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein hei\u00dfes Wogen und ein hei\u00dfes Wagen.", "tokens": ["Ein", "hei\u00b7\u00dfes", "Wo\u00b7gen", "und", "ein", "hei\u00b7\u00dfes", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl schwitzt gar manch ein Herz purpurnen Schwei\u00df", "tokens": ["Wohl", "schwitzt", "gar", "manch", "ein", "Herz", "pur\u00b7pur\u00b7nen", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schl\u00e4gt nur, um zum letzten Mal zu schlagen.", "tokens": ["Und", "schl\u00e4gt", "nur", ",", "um", "zum", "letz\u00b7ten", "Mal", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUI", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch mitten unter Leichen bl\u00fcht der Sieg;", "tokens": ["Doch", "mit\u00b7ten", "un\u00b7ter", "Lei\u00b7chen", "bl\u00fcht", "der", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht darf der Lorbeer Thr\u00e4nen uns erpressen:", "tokens": ["Nicht", "darf", "der", "Lor\u00b7beer", "Thr\u00e4\u00b7nen", "uns", "er\u00b7pres\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer feindlich Bollwerk sterbend noch erstieg,", "tokens": ["Wer", "feind\u00b7lich", "Boll\u00b7werk", "ster\u00b7bend", "noch", "er\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der lebt und w\u00e4r' er tausend Mal vergessen.", "tokens": ["Der", "lebt", "und", "w\u00e4r'", "er", "tau\u00b7send", "Mal", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VAFIN", "PPER", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn die Geschichte schreibt mit gold'nem Stift", "tokens": ["Denn", "die", "Ge\u00b7schich\u00b7te", "schreibt", "mit", "gold'\u00b7nem", "Stift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mi\u00dft Triumphe nicht nach kurzen Jahren.", "tokens": ["Und", "mi\u00dft", "Tri\u00b7um\u00b7phe", "nicht", "nach", "kur\u00b7zen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum gl\u00e4nzt es fort in heller Flammenschrift:", "tokens": ["Drum", "gl\u00e4nzt", "es", "fort", "in", "hel\u00b7ler", "Flam\u00b7men\u00b7schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbder L\u00f6we Sachsens ist's mit seinen Schaaren!\u00ab", "tokens": ["\u00bb", "der", "L\u00f6\u00b7we", "Sach\u00b7sens", "ist's", "mit", "sei\u00b7nen", "Schaa\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NE", "NE", "NE", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Horch! klingt das nicht wie ferner Donnerschall?", "tokens": ["Horch", "!", "klingt", "das", "nicht", "wie", "fer\u00b7ner", "Don\u00b7ner\u00b7schall", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es blitzt wie Wetterleuchten um die H\u00f6hen,", "tokens": ["Es", "blitzt", "wie", "Wet\u00b7ter\u00b7leuch\u00b7ten", "um", "die", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unten fluthet es wie Wogenschwall,", "tokens": ["Und", "un\u00b7ten", "flut\u00b7het", "es", "wie", "Wo\u00b7gen\u00b7schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie wirbelndes Gew\u00f6lk bei Sturmeswehen.", "tokens": ["Wie", "wir\u00b7beln\u00b7des", "Ge\u00b7w\u00f6lk", "bei", "Stur\u00b7mes\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein Einziger steht ohne Furcht und Grauen;", "tokens": ["Ein", "Ein\u00b7zi\u00b7ger", "steht", "oh\u00b7ne", "Furcht", "und", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seinen H\u00e4nden treu die Waffe ruht,", "tokens": ["In", "sei\u00b7nen", "H\u00e4n\u00b7den", "treu", "die", "Waf\u00b7fe", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tausend M\u00e4nnerherzen ihm vertrauen.", "tokens": ["Und", "tau\u00b7send", "M\u00e4n\u00b7ner\u00b7her\u00b7zen", "ihm", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbla\u00dft laufen, Kinder, was nur laufen kann!", "tokens": ["\u00bb", "la\u00dft", "lau\u00b7fen", ",", "Kin\u00b7der", ",", "was", "nur", "lau\u00b7fen", "kann", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "VVINF", "$,", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir wollen ihnen freie Bahn erringen.", "tokens": ["Wir", "wol\u00b7len", "ih\u00b7nen", "frei\u00b7e", "Bahn", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu k\u00e4mpfen, nicht zu laufen ziemt dem Mann,", "tokens": ["Zu", "k\u00e4mp\u00b7fen", ",", "nicht", "zu", "lau\u00b7fen", "ziemt", "dem", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will er sich Ehren um die Schl\u00e4fe schlingen.\u00ab", "tokens": ["Will", "er", "sich", "Eh\u00b7ren", "um", "die", "Schl\u00e4\u00b7fe", "schlin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PRF", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Hell klingt das Wort, hell leuchtet auch der Stahl,", "tokens": ["Hell", "klingt", "das", "Wort", ",", "hell", "leuch\u00b7tet", "auch", "der", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um blitzend in des Feindes Brust zu fahren.", "tokens": ["Um", "blit\u00b7zend", "in", "des", "Fein\u00b7des", "Brust", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer naht, den trifft des Todes bleicher Strahl:", "tokens": ["Wer", "naht", ",", "den", "trifft", "des", "To\u00b7des", "blei\u00b7cher", "Strahl", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ART", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der L\u00f6we Sachsens ist's mit seinen Schaaren!", "tokens": ["Der", "L\u00f6\u00b7we", "Sach\u00b7sens", "ist's", "mit", "sei\u00b7nen", "Schaa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Horch! klingt das nicht wie ferner Trommelschlag?", "tokens": ["Horch", "!", "klingt", "das", "nicht", "wie", "fer\u00b7ner", "Trom\u00b7mel\u00b7schlag", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon will der Abend blutig roth sich neigen;", "tokens": ["Schon", "will", "der", "A\u00b7bend", "blu\u00b7tig", "roth", "sich", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Pulverdampf h\u00fcllt scheidend sich der Tag,", "tokens": ["In", "Pul\u00b7ver\u00b7dampf", "h\u00fcllt", "schei\u00b7dend", "sich", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um trauernd in das Thal hinab zu steigen.", "tokens": ["Um", "trau\u00b7ernd", "in", "das", "Thal", "hin\u00b7ab", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ist Mancher in's zerstampfte Feld gesunken", "tokens": ["Ist", "Man\u00b7cher", "in's", "zer\u00b7stampf\u00b7te", "Feld", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hat, ermattet von des Kampfes M\u00fch',", "tokens": ["Und", "hat", ",", "er\u00b7mat\u00b7tet", "von", "des", "Kamp\u00b7fes", "M\u00fch'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus schwarzem Schlunde ew'gen Schlaf getrunken.", "tokens": ["Aus", "schwar\u00b7zem", "Schlun\u00b7de", "ew'\u00b7gen", "Schlaf", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Hell gl\u00e4nzen rings die Feuer der Bivouaks;", "tokens": ["Hell", "gl\u00e4n\u00b7zen", "rings", "die", "Feu\u00b7er", "der", "Bi\u00b7vou\u00b7aks", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da zieh'n mit festem, mannessich'rem Schritte", "tokens": ["Da", "zieh'n", "mit", "fes\u00b7tem", ",", "man\u00b7nes\u00b7sich'\u00b7rem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchntrotzige Gestalten durch die Nacht,", "tokens": ["K\u00fchn\u00b7trot\u00b7zi\u00b7ge", "Ge\u00b7stal\u00b7ten", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den sieggewohnten F\u00fchrer in der Mitte.", "tokens": ["Den", "sieg\u00b7ge\u00b7wohn\u00b7ten", "F\u00fch\u00b7rer", "in", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "\u00bbwer sind die Helden, die mit Eisenarm", "tokens"