{"textgrid.poem.34845": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist in die Nacht verliebt,", "tokens": ["Der", "Tag", "ist", "in", "die", "Nacht", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling in den Winter,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "in", "den", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben verliebt in den Tod \u2013", "tokens": ["Das", "Le\u00b7ben", "ver\u00b7liebt", "in", "den", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und du, du liebest mich!", "tokens": ["Und", "du", ",", "du", "lie\u00b7best", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du liebst mich \u2013 schon erfassen dich", "tokens": ["Du", "liebst", "mich", "\u2013", "schon", "er\u00b7fas\u00b7sen", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die grauenhaften Schatten,", "tokens": ["Die", "grau\u00b7en\u00b7haf\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All deine Bl\u00fcte welkt,", "tokens": ["All", "dei\u00b7ne", "Bl\u00fc\u00b7te", "welkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und deine Seele verblutet.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "ver\u00b7blu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "La\u00df ab von mir, und liebe nur", "tokens": ["La\u00df", "ab", "von", "mir", ",", "und", "lie\u00b7be", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPER", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heiteren Schmetterlinge,", "tokens": ["Die", "hei\u00b7te\u00b7ren", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die da gaukeln im Sonnenlicht \u2013", "tokens": ["Die", "da", "gau\u00b7keln", "im", "Son\u00b7nen\u00b7licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "La\u00df ab von mir und dem Ungl\u00fcck.", "tokens": ["La\u00df", "ab", "von", "mir", "und", "dem", "Un\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}