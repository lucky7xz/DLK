{"textgrid.poem.39233": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will ein Lied Euch singen, ein Lied aus grauem Nord,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will ein Lied Euch singen, ein Lied aus grauem Nord,", "tokens": ["Ich", "will", "ein", "Lied", "Euch", "sin\u00b7gen", ",", "ein", "Lied", "aus", "grau\u00b7em", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von leuchtenden Schwerterklingen und k\u00fchnem Drachenmord,", "tokens": ["Von", "leuch\u00b7ten\u00b7den", "Schwer\u00b7ter\u00b7klin\u00b7gen", "und", "k\u00fch\u00b7nem", "Dra\u00b7chen\u00b7mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Lied, das hier am rechten Ort und pa\u00dft f\u00fcr sonst und jetzt,", "tokens": ["Ein", "Lied", ",", "das", "hier", "am", "rech\u00b7ten", "Ort", "und", "pa\u00dft", "f\u00fcr", "sonst", "und", "jetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Vielleicht, da\u00df Euch ein altes Wort in junge Flammen setzt.", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "Euch", "ein", "al\u00b7tes", "Wort", "in", "jun\u00b7ge", "Flam\u00b7men", "setzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Es war ein grimmer Drache, der Drache hie\u00df Fafnir,", "tokens": ["Es", "war", "ein", "grim\u00b7mer", "Dra\u00b7che", ",", "der", "Dra\u00b7che", "hie\u00df", "Faf\u00b7nir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf rotem Gold zur Wache, da lag das Schuppentier;", "tokens": ["Auf", "ro\u00b7tem", "Gold", "zur", "Wa\u00b7che", ",", "da", "lag", "das", "Schup\u00b7pen\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich sag' Euch nicht des Hortes Wert, noch wie er ihn empfing,", "tokens": ["Ich", "sag'", "Euch", "nicht", "des", "Hor\u00b7tes", "Wert", ",", "noch", "wie", "er", "ihn", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ich sag' Euch, wie ein Heldenschwert dem Wurm ans Leben ging.", "tokens": ["Ich", "sag'", "Euch", ",", "wie", "ein", "Hel\u00b7den\u00b7schwert", "dem", "Wurm", "ans", "Le\u00b7ben", "ging", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Herr Sigurd, der Volsunge, der k\u00fchnste Held mit Fug,", "tokens": ["Herr", "Si\u00b7gurd", ",", "der", "Vol\u00b7sun\u00b7ge", ",", "der", "k\u00fchns\u00b7te", "Held", "mit", "Fug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-++--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der je ein Schwert im Schwunge durch harte Helme schlug,", "tokens": ["Der", "je", "ein", "Schwert", "im", "Schwun\u00b7ge", "durch", "har\u00b7te", "Hel\u00b7me", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der hatt' ein Schwert durch G\u00f6ttergunst, ein Schwert von solcher Art,", "tokens": ["Der", "hatt'", "ein", "Schwert", "durch", "G\u00f6t\u00b7ter\u00b7gunst", ",", "ein", "Schwert", "von", "sol\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie nie durch Nordlands Schmiedekunst ein gleiches fertig ward.", "tokens": ["Wie", "nie", "durch", "Nord\u00b7lands", "Schmie\u00b7de\u00b7kunst", "ein", "glei\u00b7ches", "fer\u00b7tig", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN", "ART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Ein Schwert, ein langes, breites, ein Schwert von scharfem Schliff,", "tokens": ["Ein", "Schwert", ",", "ein", "lan\u00b7ges", ",", "brei\u00b7tes", ",", "ein", "Schwert", "von", "schar\u00b7fem", "Schliff", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Schwert, wie nie ein zweites durch zitternde L\u00fcfte pfiff,", "tokens": ["Ein", "Schwert", ",", "wie", "nie", "ein", "zwei\u00b7tes", "durch", "zit\u00b7tern\u00b7de", "L\u00fcf\u00b7te", "pfiff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie nie von Ritterlenden eh' ein Schwert zum Sporn geklirrt,", "tokens": ["Wie", "nie", "von", "Rit\u00b7ter\u00b7len\u00b7den", "eh'", "ein", "Schwert", "zum", "Sporn", "ge\u00b7klirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie nie in Ritterh\u00e4nden je ein Schwert im Zorn geschwirrt.", "tokens": ["Wie", "nie", "in", "Rit\u00b7ter\u00b7h\u00e4n\u00b7den", "je", "ein", "Schwert", "im", "Zorn", "ge\u00b7schwirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Hin zu des Drachen Hecke, da ritt mit Stahl bewehrt", "tokens": ["Hin", "zu", "des", "Dra\u00b7chen", "He\u00b7cke", ",", "da", "ritt", "mit", "Stahl", "be\u00b7wehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "APPR", "NN", "VVPP"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Manch kampfesdurst'ger Recke, der niemals heimgekehrt;", "tokens": ["Manch", "kamp\u00b7fes\u00b7dur\u00b7st'\u00b7ger", "Re\u00b7cke", ",", "der", "nie\u00b7mals", "heim\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da rief das junge Volsungsblut. \u00bbDen Lindwurm mu\u00df ich sehn,", "tokens": ["Da", "rief", "das", "jun\u00b7ge", "Vol\u00b7sungs\u00b7blut", ".", "\u00bb", "Den", "Lind\u00b7wurm", "mu\u00df", "ich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ein altes Schwert, ein junger Mut, das l\u00e4\u00dft nicht untergehn!\u00ab", "tokens": ["Ein", "al\u00b7tes", "Schwert", ",", "ein", "jun\u00b7ger", "Mut", ",", "das", "l\u00e4\u00dft", "nicht", "un\u00b7ter\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Er blieb nicht stehn von weitem, den Drachen sprang er an,", "tokens": ["Er", "blieb", "nicht", "stehn", "von", "wei\u00b7tem", ",", "den", "Dra\u00b7chen", "sprang", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVFIN", "APPR", "ADJA", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ha was ein grimmes Streiten da Held und Wurm begann!", "tokens": ["Ha", "was", "ein", "grim\u00b7mes", "Strei\u00b7ten", "da", "Held", "und", "Wurm", "be\u00b7gann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die Doppelzunge z\u00fcngelte, der Rachen hauchte hei\u00df,", "tokens": ["Die", "Dop\u00b7pel\u00b7zun\u00b7ge", "z\u00fcn\u00b7gel\u00b7te", ",", "der", "Ra\u00b7chen", "hauch\u00b7te", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der Schuppenschweif umringelte den Volsungssohn im Kreis.", "tokens": ["Der", "Schup\u00b7pen\u00b7schweif", "um\u00b7rin\u00b7gel\u00b7te", "den", "Vol\u00b7sungs\u00b7sohn", "im", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Es schritt der Held zu Streichen, die Klinge stieg und sank,", "tokens": ["Es", "schritt", "der", "Held", "zu", "Strei\u00b7chen", ",", "die", "Klin\u00b7ge", "stieg", "und", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Wurm begann zu keuchen, und Schupp' auf Schuppe sprang.", "tokens": ["Der", "Wurm", "be\u00b7gann", "zu", "keu\u00b7chen", ",", "und", "Schupp'", "auf", "Schup\u00b7pe", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es schlug der K\u00f6nig Mal auf Mal den ungef\u00fcgsten Hieb,", "tokens": ["Es", "schlug", "der", "K\u00f6\u00b7nig", "Mal", "auf", "Mal", "den", "un\u00b7ge\u00b7f\u00fcgs\u00b7ten", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der je ein Schwert aus gutem Stahl durch Panzerringe trieb.", "tokens": ["Der", "je", "ein", "Schwert", "aus", "gu\u00b7tem", "Stahl", "durch", "Pan\u00b7zer\u00b7rin\u00b7ge", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Was soll ich weiter sagen? Ihr wi\u00dft es, was geschah:", "tokens": ["Was", "soll", "ich", "wei\u00b7ter", "sa\u00b7gen", "?", "Ihr", "wi\u00dft", "es", ",", "was", "ge\u00b7schah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$.", "PPER", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Lindwurm lag erschlagen in zuckenden St\u00fccken da.", "tokens": ["Der", "Lind\u00b7wurm", "lag", "er\u00b7schla\u00b7gen", "in", "zu\u00b7cken\u00b7den", "St\u00fc\u00b7cken", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sich im Drachenblute rot der Held den Leib gest\u00e4hlt,", "tokens": ["Wie", "sich", "im", "Dra\u00b7chen\u00b7blu\u00b7te", "rot", "der", "Held", "den", "Leib", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Das hat die Nibelungennot Euch lange schon erz\u00e4hlt. \u2013", "tokens": ["Das", "hat", "die", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7not", "Euch", "lan\u00b7ge", "schon", "er\u00b7z\u00e4hlt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Ein neuer Drache h\u00fctet des Goldes edlen Schein,", "tokens": ["Ein", "neu\u00b7er", "Dra\u00b7che", "h\u00fc\u00b7tet", "des", "Gol\u00b7des", "ed\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein neuer Drache br\u00fctet und will bestritten sein,", "tokens": ["Ein", "neu\u00b7er", "Dra\u00b7che", "br\u00fc\u00b7tet", "und", "will", "be\u00b7strit\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das ist das Ellenkr\u00e4mertum, das jetzt die Welt regiert,", "tokens": ["Das", "ist", "das", "El\u00b7len\u00b7kr\u00e4\u00b7mer\u00b7tum", ",", "das", "jetzt", "die", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Was Poesie, was Lieb' und Ruhm! Es lacht und spekuliert.", "tokens": ["Was", "Poe\u00b7sie", ",", "was", "Lieb'", "und", "Ruhm", "!", "Es", "lacht", "und", "spe\u00b7ku\u00b7liert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWS", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Erfroren sind die Saaten, die V\u00f6lker schrein nach Brot,", "tokens": ["Er\u00b7fro\u00b7ren", "sind", "die", "Saa\u00b7ten", ",", "die", "V\u00f6l\u00b7ker", "schrein", "nach", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": 