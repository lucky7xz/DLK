{"textgrid.poem.62419": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrstenblut geflossen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstenblut geflossen", "tokens": ["F\u00fcrs\u00b7ten\u00b7blut", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In der L\u00fctznerschlacht \u2013", "tokens": ["In", "der", "L\u00fctz\u00b7ner\u00b7schlacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie so gern vergossen,", "tokens": ["Wie", "so", "gern", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Willig dargebracht.", "tokens": ["Wil\u00b7lig", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kattenblut, Hessenblut,", "tokens": ["Kat\u00b7ten\u00b7blut", ",", "Hes\u00b7sen\u00b7blut", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sch\u00f6nes deutsches Blut!", "tokens": ["Sch\u00f6\u00b7nes", "deut\u00b7sches", "Blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Es entrann dem K\u00fchnen", "tokens": ["Es", "ent\u00b7rann", "dem", "K\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "All' sein Lebensblut,", "tokens": ["All'", "sein", "Le\u00b7bens\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Freudig zu vers\u00fchnen", "tokens": ["Freu\u00b7dig", "zu", "ver\u00b7s\u00fch\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schlechten F\u00fcrstenmuth.", "tokens": ["Schlech\u00b7ten", "F\u00fcrs\u00b7ten\u00b7muth", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Rothes Blut, warmes Blut,", "tokens": ["Ro\u00b7thes", "Blut", ",", "war\u00b7mes", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sch\u00f6nes Opferblut.", "tokens": ["Sch\u00f6\u00b7nes", "Op\u00b7fer\u00b7blut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und es t\u00f6nt kein Wehe", "tokens": ["Und", "es", "t\u00f6nt", "kein", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In des Vaters Schlo\u00df \u2013", "tokens": ["In", "des", "Va\u00b7ters", "Schlo\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Homburg an der H\u00f6he", "tokens": ["Hom\u00b7burg", "an", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zeugt noch manchen Spro\u00df.", "tokens": ["Zeugt", "noch", "man\u00b7chen", "Spro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Reiches Blut, junges Blut,", "tokens": ["Rei\u00b7ches", "Blut", ",", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sch\u00f6nes Prinzenblut.", "tokens": ["Sch\u00f6\u00b7nes", "Prin\u00b7zen\u00b7blut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr des Landes Sache", "tokens": ["F\u00fcr", "des", "Lan\u00b7des", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Flo\u00df auch eures gern", "tokens": ["Flo\u00df", "auch", "eu\u00b7res", "gern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "An dem Tag der Rache,", "tokens": ["An", "dem", "Tag", "der", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Br\u00fcder jenes Herrn.", "tokens": ["Br\u00fc\u00b7der", "je\u00b7nes", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kattenblut, Hessenblut,", "tokens": ["Kat\u00b7ten\u00b7blut", ",", "Hes\u00b7sen\u00b7blut", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sch\u00f6nes Freiheitsblut.", "tokens": ["Sch\u00f6\u00b7nes", "Frei\u00b7heits\u00b7blut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wendet schnell die Rosse,", "tokens": ["Wen\u00b7det", "schnell", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Boten, heimatw\u00e4rts,", "tokens": ["Bo\u00b7ten", ",", "hei\u00b7mat\u00b7w\u00e4rts", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Auf dem K\u00f6nigsschlosse", "tokens": ["Auf", "dem", "K\u00f6\u00b7nigs\u00b7schlos\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zagt ein Schwesternherz.", "tokens": ["Zagt", "ein", "Schwes\u00b7tern\u00b7herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Stolzes Blut, mildes Blut,", "tokens": ["Stol\u00b7zes", "Blut", ",", "mil\u00b7des", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sch\u00f6nes Frauenblut.", "tokens": ["Sch\u00f6\u00b7nes", "Frau\u00b7en\u00b7blut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Du von Homburgs H\u00f6hen", "tokens": ["Du", "von", "Hom\u00b7burgs", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER