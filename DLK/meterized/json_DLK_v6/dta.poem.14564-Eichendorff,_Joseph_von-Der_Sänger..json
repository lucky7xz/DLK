{"dta.poem.14564": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst Du die W\u00e4lder gl\u00fchen,", "tokens": ["Siehst", "Du", "die", "W\u00e4l\u00b7der", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Str\u00f6me flammend spr\u00fchen,", "tokens": ["Die", "Str\u00f6\u00b7me", "flam\u00b7mend", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt in Abendgluten,", "tokens": ["Die", "Welt", "in", "A\u00b7bend\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie tr\u00e4umerische Fluten,", "tokens": ["Wie", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo bl\u00fch'nde Inseln trunken", "tokens": ["Wo", "bl\u00fch'n\u00b7de", "In\u00b7seln", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich spiegeln in dem Duft? \u2014", "tokens": ["Sich", "spie\u00b7geln", "in", "dem", "Duft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es weht und rauscht und ruft:", "tokens": ["Es", "weht", "und", "rauscht", "und", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O komm, eh' wir versunken!", "tokens": ["O", "komm", ",", "eh'", "wir", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}