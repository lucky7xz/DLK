{"textgrid.poem.24801": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod des Schauspielers Hermann M\u00fcller", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Haus und wir, wir dienen einer Kunst,", "tokens": ["Dies", "Haus", "und", "wir", ",", "wir", "die\u00b7nen", "ei\u00b7ner", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die jeden tiefen Schmerz erquicklich macht", "tokens": ["Die", "je\u00b7den", "tie\u00b7fen", "Schmerz", "er\u00b7quick\u00b7lich", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schmackhaft auch den Tod.", "tokens": ["Und", "schmack\u00b7haft", "auch", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und er, den wir uns vor die Seele rufen,", "tokens": ["Und", "er", ",", "den", "wir", "uns", "vor", "die", "See\u00b7le", "ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war so stark! Sein Leib war so begabt,", "tokens": ["Es", "war", "so", "stark", "!", "Sein", "Leib", "war", "so", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich zu verwandeln, da\u00df es schien, kein Netz", "tokens": ["Sich", "zu", "ver\u00b7wan\u00b7deln", ",", "da\u00df", "es", "schien", ",", "kein", "Netz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verm\u00f6chte ihn zu fangen! Welch ein Wesen!", "tokens": ["Ver\u00b7m\u00f6ch\u00b7te", "ihn", "zu", "fan\u00b7gen", "!", "Welch", "ein", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$.", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er machte sich durchsichtig, lie\u00df das Wei\u00dfe", "tokens": ["Er", "mach\u00b7te", "sich", "durch\u00b7sich\u00b7tig", ",", "lie\u00df", "das", "Wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von seinem Aug die tiefste Heimlichkeit,", "tokens": ["Von", "sei\u00b7nem", "Aug", "die", "tiefs\u00b7te", "Heim\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in ihm schlief, verraten, atmete", "tokens": ["Die", "in", "ihm", "schlief", ",", "ver\u00b7ra\u00b7ten", ",", "at\u00b7me\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Seele der erdichteten Gesch\u00f6pfe", "tokens": ["Die", "See\u00b7le", "der", "er\u00b7dich\u00b7te\u00b7ten", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Rauch in sich und trieb sie durch die Poren", "tokens": ["Wie", "Rauch", "in", "sich", "und", "trieb", "sie", "durch", "die", "Po\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "PRF", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von seinem Leib ans Tageslicht zur\u00fcck.", "tokens": ["Von", "sei\u00b7nem", "Leib", "ans", "Ta\u00b7ges\u00b7licht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er schuf sich um und um, da quollen Wesen", "tokens": ["Er", "schuf", "sich", "um", "und", "um", ",", "da", "quol\u00b7len", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "KON", "PTKVZ", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hervor, kaum menschlich, aber so lebendig \u2013", "tokens": ["Her\u00b7vor", ",", "kaum", "menschlich", ",", "a\u00b7ber", "so", "le\u00b7ben\u00b7dig", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Das Aug bejahte sie, ob nie zuvor", "tokens": ["Das", "Aug", "be\u00b7jah\u00b7te", "sie", ",", "ob", "nie", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dergleichen es geschaut: ein einzig Blinzeln,", "tokens": ["Derg\u00b7lei\u00b7chen", "es", "ge\u00b7schaut", ":", "ein", "ein\u00b7zig", "Blin\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVPP", "$.", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein Atemholen zeugte, da\u00df sie waren", "tokens": ["Ein", "A\u00b7tem\u00b7ho\u00b7len", "zeug\u00b7te", ",", "da\u00df", "sie", "wa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und noch vom Mutterleib der Erde dampften!", "tokens": ["Und", "noch", "vom", "Mut\u00b7ter\u00b7leib", "der", "Er\u00b7de", "dampf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und Menschen! Schlie\u00dft die Augen, denkt zur\u00fcck!", "tokens": ["Und", "Men\u00b7schen", "!", "Schlie\u00dft", "die", "Au\u00b7gen", ",", "denkt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bald \u00fcppige Leiber, drin nur noch im Winkel", "tokens": ["Bald", "\u00fcp\u00b7pi\u00b7ge", "Lei\u00b7ber", ",", "drin", "nur", "noch", "im", "Win\u00b7kel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Des Augs ein letztes F\u00fcnkchen Seele glost,", "tokens": ["Des", "Augs", "ein", "letz\u00b7tes", "F\u00fcnk\u00b7chen", "See\u00b7le", "glost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Bald Seelen, die um sich, nur sich zum Dienst", "tokens": ["Bald", "See\u00b7len", ",", "die", "um", "sich", ",", "nur", "sich", "zum", "Dienst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "PRELS", "APPR", "PRF", "$,", "ADV", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ein durchsichtig Geh\u00e4us, den Leib, erbauen:", "tokens": ["Ein", "durch\u00b7sich\u00b7tig", "Ge\u00b7h\u00e4us", ",", "den", "Leib", ",", "er\u00b7bau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "NN", "$,", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Gemeine Menschen, finstre Menschen, K\u00f6nige,", "tokens": ["Ge\u00b7mei\u00b7ne", "Men\u00b7schen", ",", "finst\u00b7re", "Men\u00b7schen", ",", "K\u00f6\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Menschen zum Lachen, Menschen zum Erschaudern \u2013", "tokens": ["Men\u00b7schen", "zum", "La\u00b7chen", ",", "Men\u00b7schen", "zum", "Er\u00b7schau\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Er schuf sich um und um: da standen sie.", "tokens": ["Er", "schuf", "sich", "um", "und", "um", ":", "da", "stan\u00b7den", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "KON", "KOUI", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wenn das Spiel verlosch und sich der Vorhang", "tokens": ["Doch", "wenn", "das", "Spiel", "ver\u00b7losch", "und", "sich", "der", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "KON", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lautlos wie ein geschminktes Augenlid", "tokens": ["Laut\u00b7los", "wie", "ein", "ge\u00b7schmink\u00b7tes", "Au\u00b7gen\u00b7lid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor die erstorbne Zauberh\u00f6hle legte", "tokens": ["Vor", "die", "ers\u00b7torb\u00b7ne", "Zau\u00b7ber\u00b7h\u00f6h\u00b7le", "leg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und er hinaustrat, da war eine B\u00fchne", "tokens": ["Und", "er", "hin\u00b7aust\u00b7rat", ",", "da", "war", "ei\u00b7ne", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So vor ihm aufgetan wie ein auf ewig", "tokens": ["So", "vor", "ihm", "auf\u00b7ge\u00b7tan", "wie", "ein", "auf", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVPP", "KOKOM", "ART", "APPR", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schlafloses aufgeri\u00dfnes Aug, daran", "tokens": ["Schlaf\u00b7lo\u00b7ses", "auf\u00b7ge\u00b7ri\u00df\u00b7nes", "Aug", ",", "da\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein Vorhang je mitleidig niedersinkt:", "tokens": ["Kein", "Vor\u00b7hang", "je", "mit\u00b7lei\u00b7dig", "nie\u00b7der\u00b7sinkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die f\u00fcrchterliche B\u00fchne Wirklichkeit.", "tokens": ["Die", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "B\u00fch\u00b7ne", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da fielen der Verwandlung K\u00fcnste alle", "tokens": ["Da", "fie\u00b7len", "der", "Ver\u00b7wand\u00b7lung", "K\u00fcns\u00b7te", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von ihm, und seine arme Seele ging", "tokens": ["Von", "ihm", ",", "und", "sei\u00b7ne", "ar\u00b7me", "See\u00b7le", "ging"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ganz h\u00fcllenlos und sah aus Kindesaugen.", "tokens": ["Ganz", "h\u00fcl\u00b7len\u00b7los", "und", "sah", "aus", "Kin\u00b7des\u00b7au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da war er in ein unerbittlich Spiel", "tokens": ["Da", "war", "er", "in", "ein", "un\u00b7er\u00b7bitt\u00b7lich", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Verstrickt, unwissend, wie ihm dies geschah;", "tokens": ["Ver\u00b7strickt", ",", "un\u00b7wis\u00b7send", ",", "wie", "ihm", "dies", "ge\u00b7schah", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "PWAV", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein jeder Schritt ein tiefrer als der fr\u00fchere", "tokens": ["Ein", "je\u00b7der", "Schritt", "ein", "tief\u00b7rer", "als", "der", "fr\u00fc\u00b7he\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "ADJA", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und unerbittlich jedes stumme Zeichen:", "tokens": ["Und", "un\u00b7er\u00b7bitt\u00b7lich", "je\u00b7des", "stum\u00b7me", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Das Angesicht der Nacht war mit im Bund,", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "der", "Nacht", "war", "mit", "im", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der Wind im Bund, der sanfte Fr\u00fchlingswind,", "tokens": ["Der", "Wind", "im", "Bund", ",", "der", "sanf\u00b7te", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und alle ", "tokens": ["Und", "al\u00b7le"], "token_info": ["word", "word"], "pos": ["KON", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Den zarten Seelen stellt das dunkle Schicksal", "tokens": ["Den", "zar\u00b7ten", "See\u00b7len", "stellt", "das", "dunk\u00b7le", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Fallstricke dieser Art. Dann kam ein Tag,", "tokens": ["Fall\u00b7stri\u00b7cke", "die\u00b7ser", "Art", ".", "Dann", "kam", "ein", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Da hob er sich, und sein gequ\u00e4ltes Auge", "tokens": ["Da", "hob", "er", "sich", ",", "und", "sein", "ge\u00b7qu\u00e4l\u00b7tes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.