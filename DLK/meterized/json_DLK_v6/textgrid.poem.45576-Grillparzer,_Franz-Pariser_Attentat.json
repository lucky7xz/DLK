{"textgrid.poem.45576": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Pariser Attentat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Helden der Freiheitsliebe,", "tokens": ["Die\u00b7se", "Hel\u00b7den", "der", "Frei\u00b7heits\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sie haben strenge Studien gemacht", "tokens": ["Sie", "ha\u00b7ben", "stren\u00b7ge", "Stu\u00b7di\u00b7en", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sich fr\u00fcher ge\u00fcbt als Diebe,", "tokens": ["Und", "sich", "fr\u00fc\u00b7her", "ge\u00b7\u00fcbt", "als", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Bis sies zum Mord f\u00fcrs Vaterland gebracht.", "tokens": ["Bis", "sies", "zum", "Mord", "f\u00fcrs", "Va\u00b7ter\u00b7land", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}