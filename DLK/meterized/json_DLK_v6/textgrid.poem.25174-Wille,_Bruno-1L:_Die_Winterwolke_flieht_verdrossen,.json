{"textgrid.poem.25174": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Winterwolke flieht verdrossen,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Winterwolke flieht verdrossen,", "tokens": ["Die", "Win\u00b7ter\u00b7wol\u00b7ke", "flieht", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel schm\u00fcckt ein sanftes Blau.", "tokens": ["Den", "Him\u00b7mel", "schm\u00fcckt", "ein", "sanf\u00b7tes", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da l\u00e4cheln goldig \u00fcbergossen", "tokens": ["Da", "l\u00e4\u00b7cheln", "gol\u00b7dig", "\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00fcgel, Garten, See und Au.", "tokens": ["Ge\u00b7h\u00fc\u00b7gel", ",", "Gar\u00b7ten", ",", "See", "und", "Au", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und kosend sich die junge Sonne", "tokens": ["Und", "ko\u00b7send", "sich", "die", "jun\u00b7ge", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An die entz\u00fcckte Erde schmiegt.", "tokens": ["An", "die", "ent\u00b7z\u00fcck\u00b7te", "Er\u00b7de", "schmiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Pflanzenkind in stummer Wonne", "tokens": ["Das", "Pflan\u00b7zen\u00b7kind", "in", "stum\u00b7mer", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die zarten Glieder dehnt und biegt.", "tokens": ["Die", "zar\u00b7ten", "Glie\u00b7der", "dehnt", "und", "biegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es schaut empor; sein L\u00e4cheln schmeichelt", "tokens": ["Es", "schaut", "em\u00b7por", ";", "sein", "L\u00e4\u00b7cheln", "schmei\u00b7chelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erquickend wie ein klarer Quell;", "tokens": ["Er\u00b7qui\u00b7ckend", "wie", "ein", "kla\u00b7rer", "Quell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie von Kinderhand gestreichelt", "tokens": ["Und", "wie", "von", "Kin\u00b7der\u00b7hand", "ge\u00b7strei\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird meine d\u00fcstre Stirne hell.", "tokens": ["Wird", "mei\u00b7ne", "d\u00fcst\u00b7re", "Stir\u00b7ne", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}