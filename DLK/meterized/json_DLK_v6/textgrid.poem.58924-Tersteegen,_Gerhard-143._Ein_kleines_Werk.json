{"textgrid.poem.58924": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "143. Ein kleines Werk", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Gott mit Liebe stets gedenke", "tokens": ["An", "Gott", "mit", "Lie\u00b7be", "stets", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und innig dich in ihn ersenke,", "tokens": ["Und", "in\u00b7nig", "dich", "in", "ihn", "er\u00b7sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einf\u00e4ltig, sanft und willenlos!", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", ",", "sanft", "und", "wil\u00b7len\u00b7los", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies kleine Werk ist wundergro\u00df.", "tokens": ["Dies", "klei\u00b7ne", "Werk", "ist", "wun\u00b7der\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}