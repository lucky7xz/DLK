{"dta.poem.19475": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe, Nymphe, deine keusche Flut,", "tokens": ["Ich", "lie\u00b7be", ",", "Nym\u00b7phe", ",", "dei\u00b7ne", "keu\u00b7sche", "Flut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die k\u00fchl im allertiefsten Walde ruht.", "tokens": ["Die", "k\u00fchl", "im", "al\u00b7ler\u00b7tiefs\u00b7ten", "Wal\u00b7de", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du spiegelst weder Stadt noch Firneschnee,", "tokens": ["Du", "spie\u00b7gelst", "we\u00b7der", "Stadt", "noch", "Fir\u00b7nesc\u00b7hnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Himmel schimmerst du, mein kleiner See!", "tokens": ["Den", "Him\u00b7mel", "schim\u00b7merst", "du", ",", "mein", "klei\u00b7ner", "See", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Antlitz sagt mir Alles, rasch erregt,", "tokens": ["Dein", "Ant\u00b7litz", "sagt", "mir", "Al\u00b7les", ",", "rasch", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was dir das kindliche Gem\u00fcth bewegt,", "tokens": ["Was", "dir", "das", "kind\u00b7li\u00b7che", "Ge\u00b7m\u00fcth", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und leicht erhellt, verdunkelt ohne Grund,", "tokens": ["Und", "leicht", "er\u00b7hellt", ",", "ver\u00b7dun\u00b7kelt", "oh\u00b7ne", "Grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Macht es mir alle deine Launen kund.", "tokens": ["Macht", "es", "mir", "al\u00b7le", "dei\u00b7ne", "Lau\u00b7nen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Der Kahn, verborgen tief im Schilfe dort,", "tokens": ["Der", "Kahn", ",", "ver\u00b7bor\u00b7gen", "tief", "im", "Schil\u00b7fe", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gefesselt ist er durch ein Zauberwort.", "tokens": ["Ge\u00b7fes\u00b7selt", "ist", "er", "durch", "ein", "Zau\u00b7ber\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie hat gel\u00f6st ihn eine trunkne Schaar,", "tokens": ["Nie", "hat", "ge\u00b7l\u00f6st", "ihn", "ei\u00b7ne", "trunk\u00b7ne", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie hat sich eine Dirn im Flatterhaar,", "tokens": ["Nie", "hat", "sich", "ei\u00b7ne", "Dirn", "im", "Flat\u00b7ter\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von rohen Buhlen durch den Wald gehetzt,", "tokens": ["Von", "ro\u00b7hen", "Buh\u00b7len", "durch", "den", "Wald", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vor deinen Spiegel keuchend hingesetzt.", "tokens": ["Vor", "dei\u00b7nen", "Spie\u00b7gel", "keu\u00b7chend", "hin\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nie hat ein unstet zuckend Fackelrot", "tokens": ["Nie", "hat", "ein", "un\u00b7stet", "zu\u00b7ckend", "Fa\u00b7ckel\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "VVPP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir \u00fcber deine k\u00fchle Stirn geloht!", "tokens": ["Dir", "\u00fc\u00b7ber", "dei\u00b7ne", "k\u00fch\u00b7le", "Stirn", "ge\u00b7loht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Horch! Stimmen durch den Wald! Ein Lustgeschrei!", "tokens": ["Horch", "!", "Stim\u00b7men", "durch", "den", "Wald", "!", "Ein", "Lust\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gekreisch! Gewieher! Freches Volk, vorbei!", "tokens": ["Ge\u00b7kreisch", "!", "Ge\u00b7wie\u00b7her", "!", "Fre\u00b7ches", "Volk", ",", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Gassenhauer, liederlich gejohlt \u2014", "tokens": ["Den", "Gas\u00b7sen\u00b7hau\u00b7er", ",", "lie\u00b7der\u00b7lich", "ge\u00b7johlt"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sch\u00e4me dich, Echo! \u2014 hast du wiederholt!", "tokens": ["Sch\u00e4\u00b7me", "dich", ",", "E\u00b7cho", "!", "hast", "du", "wie\u00b7der\u00b7holt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "$(", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Verh\u00fclle, Nymphe, deiner Augen Schein,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", ",", "Nym\u00b7phe", ",", "dei\u00b7ner", "Au\u00b7gen", "Schein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbirg dich tiefer in den Wald hinein!", "tokens": ["Ver\u00b7birg", "dich", "tie\u00b7fer", "in", "den", "Wald", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und z\u00fcrnend gegen den Tumult gewandt:", "tokens": ["Und", "z\u00fcr\u00b7nend", "ge\u00b7gen", "den", "Tu\u00b7mult", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u201ehinweg!\u201c gebot ich mit erhobner Hand.", "tokens": ["\u201e", "hin\u00b7weg", "!", "\u201c", "ge\u00b7bot", "ich", "mit", "er\u00b7hob\u00b7ner", "Hand", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201enicht n\u00e4her!\u201c Und im Walde ward es Ruh.", "tokens": ["\u201e", "nicht", "n\u00e4\u00b7her", "!", "\u201c", "Und", "im", "Wal\u00b7de", "ward", "es", "Ruh", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "$.", "$(", "KON", "APPRART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Jubel zog sich einer Schenke zu.", "tokens": ["Der", "Ju\u00b7bel", "zog", "sich", "ei\u00b7ner", "Schen\u00b7ke", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bliebst in deinem blauen Kleide rein,", "tokens": ["Du", "bliebst", "in", "dei\u00b7nem", "blau\u00b7en", "Klei\u00b7de", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In deinem gr\u00fcnen Waldesd\u00e4mmerschein \u2014", "tokens": ["In", "dei\u00b7nem", "gr\u00fc\u00b7nen", "Wal\u00b7des\u00b7d\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indessen hat die Sonne sich geneigt,", "tokens": ["In\u00b7des\u00b7sen", "hat", "die", "Son\u00b7ne", "sich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie s\u00fc\u00df in jedem Blatt die Stille schweigt!", "tokens": ["Wie", "s\u00fc\u00df", "in", "je\u00b7dem", "Blatt", "die", "Stil\u00b7le", "schweigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Tannenduft und unter Himmelsruh,", "tokens": ["In", "Tan\u00b7nen\u00b7duft", "und", "un\u00b7ter", "Him\u00b7mels\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bewacht von meinem Blick, entschlummerst du!", "tokens": ["Be\u00b7wacht", "von", "mei\u00b7nem", "Blick", ",", "ent\u00b7schlum\u00b7merst", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}