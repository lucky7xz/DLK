{"dta.poem.3362": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nheit, Amme der Kunst, des Dichters traute", "tokens": ["Sch\u00f6n\u00b7heit", ",", "Am\u00b7me", "der", "Kunst", ",", "des", "Dich\u00b7ters", "trau\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gespielin,", "tokens": ["Ge\u00b7spie\u00b7lin", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Selige Tochter der Erd' und des erdumarmenden", "tokens": ["Se\u00b7li\u00b7ge", "Toch\u00b7ter", "der", "Erd'", "und", "des", "er\u00b7du\u00b7mar\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "Himmels,", "tokens": ["Him\u00b7mels", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Von dem Gedanken erzeugt, und von der Empfin-", "tokens": ["Von", "dem", "Ge\u00b7dan\u00b7ken", "er\u00b7zeugt", ",", "und", "von", "der", "Emp\u00b7fin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "dung empfangen,", "tokens": ["dung", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Die du erschienest, ein waltender D\u00e4mon, ein", "tokens": ["Die", "du", "er\u00b7schie\u00b7nest", ",", "ein", "wal\u00b7ten\u00b7der", "D\u00e4\u00b7mon", ",", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ART"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "segnender Schutzgeist,", "tokens": ["seg\u00b7nen\u00b7der", "Schutz\u00b7geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frieden zu stiften im hadernden Reiche der Kr\u00e4ft'", "tokens": ["Frie\u00b7den", "zu", "stif\u00b7ten", "im", "ha\u00b7dern\u00b7den", "Rei\u00b7che", "der", "Kr\u00e4ft'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVFIN", "APPRART", "ADJA", "NE", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.10": {"text": "und der Triebe,", "tokens": ["und", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.11": {"text": "Die du Gehorsam lehrtest das Element und den", "tokens": ["Die", "du", "Ge\u00b7hor\u00b7sam", "lehr\u00b7test", "das", "E\u00b7le\u00b7ment", "und", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "VVFIN", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "Atom,", "tokens": ["A\u00b7tom", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die du entlocktest dem starren Chaos der Sch\u00f6pfun-", "tokens": ["Die", "du", "ent\u00b7lock\u00b7test", "dem", "star\u00b7ren", "Chaos", "der", "Sch\u00f6p\u00b7fun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "gen sch\u00f6nste,", "tokens": ["gen", "sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Die du den Massen Gestalt, dem Todten Leben", "tokens": ["Die", "du", "den", "Mas\u00b7sen", "Ge\u00b7stalt", ",", "dem", "Tod\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "gew\u00e4hrtest,", "tokens": ["ge\u00b7w\u00e4hr\u00b7test", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Die du der T\u00f6ne wildes Geschrille zu lieblichem", "tokens": ["Die", "du", "der", "T\u00f6\u00b7ne", "wil\u00b7des", "Ge\u00b7schril\u00b7le", "zu", "lieb\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Wohllaut,", "tokens": ["Wohl\u00b7laut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Zu gef\u00e4lligem Einklang den Schrey der Farben ver-", "tokens": ["Zu", "ge\u00b7f\u00e4l\u00b7li\u00b7gem", "Ein\u00b7klang", "den", "Schrey", "der", "Far\u00b7ben", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.20": {"text": "schmelztest;", "tokens": ["schmelz\u00b7test", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Die du uns, M\u00e4chtige, leiser itzt, itzt wilder", "tokens": ["Die", "du", "uns", ",", "M\u00e4ch\u00b7ti\u00b7ge", ",", "lei\u00b7ser", "itzt", ",", "itzt", "wil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PPER", "$,", "NN", "$,", "ADJD", "ADV", "$,", "ADV", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "ergreifest", "tokens": ["er\u00b7grei\u00b7fest"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "In der Bewegungen Schwung, im wellenschlagen-", "tokens": ["In", "der", "Be\u00b7we\u00b7gun\u00b7gen", "Schwung", ",", "im", "wel\u00b7len\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPRART", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "den Umriss,", "tokens": ["den", "Um\u00b7riss", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.25": {"text": "In der Accorde frohem Gew\u00fchl, im Rythmus der", "tokens": ["In", "der", "Ac\u00b7cor\u00b7de", "fro\u00b7hem", "Ge\u00b7w\u00fchl", ",", "im", "Ry\u00b7th\u00b7mus", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ART"], "meter": "--++-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Farben,", "tokens": ["Far\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "In Aurorens Err\u00f6then, im schwellenden Kelche der", "tokens": ["In", "Au\u00b7ro\u00b7rens", "Er\u00b7r\u00f6\u00b7then", ",", "im", "schwel\u00b7len\u00b7den", "Kel\u00b7che", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "APPRART", "ADJA", "NN", "ART"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Rose,", "tokens": ["Ro\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "In der S\u00e4ule schlankem Gew\u00e4chs, im Bogen des", "tokens": ["In", "der", "S\u00e4u\u00b7le", "schlan\u00b7kem", "Ge\u00b7w\u00e4chs", ",", "im", "Bo\u00b7gen", "des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ART"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Domes,", "tokens": ["Do\u00b7mes", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "In der Nachtigall Schlag, im seeleschmelzenden", "tokens": ["In", "der", "Nach\u00b7ti\u00b7gall", "Schlag", ",", "im", "see\u00b7le\u00b7schmel\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPRART", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.32": {"text": "Liede,", "tokens": ["Lie\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "In des Weibes Anmuth, und in der W\u00fcrde des", "tokens": ["In", "des", "Wei\u00b7bes", "An\u00b7muth", ",", "und", "in", "der", "W\u00fcr\u00b7de", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "APPR", "ART", "NN", "ART"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Mannes;", "tokens": ["Man\u00b7nes", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Die du den Wilden bez\u00e4hmst, und den Barbaren", "tokens": ["Die", "du", "den", "Wil\u00b7den", "be\u00b7z\u00e4hmst", ",", "und", "den", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "vermenschlichst,", "tokens": ["ver\u00b7menschlichst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Die du den Rohen erweichst, und die du straffst", "tokens": ["Die", "du", "den", "Ro\u00b7hen", "er\u00b7weichst", ",", "und", "die", "du", "straffst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "PRELS", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "den Erschlafften,", "tokens": ["den", "Er\u00b7schlaff\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.39": {"text": "Die du den Zorn entwaffnest, die schnaubende", "tokens": ["Die", "du", "den", "Zorn", "ent\u00b7waff\u00b7nest", ",", "die", "schnau\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Rache vers\u00f6hnest,", "tokens": ["Ra\u00b7che", "ver\u00b7s\u00f6h\u00b7nest", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Und die wiehernde Gier veredelst zu flehender", "tokens": ["Und", "die", "wie\u00b7hern\u00b7de", "Gier", "ver\u00b7e\u00b7delst", "zu", "fle\u00b7hen\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Liebe;", "tokens": ["Lie\u00b7be", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Die du schlichtest den herben Kampf im Busen der", "tokens": ["Die", "du", "schlich\u00b7test", "den", "her\u00b7ben", "Kampf", "im", "Bu\u00b7sen", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Menschheit,", "tokens": ["Menschheit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Jegliche Fehde vertr\u00e4gst, und jegliche Zwietracht", "tokens": ["Jeg\u00b7li\u00b7che", "Feh\u00b7de", "ver\u00b7tr\u00e4gst", ",", "und", "jeg\u00b7li\u00b7che", "Zwiet\u00b7racht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.46": {"text": "vereinest,", "tokens": ["ver\u00b7ei\u00b7nest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Jegliche Pflicht verlieblichst, und adelst jegliche", "tokens": ["Jeg\u00b7li\u00b7che", "Pflicht", "ver\u00b7lieb\u00b7lichst", ",", "und", "a\u00b7delst", "jeg\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PIAT"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.48": {"text": "Neigung;", "tokens": ["Nei\u00b7gung", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "Die du uns l\u00f6sest vom Joch des Instincts, aus der", "tokens": ["Die", "du", "uns", "l\u00f6\u00b7sest", "vom", "Joch", "des", "I\u00b7nstincts", ",", "aus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Sinne Beschr\u00e4nktheit", "tokens": ["Sin\u00b7ne", "Be\u00b7schr\u00e4nk\u00b7theit"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.51": {"text": "Uns ins Unendliche hebst, aus der Kr\u00e4fte g\u00e4hren-", "tokens": ["Uns", "ins", "Un\u00b7end\u00b7li\u00b7che", "hebst", ",", "aus", "der", "Kr\u00e4f\u00b7te", "g\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.52": {"text": "dem Aufruhr", "tokens": ["dem", "Auf\u00b7ruhr"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.53": {"text": "In des Gesetzes heiliges Reich die Schwindelnden", "tokens": ["In", "des", "Ge\u00b7set\u00b7zes", "hei\u00b7li\u00b7ges", "Reich", "die", "Schwin\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.re