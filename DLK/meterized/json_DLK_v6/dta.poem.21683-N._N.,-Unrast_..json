{"dta.poem.21683": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Unrast .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr nie verl\u00f6schten heiligen Flammenk\u00f6rper,", "tokens": ["Ihr", "nie", "ver\u00b7l\u00f6schten", "hei\u00b7li\u00b7gen", "Flam\u00b7men\u00b7k\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wachtfeuer ihr der Nacht, mit eurer Strahlen", "tokens": ["Wacht\u00b7feu\u00b7er", "ihr", "der", "Nacht", ",", "mit", "eu\u00b7rer", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beredsamkeit, im Auge Friedenstr\u00e4ume!", "tokens": ["Be\u00b7red\u00b7sam\u00b7keit", ",", "im", "Au\u00b7ge", "Frie\u00b7dens\u00b7tr\u00e4u\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch ihr seid ruhelos und zittert droben", "tokens": ["Auch", "ihr", "seid", "ru\u00b7he\u00b7los", "und", "zit\u00b7tert", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit ungewissem Licht, und Wolkenschatten", "tokens": ["Mit", "un\u00b7ge\u00b7wis\u00b7sem", "Licht", ",", "und", "Wol\u00b7ken\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Umh\u00fcllen eure Stirn und bald wie bald!", "tokens": ["Um\u00b7h\u00fcl\u00b7len", "eu\u00b7re", "Stirn", "und", "bald", "wie", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Tritt euer Glanz zur\u00fcck am Firmament.", "tokens": ["Tritt", "eu\u00b7er", "Glanz", "zu\u00b7r\u00fcck", "am", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u2014 O Unrast, Unrast! Sieh, wie rings das Meer,", "tokens": ["O", "Un\u00b7rast", ",", "Un\u00b7rast", "!", "Sieh", ",", "wie", "rings", "das", "Meer", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$,", "NN", "$.", "NE", "$,", "PWAV", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Das seinen Busen frech dem Mond entbl\u00f6\u00dft,", "tokens": ["Das", "sei\u00b7nen", "Bu\u00b7sen", "frech", "dem", "Mond", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wogend und \u00e4chzend in Begier und Groll,", "tokens": ["Wo\u00b7gend", "und", "\u00e4ch\u00b7zend", "in", "Be\u00b7gier", "und", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Sich nach der Sterne schleierloser Sch\u00f6nheit,", "tokens": ["Sich", "nach", "der", "Ster\u00b7ne", "schlei\u00b7er\u00b7lo\u00b7ser", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie eine Seele nach Vollendung, sehnt!", "tokens": ["Wie", "ei\u00b7ne", "See\u00b7le", "nach", "Vol\u00b7len\u00b7dung", ",", "sehnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die ruhelosen Wolken ballen sich", "tokens": ["Die", "ru\u00b7he\u00b7lo\u00b7sen", "Wol\u00b7ken", "bal\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und l\u00f6sen sich und fliegen dort durchs Blau,", "tokens": ["Und", "l\u00f6\u00b7sen", "sich", "und", "flie\u00b7gen", "dort", "durchs", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Eisbergen gleich, bemalt mit Irisfarben", "tokens": ["Eis\u00b7ber\u00b7gen", "gleich", ",", "be\u00b7malt", "mit", "I\u00b7ris\u00b7far\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "VVFIN", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Durch die R\u00fcckspiegelung der warmen Sonne,", "tokens": ["Durch", "die", "R\u00fcck\u00b7spie\u00b7ge\u00b7lung", "der", "war\u00b7men", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.17": {"text": "Die bald in Dunst wie Schnee sie l\u00f6sen wird;", "tokens": ["Die", "bald", "in", "Dunst", "wie", "Schnee", "sie", "l\u00f6\u00b7sen", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KOKOM", "NN", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Sommer fliegt wie ein Err\u00f6then hastig", "tokens": ["Der", "Som\u00b7mer", "fliegt", "wie", "ein", "Er\u00b7r\u00f6\u00b7then", "has\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ueber der Erde Antlitz und verweht;", "tokens": ["Ue\u00b7ber", "der", "Er\u00b7de", "Ant\u00b7litz", "und", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Der Regen prasselt wild und toll hernieder,", "tokens": ["Der", "Re\u00b7gen", "pras\u00b7selt", "wild", "und", "toll", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die reuigen Winde jammern tief und schwer;", "tokens": ["Die", "reu\u00b7i\u00b7gen", "Win\u00b7de", "jam\u00b7mern", "tief", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und dieser ewige Planet der Pein", "tokens": ["Und", "die\u00b7ser", "e\u00b7wi\u00b7ge", "Pla\u00b7net", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Hat Ruhe nie gekostet. Heimathlos,", "tokens": ["Hat", "Ru\u00b7he", "nie", "ge\u00b7kos\u00b7tet", ".", "Hei\u00b7math\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$.", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "St\u00f6hnend und seufzend, eine Welt des Wahnsinns,", "tokens": ["St\u00f6h\u00b7nend", "und", "seuf\u00b7zend", ",", "ei\u00b7ne", "Welt", "des", "Wahn\u00b7sinns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Rollt durch die Tiefen er der Ewigkeit.", "tokens": ["Rollt", "durch", "die", "Tie\u00b7fen", "er", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und, Mensch! \u2014 O Kind, du fr\u00f6hlich Lichtgebild", "tokens": ["Und", ",", "Mensch", "!", "O", "Kind", ",", "du", "fr\u00f6h\u00b7lich", "Licht\u00b7ge\u00b7bild"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "$(", "NE", "NN", "$,", "PPER", "ADJD", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.27": {"text": "Aus Gottes Hand! Die tanzende Bewegung", "tokens": ["Aus", "Got\u00b7tes", "Hand", "!", "Die", "tan\u00b7zen\u00b7de", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Der muntern jungen Glieder wird gelenkt", "tokens": ["Der", "mun\u00b7tern", "jun\u00b7gen", "Glie\u00b7der", "wird", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Von deines Wesens innrer Harmonie.", "tokens": ["Von", "dei\u00b7nes", "We\u00b7sens", "inn\u00b7rer", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Seit Gott den ersten Stern erschuf \u2014 wie lange!", "tokens": ["Seit", "Gott", "den", "ers\u00b7ten", "Stern", "er\u00b7schuf", "wie", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$(", "PWAV", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "Doch seine Hand liegt noch auf deinem Haupt,", "tokens": ["Doch", "sei\u00b7ne", "Hand", "liegt", "noch", "auf", "dei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Als w\u00e4r\u2019 es gestern. Letzte Offenbarung! \u2014", "tokens": ["Als", "w\u00e4r'", "es", "ge\u00b7stern", ".", "Letz\u00b7te", "Of\u00b7fen\u00b7ba\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVINF", "$.", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Du Silberstrom, der aus dem See der Urkraft", "tokens": ["Du", "Sil\u00b7ber\u00b7strom", ",", "der", "aus", "dem", "See", "der", "Ur\u00b7kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Mit s\u00fc\u00dfem Lachen bricht, wie endest du?", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "La\u00b7chen", "bricht", ",", "wie", "en\u00b7dest", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Der Jugendleidenschaft Kaskadenstrudel", "tokens": ["Der", "Ju\u00b7gend\u00b7lei\u00b7den\u00b7schaft", "Kas\u00b7ka\u00b7dens\u00b7tru\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Furcht deiner ebnen Fl\u00e4che glatte Wange,", "tokens": ["Furcht", "dei\u00b7ner", "eb\u00b7nen", "Fl\u00e4\u00b7che", "glat\u00b7te", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Mit frischen Blasen, Gr\u00fcbchen gleich, bes\u00e4t,", "tokens": ["Mit", "fri\u00b7schen", "Bla\u00b7sen", ",", "Gr\u00fcb\u00b7chen", "gleich", ",", "be\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Zerw\u00fchlt dein Bett, tr\u00fcbt deinen klaren Spiegel,", "tokens": ["Zer\u00b7w\u00fchlt", "dein", "Bett", ",", "tr\u00fcbt", "dei\u00b7nen", "kla\u00b7ren", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und dann, befleckt von ungesundem Schutt", "tokens": ["Und", "dann", ",", "be\u00b7fleckt", "von", "un\u00b7ge\u00b7sun\u00b7dem", "Schutt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und Schmutz, den Tagsgesch\u00e4fte auf dich h\u00e4ufen,", "tokens": ["Und", "Schmutz", ",", "den", "Tags\u00b7ge\u00b7sch\u00e4f\u00b7te", "auf", "dich", "h\u00e4u\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Von manchem Fels des Mi\u00dfgeschicks beengt,", "tokens": ["Von", "man\u00b7chem", "Fels", "des", "Mi\u00df\u00b7ge\u00b7schicks", "be\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Str\u00f6mst d\u00fcster du bergab durch vielgewundne", "tokens": ["Str\u00f6mst", "d\u00fcs\u00b7ter", "du", "berg\u00b7ab", "durch", "viel\u00b7ge\u00b7wund\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Gebirgeskammern oder sumpfige Moore,", "tokens": ["Ge\u00b7bir\u00b7ges\u00b7kam\u00b7mern", "o\u00b7der", "sump\u00b7fi\u00b7ge", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Bis du zuletzt mit tr\u00e4ger fauler Ader", "tokens": ["Bis", "du", "zu\u00b7letzt", "mit", "tr\u00e4\u00b7ger", "fau\u00b7ler", "A\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Zum unbekannten Weltmeer seicht und siech", "tokens": ["Zum", "un\u00b7be\u00b7kann\u00b7ten", "Welt\u00b7meer", "seicht", "und", "siech"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Dahinschleichst: jener allgemeinen M\u00fcndung,", "tokens": ["Da\u00b7hin\u00b7schleichst", ":", "je\u00b7ner", "all\u00b7ge\u00b7mei\u00b7nen", "M\u00fcn\u00b7dung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Zu der selbst Gie\u00dfbach, Katarakt, Gebirgsstrom \u2014", "tokens": ["Zu", "der", "selbst", "Gie\u00df\u00b7bach", ",", "Ka\u00b7ta\u00b7rakt", ",", "Ge\u00b7birgs\u00b7strom"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.48": {"text": "Begeistrung, Genius, Thatkraft \u2014 bald sich w\u00e4lzen.", "tokens": ["Be\u00b7geis\u00b7trung", ",", "Ge\u00b7nius", ",", "That\u00b7kraft", "bald", "sich", "w\u00e4l\u00b7zen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$(", "ADV", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}