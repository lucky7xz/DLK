{"textgrid.poem.61149": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "O sch\u00f6ne Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ne Zeit, da mich noch jede Stunde", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", ",", "da", "mich", "noch", "je\u00b7de", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einer frisch erschlo\u00dfnen Bl\u00fcte rief,", "tokens": ["Zu", "ei\u00b7ner", "frisch", "er\u00b7schlo\u00df\u00b7nen", "Bl\u00fc\u00b7te", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da jeder Tag, ein goldner Freudenbrief,", "tokens": ["Da", "je\u00b7der", "Tag", ",", "ein", "gold\u00b7ner", "Freu\u00b7den\u00b7brief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich vor mir auftat mit begl\u00fcckter Kunde;", "tokens": ["Sich", "vor", "mir", "auf\u00b7tat", "mit", "be\u00b7gl\u00fcck\u00b7ter", "Kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da, wie die Ros' in dunklem Alpengrunde,", "tokens": ["Da", ",", "wie", "die", "Ros'", "in", "dunk\u00b7lem", "Al\u00b7pen\u00b7grun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr liebes Bild mir bl\u00fcht' im Herzen tief,", "tokens": ["Ihr", "lie\u00b7bes", "Bild", "mir", "bl\u00fcht'", "im", "Her\u00b7zen", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ich mit ihrem Namen sanft entschlief,", "tokens": ["Und", "ich", "mit", "ih\u00b7rem", "Na\u00b7men", "sanft", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00fcrd' er zum Gebet in meinem Munde!", "tokens": ["Als", "w\u00fcrd'", "er", "zum", "Ge\u00b7bet", "in", "mei\u00b7nem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du bist dahin, und doch, du bist noch mein:", "tokens": ["Du", "bist", "da\u00b7hin", ",", "und", "doch", ",", "du", "bist", "noch", "mein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "KON", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es flie\u00dft das Lied von deinen Nachtigallen", "tokens": ["Es", "flie\u00dft", "das", "Lied", "von", "dei\u00b7nen", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fr\u00fchlingsgru\u00df in meinen Herbst herein.", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7gru\u00df", "in", "mei\u00b7nen", "Herbst", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allabendlich, wenn Stadt und Flur verhallen,", "tokens": ["Al\u00b7la\u00b7bend\u00b7lich", ",", "wenn", "Stadt", "und", "Flur", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kehrt die Erinnrung tr\u00f6stend bei mir ein,", "tokens": ["Kehrt", "die", "E\u00b7rinn\u00b7rung", "tr\u00f6s\u00b7tend", "bei", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit mir im Traume durch die Nacht zu wallen.", "tokens": ["Mit", "mir", "im", "Trau\u00b7me", "durch", "die", "Nacht", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}