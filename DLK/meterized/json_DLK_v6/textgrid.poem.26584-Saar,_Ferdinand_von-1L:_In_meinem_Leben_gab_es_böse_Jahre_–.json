{"textgrid.poem.26584": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In meinem Leben gab es b\u00f6se Jahre \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Leben gab es b\u00f6se Jahre \u2013", "tokens": ["In", "mei\u00b7nem", "Le\u00b7ben", "gab", "es", "b\u00f6\u00b7se", "Jah\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie jene aus der Bibel waren's sieben \u2013", "tokens": ["Wie", "je\u00b7ne", "aus", "der", "Bi\u00b7bel", "wa\u00b7ren's", "sie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "ART", "NN", "VAFIN", "CARD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hat mich ein Verh\u00e4ngni\u00df umgetrieben,", "tokens": ["Da", "hat", "mich", "ein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "um\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wandelte \u2013 und lag doch auf der Bahre.", "tokens": ["Ich", "wan\u00b7del\u00b7te", "\u2013", "und", "lag", "doch", "auf", "der", "Bah\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Aus jener Zeit, wo, ohne Frucht geblieben,", "tokens": ["Aus", "je\u00b7ner", "Zeit", ",", "wo", ",", "oh\u00b7ne", "Frucht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "$,", "KOUI", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Geist in \u00f6dem Denken sich zerrieben,", "tokens": ["Mein", "Geist", "in", "\u00f6\u00b7dem", "Den\u00b7ken", "sich", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Gram und Sorge bleichten meine Haare!", "tokens": ["Und", "Gram", "und", "Sor\u00b7ge", "bleich\u00b7ten", "mei\u00b7ne", "Haa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gleich schwerem Traum zerflo\u00df ihr dunkles Walten,", "tokens": ["Gleich", "schwe\u00b7rem", "Traum", "zer\u00b7flo\u00df", "ihr", "dunk\u00b7les", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf vernarbte Wunden kann ich zeigen,", "tokens": ["Und", "auf", "ver\u00b7narb\u00b7te", "Wun\u00b7den", "kann", "ich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kaum wissend mehr, von wem ich sie erhalten.", "tokens": ["Kaum", "wis\u00b7send", "mehr", ",", "von", "wem", "ich", "sie", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "APPR", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur manchmal, einzeln und in wirrem Reigen,", "tokens": ["Nur", "manch\u00b7mal", ",", "ein\u00b7zeln", "und", "in", "wir\u00b7rem", "Rei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auftauchen schattenhafte Mahngestalten:", "tokens": ["Auf\u00b7tau\u00b7chen", "schat\u00b7ten\u00b7haf\u00b7te", "Mahn\u00b7ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00e4nner und Frau'n, die wie aus Gr\u00e4bern steigen.", "tokens": ["M\u00e4n\u00b7ner", "und", "Frau'n", ",", "die", "wie", "aus", "Gr\u00e4\u00b7bern", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "KOKOM", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}