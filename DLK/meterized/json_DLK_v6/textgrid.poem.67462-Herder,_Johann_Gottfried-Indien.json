{"textgrid.poem.67462": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Indien", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanftes Gef\u00fchl der Indier gab dem Schalle zum F\u00fchrer", "tokens": ["Sanf\u00b7tes", "Ge\u00b7f\u00fchl", "der", "In\u00b7dier", "gab", "dem", "Schal\u00b7le", "zum", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+--+--++-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht die gr\u00f6bere Luft, gab ihm den Aether zum Reich.", "tokens": ["Nicht", "die", "gr\u00f6\u00b7be\u00b7re", "Luft", ",", "gab", "ihm", "den", "A\u00b7e\u00b7ther", "zum", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+---+---+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er nur bildet den Ton zur zarten himmlischen Stimme,", "tokens": ["Er", "nur", "bil\u00b7det", "den", "Ton", "zur", "zar\u00b7ten", "himm\u00b7li\u00b7schen", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die die Empfindungen spricht, die die Empfindungen weckt,", "tokens": ["Die", "die", "Emp\u00b7fin\u00b7dun\u00b7gen", "spricht", ",", "die", "die", "Emp\u00b7fin\u00b7dun\u00b7gen", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und entf\u00fchret der gr\u00f6beren Luft die Seelen der Menschen", "tokens": ["Und", "ent\u00b7f\u00fch\u00b7ret", "der", "gr\u00f6\u00b7be\u00b7ren", "Luft", "die", "See\u00b7len", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "In ein einsam Gebiet, in das \u00e4therische Land,", "tokens": ["In", "ein", "ein\u00b7sam", "Ge\u00b7biet", ",", "in", "das", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Wo nicht rasselt der Wagen, der jetzt den Wolken entschwebet,", "tokens": ["Wo", "nicht", "ras\u00b7selt", "der", "Wa\u00b7gen", ",", "der", "jetzt", "den", "Wol\u00b7ken", "ent\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wo nur h\u00e4usliches Gl\u00fcck bildet der G\u00f6tter Genu\u00df,", "tokens": ["Wo", "nur", "h\u00e4us\u00b7li\u00b7ches", "Gl\u00fcck", "bil\u00b7det", "der", "G\u00f6t\u00b7ter", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Wo Sakontala lebt mit ihrem entschwundenen Knaben,", "tokens": ["Wo", "Sa\u00b7kon\u00b7ta\u00b7la", "lebt", "mit", "ih\u00b7rem", "ent\u00b7schwun\u00b7de\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wo Duschmanta sie neu, neu von den G\u00f6ttern empf\u00e4ngt.", "tokens": ["Wo", "Duschman\u00b7ta", "sie", "neu", ",", "neu", "von", "den", "G\u00f6t\u00b7tern", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sei mir gegr\u00fc\u00dft, o heiliges Land, und Du F\u00fchrer der T\u00f6ne,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "hei\u00b7li\u00b7ges", "Land", ",", "und", "Du", "F\u00fch\u00b7rer", "der", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "ADJA", "NN", "$,", "KON", "PPER", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Stimme des Herzens, erheb oft mich im Aether dahin!", "tokens": ["Stim\u00b7me", "des", "Her\u00b7zens", ",", "er\u00b7heb", "oft", "mich", "im", "A\u00b7e\u00b7ther", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADV", "PPER", "APPRART", "NN", "PAV", "$."], "meter": "+--+--+-+-+---+", "measure": "dactylic.di.plus"}}}}}