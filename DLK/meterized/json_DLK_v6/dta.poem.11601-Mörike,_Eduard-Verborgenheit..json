{"dta.poem.11601": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Verborgenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Locket nicht mit Liebesgaben!", "tokens": ["Lo\u00b7cket", "nicht", "mit", "Lie\u00b7bes\u00b7ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft dies Herz alleine haben", "tokens": ["La\u00dft", "dies", "Herz", "al\u00b7lei\u00b7ne", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDS", "NN", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Wonne, seine Pein!", "tokens": ["Sei\u00b7ne", "Won\u00b7ne", ",", "sei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich traure wei\u00df ich nicht,", "tokens": ["Was", "ich", "trau\u00b7re", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist unbekanntes Wehe;", "tokens": ["Es", "ist", "un\u00b7be\u00b7kann\u00b7tes", "We\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immerdar durch Thr\u00e4nen sehe", "tokens": ["Im\u00b7mer\u00b7dar", "durch", "Thr\u00e4\u00b7nen", "se\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich der Sonne liebes Licht.", "tokens": ["Ich", "der", "Son\u00b7ne", "lie\u00b7bes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oft bin ich mir kaum bewu\u00dft,", "tokens": ["Oft", "bin", "ich", "mir", "kaum", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die helle Freude z\u00fccket", "tokens": ["Und", "die", "hel\u00b7le", "Freu\u00b7de", "z\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Schwere, so mich dr\u00fccket", "tokens": ["Durch", "die", "Schwe\u00b7re", ",", "so", "mich", "dr\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wonniglich in meiner Brust.", "tokens": ["Won\u00b7nig\u00b7lich", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df, o Welt, o la\u00df mich seyn!", "tokens": ["La\u00df", ",", "o", "Welt", ",", "o", "la\u00df", "mich", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "FM", "VVIMP", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Locket nicht mit Liebesgaben!", "tokens": ["Lo\u00b7cket", "nicht", "mit", "Lie\u00b7bes\u00b7ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft dies Herz alleine haben", "tokens": ["La\u00dft", "dies", "Herz", "al\u00b7lei\u00b7ne", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDS", "NN", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Wonne, seine Pein!", "tokens": ["Sei\u00b7ne", "Won\u00b7ne", ",", "sei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}