"word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Mein Herz in den Weltraum", "tokens": ["Mein", "Herz", "in", "den", "Welt\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Als Erde h\u00e4ngen,", "tokens": ["Als", "Er\u00b7de", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Als siedende Erde.", "tokens": ["Als", "sie\u00b7den\u00b7de", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}