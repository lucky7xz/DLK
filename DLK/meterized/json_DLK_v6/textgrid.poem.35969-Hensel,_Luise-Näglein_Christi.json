{"textgrid.poem.35969": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4glein Christi", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Heiland ausgelitten,", "tokens": ["Als", "der", "Hei\u00b7land", "aus\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Legten sie den Leib in's Grab", "tokens": ["Leg\u00b7ten", "sie", "den", "Leib", "in's", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die blut'gen N\u00e4gel fielen", "tokens": ["Und", "die", "blut'\u00b7gen", "N\u00e4\u00b7gel", "fie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das junge Gras herab.", "tokens": ["In", "das", "jun\u00b7ge", "Gras", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und da sind wir still entsprossen,", "tokens": ["Und", "da", "sind", "wir", "still", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Blutig roth aus gr\u00fcnem Gras,", "tokens": ["Blu\u00b7tig", "roth", "aus", "gr\u00fc\u00b7nem", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit reicher Nahrung tr\u00e4nkte", "tokens": ["Und", "mit", "rei\u00b7cher", "Nah\u00b7rung", "tr\u00e4nk\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der Mutterthr\u00e4nen Na\u00df.", "tokens": ["Uns", "der", "Mut\u00b7ter\u00b7thr\u00e4\u00b7nen", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein frommer Christ schaut sinnend", "tokens": ["Und", "ein", "from\u00b7mer", "Christ", "schaut", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Bl\u00fcthe tiefes Roth,", "tokens": ["Uns\u00b7rer", "Bl\u00fc\u00b7the", "tie\u00b7fes", "Roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft drauf seine Z\u00e4hre thauen", "tokens": ["L\u00e4\u00dft", "drauf", "sei\u00b7ne", "Z\u00e4h\u00b7re", "thau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gedenkt des Herren Tod.", "tokens": ["Und", "ge\u00b7denkt", "des", "Her\u00b7ren", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}