{"textgrid.poem.48340": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6niggr\u00e4tz", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie h\u00f6hnten uns, sie glaubten es zu d\u00fcrfen;", "tokens": ["Sie", "h\u00f6hn\u00b7ten", "uns", ",", "sie", "glaub\u00b7ten", "es", "zu", "d\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Langmut war, sie nahmen's hin als Schw\u00e4che,", "tokens": ["Was", "Lang\u00b7mut", "war", ",", "sie", "nah\u00b7men's", "hin", "als", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie warfen uns, zerdeutelt und zerrissen,", "tokens": ["Sie", "war\u00b7fen", "uns", ",", "zer\u00b7deu\u00b7telt", "und", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versprechen und Vertr\u00e4ge vor die F\u00fc\u00dfe,", "tokens": ["Ver\u00b7spre\u00b7chen", "und", "Ver\u00b7tr\u00e4\u00b7ge", "vor", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und als in Ruh wir dann das Wort gesprochen:", "tokens": ["Und", "als", "in", "Ruh", "wir", "dann", "das", "Wort", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbla\u00dft uns, was ", "tokens": ["\u00bb", "la\u00dft", "uns", ",", "was"], "token_info": ["punct", "word", "word", "punct", "word"], "pos": ["$(", "VVIMP", "PPER", "$,", "PWS"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da drohten sie: \u00bbVersucht's, wir sind am Platz;", "tokens": ["Da", "droh\u00b7ten", "sie", ":", "\u00bb", "Ver\u00b7sucht's", ",", "wir", "sind", "am", "Platz", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NE", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es kost't euch Schlesien und die Grafschaft Glatz.\u00ab", "tokens": ["Es", "kost't", "euch", "Schle\u00b7si\u00b7en", "und", "die", "Graf\u00b7schaft", "Glatz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Das war zu viel. Es klang zur\u00fcck die Antwort:", "tokens": ["Das", "war", "zu", "viel", ".", "Es", "klang", "zu\u00b7r\u00fcck", "die", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "PIS", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "Und nieder von den Bergen Schlesiens, Sachsens,", "tokens": ["Und", "nie\u00b7der", "von", "den", "Ber\u00b7gen", "Schle\u00b7si\u00b7ens", ",", "Sach\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "ART", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf Wegen, die der Ruhm uns vorgezeichnet,", "tokens": ["Auf", "We\u00b7gen", ",", "die", "der", "Ruhm", "uns", "vor\u00b7ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An St\u00e4tten hin, die Siegesnamen tragen,", "tokens": ["An", "St\u00e4t\u00b7ten", "hin", ",", "die", "Sie\u00b7ge\u00b7sna\u00b7men", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In B\u00f6hmens Kessel stieg das Preu\u00dfenheer.", "tokens": ["In", "B\u00f6h\u00b7mens", "Kes\u00b7sel", "stieg", "das", "Preu\u00b7\u00dfen\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein hei\u00dfer Kessel! Manches Kriegeswetter", "tokens": ["Ein", "hei\u00b7\u00dfer", "Kes\u00b7sel", "!", "Man\u00b7ches", "Krie\u00b7ges\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Tag und Jahren, die nun r\u00fcckw\u00e4rts liegen,", "tokens": ["In", "Tag", "und", "Jah\u00b7ren", ",", "die", "nun", "r\u00fcck\u00b7w\u00e4rts", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat drin die Junihitze schon gebraut,", "tokens": ["Hat", "drin", "die", "Ju\u00b7ni\u00b7hit\u00b7ze", "schon", "ge\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch solche Wetter, wie sie jetzt sich t\u00fcrmen", "tokens": ["Doch", "sol\u00b7che", "Wet\u00b7ter", ",", "wie", "sie", "jetzt", "sich", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PPER", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Tag um Tag sich grollender entladen,", "tokens": ["Und", "Tag", "um", "Tag", "sich", "grol\u00b7len\u00b7der", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind selbst in diesem B\u00f6hmerkessel neu.", "tokens": ["Sind", "selbst", "in", "die\u00b7sem", "B\u00f6h\u00b7mer\u00b7kes\u00b7sel", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Zerbricht wie Glas die Eiserne Brigade;", "tokens": ["Zer\u00b7bricht", "wie", "Glas", "die", "Ei\u00b7ser\u00b7ne", "Bri\u00b7ga\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "H\u00e4lt L\u00f6we Steinmetz seine Beute fest;", "tokens": ["H\u00e4lt", "L\u00f6\u00b7we", "Stein\u00b7metz", "sei\u00b7ne", "Beu\u00b7te", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und h\u00fcgelan \u2013 Clam-Gallas mu\u00dfte fliehn \u2013", "tokens": ["Und", "h\u00fc\u00b7ge\u00b7lan", "\u2013", "Clam\u00b7Gal\u00b7las", "mu\u00df\u00b7te", "fliehn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "St\u00fcrmt Friedrich Karl die Stra\u00dfe von ", "tokens": ["St\u00fcrmt", "Fried\u00b7rich", "Karl", "die", "Stra\u00b7\u00dfe", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So stand das Spiel; ein siebenfaches Siegen", "tokens": ["So", "stand", "das", "Spiel", ";", "ein", "sie\u00b7ben\u00b7fa\u00b7ches", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In sieben Tagen. \u00bbWird der Sieg uns bleiben?\u00ab", "tokens": ["In", "sie\u00b7ben", "Ta\u00b7gen", ".", "\u00bb", "Wird", "der", "Sieg", "uns", "blei\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "$.", "$(", "VAFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So zwischen Furcht und Hoffnung ging die Frage;", "tokens": ["So", "zwi\u00b7schen", "Furcht", "und", "Hoff\u00b7nung", "ging", "die", "Fra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch fehlte die Entscheidung, doch sie kam.", "tokens": ["Noch", "fehl\u00b7te", "die", "Ent\u00b7schei\u00b7dung", ",", "doch", "sie", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da, wo die Elbe, die sich nordw\u00e4rts windet,", "tokens": ["Da", ",", "wo", "die", "El\u00b7be", ",", "die", "sich", "nord\u00b7w\u00e4rts", "win\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NE", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf kurze Strecke wieder s\u00fcdw\u00e4rts flie\u00dft,", "tokens": ["Auf", "kur\u00b7ze", "Stre\u00b7cke", "wie\u00b7der", "s\u00fcd\u00b7w\u00e4rts", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf weitem Feld, umstellt von H\u00fcgelkuppen,", "tokens": ["Auf", "wei\u00b7tem", "Feld", ",", "um\u00b7stellt", "von", "H\u00fc\u00b7gel\u00b7kup\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei Festung ", "tokens": ["Bei", "Fes\u00b7tung"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "An st\u00fcrmen unter Trommelklang und Pfeifen", "tokens": ["An", "st\u00fcr\u00b7men", "un\u00b7ter", "Trom\u00b7mel\u00b7klang", "und", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Altmark, Magdeburg die Regimenter,", "tokens": ["Von", "Alt\u00b7mark", ",", "Mag\u00b7de\u00b7burg", "die", "Re\u00b7gi\u00b7men\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Th\u00fcring'sche Bataillone, dicht geschlossen,", "tokens": ["Th\u00fc\u00b7ring'\u00b7sche", "Ba\u00b7tail\u00b7lo\u00b7ne", ",", "dicht", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Sie folgen unter Hurra \u2013 all vergeblich;", "tokens": ["Sie", "fol\u00b7gen", "un\u00b7ter", "Hur\u00b7ra", "\u2013", "all", "ver\u00b7geb\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie dringen vor, sie jubeln und sie fall'n.", "tokens": ["Sie", "drin\u00b7gen", "vor", ",", "sie", "ju\u00b7beln", "und", "sie", "fall'", "n."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVINF", "KON", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Regen f\u00e4llt in Str\u00f6men, schon ist Mittag,", "tokens": ["Der", "Re\u00b7gen", "f\u00e4llt", "in", "Str\u00f6\u00b7men", ",", "schon", "ist", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbwo bleiben sie?\u00ab Es fragen's nicht die Lippen,", "tokens": ["\u00bb", "wo", "blei\u00b7ben", "sie", "?", "\u00ab", "Es", "fra\u00b7gen's", "nicht", "die", "Lip\u00b7pen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es fragt's nur still das Herz. Da horch, von Westen", "tokens": ["Es", "fragt's", "nur", "still", "das", "Herz", ".", "Da", "horch", ",", "von", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$.", "ADV", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und nun von Osten her in raschen Schl\u00e4gen,", "tokens": ["Und", "nun", "von", "Os\u00b7ten", "her", "in", "ra\u00b7schen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APZR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Roll'n unsre Preu\u00dfendonner durch die Luft.", "tokens": ["Roll'n", "uns\u00b7re", "Preu\u00b7\u00dfen\u00b7don\u00b7ner", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbdas sind sie!\u00ab geht ein Jubel durch die Reihen,", "tokens": ["\u00bb", "das", "sind", "sie", "!", "\u00ab", "geht", "ein", "Ju\u00b7bel", "durch", "die", "Rei\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "$.", "$(", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbdas ist das achte Korps! das sind die Garden!\u00ab", "tokens": ["\u00bb", "das", "ist", "das", "ach\u00b7te", "Korps", "!", "das", "sind", "die", "Gar\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und rechts und links des Feindes Flanke fassend,", "tokens": ["Und", "rechts", "und", "links", "des", "Fein\u00b7des", "Flan\u00b7ke", "fas\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "So reichen jetzt zwei neue Preu\u00dfenheere", "tokens": ["So", "rei\u00b7chen", "jetzt", "zwei", "neu\u00b7e", "Preu\u00b7\u00dfen\u00b7hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV"