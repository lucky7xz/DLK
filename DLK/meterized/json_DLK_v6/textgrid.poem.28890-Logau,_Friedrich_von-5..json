{"textgrid.poem.28890": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns zu Liebe wolte Christus Marter, Schmach und Tod erleiden;", "tokens": ["Uns", "zu", "Lie\u00b7be", "wol\u00b7te", "Chris\u00b7tus", "Mar\u00b7ter", ",", "Schmach", "und", "Tod", "er\u00b7lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VMFIN", "NE", "NE", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihm zu dancken wollen Christen Marter, Schmach und Tod vermeiden.", "tokens": ["Ihm", "zu", "dan\u00b7cken", "wol\u00b7len", "Chris\u00b7ten", "Mar\u00b7ter", ",", "Schmach", "und", "Tod", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VMFIN", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wann nun aber Christus wird kummen in der Herrligkeit,", "tokens": ["Wann", "nun", "a\u00b7ber", "Chris\u00b7tus", "wird", "kum\u00b7men", "in", "der", "Herr\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NE", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wird, wer weit vom Leiden blieb, auch von Freuden bleiben weit.", "tokens": ["Wird", ",", "wer", "weit", "vom", "Lei\u00b7den", "blieb", ",", "auch", "von", "Freu\u00b7den", "blei\u00b7ben", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "ADJD", "APPRART", "NN", "VVFIN", "$,", "ADV", "APPR", "NN", "VVFIN", "ADJD", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}