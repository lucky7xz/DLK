{"dta.poem.11713": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe, sagt man, steht am Pfahl gebunden,", "tokens": ["Die", "Lie\u00b7be", ",", "sagt", "man", ",", "steht", "am", "Pfahl", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "$,", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht endlich arm, verlassen, unbeschuht,", "tokens": ["Geht", "end\u00b7lich", "arm", ",", "ver\u00b7las\u00b7sen", ",", "un\u00b7be\u00b7schuht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dies edle Haupt hat nicht mehr, wo es ruht,", "tokens": ["Dies", "ed\u00b7le", "Haupt", "hat", "nicht", "mehr", ",", "wo", "es", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihren Thr\u00e4nen nezt sie bittre Wunden.", "tokens": ["Mit", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "nezt", "sie", "bitt\u00b7re", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, Peregrinen hab' ich so gefunden!", "tokens": ["Ach", ",", "Pe\u00b7reg\u00b7ri\u00b7nen", "hab'", "ich", "so", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6n war ihr Wahnsinn, ihrer Wange Gluth,", "tokens": ["Sch\u00f6n", "war", "ihr", "Wahn\u00b7sinn", ",", "ih\u00b7rer", "Wan\u00b7ge", "Gluth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch scherzend in der Fr\u00fchlingsst\u00fcrme Wuth,", "tokens": ["Noch", "scher\u00b7zend", "in", "der", "Fr\u00fch\u00b7lings\u00b7st\u00fcr\u00b7me", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wilde Kr\u00e4nze in das Haar gewunden.", "tokens": ["Und", "wil\u00b7de", "Kr\u00e4n\u00b7ze", "in", "das", "Haar", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie? solche Sch\u00f6nheit konntest du verlassen?", "tokens": ["Wie", "?", "sol\u00b7che", "Sch\u00f6n\u00b7heit", "konn\u00b7test", "du", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So kehrt nun doppelt sch\u00f6n das alte Gl\u00fcck!", "tokens": ["So", "kehrt", "nun", "dop\u00b7pelt", "sch\u00f6n", "das", "al\u00b7te", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O komm', in diese Arme dich zu fassen!", "tokens": ["O", "komm'", ",", "in", "die\u00b7se", "Ar\u00b7me", "dich", "zu", "fas\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PDAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch weh'! o weh'! was soll mir dieser Blick?", "tokens": ["Doch", "weh'", "!", "o", "weh'", "!", "was", "soll", "mir", "die\u00b7ser", "Blick", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "FM", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie k\u00fc\u00dft mich zwischen Lieben, zwischen Hassen,", "tokens": ["Sie", "k\u00fc\u00dft", "mich", "zwi\u00b7schen", "Lie\u00b7ben", ",", "zwi\u00b7schen", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie kehrt sich ab \u2014 und kehrt mir nie zur\u00fcck.", "tokens": ["Sie", "kehrt", "sich", "ab", "und", "kehrt", "mir", "nie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}