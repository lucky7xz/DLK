{"dta.poem.13488": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mit meinem Saitenspiele,", "tokens": ["Mit", "mei\u00b7nem", "Sai\u00b7ten\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sch\u00f6n geklungen hat,", "tokens": ["Das", "sch\u00f6n", "ge\u00b7klun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm' ich durch L\u00e4nder viele", "tokens": ["Komm'", "ich", "durch", "L\u00e4n\u00b7der", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck in diese Stadt.", "tokens": ["Zu\u00b7r\u00fcck", "in", "die\u00b7se", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich ziehe durch die Gassen,", "tokens": ["Ich", "zie\u00b7he", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So finster ist die Nacht,", "tokens": ["So", "fins\u00b7ter", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles so verlassen,", "tokens": ["Und", "al\u00b7les", "so", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hatt's anders mir gedacht.", "tokens": ["Hatt's", "an\u00b7ders", "mir", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Brunnen steh ich lange,", "tokens": ["Am", "Brun\u00b7nen", "steh", "ich", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der rauscht fort, wie vorher,", "tokens": ["Der", "rauscht", "fort", ",", "wie", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt mancher wohl gegangen\u201a", "tokens": ["Kommt", "man\u00b7cher", "wohl", "ge\u00b7gan\u00b7gen", "\u201a"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es kennt mich keiner mehr.", "tokens": ["Es", "kennt", "mich", "kei\u00b7ner", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da h\u00f6rt' ich geigen, pfeifen,", "tokens": ["Da", "h\u00f6rt'", "ich", "gei\u00b7gen", ",", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fenster gl\u00e4nzten weit,", "tokens": ["Die", "Fens\u00b7ter", "gl\u00e4nz\u00b7ten", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dazwischen drehn und schleifen", "tokens": ["Da\u00b7zwi\u00b7schen", "drehn", "und", "schlei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel' fremde, fr\u00f6hliche Leut'.", "tokens": ["Viel'", "frem\u00b7de", ",", "fr\u00f6h\u00b7li\u00b7che", "Leut'", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und Herz und Sinne mir brannten\u201a", "tokens": ["Und", "Herz", "und", "Sin\u00b7ne", "mir", "brann\u00b7ten", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich trieb's in die weite Welt,", "tokens": ["Mich", "trie\u00b7b's", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es spielten die Musikanten,", "tokens": ["Es", "spiel\u00b7ten", "die", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da fiel ich hin im Feld.", "tokens": ["Da", "fiel", "ich", "hin", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}