{"textgrid.poem.57105": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dieser zarte Leib hat mich geboren;", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser zarte Leib hat mich geboren;", "tokens": ["Die\u00b7ser", "zar\u00b7te", "Leib", "hat", "mich", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "grausam dr\u00e4ngt' ich mich aus seinem Scho\u00df,", "tokens": ["grau\u00b7sam", "dr\u00e4ngt'", "ich", "mich", "aus", "sei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ri\u00df mein Leben von dem seinen los,", "tokens": ["ri\u00df", "mein", "Le\u00b7ben", "von", "dem", "sei\u00b7nen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "hab' ihn hinter mir in Nacht verloren.", "tokens": ["hab'", "ihn", "hin\u00b7ter", "mir", "in", "Nacht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kehrst du nie zur\u00fcck, auch nicht im Geiste?", "tokens": ["Kehrst", "du", "nie", "zu\u00b7r\u00fcck", ",", "auch", "nicht", "im", "Geis\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bist du mir gestorben ewiglich?", "tokens": ["Bist", "du", "mir", "ge\u00b7stor\u00b7ben", "e\u00b7wig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und doch gab es eine Zeit: da kreiste", "tokens": ["Und", "doch", "gab", "es", "ei\u00b7ne", "Zeit", ":", "da", "kreis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "deines Herzens Blut durch dich ", "tokens": ["dei\u00b7nes", "Her\u00b7zens", "Blut", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}