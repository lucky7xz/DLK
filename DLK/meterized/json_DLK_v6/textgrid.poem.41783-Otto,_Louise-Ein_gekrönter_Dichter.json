{"textgrid.poem.41783": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Ein gekr\u00f6nter Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Augsburg zog der ritterliche S\u00e4nger", "tokens": ["Gen", "Augs\u00b7burg", "zog", "der", "rit\u00b7ter\u00b7li\u00b7che", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ulrich von Hutten, aus dem fernen S\u00fcd;", "tokens": ["Ul\u00b7rich", "von", "Hut\u00b7ten", ",", "aus", "dem", "fer\u00b7nen", "S\u00fcd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Italien mag dulden den nicht l\u00e4nger,", "tokens": ["I\u00b7ta\u00b7li\u00b7en", "mag", "dul\u00b7den", "den", "nicht", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "ART", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der f\u00fcr die Wahrheit und die Freiheit gl\u00fcht.", "tokens": ["Der", "f\u00fcr", "die", "Wahr\u00b7heit", "und", "die", "Frei\u00b7heit", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Meuchler Dolche sind f\u00fcr ihn gedungen,", "tokens": ["Der", "Meuch\u00b7ler", "Dol\u00b7che", "sind", "f\u00fcr", "ihn", "ge\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im goldnen Becher sch\u00e4umt ein t\u00f6dlich Gift,", "tokens": ["Im", "gold\u00b7nen", "Be\u00b7cher", "sch\u00e4umt", "ein", "t\u00f6d\u00b7lich", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Anathem' von strengen Priesterzungen", "tokens": ["Ein", "A\u00b7na\u00b7them'", "von", "stren\u00b7gen", "Pries\u00b7ter\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wirft in den Bann des Neurers Lied und Schrift.", "tokens": ["Wirft", "in", "den", "Bann", "des", "Neu\u00b7rers", "Lied", "und", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "So kehrt er zu dem deutschen Vaterlande", "tokens": ["So", "kehrt", "er", "zu", "dem", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein treuster Sohn, und vor ihm zieht sein Ruf,", "tokens": ["Sein", "treus\u00b7ter", "Sohn", ",", "und", "vor", "ihm", "zieht", "sein", "Ruf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Name, den Begeistrung preisend nannte,", "tokens": ["Der", "Na\u00b7me", ",", "den", "Be\u00b7geis\u00b7trung", "prei\u00b7send", "nann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ihm sein Lied, sein Heldentum erschuf.", "tokens": ["Den", "ihm", "sein", "Lied", ",", "sein", "Hel\u00b7den\u00b7tum", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Augsburgs hochgelahrter B\u00fcrgermeister,", "tokens": ["Und", "Augs\u00b7burgs", "hoch\u00b7ge\u00b7lahr\u00b7ter", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Peutinger, \u00f6ffnet gastlich ihm sein Haus,", "tokens": ["Peu\u00b7tin\u00b7ger", ",", "\u00f6ff\u00b7net", "gast\u00b7lich", "ihm", "sein", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Hochehrend gr\u00fc\u00dfen ihn verwandte Geister,", "tokens": ["Ho\u00b7ch\u00b7eh\u00b7rend", "gr\u00fc\u00b7\u00dfen", "ihn", "ver\u00b7wand\u00b7te", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "In alle Lande schallt sein Ruhm hinaus.", "tokens": ["In", "al\u00b7le", "Lan\u00b7de", "schallt", "sein", "Ruhm", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein langer Festzug wogt durch Augsburgs Gassen", "tokens": ["Ein", "lan\u00b7ger", "Fest\u00b7zug", "wogt", "durch", "Augs\u00b7burgs", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei stolzer Rythmen jubelndem Geleit,", "tokens": ["Bei", "stol\u00b7zer", "Ry\u00b7th\u00b7men", "ju\u00b7beln\u00b7dem", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Jauchzen von der Reichsstadt Volkesmassen", "tokens": ["Ein", "Jauch\u00b7zen", "von", "der", "Reichs\u00b7stadt", "Vol\u00b7kes\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Preist laut des deutschen Kaisers Herrlichkeit.", "tokens": ["Preist", "laut", "des", "deut\u00b7schen", "Kai\u00b7sers", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum Reichstag war der Kaiser Max gekommen,", "tokens": ["Zum", "Reichs\u00b7tag", "war", "der", "Kai\u00b7ser", "Max", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der, noch ein Feuergeist im Silberhaar,", "tokens": ["Der", ",", "noch", "ein", "Feu\u00b7er\u00b7geist", "im", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von allen F\u00fcrsten, Habsburg Stamm entglommen", "tokens": ["Von", "al\u00b7len", "F\u00fcrs\u00b7ten", ",", "Habs\u00b7burg", "Stamm", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der edelste, der Held' und Dichter war.", "tokens": ["Der", "e\u00b7dels\u00b7te", ",", "der", "Held'", "und", "Dich\u00b7ter", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Den Helden und den Dichter will er gr\u00fc\u00dfen,", "tokens": ["Den", "Hel\u00b7den", "und", "den", "Dich\u00b7ter", "will", "er", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ulrich von Hutten, dessen k\u00fchner Sang", "tokens": ["Ul\u00b7rich", "von", "Hut\u00b7ten", ",", "des\u00b7sen", "k\u00fch\u00b7ner", "Sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Heldenthaten Anrecht wohl verhie\u00dfen", "tokens": ["Und", "Hel\u00b7den\u00b7tha\u00b7ten", "An\u00b7recht", "wohl", "ver\u00b7hie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einen Dank des Helden Theuerdank.", "tokens": ["Auf", "ei\u00b7nen", "Dank", "des", "Hel\u00b7den", "Theu\u00b7er\u00b7dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Trompetensto\u00df und drauf des Herolds Kunde:", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7sto\u00df", "und", "drauf", "des", "He\u00b7rolds", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Ritter schlug des Kaisers eigne Hand", "tokens": ["Zum", "Rit\u00b7ter", "schlug", "des", "Kai\u00b7sers", "eig\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vor aller F\u00fcrsten feierlicher Runde,", "tokens": ["Vor", "al\u00b7ler", "F\u00fcrs\u00b7ten", "fei\u00b7er\u00b7li\u00b7cher", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Hutten, der begeistert vor ihm stand.", "tokens": ["Den", "Hut\u00b7ten", ",", "der", "be\u00b7geis\u00b7tert", "vor", "ihm", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er beugt sein Knie zu einem s\u00fc\u00dfern Lohne:", "tokens": ["Er", "beugt", "sein", "Knie", "zu", "ei\u00b7nem", "s\u00fc\u00b7\u00dfern", "Loh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Peutingers Tochter naht, die sch\u00f6nste Maid,", "tokens": ["Peu\u00b7tin\u00b7gers", "Toch\u00b7ter", "naht", ",", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und dr\u00fcckt aufs Haupt ihm eine Lorberkrone,", "tokens": ["Und", "dr\u00fcckt", "aufs", "Haupt", "ihm", "ei\u00b7ne", "Lor\u00b7ber\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Dichter feierlich zum Ruhm geweiht.", "tokens": ["Dem", "Dich\u00b7ter", "fei\u00b7er\u00b7lich", "zum", "Ruhm", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da loht Begeist'rung hell in seinen Blicken,", "tokens": ["Da", "loht", "Be\u00b7geist'\u00b7rung", "hell", "in", "sei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er k\u00fc\u00dft das Schwert, das ihn zum Ritter schlug,", "tokens": ["Er", "k\u00fc\u00dft", "das", "Schwert", ",", "das", "ihn", "zum", "Rit\u00b7ter", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er k\u00fc\u00dft die wei\u00dfe Hand im Hochentz\u00fccken,", "tokens": ["Er", "k\u00fc\u00dft", "die", "wei\u00b7\u00dfe", "Hand", "im", "Ho\u00b7chent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die ihm den Lorberkranz entgegentrug.", "tokens": ["Die", "ihm", "den", "Lor\u00b7ber\u00b7kranz", "ent\u00b7ge\u00b7gen\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie reich an Wonne diese ", "tokens": ["Wie", "reich", "an", "Won\u00b7ne", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein deutscher Kaiser, selbst ein Dichterheld,", "tokens": ["Sein", "deut\u00b7scher", "Kai\u00b7ser", ",", "selbst", "ein", "Dich\u00b7ter\u00b7held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein deutsches Volk jauchzt ihm mit einem Munde,", "tokens": ["Sein", "deut\u00b7sches", "Volk", "jauchzt", "ihm", "mit", "ei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Name klingt hinaus in alle Welt.", "tokens": ["Sein", "Na\u00b7me", "klingt", "hin\u00b7aus", "in", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und vor ihm sie, die minnigliche Sch\u00f6ne,", "tokens": ["Und", "vor", "ihm", "sie", ",", "die", "min\u00b7nig\u00b7li\u00b7che", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die liebdurchgl\u00fcht ihm reicht den Lorberkranz", "tokens": ["Die", "lieb\u00b7durch\u00b7gl\u00fcht", "ihm", "reicht", "den", "Lor\u00b7ber\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er weiht das deutsche M\u00e4dchen zur Kam\u00f6ne,", "tokens": ["Er", "weiht", "das", "deut\u00b7sche", "M\u00e4d\u00b7chen", "zur", "Ka\u00b7m\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie strahlt sein Aug' von sel'gem Himmelsglanz!", "tokens": ["Wie", "strahlt", "sein", "Aug'", "von", "sel'\u00b7gem", "Him\u00b7mels\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"t