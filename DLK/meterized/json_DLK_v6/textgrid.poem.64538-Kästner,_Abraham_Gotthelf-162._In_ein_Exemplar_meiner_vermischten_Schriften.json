{"textgrid.poem.64538": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "162. In ein Exemplar meiner vermischten Schriften", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Richter, den nichts blenden kann,", "tokens": ["Ein", "Rich\u00b7ter", ",", "den", "nichts", "blen\u00b7den", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem Voltaire zittern m\u00fcssen,", "tokens": ["Vor", "dem", "Vol\u00b7tai\u00b7re", "zit\u00b7tern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Pr\u00fcft bald mein Buch, und tadelt nichts daran;", "tokens": ["Pr\u00fcft", "bald", "mein", "Buch", ",", "und", "ta\u00b7delt", "nichts", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er hei\u00dft: Des Sterbenden Gewissen.", "tokens": ["Er", "hei\u00dft", ":", "Des", "Ster\u00b7ben\u00b7den", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}