{"textgrid.poem.35342": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wandelst fort und fort,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandelst fort und fort,", "tokens": ["Wan\u00b7delst", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Stolze Schweiz, auf deiner lichten Bahn;", "tokens": ["Stol\u00b7ze", "Schweiz", ",", "auf", "dei\u00b7ner", "lich\u00b7ten", "Bahn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Baust auf Gott, wenn deine Feinde nah'n,", "tokens": ["Baust", "auf", "Gott", ",", "wenn", "dei\u00b7ne", "Fein\u00b7de", "nah'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht auf Menschenwort.", "tokens": ["Nicht", "auf", "Men\u00b7schen\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gro\u00dfes ist geschehn,", "tokens": ["Gro\u00b7\u00dfes", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und in deiner freien Berge Luft", "tokens": ["Und", "in", "dei\u00b7ner", "frei\u00b7en", "Ber\u00b7ge", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Soll kein b\u00f6ser Geist aus finstrer Gruft", "tokens": ["Soll", "kein", "b\u00f6\u00b7ser", "Geist", "aus", "finst\u00b7rer", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wieder auferstehn.", "tokens": ["Wie\u00b7der", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmutig aufgeschaut!\u00ab", "tokens": ["\u00bb", "mu\u00b7tig", "auf\u00b7ge\u00b7schaut", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hunderttausendfacher B\u00fcchsenknall", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send\u00b7fa\u00b7cher", "B\u00fcch\u00b7sen\u00b7knall"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zu den Eidgenossen \u00fcberall", "tokens": ["Zu", "den", "Eid\u00b7ge\u00b7nos\u00b7sen", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bringt den Donnerlaut,", "tokens": ["Bringt", "den", "Don\u00b7ner\u00b7laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie wunderhold", "tokens": ["Und", "wie", "wun\u00b7der\u00b7hold"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Lemans blauer Flut ein Bild entsteigt,", "tokens": ["Le\u00b7mans", "blau\u00b7er", "Flut", "ein", "Bild", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das durch Pulverrauch sich niederneigt", "tokens": ["Das", "durch", "Pul\u00b7ver\u00b7rauch", "sich", "nie\u00b7der\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "PRF", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus der Wolken Gold.", "tokens": ["Aus", "der", "Wol\u00b7ken", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Von der Alpen Fu\u00df", "tokens": ["Von", "der", "Al\u00b7pen", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hin zu dir, Helvetiens Lust und Zier,", "tokens": ["Hin", "zu", "dir", ",", "Hel\u00b7ve\u00b7ti\u00b7ens", "Lust", "und", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und zu deinem friedlichen Turnier", "tokens": ["Und", "zu", "dei\u00b7nem", "fried\u00b7li\u00b7chen", "Tur\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fliegt mein S\u00e4ngergru\u00df.", "tokens": ["Fliegt", "mein", "S\u00e4n\u00b7ger\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch, o Mutter Zeit!", "tokens": ["Doch", ",", "o", "Mut\u00b7ter", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Herzen, die dein milder Hauch belebt,", "tokens": ["Her\u00b7zen", ",", "die", "dein", "mil\u00b7der", "Hauch", "be\u00b7lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00e4hle, wenn sich fremde List erhebt,", "tokens": ["St\u00e4h\u00b7le", ",", "wenn", "sich", "frem\u00b7de", "List", "er\u00b7hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Waffne sie zum Streit.", "tokens": ["Waff\u00b7ne", "sie", "zum", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Rufe sie zur Pflicht,", "tokens": ["Ru\u00b7fe", "sie", "zur", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vaterland! und deiner S\u00f6hne Bund", "tokens": ["Va\u00b7ter\u00b7land", "!", "und", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Bund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Folgt dem wei\u00dfen Kreuz auf rotem Grund", "tokens": ["Folgt", "dem", "wei\u00b7\u00dfen", "Kreuz", "auf", "ro\u00b7tem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Nacht zum Licht.", "tokens": ["Durch", "die", "Nacht", "zum", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}