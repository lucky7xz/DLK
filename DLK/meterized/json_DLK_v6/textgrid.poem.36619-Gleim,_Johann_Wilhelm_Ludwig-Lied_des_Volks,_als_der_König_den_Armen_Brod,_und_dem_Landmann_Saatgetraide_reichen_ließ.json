{"textgrid.poem.36619": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Volks, als der K\u00f6nig den Armen Brod, und dem Landmann Saatgetraide reichen lie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig lebe! denn er sitzt", "tokens": ["Der", "K\u00f6\u00b7nig", "le\u00b7be", "!", "denn", "er", "sitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Thron, ein Vater, itzt,", "tokens": ["Auf", "sei\u00b7nem", "Thron", ",", "ein", "Va\u00b7ter", ",", "itzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht Hungersnoth,", "tokens": ["Sieht", "Hun\u00b7gers\u00b7noth", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sieht unsern Tod,", "tokens": ["Sieht", "un\u00b7sern", "Tod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und sorgt f\u00fcr uns, und giebt uns Brod.", "tokens": ["Und", "sorgt", "f\u00fcr", "uns", ",", "und", "giebt", "uns", "Brod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und giebt uns, Lebensfroh zu seyn,", "tokens": ["Und", "giebt", "uns", ",", "Le\u00b7bens\u00b7froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch dr\u00fcber, Saamen auszustreun,", "tokens": ["Noch", "dr\u00fc\u00b7ber", ",", "Saa\u00b7men", "aus\u00b7zu\u00b7streun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht uns an,", "tokens": ["Und", "sieht", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der gute Mann!", "tokens": ["Der", "gu\u00b7te", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und unser Dank steigt Himmelan.", "tokens": ["Und", "un\u00b7ser", "Dank", "steigt", "Him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sieht uns an, und freuet sich;", "tokens": ["Er", "sieht", "uns", "an", ",", "und", "freu\u00b7et", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir segnen unsern ", "tokens": ["Wir", "seg\u00b7nen", "un\u00b7sern"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wir segnen ihn,", "tokens": ["Wir", "seg\u00b7nen", "ihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wir segnen ihn,", "tokens": ["Wir", "seg\u00b7nen", "ihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wir, seine Kinder, segnen ihn!", "tokens": ["Wir", ",", "sei\u00b7ne", "Kin\u00b7der", ",", "seg\u00b7nen", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Vater war er allemal,", "tokens": ["Ein", "Va\u00b7ter", "war", "er", "al\u00b7le\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Hungersnoth, und Lebensquaal,", "tokens": ["Wenn", "Hun\u00b7gers\u00b7noth", ",", "und", "Le\u00b7bens\u00b7qua\u00b7al", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Von Gott gesandt,", "tokens": ["Von", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das Vaterland", "tokens": ["Das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schwer dr\u00fcckte, nieder in den Sand!", "tokens": ["Schwer", "dr\u00fcck\u00b7te", ",", "nie\u00b7der", "in", "den", "Sand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Held war er in Krieg und Streit,", "tokens": ["Ein", "Held", "war", "er", "in", "Krieg", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Held ist er in Friedenszeit,", "tokens": ["Ein", "Held", "ist", "er", "in", "Frie\u00b7dens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aller Welt", "tokens": ["Und", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ist er ein Held,", "tokens": ["Ist", "er", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit dem man gerne Frieden h\u00e4lt.", "tokens": ["Mit", "dem", "man", "ger\u00b7ne", "Frie\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Halt ihn mit ihm, du Nachbarschaft!", "tokens": ["Halt", "ihn", "mit", "ihm", ",", "du", "Nach\u00b7bar\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst f\u00fchlst du seines Armes Kraft,", "tokens": ["Sonst", "f\u00fchlst", "du", "sei\u00b7nes", "Ar\u00b7mes", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst f\u00fchlst du schwer", "tokens": ["Sonst", "f\u00fchlst", "du", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Geist, den er", "tokens": ["Den", "Geist", ",", "den", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von Gott empfieng, und keiner mehr", "tokens": ["Von", "Gott", "emp\u00b7fi\u00b7eng", ",", "und", "kei\u00b7ner", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "PIS", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wohl! da\u00df er unser K\u00f6nig ist!", "tokens": ["Wohl", "!", "da\u00df", "er", "un\u00b7ser", "K\u00f6\u00b7nig", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt, ob ihr einen bessern wi\u00dft?", "tokens": ["Sagt", ",", "ob", "ihr", "ei\u00b7nen", "bes\u00b7sern", "wi\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sagt ihr: Nein!", "tokens": ["Und", "sagt", "ihr", ":", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So stimmt mit ein:", "tokens": ["So", "stimmt", "mit", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "War' er es nicht; so sollt' er's seyn!", "tokens": ["Wa\u00b7r'", "er", "es", "nicht", ";", "so", "sollt'", "er's", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "$.", "ADV", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}