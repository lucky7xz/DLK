{"textgrid.poem.36768": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Natur! welch Studium ist's, das kostet des Schwei\u00dfes,", "tokens": ["Wel\u00b7che", "Na\u00b7tur", "!", "welch", "Stu\u00b7di\u00b7um", "ist's", ",", "das", "kos\u00b7tet", "des", "Schwei\u00b7\u00dfes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "PWAT", "NN", "VAFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Kostet der Tage so viel, ach und das Geld ist so rar.", "tokens": ["Kos\u00b7tet", "der", "Ta\u00b7ge", "so", "viel", ",", "ach", "und", "das", "Geld", "ist", "so", "rar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "$,", "XY", "KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Was nur der Ultramarin mich ge\u00e4ngstiget! Doch ist's der Himmel,", "tokens": ["Was", "nur", "der", "Ult\u00b7ra\u00b7ma\u00b7rin", "mich", "ge\u00b7\u00e4ngs\u00b7ti\u00b7get", "!", "Doch", "ist's", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PPER", "VVPP", "$.", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ist es das Ganze, der Geist, was ich erziele, noch nicht.", "tokens": ["Ist", "es", "das", "Gan\u00b7ze", ",", "der", "Geist", ",", "was", "ich", "er\u00b7zie\u00b7le", ",", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nein! der Vordergrund ist's, ob dem ich m\u00f6chte verzweifeln,", "tokens": ["Nein", "!", "der", "Vor\u00b7der\u00b7grund", "ist's", ",", "ob", "dem", "ich", "m\u00f6ch\u00b7te", "ver\u00b7zwei\u00b7feln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "$,", "KOUS", "ART", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Sieben Monate schon schaff' ich mit Eifer daran.", "tokens": ["Sie\u00b7ben", "Mo\u00b7na\u00b7te", "schon", "schaff'", "ich", "mit", "Ei\u00b7fer", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "PAV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Aber es gl\u00fcckt, und ich mache nun doch zwei gem\u00fcthliche Bl\u00fcmlein", "tokens": ["A\u00b7ber", "es", "gl\u00fcckt", ",", "und", "ich", "ma\u00b7che", "nun", "doch", "zwei", "ge\u00b7m\u00fcth\u00b7li\u00b7che", "Bl\u00fcm\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "CARD", "ADJA", "NN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "T\u00e4glich, und schon hab' ich hundert und zehen gemacht.", "tokens": ["T\u00e4g\u00b7lich", ",", "und", "schon", "hab'", "ich", "hun\u00b7dert", "und", "ze\u00b7hen", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADV", "VAFIN", "PPER", "CARD", "KON", "CARD", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}