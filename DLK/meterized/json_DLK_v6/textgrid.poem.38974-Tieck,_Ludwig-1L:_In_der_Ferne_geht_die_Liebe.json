{"textgrid.poem.38974": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Ferne geht die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Ferne geht die Liebe", "tokens": ["In", "der", "Fer\u00b7ne", "geht", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungekannt durch Nacht und Schatten;", "tokens": ["Un\u00b7ge\u00b7kannt", "durch", "Nacht", "und", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! wozu, da\u00df ich hier bliebe", "tokens": ["Ach", "!", "wo\u00b7zu", ",", "da\u00df", "ich", "hier", "blie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den vaterl\u00e4ndschen Matten?", "tokens": ["Auf", "den", "va\u00b7ter\u00b7l\u00e4nd\u00b7schen", "Mat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie mit s\u00fc\u00dfen Fl\u00f6tenstimmen", "tokens": ["Wie", "mit", "s\u00fc\u00b7\u00dfen", "Fl\u00f6\u00b7ten\u00b7stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufen alle goldnen Sterne:", "tokens": ["Ru\u00b7fen", "al\u00b7le", "gold\u00b7nen", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbweit mu\u00df manche Woge schwimmen,", "tokens": ["\u00bb", "weit", "mu\u00df", "man\u00b7che", "Wo\u00b7ge", "schwim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Lieb' ist in der Ferne.", "tokens": ["Dei\u00b7ne", "Lieb'", "ist", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jenes Bild vor dem du knietest,", "tokens": ["Je\u00b7nes", "Bild", "vor", "dem", "du", "knie\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich ihm ganz zu eigen gabst,", "tokens": ["Dich", "ihm", "ganz", "zu", "ei\u00b7gen", "gabst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm mit allen Sinnen gl\u00fchtest,", "tokens": ["Ihm", "mit", "al\u00b7len", "Sin\u00b7nen", "gl\u00fch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Schatten dich erlabst. \u2013", "tokens": ["An", "dem", "Schat\u00b7ten", "dich", "er\u00b7labst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was dein Geist als Zukunft dachte,", "tokens": ["Was", "dein", "Geist", "als", "Zu\u00b7kunft", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Entz\u00fccken Kunst genannt,", "tokens": ["Dein", "Ent\u00b7z\u00fc\u00b7cken", "Kunst", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was als Morgenroth dir lachte,", "tokens": ["Was", "als", "Mor\u00b7gen\u00b7roth", "dir", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft sich wieder abgewandt:", "tokens": ["Oft", "sich", "wie\u00b7der", "ab\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie nur ist es! Dein Verzagen", "tokens": ["Sie", "nur", "ist", "es", "!", "Dein", "Ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sie fort von dir gescheucht,", "tokens": ["Hat", "sie", "fort", "von", "dir", "ge\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du es nur m\u00e4nnlich wagen,", "tokens": ["Willst", "du", "es", "nur", "m\u00e4nn\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird das Ziel noch einst erreicht.", "tokens": ["Wird", "das", "Ziel", "noch", "einst", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Ketten sind gesprungen,", "tokens": ["Al\u00b7le", "Ket\u00b7ten", "sind", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und befreit ist dann dein Geist.", "tokens": ["Und", "be\u00b7freit", "ist", "dann", "dein", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Knechtschaft k\u00fchn entschwungen", "tokens": ["Je\u00b7der", "Knecht\u00b7schaft", "k\u00fchn", "ent\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlst du dich nicht mehr verwaist.", "tokens": ["F\u00fchlst", "du", "dich", "nicht", "mehr", "ver\u00b7waist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "R\u00fcckw\u00e4rts flieht das zage Bangen,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "flieht", "das", "za\u00b7ge", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Muse reicht die Hand,", "tokens": ["Und", "die", "Mu\u00b7se", "reicht", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchret sicher das Verlangen", "tokens": ["F\u00fch\u00b7ret", "si\u00b7cher", "das", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der G\u00f6tter Himmelsland.\u00ab", "tokens": ["In", "der", "G\u00f6t\u00b7ter", "Him\u00b7mels\u00b7land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, wer darf mit Kunst und Liebe", "tokens": ["Ja", ",", "wer", "darf", "mit", "Kunst", "und", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWS", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Sterblichen sich messen?", "tokens": ["Von", "den", "Sterb\u00b7li\u00b7chen", "sich", "mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem sch\u00f6nverm\u00e4hlten Triebe", "tokens": ["In", "dem", "sch\u00f6n\u00b7ver\u00b7m\u00e4hl\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Himmel selbst besessen!", "tokens": ["Wird", "der", "Him\u00b7mel", "selbst", "be\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}