{"textgrid.poem.26183": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Bl\u00fctensporen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom abgebl\u00fchten Flu\u00dfschilf fliegen die Bl\u00fctensporen himmelan,", "tokens": ["Vom", "ab\u00b7ge\u00b7bl\u00fch\u00b7ten", "Flu\u00df\u00b7schilf", "flie\u00b7gen", "die", "Bl\u00fc\u00b7tens\u00b7po\u00b7ren", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Schweben wie wei\u00dfe Federn bis zur Stadt heran", "tokens": ["Schwe\u00b7ben", "wie", "wei\u00b7\u00dfe", "Fe\u00b7dern", "bis", "zur", "Stadt", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJA", "NN", "APPR", "APPRART", "NN", "PTKVZ"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und wandern an den H\u00e4usern hin zu jedermann.", "tokens": ["Und", "wan\u00b7dern", "an", "den", "H\u00e4u\u00b7sern", "hin", "zu", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie segeln tagelang wie hingehauchter Flaum,", "tokens": ["Sie", "se\u00b7geln", "ta\u00b7ge\u00b7lang", "wie", "hin\u00b7ge\u00b7hauch\u00b7ter", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tauchen und sinken spielend durch den Raum", "tokens": ["Tau\u00b7chen", "und", "sin\u00b7ken", "spie\u00b7lend", "durch", "den", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und kommen weither von den Ufern \u00fcbern Flu\u00df,", "tokens": ["Und", "kom\u00b7men", "weit\u00b7her", "von", "den", "U\u00b7fern", "\u00fc\u00b7bern", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ob das Leben ewig wandern mu\u00df.", "tokens": ["Als", "ob", "das", "Le\u00b7ben", "e\u00b7wig", "wan\u00b7dern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADJD", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es fliegt den Wiesen die verliebte Jugend fort,", "tokens": ["Es", "fliegt", "den", "Wie\u00b7sen", "die", "ver\u00b7lieb\u00b7te", "Ju\u00b7gend", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Bl\u00fctensporn verweht, wie manch begeistert Wort.", "tokens": ["Der", "Bl\u00fc\u00b7tens\u00b7porn", "ver\u00b7weht", ",", "wie", "manch", "be\u00b7geis\u00b7tert", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}