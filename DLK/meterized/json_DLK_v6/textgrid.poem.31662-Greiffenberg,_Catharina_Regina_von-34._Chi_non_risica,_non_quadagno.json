{"textgrid.poem.31662": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "34. Chi non risica, non quadagno", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nichts waget / nicht gewinnet. Sehet Alexander an /", "tokens": ["Wer", "nichts", "wa\u00b7get", "/", "nicht", "ge\u00b7win\u00b7net", ".", "Se\u00b7het", "A\u00b7lex\u00b7an\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$(", "PTKNEG", "VVPP", "$.", "VVFIN", "NE", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "und die k\u00fchnen Amazonen / was das wagen nutzen kan!", "tokens": ["und", "die", "k\u00fch\u00b7nen", "A\u00b7maz\u00b7o\u00b7nen", "/", "was", "das", "wa\u00b7gen", "nut\u00b7zen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "PWS", "PDS", "VVINF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "drum so waget euer Gl\u00fccke / euern Leib und euer Leben:", "tokens": ["drum", "so", "wa\u00b7get", "eu\u00b7er", "Gl\u00fc\u00b7cke", "/", "eu\u00b7ern", "Leib", "und", "eu\u00b7er", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "wann ihr wolt gewinnen was / und beherzte Helden geben.", "tokens": ["wann", "ihr", "wolt", "ge\u00b7win\u00b7nen", "was", "/", "und", "be\u00b7herz\u00b7te", "Hel\u00b7den", "ge\u