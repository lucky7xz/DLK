{"textgrid.poem.64813": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ein Kranker schlummernd liegt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein Kranker schlummernd liegt,", "tokens": ["Wenn", "ein", "Kran\u00b7ker", "schlum\u00b7mernd", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mild von Traumesarm gewiegt,", "tokens": ["Mild", "von", "Trau\u00b7mes\u00b7arm", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigen Alle im Gemache,", "tokens": ["Schwei\u00b7gen", "Al\u00b7le", "im", "Ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Arme nicht erwache.", "tokens": ["Da\u00df", "der", "Ar\u00b7me", "nicht", "er\u00b7wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leis' ihr Hauch und stumm ihr Mund,", "tokens": ["Leis'", "ihr", "Hauch", "und", "stumm", "ihr", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum ber\u00fchrt ihr Fu\u00df den Grund \u2013", "tokens": ["Kaum", "be\u00b7r\u00fchrt", "ihr", "Fu\u00df", "den", "Grund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Kranke schlummert weiter", "tokens": ["Und", "der", "Kran\u00b7ke", "schlum\u00b7mert", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhbeseligt, traumesheiter.", "tokens": ["Ruh\u00b7be\u00b7se\u00b7ligt", ",", "trau\u00b7mes\u00b7hei\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Innig fleh' ich jetzt zu dir:", "tokens": ["In\u00b7nig", "fleh'", "ich", "jetzt", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halte du es so mit mir,", "tokens": ["Hal\u00b7te", "du", "es", "so", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem tiefersch\u00f6pften Herzen,", "tokens": ["Mit", "dem", "tie\u00b7fer\u00b7sch\u00f6pf\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das entschlummert ist voll Schmerzen.", "tokens": ["Das", "ent\u00b7schlum\u00b7mert", "ist", "voll", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VAFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Halb verblutet schl\u00e4ft es fort;", "tokens": ["Halb", "ver\u00b7blu\u00b7tet", "schl\u00e4ft", "es", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weck' es nicht mit Deinem Wort!", "tokens": ["Weck'", "es", "nicht", "mit", "Dei\u00b7nem", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trage schonendes Erbarmen", "tokens": ["Tra\u00b7ge", "scho\u00b7nen\u00b7des", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Kranken, M\u00fcden, Armen!", "tokens": ["Mit", "dem", "Kran\u00b7ken", ",", "M\u00fc\u00b7den", ",", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Willst du's wecken, sei's zum Gl\u00fcck;", "tokens": ["Willst", "du's", "we\u00b7cken", ",", "sei's", "zum", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kannst du die\u00df nicht, tritt zur\u00fcck!", "tokens": ["Kannst", "du", "die\u00df", "nicht", ",", "tritt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "PTKNEG", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gie\u00dfe Gift nicht in die Neige", "tokens": ["Gie\u00b7\u00dfe", "Gift", "nicht", "in", "die", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Lebens! schweige! schweige!", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "!", "schwei\u00b7ge", "!", "schwei\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}