{"dta.poem.3324": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die ruhige Unschuld.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Strahl der Fr\u00f6hligkeit", "tokens": ["Ein", "Strahl", "der", "Fr\u00f6h\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erheitert meine Stirn auch in der b\u00f6sen Zeit,", "tokens": ["Er\u00b7hei\u00b7tert", "mei\u00b7ne", "Stirn", "auch", "in", "der", "b\u00f6\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Inde\u00df aus grauenvollen B\u00fcschen", "tokens": ["In\u00b7de\u00df", "aus", "grau\u00b7en\u00b7vol\u00b7len", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll ungetreuer Dunkelheit,", "tokens": ["Voll", "un\u00b7ge\u00b7treu\u00b7er", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nattern der Verl\u00e4umdung zischen.", "tokens": ["Die", "Nat\u00b7tern", "der", "Ver\u00b7l\u00e4um\u00b7dung", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie lauert f\u00fcrchterlich,", "tokens": ["Sie", "lau\u00b7ert", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Still, wie die Mitternacht: ihr K\u00f6cher leeret sich", "tokens": ["Still", ",", "wie", "die", "Mit\u00b7ter\u00b7nacht", ":", "ihr", "K\u00f6\u00b7cher", "lee\u00b7ret", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Pfeilen, die verderblich gl\u00fchen,", "tokens": ["Von", "Pfei\u00b7len", ",", "die", "ver\u00b7derb\u00b7lich", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Funken rings um mich,", "tokens": ["Und", "ih\u00b7re", "Fun\u00b7ken", "rings", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entz\u00fcndet in der H\u00f6lle, spr\u00fchen.", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "in", "der", "H\u00f6l\u00b7le", ",", "spr\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu meinem Schutze flammt", "tokens": ["Zu", "mei\u00b7nem", "Schut\u00b7ze", "flammt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Unschuld feurig Schild! ich werd umsonst verdammt:", "tokens": ["Der", "Un\u00b7schuld", "feu\u00b7rig", "Schild", "!", "ich", "werd", "um\u00b7sonst", "ver\u00b7dammt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tugend hat mich losgesprochen,", "tokens": ["Die", "Tu\u00b7gend", "hat", "mich", "los\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da Schm\u00e4hsucht, die vom Neide stammt,", "tokens": ["Da", "Schm\u00e4h\u00b7sucht", ",", "die", "vom", "Nei\u00b7de", "stammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir t\u00fcckischfl\u00fcsternd nachgekrochen.", "tokens": ["Mir", "t\u00fc\u00b7ckischfl\u00fcs\u00b7ternd", "nach\u00b7ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es f\u00e4llt des L\u00e4strers Zahn", "tokens": ["Es", "f\u00e4llt", "des", "L\u00e4st\u00b7rers", "Zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Weisen Sch\u00e4tze nicht, nur seine Puppen an,", "tokens": ["Des", "Wei\u00b7sen", "Sch\u00e4t\u00b7ze", "nicht", ",", "nur", "sei\u00b7ne", "Pup\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Puppen unsrer Kinderjahre,", "tokens": ["Die", "Pup\u00b7pen", "uns\u00b7rer", "Kin\u00b7der\u00b7jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdr\u00e4ngt uns auf der Ehre Bahn,", "tokens": ["Ver\u00b7dr\u00e4ngt", "uns", "auf", "der", "Eh\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nagt am Lorbeer unsrer Haare.", "tokens": ["Und", "nagt", "am", "Lor\u00b7beer", "uns\u00b7rer", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich schwing an deiner Hand,", "tokens": ["Ich", "schwing", "an", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Weisheit! mich empor, hoch \u00fcber stolzen Tand,", "tokens": ["O", "Weis\u00b7heit", "!", "mich", "em\u00b7por", ",", "hoch", "\u00fc\u00b7ber", "stol\u00b7zen", "Tand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und kurzen Sonnenschein des Gl\u00fcckes,", "tokens": ["Und", "kur\u00b7zen", "Son\u00b7nen\u00b7schein", "des", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seiner Freuden Unbestand,", "tokens": ["Und", "sei\u00b7ner", "Freu\u00b7den", "Un\u00b7be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Freuden eines Augenblickes.", "tokens": ["Nur", "Freu\u00b7den", "ei\u00b7nes", "Au\u00b7gen\u00b7bli\u00b7ckes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es br\u00fcllt aus dicker Nacht", "tokens": ["Es", "br\u00fcllt", "aus", "di\u00b7cker", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Donner unter mir, inde\u00df mir Titan lacht,", "tokens": ["Der", "Don\u00b7ner", "un\u00b7ter", "mir", ",", "in\u00b7de\u00df", "mir", "Ti\u00b7tan", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und reine L\u00fcfte mich umwehen,", "tokens": ["Und", "rei\u00b7ne", "L\u00fcf\u00b7te", "mich", "um\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber giftigen Verdacht", "tokens": ["Und", "\u00fc\u00b7ber", "gif\u00b7ti\u00b7gen", "Ver\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und niedre Schm\u00e4hsucht mich erh\u00f6hen.", "tokens": ["Und", "nied\u00b7re", "Schm\u00e4h\u00b7sucht", "mich", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hoch in den Wolken fleugt", "tokens": ["Hoch", "in", "den", "Wol\u00b7ken", "fleugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der Adler, wo ein Blick ihm ferne Raben zeigt,", "tokens": ["Der", "Ad\u00b7ler", ",", "wo", "ein", "Blick", "ihm", "fer\u00b7ne", "Ra\u00b7ben", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sich beym Aas geschw\u00e4tzig freuen:", "tokens": ["Die", "sich", "beym", "Aas", "ge\u00b7schw\u00e4t\u00b7zig", "freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der k\u00f6nigliche Vogel schweigt,", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Vo\u00b7gel", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00e4\u00dft die tr\u00e4gen Thiere schreyen.", "tokens": ["Und", "l\u00e4\u00dft", "die", "tr\u00e4\u00b7gen", "Thie\u00b7re", "schre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}