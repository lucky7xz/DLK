{"dta.poem.9965": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man mich mit menschen-fleische f\u00fcllet und mit bein und", "tokens": ["Wenn", "man", "mich", "mit", "men\u00b7schen\u00b7flei\u00b7sche", "f\u00fcl\u00b7let", "und", "mit", "bein", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ADJA", "VVFIN", "KON", "APPR", "VVIMP", "KON"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "haut/", "tokens": ["haut", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dien\u2019 ich dem aus allen kr\u00e4fften/ welcher arbeit mir vertraut:", "tokens": ["Dien'", "ich", "dem", "aus", "al\u00b7len", "kr\u00e4ff\u00b7ten", "/", "wel\u00b7cher", "ar\u00b7beit", "mir", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "APPR", "PIAT", "NN", "$(", "PWAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber/ wen\u0303 mein bauch nicht voll/ n\u00fctz ich nichts an keinem orte/", "tokens": ["A\u00b7ber", "/", "we\u00f1", "mein", "bauch", "nicht", "voll", "/", "n\u00fctz", "ich", "nichts", "an", "kei\u00b7nem", "or\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "$(", "VVIMP", "PPER", "PIS", "APPR", "PIAT", "NN", "$("], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.5": {"text": "Giebt man mir freywillig nichts/ so verliehr ich keine worte.", "tokens": ["Giebt", "man", "mir", "frey\u00b7wil\u00b7lig", "nichts", "/", "so", "ver\u00b7liehr", "ich", "kei\u00b7ne", "wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "PIS", "$(", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+--++-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}