{"textgrid.poem.30995": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deiner S\u00fcnden menge beichten,", "tokens": ["Dei\u00b7ner", "S\u00fcn\u00b7den", "men\u00b7ge", "beich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan die S\u00fcnden Last zwar leichten;", "tokens": ["Kan", "die", "S\u00fcn\u00b7den", "Last", "zwar", "leich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schau, da\u00df Heucheley", "tokens": ["A\u00b7ber", "schau", ",", "da\u00df", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zu Steinen lege Bley.", "tokens": ["Nicht", "zu", "Stei\u00b7nen", "le\u00b7ge", "Bley", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}