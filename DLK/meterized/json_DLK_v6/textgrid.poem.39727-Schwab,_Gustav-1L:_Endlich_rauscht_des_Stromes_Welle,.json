{"textgrid.poem.39727": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich rauscht des Stromes Welle,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich rauscht des Stromes Welle,", "tokens": ["End\u00b7lich", "rauscht", "des", "Stro\u00b7mes", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so fremd mir klang, vertraut;", "tokens": ["Die", "so", "fremd", "mir", "klang", ",", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Berg und Th\u00e4ler schauen helle,", "tokens": ["Berg", "und", "Th\u00e4\u00b7ler", "schau\u00b7en", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Geist der Flur wird laut.", "tokens": ["Und", "der", "Geist", "der", "Flur", "wird", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Heimat ist's in meiner Seele,", "tokens": ["Hei\u00b7mat", "ist's", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimisch wird mir nun das Land;", "tokens": ["Hei\u00b7misch", "wird", "mir", "nun", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seit ich selbst mir nicht mehr fehle,", "tokens": ["Seit", "ich", "selbst", "mir", "nicht", "mehr", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Find' ich Alles rings verwandt.", "tokens": ["Find'", "ich", "Al\u00b7les", "rings", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja das macht, ich trag' im Herzen", "tokens": ["Ja", "das", "macht", ",", "ich", "trag'", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PDS", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder nun ein liebes Bild:", "tokens": ["Wie\u00b7der", "nun", "ein", "lie\u00b7bes", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was verh\u00fcllt lag unter Schmerzen,", "tokens": ["Was", "ver\u00b7h\u00fcllt", "lag", "un\u00b7ter", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt mit ihm hervor so mild.", "tokens": ["Tritt", "mit", "ihm", "her\u00b7vor", "so", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Augen f\u00e4llt die Blindheit,", "tokens": ["Von", "den", "Au\u00b7gen", "f\u00e4llt", "die", "Blind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Feld und Wald im alten Schein", "tokens": ["Feld", "und", "Wald", "im", "al\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Laden mich, wie in der Kindheit,", "tokens": ["La\u00b7den", "mich", ",", "wie", "in", "der", "Kind\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit den trauten Stimmen ein.", "tokens": ["Mit", "den", "trau\u00b7ten", "Stim\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoffnung f\u00fchrt mich auf die Fluren,", "tokens": ["Hoff\u00b7nung", "f\u00fchrt", "mich", "auf", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich sonst nur irr durchstreift;", "tokens": ["Die", "ich", "sonst", "nur", "irr", "durch\u00b7streift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O und nach geliebten Spuren", "tokens": ["O", "und", "nach", "ge\u00b7lieb\u00b7ten", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberall mein Auge schweift!", "tokens": ["Ue\u00b7be\u00b7rall", "mein", "Au\u00b7ge", "schweift", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Weg, der zu ihr gehet,", "tokens": ["Je\u00b7der", "Weg", ",", "der", "zu", "ihr", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist mir wie schon l\u00e4ngst bekannt;", "tokens": ["Ist", "mir", "wie", "schon", "l\u00e4ngst", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder Boden, drauf sie stehet,", "tokens": ["Je\u00b7der", "Bo\u00b7den", ",", "drauf", "sie", "ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist mein altes Vaterland.", "tokens": ["Ist", "mein", "al\u00b7tes", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}