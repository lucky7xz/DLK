{"textgrid.poem.55961": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Graf von Brederode", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie folgten furchtbar; ihren bunten Tod", "tokens": ["Sie", "folg\u00b7ten", "furcht\u00b7bar", ";", "ih\u00b7ren", "bun\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von ferne nach ihm werfend, w\u00e4hrend er", "tokens": ["von", "fer\u00b7ne", "nach", "ihm", "wer\u00b7fend", ",", "w\u00e4h\u00b7rend", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verloren floh, nichts weiter als: bedroht.", "tokens": ["ver\u00b7lo\u00b7ren", "floh", ",", "nichts", "wei\u00b7ter", "als", ":", "be\u00b7droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "PIS", "ADV", "KOKOM", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Ferne seiner V\u00e4ter schien nicht mehr", "tokens": ["Die", "Fer\u00b7ne", "sei\u00b7ner", "V\u00e4\u00b7ter", "schien", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "f\u00fcr ihn zu gelten; denn um so zu fliehn,", "tokens": ["f\u00fcr", "ihn", "zu", "gel\u00b7ten", ";", "denn", "um", "so", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$.", "KON", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gen\u00fcgt ein Tier vor J\u00e4gern. Bis der Flu\u00df", "tokens": ["ge\u00b7n\u00fcgt", "ein", "Tier", "vor", "J\u00e4\u00b7gern", ".", "Bis", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aufrauschte nah und blitzend. Ein Entschlu\u00df", "tokens": ["auf\u00b7rauschte", "nah", "und", "blit\u00b7zend", ".", "Ein", "Ent\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "hob ihn samt seiner Not und machte ihn", "tokens": ["hob", "ihn", "samt", "sei\u00b7ner", "Not", "und", "mach\u00b7te", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "wieder zum Knaben f\u00fcrstlichen Gebl\u00fctes.", "tokens": ["wie\u00b7der", "zum", "Kna\u00b7ben", "f\u00fcrst\u00b7li\u00b7chen", "Ge\u00b7bl\u00fc\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein L\u00e4cheln adeliger Frauen go\u00df", "tokens": ["Ein", "L\u00e4\u00b7cheln", "a\u00b7de\u00b7li\u00b7ger", "Frau\u00b7en", "go\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "noch einmal S\u00fc\u00dfigkeit in sein verfr\u00fchtes", "tokens": ["noch", "ein\u00b7mal", "S\u00fc\u00b7\u00dfig\u00b7keit", "in", "sein", "ver\u00b7fr\u00fch\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "vollendetes Gesicht. Er zwang sein Ro\u00df,", "tokens": ["voll\u00b7en\u00b7de\u00b7tes", "Ge\u00b7sicht", ".", "Er", "zwang", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gro\u00df wie sein Herz zu gehn, sein blutdurchgl\u00fchtes:", "tokens": ["gro\u00df", "wie", "sein", "Herz", "zu", "gehn", ",", "sein", "blut\u00b7durch\u00b7gl\u00fch\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es trug ihn in den Strom wie in sein Schlo\u00df.", "tokens": ["es", "trug", "ihn", "in", "den", "Strom", "wie", "in", "sein", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}