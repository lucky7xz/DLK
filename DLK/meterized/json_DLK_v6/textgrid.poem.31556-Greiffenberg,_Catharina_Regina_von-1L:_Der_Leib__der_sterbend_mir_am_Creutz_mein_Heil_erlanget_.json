{"textgrid.poem.31556": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Leib / der sterbend mir am Creutz mein Heil erlanget /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Leib / der sterbend mir am Creutz mein Heil erlanget /", "tokens": ["Der", "Leib", "/", "der", "ster\u00b7bend", "mir", "am", "Creutz", "mein", "Heil", "er\u00b7lan\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und der am Thabor zeigt sein Klarheits Herrlichkeit /", "tokens": ["und", "der", "am", "Tha\u00b7bor", "zeigt", "sein", "Klar\u00b7heits", "Herr\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in dem die Gottheit schwebt vollkommen jederzeit:", "tokens": ["in", "dem", "die", "Got\u00b7theit", "schwebt", "voll\u00b7kom\u00b7men", "je\u00b7der\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist eben der / der jetzt die Zung' im Brod empfanget.", "tokens": ["Ist", "e\u00b7ben", "der", "/", "der", "jetzt", "die", "Zung'", "im", "Brod", "emp\u00b7fan\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "$(", "ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus seinen Wunden gleich mein Mund sein Blut auffanget.", "tokens": ["Aus", "sei\u00b7nen", "Wun\u00b7den", "gleich", "mein", "Mund", "sein", "Blut", "auf\u00b7fan\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein k\u00f6stlich Mahl f\u00fcr mich / im Brod und Wein bereit!", "tokens": ["Ein", "k\u00f6st\u00b7lich", "Mahl", "f\u00fcr", "mich", "/", "im", "Brod", "und", "Wein", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "$(", "APPRART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ob die Erforschung schon sich strecket nicht so weit /", "tokens": ["ob", "die", "Er\u00b7for\u00b7schung", "schon", "sich", "stre\u00b7cket", "nicht", "so", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "VVFIN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der Glaub doch mit der Fahn der Warheit herrlich pranget.", "tokens": ["der", "Glaub", "doch", "mit", "der", "Fahn", "der", "War\u00b7heit", "herr\u00b7lich", "pran\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach da\u00df doch nicht der Leib vor Furcht und Zittern bebt /", "tokens": ["Ach", "da\u00df", "doch", "nicht", "der", "Leib", "vor", "Furcht", "und", "Zit\u00b7tern", "bebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in dem' er den aufnimmt / den selbst mit Zittern ehren", "tokens": ["in", "dem'", "er", "den", "auf\u00b7nimmt", "/", "den", "selbst", "mit", "Zit\u00b7tern", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "VVFIN", "$(", "ART", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die liechten Seraphim / die Thronen / F\u00fcrstentum.", "tokens": ["die", "liech\u00b7ten", "Se\u00b7ra\u00b7phim", "/", "die", "Thro\u00b7nen", "/", "F\u00fcrs\u00b7ten\u00b7tum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach! da\u00df er nicht im See der Buse-Thr\u00e4nen schwebt /", "tokens": ["Ach", "!", "da\u00df", "er", "nicht", "im", "See", "der", "Bu\u00b7se\u00b7Thr\u00e4\u00b7nen", "schwebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "zu reinigen ein Herz zur Wohnung diesem HERREN!", "tokens": ["zu", "rei\u00b7ni\u00b7gen", "ein", "Herz", "zur", "Woh\u00b7nung", "die\u00b7sem", "HeR\u00b7REN", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "da\u00df er in Noht-Abgrund sich senket / ist sein Ruhm.", "tokens": ["da\u00df", "er", "in", "Noht\u00b7Ab\u00b7grund", "sich", "sen\u00b7ket", "/", "ist", "sein", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "VVFIN", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}