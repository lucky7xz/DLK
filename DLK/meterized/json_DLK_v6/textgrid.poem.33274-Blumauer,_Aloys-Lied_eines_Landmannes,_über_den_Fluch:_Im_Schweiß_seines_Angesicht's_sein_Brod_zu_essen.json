{"textgrid.poem.33274": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines Landmannes, \u00fcber den Fluch: Im Schwei\u00df seines Angesicht's sein Brod zu essen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei meinem Eid! mir schmecket nichts,", "tokens": ["Bei", "mei\u00b7nem", "Eid", "!", "mir", "schme\u00b7cket", "nichts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als was im Schwei\u00df des Angesichts", "tokens": ["Als", "was", "im", "Schwei\u00df", "des", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich selbst gepflanzet habe;", "tokens": ["Ich", "selbst", "ge\u00b7pflan\u00b7zet", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwar i\u00dft sich auch der Reiche satt;", "tokens": ["Zwar", "i\u00dft", "sich", "auch", "der", "Rei\u00b7che", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein das Brod hei\u00dft in der Stadt", "tokens": ["Al\u00b7lein", "das", "Brod", "hei\u00dft", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gar selten Gottesgabe:", "tokens": ["Gar", "sel\u00b7ten", "Got\u00b7tes\u00b7ga\u00b7be", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "D'rum schl\u00e4gt es auch dem reichen Mann", "tokens": ["D'\u00b7rum", "schl\u00e4gt", "es", "auch", "dem", "rei\u00b7chen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Daselbst so wunderselten an,", "tokens": ["Da\u00b7selbst", "so", "wun\u00b7der\u00b7sel\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mag sein Mahl mir preisen,", "tokens": ["Er", "mag", "sein", "Mahl", "mir", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich dank'. Er sitzt dabei und flucht", "tokens": ["Ich", "dank'", ".", "Er", "sitzt", "da\u00b7bei", "und", "flucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PAV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Unverdaulichkeit, und sucht", "tokens": ["Der", "Un\u00b7ver\u00b7dau\u00b7lich\u00b7keit", ",", "und", "sucht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Hunger in den Speisen.", "tokens": ["Den", "Hun\u00b7ger", "in", "den", "Spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Narr! er wird ihn nimmermehr,", "tokens": ["Der", "Narr", "!", "er", "wird", "ihn", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sucht er ihn auch noch so sehr,", "tokens": ["Und", "sucht", "er", "ihn", "auch", "noch", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seiner Sch\u00fcssel finden;", "tokens": ["In", "sei\u00b7ner", "Sch\u00fcs\u00b7sel", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seufzt er denn nach Appetit", "tokens": ["Und", "seufzt", "er", "denn", "nach", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So komm' er her, und helfe mit", "tokens": ["So", "komm'", "er", "her", ",", "und", "hel\u00b7fe", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Feld die Garben binden.", "tokens": ["Im", "Feld", "die", "Gar\u00b7ben", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so das nicht den Eckel bannt,", "tokens": ["Und", "so", "das", "nicht", "den", "E\u00b7ckel", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So nehm' er noch die Axt zur Hand,", "tokens": ["So", "nehm'", "er", "noch", "die", "Axt", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und haue mit uns Buchen,", "tokens": ["Und", "hau\u00b7e", "mit", "uns", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "D'rauf setz' er sich zum Milchtopf hin,", "tokens": ["D'\u00b7rauf", "setz'", "er", "sich", "zum", "Milch\u00b7topf", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und traun! es wird der Hunger ihn,", "tokens": ["Und", "traun", "!", "es", "wird", "der", "Hun\u00b7ger", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PPER", "VAFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht er den Hunger suchen.", "tokens": ["Nicht", "er", "den", "Hun\u00b7ger", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und nach gestilltem Appetit,", "tokens": ["Und", "nach", "ge\u00b7still\u00b7tem", "Ap\u00b7pe\u00b7tit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da braucht es wohl kein Wiegenlied,", "tokens": ["Da", "braucht", "es", "wohl", "kein", "Wie\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Herrn auch einzuwiegen:", "tokens": ["Den", "Herrn", "auch", "ein\u00b7zu\u00b7wie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es wird sich dann auf hartem Brett", "tokens": ["Es", "wird", "sich", "dann", "auf", "har\u00b7tem", "Brett"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel besser, als im Himmelbett", "tokens": ["Viel", "bes\u00b7ser", ",", "als", "im", "Him\u00b7mel\u00b7bett"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf weichen Pflaumen liegen.", "tokens": ["Auf", "wei\u00b7chen", "Pflau\u00b7men", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wei\u00df Gott, was all' f\u00fcr Weh und Leid", "tokens": ["Wei\u00df", "Gott", ",", "was", "all'", "f\u00fcr", "Weh", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Magen und im Eingeweid", "tokens": ["Im", "Ma\u00b7gen", "und", "im", "Ein\u00b7ge\u00b7weid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00fc\u00dfigg\u00e4nger klagen:", "tokens": ["Die", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur zur Mittags-und Abendszeit,", "tokens": ["Nur", "zur", "Mit\u00b7tags\u00b7\u00b7und", "A\u00b7bend\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wenn er nach Trank und Speise schreit,", "tokens": ["Wenn", "er", "nach", "Trank", "und", "Spei\u00b7se", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empfind' ich meinen Magen.", "tokens": ["Emp\u00b7find'", "ich", "mei\u00b7nen", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Arbeit ist zu jeder Zeit", "tokens": ["Die", "Ar\u00b7beit", "ist", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Appetit und Munterkeit", "tokens": ["Zu", "Ap\u00b7pe\u00b7tit", "und", "Mun\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00e4chte Wunderschl\u00fcssel;", "tokens": ["Der", "\u00e4ch\u00b7te", "Wun\u00b7der\u00b7schl\u00fcs\u00b7sel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "So voll auch Topf und Teller ist,", "tokens": ["So", "voll", "auch", "Topf", "und", "Tel\u00b7ler", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich leere sie, kein Eckel fri\u00dft", "tokens": ["Ich", "lee\u00b7re", "sie", ",", "kein", "E\u00b7ckel", "fri\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit mir aus meiner Sch\u00fcssel.", "tokens": ["Mit", "mir", "aus", "mei\u00b7ner", "Sch\u00fcs\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die B\u00e4ume, die ich pflanze, sind", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "die", "ich", "pflan\u00b7ze", ",", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieb mir als mein eigen Kind;", "tokens": ["So", "lieb", "mir", "als", "mein", "ei\u00b7gen", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so sie Frucht ansetzen;", "tokens": ["Und", "so", "sie", "Frucht", "an\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00fchr' ich meine Buben hin,", "tokens": ["So", "f\u00fchr'", "ich", "mei\u00b7ne", "Bu\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lasse sie mit frohem Sinn", "tokens": ["Und", "las\u00b7se", "sie", "mit", "fro\u00b7hem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daran die Gaumen letzen.", "tokens": ["Da\u00b7ran", "die", "Gau\u00b7men", "let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und so sie dann mit frohem Muth", "tokens": ["Und", "so", "sie", "dann", "mit", "fro\u00b7hem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir zuschrei'n: Vater, das ist gut!", "tokens": ["Mir", "zu\u00b7schrei'n", ":", "Va\u00b7ter", ",", "das", "ist", "gut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "NN", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sag' ich ihnen: Sehet,", "tokens": ["So", "sag'", "ich", "ih\u00b7nen", ":", "Se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist die Frucht der Arbeit hold!", "tokens": ["So", "ist", "die", "Frucht", "der", "Ar\u00b7beit", "hold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Kinder, wenn ihr ernten wollt,", "tokens": ["Doch", "Kin\u00b7der", ",", "wenn", "ihr", "ern\u00b7ten", "wollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gehet hin, und s\u00e4et!", "tokens": ["So", "ge\u00b7het", "hin", ",", "und", "s\u00e4et", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mein G\u00e4rtchen ist best\u00e4ndig voll,", "tokens": ["Mein", "G\u00e4rt\u00b7chen", "ist", "be\u00b7st\u00e4n\u00b7dig", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich darf mit Geld um Kraut und Kohl", "tokens": ["Ich", "darf", "mit", "Geld", "um", "Kraut", "und", "Kohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht erst zu Markte laufen:", "tokens": ["Nicht", "erst", "zu", "Mark\u00b7te", "lau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Zugem\u00fcs schmeckt doppelt s\u00fc\u00df;", "tokens": ["Mein", "Zu\u00b7ge\u00b7m\u00fcs", "schmeckt", "dop\u00b7pelt", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O w\u00fc\u00dften grosse Herren dies,", "tokens": ["O", "w\u00fc\u00df\u00b7ten", "gros\u00b7se", "Her\u00b7ren", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie w\u00fcrden es nicht kaufen.", "tokens": ["Sie", "w\u00fcr\u00b7den", "es", "nicht", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mein Kapital ist Arbeit blo\u00df,", "tokens": ["Mein", "Ka\u00b7pi\u00b7tal", "ist", "Ar\u00b7beit", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das leg' ich in der Erde Schoo\u00df", "tokens": ["Das", "leg'", "ich", "in", "der", "Er\u00b7de", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf hohe Zinsen nieder;", "tokens": ["Auf", "ho\u00b7he", "Zin\u00b7sen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und diese gibt mir allemal", "tokens": ["Und", "die\u00b7se", "gibt", "mir", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zinsen sammt dem Kapital", "tokens": ["Die", "Zin\u00b7sen", "sammt", "dem", "Ka\u00b7pi\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl hundertf\u00e4ltig wieder.", "tokens": ["Wohl", "hun\u00b7dert\u00b7f\u00e4l\u00b7tig", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und f\u00fchl' ich oft der Arbeit Druck,", "tokens": ["Und", "f\u00fchl'", "ich", "oft", "der", "Ar\u00b7beit", "Druck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will vom schwergehalt'nen Pflug", "tokens": ["Und", "will", "vom", "schwer\u00b7ge\u00b7halt'\u00b7nen", "Pflug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die matte Hand mir sinken,", "tokens": ["Die", "mat\u00b7te", "Hand", "mir", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So denk' ich meiner M\u00fche Lohn,", "tokens": ["So", "denk'", "ich", "mei\u00b7ner", "M\u00fc\u00b7he", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seh' voraus im Geiste schon", "tokens": ["Und", "seh'", "vo\u00b7raus", "im", "Geis\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die vollen Aehren winken.", "tokens": ["Die", "vol\u00b7len", "A\u00b7eh\u00b7ren", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Ich bin vergn\u00fcgt, und tauschte nicht,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", ",", "und", "tauschte", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was auch davon die Bibel spricht,", "tokens": ["Was", "auch", "da\u00b7von", "die", "Bi\u00b7bel", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Adams Paradiese:", "tokens": ["Mit", "A\u00b7dams", "Pa\u00b7ra\u00b7die\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wu\u00dfte nicht, was Arbeit war,", "tokens": ["Er", "wu\u00df\u00b7te", "nicht", ",", "was", "Ar\u00b7beit", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lag das liebe, lange Jahr", "tokens": ["Und", "lag", "das", "lie\u00b7be", ",", "lan\u00b7ge", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf seiner gr\u00fcnen Wiese.", "tokens": ["Auf", "sei\u00b7ner", "gr\u00fc\u00b7nen", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und war ihm, wenn er m\u00fcssig lag,", "tokens": ["Und", "war", "ihm", ",", "wenn", "er", "m\u00fcs\u00b7sig", "lag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie mir an einem Feiertag,", "tokens": ["Wie", "mir", "an", "ei\u00b7nem", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hab' ich nichts dagegen,", "tokens": ["So", "hab'", "ich", "nichts", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und denke mir: du lieber Gott!", "tokens": ["Und", "den\u00b7ke", "mir", ":", "du", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deinem Fluch hat's keine Noth,", "tokens": ["Mit", "dei\u00b7nem", "Fluch", "hat's", "kei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir ist er lauter Segen!", "tokens": ["Mir", "ist", "er", "lau\u00b7ter", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}