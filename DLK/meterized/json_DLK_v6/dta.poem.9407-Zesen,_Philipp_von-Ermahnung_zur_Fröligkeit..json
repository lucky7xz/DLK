}, "stanza.2": {"line.1": {"text": "Es mu\u00df uns der Sommer und Herbest auch dienen", "tokens": ["Es", "mu\u00df", "uns", "der", "Som\u00b7mer", "und", "Her\u00b7best", "auch", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ADV", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der unsere Kammer und Scheinen erf\u00fcllt;", "tokens": ["Der", "un\u00b7se\u00b7re", "Kam\u00b7mer", "und", "Schei\u00b7nen", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es geben uns Honig im Sommer die Bienen/", "tokens": ["Es", "ge\u00b7ben", "uns", "Ho\u00b7nig", "im", "Som\u00b7mer", "die", "Bie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Felder und W\u00e4lder Getreydich und Wild;", "tokens": ["Die", "Fel\u00b7der", "und", "W\u00e4l\u00b7der", "Ge\u00b7trey\u00b7dich", "und", "Wild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "KON", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Herbest mu\u00df allerley Obest uns geben/", "tokens": ["Der", "Her\u00b7best", "mu\u00df", "al\u00b7ler\u00b7ley", "O\u00b7best", "uns", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Bald \u00e4pfel/ bald Birnen/ bald ede