{"dta.poem.9407": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ermahnung zur Fr\u00f6ligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lentz/ Sommer/ Herbst/ Winter und andere zeiten/", "tokens": ["Lentz", "/", "Som\u00b7mer", "/", "Herbst", "/", "Win\u00b7ter", "und", "an\u00b7de\u00b7re", "zei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die m\u00fcssen uns dienen zur Nahrung und kost:", "tokens": ["Die", "m\u00fcs\u00b7sen", "uns", "die\u00b7nen", "zur", "Nah\u00b7rung", "und", "kost", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Fr\u00fchling l\u00e4sst Streuser und Kr\u00e4ntze bereiten/", "tokens": ["Der", "Fr\u00fch\u00b7ling", "l\u00e4sst", "Streu\u00b7ser", "und", "Kr\u00e4nt\u00b7ze", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gibt Blumen und Kreuter und \u00e4ndert den frost/", "tokens": ["Gibt", "Blu\u00b7men", "und", "Kreu\u00b7ter", "und", "\u00e4n\u00b7dert", "den", "frost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Jm Fr\u00fchling die Vogel sich nehren und mehren/", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "die", "Vo\u00b7gel", "sich", "neh\u00b7ren", "und", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVINF", "KON", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie singen und klingen und la\u00dfen sich h\u00f6ren/", "tokens": ["Sie", "sin\u00b7gen", "und", "klin\u00b7gen", "und", "la\u00b7\u00dfen", "sich", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Da\u00df Th\u00e4ler und W\u00e4lder und Felder zugleich/", "tokens": ["Da\u00df", "Th\u00e4\u00b7ler", "und", "W\u00e4l\u00b7der", "und", "Fel\u00b7der", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Da\u00df alles erschallet und wallet vor freuden!", "tokens": ["Da\u00df", "al\u00b7les", "er\u00b7schal\u00b7let", "und", "wal\u00b7let", "vor", "freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Wo Coridon/ Phillis und andere weiden;", "tokens": ["Wo", "Co\u00b7ri\u00b7don", "/", "Phil\u00b7lis", "und", "an\u00b7de\u00b7re", "wei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "NE", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10