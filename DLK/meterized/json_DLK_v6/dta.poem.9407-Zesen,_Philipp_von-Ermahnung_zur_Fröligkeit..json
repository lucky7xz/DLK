, "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da alles sich freuet und fr\u00f6lich erzeigt/", "tokens": ["Da", "al\u00b7les", "sich", "freu\u00b7et", "und", "fr\u00f6\u00b7lich", "er\u00b7zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVFIN", "KON", "ADJD", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Sonne viel h\u00f6her als sonsten aufsteigt/", "tokens": ["Die", "Son\u00b7ne", "viel", "h\u00f6\u00b7her", "als", "sons\u00b7ten", "auf\u00b7steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "ADV", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Winter vergehet/", "tokens": ["Der", "Win\u00b7ter", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Fr\u00fchling entstehet/", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ent\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die fr\u00f6liche zeit;", "tokens": ["Die", "fr\u00f6\u00b7li\u00b7che", "zeit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Der Wechsel der Zeiten uns h\u00f6chlich erfreut:", "tokens": ["Der", "Wech\u00b7sel", "der", "Zei\u00b7ten", "uns", "h\u00f6ch\u00b7lich", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter"