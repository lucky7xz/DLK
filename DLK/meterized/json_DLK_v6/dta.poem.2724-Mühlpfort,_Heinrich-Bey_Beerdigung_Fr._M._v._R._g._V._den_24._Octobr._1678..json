{"dta.poem.2724": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Fr. M. v. R. g. V. den 24.  \n Octobr. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch-Adeliches Hau\u00df/ wie? sollen Wermuts-Str\u00e4uche", "tokens": ["Hoch\u00b7A\u00b7de\u00b7li\u00b7ches", "Hau\u00df", "/", "wie", "?", "sol\u00b7len", "Wer\u00b7muts\u00b7Str\u00e4u\u00b7che"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "PWAV", "$.", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und blasses Eppich-Kraut bekleiden Schwell und", "tokens": ["Und", "blas\u00b7ses", "Ep\u00b7pich\u00b7Kraut", "be\u00b7klei\u00b7den", "Schwell", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Th\u00fcr?", "tokens": ["Th\u00fcr", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vergn\u00fcget sich der Tod nicht mit des ", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "sich", "der", "Tod", "nicht", "mit", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKNEG", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der unser Nestor war des gantzen Landes Zier?", "tokens": ["Der", "un\u00b7ser", "Nes\u00b7tor", "war", "des", "gant\u00b7zen", "Lan\u00b7des", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach nein! eh als das Jahr hat seinen Lauff vollzogen", "tokens": ["Ach", "nein", "!", "eh", "als", "das", "Jahr", "hat", "sei\u00b7nen", "Lauff", "voll\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$.", "KOUS", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Sonnen g\u00fcldnes Rad den Thier-Krei\u00dfdurchgerannt/", "tokens": ["Der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Rad", "den", "Thier\u00b7Krei\u00df\u00b7durch\u00b7ge\u00b7rannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So spannt der grim\u2019ge Tod schon abermal den Bogen", "tokens": ["So", "spannt", "der", "grim'\u00b7ge", "Tod", "schon", "a\u00b7ber\u00b7mal", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und legt ach herber Schmertz! die Mutter in den Sand.", "tokens": ["Und", "legt", "ach", "her\u00b7ber", "Schmertz", "!", "die", "Mut\u00b7ter", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird euer Rosenfeld zu Bergen voller Myrrhen?", "tokens": ["Wird", "eu\u00b7er", "Ro\u00b7sen\u00b7feld", "zu", "Ber\u00b7gen", "vol\u00b7ler", "Myr\u00b7rhen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und deckt Aegypten euch mit einer langen Nacht?", "tokens": ["Und", "deckt", "A\u00b7e\u00b7gyp\u00b7ten", "euch", "mit", "ei\u00b7ner", "lan\u00b7gen", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und trinckt ihr Thr\u00e4nen Saltz aus vollen Angst-Geschirren?", "tokens": ["Und", "trinckt", "ihr", "Thr\u00e4\u00b7nen", "Saltz", "aus", "vol\u00b7len", "Angst\u00b7Ge\u00b7schir\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist bey so vielem Leyd das Hertze nicht verschmacht?", "tokens": ["Ist", "bey", "so", "vie\u00b7lem", "Leyd", "das", "Hert\u00b7ze", "nicht", "ver\u00b7schmacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PIS", "NN", "PDS", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Steht doch die Welt betr\u00fcbt/ wenn itzt bey kurtzen Tagen", "tokens": ["Steht", "doch", "die", "Welt", "be\u00b7tr\u00fcbt", "/", "wenn", "itzt", "bey", "kurt\u00b7zen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$(", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Aurora nicht so fr\u00fch den blauen Himmel mahlt.", "tokens": ["Au\u00b7ro\u00b7ra", "nicht", "so", "fr\u00fch", "den", "blau\u00b7en", "Him\u00b7mel", "mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn B\u00e4ume/ Wald und Feld ihr Sommer-Kleid beklagen/", "tokens": ["Wenn", "B\u00e4u\u00b7me", "/", "Wald", "und", "Feld", "ihr", "Som\u00b7mer\u00b7Kleid", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und keine Rose mehr in Lust-Gefildern strahlt.", "tokens": ["Und", "kei\u00b7ne", "Ro\u00b7se", "mehr", "in", "Lust\u00b7Ge\u00b7fil\u00b7dern", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn Wild und Vogel fleucht/ der schwartze Himmel weinet/", "tokens": ["Wenn", "Wild", "und", "Vo\u00b7gel", "fleucht", "/", "der", "schwart\u00b7ze", "Him\u00b7mel", "wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NE", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sich der kalte Nord mit holem Pfeiffen zeigt.", "tokens": ["Und", "sich", "der", "kal\u00b7te", "Nord", "mit", "ho\u00b7lem", "Pfeif\u00b7fen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn die verdeckte Sonn kaum aus der Wolck\u2019 erscheinet/", "tokens": ["Wenn", "die", "ver\u00b7deck\u00b7te", "Sonn", "kaum", "aus", "der", "Wolck'", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und das verlebte Jahr sich zu der Bahre neigt.", "tokens": ["Und", "das", "ver\u00b7leb\u00b7te", "Jahr", "sich", "zu", "der", "Bah\u00b7re", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie solten Kinder nicht bey ihrer Eltern Leichen/", "tokens": ["Wie", "sol\u00b7ten", "Kin\u00b7der", "nicht", "bey", "ih\u00b7rer", "El\u00b7tern", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bejammern den Verlust/ der ihre Seele kr\u00e4nckt.", "tokens": ["Be\u00b7jam\u00b7mern", "den", "Ver\u00b7lust", "/", "der", "ih\u00b7re", "See\u00b7le", "kr\u00e4nckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Denn/ wenn die Seulen schon vom Bau der Wolfahrt weichen/", "tokens": ["Denn", "/", "wenn", "die", "Seu\u00b7len", "schon", "vom", "Bau", "der", "Wol\u00b7fahrt", "wei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer glaubt nicht/ da\u00df hernach das gantze Hau\u00df sich senckt.", "tokens": ["Wer", "glaubt", "nicht", "/", "da\u00df", "her\u00b7nach", "das", "gant\u00b7ze", "Hau\u00df", "sich", "senckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$(", "KOUS", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und solt es m\u00f6glich seyn die jenen zu vergessen/", "tokens": ["Und", "solt", "es", "m\u00f6g\u00b7lich", "seyn", "die", "je\u00b7nen", "zu", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VAINF", "ART", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die unsers Lebens Quell und erster Ursprung seyn?", "tokens": ["Die", "un\u00b7sers", "Le\u00b7bens", "Quell", "und", "ers\u00b7ter", "Ur\u00b7sprung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ist gleich die Tullia so gottlo\u00df und vermessen/", "tokens": ["Ist", "gleich", "die", "Tul\u00b7lia", "so", "gott\u00b7lo\u00df", "und", "ver\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Da\u00df sie durch Vater-Mord als im Triumph zieht ein.", "tokens": ["Da\u00df", "sie", "durch", "Va\u00b7ter\u00b7Mord", "als", "im", "Tri\u00b7umph", "zieht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KOKOM", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.30": {"text": "Und hat der Heyden Volck di\u00df Strafflo\u00df angeh\u00f6ret/", "tokens": ["Und", "hat", "der", "Hey\u00b7den", "Volck", "di\u00df", "Straf\u00b7flo\u00df", "an\u00b7ge\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "PDS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So schillt die Nachwelt doch dergleichen Raserey.", "tokens": ["So", "schillt", "die", "Nach\u00b7welt", "doch", "derg\u00b7lei\u00b7chen", "Ra\u00b7se\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein Kind das Eltern nicht auch in der Aschen ehret/", "tokens": ["Ein", "Kind", "das", "El\u00b7tern", "nicht", "auch", "in", "der", "A\u00b7schen", "eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ist w\u00fcrdig da\u00df es selbst niemals gebohren sey.", "tokens": ["Ist", "w\u00fcr\u00b7dig", "da\u00df", "es", "selbst", "nie\u00b7mals", "ge\u00b7boh\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Ich wei\u00df ", "tokens": ["Ich", "wei\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Man itzt der Mutter Grab die letzte Pflicht erweist.", "tokens": ["Man", "itzt", "der", "Mut\u00b7ter", "Grab", "die", "letz\u00b7te", "Pflicht", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In welch ein kl\u00e4glich Ach und ein unendlich sehnen/", "tokens": ["In", "welch", "ein", "kl\u00e4g\u00b7lich", "Ach", "und", "ein", "un\u00b7end\u00b7lich", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "ADJD", "NN", "KON", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Geschwellten Str\u00f6hmen gleich dein Hertze sich ergeust.", "tokens": ["Ge\u00b7schwell\u00b7ten", "Str\u00f6h\u00b7men", "gleich", "dein", "Hert\u00b7ze", "sich", "er\u00b7geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPOSAT", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Dienst ist lobens werth. Die sch\u00f6nsten Ehren-Bogen/", "tokens": ["Der", "Dienst", "ist", "lo\u00b7bens", "werth", ".", "Die", "sch\u00f6ns\u00b7ten", "Eh\u00b7ren\u00b7Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Colossen und Mausol erreichen nicht den Ruhm;", "tokens": ["Co\u00b7los\u00b7sen", "und", "Mau\u00b7sol", "er\u00b7rei\u00b7chen", "nicht", "den", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Als wenn den jenigen so nun der Welt entzogen/", "tokens": ["Als", "wenn", "den", "je\u00b7ni\u00b7gen", "so", "nun", "der", "Welt", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "PIAT", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das Hertze bleibt geweyht zu einem Eigenthum.", "tokens": ["Das", "Hert\u00b7ze", "bleibt", "ge\u00b7weyht", "zu", "ei\u00b7nem", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Allein/ wenn man das Rund der grossen Welt beschauet;", "tokens": ["Al\u00b7lein", "/", "wenn", "man", "das", "Rund", "der", "gros\u00b7sen", "Welt", "be\u00b7schau\u00b7et", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PIS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und Schmertzen und Vernunfft ein wenig ruhen l\u00e4st.", "tokens": ["Und", "Schmert\u00b7zen", "und", "Ver\u00b7nunfft", "ein", "we\u00b7nig", "ru\u00b7hen", "l