{"textgrid.poem.61685": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde lockt der wilde Tauber,", "tokens": ["Im", "Wal\u00b7de", "lockt", "der", "wil\u00b7de", "Tau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am stillen See der Wei\u00dfdorn bl\u00fcht,", "tokens": ["Am", "stil\u00b7len", "See", "der", "Wei\u00df\u00b7dorn", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kommt der alte Fr\u00fchlingszauber", "tokens": ["Da", "kommt", "der", "al\u00b7te", "Fr\u00fch\u00b7lings\u00b7zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewaltig \u00fcber mein Gem\u00fct.", "tokens": ["Ge\u00b7wal\u00b7tig", "\u00fc\u00b7ber", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir ist, als sollt' ich Fl\u00fcgel dehnen", "tokens": ["Mir", "ist", ",", "als", "sollt'", "ich", "Fl\u00fc\u00b7gel", "deh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins klarvertiefte Blau dahin;", "tokens": ["Ins", "klar\u00b7ver\u00b7tief\u00b7te", "Blau", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Auge schwillt von hei\u00dfen Tr\u00e4nen,", "tokens": ["Mein", "Au\u00b7ge", "schwillt", "von", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch in Freuden steht mein Sinn.", "tokens": ["Und", "doch", "in", "Freu\u00b7den", "steht", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geheimnisvolle Glut ergreift mich", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Glut", "er\u00b7greift", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei tiefer Nacht oft wunderbar,", "tokens": ["Bei", "tie\u00b7fer", "Nacht", "oft", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie mit s\u00fc\u00dfer Ahnung streift mich", "tokens": ["Und", "wie", "mit", "s\u00fc\u00b7\u00dfer", "Ah\u00b7nung", "streift", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Traum ein flatternd Lockenhaar.", "tokens": ["Im", "Traum", "ein", "flat\u00b7ternd", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und morgens dann in roter Fr\u00fche", "tokens": ["Und", "mor\u00b7gens", "dann", "in", "ro\u00b7ter", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwacht mein Herz so reich und froh,", "tokens": ["Er\u00b7wacht", "mein", "Herz", "so", "reich", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als w\u00fc\u00dft' es, da\u00df sein Gl\u00fcck schon bl\u00fche,", "tokens": ["Als", "w\u00fc\u00dft'", "es", ",", "da\u00df", "sein", "Gl\u00fcck", "schon", "bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00fc\u00dfte nur noch raten, wo?", "tokens": ["Und", "m\u00fc\u00df\u00b7te", "nur", "noch", "ra\u00b7ten", ",", "wo", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}