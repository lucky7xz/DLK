{"textgrid.poem.66347": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der erste Schnee", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da tanzten sie, die wei\u00dfen krausen Fl\u00f6ckchen", "tokens": ["Da", "tanz\u00b7ten", "sie", ",", "die", "wei\u00b7\u00dfen", "krau\u00b7sen", "Fl\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Wolkenzeit herab;", "tokens": ["Vom", "Wol\u00b7ken\u00b7zeit", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sanft und warm, wie L\u00e4mmerwolle,", "tokens": ["Und", "sanft", "und", "warm", ",", "wie", "L\u00e4m\u00b7mer\u00b7wol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Decken sie dich, du Mutter Natur!", "tokens": ["De\u00b7cken", "sie", "dich", ",", "du", "Mut\u00b7ter", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "PPER", "NN", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "So wei\u00df ist nicht der Nonne Silberschleier,", "tokens": ["So", "wei\u00df", "ist", "nicht", "der", "Non\u00b7ne", "Sil\u00b7ber\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlehbl\u00fcthe nicht so wei\u00df:", "tokens": ["Schleh\u00b7bl\u00fc\u00b7the", "nicht", "so", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie junger Schnee im Sonnenglanze,", "tokens": ["Wie", "jun\u00b7ger", "Schnee", "im", "Son\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00e4ler und Berge blitzen von ihm.", "tokens": ["Th\u00e4\u00b7ler", "und", "Ber\u00b7ge", "blit\u00b7zen", "von", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Schon sch\u00fcttelt sich der Gaul am leichten Schlitten,", "tokens": ["Schon", "sch\u00fct\u00b7telt", "sich", "der", "Gaul", "am", "leich\u00b7ten", "Schlit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Sch\u00fctteln ist Musik.", "tokens": ["Sein", "Sch\u00fct\u00b7teln", "ist", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unterm Lied der Silberschellen", "tokens": ["Und", "un\u00b7term", "Lied", "der", "Sil\u00b7ber\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleitet der Schlitten fliegend hinweg.", "tokens": ["Glei\u00b7tet", "der", "Schlit\u00b7ten", "flie\u00b7gend", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich aber sitze am beschneiten Fenster;", "tokens": ["Ich", "a\u00b7ber", "sit\u00b7ze", "am", "be\u00b7schnei\u00b7ten", "Fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein blaues Knasterw\u00f6lkchen steigt", "tokens": ["Ein", "blau\u00b7es", "Knas\u00b7ter\u00b7w\u00f6lk\u00b7chen", "steigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit tausend luftgebauten Schl\u00f6ssern,", "tokens": ["Mit", "tau\u00b7send", "luft\u00b7ge\u00b7bau\u00b7ten", "Schl\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00fcnnere L\u00fcfte zirkelnd, empor.", "tokens": ["D\u00fcn\u00b7ne\u00b7re", "L\u00fcf\u00b7te", "zir\u00b7kelnd", ",", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Husch! schnattert sie, mich friert's!", "tokens": ["Husch", "!", "schnat\u00b7tert", "sie", ",", "mich", "friert's", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "PRF", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sanft lehnt sie sich an meine Schultern,", "tokens": ["Sanft", "lehnt", "sie", "sich", "an", "mei\u00b7ne", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leben und W\u00e4rme duftet sie aus.", "tokens": ["Le\u00b7ben", "und", "W\u00e4r\u00b7me", "duf\u00b7tet", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Durch's Winterfenster schl\u00fcpft ein wei\u00dfes Fl\u00f6ckchen,", "tokens": ["Durch's", "Win\u00b7ter\u00b7fens\u00b7ter", "schl\u00fcpft", "ein", "wei\u00b7\u00dfes", "Fl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00e4llt auf ihre Brust,", "tokens": ["Und", "f\u00e4llt", "auf", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bl\u00e4ht sich und schmilzt mit einem Seufzer:", "tokens": ["Bl\u00e4ht", "sich", "und", "schmilzt", "mit", "ei\u00b7nem", "Seuf\u00b7zer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du, kalter Nord, behalte deine Zobel!", "tokens": ["Du", ",", "kal\u00b7ter", "Nord", ",", "be\u00b7hal\u00b7te", "dei\u00b7ne", "Zo\u00b7bel", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaninchen, deinen Pelz", "tokens": ["Ka\u00b7nin\u00b7chen", ",", "dei\u00b7nen", "Pelz"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behalte du! Von R\u00f6schens Busen", "tokens": ["Be\u00b7hal\u00b7te", "du", "!", "Von", "R\u00f6\u00b7schens", "Bu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wallet ein ewiger Sommer mir zu.", "tokens": ["Wal\u00b7let", "ein", "e\u00b7wi\u00b7ger", "Som\u00b7mer", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}