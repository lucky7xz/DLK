{"dta.poem.7405": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Leicht Gep\u00e4ck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein freier Mann und singe", "tokens": ["Ich", "bin", "ein", "frei\u00b7er", "Mann", "und", "sin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich wohl in keine F\u00fcrstengruft,", "tokens": ["Mich", "wohl", "in", "kei\u00b7ne", "F\u00fcrs\u00b7ten\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Alles, was ich mir erringe,", "tokens": ["Und", "Al\u00b7les", ",", "was", "ich", "mir", "er\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Gottes liebe Himmelsluft;", "tokens": ["Ist", "Got\u00b7tes", "lie\u00b7be", "Him\u00b7mel\u00b7sluft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe keine stolze Veste,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "stol\u00b7ze", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von der man L\u00e4nder \u00fcbersieht,", "tokens": ["Von", "der", "man", "L\u00e4n\u00b7der", "\u00fc\u00b7ber\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wohn' ein Vogel nur im Neste,", "tokens": ["Ich", "wohn'", "ein", "Vo\u00b7gel", "nur", "im", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzer Reichthum ist mein Lied.", "tokens": ["Mein", "gan\u00b7zer", "Reicht\u00b7hum", "ist", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich durfte nur, wie Andre, wollen,", "tokens": ["Ich", "durf\u00b7te", "nur", ",", "wie", "And\u00b7re", ",", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "PIS", "$,", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4r' nicht leer davongeeilt,", "tokens": ["Und", "w\u00e4r'", "nicht", "leer", "da\u00b7von\u00b7ge\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn j\u00e4hrlich man im Staat die Rollen", "tokens": ["Wenn", "j\u00e4hr\u00b7lich", "man", "im", "Staat", "die", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den treuen Knechten ausgetheilt;", "tokens": ["Den", "treu\u00b7en", "Knech\u00b7ten", "aus\u00b7ge\u00b7theilt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein ich hab' nie zugegriffen,", "tokens": ["Al\u00b7lein", "ich", "hab'", "nie", "zu\u00b7ge\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So oft man mich herbei beschied,", "tokens": ["So", "oft", "man", "mich", "her\u00b7bei", "be\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich habe fort und fort gepfiffen,", "tokens": ["Ich", "ha\u00b7be", "fort", "und", "fort", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "KON", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzer Reichthum ist mein Lied.", "tokens": ["Mein", "gan\u00b7zer", "Reicht\u00b7hum", "ist", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Lord zapft Gold aus seiner Tonne,", "tokens": ["Der", "Lord", "zapft", "Gold", "aus", "sei\u00b7ner", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich aus meiner h\u00f6chstens Wein;", "tokens": ["Und", "ich", "aus", "mei\u00b7ner", "h\u00f6chs\u00b7tens", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein einzig Gold die Morgensonne,", "tokens": ["Mein", "ein\u00b7zig", "Gold", "die", "Mor\u00b7gen\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Silber all der Mondenschein!", "tokens": ["Mein", "Sil\u00b7ber", "all", "der", "Mon\u00b7den\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00e4rbt sich mein Leben herbstlich gelber,", "tokens": ["F\u00e4rbt", "sich", "mein", "Le\u00b7ben", "herbst\u00b7lich", "gel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ADJD", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Kein Erbe, der zum Tod mir rieth;", "tokens": ["Kein", "Er\u00b7be", ",", "der", "zum", "Tod", "mir", "rieth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn meine M\u00fcnzen pr\u00e4gt' ich selber;", "tokens": ["Denn", "mei\u00b7ne", "M\u00fcn\u00b7zen", "pr\u00e4gt'", "ich", "sel\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzer Reichthum ist mein Lied.", "tokens": ["Mein", "gan\u00b7zer", "Reicht\u00b7hum", "ist", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gern sing' ich Abends zu dem Reigen,", "tokens": ["Gern", "sing'", "ich", "A\u00b7bends", "zu", "dem", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Thronen spiel' ich niemals auf;", "tokens": ["Vor", "Thro\u00b7nen", "spiel'", "ich", "nie\u00b7mals", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lernte Berge wohl ersteigen,", "tokens": ["Ich", "lern\u00b7te", "Ber\u00b7ge", "wohl", "er\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pal\u00e4ste komm' ich nicht hinauf;", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", "komm'", "ich", "nicht", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Inde\u00df aus Moder, Sturz und Wettern", "tokens": ["In\u00b7de\u00df", "aus", "Mo\u00b7der", ",", "Sturz", "und", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein golden Loos sich Mancher zieht,", "tokens": ["Sein", "gol\u00b7den", "Loos", "sich", "Man\u00b7cher", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Spiel' ich mit leichten Rosenbl\u00e4ttern;", "tokens": ["Spiel'", "ich", "mit", "leich\u00b7ten", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzer Reichthum ist mein Lied.", "tokens": ["Mein", "gan\u00b7zer", "Reicht\u00b7hum", "ist", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach Dir, nach Dir steht mein Verlangen,", "tokens": ["Nach", "Dir", ",", "nach", "Dir", "steht", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O sch\u00f6nes Kind, o w\u00e4rst Du mein!", "tokens": ["O", "sch\u00f6\u00b7nes", "Kind", ",", "o", "w\u00e4rst", "Du", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Du willst B\u00e4nder, Du willst Spangen,", "tokens": ["Doch", "Du", "willst", "B\u00e4n\u00b7der", ",", "Du", "willst", "Span\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "$,", "PPER", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich soll dienen gehen? Nein!", "tokens": ["Und", "ich", "soll", "die\u00b7nen", "ge\u00b7hen", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "VVINF", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will die Freiheit nicht verkaufen,", "tokens": ["Ich", "will", "die", "Frei\u00b7heit", "nicht", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie ich die Pal\u00e4ste mied,", "tokens": ["Und", "wie", "ich", "die", "Pa\u00b7l\u00e4s\u00b7te", "mied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df ich getrost die Liebe laufen;", "tokens": ["La\u00df", "ich", "ge\u00b7trost", "die", "Lie\u00b7be", "lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzer Reichthum sei mein Lied.", "tokens": ["Mein", "gan\u00b7zer", "Reicht\u00b7hum", "sei", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}