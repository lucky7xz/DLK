{"textgrid.poem.57032": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sprachstudien", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Korf und Palmstr\u00f6m nehmen Lektionen,", "tokens": ["Korf", "und", "Palm\u00b7str\u00f6m", "neh\u00b7men", "Lek\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "um das Wetter-Wendische zu lernen.", "tokens": ["um", "das", "Wet\u00b7ter\u00b7Wen\u00b7di\u00b7sche", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "T\u00e4glich pilgern sie zu den modernen", "tokens": ["T\u00e4g\u00b7lich", "pil\u00b7gern", "sie", "zu", "den", "mo\u00b7der\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ollendorffschen Sprachlehrgrammophonen.", "tokens": ["Ol\u00b7len\u00b7dorff\u00b7schen", "Sprach\u00b7lehr\u00b7gram\u00b7mo\u00b7pho\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dort nun lassen sie mit vielen andern,", "tokens": ["Dort", "nun", "las\u00b7sen", "sie", "mit", "vie\u00b7len", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "welche gleichfalls steile Charaktere,", "tokens": ["wel\u00b7che", "gleich\u00b7falls", "stei\u00b7le", "Cha\u00b7rak\u00b7te\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "(gleich als ob's ein Ziel f\u00fcr Edle w\u00e4re),", "tokens": ["(", "gleich", "als", "ob's", "ein", "Ziel", "f\u00fcr", "Ed\u00b7le", "w\u00e4\u00b7re", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "KOKOM", "KOUS", "ART", "NN", "APPR", "ADJA", "VAFIN", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sich im Wetter-Wendischen bewandern.", "tokens": ["sich", "im", "Wet\u00b7ter\u00b7Wen\u00b7di\u00b7schen", "be\u00b7wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dies Idiom behebt den Geist der Schwere,", "tokens": ["Dies", "I\u00b7diom", "be\u00b7hebt", "den", "Geist", "der", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "macht sie unstet, launisch und cholerisch ...", "tokens": ["macht", "sie", "un\u00b7stet", ",", "lau\u00b7nisch", "und", "cho\u00b7le\u00b7risch", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch die Sache bleibt nur peripherisch.", "tokens": ["Doch", "die", "Sa\u00b7che", "bleibt", "nur", "pe\u00b7ri\u00b7phe\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und sie werden wieder \u2013 Charaktere.", "tokens": ["Und", "sie", "wer\u00b7den", "wie\u00b7der", "\u2013", "Cha\u00b7rak\u00b7te\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$(", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}