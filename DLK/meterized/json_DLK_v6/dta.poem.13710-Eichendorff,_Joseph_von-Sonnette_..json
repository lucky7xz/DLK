{"dta.poem.13710": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wen noch die alten Heimath-Kl\u00e4nge locken,", "tokens": ["Wen", "noch", "die", "al\u00b7ten", "Hei\u00b7math\u00b7Kl\u00e4n\u00b7ge", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem f\u00fcllt der Segel wie der T\u00f6ne Schwellen", "tokens": ["Dem", "f\u00fcllt", "der", "Se\u00b7gel", "wie", "der", "T\u00f6\u00b7ne", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Brust mit jungen, ewigen Gedanken.", "tokens": ["Die", "Brust", "mit", "jun\u00b7gen", ",", "e\u00b7wi\u00b7gen", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}