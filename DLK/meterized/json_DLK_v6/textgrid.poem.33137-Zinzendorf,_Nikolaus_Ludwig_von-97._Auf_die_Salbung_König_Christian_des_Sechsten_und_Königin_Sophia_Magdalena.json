{"textgrid.poem.33137": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "97. Auf die Salbung K\u00f6nig Christian des Sechsten und K\u00f6nigin Sophia Magdalena", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wach auf,", "tokens": ["Wach", "auf", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Zur Zeit des Bruna Olds verschlo\u00dfnen Ritter-Staub!", "tokens": ["Zur", "Zeit", "des", "Bru\u00b7na", "Olds", "ver\u00b7schlo\u00df\u00b7nen", "Rit\u00b7ter\u00b7Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Feuer-Flammen leiht den \u00fcberla\u00dfnen Raub,", "tokens": ["Ihr", "Feu\u00b7er\u00b7Flam\u00b7men", "leiht", "den", "\u00fc\u00b7berl\u00b7a\u00df\u00b7nen", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An diesem grossen Fest den Pallast zu verriegeln.", "tokens": ["An", "die\u00b7sem", "gros\u00b7sen", "Fest", "den", "Pal\u00b7last", "zu", "ver\u00b7rie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die H\u00fctten, die der Geist belebte, sind verzehrt,", "tokens": ["Die", "H\u00fct\u00b7ten", ",", "die", "der", "Geist", "be\u00b7leb\u00b7te", ",", "sind", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Flammen sind zur\u00fck ins Element gekehrt.", "tokens": ["Die", "Flam\u00b7men", "sind", "zu\u00b7r\u00fck", "ins", "E\u00b7le\u00b7ment", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Frey'r! wo dich jener Fels noch unverweslich h\u00e4lt,", "tokens": ["Frey'r", "!", "wo", "dich", "je\u00b7ner", "Fels", "noch", "un\u00b7ver\u00b7wes\u00b7lich", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "PDAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beut alle Riesen auf, die sich in Harnisch strekten;", "tokens": ["Beut", "al\u00b7le", "Rie\u00b7sen", "auf", ",", "die", "sich", "in", "Har\u00b7nisch", "strek\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ist Dan Mekilat' mit unter den Erwekten,", "tokens": ["Und", "ist", "Dan", "Me\u00b7kilat'", "mit", "un\u00b7ter", "den", "Er\u00b7wek\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So leit ihn auf dem Ro\u00df in diese obre Welt.", "tokens": ["So", "leit", "ihn", "auf", "dem", "Ro\u00df", "in", "die\u00b7se", "ob\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht vertr\u00e4t er gern den Platz beym Salbungs-Mahl,", "tokens": ["Viel\u00b7leicht", "ver\u00b7tr\u00e4t", "er", "gern", "den", "Platz", "beym", "Sal\u00b7bungs\u00b7Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den der Geharnischte", "tokens": ["Den", "der", "Ge\u00b7har\u00b7nischte"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Nein! ruht ihr K\u00f6nige! der Glanz von unsren Kerzen", "tokens": ["Nein", "!", "ruht", "ihr", "K\u00f6\u00b7ni\u00b7ge", "!", "der", "Glanz", "von", "un\u00b7sren", "Ker\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt einer Million von Lebenden den Schein:", "tokens": ["Gibt", "ei\u00b7ner", "Mil\u00b7li\u00b7on", "von", "Le\u00b7ben\u00b7den", "den", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Frohde Macht beschleu\u00dft ein stolzer Bauta-Stein,", "tokens": ["Des", "Froh\u00b7de", "Macht", "be\u00b7schleu\u00dft", "ein", "stol\u00b7zer", "Bauta\u00b7Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Den sanften Christian ein Wunder-Bau der Herzen;", "tokens": ["Den", "sanf\u00b7ten", "Chris\u00b7ti\u00b7an", "ein", "Wun\u00b7der\u00b7Bau", "der", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Gnaden-voller Blik wird allen zum Magnet:", "tokens": ["Sein", "Gna\u00b7den\u00b7vol\u00b7ler", "Blik", "wird", "al\u00b7len", "zum", "Mag\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er sieht so tief herab, als er erhaben sieht.", "tokens": ["Er", "sieht", "so", "tief", "her\u00b7ab", ",", "als", "er", "er\u00b7ha\u00b7ben", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier unterstehet sich ein wohl bekanter Knecht", "tokens": ["Hier", "un\u00b7ter\u00b7ste\u00b7het", "sich", "ein", "wohl", "be\u00b7kan\u00b7ter", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Niedrigkeit der Kunst vor deinen Thron zu ziehen.", "tokens": ["Mit", "Nied\u00b7rig\u00b7keit", "der", "Kunst", "vor", "dei\u00b7nen", "Thron", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr! senke deinen Blik, den Strahl la\u00df linde gl\u00fchen.", "tokens": ["Herr", "!", "sen\u00b7ke", "dei\u00b7nen", "Blik", ",", "den", "Strahl", "la\u00df", "lin\u00b7de", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVIMP", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Er tichtet, er bedenkt, er scheuet sich mit Recht:", "tokens": ["Er", "tich\u00b7tet", ",", "er", "be\u00b7denkt", ",", "er", "scheu\u00b7et", "sich", "mit", "Recht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und h\u00e4tt er Trieb und Kiel dem Dichter abgeborgt,", "tokens": ["Und", "h\u00e4tt", "er", "Trieb", "und", "Kiel", "dem", "Dich\u00b7ter", "ab\u00b7ge\u00b7borgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Carln besungen", "tokens": ["Der", "Carln", "be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Thron, die Herrlichkeit des Tages, und der Sache,", "tokens": ["Der", "Thron", ",", "die", "Herr\u00b7lich\u00b7keit", "des", "Ta\u00b7ges", ",", "und", "der", "Sa\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ungemeine Pracht, darinn der K\u00f6nig sitzt,", "tokens": ["Die", "un\u00b7ge\u00b7mei\u00b7ne", "Pracht", ",", "da\u00b7rinn", "der", "K\u00f6\u00b7nig", "sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was der K\u00f6nigin von ihrer Scheitel blitzt,", "tokens": ["Und", "was", "der", "K\u00f6\u00b7ni\u00b7gin", "von", "ih\u00b7rer", "Schei\u00b7tel", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das alles hemmet mich, sobald ich Worte mache;", "tokens": ["Das", "al\u00b7les", "hem\u00b7met", "mich", ",", "so\u00b7bald", "ich", "Wor\u00b7te", "ma\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Finger-Zeig auf das, was aller Augen sehn,", "tokens": ["Ein", "Fin\u00b7ger\u00b7Zeig", "auf", "das", ",", "was", "al\u00b7ler", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der solte einer Hand, wie meiner, \u00fcbel stehn.", "tokens": ["Der", "sol\u00b7te", "ei\u00b7ner", "Hand", ",", "wie", "mei\u00b7ner", ",", "\u00fc\u00b7bel", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$,", "PWAV", "PPOSAT", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gro\u00dfm\u00e4chtiger Monarch! Dein Wink erlaube nur,", "tokens": ["Gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Mon\u00b7arch", "!", "Dein", "Wink", "er\u00b7lau\u00b7be", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will ich unges\u00e4umt zu meinem Zwekke kommen:", "tokens": ["So", "will", "ich", "un\u00b7ge\u00b7s\u00e4umt", "zu", "mei\u00b7nem", "Zwek\u00b7ke", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herodes G\u00f6tter-Schein hat dich nicht eingenommen,", "tokens": ["He\u00b7ro\u00b7des", "G\u00f6t\u00b7ter\u00b7Schein", "hat", "dich", "nicht", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unsrer K\u00f6nigin gef\u00e4llt der Esther Spur.", "tokens": ["Und", "uns\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "ge\u00b7f\u00e4llt", "der", "E\u00b7sther", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df f\u00fcr meinen Trieb (und d\u00e4cht er tausendmal)", "tokens": ["Ich", "wei\u00df", "f\u00fcr", "mei\u00b7nen", "Trieb", "(", "und", "d\u00e4cht", "er", "tau\u00b7send\u00b7mal", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein w\u00fcrdiger Object, als deine eigne Wahl.", "tokens": ["Kein", "w\u00fcr\u00b7di\u00b7ger", "Ob\u00b7ject", ",", "als", "dei\u00b7ne", "eig\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es h\u00e4tte dich dein Knecht, ists m\u00f6glich? fast gefraget,", "tokens": ["Es", "h\u00e4t\u00b7te", "dich", "dein", "Knecht", ",", "ists", "m\u00f6g\u00b7lich", "?", "fast", "ge\u00b7fra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$.", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch liebensw\u00fcrdiger, als Ehren-volles Haupt!", "tokens": ["Noch", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "als", "Eh\u00b7ren\u00b7vol\u00b7les", "Haupt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(allein wie w\u00e4re ihm das Fragen nicht erlaubt?", "tokens": ["(", "al\u00b7lein", "wie", "w\u00e4\u00b7re", "ihm", "das", "Fra\u00b7gen", "nicht", "er\u00b7laubt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da du die Antwort schon von Herzen weggesaget!)", "tokens": ["Da", "du", "die", "Ant\u00b7wort", "schon", "von", "Her\u00b7zen", "weg\u00b7ge\u00b7sa\u00b7get", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wem soll das Gegen-Bild von dieses Tages Schein:", "tokens": ["Wem", "soll", "das", "Ge\u00b7gen\u00b7Bild", "von", "die\u00b7ses", "Ta\u00b7ges", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott und dem Volke will dein Scepter dienstbar seyn.", "tokens": ["Gott", "und", "dem", "Vol\u00b7ke", "will", "dein", "Scep\u00b7ter", "dienst\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VMFIN", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Gott? das ist bald gesagt; allein wo ist Beweis?", "tokens": ["Gott", "?", "das", "i