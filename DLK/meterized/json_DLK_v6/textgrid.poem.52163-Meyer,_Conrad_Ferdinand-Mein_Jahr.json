{"textgrid.poem.52163": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Mein Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht vom letzten Schlittengleise", "tokens": ["Nicht", "vom", "letz\u00b7ten", "Schlit\u00b7ten\u00b7glei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum neuen Flockentraum", "tokens": ["Bis", "zum", "neu\u00b7en", "Flo\u00b7cken\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4hl ich auf der Lebensreise", "tokens": ["Z\u00e4hl", "ich", "auf", "der", "Le\u00b7bens\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den erf\u00fcllten Jahresraum.", "tokens": ["Den", "er\u00b7f\u00fcll\u00b7ten", "Jah\u00b7res\u00b7raum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht vom ersten frischen Singen,", "tokens": ["Nicht", "vom", "ers\u00b7ten", "fri\u00b7schen", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das im Wald geboren ist,", "tokens": ["Das", "im", "Wald", "ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die Zweige wieder klingen,", "tokens": ["Bis", "die", "Zwei\u00b7ge", "wie\u00b7der", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dauert mir die Jahresfrist.", "tokens": ["Dau\u00b7ert", "mir", "die", "Jah\u00b7res\u00b7frist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von der Kelter nicht zur Kelter", "tokens": ["Von", "der", "Kel\u00b7ter", "nicht", "zur", "Kel\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreht sich mir des Jahres Schwung,", "tokens": ["Dreht", "sich", "mir", "des", "Jah\u00b7res", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, in Flammen werd ich \u00e4lter", "tokens": ["Nein", ",", "in", "Flam\u00b7men", "werd", "ich", "\u00e4l\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Flammen wieder jung.", "tokens": ["Und", "in", "Flam\u00b7men", "wie\u00b7der", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von dem ersten Blitze heuer,", "tokens": ["Von", "dem", "ers\u00b7ten", "Blit\u00b7ze", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der aus dunkler Wolke sprang,", "tokens": ["Der", "aus", "dunk\u00b7ler", "Wol\u00b7ke", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis zu neuem Himmelsfeuer", "tokens": ["Bis", "zu", "neu\u00b7em", "Him\u00b7mels\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rechn ich meinen Jahresgang.", "tokens": ["Rechn", "ich", "mei\u00b7nen", "Jah\u00b7res\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}