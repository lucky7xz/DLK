{"textgrid.poem.25206": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf sandiger Haide am Kiefernforst", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf sandiger Haide am Kiefernforst", "tokens": ["Auf", "san\u00b7di\u00b7ger", "Hai\u00b7de", "am", "Kie\u00b7fern\u00b7forst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kauert ein H\u00e4uschen gedr\u00fcckt", "tokens": ["Kau\u00b7ert", "ein", "H\u00e4usc\u00b7hen", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "An Fenster, Dach und Lehmgewand", "tokens": ["An", "Fens\u00b7ter", ",", "Dach", "und", "Lehm\u00b7ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwahrlost und zerst\u00fcckt.", "tokens": ["Ver\u00b7wahr\u00b7lost", "und", "zer\u00b7st\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des bretternen Stalles Th\u00fcre klafft, \u2013", "tokens": ["Des", "bret\u00b7ter\u00b7nen", "Stal\u00b7les", "Th\u00fc\u00b7re", "klafft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verkauft sind Schaafe und Ziegen;", "tokens": ["Ver\u00b7kauft", "sind", "Schaa\u00b7fe", "und", "Zie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im D\u00fcnger ein letztes H\u00fchnchen scharrt,", "tokens": ["Im", "D\u00fcn\u00b7ger", "ein", "letz\u00b7tes", "H\u00fchn\u00b7chen", "scharrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und m\u00fcrrisch brummen die Fliegen;", "tokens": ["Und", "m\u00fcr\u00b7risch", "brum\u00b7men", "die", "Flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und in der Stube, da quarrt das Kind,", "tokens": ["Und", "in", "der", "Stu\u00b7be", ",", "da", "quarrt", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Weib, das zornige, schilt,", "tokens": ["Das", "Weib", ",", "das", "zor\u00b7ni\u00b7ge", ",", "schilt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des H\u00e4uslers Stimme, trunken und rauh,", "tokens": ["Des", "H\u00e4us\u00b7lers", "Stim\u00b7me", ",", "trun\u00b7ken", "und", "rauh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "L\u00e4stert dazwischen wild ...", "tokens": ["L\u00e4s\u00b7tert", "da\u00b7zwi\u00b7schen", "wild", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJD", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Am Fenster die schlanke Sonnenblume", "tokens": ["Am", "Fens\u00b7ter", "die", "schlan\u00b7ke", "Son\u00b7nen\u00b7blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erbebt in geheimem Leid;", "tokens": ["Er\u00b7bebt", "in", "ge\u00b7hei\u00b7mem", "Leid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus Schutt und Unkraut strebt sie scheu", "tokens": ["Aus", "Schutt", "und", "Un\u00b7kraut", "strebt", "sie", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und starrt in die Ferne weit.", "tokens": ["Und", "starrt", "in", "die", "Fer\u00b7ne", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort hinter vergilbtem Kartoffelkraut", "tokens": ["Dort", "hin\u00b7ter", "ver\u00b7gilb\u00b7tem", "Kar\u00b7tof\u00b7fel\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und blondem Stoppelhaar", "tokens": ["Und", "blon\u00b7dem", "Stop\u00b7pel\u00b7haar"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ergl\u00e4nzt der Himmel so goldig zart,", "tokens": ["Er\u00b7gl\u00e4nzt", "der", "Him\u00b7mel", "so", "gol\u00b7dig", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Gesang so wunderklar.", "tokens": ["Wie", "Ge\u00b7sang", "so", "wun\u00b7der\u00b7klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Dufte dort mit schmetternder Glut", "tokens": ["Im", "Duf\u00b7te", "dort", "mit", "schmet\u00b7tern\u00b7der", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Verbl\u00fcht die Abendsonne; \u2013", "tokens": ["Ver\u00b7bl\u00fcht", "die", "A\u00b7bend\u00b7son\u00b7ne", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O schmachtende Seele, starre hinein", "tokens": ["O", "schmach\u00b7ten\u00b7de", "See\u00b7le", ",", "star\u00b7re", "hin\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und trinke dir einzige Wonne! \u2013", "tokens": ["Und", "trin\u00b7ke", "dir", "ein\u00b7zi\u00b7ge", "Won\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Blume \u2013 am taumelnden Sonnenball", "tokens": ["Und", "die", "Blu\u00b7me", "\u2013", "am", "tau\u00b7meln\u00b7den", "Son\u00b7nen\u00b7ball"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "H\u00e4ngt schw\u00e4rmerisch starr ihr Angesicht,", "tokens": ["H\u00e4ngt", "schw\u00e4r\u00b7me\u00b7risch", "starr", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr gelbumlodertes frommes Gesicht,", "tokens": ["Ihr", "gel\u00b7bum\u00b7lo\u00b7der\u00b7tes", "from\u00b7mes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Versunken im Licht, ertrunken im Licht.", "tokens": ["Ver\u00b7sun\u00b7ken", "im", "Licht", ",", "er\u00b7trun\u00b7ken", "im", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die breiten graugr\u00fcnen Bl\u00e4tter spreitet", "tokens": ["Die", "brei\u00b7ten", "grau\u00b7gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "sprei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sehnlich in zitternder Scheidetrauer,", "tokens": ["Sie", "sehn\u00b7lich", "in", "zit\u00b7tern\u00b7der", "Schei\u00b7de\u00b7trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hinter der sinkenden Sonne gleitet", "tokens": ["Und", "hin\u00b7ter", "der", "sin\u00b7ken\u00b7den", "Son\u00b7ne", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr Sinnen hinunter mit Andachtsschauer.", "tokens": ["Ihr", "Sin\u00b7nen", "hin\u00b7un\u00b7ter", "mit", "An\u00b7dachts\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}