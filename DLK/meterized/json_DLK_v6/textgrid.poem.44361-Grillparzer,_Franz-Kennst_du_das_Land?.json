{"textgrid.poem.44361": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Kennst du das Land?", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelobt sei Gott! die Stund ist da!", "tokens": ["Ge\u00b7lobt", "sei", "Gott", "!", "die", "Stund", "ist", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wanderstab in die Hand!", "tokens": ["Den", "Wan\u00b7der\u00b7stab", "in", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu dir hin gehts, Italia,", "tokens": ["Zu", "dir", "hin", "gehts", ",", "I\u00b7ta\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du hochgelobtes Land!", "tokens": ["Du", "hoch\u00b7ge\u00b7lob\u00b7tes", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Pilger zieht mit Hut und Stab", "tokens": ["Der", "Pil\u00b7ger", "zieht", "mit", "Hut", "und", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum heiligen Grabe weit,", "tokens": ["Zum", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So zieh auch ich zu ", "tokens": ["So", "zieh", "auch", "ich", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Du heilge, entschlafene Zeit!", "tokens": ["Du", "heil\u00b7ge", ",", "ent\u00b7schla\u00b7fe\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und wie der Pilger auf seiner Brust", "tokens": ["Und", "wie", "der", "Pil\u00b7ger", "auf", "sei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Reliquien tr\u00e4gt nach Haus,", "tokens": ["Re\u00b7li\u00b7qui\u00b7en", "tr\u00e4gt", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So trag auch ich ", "tokens": ["So", "trag", "auch", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mir heilige Reste heraus.", "tokens": ["Mir", "hei\u00b7li\u00b7ge", "Res\u00b7te", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die letzten Tropfen vom Wunderborn,", "tokens": ["Die", "letz\u00b7ten", "Trop\u00b7fen", "vom", "Wun\u00b7der\u00b7born", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der einst so reichlich quoll,", "tokens": ["Der", "einst", "so", "reich\u00b7lich", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein F\u00fcnkchen von deinem G\u00f6tterzorn,", "tokens": ["Ein", "F\u00fcnk\u00b7chen", "von", "dei\u00b7nem", "G\u00f6t\u00b7ter\u00b7zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du g\u00f6ttlicher Apoll,", "tokens": ["Du", "g\u00f6tt\u00b7li\u00b7cher", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Den Abdruck, Weltgebieter Zeus,", "tokens": ["Den", "Ab\u00b7druck", ",", "Welt\u00b7ge\u00b7bie\u00b7ter", "Zeus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deiner Majest\u00e4t,", "tokens": ["Von", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Dichterbaum ein Lorbeerreis,", "tokens": ["Vom", "Dich\u00b7ter\u00b7baum", "ein", "Lor\u00b7beer\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Maros Grab umweht.", "tokens": ["Der", "Ma\u00b7ros", "Grab", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein Bild, so sehr und unbefleckt,", "tokens": ["Dein", "Bild", ",", "so", "sehr", "und", "un\u00b7be\u00b7fleckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Hohe von Medici,", "tokens": ["Du", "Ho\u00b7he", "von", "Me\u00b7di\u00b7ci", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die, wenn sie den Schauern die Sch\u00e4tze bedeckt,", "tokens": ["Die", ",", "wenn", "sie", "den", "Schau\u00b7ern", "die", "Sch\u00e4t\u00b7ze", "be\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "F\u00fcr sich nicht err\u00f6tet, f\u00fcr sie.", "tokens": ["F\u00fcr", "sich", "nicht", "er\u00b7r\u00f6\u00b7tet", ",", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKNEG", "VVPP", "$,", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Ja, knieen will ich, Vergangenheit,", "tokens": ["Ja", ",", "kni\u00b7e\u00b7en", "will", "ich", ",", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor deinen Gebilden aus Stein,", "tokens": ["Vor", "dei\u00b7nen", "Ge\u00b7bil\u00b7den", "aus", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der nackt die ernste Sch\u00f6nheit beut,", "tokens": ["Der", "nackt", "die", "erns\u00b7te", "Sch\u00f6n\u00b7heit", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verachtend des Reizes Schein,", "tokens": ["Ver\u00b7ach\u00b7tend", "des", "Rei\u00b7zes", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ihn lassend der fr\u00f6mmelnden Enkelwelt,", "tokens": ["Ihn", "las\u00b7send", "der", "fr\u00f6m\u00b7meln\u00b7den", "En\u00b7kel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die, von Gleisnersinn erf\u00fcllt,", "tokens": ["Die", ",", "von", "Gleis\u00b7ner\u00b7sinn", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die L\u00fcsternheit zu erg\u00e4nzen qu\u00e4lt,", "tokens": ["Die", "L\u00fcs\u00b7tern\u00b7heit", "zu", "er\u00b7g\u00e4n\u00b7zen", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was der schlaue Bildner verh\u00fcllt;", "tokens": ["Was", "der", "schlau\u00b7e", "Bild\u00b7ner", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und lernen will ich auf deinen Laut,", "tokens": ["Und", "ler\u00b7nen", "will", "ich", "auf", "dei\u00b7nen", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was der Mensch bewirkt und erschafft,", "tokens": ["Was", "der", "Mensch", "be\u00b7wirkt", "und", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wenn er dem Gott im Busen vertraut,", "tokens": ["Wenn", "er", "dem", "Gott", "im", "Bu\u00b7sen", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der selbstgegebenen Kraft.", "tokens": ["Und", "der", "selbst\u00b7ge\u00b7ge\u00b7be\u00b7nen", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Dann kehr ich heim mit stolzem Sinn", "tokens": ["Dann", "kehr", "ich", "heim", "mit", "stol\u00b7zem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaff in ges\u00e4ttigter Ruh,", "tokens": ["Und", "schaff", "in", "ge\u00b7s\u00e4t\u00b7tig\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Was jung soll sein, wie ich es bin,", "tokens": ["Was", "jung", "soll", "sein", ",", "wie", "ich", "es", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VMFIN", "VAINF", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alt soll werden, wie du.", "tokens": ["Und", "alt", "soll", "wer\u00b7den", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VAINF", "$,", "PWAV", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}