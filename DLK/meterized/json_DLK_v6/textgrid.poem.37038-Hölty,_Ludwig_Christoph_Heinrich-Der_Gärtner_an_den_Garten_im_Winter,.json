{"textgrid.poem.37038": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner an den Garten im Winter,", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Silberh\u00fcllen eingeschleyert", "tokens": ["In", "Sil\u00b7ber\u00b7h\u00fcl\u00b7len", "ein\u00b7ge\u00b7schley\u00b7ert"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht jetzt der Baum,", "tokens": ["Steht", "jetzt", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und strecket seine nackten \u00c4ste", "tokens": ["Und", "stre\u00b7cket", "sei\u00b7ne", "nack\u00b7ten", "\u00c4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Himmel zu.", "tokens": ["Dem", "Him\u00b7mel", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo j\u00fcngst das reife Gold des Fruchtbaums", "tokens": ["Wo", "j\u00fcngst", "das", "rei\u00b7fe", "Gold", "des", "Frucht\u00b7baums"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geblinket, h\u00e4ngt", "tokens": ["Ge\u00b7blin\u00b7ket", ",", "h\u00e4ngt"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jetzt Ei\u00df herab, das keine Sonne", "tokens": ["Jetzt", "Ei\u00df", "her\u00b7ab", ",", "das", "kei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerschmelzen kan.", "tokens": ["Zer\u00b7schmel\u00b7zen", "kan", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Entbl\u00e4ttert steht die Rebenlaube,", "tokens": ["Ent\u00b7bl\u00e4t\u00b7tert", "steht", "die", "Re\u00b7ben\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich in Nacht", "tokens": ["Die", "mich", "in", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verschlo\u00df, wenn Phoebus flammenathmend", "tokens": ["Ver\u00b7schlo\u00df", ",", "wenn", "Phoe\u00b7bus", "flam\u00b7me\u00b7nath\u00b7mend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herniedersah.", "tokens": ["Her\u00b7nie\u00b7der\u00b7sah", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das Blumenbeet, wo Florens T\u00f6chter", "tokens": ["Das", "Blu\u00b7men\u00b7beet", ",", "wo", "Flo\u00b7rens", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Morgenroth", "tokens": ["In", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gekleidet, Wohlgeruch verhauchten,", "tokens": ["Ge\u00b7klei\u00b7det", ",", "Wohl\u00b7ge\u00b7ruch", "ver\u00b7hauch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versinkt in Schnee.", "tokens": ["Ver\u00b7sinkt", "in", "Schnee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nur du, mein kleiner Buchsbaum, pflanzest", "tokens": ["Nur", "du", ",", "mein", "klei\u00b7ner", "Buchs\u00b7baum", ",", "pflan\u00b7zest"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein gr\u00fcnes Haupt", "tokens": ["Dein", "gr\u00fc\u00b7nes", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem Frost entgegen, und verh\u00f6hnest", "tokens": ["Dem", "Frost", "ent\u00b7ge\u00b7gen", ",", "und", "ver\u00b7h\u00f6h\u00b7nest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Winters Macht.", "tokens": ["Des", "Win\u00b7ters", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mit Goldschaum \u00fcberzogen, funkelst", "tokens": ["Mit", "Gold\u00b7schaum", "\u00fc\u00b7berz\u00b7o\u00b7gen", ",", "fun\u00b7kelst"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du an der Brust", "tokens": ["Du", "an", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des M\u00e4dchens, das die Dorfschalmeye", "tokens": ["Des", "M\u00e4d\u00b7chens", ",", "das", "die", "Dorf\u00b7schal\u00b7me\u00b7ye"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zum Tanze ruft.", "tokens": ["Zum", "Tan\u00b7ze", "ruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ruh sanft mein Garten, bis der Fr\u00fchling", "tokens": ["Ruh", "sanft", "mein", "Gar\u00b7ten", ",", "bis", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Erde sinkt,", "tokens": ["Zur", "Er\u00b7de", "sinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Silberkr\u00e4nze auf die Wipfel", "tokens": ["Und", "Sil\u00b7ber\u00b7kr\u00e4n\u00b7ze", "auf", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der B\u00e4ume streut.", "tokens": ["Der", "B\u00e4u\u00b7me", "streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Dann gaukelt Zephyr in den Bl\u00fcthen,", "tokens": ["Dann", "gau\u00b7kelt", "Ze\u00b7phyr", "in", "den", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00dfet sie,", "tokens": ["Und", "k\u00fc\u00b7\u00dfet", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und weht mir mit den D\u00fcften Freude", "tokens": ["Und", "weht", "mir", "mit", "den", "D\u00fcf\u00b7ten", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meine Brust.", "tokens": ["In", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}