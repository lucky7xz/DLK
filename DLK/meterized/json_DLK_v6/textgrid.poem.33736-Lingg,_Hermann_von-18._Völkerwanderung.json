{"textgrid.poem.33736": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "18. V\u00f6lkerwanderung", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch wenn ein Rom den Erdenkreis ersch\u00fcttert,", "tokens": ["Doch", "wenn", "ein", "Rom", "den", "Er\u00b7den\u00b7kreis", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indem es st\u00fcrzt, wenn durch die finstern Zeiten", "tokens": ["In\u00b7dem", "es", "st\u00fcrzt", ",", "wenn", "durch", "die", "fins\u00b7tern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bahrtuch \u00fcber die Gefallnen breiten", "tokens": ["Ein", "Bahr\u00b7tuch", "\u00fc\u00b7ber", "die", "Ge\u00b7fall\u00b7nen", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befreite V\u00f6lker, deren Joch zersplittert:", "tokens": ["Be\u00b7frei\u00b7te", "V\u00f6l\u00b7ker", ",", "de\u00b7ren", "Joch", "zer\u00b7split\u00b7tert", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann dr\u00f6hnt der Boden, und die Luft erzittert", "tokens": ["Dann", "dr\u00f6hnt", "der", "Bo\u00b7den", ",", "und", "die", "Luft", "er\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von k\u00fchnen Wanderungen; Riesen streiten,", "tokens": ["Von", "k\u00fch\u00b7nen", "Wan\u00b7de\u00b7run\u00b7gen", ";", "Rie\u00b7sen", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heroen sieht man und Giganten schreiten", "tokens": ["He\u00b7roen", "sieht", "man", "und", "Gi\u00b7gan\u00b7ten", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und jenen Werwolf, der das Weltend' wittert.", "tokens": ["Und", "je\u00b7nen", "Wer\u00b7wolf", ",", "der", "das", "Wel\u00b7tend'", "wit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Noch einmal stehn die S\u00f6hne gro\u00dfer V\u00e4ter", "tokens": ["Noch", "ein\u00b7mal", "stehn", "die", "S\u00f6h\u00b7ne", "gro\u00b7\u00dfer", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Taten auf, noch einmal gl\u00fcht im \u00c4ther", "tokens": ["Zu", "Ta\u00b7ten", "auf", ",", "noch", "ein\u00b7mal", "gl\u00fcht", "im", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alten G\u00f6tter Stern, dann bleich, dann nimmer.", "tokens": ["Der", "al\u00b7ten", "G\u00f6t\u00b7ter", "Stern", ",", "dann", "bleich", ",", "dann", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ein Volk sieht man \u00fcber alle wandern", "tokens": ["Und", "ein", "Volk", "sieht", "man", "\u00fc\u00b7ber", "al\u00b7le", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis zu des fernsten Tages Abendschimmer,", "tokens": ["Bis", "zu", "des", "ferns\u00b7ten", "Ta\u00b7ges", "A\u00b7bend\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verh\u00f6hnt, verha\u00dft, verfolgt von allen andern.", "tokens": ["Ver\u00b7h\u00f6hnt", ",", "ver\u00b7ha\u00dft", ",", "ver\u00b7folgt", "von", "al\u00b7len", "an\u00b7dern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVFIN", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}