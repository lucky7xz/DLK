{"dta.poem.13117": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewie vieles h\u00e4tt ich nicht der theuren Kunst zu lieb", "tokens": ["\u201e", "wie", "vie\u00b7les", "h\u00e4tt", "ich", "nicht", "der", "theu\u00b7ren", "Kunst", "zu", "lieb"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PIS", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "510\u201dEuch anzuzeigen? nein: ich hemme meinen Trieb;", "tokens": ["\"", "Euch", "an\u00b7zu\u00b7zei\u00b7gen", "?", "nein", ":", "ich", "hem\u00b7me", "mei\u00b7nen", "Trieb", ";"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVIZU", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eerwarte den Entschlu\u00df des Tugend-Raths gelassen", "tokens": ["\u201e", "er\u00b7war\u00b7te", "den", "Ent\u00b7schlu\u00df", "des", "Tu\u00b7gen\u00b7dRaths", "ge\u00b7las\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund stehe nimmer an, er sey f\u00fcr mich zu fassen.", "tokens": ["\u201e", "und", "ste\u00b7he", "nim\u00b7mer", "an", ",", "er", "sey", "f\u00fcr", "mich", "zu", "fas\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich wei\u00df, man g\u00f6nnet mir Recht, Vorzug, Rang und Ehr,", "tokens": ["\u201e", "ich", "wei\u00df", ",", "man", "g\u00f6n\u00b7net", "mir", "Recht", ",", "Vor\u00b7zug", ",", "Rang", "und", "Ehr", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso ich mit Billigkeit f\u00fcr meine Kunst begehr.", "tokens": ["\u201e", "so", "ich", "mit", "Bil\u00b7lig\u00b7keit", "f\u00fcr", "mei\u00b7ne", "Kunst", "be\u00b7gehr", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "515\u201dEs kommt nur auf den Schlu\u00df, zu was vor einem Wercke", "tokens": ["\"", "Es", "kommt", "nur", "auf", "den", "Schlu\u00df", ",", "zu", "was", "vor", "ei\u00b7nem", "Wer\u00b7cke"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eder Dicht-Kunst Treflichkeit, und Welt-bekannte St\u00e4rcke", "tokens": ["\u201e", "der", "Dicht\u00b7Kunst", "Tref\u00b7lich\u00b7keit", ",", "und", "Welt\u00b7be\u00b7kann\u00b7te", "St\u00e4r\u00b7cke"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201enun anzuwenden sey. Die Cither ist bereit.", "tokens": ["\u201e", "nun", "an\u00b7zu\u00b7wen\u00b7den", "sey", ".", "Die", "Cit\u00b7her", "ist", "be\u00b7reit", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIZU", "VAFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eermuntre dich mein Geist! zeig Stolz und Tapferkeit!", "tokens": ["\u201e", "er\u00b7munt\u00b7re", "dich", "mein", "Geist", "!", "zeig", "Stolz", "und", "Tap\u00b7fer\u00b7keit", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ekr\u00f6n deiner Tugend Werth! verlach der Mi\u00dfgunst Bellen!", "tokens": ["\u201e", "kr\u00f6n", "dei\u00b7ner", "Tu\u00b7gend", "Werth", "!", "ver\u00b7lach", "der", "Mi\u00df\u00b7gunst", "Bel\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPOSAT", "NN", "NN", "$.", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "520\u201dDer Worte Prei\u00df und Ruhm mu\u00df aus der That erhellen!", "tokens": ["\"", "Der", "Wor\u00b7te", "Prei\u00df", "und", "Ruhm", "mu\u00df", "aus", "der", "That", "er\u00b7hel\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201everschm\u00e4ht man aber mich ..... \u201eSo \u201e, sprach der gute Rath,", "tokens": ["\u201e", "ver\u00b7schm\u00e4ht", "man", "a\u00b7ber", "mich", ".....", "\u201e", "So", "\u201e", ",", "sprach", "der", "gu\u00b7te", "Rath", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "ADV", "PPER", "$.", "$(", "ADV", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eso glaub ich da\u00df der Krei\u00df genug vernommen hat.", "tokens": ["\u201e", "so", "glaub", "ich", "da\u00df", "der", "Krei\u00df", "ge\u00b7nug", "ver\u00b7nom\u00b7men", "hat", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eich unterbreche dich; nicht da\u00df ich dich verlache;", "tokens": ["\u201e", "ich", "un\u00b7ter\u00b7bre\u00b7che", "dich", ";", "nicht", "da\u00df", "ich", "dich", "ver\u00b7la\u00b7che", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "PTKNEG", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201enur da\u00df ich dem Gesang der Leyr ein Ende mache.", "tokens": ["\u201e", "nur", "da\u00df", "ich", "dem", "Ge\u00b7sang", "der", "Leyr", "ein", "En\u00b7de", "ma\u00b7che", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "525\u201dWir wissen insgesamt was jede Kunst vermag,", "tokens": ["\"", "Wir", "wis\u00b7sen", "ins\u00b7ge\u00b7samt", "was", "je\u00b7de", "Kunst", "ver\u00b7mag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201ewas deine Fl\u00f6te kann, und deiner Cither Schlag.", "tokens": ["\u201e", "was", "dei\u00b7ne", "Fl\u00f6\u00b7te", "kann", ",", "und", "dei\u00b7ner", "Cit\u00b7her", "Schlag", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VMFIN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}