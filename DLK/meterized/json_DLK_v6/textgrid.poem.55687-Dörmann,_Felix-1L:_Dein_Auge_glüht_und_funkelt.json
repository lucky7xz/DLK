{"textgrid.poem.55687": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Auge gl\u00fcht und funkelt", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge gl\u00fcht und funkelt", "tokens": ["Dein", "Au\u00b7ge", "gl\u00fcht", "und", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Morgensonnenlicht,", "tokens": ["Wie", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meines, qualenverdunkelt,", "tokens": ["Und", "mei\u00b7nes", ",", "qua\u00b7len\u00b7ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "$,", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von fressendem Elend spricht.", "tokens": ["Von", "fres\u00b7sen\u00b7dem", "E\u00b7lend", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was treibt den asketischen Schw\u00e4rmer", "tokens": ["Was", "treibt", "den", "as\u00b7ke\u00b7ti\u00b7schen", "Schw\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu Deines Daseins Pracht?", "tokens": ["Zu", "Dei\u00b7nes", "Da\u00b7seins", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kehrt ja nur schroffer und \u00e4rmer", "tokens": ["Er", "kehrt", "ja", "nur", "schrof\u00b7fer", "und", "\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zur\u00fcck in die heimische Nacht!", "tokens": ["Zu\u00b7r\u00fcck", "in", "die", "hei\u00b7mi\u00b7sche", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}