{"dta.poem.16063": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott, der Luftwassererdundfeuergeister schuf,", "tokens": ["Gott", ",", "der", "Luft\u00b7was\u00b7ser\u00b7er\u00b7dund\u00b7feu\u00b7er\u00b7geis\u00b7ter", "schuf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gab jedem eignen Sinn und eigenen Beruf.", "tokens": ["Gab", "je\u00b7dem", "eig\u00b7nen", "Sinn", "und", "ei\u00b7ge\u00b7nen", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Menschen schuf er nicht aus Fluten noch aus Flammen,", "tokens": ["Den", "Men\u00b7schen", "schuf", "er", "nicht", "aus", "Flu\u00b7ten", "noch", "aus", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Lufthauch noch aus Staub, aus alle dem zusammen.", "tokens": ["Aus", "Luft\u00b7hauch", "noch", "aus", "Staub", ",", "aus", "al\u00b7le", "dem", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "$,", "APPR", "PIS", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du kanst bald diesem Geist, und jenem bald verfallen,", "tokens": ["Du", "kanst", "bald", "die\u00b7sem", "Geist", ",", "und", "je\u00b7nem", "bald", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PDAT", "NN", "$,", "KON", "PDAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Aller Einheit sollst du seyn, nicht eins von allen.", "tokens": ["Doch", "Al\u00b7ler", "Ein\u00b7heit", "sollst", "du", "seyn", ",", "nicht", "eins", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,", "PTKNEG", "PIS", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}