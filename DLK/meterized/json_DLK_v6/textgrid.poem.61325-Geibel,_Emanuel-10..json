{"textgrid.poem.61325": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um keinen Preis gestehe du", "tokens": ["Um", "kei\u00b7nen", "Preis", "ge\u00b7ste\u00b7he", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mittelm\u00e4\u00dfigkeit was zu.", "tokens": ["Der", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit", "was", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du dich erst mit ihr vertragen,", "tokens": ["Hast", "du", "dich", "erst", "mit", "ihr", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So wird dir's bald bei ihr behagen,", "tokens": ["So", "wird", "dir's", "bald", "bei", "ihr", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis du zuletzt, du wei\u00dft nicht wie,", "tokens": ["Bis", "du", "zu\u00b7letzt", ",", "du", "wei\u00dft", "nicht", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geworden bist so flach wie sie.", "tokens": ["Ge\u00b7wor\u00b7den", "bist", "so", "flach", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}