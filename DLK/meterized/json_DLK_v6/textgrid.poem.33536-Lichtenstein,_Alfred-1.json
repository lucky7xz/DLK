{"textgrid.poem.33536": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du Berlin, du bunter Stein, du Biest.", "tokens": ["O", "du", "Ber\u00b7lin", ",", "du", "bun\u00b7ter", "Stein", ",", "du", "Biest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wirfst mich mit Laternen wie mit Kletten.", "tokens": ["Du", "wirfst", "mich", "mit", "La\u00b7ter\u00b7nen", "wie", "mit", "Klet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, wenn man nachts durch deine Lichter flie\u00dft", "tokens": ["Ach", ",", "wenn", "man", "nachts", "durch", "dei\u00b7ne", "Lich\u00b7ter", "flie\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Weibern nach, den seidenen, den fetten.", "tokens": ["Den", "Wei\u00b7bern", "nach", ",", "den", "sei\u00b7de\u00b7nen", ",", "den", "fet\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So taumelnd wird man von den Augenspielen.", "tokens": ["So", "tau\u00b7melnd", "wird", "man", "von", "den", "Au\u00b7gen\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Himmel s\u00fc\u00dft der kleine Mondbonbon.", "tokens": ["Den", "Him\u00b7mel", "s\u00fc\u00dft", "der", "klei\u00b7ne", "Mond\u00b7bon\u00b7bon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn schon die Tage auf die T\u00fcrme fielen", "tokens": ["Wenn", "schon", "die", "Ta\u00b7ge", "auf", "die", "T\u00fcr\u00b7me", "fie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gl\u00fcht noch der Kopf, ein roter Lampion.", "tokens": ["Gl\u00fcht", "noch", "der", "Kopf", ",", "ein", "ro\u00b7ter", "Lam\u00b7pi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}