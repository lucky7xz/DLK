{"textgrid.poem.47979": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da klingt im Wind das alte Lied", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da klingt im Wind das alte Lied", "tokens": ["Da", "klingt", "im", "Wind", "das", "al\u00b7te", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "voll Seufzer und voll Tr\u00e4nen \u2013", "tokens": ["voll", "Seuf\u00b7zer", "und", "voll", "Tr\u00e4\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "durch meine m\u00fcde Seele zieht", "tokens": ["durch", "mei\u00b7ne", "m\u00fc\u00b7de", "See\u00b7le", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein namenloses Sehnen;", "tokens": ["ein", "na\u00b7men\u00b7lo\u00b7ses", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "es ist, als ging ich ganz allein", "tokens": ["es", "ist", ",", "als", "ging", "ich", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "auf schneeverwehter Halde", "tokens": ["auf", "schnee\u00b7ver\u00b7weh\u00b7ter", "Hal\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und tr\u00e4umt vom goldnen Sonnenschein,", "tokens": ["und", "tr\u00e4umt", "vom", "gold\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dem ersten Gr\u00fcn im Walde.", "tokens": ["dem", "ers\u00b7ten", "Gr\u00fcn", "im", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du wonnesel'ge Jugendzeit,", "tokens": ["Du", "won\u00b7ne\u00b7sel'\u00b7ge", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "heut la\u00df mich dein gedenken,", "tokens": ["heut", "la\u00df", "mich", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "in deine Tiefen all das Leid", "tokens": ["in", "dei\u00b7ne", "Tie\u00b7fen", "all", "das", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des grauen Jetzt versenken, \u2013", "tokens": ["des", "grau\u00b7en", "Jetzt", "ver\u00b7sen\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df wie ein Blumenkelch betaut", "tokens": ["da\u00df", "wie", "ein", "Blu\u00b7men\u00b7kelch", "be\u00b7taut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mein Aug' noch einmal strahle;", "tokens": ["mein", "Aug'", "noch", "ein\u00b7mal", "strah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "du lockst so s\u00fc\u00df wie Glockenlaut", "tokens": ["du", "lockst", "so", "s\u00fc\u00df", "wie", "Glo\u00b7cken\u00b7laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in meinem Heimattale.", "tokens": ["in", "mei\u00b7nem", "Hei\u00b7mat\u00b7ta\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So sei gegr\u00fc\u00dft, mein Morgenstern,", "tokens": ["So", "sei", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "um den die Nebel weben!", "tokens": ["um", "den", "die", "Ne\u00b7bel", "we\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du warst ein Traumbild, licht und fern,", "tokens": ["Du", "warst", "ein", "Traum\u00b7bild", ",", "licht", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "doch wert ein ganzes Leben!", "tokens": ["doch", "wert", "ein", "gan\u00b7zes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und ob ich abgrundtief in Pein,", "tokens": ["und", "ob", "ich", "ab\u00b7grund\u00b7tief", "in", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in Schuld und Weh vers\u00e4nke:", "tokens": ["in", "Schuld", "und", "Weh", "ver\u00b7s\u00e4n\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich kann nicht ganz verloren sein,", "tokens": ["Ich", "kann", "nicht", "ganz", "ver\u00b7lo\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "so lang ich dein gedenke.", "tokens": ["so", "lang", "ich", "dein", "ge\u00b7den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}