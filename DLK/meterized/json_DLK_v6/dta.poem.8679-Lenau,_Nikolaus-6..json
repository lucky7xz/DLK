{"dta.poem.8679": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schon seh' ich Hirt' und Herde nimmer", "tokens": ["Schon", "seh'", "ich", "Hirt'", "und", "Her\u00b7de", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zirbel nur ist mein Geleit;", "tokens": ["Die", "Zir\u00b7bel", "nur", "ist", "mein", "Ge\u00b7leit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der steile Pfad wird steiler immer,", "tokens": ["Der", "stei\u00b7le", "Pfad", "wird", "stei\u00b7ler", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es w\u00e4chst die wilde Einsamkeit.", "tokens": ["Es", "w\u00e4chst", "die", "wil\u00b7de", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort st\u00fcrzt aus dunkler Felsenpforte", "tokens": ["Dort", "st\u00fcrzt", "aus", "dunk\u00b7ler", "Fel\u00b7sen\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Quell mit einem bangen Schrei,", "tokens": ["Der", "Quell", "mit", "ei\u00b7nem", "ban\u00b7gen", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Enteilt dem grauenvollen Orte,", "tokens": ["Ent\u00b7eilt", "dem", "grau\u00b7en\u00b7vol\u00b7len", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab zum freundlich gr\u00fcnen Mai.", "tokens": ["Hin\u00b7ab", "zum", "freund\u00b7lich", "gr\u00fc\u00b7nen", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschwunden ist das lezte Leben,", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "das", "lez\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier gr\u00fcnt kein Blatt, kein Vogel ruft,", "tokens": ["Hier", "gr\u00fcnt", "kein", "Blatt", ",", "kein", "Vo\u00b7gel", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selbst der Pfad scheint hier zu beben,", "tokens": ["Und", "selbst", "der", "Pfad", "scheint", "hier", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So zwischen Wand und Todeskluft.", "tokens": ["So", "zwi\u00b7schen", "Wand", "und", "To\u00b7des\u00b7kluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, Gottesl\u00e4ugner, Gott zu f\u00fchlen,", "tokens": ["Komm", ",", "Got\u00b7tes\u00b7l\u00e4ug\u00b7ner", ",", "Gott", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Frevel wird auf diesem Rand", "tokens": ["Dein", "Fre\u00b7vel", "wird", "auf", "die\u00b7sem", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Todesabgrund tiefer w\u00fchlen,", "tokens": ["Den", "To\u00b7des\u00b7ab\u00b7grund", "tie\u00b7fer", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir steiler th\u00fcrmen diese Wand! \u2014", "tokens": ["Dir", "stei\u00b7ler", "th\u00fcr\u00b7men", "die\u00b7se", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}