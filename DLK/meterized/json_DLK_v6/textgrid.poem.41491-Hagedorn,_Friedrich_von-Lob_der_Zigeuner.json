{"textgrid.poem.41491": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Zigeuner", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uraltes Landvolk, eure H\u00fctten", "tokens": ["Ur\u00b7al\u00b7tes", "Land\u00b7volk", ",", "eu\u00b7re", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschont der St\u00e4dter Stolz und Neid;", "tokens": ["Ver\u00b7schont", "der", "St\u00e4d\u00b7ter", "Stolz", "und", "Neid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fehlt es euch an feinen Sitten,", "tokens": ["Und", "fehlt", "es", "euch", "an", "fei\u00b7nen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fehlt's euch nicht an Fr\u00f6hlichkeit,", "tokens": ["So", "fehlt's", "euch", "nicht", "an", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr scherzt auf Gras und unter Zweigen,", "tokens": ["Ihr", "scherzt", "auf", "Gras", "und", "un\u00b7ter", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn' allen Zwang und ohne Zeugen.", "tokens": ["Ohn'", "al\u00b7len", "Zwang", "und", "oh\u00b7ne", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr \u00fcbet euch in steten Reisen;", "tokens": ["Ihr", "\u00fc\u00b7bet", "euch", "in", "ste\u00b7ten", "Rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist euer Vaterland.", "tokens": ["Die", "Welt", "ist", "eu\u00b7er", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man lobte dies an alten Weisen,", "tokens": ["Man", "lob\u00b7te", "dies", "an", "al\u00b7ten", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur in euch wird's nicht erkannt.", "tokens": ["Und", "nur", "in", "euch", "wird's", "nicht", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum? Ihr gleichet nicht den Reichen,", "tokens": ["Wa\u00b7rum", "?", "Ihr", "glei\u00b7chet", "nicht", "den", "Rei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die pr\u00e4chtig durch die Fremde streichen.", "tokens": ["Die", "pr\u00e4ch\u00b7tig", "durch", "die", "Frem\u00b7de", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu gro\u00dfe Furcht, zu gro\u00dfes Hoffen", "tokens": ["Zu", "gro\u00b7\u00dfe", "Furcht", ",", "zu", "gro\u00b7\u00dfes", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht oft die Kl\u00fcgsten unruhvoll.", "tokens": ["Macht", "oft", "die", "Kl\u00fcgs\u00b7ten", "un\u00b7ruh\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch steht das Buch des Schicksals offen:", "tokens": ["Euch", "steht", "das", "Buch", "des", "Schick\u00b7sals", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr weissagt, was geschehen soll.", "tokens": ["Ihr", "weis\u00b7sagt", ",", "was", "ge\u00b7sche\u00b7hen", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will man geheime Dinge wissen,", "tokens": ["Will", "man", "ge\u00b7hei\u00b7me", "Din\u00b7ge", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird man euch befragen m\u00fcssen.", "tokens": ["So", "wird", "man", "euch", "be\u00b7fra\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es wird der Muth euch angeboren:", "tokens": ["Es", "wird", "der", "Muth", "euch", "an\u00b7ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kennt nicht eure Streitbarkeit?", "tokens": ["Wer", "kennt", "nicht", "eu\u00b7re", "Streit\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von euch wird keine Schlacht verloren,", "tokens": ["Von", "euch", "wird", "kei\u00b7ne", "Schlacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wo ihr \u00fcbermannet seid.", "tokens": ["Als", "wo", "ihr", "\u00fc\u00b7berm\u00b7an\u00b7net", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann suchet ihr zwar nicht zu fliehen,", "tokens": ["Dann", "su\u00b7chet", "ihr", "zwar", "nicht", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch zierlich euch zur\u00fcck zu ziehen.", "tokens": ["Doch", "zier\u00b7lich", "euch", "zu\u00b7r\u00fcck", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man wei\u00df, ihr z\u00e4hlet wenig Freunde;", "tokens": ["Man", "wei\u00df", ",", "ihr", "z\u00e4h\u00b7let", "we\u00b7nig", "Freun\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein ihr kennt den Lauf der Welt.", "tokens": ["Al\u00b7lein", "ihr", "kennt", "den", "Lauf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gr\u00f6\u00dften haben ihre Feinde:", "tokens": ["Die", "Gr\u00f6\u00df\u00b7ten", "ha\u00b7ben", "ih\u00b7re", "Fein\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdiensten wird stets nachgestellt.", "tokens": ["Ver\u00b7diens\u00b7ten", "wird", "stets", "nach\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mancher R\u00f6mer wird gepriesen,", "tokens": ["Wie", "man\u00b7cher", "R\u00f6\u00b7mer", "wird", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den die Gewalt, wie euch, verwiesen!", "tokens": ["Den", "die", "Ge\u00b7walt", ",", "wie", "euch", ",", "ver\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr rennet nicht nach hohen Ehren:", "tokens": ["Ihr", "ren\u00b7net", "nicht", "nach", "ho\u00b7hen", "Eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr w\u00fcnscht euch nicht an Titeln reich.", "tokens": ["Ihr", "w\u00fcnscht", "euch", "nicht", "an", "Ti\u00b7teln", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Zwiespalt in geweihten Lehren,", "tokens": ["Kein", "Zwies\u00b7palt", "in", "ge\u00b7weih\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Federkrieg verhetzet euch.", "tokens": ["Kein", "Fe\u00b7der\u00b7krieg", "ver\u00b7het\u00b7zet", "euch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr seid (was kann den Vorzug rauben?)", "tokens": ["Ihr", "seid", "(", "was", "kann", "den", "Vor\u00b7zug", "rau\u00b7ben", "?", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWS", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Einer Farb' und Einem Glauben.", "tokens": ["Von", "Ei\u00b7ner", "Fa\u00b7rb'", "und", "Ei\u00b7nem", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}