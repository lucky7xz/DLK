{"dta.poem.20892": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich brannte vor der Zeit um meine junge Brust/", "tokens": ["Ich", "brann\u00b7te", "vor", "der", "Zeit", "um", "mei\u00b7ne", "jun\u00b7ge", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu schauen was die Welt in ihrem Busem tr\u00e4get/", "tokens": ["Zu", "schau\u00b7en", "was", "die", "Welt", "in", "ih\u00b7rem", "Bu\u00b7sem", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mein geschwinder Geist hielt vor die gr\u00f6ste Lust/", "tokens": ["Und", "mein", "ge\u00b7schwin\u00b7der", "Geist", "hielt", "vor", "die", "gr\u00f6s\u00b7te", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu wissen wie der Trieb sich der Natur beweget/", "tokens": ["Zu", "wis\u00b7sen", "wie", "der", "Trieb", "sich", "der", "Na\u00b7tur", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das alles war f\u00fcr mich die allers\u00fcste Kost.", "tokens": ["Das", "al\u00b7les", "war", "f\u00fcr", "mich", "die", "al\u00b7ler\u00b7s\u00fcs\u00b7te", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seele wa", "tokens": ["Die", "See\u00b7le", "wa"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es f\u00fchrte mich der Trieb zur Wissenschafft und Kunst/", "tokens": ["Es", "f\u00fchr\u00b7te", "mich", "der", "Trieb", "zur", "Wis\u00b7sen\u00b7schafft", "und", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Fortgang tr\u00f6ste mich in kurtzer Zeit zu siegen/", "tokens": ["Der", "Fort\u00b7gang", "tr\u00f6s\u00b7te", "mich", "in", "kurt\u00b7zer", "Zeit", "zu", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es war kein Augenblick vergebens und umsonst/", "tokens": ["Es", "war", "kein", "Au\u00b7gen\u00b7blick", "ver\u00b7ge\u00b7bens", "und", "um\u00b7sonst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich ma\u00df fast allbereit/ wie hoch ich war gestiegen.", "tokens": ["Ich", "ma\u00df", "fast", "all\u00b7be\u00b7reit", "/", "wie", "hoch", "ich", "war", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich hielt gewi\u00df davor/ da\u00df ein recht edel Blut", "tokens": ["Ich", "hielt", "ge\u00b7wi\u00df", "da\u00b7vor", "/", "da\u00df", "ein", "recht", "e\u00b7del", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$(", "KOUS", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genau erwegen soll/ wie alle Sachen stehen/", "tokens": ["Ge\u00b7nau", "er\u00b7we\u00b7gen", "soll", "/", "wie", "al\u00b7le", "Sa\u00b7chen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$(", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wissen wie sich regt der Sonnen klare Glut/", "tokens": ["Und", "wis\u00b7sen", "wie", "sich", "regt", "der", "Son\u00b7nen", "kla\u00b7re", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PRF", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum die Thiere hier bald werden/ bald vergehen/", "tokens": ["Wa\u00b7rum", "die", "Thie\u00b7re", "hier", "bald", "wer\u00b7den", "/", "bald", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VAINF", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df alles sch\u00e4rffte mir iemehr und mehr den Muth.", "tokens": ["Di\u00df", "al\u00b7les", "sch\u00e4rff\u00b7te", "mir", "ie\u00b7mehr", "und", "mehr", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nachdem mein leichter Geist genug herum gegangen/", "tokens": ["Nach\u00b7dem", "mein", "leich\u00b7ter", "Geist", "ge\u00b7nug", "he\u00b7rum", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ward ich endlich fast auf die Gedancken bracht/", "tokens": ["So", "ward", "ich", "end\u00b7lich", "fast", "auf", "die", "Ge\u00b7dan\u00b7cken", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob der ", "tokens": ["Ob", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "In dem das F\u00e4ulni\u00df sich zu Hitz und K\u00e4lte macht/", "tokens": ["In", "dem", "das", "F\u00e4ul\u00b7ni\u00df", "sich", "zu", "Hitz", "und", "K\u00e4l\u00b7te", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df alle Thier allhier die rechte Krafft empfangen.", "tokens": ["Da\u00df", "al\u00b7le", "Thier", "all\u00b7hier", "die", "rech\u00b7te", "Krafft", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}