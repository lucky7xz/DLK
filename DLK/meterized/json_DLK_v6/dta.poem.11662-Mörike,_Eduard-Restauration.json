{"dta.poem.11662": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Restauration", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat mir all mein Ged\u00e4rm erschlafft.", "tokens": ["Es", "hat", "mir", "all", "mein", "Ge\u00b7d\u00e4rm", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es roch, ich will des Henkers seyn,", "tokens": ["Es", "roch", ",", "ich", "will", "des", "Hen\u00b7kers", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lauter welke Rosen und Camille-Bl\u00fcmlein.", "tokens": ["Wie", "lau\u00b7ter", "wel\u00b7ke", "Ro\u00b7sen", "und", "Ca\u00b7mil\u00b7le\u00b7B\u00b7l\u00fcm\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mir ward ganz \u00fcbel, mauserig, dumm,", "tokens": ["Mir", "ward", "ganz", "\u00fc\u00b7bel", ",", "mau\u00b7se\u00b7rig", ",", "dumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ich sah mich schnell nach was T\u00fcchtigem um,", "tokens": ["Ich", "sah", "mich", "schnell", "nach", "was", "T\u00fcch\u00b7ti\u00b7gem", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PRELS", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lief in den Garten hinter'm Haus,", "tokens": ["Lief", "in", "den", "Gar\u00b7ten", "hin\u00b7ter'm", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zog einen herzhaften Rettig aus,", "tokens": ["Zog", "ei\u00b7nen", "herz\u00b7haf\u00b7ten", "Ret\u00b7tig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Fra\u00df ihn auch auf bis auf den Schwanz,", "tokens": ["Fra\u00df", "ihn", "auch", "auf", "bis", "auf", "den", "Schwanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da war ich wieder frisch und genesen ganz.", "tokens": ["Da", "war", "ich", "wie\u00b7der", "frisch", "und", "ge\u00b7ne\u00b7sen", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVPP", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}