{"dta.poem.10968": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahrheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein gro\u00df-zugleich auch kleiner Geist, dein unverst\u00e4ndiger\nVerstand,", "tokens": ["Dein", "gro\u00df\u00b7zu\u00b7gleich", "auch", "klei\u00b7ner", "Geist", ",", "dein", "un\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7ger", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADV", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "O Mensch! womit du sehr gerecht, (zugleich Partey und", "tokens": ["O", "Mensch", "!", "wo\u00b7mit", "du", "sehr", "ge\u00b7recht", ",", "(", "zu\u00b7gleich", "Par\u00b7tey", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "PPER", "ADV", "ADJD", "$,", "$(", "ADV", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mehrentheils zu deinem Vortheil, die allermeisten Din-", "tokens": ["Und", "meh\u00b7ren\u00b7theils", "zu", "dei\u00b7nem", "Vor\u00b7theil", ",", "die", "al\u00b7ler\u00b7meis\u00b7ten", "Din"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Spricht k\u00fchn: So wie mir alles fast, so ist auch dieses mir", "tokens": ["Spricht", "k\u00fchn", ":", "So", "wie", "mir", "al\u00b7les", "fast", ",", "so", "ist", "auch", "die\u00b7ses", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$.", "ADV", "KOKOM", "PPER", "PIS", "ADV", "$,", "ADV", "VAFIN", "ADV", "PDAT", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Da\u00df alle Wahrheit einzig ist. Allein, wie du in vie-\nlen Sachen", "tokens": ["Da\u00df", "al\u00b7le", "Wahr\u00b7heit", "ein\u00b7zig", "ist", ".", "Al\u00b7lein", ",", "wie", "du", "in", "vie", "len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAFIN", "$.", "ADV", "$,", "PWAV", "PPER", "APPR", "TRUNC", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Dich \u00fcbereilst: So gehts auch hier.", "tokens": ["Dich", "\u00fc\u00b7be\u00b7reilst", ":", "So", "gehts", "auch", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist es nicht wahr, da\u00df drey und vier,", "tokens": ["Ist", "es", "nicht", "wahr", ",", "da\u00df", "drey", "und", "vier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jmgleichen sechs und eins sowohl, als zwey und f\u00fcnfe, sieben", "tokens": ["Jm\u00b7glei\u00b7chen", "sechs", "und", "eins", "so\u00b7wohl", ",", "als", "zwey", "und", "f\u00fcn\u00b7fe", ",", "sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "CARD", "KON", "CARD", "KON", "$,", "KOUS", "CARD", "KON", "VVFIN", "$,", "CARD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}