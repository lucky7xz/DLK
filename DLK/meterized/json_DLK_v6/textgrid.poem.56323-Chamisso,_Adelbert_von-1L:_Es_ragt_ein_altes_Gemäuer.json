{"textgrid.poem.56323": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ragt ein altes Gem\u00e4uer", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ragt ein altes Gem\u00e4uer", "tokens": ["Es", "ragt", "ein", "al\u00b7tes", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hervor aus Waldesnacht,", "tokens": ["Her\u00b7vor", "aus", "Wal\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl standen Kl\u00f6ster und Burgen", "tokens": ["Wohl", "stan\u00b7den", "Kl\u00f6s\u00b7ter", "und", "Bur\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einst dort in herrlicher Pracht.", "tokens": ["Einst", "dort", "in", "herr\u00b7li\u00b7cher", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es liegen im k\u00fchlen Grunde", "tokens": ["Es", "lie\u00b7gen", "im", "k\u00fch\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Behauene Steine gereiht:", "tokens": ["Be\u00b7hau\u00b7e\u00b7ne", "Stei\u00b7ne", "ge\u00b7reiht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dort schlummern die Frommen, die Starken,", "tokens": ["Dort", "schlum\u00b7mern", "die", "From\u00b7men", ",", "die", "Star\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die M\u00e4cht'gen der alten Zeit.", "tokens": ["Die", "M\u00e4cht'\u00b7gen", "der", "al\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was kommst du bei n\u00e4chtlicher Weile", "tokens": ["Was", "kommst", "du", "bei", "n\u00e4cht\u00b7li\u00b7cher", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Durchw\u00fchlen das alte Gestein?", "tokens": ["Durch\u00b7w\u00fch\u00b7len", "das", "al\u00b7te", "Ge\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und f\u00f6rderst herauf aus den Gr\u00e4bern \u2013", "tokens": ["Und", "f\u00f6r\u00b7derst", "her\u00b7auf", "aus", "den", "Gr\u00e4\u00b7bern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nur Staub und Totengebein!", "tokens": ["Nur", "Staub", "und", "To\u00b7ten\u00b7ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Unm\u00e4chtiger Sohn der Stunde,", "tokens": ["Un\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Sohn", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist der Zeiten Lauf.", "tokens": ["Das", "ist", "der", "Zei\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df ruhn, la\u00df ruhn die Toten,", "tokens": ["La\u00df", "ruhn", ",", "la\u00df", "ruhn", "die", "To\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du weckst sie mit Klagen nicht auf.", "tokens": ["Du", "weckst", "sie", "mit", "Kla\u00b7gen", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}