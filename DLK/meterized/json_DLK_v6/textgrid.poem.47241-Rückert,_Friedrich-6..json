{"textgrid.poem.47241": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schreibt\u00e4felchen im Busen", "tokens": ["Ein", "Schreib\u00b7t\u00e4\u00b7fel\u00b7chen", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ging ich in den Fr\u00fchlingswald;", "tokens": ["Ging", "ich", "in", "den", "Fr\u00fch\u00b7lings\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch, mir lang' entw\u00f6hnte Musen,", "tokens": ["Euch", ",", "mir", "lang'", "ent\u00b7w\u00f6hn\u00b7te", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht' ich auf und fand euch bald.", "tokens": ["Sucht'", "ich", "auf", "und", "fand", "euch", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In die Tafel auf den Knieen", "tokens": ["In", "die", "Ta\u00b7fel", "auf", "den", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Schrieb ich, was mir gab ein Hauch;", "tokens": ["Schrieb", "ich", ",", "was", "mir", "gab", "ein", "Hauch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich w\u00e4hnt' es mir verliehen", "tokens": ["Und", "ich", "w\u00e4hnt'", "es", "mir", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem nahen Bl\u00fctenstrauch.", "tokens": ["Von", "dem", "na\u00b7hen", "Bl\u00fc\u00b7ten\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch aus meiner Tafel wittert", "tokens": ["Doch", "aus", "mei\u00b7ner", "Ta\u00b7fel", "wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich ein andrer Odem an,", "tokens": ["Mich", "ein", "an\u00b7drer", "O\u00b7dem", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welke Blumen, halbzerknittert,", "tokens": ["Wel\u00b7ke", "Blu\u00b7men", ",", "halb\u00b7zer\u00b7knit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich j\u00fcngst dort eingethan,", "tokens": ["Die", "ich", "j\u00fcngst", "dort", "ein\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als zu meiner Kindheit Fluren", "tokens": ["Als", "zu", "mei\u00b7ner", "Kind\u00b7heit", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich der vor'ge Herbst gef\u00fchrt,", "tokens": ["Mich", "der", "vor'\u00b7ge", "Herbst", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich den verwehten Spuren", "tokens": ["Wo", "ich", "den", "ver\u00b7weh\u00b7ten", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fr\u00fchen Gl\u00fcckes nachgesp\u00fcrt.", "tokens": ["Fr\u00fc\u00b7hen", "Gl\u00fc\u00b7ckes", "nach\u00b7ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, so bist du nun gealtet!", "tokens": ["Ja", ",", "so", "bist", "du", "nun", "ge\u00b7al\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht der Fr\u00fchling, der nun bl\u00fcht,", "tokens": ["Nicht", "der", "Fr\u00fch\u00b7ling", ",", "der", "nun", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein l\u00e4ngst verbl\u00fchter waltet", "tokens": ["Nur", "ein", "l\u00e4ngst", "ver\u00b7bl\u00fch\u00b7ter", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJD", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dir nachduftend im Gem\u00fct.", "tokens": ["Dir", "nach\u00b7duf\u00b7tend", "im", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}