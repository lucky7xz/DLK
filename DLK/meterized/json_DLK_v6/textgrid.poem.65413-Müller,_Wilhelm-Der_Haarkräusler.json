{"textgrid.poem.65413": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Haarkr\u00e4usler", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gr\u00fcbchen deiner Wangen", "tokens": ["Ein", "Gr\u00fcb\u00b7chen", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ersah sich einst zum Lager", "tokens": ["Er\u00b7sah", "sich", "einst", "zum", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der kleine Gott der Liebe.", "tokens": ["Der", "klei\u00b7ne", "Gott", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er heraus nicht fiele,", "tokens": ["Da\u00df", "er", "he\u00b7raus", "nicht", "fie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So schlang er um den Nacken", "tokens": ["So", "schlang", "er", "um", "den", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Schultern und den K\u00f6cher", "tokens": ["Die", "Schul\u00b7tern", "und", "den", "K\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich deines Haares Bande.", "tokens": ["Sich", "dei\u00b7nes", "Haa\u00b7res", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Davon sind sie bis heute", "tokens": ["Da\u00b7von", "sind", "sie", "bis", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Geringelt noch geblieben.", "tokens": ["Ge\u00b7rin\u00b7gelt", "noch", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}