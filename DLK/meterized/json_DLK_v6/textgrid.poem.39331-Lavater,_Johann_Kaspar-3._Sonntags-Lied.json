{"textgrid.poem.39331": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "3. Sonntags-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der einst im Grabe lag,", "tokens": ["O", "du", ",", "der", "einst", "im", "Gra\u00b7be", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr, heilig sey uns dieser Tag!", "tokens": ["Herr", ",", "hei\u00b7lig", "sey", "uns", "die\u00b7ser", "Tag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An diesem Tage giengest du", "tokens": ["An", "die\u00b7sem", "Ta\u00b7ge", "gien\u00b7gest", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4rt aus deiner Todes-Ruh!", "tokens": ["Ver\u00b7kl\u00e4rt", "aus", "dei\u00b7ner", "To\u00b7des\u00b7Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du zeigtest dich der J\u00fcnger-Schaar,", "tokens": ["Du", "zeig\u00b7test", "dich", "der", "J\u00fcn\u00b7ger\u00b7Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die andachtsvoll versammelt war;", "tokens": ["Die", "an\u00b7dachts\u00b7voll", "ver\u00b7sam\u00b7melt", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie war sie vor Erstaunen bleich,", "tokens": ["Wie", "war", "sie", "vor", "Er\u00b7stau\u00b7nen", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da du sprachst: Friede sey mit Euch!", "tokens": ["Da", "du", "sprachst", ":", "Frie\u00b7de", "sey", "mit", "Euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie unaussprechlich war entz\u00fcckt", "tokens": ["Wie", "un\u00b7aus\u00b7sprech\u00b7lich", "war", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schaar, die n\u00e4her dich erblickt!", "tokens": ["Die", "Schaar", ",", "die", "n\u00e4\u00b7her", "dich", "er\u00b7blickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! m\u00f6gten, Christus, wir uns dein", "tokens": ["O", "!", "m\u00f6g\u00b7ten", ",", "Chris\u00b7tus", ",", "wir", "uns", "dein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VMFIN", "$,", "NE", "$,", "PPER", "PRF", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich deinen J\u00fcngern herzlich freun!", "tokens": ["Gleich", "dei\u00b7nen", "J\u00fcn\u00b7gern", "herz\u00b7lich", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du lebst, und lebst f\u00fcr uns, und bist", "tokens": ["Du", "lebst", ",", "und", "lebst", "f\u00fcr", "uns", ",", "und", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "$,", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch unser, unser Jesus Christ!", "tokens": ["Auch", "un\u00b7ser", ",", "un\u00b7ser", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "$,", "PPOSAT", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst und h\u00f6rst uns, singen wir,", "tokens": ["Du", "siehst", "und", "h\u00f6rst", "uns", ",", "sin\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Loblied dringt zu dir!", "tokens": ["Und", "un\u00b7ser", "Lob\u00b7lied", "dringt", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und von dem Himmel rufest du", "tokens": ["Und", "von", "dem", "Him\u00b7mel", "ru\u00b7fest", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Geist auch unsern Herzen zu!", "tokens": ["Im", "Geist", "auch", "un\u00b7sern", "Her\u00b7zen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Kindlein: Friede sey mit Euch,", "tokens": ["Ihr", "Kin\u00b7dlein", ":", "Frie\u00b7de", "sey", "mit", "Euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Euer ist mein himmlisch Reich!", "tokens": ["Auch", "Eu\u00b7er", "ist", "mein", "himm\u00b7lisch", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Verg\u00e4ssen wir nur deiner nicht!", "tokens": ["Ver\u00b7g\u00e4s\u00b7sen", "wir", "nur", "dei\u00b7ner", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Lust w\u00e4r uns die schwerste Pflicht!", "tokens": ["Nur", "Lust", "w\u00e4r", "uns", "die", "schwers\u00b7te", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lebst f\u00fcr uns, o glaubten wir,", "tokens": ["Du", "lebst", "f\u00fcr", "uns", ",", "o", "glaub\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "FM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir lebten und wir st\u00fcrben dir!", "tokens": ["Wir", "leb\u00b7ten", "und", "wir", "st\u00fcr\u00b7ben", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die gerne h\u00f6rten wir dein Wort!", "tokens": ["Die", "ger\u00b7ne", "h\u00f6r\u00b7ten", "wir", "dein", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schritten wir im Guten fort!", "tokens": ["Wie", "schrit\u00b7ten", "wir", "im", "Gu\u00b7ten", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie froh, wie andachtvoll, wie rein", "tokens": ["Wie", "froh", ",", "wie", "an\u00b7dacht\u00b7voll", ",", "wie", "rein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrd' unser Herz und Leben seyn!", "tokens": ["W\u00fcrd'", "un\u00b7ser", "Herz", "und", "Le\u00b7ben", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie aufmerksam vern\u00e4hmen wir", "tokens": ["Wie", "auf\u00b7merk\u00b7sam", "ver\u00b7n\u00e4h\u00b7men", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Heut jeden Unterricht von Dir!", "tokens": ["Heut", "je\u00b7den", "Un\u00b7ter\u00b7richt", "von", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie unertr\u00e4glich w\u00e4r uns heut',", "tokens": ["Wie", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "w\u00e4r", "uns", "heut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was unsern Geist von dir zerstreut!", "tokens": ["Was", "un\u00b7sern", "Geist", "von", "dir", "zer\u00b7streut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum send' uns, Jesus, deinen Geist,", "tokens": ["Drum", "send'", "uns", ",", "Je\u00b7sus", ",", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets an dich uns denken hei\u00dft!", "tokens": ["Der", "stets", "an", "dich", "uns", "den\u00b7ken", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey immer nah uns, Jesus Christ,", "tokens": ["Sey", "im\u00b7mer", "nah", "uns", ",", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPER", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der du vom Tod erstanden bist!", "tokens": ["Der", "du", "vom", "Tod", "er\u00b7stan\u00b7den", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}