{"textgrid.poem.55431": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn's jemand ziemt, zu sprechen mit Vertrauen,", "tokens": ["Wenn's", "je\u00b7mand", "ziemt", ",", "zu", "spre\u00b7chen", "mit", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ziemt es mir: ich stelle heut den Chor", "tokens": ["So", "ziemt", "es", "mir", ":", "ich", "stel\u00b7le", "heut", "den", "Chor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebildeter und liebevoller Frauen,", "tokens": ["Ge\u00b7bil\u00b7de\u00b7ter", "und", "lie\u00b7be\u00b7vol\u00b7ler", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sich so gern um ", "tokens": ["Der", "sich", "so", "gern", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mir ist verg\u00f6nnt, an ", "tokens": ["Mir", "ist", "ver\u00b7g\u00f6nnt", ",", "an"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "Mich zu erquicken an dem frischen Flor,", "tokens": ["Mich", "zu", "er\u00b7qui\u00b7cken", "an", "dem", "fri\u00b7schen", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der jede Stunde neuen Wert bet\u00e4tig", "tokens": ["Der", "je\u00b7de", "Stun\u00b7de", "neu\u00b7en", "Wert", "be\u00b7t\u00e4\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Frauenw\u00fcrde ewiglich best\u00e4tigt.", "tokens": ["Und", "Frau\u00b7en\u00b7w\u00fcr\u00b7de", "e\u00b7wig\u00b7lich", "be\u00b7st\u00e4\u00b7tigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}