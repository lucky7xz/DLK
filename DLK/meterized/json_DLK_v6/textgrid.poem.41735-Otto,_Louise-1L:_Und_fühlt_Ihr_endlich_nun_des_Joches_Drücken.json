{"textgrid.poem.41735": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und f\u00fchlt Ihr endlich nun des Joches Dr\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und f\u00fchlt Ihr endlich nun des Joches Dr\u00fccken", "tokens": ["Und", "f\u00fchlt", "Ihr", "end\u00b7lich", "nun", "des", "Jo\u00b7ches", "Dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wollt Ihr r\u00e4chen nun die lange Schmach?", "tokens": ["Und", "wollt", "Ihr", "r\u00e4\u00b7chen", "nun", "die", "lan\u00b7ge", "Schmach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seid Ihr es m\u00fcde, da\u00df auf Deutschlands R\u00fccken", "tokens": ["Seid", "Ihr", "es", "m\u00fc\u00b7de", ",", "da\u00df", "auf", "Deutschlands", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "PPER", "ADJD", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des R\u00f6mers Fu\u00df noch immer treten mag?", "tokens": ["Des", "R\u00f6\u00b7mers", "Fu\u00df", "noch", "im\u00b7mer", "tre\u00b7ten", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df immer noch das alte St\u00fcck soll spielen,", "tokens": ["Da\u00df", "im\u00b7mer", "noch", "das", "al\u00b7te", "St\u00fcck", "soll", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das alte St\u00fcck aus Deutschlands Kaiserzeit,", "tokens": ["Das", "al\u00b7te", "St\u00fcck", "aus", "Deutschlands", "Kai\u00b7ser\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wo, wenn mit Frevelsfl\u00fcchen Rom gedr\u00e4ut", "tokens": ["Wo", ",", "wenn", "mit", "Fre\u00b7vels\u00b7fl\u00fc\u00b7chen", "Rom", "ge\u00b7dr\u00e4ut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "APPR", "NN", "NE", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die M\u00e4chtgen selber ihm zu F\u00fc\u00dfen fielen?", "tokens": ["Die", "M\u00e4cht\u00b7gen", "sel\u00b7ber", "ihm", "zu", "F\u00fc\u00b7\u00dfen", "fie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ahnt Ihr endlich, da\u00df die Nacht mu\u00df weichen,", "tokens": ["Und", "ahnt", "Ihr", "end\u00b7lich", ",", "da\u00df", "die", "Nacht", "mu\u00df", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der noch finstrer Zauberwahn regiert", "tokens": ["In", "der", "noch", "finst\u00b7rer", "Zau\u00b7ber\u00b7wahn", "re\u00b7giert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und manchen Wanderer ein tr\u00fcgend Zeichen", "tokens": ["Und", "man\u00b7chen", "Wan\u00b7de\u00b7rer", "ein", "tr\u00fc\u00b7gend", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Irrlicht in die tiefsten S\u00fcmpfe f\u00fchrt?", "tokens": ["Als", "Irr\u00b7licht", "in", "die", "tiefs\u00b7ten", "S\u00fcmp\u00b7fe", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch einmal zuckte jene Riesenschlange,", "tokens": ["Noch", "ein\u00b7mal", "zuck\u00b7te", "je\u00b7ne", "Rie\u00b7sen\u00b7schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die zweite Hyder, welche Rom gebar,", "tokens": ["Die", "zwei\u00b7te", "Hy\u00b7der", ",", "wel\u00b7che", "Rom", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr Haupt empor und rief der Christen Schar", "tokens": ["Ihr", "Haupt", "em\u00b7por", "und", "rief", "der", "Chris\u00b7ten", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zum G\u00f6tzendienst mit schn\u00f6dem Glaubenszwange.", "tokens": ["Zum", "G\u00f6t\u00b7zen\u00b7dienst", "mit", "schn\u00f6\u00b7dem", "Glau\u00b7bens\u00b7zwan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie meinte jubelnd schon zu triumphieren", "tokens": ["Sie", "mein\u00b7te", "ju\u00b7belnd", "schon", "zu", "tri\u00b7um\u00b7phie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcberall wuchs ihr ein neues Haupt,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "wuchs", "ihr", "ein", "neu\u00b7es", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mochte wohl die Frage uns geb\u00fchren:", "tokens": ["Da", "moch\u00b7te", "wohl", "die", "Fra\u00b7ge", "uns", "ge\u00b7b\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer hat dies neue Gaukelspiel erlaubt? \u2013", "tokens": ["Wer", "hat", "dies", "neu\u00b7e", "Gau\u00b7kel\u00b7spiel", "er\u00b7laubt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PDS", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mag es noch so gl\u00e4nzend sich geb\u00e4rden,", "tokens": ["Und", "mag", "es", "noch", "so", "gl\u00e4n\u00b7zend", "sich", "ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht f\u00fcrder wachsen soll die L\u00fcgensaat,", "tokens": ["Nicht", "f\u00fcr\u00b7der", "wach\u00b7sen", "soll", "die", "L\u00fc\u00b7gen\u00b7saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schon ist sie reif und eine Ernte naht,", "tokens": ["Schon", "ist", "sie", "reif", "und", "ei\u00b7ne", "Ern\u00b7te", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo all ihr Gift uns soll zum Heilkraut werden.", "tokens": ["Wo", "all", "ihr", "Gift", "uns", "soll", "zum", "Heil\u00b7kraut", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PPOSAT", "NN", "PPER", "VMFIN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Deutschland kann trotzen ", "tokens": ["Deutschland", "kann", "trot\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und lieber ", "tokens": ["Und", "lie\u00b7ber"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Kein fremder Zwang soll seinen Glauben knechten,", "tokens": ["Kein", "frem\u00b7der", "Zwang", "soll", "sei\u00b7nen", "Glau\u00b7ben", "knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, seinen Glauben mu\u00df es sich befrei'n.", "tokens": ["Ja", ",", "sei\u00b7nen", "Glau\u00b7ben", "mu\u00df", "es", "sich", "be\u00b7frei'", "n."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst schlug Hermann Romas Legionen,", "tokens": ["Um\u00b7sonst", "schlug", "Her\u00b7mann", "Ro\u00b7mas", "Le\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Umsonst wird ihm ein ehern Mal geweiht,", "tokens": ["Um\u00b7sonst", "wird", "ihm", "ein", "e\u00b7hern", "Mal", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn Rom noch herrscht in sp\u00e4ter Enkel Zeit,", "tokens": ["Wenn", "Rom", "noch", "herrscht", "in", "sp\u00e4\u00b7ter", "En\u00b7kel", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Deutschland sich beugt wo einst gesiegt Teutonen.", "tokens": ["Deutschland", "sich", "beugt", "wo", "einst", "ge\u00b7siegt", "Teu\u00b7to\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVFIN", "PWAV", "ADV", "VVPP", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "An uns're Zeit ergeht ein heilig Mahnen,", "tokens": ["An", "un\u00b7s'\u00b7re", "Zeit", "er\u00b7geht", "ein", "hei\u00b7lig", "Mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein heilig Mahnen an das Vaterland,", "tokens": ["Ein", "hei\u00b7lig", "Mah\u00b7nen", "an", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Deutschlands Volk: sei ", "tokens": ["An", "Deutschlands", "Volk", ":", "sei"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NN", "$.", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sei ", "tokens": ["Sei"], "token_info": ["word"], "pos": ["VAIMP"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Wir stehen fragend an der Zukunft Pforten,", "tokens": ["Wir", "ste\u00b7hen", "fra\u00b7gend", "an", "der", "Zu\u00b7kunft", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir suchen Rat bei der Vergangenheit,", "tokens": ["Wir", "su\u00b7chen", "Rat", "bei", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch ist ein Traum ja Deutschlands Einigkeit \u2013:", "tokens": ["Noch", "ist", "ein", "Traum", "ja", "Deutschlands", "Ei\u00b7nig\u00b7keit", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$(", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wir werden ", "tokens": ["Wir", "wer\u00b7den"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Ja nur erst ", "tokens": ["Ja", "nur", "erst"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wohl auch im Sinn, doch nicht in Sitt und Brauch,", "tokens": ["Wohl", "auch", "im", "Sinn", ",", "doch", "nicht", "in", "Sitt", "und", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In unsre Heimat streut man fremde Samen,", "tokens": ["In", "uns\u00b7re", "Hei\u00b7mat", "streut", "man", "frem\u00b7de", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch unsre Th\u00e4ler weht ein fremder Hauch.", "tokens": ["Durch", "uns\u00b7re", "Th\u00e4\u00b7ler", "weht", "ein", "frem\u00b7der", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die fremde Saat tilgt von der fremden Erde,", "tokens": ["Die", "frem\u00b7de", "Saat", "tilgt", "von", "der", "frem\u00b7den", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das fremde Reich brecht ab vom deutschen Baum", "tokens": ["Das", "frem\u00b7de", "Reich", "brecht", "ab", "vom", "deut\u00b7schen", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gebt seinem Wachstum Luft und freien Raum \u2013", "tokens": ["Gebt", "sei\u00b7nem", "Wachs\u00b7tum", "Luft", "und", "frei\u00b7en", "Raum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du wirst Dich jetzt, mein Vaterland, erheben,", "tokens": ["Du", "wirst", "Dich", "jetzt", ",", "mein", "Va\u00b7ter\u00b7land", ",", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Deinen ", "tokens": ["F\u00fcr", "Dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Du willst nicht mehr im r\u00f6mschen Joche leben,", "tokens": ["Du", "willst", "nicht", "mehr", "im", "r\u00f6m\u00b7schen", "Jo\u00b7che", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Frei forderst Du den ", "tokens": ["Frei", "for\u00b7derst", "Du"