{"textgrid.poem.43775": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V\u00f6gleins Klage", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkehr' wieder, lieber Sonnenschein!", "tokens": ["\u00bb", "kehr'", "wie\u00b7der", ",", "lie\u00b7ber", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kehr' wieder, heitre Bl\u00e4ue!", "tokens": ["Kehr'", "wie\u00b7der", ",", "heit\u00b7re", "Bl\u00e4u\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mich armes V\u00f6gelein", "tokens": ["Da\u00df", "ich", "mich", "ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal wieder freue.", "tokens": ["Noch", "ein\u00b7mal", "wie\u00b7der", "freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie war mir sonst so lieb der Wald,", "tokens": ["Wie", "war", "mir", "sonst", "so", "lieb", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieb das Dach der Bl\u00e4tter!", "tokens": ["So", "lieb", "das", "Dach", "der", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt ist es ringsum tr\u00fcb' und kalt,", "tokens": ["Jetzt", "ist", "es", "ring\u00b7sum", "tr\u00fcb'", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Wind und Regenwetter.\u00ab", "tokens": ["Bei", "Wind", "und", "Re\u00b7gen\u00b7wet\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So klagt das arme V\u00f6gelein", "tokens": ["So", "klagt", "das", "ar\u00b7me", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei jedem Regenschauer,", "tokens": ["Bei", "je\u00b7dem", "Re\u00b7gen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schl\u00e4ft Abends sp\u00e4t in Trauer ein,", "tokens": ["Schl\u00e4ft", "A\u00b7bends", "sp\u00e4t", "in", "Trau\u00b7er", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wacht Morgens auf in Trauer.", "tokens": ["Wacht", "Mor\u00b7gens", "auf", "in", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}