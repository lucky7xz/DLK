{"textgrid.poem.35330": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmen schm\u00fccken meine Heimat,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmen schm\u00fccken meine Heimat,", "tokens": ["Pal\u00b7men", "schm\u00fc\u00b7cken", "mei\u00b7ne", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so traulich ist es da,", "tokens": ["Und", "so", "trau\u00b7lich", "ist", "es", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo von gr\u00fcnen Bl\u00e4tterkronen", "tokens": ["Wo", "von", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns begr\u00fc\u00dft der Sabia.", "tokens": ["Uns", "be\u00b7gr\u00fc\u00dft", "der", "Sa\u00b7bia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Zeigt mir holden Waldesschatten,", "tokens": ["Zeigt", "mir", "hol\u00b7den", "Wal\u00b7des\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fluren, die den unsern gleich,", "tokens": ["Flu\u00b7ren", ",", "die", "den", "un\u00b7sern", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne, wie sie niederleuchten", "tokens": ["Ster\u00b7ne", ",", "wie", "sie", "nie\u00b7der\u00b7leuch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Liebe Zauberreich.", "tokens": ["Auf", "der", "Lie\u00b7be", "Zau\u00b7ber\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In den tr\u00fcben Wintern\u00e4chten,", "tokens": ["In", "den", "tr\u00fc\u00b7ben", "Win\u00b7ter\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, wie gramvoll denk' ich da", "tokens": ["O", ",", "wie", "gram\u00b7voll", "denk'", "ich", "da"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An das Land der Palmenhaine", "tokens": ["An", "das", "Land", "der", "Pal\u00b7men\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des S\u00e4ngers Sabia.", "tokens": ["Und", "des", "S\u00e4n\u00b7gers", "Sa\u00b7bia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Denn es strahlt in Sch\u00f6nheitsf\u00fclle,", "tokens": ["Denn", "es", "strahlt", "in", "Sch\u00f6n\u00b7heits\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich sonst sie nirgends sah,", "tokens": ["Wie", "ich", "sonst", "sie", "nir\u00b7gends", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in allen Traumgebilden", "tokens": ["Und", "in", "al\u00b7len", "Traum\u00b7ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es meiner Sehnsucht nah,", "tokens": ["Ist", "es", "mei\u00b7ner", "Sehn\u00b7sucht", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Fl\u00fcstern seiner Palmen,", "tokens": ["Mit", "dem", "Fl\u00fcs\u00b7tern", "sei\u00b7ner", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Gru\u00df des Sabia.", "tokens": ["Mit", "dem", "Gru\u00df", "des", "Sa\u00b7bia", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "La\u00df, o Gott, erst dann mich sterben,", "tokens": ["La\u00df", ",", "o", "Gott", ",", "erst", "dann", "mich", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann mein Land ich wiedersah,", "tokens": ["Wann", "mein", "Land", "ich", "wie\u00b7der\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Heimat mich begl\u00fcckte,", "tokens": ["Und", "die", "Hei\u00b7mat", "mich", "be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es hier noch nie geschah;", "tokens": ["Wie", "es", "hier", "noch", "nie", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Palmen es verk\u00fcnden", "tokens": ["Wie", "die", "Pal\u00b7men", "es", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Ruf des Sabia.", "tokens": ["Und", "der", "Ruf", "des", "Sa\u00b7bia", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}