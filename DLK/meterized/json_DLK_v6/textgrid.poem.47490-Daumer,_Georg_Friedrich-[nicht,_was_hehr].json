{"textgrid.poem.47490": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht, was hehr]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, was hehr,", "tokens": ["Nicht", ",", "was", "hehr", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Weil ich es nicht fassen kann;", "tokens": ["Weil", "ich", "es", "nicht", "fas\u00b7sen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht, was schwer,", "tokens": ["Nicht", ",", "was", "schwer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "B\u00fcrde mir auf, mir armen Mann!", "tokens": ["B\u00fcr\u00b7de", "mir", "auf", ",", "mir", "ar\u00b7men", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Da so viel", "tokens": ["Da", "so", "viel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Schon mir ein L\u00f6ckchen zu schaffen macht,", "tokens": ["Schon", "mir", "ein", "L\u00f6ck\u00b7chen", "zu", "schaf\u00b7fen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie vielmehr", "tokens": ["Wie", "viel\u00b7mehr"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "St\u00f6hnt' ich erliegend in solchem Bann!", "tokens": ["St\u00f6hnt'", "ich", "er\u00b7lie\u00b7gend", "in", "sol\u00b7chem", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}