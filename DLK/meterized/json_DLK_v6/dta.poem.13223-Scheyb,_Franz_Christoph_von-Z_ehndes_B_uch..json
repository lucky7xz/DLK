{"dta.poem.13223": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drauf hat die Kayserinn zu reden angefangen:", "tokens": ["Drauf", "hat", "die", "Kay\u00b7se\u00b7rinn", "zu", "re\u00b7den", "an\u00b7ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewir wissen, wie der Feind mit uns ist umgegangen;", "tokens": ["\u201e", "wir", "wis\u00b7sen", ",", "wie", "der", "Feind", "mit", "uns", "ist", "um\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "525\u201dWir wissen\u201d, trug sie vor, was euer Rath gen\u00fczt;", "tokens": ["\"", "Wir", "wis\u00b7sen", "\"", ",", "trug", "sie", "vor", ",", "was", "eu\u00b7er", "Rath", "ge\u00b7n\u00fczt", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewer meiner Tochter Haus und L\u00e4nder hat besch\u00fczt.", "tokens": ["\u201e", "wer", "mei\u00b7ner", "Toch\u00b7ter", "Haus", "und", "L\u00e4n\u00b7der", "hat", "be\u00b7sch\u00fczt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eund ihr erinnert euch der schweren F\u00fcrsten-Tugend,", "tokens": ["\u201e", "und", "ihr", "e\u00b7rin\u00b7nert", "euch", "der", "schwe\u00b7ren", "F\u00fcrs\u00b7ten\u00b7Tu\u00b7gend", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie meiner Tochter Herz schon in der zarten Jugend", "tokens": ["\u201e", "die", "mei\u00b7ner", "Toch\u00b7ter", "Herz", "schon", "in", "der", "zar\u00b7ten", "Ju\u00b7gend"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "PPOSAT", "NN", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edurch euern Finger-Zeig die Helden-Kunst gelernt;", "tokens": ["\u201e", "durch", "eu\u00b7ern", "Fin\u00b7ger\u00b7Zeig", "die", "Hel\u00b7den\u00b7Kunst", "ge\u00b7lernt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "530\u201dJhr wi\u00dft, da\u00df ihr Gem\u00fcth sich nicht davon entfernt;", "tokens": ["\"", "Ihr", "wi\u00dft", ",", "da\u00df", "ihr", "Ge\u00b7m\u00fcth", "sich", "nicht", "da\u00b7von", "ent\u00b7fernt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eda\u00df uns der Sachen Lauf durch euern Schuz gelungen;", "tokens": ["\u201e", "da\u00df", "uns", "der", "Sa\u00b7chen", "Lauf", "durch", "eu\u00b7ern", "Schuz", "ge\u00b7lun\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eda\u00df die\u00df die Quelle sey woraus uns Heil entsprungen.", "tokens": ["\u201e", "da\u00df", "die\u00df", "die", "Quel\u00b7le", "sey", "wo\u00b7raus", "uns", "Heil", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PDS", "ART", "NN", "VAFIN", "PWAV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso fahret, Tugenden! in der Gewohnheit fort", "tokens": ["\u201e", "so", "fah\u00b7ret", ",", "Tu\u00b7gen\u00b7den", "!", "in", "der", "Ge\u00b7wohn\u00b7heit", "fort"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "$.", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "\u201edem Thron geneigt zu seyn! besch\u00fczet diesen Ort,", "tokens": ["\u201e", "dem", "Thron", "ge\u00b7neigt", "zu", "seyn", "!", "be\u00b7sch\u00fc\u00b7zet", "die\u00b7sen", "Ort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$.", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "535\u201dDen ihr so lang besizt. Besch\u00fczet Volck und L\u00e4nder!", "tokens": ["\"", "Den", "ihr", "so", "lang", "be\u00b7sizt", ".", "Be\u00b7sch\u00fc\u00b7zet", "Volck", "und", "L\u00e4n\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPER", "ADV", "ADJD", "VVFIN", "$.", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ejhr habt f\u00fcr euer Amt nunmehr gew\u00fcnschte Pf\u00e4nder,", "tokens": ["\u201e", "jhr", "habt", "f\u00fcr", "eu\u00b7er", "Amt", "nun\u00b7mehr", "ge\u00b7w\u00fcnschte", "Pf\u00e4n\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "\u201edie Frucht der Danckbarkeit. Beflei\u00dft euch sonderlich", "tokens": ["\u201e", "die", "Frucht", "der", "Dan\u00b7ck\u00b7bar\u00b7keit", ".", "Be\u00b7flei\u00dft", "euch", "son\u00b7der\u00b7lich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ART", "NN", "$.", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "\u201ein Zukunft, da\u00df ihr stets vereint und schwesterlich", "tokens": ["\u201e", "in", "Zu\u00b7kunft", ",", "da\u00df", "ihr", "stets", "ver\u00b7eint", "und", "schwes\u00b7ter\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ewas jeder obligt, \u00fcbt. Man sieht den Feind sich mehren;", "tokens": ["\u201e", "was", "je\u00b7der", "ob\u00b7ligt", ",", "\u00fcbt", ".", "Man", "sieht", "den", "Feind", "sich", "meh\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "VVFIN", "$,", "VVFIN", "$.", "PIS", "VVFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "540\u201dEr eilt schon wiederum mit neu-geworbnen Heeren", "tokens": ["\"", "Er", "eilt", "schon", "wie\u00b7de\u00b7rum", "mit", "neu\u00b7ge\u00b7worb\u00b7nen", "Hee\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eauf unsre Grenzen zu. List und Verr\u00e4therey", "tokens": ["\u201e", "auf", "uns\u00b7re", "Gren\u00b7zen", "zu", ".", "List", "und", "Ver\u00b7r\u00e4\u00b7the\u00b7rey"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "NN", "KON", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.20": {"text": "\u201esteht seiner L\u00e4ndersucht, die er besch\u00f6net, bey;", "tokens": ["\u201e", "steht", "sei\u00b7ner", "L\u00e4n\u00b7der\u00b7sucht", ",", "die", "er", "be\u00b7sch\u00f6\u00b7net", ",", "bey", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201edie kann der gr\u00f6\u00dften Macht oft pl\u00f6zlich Ungl\u00fcck bringen,", "tokens": ["\u201e", "die", "kann", "der", "gr\u00f6\u00df\u00b7ten", "Macht", "oft", "pl\u00f6z\u00b7lich", "Un\u00b7gl\u00fcck", "brin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201ebesonders wo man pflegt mit Sicherheit zu ringen.", "tokens": ["\u201e", "be\u00b7son\u00b7ders", "wo", "man", "pflegt", "mit", "Si\u00b7cher\u00b7heit", "zu", "rin\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWAV", "PIS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "545\u201dMi\u00dftrauet eigner Macht, und f\u00f6rchtet euern Feind,", "tokens": ["\"", "Mi\u00df\u00b7trau\u00b7et", "eig\u00b7ner", "Macht", ",", "und", "f\u00f6rch\u00b7tet", "eu\u00b7ern", "Feind", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201egedenckt, da\u00df \u00fcberall desselben Schlingen seynd.", "tokens": ["\u201e", "ge\u00b7denckt", ",", "da\u00df", "\u00fc\u00b7be\u00b7rall", "des\u00b7sel\u00b7ben", "Schlin\u00b7gen", "seynd", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "KOUS", "ADV", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ela\u00dft euch die St\u00e4rcke nicht mit ihren Fahnen schmeicheln,", "tokens": ["\u201e", "la\u00dft", "euch", "die", "St\u00e4r\u00b7cke", "nicht", "mit", "ih\u00b7ren", "Fah\u00b7nen", "schmei\u00b7cheln", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "\u201ejhr Schaaren-reicher Schein beruht oft nur im Heucheln.", "tokens": ["\u201e", "jhr", "Schaa\u00b7ren\u00b7rei\u00b7cher", "Schein", "be\u00b7ruht", "oft", "nur", "im", "Heu\u00b7cheln", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201ezu viele Sicherheit n\u00fczt oft dem Feinde mehr", "tokens": ["\u201e", "zu", "vie\u00b7le", "Si\u00b7cher\u00b7heit", "n\u00fczt", "oft", "dem", "Fein\u00b7de", "mehr"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "550\u201dAls seiner Waffen Macht, als ein sieghaftes Heer;", "tokens": ["\"", "Als", "sei\u00b7ner", "Waf\u00b7fen", "Macht", ",", "als", "ein", "sieg\u00b7haf\u00b7tes", "Heer", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201eabsonderlich wo wir uns nur auf sie verlassen,", "tokens": ["\u201e", "ab\u00b7son\u00b7der\u00b7lich", "wo", "wir", "uns", "nur", "auf", "sie", "ver\u00b7las\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PWAV", "PPER", "PRF", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "\u201eund unsre Feinde nicht mit scharffem Auge fassen.", "tokens": ["\u201e", "und", "uns\u00b7re", "Fein\u00b7de", "nicht", "mit", "scharf\u00b7fem", "Au\u00b7ge", "fas\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "\u201eoft mitten in der Flucht siegt eine schwache Schaar,", "tokens": ["\u201e", "oft", "mit\u00b7ten", "in", "der", "Flucht", "siegt", "ei\u00b7ne", "schwa\u00b7che", "Schaar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201eund macht das zweifelhaft, was schon erfochten war.", "tokens": ["\u201e", "und", "macht", "das", "zwei\u00b7fel\u00b7haft", ",", "was", "schon", "er\u00b7foch\u00b7ten", "war", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "ADJD", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "555\u201dSo will ich, Tugenden! die Vorsicht euch empfehlen,", "tokens": ["\"", "So", "will", "ich", ",", "Tu\u00b7gen\u00b7den", "!", "die", "Vor\u00b7sicht", "euch", "emp\u00b7feh\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "$,", "NN", "$.", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u201edie Eintracht euers Amts; die kann den Feind entseelen.", "tokens": ["\u201e", "die", "Ein\u00b7tracht", "eu\u00b7ers", "Amts", ";", "die", "kann", "den", "Feind", "ent\u00b7see\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$.", "PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u201ebetrachtet nur den Tag des Einfalls und die Zeit,", "tokens": ["\u201e", "be\u00b7trach\u00b7tet", "nur", "den", "Tag", "des", "Ein\u00b7falls", "und", "die", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "\u201ein welcher ihr den Feind zum ersten Mahl zerstreut;", "tokens": ["\u201e", "in", "wel\u00b7cher", "ihr", "den", "Feind", "zum", "ers\u00b7ten", "Mahl", "zer\u00b7streut", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u201ebedencket, da\u00df ihr ihm den Vorsaz abgezwungen,", "tokens": ["\u201e", "be\u00b7den\u00b7cket", ",", "da\u00df", "ihr", "ihm", "den", "Vor\u00b7saz", "ab\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "560\u201dWorauf Jahr-hundert her desselben V\u00e4ter drungen.", "tokens": ["\"", "Wo\u00b7rauf", "Jahr\u00b7hun\u00b7dert", "her", "des\u00b7sel\u00b7ben", "V\u00e4\u00b7ter", "drun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "NN", "APZR", "PDAT", "NN"