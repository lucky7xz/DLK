{"textgrid.poem.57741": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die blaue Flamme", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Bornbusch wohnt eine Nachtigall,", "tokens": ["Im", "Born\u00b7busch", "wohnt", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die singt im Jahr ein einziges Mal,", "tokens": ["Die", "singt", "im", "Jahr", "ein", "ein\u00b7zi\u00b7ges", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Singt nur in einer Sommernacht,", "tokens": ["Singt", "nur", "in", "ei\u00b7ner", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was sie singt, ist Leid und Qual.", "tokens": ["Und", "was", "sie", "singt", ",", "ist", "Leid", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4ume, die im Bornbusch stehn,", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "die", "im", "Born\u00b7busch", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Fl\u00fcstern ist wie Todeswehn,", "tokens": ["Ihr", "Fl\u00fcs\u00b7tern", "ist", "wie", "To\u00b7des\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihre Zweige kreischen gellend,", "tokens": ["Ih\u00b7re", "Zwei\u00b7ge", "krei\u00b7schen", "gel\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn nachts die Winde wehn.", "tokens": ["Wenn", "nachts", "die", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Blume, die im Bornbusch bl\u00fcht,", "tokens": ["Die", "Blu\u00b7me", ",", "die", "im", "Born\u00b7busch", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein bitterb\u00f6ses Kraut;", "tokens": ["Das", "ist", "ein", "bit\u00b7ter\u00b7b\u00f6\u00b7ses", "Kraut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihre Bl\u00e4tter sind mit Blut besprengt,", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", "sind", "mit", "Blut", "be\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Bl\u00fcten mit Gift und Galle getr\u00e4nkt", "tokens": ["Ih\u00b7re", "Bl\u00fc\u00b7ten", "mit", "Gift", "und", "Gal\u00b7le", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und mit kaltem Schwei\u00df betaut.", "tokens": ["Und", "mit", "kal\u00b7tem", "Schwei\u00df", "be\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Bach, der aus dem Bornbusch quillt,", "tokens": ["Der", "Bach", ",", "der", "aus", "dem", "Born\u00b7busch", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist klar wie Glas und kalt wie Eis;", "tokens": ["Ist", "klar", "wie", "Glas", "und", "kalt", "wie", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Fisch in seinen Fluten lebt,", "tokens": ["Kein", "Fisch", "in", "sei\u00b7nen", "Flu\u00b7ten", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Vogel an seinem Ufer schwebt,", "tokens": ["Kein", "Vo\u00b7gel", "an", "sei\u00b7nem", "U\u00b7fer", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sein Grund ist leichenlakenwei\u00df.", "tokens": ["Sein", "Grund", "ist", "lei\u00b7chen\u00b7la\u00b7ken\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Weiher, in dem der Bach ertrinkt,", "tokens": ["Der", "Wei\u00b7her", ",", "in", "dem", "der", "Bach", "er\u00b7trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist uferlos und ohne Grund;", "tokens": ["Ist", "u\u00b7fer\u00b7los", "und", "oh\u00b7ne", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blutrote Blumen ihn umbl\u00fchn,", "tokens": ["Blut\u00b7ro\u00b7te", "Blu\u00b7men", "ihn", "um\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blutf\u00e4den seine Flut durchziehn,", "tokens": ["Blut\u00b7f\u00e4\u00b7den", "sei\u00b7ne", "Flut", "durch\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es st\u00f6hnt bei Nacht sein Mund.", "tokens": ["Es", "st\u00f6hnt", "bei", "Nacht", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "An dem Bach steht ein Machangelbaum,", "tokens": ["An", "dem", "Bach", "steht", "ein", "Ma\u00b7chan\u00b7gel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Oben schwarz und unten grau;", "tokens": ["O\u00b7ben", "schwarz", "und", "un\u00b7ten", "grau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da liegt die Otter und sammelt Gift,", "tokens": ["Da", "liegt", "die", "Ot\u00b7ter", "und", "sam\u00b7melt", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da f\u00e4llt nicht Regen noch Tau.", "tokens": ["Da", "f\u00e4llt", "nicht", "Re\u00b7gen", "noch", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Seine Zweige strecken wie Finger sich,", "tokens": ["Sei\u00b7ne", "Zwei\u00b7ge", "stre\u00b7cken", "wie", "Fin\u00b7ger", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "PRF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sein Stamm ist verrenkt und verdreht;", "tokens": ["Sein", "Stamm", "ist", "ver\u00b7renkt", "und", "ver\u00b7dreht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kein Vogel baut sein Nest hinein,", "tokens": ["Kein", "Vo\u00b7gel", "baut", "sein", "Nest", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch, der dort vor\u00fcbergeht,", "tokens": ["Der", "Mensch", ",", "der", "dort", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem f\u00e4llt nur Trauriges ein.", "tokens": ["Dem", "f\u00e4llt", "nur", "Trau\u00b7ri\u00b7ges", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Im Bornbusch singt die Nachtigall", "tokens": ["Im", "Born\u00b7busch", "singt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blutrotes hei\u00dfes Herzeleid,", "tokens": ["Blut\u00b7ro\u00b7tes", "hei\u00b7\u00dfes", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Singt blasse kalte Todesangst", "tokens": ["Singt", "blas\u00b7se", "kal\u00b7te", "To\u00b7de\u00b7sangst"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und graue Hoffnungslosigkeit.", "tokens": ["Und", "grau\u00b7e", "Hoff\u00b7nungs\u00b7lo\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Himmel ist so tief und tot,", "tokens": ["Der", "Him\u00b7mel", "ist", "so", "tief", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Sterne stehen darin;", "tokens": ["Zwei", "Ster\u00b7ne", "ste\u00b7hen", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der eine ist rot wie Liebesgl\u00fchn,", "tokens": ["Der", "ei\u00b7ne", "ist", "rot", "wie", "Lie\u00b7bes\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der andere ist wie Gift so gr\u00fcn,", "tokens": ["Der", "an\u00b7de\u00b7re", "ist", "wie", "Gift", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er hat einen b\u00f6sen Sinn.", "tokens": ["Er", "hat", "ei\u00b7nen", "b\u00f6\u00b7sen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gl\u00fchw\u00fcrmer schweben langsam hin,", "tokens": ["Gl\u00fch\u00b7w\u00fcr\u00b7mer", "schwe\u00b7ben", "lang\u00b7sam", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Perlen aufgereiht;", "tokens": ["Wie", "Per\u00b7len", "auf\u00b7ge\u00b7reiht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schweben still und feierlich,", "tokens": ["Sie", "schwe\u00b7ben", "still", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heben sich und senken sich,", "tokens": ["He\u00b7ben", "sich", "und", "sen\u00b7ken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vergehn in Dunkelheit.", "tokens": ["Ver\u00b7gehn", "in", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Schimmel trabt von der Heide her,", "tokens": ["Ein", "Schim\u00b7mel", "trabt", "von", "der", "Hei\u00b7de", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der ist so wei\u00df wie der Schnee;", "tokens": ["Der", "ist", "so", "wei\u00df", "wie", "der", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sein Zaumzeug ist mit Gold ben\u00e4ht,", "tokens": ["Sein", "Zaum\u00b7zeug", "ist", "mit", "Gold", "be\u00b7n\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Halsbug purpurn Bandwerk weht,", "tokens": ["Vom", "Hals\u00b7bug", "pur\u00b7purn", "Band\u00b7werk", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sattel ist rot wie der Klee.", "tokens": ["Der", "Sat\u00b7tel", "ist", "rot", "wie", "der", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Der Nachtigall Lied zerspringt wie Glas,", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "Lied", "zer\u00b7springt", "wie", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der B\u00e4ume Gefl\u00fcster verhallt;", "tokens": ["Der", "B\u00e4u\u00b7me", "Ge\u00b7fl\u00fcs\u00b7ter", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus der Heide kommt ein hohler Ruf,", "tokens": ["Aus", "der", "Hei\u00b7de", "kommt", "ein", "hoh\u00b7ler", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist fern und nah, ist hier und da,", "tokens": ["Ist", "fern", "und", "nah", ",", "ist", "hier", "und", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "VAFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": 