{"textgrid.poem.24717": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sollen wir mit leeren H\u00e4nden kommen", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollen wir mit leeren H\u00e4nden kommen", "tokens": ["Sol\u00b7len", "wir", "mit", "lee\u00b7ren", "H\u00e4n\u00b7den", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der leere fremde Abendwind,", "tokens": ["Wie", "der", "lee\u00b7re", "frem\u00b7de", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So mit leerer Hand zu denen kommen,", "tokens": ["So", "mit", "lee\u00b7rer", "Hand", "zu", "de\u00b7nen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die uns mehr als alle andern sind?", "tokens": ["Die", "uns", "mehr", "als", "al\u00b7le", "an\u00b7dern", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "KOKOM", "PIAT", "PIS", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Doch an g\u00fctigsten D\u00e4monen gehen", "tokens": ["Doch", "an", "g\u00fc\u00b7tigs\u00b7ten", "D\u00e4\u00b7mo\u00b7nen", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ohne Gabe wir vor\u00fcber stumm:", "tokens": ["Oh\u00b7ne", "Ga\u00b7be", "wir", "vor\u00b7\u00fc\u00b7ber", "stumm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gie\u00dft der Baum den s\u00fc\u00dfen Schatten nieder,", "tokens": ["Gie\u00dft", "der", "Baum", "den", "s\u00fc\u00b7\u00dfen", "Schat\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Keiner h\u00e4ngt ihm goldne Ketten um,", "tokens": ["Kei\u00b7ner", "h\u00e4ngt", "ihm", "gold\u00b7ne", "Ket\u00b7ten", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Keiner wirft zur Gabe Schmuck und Blumen", "tokens": ["Kei\u00b7ner", "wirft", "zur", "Ga\u00b7be", "Schmuck", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Bach, den pl\u00e4tschernden, zu Dank,", "tokens": ["In", "den", "Bach", ",", "den", "pl\u00e4t\u00b7schern\u00b7den", ",", "zu", "Dank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "NN", "$,"], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der ihm doch lebendig, gottgeboren,", "tokens": ["Der", "ihm", "doch", "le\u00b7ben\u00b7dig", ",", "gott\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hell vorbeistr\u00f6mt an der Gartenbank.", "tokens": ["Hell", "vor\u00b7bei\u00b7str\u00f6mt", "an", "der", "Gar\u00b7ten\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Was mit Gottes Anmut zu uns redet", "tokens": ["Was", "mit", "Got\u00b7tes", "An\u00b7mut", "zu", "uns", "re\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der Bach, der Baum, und so wie du:", "tokens": ["Wie", "der", "Bach", ",", "der", "Baum", ",", "und", "so", "wie", "du", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Solchen nahn wir nicht mit \u00e4u\u00dfern Gaben,", "tokens": ["Sol\u00b7chen", "nahn", "wir", "nicht", "mit", "\u00e4u\u00b7\u00dfern", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rechnens ja dem eignen Innern zu.", "tokens": ["Rech\u00b7nens", "ja", "dem", "eig\u00b7nen", "In\u00b7nern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}