{"textgrid.poem.34848": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig Harald Harfagar", "tokens": ["Der", "K\u00f6\u00b7nig", "Ha\u00b7rald", "Har\u00b7fa\u00b7gar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt unten in Meeresgr\u00fcnden", "tokens": ["Sitzt", "un\u00b7ten", "in", "Mee\u00b7res\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bei seiner sch\u00f6nen Wasserfee;", "tokens": ["Bei", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Was\u00b7ser\u00b7fee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Jahre kommen und schwinden.", "tokens": ["Die", "Jah\u00b7re", "kom\u00b7men", "und", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Von Nixenzauber gebannt und gefeit,", "tokens": ["Von", "Ni\u00b7xen\u00b7zau\u00b7ber", "ge\u00b7bannt", "und", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kann nicht leben, nicht sterben;", "tokens": ["Er", "kann", "nicht", "le\u00b7ben", ",", "nicht", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zweihundert Jahre dauert schon", "tokens": ["Zwei\u00b7hun\u00b7dert", "Jah\u00b7re", "dau\u00b7ert", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein seliges Verderben.", "tokens": ["Sein", "se\u00b7li\u00b7ges", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Des K\u00f6nigs Haupt liegt auf dem Scho\u00df", "tokens": ["Des", "K\u00f6\u00b7nigs", "Haupt", "liegt", "auf", "dem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der holden Frau, und mit Schmachten", "tokens": ["Der", "hol\u00b7den", "Frau", ",", "und", "mit", "Schmach\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schaut er nach ihren Augen empor;", "tokens": ["Schaut", "er", "nach", "ih\u00b7ren", "Au\u00b7gen", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kann nicht genug sie betrachten.", "tokens": ["Kann", "nicht", "ge\u00b7nug", "sie", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Sein goldnes Haar ward silbergrau,", "tokens": ["Sein", "gold\u00b7nes", "Haar", "ward", "sil\u00b7ber\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es treten die Backenknochen", "tokens": ["Es", "tre\u00b7ten", "die", "Ba\u00b7cken\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gespenstisch hervor aus dem gelben Gesicht,", "tokens": ["Ge\u00b7spens\u00b7tisch", "her\u00b7vor", "aus", "dem", "gel\u00b7ben", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Leib ist welk und gebrochen.", "tokens": ["Der", "Leib", "ist", "welk", "und", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Manchmal aus seinem Liebestraum", "tokens": ["Manch\u00b7mal", "aus", "sei\u00b7nem", "Lie\u00b7best\u00b7raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird er pl\u00f6tzlich aufgesch\u00fcttert,", "tokens": ["Wird", "er", "pl\u00f6tz\u00b7lich", "auf\u00b7ge\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn droben st\u00fcrmt so wild die Flut,", "tokens": ["Denn", "dro\u00b7ben", "st\u00fcrmt", "so", "wild", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das gl\u00e4serne Schlo\u00df erzittert.", "tokens": ["Und", "das", "gl\u00e4\u00b7ser\u00b7ne", "Schlo\u00df", "er\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Manchmal ist ihm, als h\u00f6rt' er im Wind", "tokens": ["Manch\u00b7mal", "ist", "ihm", ",", "als", "h\u00f6rt'", "er", "im", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Normannenruf erschallen;", "tokens": ["Nor\u00b7man\u00b7nen\u00b7ruf", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er hebt die Arme mit freudiger Hast,", "tokens": ["Er", "hebt", "die", "Ar\u00b7me", "mit", "freu\u00b7di\u00b7ger", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4\u00dft traurig sie wieder fallen.", "tokens": ["L\u00e4\u00dft", "trau\u00b7rig", "sie", "wie\u00b7der", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Manchmal ist ihm, als h\u00f6rt' er gar,", "tokens": ["Manch\u00b7mal", "ist", "ihm", ",", "als", "h\u00f6rt'", "er", "gar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wie die Schiffer singen hier oben", "tokens": ["Wie", "die", "Schif\u00b7fer", "sin\u00b7gen", "hier", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und den K\u00f6nig Harald Harfagar", "tokens": ["Und", "den", "K\u00f6\u00b7nig", "Ha\u00b7rald", "Har\u00b7fa\u00b7gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Im Heldenliede loben.", "tokens": ["Im", "Hel\u00b7den\u00b7lie\u00b7de", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der K\u00f6nig st\u00f6hnt und schluchzt und weint", "tokens": ["Der", "K\u00f6\u00b7nig", "st\u00f6hnt", "und", "schluchzt", "und", "weint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alsdann aus Herzensgrunde.", "tokens": ["Als\u00b7dann", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schnell beugt sich hinab die Wasserfee", "tokens": ["Schnell", "beugt", "sich", "hin\u00b7ab", "die", "Was\u00b7ser\u00b7fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und k\u00fc\u00dft ihn mit lachendem Munde.", "tokens": ["Und", "k\u00fc\u00dft", "ihn", "mit", "la\u00b7chen\u00b7dem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}