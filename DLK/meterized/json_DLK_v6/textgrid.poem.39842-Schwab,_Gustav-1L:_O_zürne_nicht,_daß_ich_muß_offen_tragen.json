{"textgrid.poem.39842": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: O z\u00fcrne nicht, da\u00df ich mu\u00df offen tragen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O z\u00fcrne nicht, da\u00df ich mu\u00df offen tragen", "tokens": ["O", "z\u00fcr\u00b7ne", "nicht", ",", "da\u00df", "ich", "mu\u00df", "of\u00b7fen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein sehnend Herz, von Liebesglut verzehrt:", "tokens": ["Mein", "seh\u00b7nend", "Herz", ",", "von", "Lie\u00b7bes\u00b7glut", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was Hoffnung leis' als Bl\u00fcte hat gen\u00e4hrt,", "tokens": ["Was", "Hoff\u00b7nung", "leis'", "als", "Bl\u00fc\u00b7te", "hat", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "KOKOM", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das haben schnell zur Frucht gereift die Klagen.", "tokens": ["Das", "ha\u00b7ben", "schnell", "zur", "Frucht", "ge\u00b7reift", "die", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verborgen hab' ich's lang, mit scheuem Zagen,", "tokens": ["Ver\u00b7bor\u00b7gen", "hab'", "ich's", "lang", ",", "mit", "scheu\u00b7em", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Liede selbst, es zu gestehn, verwehrt;", "tokens": ["Dem", "Lie\u00b7de", "selbst", ",", "es", "zu", "ge\u00b7stehn", ",", "ver\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu werden dein, durch stille Treue, wert,", "tokens": ["Zu", "wer\u00b7den", "dein", ",", "durch", "stil\u00b7le", "Treu\u00b7e", ",", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "VAFIN", "PPOSAT", "$,", "APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dich zu r\u00fchren durch ein fromm Entsagen.", "tokens": ["Und", "dich", "zu", "r\u00fch\u00b7ren", "durch", "ein", "fromm", "Ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber mein Bem\u00fchen ist verloren,", "tokens": ["Nun", "a\u00b7ber", "mein", "Be\u00b7m\u00fc\u00b7hen", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun Gegenliebe mir dein Blick versagt,", "tokens": ["Nun", "Ge\u00b7gen\u00b7lie\u00b7be", "mir", "dein", "Blick", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und du dir ach! ein fremdes Herz erkoren:", "tokens": ["Und", "du", "dir", "ach", "!", "ein", "frem\u00b7des", "Herz", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was bleibt von Furcht und Hoffnung noch dem Armen?", "tokens": ["Was", "bleibt", "von", "Furcht", "und", "Hoff\u00b7nung", "noch", "dem", "Ar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O g\u00f6nn' ihm, da\u00df er laut im Liede klagt;", "tokens": ["O", "g\u00f6nn'", "ihm", ",", "da\u00df", "er", "laut", "im", "Lie\u00b7de", "klagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Liebe fern, la\u00df ihn im Schmerz erwarmen!", "tokens": ["Von", "Lie\u00b7be", "fern", ",", "la\u00df", "ihn", "im", "Schmerz", "er\u00b7war\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}