{"textgrid.poem.40793": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebewohl, du lieber Pilger, gr\u00fc\u00dfe mir den fernen Strand,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebewohl, du lieber Pilger, gr\u00fc\u00dfe mir den fernen Strand,", "tokens": ["Le\u00b7be\u00b7wohl", ",", "du", "lie\u00b7ber", "Pil\u00b7ger", ",", "gr\u00fc\u00b7\u00dfe", "mir", "den", "fer\u00b7nen", "Strand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo einst Franklin Weisheit s\u00e4te, Washington einst fechtend stand;", "tokens": ["Wo", "einst", "Fran\u00b7klin", "Weis\u00b7heit", "s\u00e4\u00b7te", ",", "Was\u00b7hing\u00b7ton", "einst", "fech\u00b7tend", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "NN", "VVFIN", "$,", "NE", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Deine Seele, rein und edel, bleibe dr\u00fcben so wie hier,", "tokens": ["Dei\u00b7ne", "See\u00b7le", ",", "rein", "und", "e\u00b7del", ",", "blei\u00b7be", "dr\u00fc\u00b7ben", "so", "wie", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur der Blick, der tr\u00fcbe, werde heitrer \u00fcber'm Meere dir!", "tokens": ["Nur", "der", "Blick", ",", "der", "tr\u00fc\u00b7be", ",", "wer\u00b7de", "hei\u00b7trer", "\u00fc\u00b7ber'm", "Mee\u00b7re", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "ADJD", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Lebewohl! \u2013 Dein sch\u00f6nes Auge, ach, nie sah ich's freudenhell,", "tokens": ["Le\u00b7be\u00b7wohl", "!", "\u2013", "Dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", ",", "ach", ",", "nie", "sah", "ich's", "freu\u00b7den\u00b7hell", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,", "ITJ", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nur, gleich schwarzer Wolke, sch\u00fcttelnd einzle Blitze lustiggrell;", "tokens": ["Nur", ",", "gleich", "schwar\u00b7zer", "Wol\u00b7ke", ",", "sch\u00fct\u00b7telnd", "einz\u00b7le", "Blit\u00b7ze", "lus\u00b7tig\u00b7grell", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch gesenkt sonst immer neigte wehmutvoll und feierlich,", "tokens": ["Doch", "ge\u00b7senkt", "sonst", "im\u00b7mer", "neig\u00b7te", "weh\u00b7mut\u00b7voll", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eine schwarze Trauerfahne, \u00fcbers Vaterland es sich.", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "Trau\u00b7er\u00b7fah\u00b7ne", ",", "\u00fc\u00b7bers", "Va\u00b7ter\u00b7land", "es", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Lebewohl! \u2013 Ha, wei\u00dfe Segel seh' ich schon im Wind sich bl\u00e4h'n,", "tokens": ["Le\u00b7be\u00b7wohl", "!", "\u2013", "Ha", ",", "wei\u00b7\u00dfe", "Se\u00b7gel", "seh'", "ich", "schon", "im", "Wind", "sich", "bl\u00e4h'n", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ITJ", "$,", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seh', umgl\u00e4nzt vom Meeresspiegel, dich an Bord des Schiffes steh'n,", "tokens": ["Seh'", ",", "um\u00b7gl\u00e4nzt", "vom", "Mee\u00b7res\u00b7spie\u00b7gel", ",", "dich", "an", "Bord", "des", "Schif\u00b7fes", "steh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPRART", "NN", "$,", "PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Das, statt Perlen fremder Meere uns zu zollen, jetzt verkehrt", "tokens": ["Das", ",", "statt", "Per\u00b7len", "frem\u00b7der", "Mee\u00b7re", "uns", "zu", "zol\u00b7len", ",", "jetzt", "ver\u00b7kehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "KOUI", "NN", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wohl der sch\u00f6nsten, hellsten eine raubend uns, von dannen f\u00e4hrt.", "tokens": ["Wohl", "der", "sch\u00f6ns\u00b7ten", ",", "hells\u00b7ten", "ei\u00b7ne", "rau\u00b7bend", "uns", ",", "von", "dan\u00b7nen", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "VVFIN", "ART", "ADJD", "PPER", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Lebewohl! \u2013 Gleich Liebesboten tragen flink noch durch das Meer", "tokens": ["Le\u00b7be\u00b7wohl", "!", "\u2013", "Gleich", "Lie\u00b7bes\u00b7bo\u00b7ten", "tra\u00b7gen", "flink", "noch", "durch", "das", "Meer"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "NN", "VVINF", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zwischen Schiff und Land die Wellen Abschiedsk\u00fcsse hin und her,", "tokens": ["Zwi\u00b7schen", "Schiff", "und", "Land", "die", "Wel\u00b7len", "Ab\u00b7schieds\u00b7k\u00fcs\u00b7se", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch es schifft vom Heimatboden nichts mit dir durch Meeresfluth", "tokens": ["Doch", "es", "schifft", "vom", "Hei\u00b7mat\u00b7bo\u00b7den", "nichts", "mit", "dir", "durch", "Mee\u00b7res\u00b7fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PIS", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Als Erinnerung im Herzen und ein gr\u00fcner Strau\u00df am Hut.", "tokens": ["Als", "E\u00b7rin\u00b7ne\u00b7rung", "im", "Her\u00b7zen", "und", "ein", "gr\u00fc\u00b7ner", "Strau\u00df", "am", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "KON", "ART", "ADJA", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Und es ist, so will's mich mahnen, dieser Strau\u00df gleich mir und dir:", "tokens": ["Und", "es", "ist", ",", "so", "will's", "mich", "mah\u00b7nen", ",", "die\u00b7ser", "Strau\u00df", "gleich", "mir", "und", "dir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,", "PDAT", "NE", "ADV", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Frische Zweige, festgewunden in den Kranz der Fr\u00fchlingszier,", "tokens": ["Fri\u00b7sche", "Zwei\u00b7ge", ",", "fest\u00b7ge\u00b7wun\u00b7den", "in", "den", "Kranz", "der", "Fr\u00fch\u00b7lings\u00b7zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und entkeimt dem Heimatboden, der ihm Trieb und Bl\u00fcthen bot,", "tokens": ["Und", "ent\u00b7keimt", "dem", "Hei\u00b7mat\u00b7bo\u00b7den", ",", "der", "ihm", "Trieb", "und", "Bl\u00fc\u00b7then", "bot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und aus dem auch wir gesogen Jugendmuth und Wangenroth.", "tokens": ["Und", "aus", "dem", "auch", "wir", "ge\u00b7so\u00b7gen", "Ju\u00b7gend\u00b7muth", "und", "Wan\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ADV", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Lebewohl! \u2013 Die M\u00f6rser donnern! Stolz entschwebt das Schiff gen West,", "tokens": ["Le\u00b7be\u00b7wohl", "!", "\u2013", "Die", "M\u00f6r\u00b7ser", "don\u00b7nern", "!", "Stolz", "ent\u00b7schwebt", "das", "Schiff", "gen", "West", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVINF", "$.", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wimpel all' und Flaggen deuten, Fingern gleich, die Bahn gen West;", "tokens": ["Wim\u00b7pel", "all'", "und", "Flag\u00b7gen", "deu\u00b7ten", ",", "Fin\u00b7gern", "gleich", ",", "die", "Bahn", "gen", "West", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KON", "NN", "VVINF", "$,", "NN", "ADV", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mit verschr\u00e4nkten Armen seh' ich an den Mast gelehnt dich steh'n,", "tokens": ["Mit", "ver\u00b7schr\u00e4nk\u00b7ten", "Ar\u00b7men", "seh'", "ich", "an", "den", "Mast", "ge\u00b7lehnt", "dich", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber gegen Ost dein Auge nach der Heimat K\u00fcsten sp\u00e4h'n.", "tokens": ["A\u00b7ber", "ge\u00b7gen", "Ost", "dein", "Au\u00b7ge", "nach", "der", "Hei\u00b7mat", "K\u00fcs\u00b7ten", "sp\u00e4h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "NE", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Mich bed\u00fcnkt, es mag das Auge wohl des Herzens Flagge sein,", "tokens": ["Mich", "be\u00b7d\u00fcnkt", ",", "es", "mag", "das", "Au\u00b7ge", "wohl", "des", "Her\u00b7zens", "Flag\u00b7ge", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und dein Herz, die\u00df edle Schifflein, darf des Augs Verrath nicht scheu'n,", "tokens": ["Und", "dein", "Herz", ",", "die\u00df", "ed\u00b7le", "Schif\u00b7flein", ",", "darf", "des", "Augs", "Ver\u00b7rath", "nicht", "scheu'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "ADJA", "NN", "$,", "VMFIN", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schwer wohl ri\u00df es los die Anker, eingebohrt ans Vaterland,", "tokens": ["Schwer", "wohl", "ri\u00df", "es", "los", "die", "An\u00b7ker", ",", "ein\u00b7ge\u00b7bohrt", "ans", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und vielleicht noch blieb manch einer h\u00e4ngen fest am heim'schen Strand.", "tokens": ["Und", "viel\u00b7leicht", "noch", "blieb", "manch", "ei\u00b7ner", "h\u00e4n\u00b7gen", "fest", "am", "heim'\u00b7schen", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PIAT", "PIS", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Drum, o sprich, was lockt dich dr\u00fcben, das die Heimat dir versagt?", "tokens": ["Drum", ",", "o", "sprich", ",", "was", "lockt", "dich", "dr\u00fc\u00b7ben", ",", "das", "die", "Hei\u00b7mat", "dir", "ver\u00b7sagt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ist's des Rechts erhabner Leuchtthurm, der dir hell her\u00fcbertagt?", "tokens": ["Ist's", "des", "Rechts", "er\u00b7hab\u00b7ner", "Leucht\u00b7thurm", ",", "der", "dir", "hell", "her\u00b7\u00fc\u00b7ber\u00b7tagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ist's der Gnadenort der Freiheit, der Madonna unsrer Zeit?", "tokens": ["Ist's", "der", "Gna\u00b7den\u00b7ort", "der", "Frei\u00b7heit", ",", "der", "Ma\u00b7don\u00b7na", "uns\u00b7rer", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Hast auch du der gro\u00dfen Wallfahrt gl\u00e4ub'gen Volks dich angereiht?", "tokens": ["Hast", "auch", "du", "der", "gro\u00b7\u00dfen", "Wall\u00b7fahrt", "gl\u00e4ub'\u00b7gen", "Volks", "dich", "an\u00b7ge\u00b7reiht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Wie der Kreuzespilger Schaaren einst gen Zions Tr\u00fcmmerrest,", "tokens": ["Wie", "der", "Kreu\u00b7ze\u00b7spil\u00b7ger", "Schaa\u00b7ren", "einst", "gen", "Zi\u00b7ons", "Tr\u00fcm\u00b7mer\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00e4lzt sich jetzt der V\u00f6lker Heerzug ins gelobte Land gen West;", "tokens": ["W\u00e4lzt", "sich", "jetzt", "der", "V\u00f6l\u00b7ker", "Heer\u00b7zug", "ins", "ge\u00b7lob\u00b7te", "Land", "gen", "West", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ach, wohl wird's auch euch ergehen, wie sich's jenen einst begab:", "tokens": ["Ach", ",", "wohl", "w