{"dta.poem.18357": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.57", "af:0.42"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Hof her poltert die Fabrik", "tokens": ["Vom", "Hof", "her", "pol\u00b7tert", "die", "Fab\u00b7rik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und walkt und stampft und pocht und h\u00e4mmert,", "tokens": ["Und", "walkt", "und", "stampft", "und", "pocht", "und", "h\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein hirnzermarterndes Gequik!", "tokens": ["Ein", "hirn\u00b7zer\u00b7mar\u00b7tern\u00b7des", "Ge\u00b7quik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht verrinnt, der Traumgott ruht nun,", "tokens": ["Die", "Nacht", "ver\u00b7rinnt", ",", "der", "Traum\u00b7gott", "ruht", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt geht wieder ihren Lauf,", "tokens": ["Die", "Welt", "geht", "wie\u00b7der", "ih\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Himmel spritzt der Tag sein Blut nun,", "tokens": ["Zum", "Him\u00b7mel", "spritzt", "der", "Tag", "sein", "Blut", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Nacht verrinnt und seufzend thut nun", "tokens": ["Die", "Nacht", "ver\u00b7rinnt", "und", "seuf\u00b7zend", "thut", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Das Elend seine Augen auf!", "tokens": ["Das", "E\u00b7lend", "sei\u00b7ne", "Au\u00b7gen", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Schl\u00e4fen zittern mir und zucken,", "tokens": ["Die", "Schl\u00e4\u00b7fen", "zit\u00b7tern", "mir", "und", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denk ich, o Volk, an deine Noth,", "tokens": ["Denk", "ich", ",", "o", "Volk", ",", "an", "dei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie du dich winden mu\u00dft und ducken,", "tokens": ["Wie", "du", "dich", "win\u00b7den", "mu\u00dft", "und", "du\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich ducken um ein St\u00fcckchen Brod!", "tokens": ["Dich", "du\u00b7cken", "um", "ein", "St\u00fcck\u00b7chen", "Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du w\u00e4lzst verthiert dich in der Gosse", "tokens": ["Du", "w\u00e4lz\u00b7st", "ver\u00b7thiert", "dich", "in", "der", "Gos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und baust dir selbst dein Blutger\u00fcst,", "tokens": ["Und", "baust", "dir", "selbst", "dein", "Blut\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Inde\u00df in goldener Karosse", "tokens": ["In\u00b7de\u00df", "in", "gol\u00b7de\u00b7ner", "Ka\u00b7ros\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor seinem sandsteingelben Schlosse", "tokens": ["Vor", "sei\u00b7nem", "sand\u00b7stein\u00b7gel\u00b7ben", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Dandy seine Dirne k\u00fc\u00dft!", "tokens": ["Der", "Dan\u00b7dy", "sei\u00b7ne", "Dir\u00b7ne", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Ritter von der engen Taille,", "tokens": ["Die", "Rit\u00b7ter", "von", "der", "en\u00b7gen", "Tail\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind die schlimmsten aus dem Chor,", "tokens": ["Das", "sind", "die", "schlimms\u00b7ten", "aus", "dem", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schimpfen h\u00fcndisch dich \u201eKanaille\u201c!", "tokens": ["Sie", "schimp\u00b7fen", "h\u00fcn\u00b7disch", "dich", "\u201e", "Ka\u00b7nail\u00b7le", "\u201c", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "$(", "NN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und haun dich schamlos \u00fcbers Ohr.", "tokens": ["Und", "haun", "dich", "scham\u00b7los", "\u00fc\u00b7bers", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was k\u00fcmmert sie's, wenn Millionen", "tokens": ["Was", "k\u00fcm\u00b7mert", "sie's", ",", "wenn", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Verreckt sind hinterm Hungerzaun?", "tokens": ["Ver\u00b7reckt", "sind", "hin\u00b7term", "Hun\u00b7ger\u00b7zaun", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch giebt's ja lachende Dublonen,", "tokens": ["Noch", "giebt's", "ja", "la\u00b7chen\u00b7de", "Du\u00b7blo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kasernen, Kirchen und Kanonen", "tokens": ["Ka\u00b7ser\u00b7nen", ",", "Kir\u00b7chen", "und", "Ka\u00b7no\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und .... k\u00f6stlich mundet ein Kapaun!", "tokens": ["Und", "....", "k\u00f6st\u00b7lich", "mun\u00b7det", "ein", "Ka\u00b7paun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, sprich, wie lang noch soll es dauern,", "tokens": ["O", ",", "sprich", ",", "wie", "lang", "noch", "soll", "es", "dau\u00b7ern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWAV", "ADJD", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das alte Reich der Barbarei!", "tokens": ["Das", "al\u00b7te", "Reich", "der", "Bar\u00b7ba\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch st\u00fctzen tausend dunkle Mauern", "tokens": ["Noch", "st\u00fct\u00b7zen", "tau\u00b7send", "dunk\u00b7le", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die feste Burg der Tyrannei.", "tokens": ["Die", "fes\u00b7te", "Burg", "der", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ach, dein Herz ward zur Ruine,", "tokens": ["Doch", "ach", ",", "dein", "Herz", "ward", "zur", "Ru\u00b7i\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du l\u00e4chelst nur und nickst dazu!", "tokens": ["Du", "l\u00e4\u00b7chelst", "nur", "und", "nickst", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn auch der Mensch wird zur Maschine,", "tokens": ["Denn", "auch", "der", "Mensch", "wird", "zur", "Ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn er mit hungerbleicher Miene", "tokens": ["Wenn", "er", "mit", "hun\u00b7ger\u00b7blei\u00b7cher", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das alte Tretrad schwingt wie du!", "tokens": ["Das", "al\u00b7te", "Tre\u00b7trad", "schwingt", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}