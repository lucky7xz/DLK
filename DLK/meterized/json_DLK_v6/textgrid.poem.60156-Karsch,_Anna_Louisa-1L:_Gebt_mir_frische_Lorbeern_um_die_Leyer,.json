{"textgrid.poem.60156": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gebt mir frische Lorbeern um die Leyer,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt mir frische Lorbeern um die Leyer,", "tokens": ["Gebt", "mir", "fri\u00b7sche", "Lor\u00b7beern", "um", "die", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Denn ich gl\u00fche von der Helden Feuer,", "tokens": ["Denn", "ich", "gl\u00fc\u00b7he", "von", "der", "Hel\u00b7den", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Braunschweigs j\u00fcngster Sieger sey mein Lied!", "tokens": ["Braun\u00b7schweigs", "j\u00fcngs\u00b7ter", "Sie\u00b7ger", "sey", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Friedrich, seines Bruders tapfrer R\u00e4cher,", "tokens": ["Fried\u00b7rich", ",", "sei\u00b7nes", "Bru\u00b7ders", "tapf\u00b7rer", "R\u00e4\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kam geflogen, schlug die Mauerbrecher;", "tokens": ["Kam", "ge\u00b7flo\u00b7gen", ",", "schlug", "die", "Mau\u00b7er\u00b7bre\u00b7cher", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zorn des L\u00f6wen hat in ihm gegl\u00fcht!", "tokens": ["Zorn", "des", "L\u00f6\u00b7wen", "hat", "in", "ihm", "ge\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Also gl\u00fchte Cyrus, da er fragte,", "tokens": ["Al\u00b7so", "gl\u00fch\u00b7te", "Cy\u00b7rus", ",", "da", "er", "frag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was die fremde R\u00e4uber-Rotte wagte,", "tokens": ["Was", "die", "frem\u00b7de", "R\u00e4u\u00b7ber\u00b7Rot\u00b7te", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die der Meder Heerden rauben kam.", "tokens": ["Die", "der", "Me\u00b7der", "Heer\u00b7den", "rau\u00b7ben", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fctend grif er mit der zarten Rechte", "tokens": ["W\u00fc\u00b7tend", "grif", "er", "mit", "der", "zar\u00b7ten", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seinen S\u00e4bel, hieb dem Kriegesknechte", "tokens": ["Sei\u00b7nen", "S\u00e4\u00b7bel", ",", "hieb", "dem", "Krie\u00b7ges\u00b7knech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Klauen ab, womit er Rinder nahm.", "tokens": ["Klau\u00b7en", "ab", ",", "wo\u00b7mit", "er", "Rin\u00b7der", "nahm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Friedrich, der zum erstenmahl ger\u00fcstet", "tokens": ["Fried\u00b7rich", ",", "der", "zum", "ers\u00b7ten\u00b7mahl", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPRART", "ADV", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geht in Waffen, sah im Geist verw\u00fcstet", "tokens": ["Geht", "in", "Waf\u00b7fen", ",", "sah", "im", "Geist", "ver\u00b7w\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seines v\u00e4terlichen Hauses Stadt.", "tokens": ["Sei\u00b7nes", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Hau\u00b7ses", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gleich den Drachen, welche Feuer speien,", "tokens": ["Gleich", "den", "Dra\u00b7chen", ",", "wel\u00b7che", "Feu\u00b7er", "spei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lagen Feindes Donner, sie bedr\u00e4uen", "tokens": ["La\u00b7gen", "Fein\u00b7des", "Don\u00b7ner", ",", "sie", "be\u00b7dr\u00e4u\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rings um veste Warten, die sie hat.", "tokens": ["Rings", "um", "ves\u00b7te", "War\u00b7ten", ",", "die", "sie", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Das Geschrey der Kinder und der M\u00fctter", "tokens": ["Das", "Ge\u00b7schrey", "der", "Kin\u00b7der", "und", "der", "M\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Drang zum Gotte, der das Ungewitter", "tokens": ["Drang", "zum", "Got\u00b7te", ",", "der", "das", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der hohen Luft sich theilen hei\u00dft.", "tokens": ["In", "der", "ho\u00b7hen", "Luft", "sich", "thei\u00b7len", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00e4chtig sah er aus dem Wolkenbogen;", "tokens": ["M\u00e4ch\u00b7tig", "sah", "er", "aus", "dem", "Wol\u00b7ken\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mit st\u00e4rkerm Muthe angezogen", "tokens": ["Und", "mit", "st\u00e4r\u00b7kerm", "Mu\u00b7the", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ward des jungen Helden k\u00fchner Geist!", "tokens": ["Ward", "des", "jun\u00b7gen", "Hel\u00b7den", "k\u00fch\u00b7ner", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wie ein Adler, die versuchten Schwingen,", "tokens": ["Wie", "ein", "Ad\u00b7ler", ",", "die", "ver\u00b7such\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem gr\u00f6ssern Feinde stark zu ringen,", "tokens": ["Mit", "dem", "gr\u00f6s\u00b7sern", "Fein\u00b7de", "stark", "zu", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schnell und kl\u00fcglich zu gebrauchen sucht:", "tokens": ["Schnell", "und", "kl\u00fcg\u00b7lich", "zu", "ge\u00b7brau\u00b7chen", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Also muthig foderte die Kr\u00e4fte", "tokens": ["Al\u00b7so", "mut\u00b7hig", "fo\u00b7der\u00b7te", "die", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Friedrich, zu dem tapfersten Gesch\u00e4fte.", "tokens": ["Fried\u00b7rich", ",", "zu", "dem", "tap\u00b7fers\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Pl\u00f6zlich bracht er in die Feinde Flucht.", "tokens": ["Pl\u00f6z\u00b7lich", "bracht", "er", "in", "die", "Fein\u00b7de", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Bey dem Grabe Heinrichs laut erschollen", "tokens": ["Bey", "dem", "Gra\u00b7be", "Hein\u00b7richs", "laut", "er\u00b7schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind die Jubel von den freudenvollen", "tokens": ["Sind", "die", "Ju\u00b7bel", "von", "den", "freu\u00b7den\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "B\u00fcrgern, auf dem frey gewordnen Wall.", "tokens": ["B\u00fcr\u00b7gern", ",", "auf", "dem", "frey", "ge\u00b7word\u00b7nen", "Wall", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Tausend Stimmen riefen: Friedrich lebe!", "tokens": ["Tau\u00b7send", "Stim\u00b7men", "rie\u00b7fen", ":", "Fried\u00b7rich", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$.", "NE", "VVFIN", "$."], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und ein zweytes; Friedrich, Friedrich lebe!", "tokens": ["Und", "ein", "zwey\u00b7tes", ";", "Fried\u00b7rich", ",", "Fried\u00b7rich", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$.", "NE", "$,", "NE", "VVFIN", "$."], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sprach des Harzgeb\u00fcrges Wiederhall!", "tokens": ["Sprach", "des", "Harz\u00b7ge\u00b7b\u00fcr\u00b7ges", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Auf dem Brocken h\u00f6rten es die Rehe;", "tokens": ["Auf", "dem", "Bro\u00b7cken", "h\u00f6r\u00b7ten", "es", "die", "Re\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hirsche warfen pl\u00f6zlich in die H\u00f6he", "tokens": ["Hir\u00b7sche", "war\u00b7fen", "pl\u00f6z\u00b7lich", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihrer H\u00e4upter zackigtes Geweyh;", "tokens": ["Ih\u00b7rer", "H\u00e4up\u00b7ter", "za\u00b7ckig\u00b7tes", "Ge\u00b7weyh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den Betten w\u00e4lzten sich die Thiere,", "tokens": ["Aus", "den", "Bet\u00b7ten", "w\u00e4lz\u00b7ten", "sich", "die", "Thie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und im Thale liessen zweene Stiere", "tokens": ["Und", "im", "Tha\u00b7le", "lies\u00b7sen", "zwee\u00b7ne", "Stie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihren Kampf, und horchten dem Geschrey.", "tokens": ["Ih\u00b7ren", "Kampf", ",", "und", "horch\u00b7ten", "dem", "Ge\u00b7schrey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Also wird der ganze Wald erf\u00fcllet,", "tokens": ["Al\u00b7so", "wird", "der", "gan\u00b7ze", "Wald", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der L\u00f6we Siegeslieder br\u00fcllet,", "tokens": ["Wenn", "der", "L\u00f6\u00b7we", "Sie\u00b7ges\u00b7lie\u00b7der", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der den Tyger tapfer \u00fcberwand.", "tokens": ["Der", "den", "Ty\u00b7ger", "tap\u00b7fer", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dieses Thier, voll Blutdurst in der Seele,", "tokens": ["Die\u00b7ses", "Thier", ",", "voll", "Blut\u00b7durst", "in", "der", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Trat verwegen vor des L\u00f6wen H\u00f6le;", "tokens": ["Trat", "ver\u00b7we\u00b7gen", "vor", "des", "L\u00f6\u00b7wen", "H\u00f6\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der zerri\u00df ihn, den er schlafen fand.", "tokens": ["Der", "zer\u00b7ri\u00df", "ihn", ",", "den", "er", "schla\u00b7fen", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}