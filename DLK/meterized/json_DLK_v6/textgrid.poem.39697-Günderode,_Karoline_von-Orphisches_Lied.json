{"textgrid.poem.39697": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Orphisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re mich Phoibos Apoll! Du, der auf bl\u00e4uligem Bogen", "tokens": ["H\u00f6\u00b7re", "mich", "Phoi\u00b7bos", "A\u00b7poll", "!", "Du", ",", "der", "auf", "bl\u00e4u\u00b7li\u00b7gem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "NE", "$.", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Siegreich schreitet herauf an w\u00f6lbichter Feste des Himmels,", "tokens": ["Sieg\u00b7reich", "schrei\u00b7tet", "her\u00b7auf", "an", "w\u00f6l\u00b7bich\u00b7ter", "Fes\u00b7te", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Spendend die heilige Helle der Wolkenerzeugenden Erde,", "tokens": ["Spen\u00b7dend", "die", "hei\u00b7li\u00b7ge", "Hel\u00b7le", "der", "Wol\u00b7ke\u00b7ner\u00b7zeu\u00b7gen\u00b7den", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Leuchtend Okeanos hin zur Tiefe des felsichten Bettes,", "tokens": ["Leuch\u00b7tend", "O\u00b7kea\u00b7nos", "hin", "zur", "Tie\u00b7fe", "des", "fel\u00b7sich\u00b7ten", "Bet\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.5": {"text": "H\u00f6re mich Liebling des Zeus! Sieh gn\u00e4dig auf deinen Geweihten!", "tokens": ["H\u00f6\u00b7re", "mich", "Lieb\u00b7ling", "des", "Zeus", "!", "Sieh", "gn\u00e4\u00b7dig", "auf", "dei\u00b7nen", "Ge\u00b7weih\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NE", "$.", "NE", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Sei im Gesang mir gew\u00e4rtig, und lasse der goldenen Leyer", "tokens": ["Sei", "im", "Ge\u00b7sang", "mir", "ge\u00b7w\u00e4r\u00b7tig", ",", "und", "las\u00b7se", "der", "gol\u00b7de\u00b7nen", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "ADJD", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.7": {"text": "Saiten mir klingen, wie dir, wenn mit siegender Lippe du singest", "tokens": ["Sai\u00b7ten", "mir", "klin\u00b7gen", ",", "wie", "dir", ",", "wenn", "mit", "sie\u00b7gen\u00b7der", "Lip\u00b7pe", "du", "sin\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Pythons des schrecklichen Fall dem Chore melodischer Musen,", "tokens": ["Py\u00b7thons", "des", "schreck\u00b7li\u00b7chen", "Fall", "dem", "Cho\u00b7re", "me\u00b7lo\u00b7di\u00b7scher", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+---+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Oder im Liede besingst ferntreffende Pfeile des Bogens,", "tokens": ["O\u00b7der", "im", "Lie\u00b7de", "be\u00b7singst", "fern\u00b7tref\u00b7fen\u00b7de", "Pfei\u00b7le", "des", "Bo\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Also, o Phoibos Apoll! la\u00df von begeistertem Munde", "tokens": ["Al\u00b7so", ",", "o", "Phoi\u00b7bos", "A\u00b7poll", "!", "la\u00df", "von", "be\u00b7geis\u00b7ter\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NE", "NE", "$.", "VVIMP", "APPR", "ADJA", "NN"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Str\u00f6men mir wogende Rythmen des sinnebeherrschenden Wohllauts,", "tokens": ["Str\u00f6\u00b7men", "mir", "wo\u00b7gen\u00b7de", "Ry\u00b7th\u00b7men", "des", "sin\u00b7ne\u00b7be\u00b7herr\u00b7schen\u00b7den", "Wohl\u00b7lauts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+--+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Da\u00df sich der Wald mit beseele, die Dryas des Baumes mir lausche,", "tokens": ["Da\u00df", "sich", "der", "Wald", "mit", "be\u00b7see\u00b7le", ",", "die", "Dryas", "des", "Bau\u00b7mes", "mir", "lau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "VVFIN", "$,", "ART", "NN", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Schl\u00e4ngelnde Str\u00f6me mir folgen, und rei\u00dfende Thiere unsch\u00e4dlich", "tokens": ["Schl\u00e4n\u00b7geln\u00b7de", "Str\u00f6\u00b7me", "mir", "fol\u00b7gen", ",", "und", "rei\u00b7\u00dfen\u00b7de", "Thie\u00b7re", "un\u00b7sch\u00e4d\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,", "KON", "ADJA", "NN", "ADJD"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.14": {"text": "Schmeichelnd zu mir sich gesellen. Vor allem Erzeugter Kronions!", "tokens": ["Schmei\u00b7chelnd", "zu", "mir", "sich", "ge\u00b7sel\u00b7len", ".", "Vor", "al\u00b7lem", "Er\u00b7zeug\u00b7ter", "Kro\u00b7nions", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PRF", "VVPP", "$.", "APPR", "PIS", "NN", "NE", "$."], "meter": "+-+-+-+-+---+-+-", "measure": "unknown.measure.septa"}, "line.15": {"text": "Gieb des Gesanges herrschende Kraft, die drunten gewaltig", "tokens": ["Gieb", "des", "Ge\u00b7san\u00b7ges", "herr\u00b7schen\u00b7de", "Kraft", ",", "die", "drun\u00b7ten", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "\u00c4is den K\u00f6nig bewege des Landes am stygischem Strome.", "tokens": ["\u00c4\u00b7is", "den", "K\u00f6\u00b7nig", "be\u00b7we\u00b7ge", "des", "Lan\u00b7des", "am", "sty\u00b7gi\u00b7schem", "Stro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Lehre vergessene Schmerzen mich wecken im Busen der G\u00f6ttin", "tokens": ["Leh\u00b7re", "ver\u00b7ges\u00b7se\u00b7ne", "Schmer\u00b7zen", "mich", "we\u00b7cken", "im", "Bu\u00b7sen", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Die ein zu strenges Gebot dem d\u00fcsteren Herrscher verm\u00e4hlet,", "tokens": ["Die", "ein", "zu", "stren\u00b7ges", "Ge\u00b7bot", "dem", "d\u00fcs\u00b7te\u00b7ren", "Herr\u00b7scher", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PTKZU", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da\u00df sie erbarmend sich zeige dem Schwestergeschick der Geliebten,", "tokens": ["Da\u00df", "sie", "er\u00b7bar\u00b7mend", "sich", "zei\u00b7ge", "dem", "Schwes\u00b7ter\u00b7ge\u00b7schick", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Wieder ihr g\u00f6nne zu schaun des Tages sonnige Klarheit,", "tokens": ["Wie\u00b7der", "ihr", "g\u00f6n\u00b7ne", "zu", "schaun", "des", "Ta\u00b7ges", "son\u00b7ni\u00b7ge", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Deines unsterblichen Haupts fern leuchtende Strahlen, o Phoibos!", "tokens": ["Dei\u00b7nes", "uns\u00b7terb\u00b7li\u00b7chen", "Haupts", "fern", "leuch\u00b7ten\u00b7de", "Strah\u00b7len", ",", "o", "Phoi\u00b7bos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,", "FM", "NE", "$."], "meter": "+--+--+-+--+-+++", "measure": "dactylic.di.plus"}}}}}