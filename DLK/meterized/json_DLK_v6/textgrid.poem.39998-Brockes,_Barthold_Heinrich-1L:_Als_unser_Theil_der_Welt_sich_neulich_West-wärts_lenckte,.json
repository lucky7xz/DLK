{"textgrid.poem.39998": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als unser Theil der Welt sich neulich West-w\u00e4rts lenckte,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als unser Theil der Welt sich neulich West-w\u00e4rts lenckte,", "tokens": ["Als", "un\u00b7ser", "Theil", "der", "Welt", "sich", "neu\u00b7lich", "West\u00b7w\u00e4rts", "lenck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PRF", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in das dunckle Reich der k\u00fchlen Schatten senckte;", "tokens": ["Und", "in", "das", "dunck\u00b7le", "Reich", "der", "k\u00fch\u00b7len", "Schat\u00b7ten", "senck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hatt' ein sanft-rauschendes und tr\u00f6pfelnd Wolcken-Na\u00df,", "tokens": ["Hatt'", "ein", "sanft\u00b7rau\u00b7schen\u00b7des", "und", "tr\u00f6p\u00b7felnd", "Wol\u00b7cken\u00b7Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So in der Luft gekocht, vom Himmels-Saamen schwanger,", "tokens": ["So", "in", "der", "Luft", "ge\u00b7kocht", ",", "vom", "Him\u00b7mels\u00b7Saa\u00b7men", "schwan\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das d\u00fcrre Feld, den durst'gen Anger,", "tokens": ["Das", "d\u00fcr\u00b7re", "Feld", ",", "den", "dur\u00b7st'\u00b7gen", "An\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das lechzende Gestr\u00e4uch, das welcke Laub und Gras", "tokens": ["Das", "lech\u00b7zen\u00b7de", "Ge\u00b7str\u00e4uch", ",", "das", "wel\u00b7cke", "Laub", "und", "Gras"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Genetzt, getr\u00e4nckt, erquickt, erfrischet,", "tokens": ["Ge\u00b7netzt", ",", "ge\u00b7tr\u00e4nckt", ",", "er\u00b7quickt", ",", "er\u00b7fri\u00b7schet", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Himmels m\u00e4nnlich Feur mit ird'schem Saltz gemischet,", "tokens": ["Des", "Him\u00b7mels", "m\u00e4nn\u00b7lich", "Feur", "mit", "ird'\u00b7schem", "Saltz", "ge\u00b7mi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und neue Fruchtbarkeit in Schoo\u00df der Erde bracht.", "tokens": ["Und", "neu\u00b7e", "Frucht\u00b7bar\u00b7keit", "in", "Schoo\u00df", "der", "Er\u00b7de", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es weht' ein frischer Wind aus k\u00fchler Mitter-Nacht;", "tokens": ["Es", "weht'", "ein", "fri\u00b7scher", "Wind", "aus", "k\u00fch\u00b7ler", "Mit\u00b7ter\u00b7Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Himmel ward hiedurch von Duft und Dunst gel\u00e4utert,", "tokens": ["Der", "Him\u00b7mel", "ward", "hie\u00b7durch", "von", "Duft", "und", "Dunst", "ge\u00b7l\u00e4u\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Grentzen-lose Reich des Luft-Raums ausgeheitert,", "tokens": ["Das", "Grent\u00b7zen\u00b7lo\u00b7se", "Reich", "des", "Luft\u00b7Raums", "aus\u00b7ge\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und stellt, mit solcher Wunder-Pracht,", "tokens": ["Und", "stellt", ",", "mit", "sol\u00b7cher", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In unergr\u00fcndlich-tiefer Ferne", "tokens": ["In", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich\u00b7tie\u00b7fer", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der d\u00fcnnen Luft, solch eine Menge Sterne", "tokens": ["Der", "d\u00fcn\u00b7nen", "Luft", ",", "solch", "ei\u00b7ne", "Men\u00b7ge", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Den starren Augen vor; da\u00df, bey so heiterm Schein,", "tokens": ["Den", "star\u00b7ren", "Au\u00b7gen", "vor", ";", "da\u00df", ",", "bey", "so", "hei\u00b7term", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "KOUS", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das d\u00fcst're Blau gantz silbern schien zu seyn:", "tokens": ["Das", "d\u00fcst'\u00b7re", "Blau", "gantz", "sil\u00b7bern", "schien", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das Auge kann, an den gestirnten H\u00f6hen,", "tokens": ["Das", "Au\u00b7ge", "kann", ",", "an", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ein ewig Freuden-Feur, mit tausend Freuden, sehen,", "tokens": ["Ein", "e\u00b7wig", "Freu\u00b7den\u00b7Feur", ",", "mit", "tau\u00b7send", "Freu\u00b7den", ",", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "CARD", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das, Gott zur Ehre, strahl't und unverbrennlich brennt.", "tokens": ["Das", ",", "Gott", "zur", "Eh\u00b7re", ",", "strahl't", "und", "un\u00b7ver\u00b7brenn\u00b7lich", "brennt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "APPRART", "NN", "$,", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus tausend Lichtern stammt ein allgemeines Licht,", "tokens": ["Aus", "tau\u00b7send", "Lich\u00b7tern", "stammt", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Durch welches jedennoch, mit immer regem Strahl,", "tokens": ["Durch", "wel\u00b7ches", "je\u00b7den\u00b7noch", ",", "mit", "im\u00b7mer", "re\u00b7gem", "Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So mancher Sternen Glantz mit st\u00e4rckerm Funckeln bricht,", "tokens": ["So", "man\u00b7cher", "Ster\u00b7nen", "Glantz", "mit", "st\u00e4r\u00b7ckerm", "Fun\u00b7ckeln", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und es bald st\u00e4rckt, bald schw\u00e4cht. Hier flammten ohne Zahl", "tokens": ["Und", "es", "bald", "st\u00e4rckt", ",", "bald", "schw\u00e4cht", ".", "Hier", "flamm\u00b7ten", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Viel tausend, welche theils, wie sch\u00fctternde Rubinen", "tokens": ["Viel", "tau\u00b7send", ",", "wel\u00b7che", "theils", ",", "wie", "sch\u00fct\u00b7tern\u00b7de", "Ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PRELS", "ADV", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In r\u00f6thlich-reger Gluth, theils Diamanten gleich,", "tokens": ["In", "r\u00f6\u00b7th\u00b7lich\u00b7re\u00b7ger", "Gluth", ",", "theils", "Di\u00b7a\u00b7man\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "(doch welch ein Edelstein war je so Feuer-reich?)", "tokens": ["(", "doch", "welch", "ein", "E\u00b7del\u00b7stein", "war", "je", "so", "Feu\u00b7er\u00b7reich", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAT", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit blendenden Schnee-weissen Blitzen, schienen.", "tokens": ["Mit", "blen\u00b7den\u00b7den", "Schnee\u00b7weis\u00b7sen", "Blit\u00b7zen", ",", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Jedweden sichtbar'n Stern umh\u00fcllt' ein weisser Schein", "tokens": ["Jed\u00b7we\u00b7den", "sicht\u00b7ba\u00b7r'n", "Stern", "um\u00b7h\u00fcllt'", "ein", "weis\u00b7ser", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Von Sternen, die in ungeheuren H\u00f6hen,", "tokens": ["Von", "Ster\u00b7nen", ",", "die", "in", "un\u00b7ge\u00b7heu\u00b7ren", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So wie das Sternen-Heer des Milch-Weg's, nicht zu sehen:", "tokens": ["So", "wie", "das", "Ster\u00b7nen\u00b7Heer", "des", "Milch\u00b7Weg's", ",", "nicht", "zu", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Daher schien jeder Stern ein Sieben-Stern zu seyn.", "tokens": ["Da\u00b7her", "schien", "je\u00b7der", "Stern", "ein", "Sie\u00b7ben\u00b7S\u00b7tern", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Ob diesem der Natur so weiten Schau-Platz starrt", "tokens": ["Ob", "die\u00b7sem", "der", "Na\u00b7tur", "so", "wei\u00b7ten", "Schau\u00b7Platz", "starrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein drin versinckend Aug'; die Seele wird ger\u00fchret;", "tokens": ["Mein", "drin", "ver\u00b7sin\u00b7ckend", "Aug'", ";", "die", "See\u00b7le", "wird", "ge\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVPP", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es l\u00e4sst, als wenn mein Hertz des Sch\u00f6pfers Gegenwart", "tokens": ["Es", "l\u00e4sst", ",", "als", "wenn", "mein", "Hertz", "des", "Sch\u00f6p\u00b7fers", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In unaussprechlicher Pracht, Gr\u00f6ss' und Klarheit sp\u00fcret.", "tokens": ["In", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Pracht", ",", "Gr\u00f6ss'", "und", "Klar\u00b7heit", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Mich deucht, ob s\u00e4h' mein Geist den unsichtbaren Gott,", "tokens": ["Mich", "deucht", ",", "ob", "s\u00e4h'", "mein", "Geist", "den", "un\u00b7sicht\u00b7ba\u00b7ren", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der selbst der Ewigkeit Unendlichkeiten f\u00fcllet,", "tokens": ["Der", "selbst", "der", "E\u00b7wig\u00b7keit", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Seraphinen Herrn, den Herrscher Zebaoth,", "tokens": ["Der", "Se\u00b7ra\u00b7phi\u00b7nen", "Herrn", ",", "den", "Herr\u00b7scher", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als w\u00e4r' Er in ein Kleid von Glantz und Licht geh\u00fcllet,", "tokens": ["Als", "w\u00e4r'", "Er", "in", "ein", "Kleid", "von", "Glantz", "und", "Licht", "ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In ein unendlich Kleid, drauf, statt der Edelsteinen,", "tokens": ["In", "ein", "un\u00b7end\u00b7lich", "Kleid", ",", "drauf", ",", "statt", "der", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,", "PAV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel tausend tausend Sonnen scheinen,", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send", "Son\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Statt Perl und Gold, viel Millionen Erden.", "tokens": ["Statt", "Perl", "und", "Gold", ",", "viel", "Mil\u00b7lion\u00b7en", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ach! rief ich, m\u00f6cht' ich, die\u00df recht zu betrachten, taugen!", "tokens": ["Ach", "!", "rief", "ich", ",", "m\u00f6cht'", "ich", ",", "die\u00df", "recht", "zu", "be\u00b7trach\u00b7ten", ",", "tau\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "$,", "PDS", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach! m\u00f6chte Leib und Geist, zu Gottes Ehr', zu Augen,", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "Leib", "und", "Geist", ",", "zu", "Got\u00b7tes", "Ehr'", ",", "zu", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "NN", "KON", "NN", "$,", "APPR", "NN", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dann, zu seinem Ruhm, zu lauter Zungen werden!", "tokens": ["Und", "dann", ",", "zu", "sei\u00b7nem", "Ruhm", ",", "zu", "lau\u00b7ter", "Zun\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}