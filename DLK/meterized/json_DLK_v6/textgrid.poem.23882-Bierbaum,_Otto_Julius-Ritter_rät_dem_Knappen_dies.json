{"textgrid.poem.23882": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ritter r\u00e4t dem Knappen dies", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sitz im Sattel, reite,", "tokens": ["Sitz", "im", "Sat\u00b7tel", ",", "rei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Reite auf die Freite,", "tokens": ["Rei\u00b7te", "auf", "die", "Frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freie dir die Fee der Freien,", "tokens": ["Frei\u00b7e", "dir", "die", "Fee", "der", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freie sie im milden Maien;", "tokens": ["Frei\u00b7e", "sie", "im", "mil\u00b7den", "Mai\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Narzissen in den H\u00e4nden", "tokens": ["Mit", "Nar\u00b7zis\u00b7sen", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geh ihr nah, doch an der Lenden", "tokens": ["Geh", "ihr", "nah", ",", "doch", "an", "der", "Len\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwebe dir dein Schwert.", "tokens": ["Schwe\u00b7be", "dir", "dein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sprich zu ihr: Madleine,", "tokens": ["Sprich", "zu", "ihr", ":", "Mad\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "$.", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Rose, Rose, reine,", "tokens": ["Ro\u00b7se", ",", "Ro\u00b7se", ",", "rei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Willst du dich mir freundlich neigen?", "tokens": ["Willst", "du", "dich", "mir", "freund\u00b7lich", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du mir den Himmel zeigen?", "tokens": ["Willst", "du", "mir", "den", "Him\u00b7mel", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie wird die Blicke senken,", "tokens": ["Und", "sie", "wird", "die", "Bli\u00b7cke", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird dir alle Himmel schenken.", "tokens": ["Wird", "dir", "al\u00b7le", "Him\u00b7mel", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm sie auf dein Pferd.", "tokens": ["Nimm", "sie", "auf", "dein", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sitz im Sattel, sause,", "tokens": ["Sitz", "im", "Sat\u00b7tel", ",", "sau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Reit mit ihr nach Hause;", "tokens": ["Reit", "mit", "ihr", "nach", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zwischen seidenbunten Decken", "tokens": ["Zwi\u00b7schen", "sei\u00b7den\u00b7bun\u00b7ten", "De\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du dir dein Gl\u00fcck verstecken.", "tokens": ["Sollst", "du", "dir", "dein", "Gl\u00fcck", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Thore zugeschlossen!", "tokens": ["Al\u00b7le", "Tho\u00b7re", "zu\u00b7ge\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00e4mmergold ist ausgegossen", "tokens": ["D\u00e4m\u00b7mer\u00b7gold", "ist", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueber euern Herd.", "tokens": ["Ue\u00b7ber", "eu\u00b7ern", "Herd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}