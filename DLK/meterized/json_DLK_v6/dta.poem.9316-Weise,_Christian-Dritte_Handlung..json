{"dta.poem.9316": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Handlung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mechanie du sch\u00f6nes bild/", "tokens": ["Me\u00b7cha\u00b7nie", "du", "sch\u00f6\u00b7nes", "bild", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du hast dich allbereit verh\u00fcllt/", "tokens": ["Du", "hast", "dich", "all\u00b7be\u00b7reit", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem zarten bette:", "tokens": ["In", "dei\u00b7nem", "zar\u00b7ten", "bet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach wenn ich auch dergleichen ruh/", "tokens": ["Ach", "wenn", "ich", "auch", "derg\u00b7lei\u00b7chen", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "PIS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auff meinem lager h\u00e4tte!", "tokens": ["Auff", "mei\u00b7nem", "la\u00b7ger", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wiewohl du loses diebgen du/", "tokens": ["Wie\u00b7wohl", "du", "lo\u00b7ses", "dieb\u00b7gen", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein schlaff thut meinem wachen weh/", "tokens": ["Dein", "schlaff", "thut", "mei\u00b7nem", "wa\u00b7chen", "weh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mechanie.", "tokens": ["Me\u00b7cha\u00b7nie", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Gedencke/ da\u00df ich krafftlo\u00df bin/", "tokens": ["Ge\u00b7den\u00b7cke", "/", "da\u00df", "ich", "kraff\u00b7tlo\u00df", "bin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern ich nicht empfange", "tokens": ["Wo\u00b7fern", "ich", "nicht", "emp\u00b7fan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schatz/ woran ich allbereit", "tokens": ["Den", "schatz", "/", "wo\u00b7ran", "ich", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit gantzem hertzen hange.", "tokens": ["Mit", "gant\u00b7zem", "hert\u00b7zen", "han\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entferne dich nicht allzuweit/", "tokens": ["Ent\u00b7fer\u00b7ne", "dich", "nicht", "all\u00b7zu\u00b7weit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein liebgen/ oder ich vergeh/", "tokens": ["Mein", "lieb\u00b7gen", "/", "o\u00b7der", "ich", "ver\u00b7geh", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$(", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mechanie.", "tokens": ["Me\u00b7cha\u00b7nie", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "So weicht der gantze jammer fort:", "tokens": ["So", "weicht", "der", "gant\u00b7ze", "jam\u00b7mer", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich nur zu meinem leben:", "tokens": ["Sprich", "nur", "zu", "mei\u00b7nem", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wollest deiner augen schein", "tokens": ["Du", "wol\u00b7lest", "dei\u00b7ner", "au\u00b7gen", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu meiner lust ergeben.", "tokens": ["Zu", "mei\u00b7ner", "lust", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So will ich frisch und fr\u00f6lich seyn/", "tokens": ["So", "will", "ich", "frisch", "und", "fr\u00f6\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So will ich ruffen/ wo ich steh/", "tokens": ["So", "will", "ich", "ruf\u00b7fen", "/", "wo", "ich", "steh", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mechanie.", "tokens": ["Me\u00b7cha\u00b7nie", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Hat dich zwar in den schlaff gebracht/", "tokens": ["Hat", "dich", "zwar", "in", "den", "schlaff", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch morgen will ich hoffen/", "tokens": ["Doch", "mor\u00b7gen", "will", "ich", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mich das allcrsch\u00f6nste lo\u00df", "tokens": ["Da\u00df", "mich", "das", "allcr\u00b7sch\u00f6ns\u00b7te", "lo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hat allermeist betroffen/", "tokens": ["Hat", "al\u00b7ler\u00b7meist", "be\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und so wird meine freude gro\u00df/", "tokens": ["Und", "so", "wird", "mei\u00b7ne", "freu\u00b7de", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sag' auf diese nacht ade!", "tokens": ["Und", "sag'", "auf", "die\u00b7se", "nacht", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mechanie.", "tokens": ["Me\u00b7cha\u00b7nie", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}