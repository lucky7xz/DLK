{"dta.poem.12232": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vogel-Urtheil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "             Als ich j\u00fcngst, mich zu erquicken,", "tokens": ["Als", "ich", "j\u00fcngst", ",", "mich", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "             Unter dunklen B\u00e4umen sass,", "tokens": ["Un\u00b7ter", "dunk\u00b7len", "B\u00e4u\u00b7men", "sass", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "             H\u00f6rt\u2019 ich ticken, leise ticken,", "tokens": ["H\u00f6rt'", "ich", "ti\u00b7cken", ",", "lei\u00b7se", "ti\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Zierlich, wie nach Takt und Maass.", "tokens": ["Zier\u00b7lich", ",", "wie", "nach", "Takt", "und", "Maass", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         B\u00f6se wurd\u2019 ich, zog Gesichter,", "tokens": ["B\u00f6\u00b7se", "wurd'", "ich", ",", "zog", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "         Endlich aber gab ich nach,", "tokens": ["End\u00b7lich", "a\u00b7ber", "gab", "ich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "             Bis ich gar, gleich einem Dichter,", "tokens": ["Bis", "ich", "gar", ",", "gleich", "ei\u00b7nem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "             Selber mit im Tiktak sprach.", "tokens": ["Sel\u00b7ber", "mit", "im", "Tik\u00b7tak", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "             Wie mir so im Versemachen", "tokens": ["Wie", "mir", "so", "im", "Ver\u00b7se\u00b7ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "         Silb\u2019 um Silb\u2019 ihr Hopsa sprang,", "tokens": ["Silb'", "um", "Silb'", "ihr", "Hop\u00b7sa", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "         Musst ich pl\u00f6tzlich lachen, lachen", "tokens": ["Musst", "ich", "pl\u00f6tz\u00b7lich", "la\u00b7chen", ",", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Eine Viertelstunde lang,", "tokens": ["Ei\u00b7ne", "Vier\u00b7tel\u00b7stun\u00b7de", "lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "             Du ein Dichter? Du sein Dichter?", "tokens": ["Du", "ein", "Dich\u00b7ter", "?", "Du", "sein", "Dich\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": " stehts mit deinem Kopf so schlecht? \u2014", "tokens": ["stehts", "mit", "dei\u00b7nem", "Kopf", "so", "schlecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "         \u00abJa, mein Herr! Sie sind ein Dichter!\u00bb", "tokens": ["\u00ab", "Ja", ",", "mein", "Herr", "!", "Sie", "sind", "ein", "Dich\u00b7ter", "!", "\u00bb"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "         \u2014 Also sprach der Vogel Specht.", "tokens": ["Al\u00b7so", "sprach", "der", "Vo\u00b7gel", "Specht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}