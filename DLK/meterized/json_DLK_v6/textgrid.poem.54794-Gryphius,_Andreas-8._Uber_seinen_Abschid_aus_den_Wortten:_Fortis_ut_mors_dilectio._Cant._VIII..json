{"textgrid.poem.54794": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "8. Uber seinen Abschid aus den Wortten: Fortis ut mors dilectio. Cant. VIII.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Libe ruffte dir/ es ruffte dir der Tod/", "tokens": ["Die", "Li\u00b7be", "ruff\u00b7te", "dir", "/", "es", "ruff\u00b7te", "dir", "der", "Tod", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie/ in der Himmel Burg; Er/ aus der Erden Noht/", "tokens": ["Sie", "/", "in", "der", "Him\u00b7mel", "Burg", ";", "Er", "/", "aus", "der", "Er\u00b7den", "Noht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "ART", "NN", "NN", "$.", "PPER", "$(", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod li\u00df seine Pfeil/ die Lib auch ihre sehen;", "tokens": ["Der", "Tod", "li\u00df", "sei\u00b7ne", "Pfeil", "/", "die", "Lib", "auch", "ih\u00b7re", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Libe Schlu\u00df geht fort; Des Todes/ mu\u00df geschehen.", "tokens": ["Der", "Li\u00b7be", "Schlu\u00df", "geht", "fort", ";", "Des", "To\u00b7des", "/", "mu\u00df", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Libe nam dein Hertz/ der Todt die Glieder ein/", "tokens": ["Die", "Li\u00b7be", "nam", "dein", "Hertz", "/", "der", "Todt", "die", "Glie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er d\u00e4mpfte deine Krafft/ sie/ deiner Schmertzen Pein/", "tokens": ["Er", "d\u00e4mpf\u00b7te", "dei\u00b7ne", "Krafft", "/", "sie", "/", "dei\u00b7ner", "Schmert\u00b7zen", "Pein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie/ wolte deine Seel'/ Er/ die entseelte Leichen/", "tokens": ["Sie", "/", "wol\u00b7te", "dei\u00b7ne", "Seel'", "/", "Er", "/", "die", "ent\u00b7seel\u00b7te", "Lei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VMFIN", "PPOSAT", "NN", "$(", "PPER", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Libe wolte nicht dem Tod/ er ihr nicht weichen.", "tokens": ["Die", "Li\u00b7be", "wol\u00b7te", "nicht", "dem", "Tod", "/", "er", "ihr", "nicht", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ART", "NN", "$(", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bi\u00df GOTT (der beider Pfeill auch als er starb empfand)", "tokens": ["Bi\u00df", "GoTT", "(", "der", "bei\u00b7der", "Pfeill", "auch", "als", "er", "starb", "emp\u00b7fand", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "ART", "PIAT", "NN", "ADV", "KOUS", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Selbst in das Mittel tratt und deine Seel entband/", "tokens": ["Selbst", "in", "das", "Mit\u00b7tel", "tratt", "und", "dei\u00b7ne", "Seel", "ent\u00b7band", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da wi\u00df die Libe/ Gott den Geist zum Siges Zeichen:", "tokens": ["Da", "wi\u00df", "die", "Li\u00b7be", "/", "Gott", "den", "Geist", "zum", "Si\u00b7ges", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und uns der strenge Tod zum Schawspill seiner Leichen.", "tokens": ["Und", "uns", "der", "stren\u00b7ge", "Tod", "zum", "Schaw\u00b7spill", "sei\u00b7ner", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}