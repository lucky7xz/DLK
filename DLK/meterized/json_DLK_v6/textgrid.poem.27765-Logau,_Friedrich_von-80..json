{"textgrid.poem.27765": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fcck ist abgetheilt vons Ungel\u00fcckes T\u00fccke", "tokens": ["Das", "Gl\u00fcck", "ist", "ab\u00b7ge\u00b7theilt", "vons", "Un\u00b7ge\u00b7l\u00fc\u00b7ckes", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den Buchstaben U und den Buchstaben N;", "tokens": ["Durch", "den", "Buch\u00b7sta\u00b7ben", "U", "und", "den", "Buch\u00b7sta\u00b7ben", "N", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "XY", "KON", "ART", "NN", "XY", "$."], "meter": "+-++-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "So da\u00df man N vor und U zuletzte nenne,", "tokens": ["So", "da\u00df", "man", "N", "vor", "und", "U", "zu\u00b7letz\u00b7te", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "XY", "PTKVZ", "KON", "XY", "VVFIN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So ist in einem NU Ungl\u00fccke bei dem Gl\u00fccke.", "tokens": ["So", "ist", "in", "ei\u00b7nem", "Nu", "Un\u00b7gl\u00fc\u00b7cke", "bei", "dem", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}