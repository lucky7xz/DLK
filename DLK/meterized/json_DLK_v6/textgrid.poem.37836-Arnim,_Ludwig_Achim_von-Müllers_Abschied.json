{"textgrid.poem.37836": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcllers Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge,", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da steht ein goldnes Haus,", "tokens": ["Da", "steht", "ein", "gold\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schauen wohl alle Fr\u00fchmorgen", "tokens": ["Da", "schau\u00b7en", "wohl", "al\u00b7le", "Fr\u00fch\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Drey sch\u00f6ne Jungfrauen heraus;", "tokens": ["Drey", "sch\u00f6\u00b7ne", "Jung\u00b7frau\u00b7en", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die eine, die hei\u00dft Elisabeth.", "tokens": ["Die", "ei\u00b7ne", ",", "die", "hei\u00dft", "E\u00b7lis\u00b7a\u00b7beth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PRELS", "VVFIN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die andre Bernharda mein,", "tokens": ["Die", "and\u00b7re", "Bern\u00b7har\u00b7da", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die dritte, die will ich nicht nennen,", "tokens": ["Die", "drit\u00b7te", ",", "die", "will", "ich", "nicht", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die sollt mein eigen seyn.", "tokens": ["Die", "sollt", "mein", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da unten in jenem Thale,", "tokens": ["Da", "un\u00b7ten", "in", "je\u00b7nem", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da treibt das Wasser ein Rad,", "tokens": ["Da", "treibt", "das", "Was\u00b7ser", "ein", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das treibet nichts als Liebe,", "tokens": ["Das", "trei\u00b7bet", "nichts", "als", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Abend bis wieder an Tag;", "tokens": ["Vom", "A\u00b7bend", "bis", "wie\u00b7der", "an", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das Rad das ist gebrochen,", "tokens": ["Das", "Rad", "das", "ist", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Liebe, die hat ein End,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "hat", "ein", "End", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und wenn zwey Liebende scheiden,", "tokens": ["Und", "wenn", "zwey", "Lie\u00b7ben\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sie reichen einander die H\u00e4nd.", "tokens": ["Sie", "rei\u00b7chen", "ein\u00b7an\u00b7der", "die", "H\u00e4nd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ach Scheiden, ach, ach!", "tokens": ["Ach", "Schei\u00b7den", ",", "ach", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wer hat doch das Scheiden erdacht,", "tokens": ["Wer", "hat", "doch", "das", "Schei\u00b7den", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das hat mein jung frisch Herzelein", "tokens": ["Das", "hat", "mein", "jung", "frisch", "Her\u00b7ze\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fr\u00fchzeitig traurig gemacht.", "tokens": ["So", "fr\u00fch\u00b7zei\u00b7tig", "trau\u00b7rig", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dies Liedlein, ach, ach!", "tokens": ["Dies", "Lied\u00b7lein", ",", "ach", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Hat wohl ein M\u00fcller erdacht;", "tokens": ["Hat", "wohl", "ein", "M\u00fcl\u00b7ler", "er\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NE", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Den hat des Ritters T\u00f6chterlein", "tokens": ["Den", "hat", "des", "Rit\u00b7ters", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Lieben zum Scheiden gebracht.", "tokens": ["Vom", "Lie\u00b7ben", "zum", "Schei\u00b7den", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}