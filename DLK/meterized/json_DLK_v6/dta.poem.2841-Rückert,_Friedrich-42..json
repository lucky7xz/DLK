{"dta.poem.2841": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel wichtiger als was du hast gelernt, mein Sohn,", "tokens": ["Viel", "wich\u00b7ti\u00b7ger", "als", "was", "du", "hast", "ge\u00b7lernt", ",", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PWS", "PPER", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist was du hast gethan, und mehr hast du davon.", "tokens": ["Ist", "was", "du", "hast", "ge\u00b7than", ",", "und", "mehr", "hast", "du", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWS", "PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du gelernet, mu\u00dft du f\u00fcrchten zu vergessen;", "tokens": ["Was", "du", "ge\u00b7ler\u00b7net", ",", "mu\u00dft", "du", "f\u00fcrch\u00b7ten", "zu", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du gethan, von selbst erinnerst du dich dessen.", "tokens": ["Was", "du", "ge\u00b7than", ",", "von", "selbst", "e\u00b7rin\u00b7nerst", "du", "dich", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "APPR", "ADV", "VVFIN", "PPER", "PRF", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es mag dich nun erfreun, es mag dich nun gereun,", "tokens": ["Es", "mag", "dich", "nun", "er\u00b7freun", ",", "es", "mag", "dich", "nun", "ge\u00b7reun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von selber wird sich die Erinnrung dir erneun.", "tokens": ["Von", "sel\u00b7ber", "wird", "sich", "die", "E\u00b7rinn\u00b7rung", "dir", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PRF", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Einmal geschrieben, ists nicht wieder auszustreichen,", "tokens": ["Ein\u00b7mal", "ge\u00b7schrie\u00b7ben", ",", "ists", "nicht", "wie\u00b7der", "aus\u00b7zu\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VAFIN", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und in des Lebens Buch steht es als ewiges Zeichen.", "tokens": ["Und", "in", "des", "Le\u00b7bens", "Buch", "steht", "es", "als", "e\u00b7wi\u00b7ges", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum was du schreibest, denk, ob du es immer sehn", "tokens": ["Drum", "was", "du", "schrei\u00b7best", ",", "denk", ",", "ob", "du", "es", "im\u00b7mer", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor Augen m\u00f6chtest, nie es w\u00fcnschen ungeschehn.", "tokens": ["Vor", "Au\u00b7gen", "m\u00f6ch\u00b7test", ",", "nie", "es", "w\u00fcn\u00b7schen", "un\u00b7ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "$,", "ADV", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Einmal geschrieben, ists nicht wieder umzuschreiben;", "tokens": ["Ein\u00b7mal", "ge\u00b7schrie\u00b7ben", ",", "ists", "nicht", "wie\u00b7der", "um\u00b7zu\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VAFIN", "PTKNEG", "ADV", "VVIZU", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und streichest du's auch aus, so wird der Strich doch bleiben.", "tokens": ["Und", "strei\u00b7chest", "du's", "auch", "aus", ",", "so", "wird", "der", "Strich", "doch", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und kratzest du es aus, so bleibet doch der Kratz,", "tokens": ["Und", "krat\u00b7zest", "du", "es", "aus", ",", "so", "blei\u00b7bet", "doch", "der", "Kratz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Neues l\u00e4\u00dft sich nie rein schreiben an dem Platz.", "tokens": ["Und", "Neu\u00b7es", "l\u00e4\u00dft", "sich", "nie", "rein", "schrei\u00b7ben", "an", "dem", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}