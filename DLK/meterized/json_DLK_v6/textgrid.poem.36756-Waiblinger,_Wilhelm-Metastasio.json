{"textgrid.poem.36756": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Metastasio", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich bist du, ich lese dich gern, ich h\u00f6re dich lieber,", "tokens": ["Lieb\u00b7lich", "bist", "du", ",", "ich", "le\u00b7se", "dich", "gern", ",", "ich", "h\u00f6\u00b7re", "dich", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn dich ein r\u00f6mischer Mund, wenn der Gesang dich beseelt.", "tokens": ["Wenn", "dich", "ein", "r\u00f6\u00b7mi\u00b7scher", "Mund", ",", "wenn", "der", "Ge\u00b7sang", "dich", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dir fehlt's nicht an treuer Natur und artiger Einfalt,", "tokens": ["Dir", "fehlt's", "nicht", "an", "treu\u00b7er", "Na\u00b7tur", "und", "ar\u00b7ti\u00b7ger", "Ein\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Immer das N\u00e4chste nur bringst du verst\u00e4ndlich mir vor.", "tokens": ["Im\u00b7mer", "das", "N\u00e4chs\u00b7te", "nur", "bringst", "du", "ver\u00b7st\u00e4nd\u00b7lich", "mir", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Deine Sprach' ist entz\u00fcckend, ich lausche dem z\u00e4rtlichen Dichter,", "tokens": ["Dei\u00b7ne", "Sprach'", "ist", "ent\u00b7z\u00fc\u00b7ckend", ",", "ich", "lau\u00b7sche", "dem", "z\u00e4rt\u00b7li\u00b7chen", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber spr\u00e4chest du deutsch, f\u00e4nd' ich den Dichter nicht mehr.", "tokens": ["A\u00b7ber", "spr\u00e4\u00b7chest", "du", "deutsch", ",", "f\u00e4nd'", "ich", "den", "Dich\u00b7ter", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}