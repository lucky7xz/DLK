{"dta.poem.10379": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Lehn' deine Wang' an meine Wang',", "tokens": ["Lehn'", "dei\u00b7ne", "Wang'", "an", "mei\u00b7ne", "Wang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann flie\u00dfen die Thr\u00e4nen zusammen;", "tokens": ["Dann", "flie\u00b7\u00dfen", "die", "Thr\u00e4\u00b7nen", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und an mein Herz dr\u00fcck' fest dein Herz,", "tokens": ["Und", "an", "mein", "Herz", "dr\u00fcck", "fest", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann schlagen zusammen die Flammen!", "tokens": ["Dann", "schla\u00b7gen", "zu\u00b7sam\u00b7men", "die", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn in die gro\u00dfe Flamme flie\u00dft", "tokens": ["Und", "wenn", "in", "die", "gro\u00b7\u00dfe", "Flam\u00b7me", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Strom von unsern Thr\u00e4nen,", "tokens": ["Der", "Strom", "von", "un\u00b7sern", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn dich mein Arm gewaltig umschlie\u00dft \u2014", "tokens": ["Und", "wenn", "dich", "mein", "Arm", "ge\u00b7wal\u00b7tig", "um\u00b7schlie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sterb' ich vor Liebessehnen!", "tokens": ["Sterb'", "ich", "vor", "Lie\u00b7bes\u00b7seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}