{"dta.poem.2969": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch mir will oft das Haupt der Greisenwahn umd\u00fcstern,", "tokens": ["Auch", "mir", "will", "oft", "das", "Haupt", "der", "Grei\u00b7sen\u00b7wahn", "um\u00b7d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von alter be\u00dfrer Zeit und neuer schlechtren fl\u00fcstern.", "tokens": ["Von", "al\u00b7ter", "be\u00df\u00b7rer", "Zeit", "und", "neu\u00b7er", "schlecht\u00b7ren", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "KON", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch gleich danieder schl\u00e4gt den Wahn, und die Verachtung", "tokens": ["Doch", "gleich", "da\u00b7nie\u00b7der", "schl\u00e4gt", "den", "Wahn", ",", "und", "die", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gegenwart zerstreut die doppelte Betrachtung:", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", "zer\u00b7streut", "die", "dop\u00b7pel\u00b7te", "Be\u00b7trach\u00b7tung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich doch schlechter nicht geworden, als gewesen,", "tokens": ["Da\u00df", "ich", "doch", "schlech\u00b7ter", "nicht", "ge\u00b7wor\u00b7den", ",", "als", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "VAPP", "$,", "KOUS", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja besser als es war zu hoffen, bin genesen;", "tokens": ["Ja", "bes\u00b7ser", "als", "es", "war", "zu", "hof\u00b7fen", ",", "bin", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KOKOM", "PPER", "VAFIN", "PTKZU", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und da\u00df nun andre nicht sind schlechter als ich war,", "tokens": ["Und", "da\u00df", "nun", "and\u00b7re", "nicht", "sind", "schlech\u00b7ter", "als", "ich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "PTKNEG", "VAFIN", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00f6nnen darum noch viel besser werden gar.", "tokens": ["Und", "k\u00f6n\u00b7nen", "da\u00b7rum", "noch", "viel", "bes\u00b7ser", "wer\u00b7den", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ADV", "ADV", "ADJD", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}