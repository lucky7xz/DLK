{"textgrid.poem.49282": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Klagelied", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hat das sch\u00f6ne Gold so h\u00e4\u00dflich k\u00f6nnen werden?", "tokens": ["Wie", "hat", "das", "sch\u00f6\u00b7ne", "Gold", "so", "h\u00e4\u00df\u00b7lich", "k\u00f6n\u00b7nen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie tunckel sieht es au\u00df? Wie ligt doch auff der Erden", "tokens": ["Wie", "tun\u00b7ckel", "sieht", "es", "au\u00df", "?", "Wie", "ligt", "doch", "auff", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Bau de\u00df Heyligthumbs? Wie sind doch weit und breit", "tokens": ["Der", "Bau", "de\u00df", "Hey\u00b7lig\u00b7thumbs", "?", "Wie", "sind", "doch", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PWAV", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Marmor de\u00df Pallasts zerbrochen umbgestreut?", "tokens": ["Die", "Mar\u00b7mor", "de\u00df", "Pal\u00b7lasts", "zer\u00b7bro\u00b7chen", "umb\u00b7ge\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie k\u00f6mpt es immermehr, da\u00df Zions edle Kinder", "tokens": ["Wie", "k\u00f6mpt", "es", "im\u00b7mer\u00b7mehr", ",", "da\u00df", "Zi\u00b7ons", "ed\u00b7le", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als irrdne T\u00f6pffe sind, die jederman nicht minder", "tokens": ["Als", "irrd\u00b7ne", "T\u00f6pf\u00b7fe", "sind", ",", "die", "je\u00b7der\u00b7man", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PIS", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann Gold zuvor gesch\u00e4tzt? Jetzt sind sie Koth und Wust.", "tokens": ["Dann", "Gold", "zu\u00b7vor", "ge\u00b7sch\u00e4tzt", "?", "Jetzt", "sind", "sie", "Koth", "und", "Wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVPP", "$.", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Drachen reichen ja den Jungen noch die Brust,", "tokens": ["Die", "Dra\u00b7chen", "rei\u00b7chen", "ja", "den", "Jun\u00b7gen", "noch", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie grausam sie auch sind, und geben ihren Fr\u00fcchten", "tokens": ["Wie", "grau\u00b7sam", "sie", "auch", "sind", ",", "und", "ge\u00b7ben", "ih\u00b7ren", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die gelbicht weisse Milch mit treuen Mutterpflichten,", "tokens": ["Die", "gel\u00b7bicht", "weis\u00b7se", "Milch", "mit", "treu\u00b7en", "Mut\u00b7ter\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bi\u00df da\u00df sie selbst hernach sich richten auffs Gebein;", "tokens": ["Bi\u00df", "da\u00df", "sie", "selbst", "her\u00b7nach", "sich", "rich\u00b7ten", "auffs", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Tochter meines Volcks mu\u00df unbarmhertzig sein,", "tokens": ["Die", "Toch\u00b7ter", "mei\u00b7nes", "Volcks", "mu\u00df", "un\u00b7barm\u00b7hert\u00b7zig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mu\u00df h\u00e4rten ihren Sinn, als wie in stiller W\u00fcsten", "tokens": ["Mu\u00df", "h\u00e4r\u00b7ten", "ih\u00b7ren", "Sinn", ",", "als", "wie", "in", "stil\u00b7ler", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der unbedachte Strau\u00df pflegt in den Sand zu nisten,", "tokens": ["Der", "un\u00b7be\u00b7dach\u00b7te", "Strau\u00df", "pflegt", "in", "den", "Sand", "zu", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "L\u00e4st aber unbesorgt die waisen Eyer stehn,", "tokens": ["L\u00e4st", "a\u00b7ber", "un\u00b7be\u00b7sorgt", "die", "wai\u00b7sen", "Ey\u00b7er", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es mag auch ihnen gleich gut oder \u00fcbel gehn.", "tokens": ["Es", "mag", "auch", "ih\u00b7nen", "gleich", "gut", "o\u00b7der", "\u00fc\u00b7bel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Seugling stirbet schon und f\u00e4ngt kaum an zu leben,", "tokens": ["Der", "Seug\u00b7ling", "stir\u00b7bet", "schon", "und", "f\u00e4ngt", "kaum", "an", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Zunge bleibt ihm f\u00fcr Durst am Gaumen kleben,", "tokens": ["Die", "Zun\u00b7ge", "bleibt", "ihm", "f\u00fcr", "Durst", "am", "Gau\u00b7men", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die Kinder heischen Brod und niemand bricht es nicht.", "tokens": ["Die", "Kin\u00b7der", "hei\u00b7schen", "Brod", "und", "nie\u00b7mand", "bricht", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den vormahls Spei\u00df unnd Tranck, so weit der Sonnen Liecht", "tokens": ["Den", "vor\u00b7mahls", "Spei\u00df", "unnd", "Tranck", ",", "so", "weit", "der", "Son\u00b7nen", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "ADV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die grosse Rundt bestralt, zu ihrem reichen Tische", "tokens": ["Die", "gros\u00b7se", "Rundt", "be\u00b7stralt", ",", "zu", "ih\u00b7rem", "rei\u00b7chen", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ward mit Gefahr gesucht, den kaum genungsamb Fische,", "tokens": ["Ward", "mit", "Ge\u00b7fahr", "ge\u00b7sucht", ",", "den", "kaum", "ge\u00b7nun\u00b7gsamb", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,", "PRELS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die grosse Mittelsee und Nilus hat geschickt,", "tokens": ["Die", "gros\u00b7se", "Mit\u00b7tel\u00b7see", "und", "Ni\u00b7lus", "hat", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die gehn jetzt schm\u00e4chtig her; den Purpur ward gestickt", "tokens": ["Die", "gehn", "jetzt", "schm\u00e4ch\u00b7tig", "her", ";", "den", "Pur\u00b7pur", "ward", "ge\u00b7stickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zu Tyrus und Sidon am Palestiner Strande,", "tokens": ["Zu", "Ty\u00b7rus", "und", "Si\u00b7don", "am", "Pa\u00b7les\u00b7ti\u00b7ner", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "APPRART", "NN", "NN", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Die liegen jetzt im Koth und sind ihr' eigne Schande.", "tokens": ["Die", "lie\u00b7gen", "jetzt", "im", "Koth", "und", "sind", "ih\u00b7r'", "eig\u00b7ne", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "KON", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ich wei\u00df, da\u00df sie mehr Schuld auff ihren Halse hat,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "sie", "mehr", "Schuld", "auff", "ih\u00b7ren", "Hal\u00b7se", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Tochter meines Volcks, und gr\u00f6sser Uebelthat", "tokens": ["Die", "Toch\u00b7ter", "mei\u00b7nes", "Volcks", ",", "und", "gr\u00f6s\u00b7ser", "Ue\u00b7belt\u00b7hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Als Sodom, welche doch vom Himmel ward verzehret", "tokens": ["Als", "So\u00b7dom", ",", "wel\u00b7che", "doch", "vom", "Him\u00b7mel", "ward", "ver\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "PRELS", "ADV", "APPRART", "NN", "VAFIN", "VVFIN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Durch strenge Regenglut und pl\u00f6tzlich umbgekehret,", "tokens": ["Durch", "stren\u00b7ge", "Re\u00b7gen\u00b7glut", "und", "pl\u00f6tz\u00b7lich", "umb\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo Asphalites noch an seinen todten Randt", "tokens": ["Wo", "As\u00b7pha\u00b7li\u00b7tes", "noch", "an", "sei\u00b7nen", "tod\u00b7ten", "Randt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das schwartze Hartz au\u00dfwirfft unnd zeigt auff diesen Brandt.", "tokens": ["Das", "schwart\u00b7ze", "Hartz", "au\u00df\u00b7wirfft", "unnd", "zeigt", "auff", "die\u00b7sen", "Brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ihr heil'ges Priestervolck, die keuschen Nazarener,", "tokens": ["Ihr", "heil'\u00b7ges", "Pries\u00b7ter\u00b7volck", ",", "die", "keu\u00b7schen", "Na\u00b7za\u00b7re\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die waren rein' als Schnee, viel kl\u00e4rer noch und sch\u00f6ner,", "tokens": ["Die", "wa\u00b7ren", "rein'", "als", "Schnee", ",", "viel", "kl\u00e4\u00b7rer", "noch", "und", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KOKOM", "NN", "$,", "ADV", "ADJD", "ADV", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als eine weisse Milch, und ihre grosse Zier", "tokens": ["Als", "ei\u00b7ne", "weis\u00b7se", "Milch", ",", "und", "ih\u00b7re", "gros\u00b7se", "Zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "War r\u00f6ter, als Corall, ihr Ansehn wie Saffir.", "tokens": ["War", "r\u00f6\u00b7ter", ",", "als", "Co\u00b7rall", ",", "ihr", "An\u00b7sehn", "wie", "Saf\u00b7fir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "NN", "$,", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.37": {"text": "Nun sind sie schwartz wie die, so stets der Mittag brennet,", "tokens": ["Nun", "sind", "sie", "schwartz", "wie", "die", ",", "so", "stets", "der", "Mit\u00b7tag", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und sehn vertunckelt au\u00df, da\u00df niemand sie mehr kennet,", "tokens": ["Und", "sehn", "ver\u00b7tun\u00b7ckelt", "au\u00df", ",", "da\u00df", "nie\u00b7mand", "sie", "mehr", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Haut hengt am Gebein' und ist nun gar zu weit,", "tokens": ["Die", "Haut", "hengt", "am", "Ge\u00b7bein'", "und", "ist", "nun", "gar", "zu", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der gantze C\u00f6rper ist noch d\u00fcrrer als ein Scheidt,", "tokens": ["Der", "gant\u00b7ze", "C\u00f6r\u00b7per", "ist", "noch", "d\u00fcr\u00b7rer", "als", "ein", "Scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das an der Sonnen liegt. Die durch das Schwerdt verdorben,", "tokens": ["Das", "an", "der", "Son\u00b7nen", "liegt", ".", "Die", "durch", "das", "Schwerdt", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die kamen besser umb als die, so endlich sturben", "tokens": ["Die", "ka\u00b7men", "bes\u00b7ser", "umb", "als", "die", ",", "so", "end\u00b7lich", "stur\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "KOUS", "ART", "$,", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Au\u00df Durst' und Hungerspein unnd worden erst hernach", "tokens": ["Au\u00df", "Dur\u00b7st'", "und", "Hun\u00b7ger\u00b7spein", "unnd", "wor\u00b7den", "erst", "her\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VAPP", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.44": {"text": "Vom Feinde hingew\u00fcrgt, weil Frucht und Korn gebrach.", "tokens": ["Vom", "Fein\u00b7de", "hin\u00b7ge\u00b7w\u00fcrgt", ",", "weil", "Frucht", "und", "Korn", "ge\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die M\u00fctter, derer Hertz' au\u00df Liebe sonst zerbrochen,", "tokens": ["Die", "M\u00fct\u00b7ter", ",", "de\u00b7rer", "Hertz'", "au\u00df", "Lie\u00b7be", "sonst", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die musten (ach der Angst) ihr' eigne Kinder kochen", "tokens": ["Die", "mus\u00b7ten", "(", "ach", "der", "Angst", ")", "ih\u00b7r'", "eig\u00b7ne", "Kin\u00b7der", "ko\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "$(", "ADV", "ART", "NN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.47": {"text": "Zu stillen ihren Bauch in solcher grossen Noth", "tokens": ["Zu", "stil\u00b7len", "ih\u00b7ren", "Bauch", "in", "sol\u00b7cher", "gros\u00b7sen", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der Tochter meines Volcks. Der Herr, der starcke Gott,", "tokens": ["Der", "Toch\u00b7ter", "mei\u00b7nes", "Volcks", ".", "Der", "Herr", ",", "der", "star\u00b7cke", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hat seinen Grimm vollbracht, hat gantz und gar zusammen", "tokens": ["Hat", "sei\u00b7nen", "Grimm", "voll\u00b7bracht", ",", "hat", "gantz", "und", "gar", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NE", "VVPP", "$,", "VAFIN", "ADV", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Gesch\u00fct