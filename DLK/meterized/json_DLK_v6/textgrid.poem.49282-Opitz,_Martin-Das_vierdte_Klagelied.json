ir", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Es wird nun deine Schuld entdecken, als wie mir.", "tokens": ["Es", "wird", "nun", "dei\u00b7ne", "Schuld", "ent\u00b7de\u00b7cken", ",", "als", "wie", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}