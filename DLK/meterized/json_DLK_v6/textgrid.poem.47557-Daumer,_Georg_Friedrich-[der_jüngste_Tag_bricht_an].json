{"textgrid.poem.47557": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der j\u00fcngste Tag bricht an]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der j\u00fcngste Tag bricht an,", "tokens": ["Der", "j\u00fcngs\u00b7te", "Tag", "bricht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der in ein ernst Gerichte", "tokens": ["Der", "in", "ein", "ernst", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit s\u00e4mmtlichen Geschlechten", "tokens": ["Mit", "s\u00e4mmt\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf dieser Erde geht.", "tokens": ["Auf", "die\u00b7ser", "Er\u00b7de", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf hohem Stuhle sitzet", "tokens": ["Auf", "ho\u00b7hem", "Stuh\u00b7le", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hafis, der Weinprophet.", "tokens": ["Ha\u00b7fis", ",", "der", "Wein\u00b7pro\u00b7phet", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wie wird er richten? H\u00f6ret!", "tokens": ["Wie", "wird", "er", "rich\u00b7ten", "?", "H\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbihr Trunkenen, ihr hoch mir", "tokens": ["\u00bb", "ihr", "Trun\u00b7ke\u00b7nen", ",", "ihr", "hoch", "mir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "ADJD", "PPER"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gesegneten zur Rechten!", "tokens": ["Ge\u00b7se\u00b7gne\u00b7ten", "zur", "Rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ihr N\u00fcchternen, ihr Schlechten,", "tokens": ["Ihr", "N\u00fcch\u00b7ter\u00b7nen", ",", "ihr", "Schlech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu meiner Linken steht!\u00ab", "tokens": ["Zu", "mei\u00b7ner", "Lin\u00b7ken", "steht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}