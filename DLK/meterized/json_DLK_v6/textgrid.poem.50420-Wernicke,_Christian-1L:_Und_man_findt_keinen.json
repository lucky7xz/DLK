{"textgrid.poem.50420": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und man findt keinen ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und man findt keinen ", "tokens": ["Und", "man", "findt", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den seine ", "tokens": ["Den", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der erste urtheilt zu geschwind',", "tokens": ["Der", "ers\u00b7te", "ur\u00b7theilt", "zu", "ge\u00b7schwind'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fehlt aus ", "tokens": ["Und", "fehlt", "aus"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der andre ", "tokens": ["Der", "and\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und scheut sich vor der Ruht.", "tokens": ["Und", "scheut", "sich", "vor", "der", "Ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sey drum ", "tokens": ["Sey", "drum"], "token_info": ["word", "word"], "pos": ["VAFIN", "PAV"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Halt' auf dich nicht zu viel;", "tokens": ["Halt'", "auf", "dich", "nicht", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und mach' aus einer ", "tokens": ["Und", "mach'", "aus", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Es zeigt ein tugendhaffter Mann", "tokens": ["Es", "zeigt", "ein", "tu\u00b7gend\u00b7haff\u00b7ter", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Stets, ", "tokens": ["Stets", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}}}}}