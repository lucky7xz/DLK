{"textgrid.poem.56166": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon, horch, h\u00f6rst du der ersten Harken", "tokens": ["Schon", ",", "horch", ",", "h\u00f6rst", "du", "der", "ers\u00b7ten", "Har\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVIMP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeit; wieder den menschlichen Takt", "tokens": ["Ar\u00b7beit", ";", "wie\u00b7der", "den", "menschli\u00b7chen", "Takt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "in der verhaltenen Stille der starken", "tokens": ["in", "der", "ver\u00b7hal\u00b7te\u00b7nen", "Stil\u00b7le", "der", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vorfr\u00fchlingserde. Unabgeschmackt", "tokens": ["Vor\u00b7fr\u00fch\u00b7lings\u00b7er\u00b7de", ".", "Un\u00b7ab\u00b7ge\u00b7schmackt"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "scheint dir das Kommende. Jenes so oft", "tokens": ["scheint", "dir", "das", "Kom\u00b7men\u00b7de", ".", "Je\u00b7nes", "so", "oft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PDS", "ADV", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "dir schon Gekommene scheint dir zu kommen", "tokens": ["dir", "schon", "Ge\u00b7kom\u00b7me\u00b7ne", "scheint", "dir", "zu", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wieder wie Neues. Immer erhofft,", "tokens": ["wie\u00b7der", "wie", "Neu\u00b7es", ".", "Im\u00b7mer", "er\u00b7hofft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "nahmst du es niemals. Es hat dich genommen.", "tokens": ["nahmst", "du", "es", "nie\u00b7mals", ".", "Es", "hat", "dich", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Selbst die Bl\u00e4tter durchwinterter Eichen", "tokens": ["Selbst", "die", "Bl\u00e4t\u00b7ter", "durch\u00b7win\u00b7ter\u00b7ter", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "scheinen im Abend ein k\u00fcnftiges Braun.", "tokens": ["schei\u00b7nen", "im", "A\u00b7bend", "ein", "k\u00fcnf\u00b7ti\u00b7ges", "Braun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Manchmal geben sich L\u00fcfte ein Zeichen.", "tokens": ["Manch\u00b7mal", "ge\u00b7ben", "sich", "L\u00fcf\u00b7te", "ein", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Schwarz sind die Str\u00e4ucher. Doch Haufen von D\u00fcnger", "tokens": ["Schwarz", "sind", "die", "Str\u00e4u\u00b7cher", ".", "Doch", "Hau\u00b7fen", "von", "D\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "KON", "NN", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "lagern als satteres Schwarz in den Aun.", "tokens": ["la\u00b7gern", "als", "sat\u00b7te\u00b7res", "Schwarz", "in", "den", "Aun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Jede Stunde, die hingeht, wird j\u00fcnger.", "tokens": ["Je\u00b7de", "Stun\u00b7de", ",", "die", "hin\u00b7geht", ",", "wird", "j\u00fcn\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}