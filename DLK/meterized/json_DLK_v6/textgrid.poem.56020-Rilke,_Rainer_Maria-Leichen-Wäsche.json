{"textgrid.poem.56020": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Leichen-W\u00e4sche", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hatten sich an ihn gew\u00f6hnt. Doch als", "tokens": ["Sie", "hat\u00b7ten", "sich", "an", "ihn", "ge\u00b7w\u00f6hnt", ".", "Doch", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPER", "VVPP", "$.", "KON", "KOUS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "die K\u00fcchenlampe kam und unruhig brannte", "tokens": ["die", "K\u00fc\u00b7chen\u00b7lam\u00b7pe", "kam", "und", "un\u00b7ru\u00b7hig", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "im dunkeln Luftzug, war der Unbekannte", "tokens": ["im", "dun\u00b7keln", "Luft\u00b7zug", ",", "war", "der", "Un\u00b7be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ganz unbekannt. Sie wuschen seinen Hals,", "tokens": ["ganz", "un\u00b7be\u00b7kannt", ".", "Sie", "wu\u00b7schen", "sei\u00b7nen", "Hals", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und da sie nichts von seinem Schicksal wu\u00dften,", "tokens": ["und", "da", "sie", "nichts", "von", "sei\u00b7nem", "Schick\u00b7sal", "wu\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "so logen sie ein anderes zusamm,", "tokens": ["so", "lo\u00b7gen", "sie", "ein", "an\u00b7de\u00b7res", "zu\u00b7samm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "fortw\u00e4hrend waschend. Eine mu\u00dfte husten", "tokens": ["fort\u00b7w\u00e4h\u00b7rend", "wa\u00b7schend", ".", "Ei\u00b7ne", "mu\u00df\u00b7te", "hus\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und lie\u00df solang den schweren Essigschwamm", "tokens": ["und", "lie\u00df", "so\u00b7lang", "den", "schwe\u00b7ren", "Es\u00b7sig\u00b7schwamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "auf dem Gesicht. Da gab es eine Pause", "tokens": ["auf", "dem", "Ge\u00b7sicht", ".", "Da", "gab", "es", "ei\u00b7ne", "Pau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auch f\u00fcr die zweite. Aus der harten B\u00fcrste", "tokens": ["auch", "f\u00fcr", "die", "zwei\u00b7te", ".", "Aus", "der", "har\u00b7ten", "B\u00fcrs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "klopften die Tropfen; w\u00e4hrend seine grause", "tokens": ["klopf\u00b7ten", "die", "Trop\u00b7fen", ";", "w\u00e4h\u00b7rend", "sei\u00b7ne", "grau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "gekrampfte Hand dem ganzen Hause", "tokens": ["ge\u00b7krampf\u00b7te", "Hand", "dem", "gan\u00b7zen", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "beweisen wollte, da\u00df ihn nicht mehr d\u00fcrste.", "tokens": ["be\u00b7wei\u00b7sen", "woll\u00b7te", ",", "da\u00df", "ihn", "nicht", "mehr", "d\u00fcrs\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und er bewies. Sie nahmen wie betreten", "tokens": ["Und", "er", "be\u00b7wies", ".", "Sie", "nah\u00b7men", "wie", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "KOKOM", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "eiliger jetzt mit einem kurzen Huster", "tokens": ["ei\u00b7li\u00b7ger", "jetzt", "mit", "ei\u00b7nem", "kur\u00b7zen", "Hus\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "die Arbeit auf, so da\u00df an den Tapeten", "tokens": ["die", "Ar\u00b7beit", "auf", ",", "so", "da\u00df", "an", "den", "Ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ihr krummer Schatten in dem stummen Muster", "tokens": ["ihr", "krum\u00b7mer", "Schat\u00b7ten", "in", "dem", "stum\u00b7men", "Mus\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "sich wand und w\u00e4lzte wie in einem Netze,", "tokens": ["sich", "wand", "und", "w\u00e4lz\u00b7te", "wie", "in", "ei\u00b7nem", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bis da\u00df die Waschenden zu Ende kamen.", "tokens": ["bis", "da\u00df", "die", "Wa\u00b7schen\u00b7den", "zu", "En\u00b7de", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Nacht im vorhanglosen Fensterrahmen", "tokens": ["Die", "Nacht", "im", "vor\u00b7hang\u00b7lo\u00b7sen", "Fens\u00b7ter\u00b7rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "war r\u00fccksichtslos. Und einer ohne Namen", "tokens": ["war", "r\u00fcck\u00b7sichts\u00b7los", ".", "Und", "ei\u00b7ner", "oh\u00b7ne", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "KON", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "lag bar und reinlich da und gab Gesetze.", "tokens": ["lag", "bar", "und", "rein\u00b7lich", "da", "und", "gab", "Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ADV", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}