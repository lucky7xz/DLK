{"textgrid.poem.53870": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Finish", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcher, wenn mal etwas Komisches war:", "tokens": ["Fr\u00fc\u00b7her", ",", "wenn", "mal", "et\u00b7was", "Ko\u00b7mi\u00b7sches", "war", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PIAT", "ADJA", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "ein Rednerschwupper an Thron und Altar,", "tokens": ["ein", "Red\u00b7ner\u00b7schwup\u00b7per", "an", "Thron", "und", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "der Kindermund eines Generals,", "tokens": ["der", "Kin\u00b7der\u00b7mund", "ei\u00b7nes", "Ge\u00b7ne\u00b7rals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der Duft eines Reichsgerichtsskandals,", "tokens": ["der", "Duft", "ei\u00b7nes", "Reichs\u00b7ge\u00b7richts\u00b7skan\u00b7dals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Adele Sandrocks herrlicher Ba\u00df,", "tokens": ["A\u00b7de\u00b7le", "San\u00b7drocks", "herr\u00b7li\u00b7cher", "Ba\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "ein d\u00e4mlicher Kabinettserla\u00df;", "tokens": ["ein", "d\u00e4m\u00b7li\u00b7cher", "Ka\u00b7bi\u00b7nett\u00b7ser\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "wenn mit Recht ein Verleger Pleite gemacht,", "tokens": ["wenn", "mit", "Recht", "ein", "Ver\u00b7le\u00b7ger", "Plei\u00b7te", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "wenn ein Tisch sich mit Literaten zerkracht \u2013", "tokens": ["wenn", "ein", "Tisch", "sich", "mit", "Li\u00b7te\u00b7ra\u00b7ten", "zer\u00b7kracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "NN", "VVPP", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "dann tat eine innere Stimme befehlen:", "tokens": ["dann", "tat", "ei\u00b7ne", "in\u00b7ne\u00b7re", "Stim\u00b7me", "be\u00b7feh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Das mu\u00dft du gleich S. J. erz\u00e4hlen!", "tokens": ["Das", "mu\u00dft", "du", "gleich", "S.", "J.", "er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dahin.", "tokens": ["Da\u00b7hin", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Jetzt sitz ich ganz allein.", "tokens": ["Jetzt", "sitz", "ich", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Keinen h\u00f6r ich vor Beifall schrein;", "tokens": ["Kei\u00b7nen", "h\u00f6r", "ich", "vor", "Bei\u00b7fall", "schrein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "h\u00f6r nie mehr das schmetternde Gelach,", "tokens": ["h\u00f6r", "nie", "mehr", "das", "schmet\u00b7tern\u00b7de", "Ge\u00b7lach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "nie mehr die Herzensfreude mit Krach . . .", "tokens": ["nie", "mehr", "die", "Her\u00b7zens\u00b7freu\u00b7de", "mit", "Krach", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Doch dreimal am Tage, wenn was passiert,", "tokens": ["Doch", "drei\u00b7mal", "am", "Ta\u00b7ge", ",", "wenn", "was", "pas\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "wenn die Filmzensur sich selbst parodiert;", "tokens": ["wenn", "die", "Film\u00b7zen\u00b7sur", "sich", "selbst", "pa\u00b7ro\u00b7diert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "wenn Deutschland mit Polen zusammenschliddert,", "tokens": ["wenn", "Deutschland", "mit", "Po\u00b7len", "zu\u00b7sam\u00b7men\u00b7schlid\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "wenn ein Parteivorstand um die \u00c4mter zittert \u2013:", "tokens": ["wenn", "ein", "Par\u00b7tei\u00b7vor\u00b7stand", "um", "die", "\u00c4m\u00b7ter", "zit\u00b7tert", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$(", "$."], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "dann denk ich: Das darf er nicht verfehlen \u2013", "tokens": ["dann", "denk", "ich", ":", "Das", "darf", "er", "nicht", "ver\u00b7feh\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "das mu\u00dft du gleich S. J. erz\u00e4hlen!", "tokens": ["das", "mu\u00dft", "du", "gleich", "S.", "J.", "er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das machen wir noch so drei\u00dfig Jahr.", "tokens": ["Das", "ma\u00b7chen", "wir", "noch", "so", "drei\u00b7\u00dfig", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "CARD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dann ist alles nicht mehr wahr.", "tokens": ["Dann", "ist", "al\u00b7les", "nicht", "mehr", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Dann pflanzen sie uns mit Chor\u00e4len ein,", "tokens": ["Dann", "pflan\u00b7zen", "sie", "uns", "mit", "Cho\u00b7r\u00e4\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "wir liegen still und ziemlich allein . . .", "tokens": ["wir", "lie\u00b7gen", "still", "und", "ziem\u00b7lich", "al\u00b7lein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "und die Seele steigt aus dem engen Verlies", "tokens": ["und", "die", "See\u00b7le", "steigt", "aus", "dem", "en\u00b7gen", "Ver\u00b7lies"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "mit der Pressekarte ins Paradies.", "tokens": ["mit", "der", "Pres\u00b7se\u00b7kar\u00b7te", "ins", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Dann will ich ihn wiedersehn.", "tokens": ["Dann", "will", "ich", "ihn", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Und alles, was bis dahin geschehn:", "tokens": ["Und", "al\u00b7les", ",", "was", "bis", "da\u00b7hin", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "PAV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "deine Arbeit und meine Malheure,", "tokens": ["dei\u00b7ne", "Ar\u00b7beit", "und", "mei\u00b7ne", "Mal\u00b7heu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "den letzten Radau der Regisseure,", "tokens": ["den", "letz\u00b7ten", "Ra\u00b7dau", "der", "Re\u00b7gis\u00b7seu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "eure Treue und unsre M\u00fchn,", "tokens": ["eu\u00b7re", "Treu\u00b7e", "und", "uns\u00b7re", "M\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "und die besten Witze aus ganz Berlin,", "tokens": ["und", "die", "bes\u00b7ten", "Wit\u00b7ze", "aus", "ganz", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADV", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Manna f\u00fcr die unsterblichen Seelen \u2013:", "tokens": ["Man\u00b7na", "f\u00fcr", "die", "uns\u00b7terb\u00b7li\u00b7chen", "See\u00b7len", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Das will ich dann alles S. J. erz\u00e4hlen.", "tokens": ["Das", "will", "ich", "dann", "al\u00b7les", "S.", "J.", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PIAT", "NN", "NE", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}