{"textgrid.poem.58128": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "166. Kinderstand, unbekannt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eigenwillig, gro\u00df und klug,", "tokens": ["Ei\u00b7gen\u00b7wil\u00b7lig", ",", "gro\u00df", "und", "klug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solcher Menschen find't man g'nug;", "tokens": ["Sol\u00b7cher", "Men\u00b7schen", "find't", "man", "g'\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Klein, einf\u00e4ltig, untert\u00e4nig,", "tokens": ["Klein", ",", "ein\u00b7f\u00e4l\u00b7tig", ",", "un\u00b7ter\u00b7t\u00e4\u00b7nig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Kinder sind gar wenig.", "tokens": ["Die\u00b7ser", "Kin\u00b7der", "sind", "gar", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}