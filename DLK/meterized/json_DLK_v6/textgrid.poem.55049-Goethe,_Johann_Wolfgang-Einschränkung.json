{"textgrid.poem.55049": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Einschr\u00e4nkung", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was mir hier gef\u00e4llt,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "mir", "hier", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser engen, kleinen Welt", "tokens": ["In", "die\u00b7ser", "en\u00b7gen", ",", "klei\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit holdem Zauberband mich h\u00e4lt?", "tokens": ["Mit", "hol\u00b7dem", "Zau\u00b7ber\u00b7band", "mich", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verge\u00df ich doch, verge\u00df ich gern,", "tokens": ["Ver\u00b7ge\u00df", "ich", "doch", ",", "ver\u00b7ge\u00df", "ich", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie seltsam mich das Schicksal leitet;", "tokens": ["Wie", "selt\u00b7sam", "mich", "das", "Schick\u00b7sal", "lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ach, ich f\u00fchle, nah und fern", "tokens": ["Und", "ach", ",", "ich", "f\u00fch\u00b7le", ",", "nah", "und", "fern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist mir noch manches zubereitet.", "tokens": ["Ist", "mir", "noch", "man\u00b7ches", "zu\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O w\u00e4re doch das rechte Ma\u00df getroffen!", "tokens": ["O", "w\u00e4\u00b7re", "doch", "das", "rech\u00b7te", "Ma\u00df", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Was bleibt mir nun, als, eingeh\u00fcllt,", "tokens": ["Was", "bleibt", "mir", "nun", ",", "als", ",", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von holder Lebenskraft erf\u00fcllt,", "tokens": ["Von", "hol\u00b7der", "Le\u00b7bens\u00b7kraft", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In stiller Gegenwart die Zukunft zu erhoffen!", "tokens": ["In", "stil\u00b7ler", "Ge\u00b7gen\u00b7wart", "die", "Zu\u00b7kunft", "zu", "er\u00b7hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}