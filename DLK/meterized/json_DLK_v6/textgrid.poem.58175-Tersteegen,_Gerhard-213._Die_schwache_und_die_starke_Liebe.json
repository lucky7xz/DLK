{"textgrid.poem.58175": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "213. Die schwache und die starke Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schwache Liebe klagt, wenn sie den Freund nicht siehet,", "tokens": ["Die", "schwa\u00b7che", "Lie\u00b7be", "klagt", ",", "wenn", "sie", "den", "Freund", "nicht", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wird noch leicht verzagt, dem Leiden sich entziehet;", "tokens": ["Sie", "wird", "noch", "leicht", "ver\u00b7zagt", ",", "dem", "Lei\u00b7den", "sich", "ent\u00b7zie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die starke Liebe sich und ihre Ruh vergi\u00dft,", "tokens": ["Die", "star\u00b7ke", "Lie\u00b7be", "sich", "und", "ih\u00b7re", "Ruh", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist der Geliebte wohl, auch sie zufrieden ist.", "tokens": ["Ist", "der", "Ge\u00b7lieb\u00b7te", "wohl", ",", "auch", "sie", "zu\u00b7frie\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,", "ADV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}