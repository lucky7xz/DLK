{"textgrid.poem.56105": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch vor dem Apostel Thomas, der", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch vor dem Apostel Thomas, der", "tokens": ["Doch", "vor", "dem", "A\u00b7pos\u00b7tel", "Tho\u00b7mas", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "$,", "PRELS"], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "kam, da es zu sp\u00e4t war, trat der schnelle", "tokens": ["kam", ",", "da", "es", "zu", "sp\u00e4t", "war", ",", "trat", "der", "schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "l\u00e4ngst darauf gefa\u00dfte Engel her", "tokens": ["l\u00e4ngst", "da\u00b7rauf", "ge\u00b7fa\u00df\u00b7te", "En\u00b7gel", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "ADJA", "NN", "APZR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und befahl an der Begr\u00e4bnisstelle:", "tokens": ["und", "be\u00b7fahl", "an", "der", "Be\u00b7gr\u00e4b\u00b7nis\u00b7stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dr\u00e4ng den Stein beiseite. Willst du wissen,", "tokens": ["Dr\u00e4ng", "den", "Stein", "bei\u00b7sei\u00b7te", ".", "Willst", "du", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wo die ist, die dir das Herz bewegt:", "tokens": ["wo", "die", "ist", ",", "die", "dir", "das", "Herz", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh: sie ward wie ein Lavendelkissen", "tokens": ["Sieh", ":", "sie", "ward", "wie", "ein", "La\u00b7ven\u00b7del\u00b7kis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "KOKOM", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "eine Weile da hineingelegt,", "tokens": ["ei\u00b7ne", "Wei\u00b7le", "da", "hin\u00b7ein\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "da\u00df die Erde k\u00fcnftig nach ihr rieche", "tokens": ["da\u00df", "die", "Er\u00b7de", "k\u00fcnf\u00b7tig", "nach", "ihr", "rie\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in den Falten wie ein feines Tuch.", "tokens": ["in", "den", "Fal\u00b7ten", "wie", "ein", "fei\u00b7nes", "Tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alles Tote (f\u00fchlst du), alles Sieche", "tokens": ["Al\u00b7les", "To\u00b7te", "(", "f\u00fchlst", "du", ")", ",", "al\u00b7les", "Sie\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PIAT", "NN", "$(", "VVFIN", "PPER", "$(", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ist bet\u00e4ubt von ihrem Wohl-Geruch.", "tokens": ["ist", "be\u00b7t\u00e4ubt", "von", "ih\u00b7rem", "Wohl\u00b7Ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Schau den Leinwand: wo ist eine Bleiche,", "tokens": ["Schau", "den", "Lein\u00b7wand", ":", "wo", "ist", "ei\u00b7ne", "Blei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "wo er blendend wird und geht nicht ein?", "tokens": ["wo", "er", "blen\u00b7dend", "wird", "und", "geht", "nicht", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses Licht aus dieser reinen Leiche", "tokens": ["Die\u00b7ses", "Licht", "aus", "die\u00b7ser", "rei\u00b7nen", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "war ihm kl\u00e4render als Sonnenschein.", "tokens": ["war", "ihm", "kl\u00e4\u00b7ren\u00b7der", "als", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Staunst du nicht, wie sanft sie ihm entging?", "tokens": ["Staunst", "du", "nicht", ",", "wie", "sanft", "sie", "ihm", "ent\u00b7ging", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Fast als w\u00e4r sie's noch, nichts ist verschoben.", "tokens": ["Fast", "als", "w\u00e4r", "sie's", "noch", ",", "nichts", "ist", "ver\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "PPER", "ADV", "$,", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch die Himmel sind ersch\u00fcttert oben:", "tokens": ["Doch", "die", "Him\u00b7mel", "sind", "er\u00b7sch\u00fct\u00b7tert", "o\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mann, knie hin und sieh mir nach und sing.", "tokens": ["Mann", ",", "knie", "hin", "und", "sieh", "mir", "nach", "und", "sing", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "APPR", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}