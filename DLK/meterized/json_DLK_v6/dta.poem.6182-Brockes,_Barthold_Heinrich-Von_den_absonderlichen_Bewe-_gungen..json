{"dta.poem.6182": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diesen fest und unbewegten Grund", "tokens": ["Durch", "die\u00b7sen", "fest", "und", "un\u00b7be\u00b7weg\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entdecket man zu unsrer Zeit", "tokens": ["Ent\u00b7de\u00b7cket", "man", "zu", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur der Bewegung Seltenheit,", "tokens": ["Nur", "der", "Be\u00b7we\u00b7gung", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die aus der ", "tokens": ["Die", "aus", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wenn wir die Lufft, als fl\u00fc\u00dfig, recht erwegen,", "tokens": ["Wenn", "wir", "die", "Lufft", ",", "als", "fl\u00fc\u00b7\u00dfig", ",", "recht", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn wir auch, da\u00df sie schwer, wol \u00fcberlegen;", "tokens": ["Wenn", "wir", "auch", ",", "da\u00df", "sie", "schwer", ",", "wol", "\u00fc\u00b7berl\u00b7e\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erkennen wir so gleich der Antlia Natur:", "tokens": ["Er\u00b7ken\u00b7nen", "wir", "so", "gleich", "der", "Ant\u00b7lia", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Denn es geschicht", "tokens": ["Denn", "es", "ge\u00b7schicht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Blos durch der Lufft Gewicht,", "tokens": ["Blos", "durch", "der", "Lufft", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df sich das Wasser aufwerts hebet,", "tokens": ["Da\u00df", "sich", "das", "Was\u00b7ser", "auf\u00b7werts", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bis zu gewisser H\u00f6h. Nicht aber, wie man spricht,", "tokens": ["Bis", "zu", "ge\u00b7wis\u00b7ser", "H\u00f6h", ".", "Nicht", "a\u00b7ber", ",", "wie", "man", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$.", "PTKNEG", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus einer Leere-Scheu,", "tokens": ["Aus", "ei\u00b7ner", "Lee\u00b7re\u00b7Scheu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der die Natur gantz widerstrebet.", "tokens": ["Der", "die", "Na\u00b7tur", "gantz", "wi\u00b7der\u00b7stre\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Denn, wenn es diese H\u00f6h erstiegen, siehet man,", "tokens": ["Denn", ",", "wenn", "es", "die\u00b7se", "H\u00f6h", "er\u00b7stie\u00b7gen", ",", "sie\u00b7het", "man", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PDAT", "NN", "VVPP", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob gleich das Leere noch zu scheuen;", "tokens": ["Ob", "gleich", "das", "Lee\u00b7re", "noch", "zu", "scheu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df doch das Wasser dann nicht h\u00f6her steigen kan.", "tokens": ["Da\u00df", "doch", "das", "Was\u00b7ser", "dann", "nicht", "h\u00f6\u00b7her", "stei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}