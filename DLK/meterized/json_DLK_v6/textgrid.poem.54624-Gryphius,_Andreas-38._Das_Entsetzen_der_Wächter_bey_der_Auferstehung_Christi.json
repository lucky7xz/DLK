{"textgrid.poem.54624": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "38. Das Entsetzen der W\u00e4chter bey der Auferstehung Christi", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Furcht komt die W\u00e4chter an und Beben und Erschrecken/", "tokens": ["Furcht", "komt", "die", "W\u00e4ch\u00b7ter", "an", "und", "Be\u00b7ben", "und", "Er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "PTKVZ", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem der Lebens F\u00fcrst aus seinem Schlaff erwacht.", "tokens": ["In", "dem", "der", "Le\u00b7bens", "F\u00fcrst", "aus", "sei\u00b7nem", "Schlaff", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn frembdes Auferstehn hier so voll Schrecken macht/", "tokens": ["Wenn", "fremb\u00b7des", "Auf\u00b7er\u00b7stehn", "hier", "so", "voll", "Schre\u00b7cken", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie zitternd wird euch dort der grosse Tag erwecken.", "tokens": ["Wie", "zit\u00b7ternd", "wird", "euch", "dort", "der", "gros\u00b7se", "Tag", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "ADV", "ART", "AD