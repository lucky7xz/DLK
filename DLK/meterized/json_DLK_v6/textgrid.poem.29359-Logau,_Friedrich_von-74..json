{"textgrid.poem.29359": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kinder lieben den, der nachgiebt ihrem Mute;", "tokens": ["Die", "Kin\u00b7der", "lie\u00b7ben", "den", ",", "der", "nach\u00b7giebt", "ih\u00b7rem", "Mu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kinder hassen den, der ihnen zeigt das gute.", "tokens": ["Die", "Kin\u00b7der", "has\u00b7sen", "den", ",", "der", "ih\u00b7nen", "zeigt", "das", "gu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist die Hofe-Gunst als wie die Gunst der Kinder:", "tokens": ["Es", "ist", "die", "Ho\u00b7fe\u00b7Gunst", "als", "wie", "die", "Gunst", "der", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Heucheley hat Preis, die Warheit Ha\u00df nicht minder.", "tokens": ["Die", "Heu\u00b7che\u00b7ley", "hat", "Preis", ",", "die", "War\u00b7heit", "Ha\u00df", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "