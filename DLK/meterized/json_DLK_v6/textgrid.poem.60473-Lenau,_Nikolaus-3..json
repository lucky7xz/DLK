{"textgrid.poem.60473": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den Hain mit bangem Sto\u00dfe", "tokens": ["Durch", "den", "Hain", "mit", "ban\u00b7gem", "Sto\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gewitterl\u00fcfte streichen;", "tokens": ["Die", "Ge\u00b7wit\u00b7ter\u00b7l\u00fcf\u00b7te", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tropfen sinken, schwere, gro\u00dfe,", "tokens": ["Trop\u00b7fen", "sin\u00b7ken", ",", "schwe\u00b7re", ",", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Bl\u00e4tter dieser Eichen.", "tokens": ["Auf", "die", "Bl\u00e4t\u00b7ter", "die\u00b7ser", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An ein banges Herzensklopfen", "tokens": ["An", "ein", "ban\u00b7ges", "Her\u00b7zens\u00b7klop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt mich dieser B\u00e4ume Schwanken,", "tokens": ["Mahnt", "mich", "die\u00b7ser", "B\u00e4u\u00b7me", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahnt mich an Gewittertropfen,", "tokens": ["Mahnt", "mich", "an", "Ge\u00b7wit\u00b7ter\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die aus lieben Augen sanken.", "tokens": ["Die", "aus", "lie\u00b7ben", "Au\u00b7gen", "san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mu\u00df ein gro\u00dfer Schmerz in Z\u00e4hren", "tokens": ["Mu\u00df", "ein", "gro\u00b7\u00dfer", "Schmerz", "in", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich entlasten unaufhaltsam,", "tokens": ["Sich", "ent\u00b7las\u00b7ten", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrzen ihm die gro\u00dfen, schweren", "tokens": ["St\u00fcr\u00b7zen", "ihm", "die", "gro\u00b7\u00dfen", ",", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tropfen pl\u00f6tzlich und gewaltsam.", "tokens": ["Trop\u00b7fen", "pl\u00f6tz\u00b7lich", "und", "ge\u00b7walt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "War die Tr\u00e4ne noch zu fassen,", "tokens": ["War", "die", "Tr\u00e4\u00b7ne", "noch", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam sie nicht hervorgebrochen,", "tokens": ["Kam", "sie", "nicht", "her\u00b7vor\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Schmerz will sie nicht lassen,", "tokens": ["Denn", "der", "Schmerz", "will", "sie", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sie hei\u00dfer, herber kochen.", "tokens": ["Will", "sie", "hei\u00b7\u00dfer", ",", "her\u00b7ber", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O! es waren hei\u00dfe, herbe,", "tokens": ["O", "!", "es", "wa\u00b7ren", "hei\u00b7\u00dfe", ",", "her\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die aus ihren Augen quollen;", "tokens": ["Die", "aus", "ih\u00b7ren", "Au\u00b7gen", "quol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich werde, bis ich sterbe,", "tokens": ["Und", "ich", "wer\u00b7de", ",", "bis", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehen diese Tr\u00e4nen rollen.", "tokens": ["Se\u00b7hen", "die\u00b7se", "Tr\u00e4\u00b7nen", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}