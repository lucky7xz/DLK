{"textgrid.poem.57272": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der deutschen Dichter Hainen entweht", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutschen Dichter Hainen entweht", "tokens": ["Der", "deut\u00b7schen", "Dich\u00b7ter", "Hai\u00b7nen", "ent\u00b7weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Gesang Alz\u00e4us und des Homer.", "tokens": ["Der", "Ge\u00b7sang", "A\u00b7lz\u00e4us", "und", "des", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ART", "NE", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Deinen Gang auf dem Kothurn, Sophokles,", "tokens": ["Dei\u00b7nen", "Gang", "auf", "dem", "Ko\u00b7thurn", ",", "So\u00b7phok\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meidet, und geht Jambanap\u00e4st.", "tokens": ["Mei\u00b7det", ",", "und", "geht", "Jam\u00b7ba\u00b7na\u00b7p\u00e4st", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Viel hats der Reize, Cynthius Tanz", "tokens": ["Viel", "hats", "der", "Rei\u00b7ze", ",", "Cyn\u00b7thi\u00b7us", "Tanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "NE", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu ereilen, und der H\u00f6rer belohnts;", "tokens": ["Zu", "er\u00b7ei\u00b7len", ",", "und", "der", "H\u00f6\u00b7rer", "be\u00b7lohnts", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dennoch hielt lieber den Reihn Teutons Volk,", "tokens": ["Den\u00b7noch", "hielt", "lie\u00b7ber", "den", "Reihn", "Teu\u00b7tons", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NE", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welcher voran Bragor einst flog.", "tokens": ["Wel\u00b7cher", "vo\u00b7ran", "Bra\u00b7gor", "einst", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Doch ach verstumt in ewiger Nacht", "tokens": ["Doch", "ach", "ver\u00b7stumt", "in", "e\u00b7wi\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist Bardiet! und Skofliod! und verhallt", "tokens": ["Ist", "Bar\u00b7diet", "!", "und", "Skof\u00b7li\u00b7od", "!", "und", "ver\u00b7hallt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NE", "$.", "KON", "NN", "$.", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Euer Schall, Telyn! Triomb! Hoch gesang,", "tokens": ["Eu\u00b7er", "Schall", ",", "Te\u00b7lyn", "!", "Tri\u00b7omb", "!", "Hoch", "ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "NN", "$.", "ADJD", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Deinem sogar klagen wir nach!", "tokens": ["Dei\u00b7nem", "so\u00b7gar", "kla\u00b7gen", "wir", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "O Sponda! rufet nun in dem Hain", "tokens": ["O", "Spon\u00b7da", "!", "ru\u00b7fet", "nun", "in", "dem", "Hain"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Des ruinentflohnen Griechen Gef\u00e4hrt,", "tokens": ["Des", "ru\u00b7i\u00b7nent\u00b7floh\u00b7nen", "Grie\u00b7chen", "Ge\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sponda! dich such' ich zu oft, ach umsonst!", "tokens": ["Spon\u00b7da", "!", "dich", "such'", "ich", "zu", "oft", ",", "ach", "um\u00b7sonst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "PTKA", "ADV", "$,", "ITJ", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Horche nach dir, finde dich nicht!", "tokens": ["Hor\u00b7che", "nach", "dir", ",", "fin\u00b7de", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wo, Echo, wallt ihr t\u00f6nender Schritt?", "tokens": ["Wo", ",", "E\u00b7cho", ",", "wallt", "ihr", "t\u00f6\u00b7nen\u00b7der", "Schritt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und in welche Grott' entf\u00fchrtest du sie,", "tokens": ["Und", "in", "wel\u00b7che", "Grott'", "ent\u00b7f\u00fchr\u00b7test", "du", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sprache, mir? Echo, du rufst sanft mir nach,", "tokens": ["Spra\u00b7che", ",", "mir", "?", "E\u00b7cho", ",", "du", "rufst", "sanft", "mir", "nach", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "$.", "NN", "$,", "PPER", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Aber auch dich h\u00f6ret sie nicht.", "tokens": ["A\u00b7ber", "auch", "dich", "h\u00f6\u00b7ret", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Es dr\u00e4ngten alle Genien sich", "tokens": ["Es", "dr\u00e4ng\u00b7ten", "al\u00b7le", "Ge\u00b7ni\u00b7en", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der entz\u00fcckten Melodie um ihn her.", "tokens": ["Der", "ent\u00b7z\u00fcck\u00b7ten", "Me\u00b7lo\u00b7die", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Riefen auch, klagten mit ihm, aber Stolz", "tokens": ["Rie\u00b7fen", "auch", ",", "klag\u00b7ten", "mit", "ihm", ",", "a\u00b7ber", "Stolz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "VVFIN", "APPR", "PPER", "$,", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Funkelt' im Blick einiger auch.", "tokens": ["Fun\u00b7kelt'", "im", "Blick", "ei\u00b7ni\u00b7ger", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PIAT", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Erhaben trat der Daktylos hier:", "tokens": ["Er\u00b7ha\u00b7ben", "trat", "der", "Dak\u00b7ty\u00b7los", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bin ich Herscher nicht im Liede M\u00e4oons?", "tokens": ["Bin", "ich", "Her\u00b7scher", "nicht", "im", "Lie\u00b7de", "M\u00e4\u00b7oons", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "APPRART", "NN", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Rufe denn Sponda nicht stets, bilde mich", "tokens": ["Ru\u00b7fe", "denn", "Spon\u00b7da", "nicht", "stets", ",", "bil\u00b7de", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NE", "PTKNEG", "ADV", "$,", "VVFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Oft zu Homers fliegendem Hall.", "tokens": ["Oft", "zu", "Ho\u00b7mers", "flie\u00b7gen\u00b7dem", "Hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Und h\u00f6rte nicht Choreos dich stets?", "tokens": ["Und", "h\u00f6r\u00b7te", "nicht", "Cho\u00b7re\u00b7os", "dich", "stets", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat er oft nicht Sponda's schwebenden Gang?", "tokens": ["Hat", "er", "oft", "nicht", "Spon\u00b7da's", "schwe\u00b7ben\u00b7den", "Gang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Geht sie denn, Kretikos t\u00f6nt's, meinen Gang?", "tokens": ["Geht", "sie", "denn", ",", "Kre\u00b7ti\u00b7kos", "t\u00f6nt's", ",", "mei\u00b7nen", "Gang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir, Choriamb, weich' ich allein!", "tokens": ["Dir", ",", "Cho\u00b7ri\u00b7amb", ",", "weich'", "ich", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da sang der Laute Silbergesang", "tokens": ["Da", "sang", "der", "Lau\u00b7te", "Sil\u00b7ber\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Choriambos: Ich bin Smintheus Apolls", "tokens": ["Cho\u00b7ri\u00b7am\u00b7bos", ":", "Ich", "bin", "Smin\u00b7theus", "A\u00b7polls"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "NE", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Liebling! mich lehrte sein Lied Hain und Strom,", "tokens": ["Lieb\u00b7ling", "!", "mich", "lehr\u00b7te", "sein", "Lied", "Hain", "und", "Strom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mich, da es flog nach dem Olymp,", "tokens": ["Mich", ",", "da", "es", "flog", "nach", "dem", "O\u00b7lymp", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Erkohr nicht Smintheus Pindarus mich", "tokens": ["Er\u00b7kohr", "nicht", "Smin\u00b7theus", "Pin\u00b7da\u00b7rus", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "NE", "NE", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anap\u00e4st, da er der Saite Get\u00f6n", "tokens": ["A\u00b7na\u00b7p\u00e4st", ",", "da", "er", "der", "Sai\u00b7te", "Ge\u00b7t\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lispeln liess? Jambos, Apolls alter Freund,", "tokens": ["Lis\u00b7peln", "liess", "?", "Jam\u00b7bos", ",", "A\u00b7polls", "al\u00b7ter", "Freund", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NE", "$,", "NE", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Hielt sich nicht mehr, z\u00fcrnt', und begann.", "tokens": ["Hielt", "sich", "nicht", "mehr", ",", "z\u00fcrnt'", ",", "und", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ADV", "$,", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Und geh nicht ich den Gang des Kothurns?", "tokens": ["Und", "geh", "nicht", "ich", "den", "Gang", "des", "Ko\u00b7thurns", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wo ... Baccheos schritt in lyrischem Tanz:", "tokens": ["Wo", "...", "Bac\u00b7che\u00b7os", "schritt", "in", "ly\u00b7ri\u00b7schem", "Tanz", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stolze, schweigt! ha, Choriamb, t\u00f6ntest du,", "tokens": ["Stol\u00b7ze", ",", "schweigt", "!", "ha", ",", "Cho\u00b7ri\u00b7amb", ",", "t\u00f6n\u00b7test", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$.", "ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Daktylos, du, t\u00f6nt' ich nicht mit?", "tokens": ["Dak\u00b7ty\u00b7los", ",", "du", ",", "t\u00f6nt'", "ich", "nicht", "mit", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Der sch\u00f6nste P\u00e4on eilte daher,", "tokens": ["Der", "sch\u00f6ns\u00b7te", "P\u00e4on", "eil\u00b7te", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Didym\u00e4os, leichtgewendet daher:", "tokens": ["Di\u00b7dy\u00b7m\u00e4\u00b7os", ",", "leicht\u00b7ge\u00b7wen\u00b7det", "da\u00b7her", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PAV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Fl\u00f6gen Thyrs' und Dithyramb' schnell genug;", "tokens": ["Fl\u00f6\u00b7gen", "Thyr\u00b7s'", "und", "Di\u00b7thy\u00b7ramb'", "schnell", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "ADJD", "ADV", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Risse sie nicht ich mit mir fort.", "tokens": ["Ris\u00b7se", "sie", "nicht", "ich", "mit", "mir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": "Ach Sponda! rief der Dichter, und hiess", "tokens": ["Ach", "Spon\u00b7da", "!", "rief", "der", "Dich\u00b7ter", ",", "und", "hiess"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In den Hain nacht ihr Pyrrichios gehn.", "tokens": ["In", "den", "Hain", "nacht", "ihr", 