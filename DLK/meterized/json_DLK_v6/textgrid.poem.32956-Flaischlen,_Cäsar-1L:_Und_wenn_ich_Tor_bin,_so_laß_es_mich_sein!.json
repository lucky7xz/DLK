{"textgrid.poem.32956": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wenn ich Tor bin, so la\u00df es mich sein!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn ich Tor bin, so la\u00df es mich sein!", "tokens": ["Und", "wenn", "ich", "Tor", "bin", ",", "so", "la\u00df", "es", "mich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "ADV", "VVIMP", "PPER", "PRF", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ich bin es ja doch nur f\u00fcr mich allein!", "tokens": ["ich", "bin", "es", "ja", "doch", "nur", "f\u00fcr", "mich", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und nennst du es Dummheit und Narretei,", "tokens": ["und", "nennst", "du", "es", "Dumm\u00b7heit", "und", "Nar\u00b7re\u00b7tei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "selbst wenn du recht h\u00e4ttst, was w\u00e4r viel dabei!?", "tokens": ["selbst", "wenn", "du", "recht", "h\u00e4ttst", ",", "was", "w\u00e4r", "viel", "da\u00b7bei", "!?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PWS", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Den einen freut dies, den anderen das,", "tokens": ["Den", "ei\u00b7nen", "freut", "dies", ",", "den", "an\u00b7de\u00b7ren", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PDS", "$,", "ART", "ADJA", "PDS", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der eine will trocken, der andere na\u00df!", "tokens": ["der", "ei\u00b7ne", "will", "tro\u00b7cken", ",", "der", "an\u00b7de\u00b7re", "na\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,", "PRELS", "PIS", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Duschiebst gern Kegel und sitzst gern beim Bier!", "tokens": ["Du\u00b7schiebst", "gern", "Ke\u00b7gel", "und", "sitz\u00b7st", "gern", "beim", "Bier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "also schieb deine Kegel und setz dich zum Bier!", "tokens": ["al\u00b7so", "schieb", "dei\u00b7ne", "Ke\u00b7gel", "und", "setz", "dich", "zum", "Bier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "ich mache Verse! ... wozu des Gegreins?!", "tokens": ["ich", "ma\u00b7che", "Ver\u00b7se", "!", "...", "wo\u00b7zu", "des", "Ge\u00b7greins", "?!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "$(", "PWAV", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "du lebst dein Leben, ich lebe meins!", "tokens": ["du", "lebst", "dein", "Le\u00b7ben", ",", "ich", "le\u00b7be", "meins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "We\u00df einer Spa\u00df hat, de\u00df hab er den Lohn!", "tokens": ["We\u00df", "ei\u00b7ner", "Spa\u00df", "hat", ",", "de\u00df", "hab", "er", "den", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein jeder sei Narr, Freund, auf seine Fa\u00e7on!", "tokens": ["ein", "je\u00b7der", "sei", "Narr", ",", "Freund", ",", "auf", "sei\u00b7ne", "Fa\u00e7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "NE", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}