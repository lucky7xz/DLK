{"dta.poem.13220": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evon GOtt besch\u00fczte Frau! Bewundrungs-werther Thron!", "tokens": ["\u201e", "von", "Gott", "be\u00b7sch\u00fcz\u00b7te", "Frau", "!", "Be\u00b7wun\u00b7drungs\u00b7wer\u00b7ther", "Thron", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "490\u201dGeb\u00fchrt der Fr\u00f6mmigkeit nicht ein besondrer Lohn?", "tokens": ["\"", "Ge\u00b7b\u00fchrt", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "nicht", "ein", "be\u00b7sond\u00b7rer", "Lohn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ejahr-hundert seynd vorbey; Jahr-hundert werden fliessen,", "tokens": ["\u201e", "jahr\u00b7hun\u00b7dert", "seynd", "vor\u00b7bey", ";", "Jahr\u00b7hun\u00b7dert", "wer\u00b7den", "flies\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADV", "$.", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso wird man jederzeit bey dir mich finden m\u00fcssen.", "tokens": ["\u201e", "so", "wird", "man", "je\u00b7der\u00b7zeit", "bey", "dir", "mich", "fin\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIS", "ADV", "APPR", "PPER", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eerlaube diesem nach, da\u00df ich O K\u00f6niginn", "tokens": ["\u201e", "er\u00b7lau\u00b7be", "die\u00b7sem", "nach", ",", "da\u00df", "ich", "O", "K\u00f6\u00b7ni\u00b7ginn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PDAT", "APPR", "$,", "KOUS", "PPER", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eder V\u00f6lcker Heil und Trost und Ueberwinderinn!", "tokens": ["\u201e", "der", "V\u00f6l\u00b7cker", "Heil", "und", "Trost", "und", "Ue\u00b7ber\u00b7win\u00b7de\u00b7rinn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "495\u201dDie du der Fr\u00f6mmigkeit Verdienst hast \u00fcbertrossen,", "tokens": ["\"", "Die", "du", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "Ver\u00b7dienst", "hast", "\u00fc\u00b7bert\u00b7ros\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPER", "ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201e(GOtt liesse sonder der uns keine Rettung hoffen)", "tokens": ["\u201e", "(", "Gott", "lies\u00b7se", "son\u00b7der", "der", "uns", "kei\u00b7ne", "Ret\u00b7tung", "hof\u00b7fen", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "VVFIN", "APPR", "PRELS", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eerlaube mir, da\u00df ich nicht um ein eitles Lob,", "tokens": ["\u201e", "er\u00b7lau\u00b7be", "mir", ",", "da\u00df", "ich", "nicht", "um", "ein", "eit\u00b7les", "Lob", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "\u201ewomit der Krei\u00df bi\u00dfher fast jede That erhob;", "tokens": ["\u201e", "wo\u00b7mit", "der", "Krei\u00df", "bi\u00df\u00b7her", "fast", "je\u00b7de", "That", "er\u00b7hob", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201enein: sondern um das Dach der GOtt-geweihten H\u00fctte", "tokens": ["\u201e", "nein", ":", "son\u00b7dern", "um", "das", "Dach", "der", "Got\u00b7tge\u00b7weih\u00b7ten", "H\u00fct\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "KON", "APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "500\u201dIn welcher ich so lang gewohnet habe, bitte.", "tokens": ["\"", "In", "wel\u00b7cher", "ich", "so", "lang", "ge\u00b7woh\u00b7net", "ha\u00b7be", ",", "bit\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edu siehst, ", "tokens": ["\u201e", "du", "siehst", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "\u201eda\u00df ich nichts weniger als auf die Prachten schau;", "tokens": ["\u201e", "da\u00df", "ich", "nichts", "we\u00b7ni\u00b7ger", "als", "auf", "die", "Prach\u00b7ten", "schau", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIS", "PIS", "KOKOM", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201emein Herz verlanget nichts was schn\u00f6der Ruhmsucht eigen;", "tokens": ["\u201e", "mein", "Herz", "ver\u00b7lan\u00b7get", "nichts", "was", "schn\u00f6\u00b7der", "Ruhm\u00b7sucht", "ei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PIS", "PWS", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ees will nur einen Plaz bey dir zu bleiben, zeigen.", "tokens": ["\u201e", "es", "will", "nur", "ei\u00b7nen", "Plaz", "bey", "dir", "zu", "blei\u00b7ben", ",", "zei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von dieser H\u00fctte wird oben\nim 8. Buch im 130. auch in den vor\nund nachgehenden Versen weitl\u00e4ufige-\n\nre Erw\u00e4hnung gemacht. Welche die\ndortige Anmerckung erkl\u00e4ret.", "tokens": ["Von", "die\u00b7ser", "H\u00fct\u00b7te", "wird", "o\u00b7ben", "im", "8.", "Buch", "im", "130.", "auch", "in", "den", "vor", "und", "nach\u00b7ge\u00b7hen\u00b7den", "Ver\u00b7sen", "weit\u00b7l\u00e4u\u00b7fi\u00b7ge", "re", "Er\u00b7w\u00e4h\u00b7nung", "ge\u00b7macht", ".", "Wel\u00b7che", "die", "dor\u00b7ti\u00b7ge", "An\u00b7mer\u00b7ckung", "er\u00b7kl\u00e4\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "ordinal", "word", "word", "ordinal", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "ADV", "APPR", "ART", "PTKVZ", "KON", "ADJA", "NN", "TRUNC", "ADJA", "NN", "VVPP", "$.", "PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+-+-+-+-+-+---+-+---+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "505\u201dIch w\u00fcnsche mir sonst nichts; ich bitte: sprich es aus!", "tokens": ["\"", "Ich", "w\u00fcn\u00b7sche", "mir", "sonst", "nichts", ";", "ich", "bit\u00b7te", ":", "sprich", "es", "aus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PIS", "$.", "PPER", "ADV", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eich wach\u2019 und bette stets f\u00fcr dich und f\u00fcr dein Haus.", "tokens": ["\u201e", "ich", "wach'", "und", "bet\u00b7te", "stets", "f\u00fcr", "dich", "und", "f\u00fcr", "dein", "Haus", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}