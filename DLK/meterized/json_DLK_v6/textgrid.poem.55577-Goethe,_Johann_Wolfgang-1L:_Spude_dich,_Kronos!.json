{"textgrid.poem.55577": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Spude dich, Kronos!", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spude dich, Kronos!", "tokens": ["Spu\u00b7de", "dich", ",", "Kro\u00b7nos", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Fort den rasselnden Trott!", "tokens": ["Fort", "den", "ras\u00b7seln\u00b7den", "Trott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bergab gleitet der Weg;", "tokens": ["Berg\u00b7ab", "glei\u00b7tet", "der", "Weg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ekles Schwindeln z\u00f6gert", "tokens": ["Ek\u00b7les", "Schwin\u00b7deln", "z\u00f6\u00b7gert"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mir vor die Stirne dein Haudern.", "tokens": ["Mir", "vor", "die", "Stir\u00b7ne", "dein", "Hau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Frisch den holpernden", "tokens": ["Frisch", "den", "hol\u00b7pern\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Stock, Wurzeln, Steine den Trott", "tokens": ["Stock", ",", "Wur\u00b7zeln", ",", "Stei\u00b7ne", "den", "Trott"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Rasch ins Leben hinein!", "tokens": ["Rasch", "ins", "Le\u00b7ben", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Nun schon wieder", "tokens": ["Nun", "schon", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Den eratmenden Schritt", "tokens": ["Den", "e\u00b7rat\u00b7men\u00b7den", "Schritt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "M\u00fchsam Berg hinauf.", "tokens": ["M\u00fch\u00b7sam", "Berg", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auf denn, nicht tr\u00e4ge denn!", "tokens": ["Auf", "denn", ",", "nicht", "tr\u00e4\u00b7ge", "denn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Strebend und hoffend an.", "tokens": ["Stre\u00b7bend", "und", "hof\u00b7fend", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Weit, hoch, herrlich der Blick", "tokens": ["Weit", ",", "hoch", ",", "herr\u00b7lich", "der", "Blick"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Rings ins Leben hinein;", "tokens": ["Rings", "ins", "Le\u00b7ben", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vom Geb\u00fcrg zum Geb\u00fcrg,", "tokens": ["Vom", "Ge\u00b7b\u00fcrg", "zum", "Ge\u00b7b\u00fcrg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber der ewige Geist", "tokens": ["\u00dc\u00b7ber", "der", "e\u00b7wi\u00b7ge", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ewigen Lebens ahndevoll.", "tokens": ["E\u00b7wi\u00b7gen", "Le\u00b7bens", "ahn\u00b7de\u00b7voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Seitw\u00e4rts des \u00dcberdachs Schatten", "tokens": ["Seit\u00b7w\u00e4rts", "des", "\u00dc\u00b7berd\u00b7achs", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zieht dich an", "tokens": ["Zieht", "dich", "an"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und der Frischung verhei\u00dfende Blick", "tokens": ["Und", "der", "Fri\u00b7schung", "ver\u00b7hei\u00b7\u00dfen\u00b7de", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Auf der Schwelle des M\u00e4dchens da.", "tokens": ["Auf", "der", "Schwel\u00b7le", "des", "M\u00e4d\u00b7chens", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Labe dich! \u2013 Mir auch, M\u00e4dchen,", "tokens": ["La\u00b7be", "dich", "!", "\u2013", "Mir", "auch", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Diesen sch\u00e4umenden Trunk", "tokens": ["Die\u00b7sen", "sch\u00e4u\u00b7men\u00b7den", "Trunk"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und den freundlichen Gesundheitsblick!", "tokens": ["Und", "den", "freund\u00b7li\u00b7chen", "Ge\u00b7sund\u00b7heits\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ab dann, frischer hinab!", "tokens": ["Ab", "dann", ",", "fri\u00b7scher", "hin\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sieh, die Sonne sinkt!", "tokens": ["Sieh", ",", "die", "Son\u00b7ne", "sinkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eh sie sinkt, eh mich fa\u00dft", "tokens": ["Eh", "sie", "sinkt", ",", "eh", "mich", "fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Greisen im Moore Nebelduft,", "tokens": ["Grei\u00b7sen", "im", "Moo\u00b7re", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Entzahnte Kiefer schnattern", "tokens": ["Ent\u00b7zahn\u00b7te", "Kie\u00b7fer", "schnat\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und das schlockernde Gebein \u2013.", "tokens": ["Und", "das", "schlo\u00b7ckern\u00b7de", "Ge\u00b7bein", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Trunknen vom letzten Strahl", "tokens": ["Trunk\u00b7nen", "vom", "letz\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Rei\u00df mich, ein Feuermeer", "tokens": ["Rei\u00df", "mich", ",", "ein", "Feu\u00b7er\u00b7meer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mir im sch\u00e4umenden Aug,", "tokens": ["Mir", "im", "sch\u00e4u\u00b7men\u00b7den", "Aug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mich Geblendeten, Taumelnden", "tokens": ["Mich", "Ge\u00b7blen\u00b7de\u00b7ten", ",", "Tau\u00b7meln\u00b7den"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "NN", "$,", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "In der H\u00f6lle n\u00e4chtliches Tor.", "tokens": ["In", "der", "H\u00f6l\u00b7le", "n\u00e4cht\u00b7li\u00b7ches", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "T\u00f6ne, Schwager, dein Horn,", "tokens": ["T\u00f6\u00b7ne", ",", "Schwa\u00b7ger", ",", "dein", "Horn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ra\u00dfle den schallenden Trab,", "tokens": ["Ra\u00df\u00b7le", "den", "schal\u00b7len\u00b7den", "Trab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df der Orkus vernehme: ein F\u00fcrst kommt,", "tokens": ["Da\u00df", "der", "Or\u00b7kus", "ver\u00b7neh\u00b7me", ":", "ein", "F\u00fcrst", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Drunten von ihren Sitzen", "tokens": ["Drun\u00b7ten", "von", "ih\u00b7ren", "Sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sich die Gewaltigen l\u00fcften.", "tokens": ["Sich", "die", "Ge\u00b7wal\u00b7ti\u00b7gen", "l\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}