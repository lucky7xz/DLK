{"textgrid.poem.43474": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hilft's, da\u00df billig ward der Schiller,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilft's, da\u00df billig ward der Schiller,", "tokens": ["Was", "hilft's", ",", "da\u00df", "bil\u00b7lig", "ward", "der", "Schil\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df es giebt so manches Weinhaus?", "tokens": ["Und", "da\u00df", "es", "giebt", "so", "man\u00b7ches", "Wein\u00b7haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt wird still und immer stiller", "tokens": ["Die", "Welt", "wird", "still", "und", "im\u00b7mer", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und graulich wie ein altes Beinhaus.", "tokens": ["Und", "grau\u00b7lich", "wie", "ein", "al\u00b7tes", "Be\u00b7in\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Zeit, sie kehrt nicht wieder,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Zeit", ",", "sie", "kehrt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jedes Herz und jedes Geld klang.", "tokens": ["Wo", "je\u00b7des", "Herz", "und", "je\u00b7des", "Geld", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer braucht noch heute Klang und Lieder?", "tokens": ["Wer", "braucht", "noch", "heu\u00b7te", "Klang", "und", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stumm und papieren ist der Weltgang.", "tokens": ["Stumm", "und", "pa\u00b7pie\u00b7ren", "ist", "der", "Welt\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur haben, haben und behalten,", "tokens": ["Nur", "ha\u00b7ben", ",", "ha\u00b7ben", "und", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VAFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht weiter bringt es die Genu\u00dflust;", "tokens": ["Nicht", "wei\u00b7ter", "bringt", "es", "die", "Ge\u00b7nu\u00df\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sie einmal den Frohsinn walten,", "tokens": ["L\u00e4\u00dft", "sie", "ein\u00b7mal", "den", "Froh\u00b7sinn", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So sieht es aus als w\u00e4r's Verdru\u00df just.", "tokens": ["So", "sieht", "es", "aus", "als", "w\u00e4r's", "Ver\u00b7dru\u00df", "just", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch soll mein Herz vor Freude springen,", "tokens": ["Doch", "soll", "mein", "Herz", "vor", "Freu\u00b7de", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schreit auch der Nachbar: welch ein Leichtsinn!", "tokens": ["Schreit", "auch", "der", "Nach\u00b7bar", ":", "welch", "ein", "Leicht\u00b7sinn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und leben will ich, trinken, singen,", "tokens": ["Und", "le\u00b7ben", "will", "ich", ",", "trin\u00b7ken", ",", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht dr\u00fcber der Credit auch leicht hin.", "tokens": ["Geht", "dr\u00fc\u00b7ber", "der", "Cre\u00b7dit", "auch", "leicht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Kommt her, ihr Kargen, kommt, ihr K\u00e4rgern,", "tokens": ["Kommt", "her", ",", "ihr", "Kar\u00b7gen", ",", "kommt", ",", "ihr", "K\u00e4r\u00b7gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trinke, kommt und seht genau hin,", "tokens": ["Ich", "trin\u00b7ke", ",", "kommt", "und", "seht", "ge\u00b7nau", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trink' und stell', euch recht zu \u00e4rgern,", "tokens": ["Ich", "trink'", "und", "stell'", ",", "euch", "recht", "zu", "\u00e4r\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die leeren Flaschen hier zur Schau hin.", "tokens": ["Die", "lee\u00b7ren", "Fla\u00b7schen", "hier", "zur", "Schau", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Euch ward gar vieles hier auf Erden,", "tokens": ["Euch", "ward", "gar", "vie\u00b7les", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir nur ein Wunsch, f\u00fcrwahr ein klein Heil:", "tokens": ["Mir", "nur", "ein", "Wunsch", ",", "f\u00fcr\u00b7wahr", "ein", "klein", "Heil", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nie Mangel des Gef\u00fchls mag werden", "tokens": ["Nie", "Man\u00b7gel", "des", "Ge\u00b7f\u00fchls", "mag", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ART", "NN", "VMFIN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nie Gef\u00fchl des Mangels ", "tokens": ["Und", "nie", "Ge\u00b7f\u00fchl", "des", "Man\u00b7gels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}