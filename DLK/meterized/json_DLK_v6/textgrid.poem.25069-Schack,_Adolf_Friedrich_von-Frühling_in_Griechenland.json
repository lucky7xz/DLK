{"textgrid.poem.25069": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling in Griechenland", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun zieht in die Fluten der Schiffer den Kiel;", "tokens": ["Nun", "zieht", "in", "die", "Flu\u00b7ten", "der", "Schif\u00b7fer", "den", "Kiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Heim kehren die zwitschernden Schwalben vom Nil", "tokens": ["Heim", "keh\u00b7ren", "die", "zwit\u00b7schern\u00b7den", "Schwal\u00b7ben", "vom", "Nil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zu ihren geliebten Cykladen,", "tokens": ["Zu", "ih\u00b7ren", "ge\u00b7lieb\u00b7ten", "Cyk\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und jauchzend, erwacht aus dem Wintertraum,", "tokens": ["Und", "jauch\u00b7zend", ",", "er\u00b7wacht", "aus", "dem", "Win\u00b7ter\u00b7traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Durchflattert die M\u00f6we den spritzenden Schaum", "tokens": ["Durch\u00b7flat\u00b7tert", "die", "M\u00f6\u00b7we", "den", "sprit\u00b7zen\u00b7den", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "An allen den Inselgestaden.", "tokens": ["An", "al\u00b7len", "den", "In\u00b7sel\u00b7ge\u00b7sta\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Am duft'gen Hymettus von neuem umsummt", "tokens": ["Am", "duft'\u00b7gen", "Hy\u00b7met\u00b7tus", "von", "neu\u00b7em", "um\u00b7summt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Chor der Bienen, der lange verstummt,", "tokens": ["Der", "Chor", "der", "Bie\u00b7nen", ",", "der", "lan\u00b7ge", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Ginsters goldene Bl\u00fcten;", "tokens": ["Des", "Gins\u00b7ters", "gol\u00b7de\u00b7ne", "Bl\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und es wacht in der milderen Nacht des April", "tokens": ["Und", "es", "wacht", "in", "der", "mil\u00b7de\u00b7ren", "Nacht", "des", "Ap\u00b7ril"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Am Bach im Gest\u00e4ude von Asphodill", "tokens": ["Am", "Bach", "im", "Ge\u00b7st\u00e4u\u00b7de", "von", "As\u00b7pho\u00b7dill"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "APPR", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Hirt, um die Herde zu h\u00fcten.", "tokens": ["Der", "Hirt", ",", "um", "die", "Her\u00b7de", "zu", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "O Hellas! Ruhn, der Jahrtausende Raub,", "tokens": ["O", "Hel\u00b7las", "!", "Ruhn", ",", "der", "Jahr\u00b7tau\u00b7sen\u00b7de", "Raub", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch deine Tempel in Tr\u00fcmmer und Staub", "tokens": ["Auch", "dei\u00b7ne", "Tem\u00b7pel", "in", "Tr\u00fcm\u00b7mer", "und", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der V\u00f6lkerst\u00fcrme gebettet,", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7st\u00fcr\u00b7me", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich hat aus dem leuchtenden Morgen der Welt", "tokens": ["Dich", "hat", "aus", "dem", "leuch\u00b7ten\u00b7den", "Mor\u00b7gen", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Dein Genius, ein unsterblicher Held,", "tokens": ["Dein", "Ge\u00b7nius", ",", "ein", "uns\u00b7terb\u00b7li\u00b7cher", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zu uns her\u00fcbergerettet.", "tokens": ["Zu", "uns", "her\u00b7\u00fc\u00b7ber\u00b7ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Noch singt den ewigen Siegesp\u00e4an", "tokens": ["Noch", "singt", "den", "e\u00b7wi\u00b7gen", "Sie\u00b7ge\u00b7sp\u00e4\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An Salamis' Ufern der Ocean", "tokens": ["An", "Sa\u00b7la\u00b7mis'", "U\u00b7fern", "der", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit der Wogen melodischen Lippen,", "tokens": ["Mit", "der", "Wo\u00b7gen", "me\u00b7lo\u00b7di\u00b7schen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und, brausend um des Themistokles Grab,", "tokens": ["Und", ",", "brau\u00b7send", "um", "des", "The\u00b7mis\u00b7to\u00b7kles", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erweckt er das Echo von Kap zu Kap", "tokens": ["Er\u00b7weckt", "er", "das", "E\u00b7cho", "von", "Kap", "zu", "Kap"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Weithin an den Inseln und Klippen.", "tokens": ["Weit\u00b7hin", "an", "den", "In\u00b7seln", "und", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Hoch \u00fcber Asiens Berge heran", "tokens": ["Hoch", "\u00fc\u00b7ber", "A\u00b7siens", "Ber\u00b7ge", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NE", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "F\u00fchrt Helios der strahlenden Rosse Gespann", "tokens": ["F\u00fchrt", "He\u00b7li\u00b7os", "der", "strah\u00b7len\u00b7den", "Ros\u00b7se", "Ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gr\u00fc\u00dft sein liebstes der L\u00e4nder;", "tokens": ["Und", "gr\u00fc\u00dft", "sein", "liebs\u00b7tes", "der", "L\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf H\u00fcgeln wird es, auf Fluren wach;", "tokens": ["Auf", "H\u00fc\u00b7geln", "wird", "es", ",", "auf", "Flu\u00b7ren", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im Myrtengeb\u00fcsch, am st\u00fcrzenden Bach,", "tokens": ["Im", "Myr\u00b7ten\u00b7ge\u00b7b\u00fcsch", ",", "am", "st\u00fcr\u00b7zen\u00b7den", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Was schimmern so wei\u00df die Gew\u00e4nder?", "tokens": ["Was", "schim\u00b7mern", "so", "wei\u00df", "die", "Ge\u00b7w\u00e4n\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Die Jungfraun sind es, die heiligen neun,", "tokens": ["Die", "Jung\u00b7fraun", "sind", "es", ",", "die", "hei\u00b7li\u00b7gen", "neun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ART", "ADJA", "CARD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die auf Erden die Saat des Sch\u00f6nen verstreun,", "tokens": ["Die", "auf", "Er\u00b7den", "die", "Saat", "des", "Sch\u00f6\u00b7nen", "ver\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Tr\u00e4gerinnen der Leier;", "tokens": ["Die", "Tr\u00e4\u00b7ge\u00b7rin\u00b7nen", "der", "Lei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Neu lassen die Th\u00e4ler sie bl\u00fchen, die H\u00f6hn,", "tokens": ["Neu", "las\u00b7sen", "die", "Th\u00e4\u00b7ler", "sie", "bl\u00fc\u00b7hen", ",", "die", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und singen zu bebender Saiten Get\u00f6n", "tokens": ["Und", "sin\u00b7gen", "zu", "be\u00b7ben\u00b7der", "Sai\u00b7ten", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der hohen Unsterblichen Feier.", "tokens": ["Der", "ho\u00b7hen", "U\u00b7nsterb\u00b7li\u00b7chen", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Nicht ist gestorben der alte Pan;", "tokens": ["Nicht", "ist", "ge\u00b7stor\u00b7ben", "der", "al\u00b7te", "Pan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entschlafen auf gr\u00fcnendem Wiesenplan", "tokens": ["Ent\u00b7schla\u00b7fen", "auf", "gr\u00fc\u00b7nen\u00b7dem", "Wie\u00b7sen\u00b7plan"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Nur war er, von Ulmen beschattet,", "tokens": ["Nur", "war", "er", ",", "von", "Ul\u00b7men", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und bei der Syrinx ersterbendem Ton", "tokens": ["Und", "bei", "der", "Sy\u00b7rinx", "ers\u00b7ter\u00b7ben\u00b7dem", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch senkten das Haupt, bekr\u00e4nzt mit Mohn,", "tokens": ["Auch", "senk\u00b7ten", "das", "Haupt", ",", "be\u00b7kr\u00e4nzt", "mit", "Mohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die anderen G\u00f6tter ermattet.", "tokens": ["Die", "an\u00b7de\u00b7ren", "G\u00f6t\u00b7ter", "er\u00b7mat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Nachtd\u00fcstre D\u00e4monen umklammerten kalt,", "tokens": ["Nacht\u00b7d\u00fcs\u00b7tre", "D\u00e4\u00b7mo\u00b7nen", "um\u00b7klam\u00b7mer\u00b7ten", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie der Alp in die Brust des Schl\u00e4fers sich krallt,", "tokens": ["Wie", "der", "Alp", "in", "die", "Brust", "des", "Schl\u00e4\u00b7fers", "sich", "krallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Menschen ge\u00e4ngstete Seelen,", "tokens": ["Der", "Men\u00b7schen", "ge\u00b7\u00e4ngs\u00b7te\u00b7te", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sie tr\u00e4umten, anstatt vom lichten Parna\u00df,", "tokens": ["Und", "sie", "tr\u00e4um\u00b7ten", ",", "an\u00b7statt", "vom", "lich\u00b7ten", "Par\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUI", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Von blutenden Heiligen, leichenbla\u00df,", "tokens": ["Von", "blu\u00b7ten\u00b7den", "Hei\u00b7li\u00b7gen", ",", "lei\u00b7chen\u00b7bla\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Von Kreuzen und Marterpf\u00e4hlen.", "tokens": ["Von", "Kreu\u00b7zen", "und", "Mar\u00b7ter\u00b7pf\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Doch als die Nacht und der Winter entfloh,", "tokens": ["Doch", "als", "die", "Nacht", "und", "der", "Win\u00b7ter", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aufschlugen den Blick sie und l\u00e4chelten froh", "tokens": ["Auf\u00b7schlu\u00b7gen", "den", "Blick", "sie", "und", "l\u00e4\u00b7chel\u00b7ten", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In des Himmels selige Bl\u00e4ue,", "tokens": ["In", "des", "Him\u00b7mels", "se\u00b7li\u00b7ge", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct