{"textgrid.poem.57051": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wort-Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palma Kunkel spricht auch. O gewi\u00df.", "tokens": ["Pal\u00b7ma", "Kun\u00b7kel", "spricht", "auch", ".", "O", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$.", "NE", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Freilich nicht wie Volk der Finsternis.", "tokens": ["Frei\u00b7lich", "nicht", "wie", "Volk", "der", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "KOKOM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht von Worten kollernd wie ein Bronnen,", "tokens": ["Nicht", "von", "Wor\u00b7ten", "kol\u00b7lernd", "wie", "ein", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "niemals nachw\u00e4rts-, immer vorbesonnen.", "tokens": ["nie\u00b7mals", "nach\u00b7w\u00e4rts", ",", "im\u00b7mer", "vor\u00b7be\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "V\u00f6llig fremd den hilflos vielen Sch\u00e4llen,", "tokens": ["V\u00f6l\u00b7lig", "fremd", "den", "hil\u00b7flos", "vie\u00b7len", "Sch\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "fragt sie nur in wirklich gro\u00dfen F\u00e4llen.", "tokens": ["fragt", "sie", "nur", "in", "wirk\u00b7lich", "gro\u00b7\u00dfen", "F\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Fragt den Zwergen niemals, nur den Riesen,", "tokens": ["Fragt", "den", "Zwer\u00b7gen", "nie\u00b7mals", ",", "nur", "den", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und auch nicht, wie es ihm gehe, diesen.", "tokens": ["und", "auch", "nicht", ",", "wie", "es", "ihm", "ge\u00b7he", ",", "die\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PDAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht vom Wetter spricht sie, nicht vom Schneider,", "tokens": ["Nicht", "vom", "Wet\u00b7ter", "spricht", "sie", ",", "nicht", "vom", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "APPRART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "h\u00f6chstens von den Grundproblemen beider.", "tokens": ["h\u00f6chs\u00b7tens", "von", "den", "Grund\u00b7prob\u00b7le\u00b7men", "bei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und so bleibt sie jung und unverbraucht,", "tokens": ["Und", "so", "bleibt", "sie", "jung", "und", "un\u00b7ver\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "weil ihr Odem nicht wie Dunst verraucht.", "tokens": ["weil", "ihr", "O\u00b7dem", "nicht", "wie", "Dunst", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "KOKOM", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}