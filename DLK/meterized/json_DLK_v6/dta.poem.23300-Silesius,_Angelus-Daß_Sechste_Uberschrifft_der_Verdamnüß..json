{"dta.poem.23300": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df Sechste  \n Uberschrifft der Verdamn\u00fc\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist ein\u2019 Ewge Nacht: man wei\u00df von keinem la-", "tokens": ["Hier", "ist", "ein'", "Ew\u00b7ge", "Nacht", ":", "man", "wei\u00df", "von", "kei\u00b7nem", "la"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Jam\u0303er Ach un\u0303 Weh/ achewig sein verlohrn! (che\u0303/", "tokens": ["Ein", "Jam\u0303er", "Ach", "u\u00f1", "Weh", "/", "ac\u00b7he\u00b7wig", "sein", "ver\u00b7lohrn", "!", "(", "ch\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ADJD", "VAINF", "VVINF", "$.", "$(", "NE", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wird jm\u0303er fort geschriehn/ un\u0303 w\u00e4rn wir nie gebohrn!", "tokens": ["Wird", "jm\u0303er", "fort", "ge\u00b7schriehn", "/", "u\u00f1", "w\u00e4rn", "wir", "nie", "ge\u00b7bohrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "VVPP", "$(", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Beyneben h\u00f6rt man nichts als Donnern/ Hageln/", "tokens": ["Bey\u00b7ne\u00b7ben", "h\u00f6rt", "man", "nichts", "als", "Don\u00b7nern", "/", "Ha\u00b7geln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PIS", "KOKOM", "NN", "$(", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Krachen.(Drachen/", "tokens": ["Kra\u00b7chen", ".", "(", "Dra\u00b7chen", "/"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Man sicht den Basilischk mit Kr\u00f6ten/ Schlangen/", "tokens": ["Man", "sicht", "den", "Ba\u00b7si\u00b7lischk", "mit", "Kr\u00f6\u00b7ten", "/", "Schlan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und tausend ungeheur: Man ist f\u00fcr K\u00e4lt\u2019 erfrohrn/", "tokens": ["Und", "tau\u00b7send", "un\u00b7ge\u00b7heur", ":", "Man", "ist", "f\u00fcr", "K\u00e4lt'", "er\u00b7frohrn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$.", "PIS", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und schmeltzt f\u00fcr grosser Glutt: man schilt sich Narrn", "tokens": ["Und", "schmeltzt", "f\u00fcr", "gros\u00b7ser", "Glutt", ":", "man", "schilt", "sich", "Narrn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "PIS", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und Thorn/(rachen.", "tokens": ["und", "Thorn", "/", "(", "ra\u00b7chen", "."], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "NN", "$(", "$(", "VVINF", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Und kombt doch nimmermehr au\u00df diesem Teufels-", "tokens": ["Und", "kombt", "doch", "nim\u00b7mer\u00b7mehr", "au\u00df", "die\u00b7sem", "Teu\u00b7fels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Man stirbt un\u0303 stirbt doch nie/ man ligt im ewgen Tod/", "tokens": ["Man", "stirbt", "u\u00f1", "stirbt", "doch", "nie", "/", "man", "ligt", "im", "ew\u00b7gen", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$(", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man w\u00fcttet tobt un\u0303 z\u00f6rnt/ man flucht un\u0303 l\u00e4stert Gott.", "tokens": ["Man", "w\u00fct\u00b7tet", "tobt", "u\u00f1", "z\u00f6rnt", "/", "man", "flucht", "u\u00f1", "l\u00e4s\u00b7tert", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVFIN", "KON", "VVFIN", "$(", "PIS", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man beist und Hadert sich/ man lebt wie Hund\u2019 und", "tokens": ["Man", "beist", "und", "Ha\u00b7dert", "sich", "/", "man", "lebt", "wie", "Hund'", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "NE", "PRF", "$(", "PIS", "VVFIN", "KOKOM", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Katzen:", "tokens": ["Kat\u00b7zen", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Man mu\u00df sich ewiglich mit allen Teuffeln kratzen:", "tokens": ["Man", "mu\u00df", "sich", "e\u00b7wig\u00b7lich", "mit", "al\u00b7len", "Teuf\u00b7feln", "krat\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man frisset H\u00fcttenrauch/ Pech/ Schweffel/ Teuffels-", "tokens": ["Man", "fris\u00b7set", "H\u00fct\u00b7ten\u00b7rauch", "/", "Pech", "/", "Schwef\u00b7fel", "/", "Teuf\u00b7fels"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "NN", "$(", "NE", "$(", "NN", "$(", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "mist:", "tokens": ["mist", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Ach S\u00fcnder thu doch Buss\u2019 eh du darinnen bist!", "tokens": ["Ach", "S\u00fcn\u00b7der", "thu", "doch", "Buss'", "eh", "du", "da\u00b7rin\u00b7nen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "ADV", "NN", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}