{"textgrid.poem.65736": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schmach! vernein' es irgendwen zu lieben,", "tokens": ["O", "Schmach", "!", "ver\u00b7nein'", "es", "ir\u00b7gend\u00b7wen", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, der du auf dich selbst so unbedacht!", "tokens": ["Du", ",", "der", "du", "auf", "dich", "selbst", "so", "un\u00b7be\u00b7dacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gib zu, du seist das Ziel von vieler Trieben,", "tokens": ["Gib", "zu", ",", "du", "seist", "das", "Ziel", "von", "vie\u00b7ler", "Trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch da\u00df ", "tokens": ["Doch", "da\u00df"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Denn dich beherrscht ein m\u00f6rderischer Ha\u00df,", "tokens": ["Denn", "dich", "be\u00b7herrscht", "ein", "m\u00f6r\u00b7de\u00b7ri\u00b7scher", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df du nicht zauderst, selbst dich zu bedr\u00e4uen,", "tokens": ["Da\u00df", "du", "nicht", "zau\u00b7derst", ",", "selbst", "dich", "zu", "be\u00b7dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das edle Haus zerr\u00fctten m\u00f6chtest, das", "tokens": ["Das", "ed\u00b7le", "Haus", "zer\u00b7r\u00fct\u00b7ten", "m\u00f6ch\u00b7test", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor allen dir geziemte zu erneuen.", "tokens": ["Vor", "al\u00b7len", "dir", "ge\u00b7ziem\u00b7te", "zu", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O \u00e4ndre deinen Sinn, und meine Meinung!", "tokens": ["O", "\u00e4nd\u00b7re", "dei\u00b7nen", "Sinn", ",", "und", "mei\u00b7ne", "Mei\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Birgt Ha\u00df in holder Liebe Wohnung sich?", "tokens": ["Birgt", "Ha\u00df", "in", "hol\u00b7der", "Lie\u00b7be", "Woh\u00b7nung", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sei mild wie deine liebliche Erscheinung:", "tokens": ["Sei", "mild", "wie", "dei\u00b7ne", "lieb\u00b7li\u00b7che", "Er\u00b7schei\u00b7nung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sei mindestens barmherzig gegen dich.", "tokens": ["Sei", "min\u00b7des\u00b7tens", "barm\u00b7her\u00b7zig", "ge\u00b7gen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erschaffe neu, aus Liebe dich zu mir,", "tokens": ["Er\u00b7schaf\u00b7fe", "neu", ",", "aus", "Lie\u00b7be", "dich", "zu", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "APPR", "NN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df Sch\u00f6nheit leb' im deinen oder dir.", "tokens": ["Da\u00df", "Sch\u00f6n\u00b7heit", "leb'", "im", "dei\u00b7nen", "o\u00b7der", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPRART", "PPOSAT", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}