{"textgrid.poem.51169": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Rote Lehre", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich bin rot und hab's erwogen", "tokens": ["\u00bb", "ich", "bin", "rot", "und", "hab's", "er\u00b7wo\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "KON", "NE", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und behaupt es unverweilt!", "tokens": ["Und", "be\u00b7haupt", "es", "un\u00b7ver\u00b7weilt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich, w\u00fcrd ich jeden k\u00f6pfen,", "tokens": ["K\u00f6nnt", "ich", ",", "w\u00fcrd", "ich", "je\u00b7den", "k\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der nicht meine Meinung teilt!\u00ab", "tokens": ["Der", "nicht", "mei\u00b7ne", "Mei\u00b7nung", "teilt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des Baders enger Stube", "tokens": ["In", "des", "Ba\u00b7ders", "en\u00b7ger", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Vetter Hansen also sprach,", "tokens": ["Vet\u00b7ter", "Han\u00b7sen", "al\u00b7so", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eben als 'nem feisten B\u00e4cker", "tokens": ["E\u00b7ben", "als", "'nem", "feis\u00b7ten", "B\u00e4\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener in die Ader stach.", "tokens": ["Je\u00b7ner", "in", "die", "A\u00b7der", "stach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und des Blutes muntrer Bogen", "tokens": ["Und", "des", "Blu\u00b7tes", "mun\u00b7trer", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem dicken drallen Arm", "tokens": ["Aus", "dem", "di\u00b7cken", "dral\u00b7len", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fiel dem Vetter auf die Nase,", "tokens": ["Fiel", "dem", "Vet\u00b7ter", "auf", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie begr\u00fc\u00dfend freundlich warm.", "tokens": ["Sie", "be\u00b7gr\u00fc\u00b7\u00dfend", "freund\u00b7lich", "warm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bleich, entsetzt fuhr er zusammen,", "tokens": ["Bleich", ",", "ent\u00b7setzt", "fuhr", "er", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wusch darauf sich siebenmal;", "tokens": ["Wusch", "da\u00b7rauf", "sich", "sie\u00b7ben\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch noch lang r\u00fcmpft' er die Nase,", "tokens": ["Doch", "noch", "lang", "r\u00fcmpft'", "er", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' noch lang den warmen Strahl.", "tokens": ["F\u00fchlt'", "noch", "lang", "den", "war\u00b7men", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mittags widert ihm die Suppe,", "tokens": ["Mit\u00b7tags", "wi\u00b7dert", "ihm", "die", "Sup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "R\u00f6tlich dampft sie, wie noch nie;", "tokens": ["R\u00f6t\u00b7lich", "dampft", "sie", ",", "wie", "noch", "nie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer geht es so der alten", "tokens": ["Im\u00b7mer", "geht", "es", "so", "der", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grauen Eselstheorie!", "tokens": ["Grau\u00b7en", "E\u00b7sel\u00b7sthe\u00b7o\u00b7rie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Manches Br\u00fcnnlein mag noch springen", "tokens": ["Man\u00b7ches", "Br\u00fcnn\u00b7lein", "mag", "noch", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Gras mit rotem Schein;", "tokens": ["In", "das", "Gras", "mit", "ro\u00b7tem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Freiheit echter, rechter", "tokens": ["Doch", "der", "Frei\u00b7heit", "ech\u00b7ter", ",", "rech\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letzter Sieg wird trocken sein.", "tokens": ["Letz\u00b7ter", "Sieg", "wird", "tro\u00b7cken", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}