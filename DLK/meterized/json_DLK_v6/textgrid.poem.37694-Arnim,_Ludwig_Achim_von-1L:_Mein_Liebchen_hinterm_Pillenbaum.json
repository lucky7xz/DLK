{"textgrid.poem.37694": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Liebchen hinterm Pillenbaum", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebchen hinterm Pillenbaum", "tokens": ["Mein", "Lieb\u00b7chen", "hin\u00b7term", "Pil\u00b7len\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versteckt ihr lieblich Angesicht", "tokens": ["Ver\u00b7steckt", "ihr", "lieb\u00b7lich", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihren beiden H\u00e4nden,", "tokens": ["Mit", "ih\u00b7ren", "bei\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So meinte sie, sie s\u00e4h' mich nicht,", "tokens": ["So", "mein\u00b7te", "sie", ",", "sie", "s\u00e4h'", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sieht mich durch die Finger kaum,", "tokens": ["Und", "sieht", "mich", "durch", "die", "Fin\u00b7ger", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tr\u00fcg' mich doch gern auf beiden H\u00e4nden!", "tokens": ["Und", "tr\u00fcg'", "mich", "doch", "gern", "auf", "bei\u00b7den", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}