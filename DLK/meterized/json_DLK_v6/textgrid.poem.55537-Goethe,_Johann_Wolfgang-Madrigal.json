{"textgrid.poem.55537": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Climene lebt in tausend Sorgen,", "tokens": ["Cli\u00b7me\u00b7ne", "lebt", "in", "tau\u00b7send", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df heut den Schatz ihr Hymen m\u00e4chtig raubt,", "tokens": ["Da\u00df", "heut", "den", "Schatz", "ihr", "Hy\u00b7men", "m\u00e4ch\u00b7tig", "raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den sie der Liebe lang verborgen.", "tokens": ["Den", "sie", "der", "Lie\u00b7be", "lang", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O h\u00e4tte sie l\u00e4ngst meinem Rat geglaubt;", "tokens": ["O", "h\u00e4t\u00b7te", "sie", "l\u00e4ngst", "mei\u00b7nem", "Rat", "ge\u00b7glaubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie h\u00e4tte jetzt nichts mehr zu sorgen.", "tokens": ["Sie", "h\u00e4t\u00b7te", "jetzt", "nichts", "mehr", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}