{"textgrid.poem.24950": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weh nun, da den B\u00e4umen der herbstliche Wind", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh nun, da den B\u00e4umen der herbstliche Wind", "tokens": ["Weh", "nun", ",", "da", "den", "B\u00e4u\u00b7men", "der", "herbst\u00b7li\u00b7che", "Wind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Absch\u00fcttelt das Laub, das falbe,", "tokens": ["Ab\u00b7sch\u00fct\u00b7telt", "das", "Laub", ",", "das", "fal\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weh dir, der die Schwingen gebrochen sind,", "tokens": ["Weh", "dir", ",", "der", "die", "Schwin\u00b7gen", "ge\u00b7bro\u00b7chen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du arme verlassene Schwalbe!", "tokens": ["Du", "ar\u00b7me", "ver\u00b7las\u00b7se\u00b7ne", "Schwal\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Voll Trauer blickst du von deinem Dach", "tokens": ["Voll", "Trau\u00b7er", "blickst", "du", "von", "dei\u00b7nem", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem Zug der Gespielen, dem scheidenden, nach.", "tokens": ["Dem", "Zug", "der", "Ge\u00b7spie\u00b7len", ",", "dem", "schei\u00b7den\u00b7den", ",", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ziehen hinweg in den herrlichen S\u00fcd,", "tokens": ["Sie", "zie\u00b7hen", "hin\u00b7weg", "in", "den", "herr\u00b7li\u00b7chen", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie lassen die krankende Schwester", "tokens": ["Sie", "las\u00b7sen", "die", "kran\u00b7ken\u00b7de", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und suchen im Fr\u00fchling, der ewig bl\u00fcht,", "tokens": ["Und", "su\u00b7chen", "im", "Fr\u00fch\u00b7ling", ",", "der", "e\u00b7wig", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die myrtenbeschatteten Nester", "tokens": ["Die", "myr\u00b7ten\u00b7be\u00b7schat\u00b7te\u00b7ten", "Nes\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und spotten am griechischen Tempelsims", "tokens": ["Und", "spot\u00b7ten", "am", "grie\u00b7chi\u00b7schen", "Tem\u00b7pel\u00b7sims"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Des rauhen, des nordischen Wintergrimms.", "tokens": ["Des", "rau\u00b7hen", ",", "des", "nor\u00b7di\u00b7schen", "Win\u00b7ter\u00b7grimms", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bald streifen sie nun mit dem Fl\u00fcgelschlag", "tokens": ["Bald", "strei\u00b7fen", "sie", "nun", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Mittelmeers blauende Wellen", "tokens": ["Des", "Mit\u00b7tel\u00b7meers", "blau\u00b7en\u00b7de", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und schwingen sich auf mit dem leuchtenden Tag", "tokens": ["Und", "schwin\u00b7gen", "sich", "auf", "mit", "dem", "leuch\u00b7ten\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In die Aetherh\u00f6hen, die hellen;", "tokens": ["In", "die", "A\u00b7e\u00b7ther\u00b7h\u00f6\u00b7hen", ",", "die", "hel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Du aber, Verwaiste, in einsamem Weh", "tokens": ["Du", "a\u00b7ber", ",", "Ver\u00b7wais\u00b7te", ",", "in", "ein\u00b7sa\u00b7mem", "Weh"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sinkst sterbend dahin auf die Felder voll Schnee!", "tokens": ["Sinkst", "ster\u00b7bend", "da\u00b7hin", "auf", "die", "Fel\u00b7der", "voll", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PAV", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}}}}