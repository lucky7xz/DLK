{"dta.poem.18242": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Das himmlische Kind,", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Als ich tr\u00e4umend im Walde gelegen,", "tokens": ["Als", "ich", "tr\u00e4u\u00b7mend", "im", "Wal\u00b7de", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und hinter ihm schritt", "tokens": ["Und", "hin\u00b7ter", "ihm", "schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit trippelndem Tritt", "tokens": ["Mit", "trip\u00b7peln\u00b7dem", "Tritt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sein Bruder, der Sommerregen.", "tokens": ["Sein", "Bru\u00b7der", ",", "der", "Som\u00b7mer\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Wipfeln da ging's", "tokens": ["In", "den", "Wip\u00b7feln", "da", "ging's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nach rechts und nach links,", "tokens": ["Nach", "rechts", "und", "nach", "links", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als wiegte der Wind sich im Bettchen,", "tokens": ["Als", "wieg\u00b7te", "der", "Wind", "sich", "im", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sein Br\u00fcderchen sang:", "tokens": ["Und", "sein", "Br\u00fc\u00b7der\u00b7chen", "sang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Di Binke di Bank,", "tokens": ["Di", "Bin\u00b7ke", "di", "Bank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und schl\u00fcpfte von Bl\u00e4ttchen zu Bl\u00e4ttchen.", "tokens": ["Und", "schl\u00fcpf\u00b7te", "von", "Bl\u00e4tt\u00b7chen", "zu", "Bl\u00e4tt\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Wei\u00df selbst nicht, wie's kam,", "tokens": ["Wei\u00df", "selbst", "nicht", ",", "wie's", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Gar zu wundersam", "tokens": ["Gar", "zu", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es regnete, tropfte und rauschte,", "tokens": ["Es", "reg\u00b7ne\u00b7te", ",", "tropf\u00b7te", "und", "rauschte", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da\u00df ich selber ein Kind,", "tokens": ["Da\u00df", "ich", "sel\u00b7ber", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie Regen und Wind,", "tokens": ["Wie", "Re\u00b7gen", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das Spielen der beiden belauschte.", "tokens": ["Das", "Spie\u00b7len", "der", "bei\u00b7den", "be\u00b7lauschte", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "ADJA", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Dann wurde es Nacht,", "tokens": ["Dann", "wur\u00b7de", "es", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und eh ich's gedacht,", "tokens": ["Und", "eh", "ich's", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Waren fort, die das M\u00e4rchen mir schufen.", "tokens": ["Wa\u00b7ren", "fort", ",", "die", "das", "M\u00e4r\u00b7chen", "mir", "schu\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihr M\u00fctterlein", "tokens": ["Ihr", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hatte sie fein", "tokens": ["Hat\u00b7te", "sie", "fein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Hinauf in den Himmel gerufen!", "tokens": ["Hin\u00b7auf", "in", "den", "Him\u00b7mel", "ge\u00b7ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}