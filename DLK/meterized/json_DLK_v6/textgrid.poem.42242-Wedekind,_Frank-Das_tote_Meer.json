{"textgrid.poem.42242": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Das tote Meer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist leer wie eine taube Nu\u00df,", "tokens": ["Mein", "Herz", "ist", "leer", "wie", "ei\u00b7ne", "tau\u00b7be", "Nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Kobold spukt darin der \u00dcberdru\u00df.", "tokens": ["Als", "Ko\u00b7bold", "spukt", "da\u00b7rin", "der", "\u00dc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich's bei Licht mir nah vors Auge halte,", "tokens": ["Wenn", "ich's", "bei", "Licht", "mir", "nah", "vors", "Au\u00b7ge", "hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bleckt er mich h\u00e4misch an aus enger Spalte.", "tokens": ["Bleckt", "er", "mich", "h\u00e4\u00b7misch", "an", "aus", "en\u00b7ger", "Spal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "An hundert Weiber hatt ich wohl im Sold,", "tokens": ["An", "hun\u00b7dert", "Wei\u00b7ber", "hatt", "ich", "wohl", "im", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit denen ich mein Gut und Blut vertollt,", "tokens": ["Mit", "de\u00b7nen", "ich", "mein", "Gut", "und", "Blut", "ver\u00b7tollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sch\u00f6nsten Nymphen im modernen Babel.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Nym\u00b7phen", "im", "mo\u00b7der\u00b7nen", "Ba\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-++-+---+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und ich blieb leer, vom Scheitel bis zum Nabel.", "tokens": ["Und", "ich", "blieb", "leer", ",", "vom", "Schei\u00b7tel", "bis", "zum", "Na\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Funke mehr, kein Stern aus fr\u00fcherer Welt;", "tokens": ["Kein", "Fun\u00b7ke", "mehr", ",", "kein", "Stern", "aus", "fr\u00fc\u00b7he\u00b7rer", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kein Fl\u00e4mmchen, das den Busen sanft erhellt.", "tokens": ["Kein", "Fl\u00e4mm\u00b7chen", ",", "das", "den", "Bu\u00b7sen", "sanft", "er\u00b7hellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Pharus ragt noch stets mit gl\u00fchnden Kohlen", "tokens": ["Nur", "Pha\u00b7rus", "ragt", "noch", "stets", "mit", "gl\u00fchn\u00b7den", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoch in die Nacht. Der Teufel soll ihn holen!", "tokens": ["Hoch", "in", "die", "Nacht", ".", "Der", "Teu\u00b7fel", "soll", "ihn", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}