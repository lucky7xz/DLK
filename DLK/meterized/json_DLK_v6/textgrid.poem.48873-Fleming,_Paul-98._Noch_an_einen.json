{"textgrid.poem.48873": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "98. Noch an einen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So reise denn auch du, du freundlicher Smaragd,", "tokens": ["So", "rei\u00b7se", "denn", "auch", "du", ",", "du", "freund\u00b7li\u00b7cher", "Sma\u00b7ragd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu meiner Freundin hin und lasse dir behagen,", "tokens": ["zu", "mei\u00b7ner", "Freun\u00b7din", "hin", "und", "las\u00b7se", "dir", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df eine solche Hand dich f\u00f6rderhin soll tragen,", "tokens": ["da\u00df", "ei\u00b7ne", "sol\u00b7che", "Hand", "dich", "f\u00f6r\u00b7der\u00b7hin", "soll", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die auch, wie keusch du bist, dich doch noch keuscher macht.", "tokens": ["die", "auch", ",", "wie", "keusch", "du", "bist", ",", "dich", "doch", "noch", "keu\u00b7scher", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei um sie, wenn sie schl\u00e4ft, sei um sie, wenn sie wacht.", "tokens": ["Sei", "um", "sie", ",", "wenn", "sie", "schl\u00e4ft", ",", "sei", "um", "sie", ",", "wenn", "sie", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft wird sie dich von mir und meiner Liebe fragen.", "tokens": ["Oft", "wird", "sie", "dich", "von", "mir", "und", "mei\u00b7ner", "Lie\u00b7be", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halt' andrer Steine Brauch, die nichts nicht wieder sagen;", "tokens": ["Halt'", "an\u00b7drer", "Stei\u00b7ne", "Brauch", ",", "die", "nichts", "nicht", "wie\u00b7der", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "NN", "$,", "PRELS", "PIS", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "schweig, was du siehst und h\u00f6rst und nim dich selbst in Acht.", "tokens": ["schweig", ",", "was", "du", "siehst", "und", "h\u00f6rst", "und", "nim", "dich", "selbst", "in", "Acht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVIMP", "PPER", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geschicht es etwan denn, da\u00df sie dir in Gedanken", "tokens": ["Ge\u00b7schicht", "es", "et\u00b7wan", "denn", ",", "da\u00df", "sie", "dir", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein feuchtes K\u00fc\u00dflein reicht, so heb' es auf f\u00fcr mich", "tokens": ["ein", "feuch\u00b7tes", "K\u00fc\u00df\u00b7lein", "reicht", ",", "so", "heb'", "es", "auf", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bis morgen gegen Nacht. Und wolten etwan sich", "tokens": ["bis", "mor\u00b7gen", "ge\u00b7gen", "Nacht", ".", "Und", "wol\u00b7ten", "et\u00b7wan", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "$.", "KON", "VMFIN", "ADV", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "die L\u00fcfte, die es sehn, hier\u00fcber mit dir zanken", "tokens": ["die", "L\u00fcf\u00b7te", ",", "die", "es", "sehn", ",", "hier\u00b7\u00fc\u00b7ber", "mit", "dir", "zan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "PAV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und mir es bringen eh', als ich mich stellet' ein,", "tokens": ["und", "mir", "es", "brin\u00b7gen", "eh'", ",", "als", "ich", "mich", "stel\u00b7let'", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so send' es mir durch sie und la\u00df es heimlich sein.", "tokens": ["so", "send'", "es", "mir", "durch", "sie", "und", "la\u00df", "es", "heim\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "KON", "VVIMP", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}