{"textgrid.poem.30449": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thraso wil, da\u00df seine Thaten sollen weit und breit erschallen,", "tokens": ["Thra\u00b7so", "wil", ",", "da\u00df", "sei\u00b7ne", "Tha\u00b7ten", "sol\u00b7len", "weit", "und", "breit", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "KOUS", "PPOSAT", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da sie hier doch keinem k\u00fcndig. Dieses ist mir beygefallen,", "tokens": ["Da", "sie", "hier", "doch", "kei\u00b7nem", "k\u00fcn\u00b7dig", ".", "Die\u00b7ses", "ist", "mir", "bey\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "ADJD", "$.", "PDS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wann er gienge zu der Oder, schriebe drein sein Thun und Wesen,", "tokens": ["Wann", "er", "gien\u00b7ge", "zu", "der", "O\u00b7der", ",", "schrie\u00b7be", "drein", "sein", "Thun", "und", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NE", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "W\u00fcrde man in wenig Tagen solches in der Ost-See lesen.", "tokens": ["W\u00fcr\u00b7de", "man", "in", "we\u00b7nig", "Ta\u00b7gen", "sol\u00b7ches", "in", "der", "Ost\u00b7See", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PIAT", "NN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}