{"textgrid.poem.47481": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein doppelter Rubin ist mein Ruin]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein doppelter Rubin ist mein Ruin;", "tokens": ["Ein", "dop\u00b7pel\u00b7ter", "Ru\u00b7bin", "ist", "mein", "Ru\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Denn nimmermehr zu meiden und zu flieh'n,", "tokens": ["Denn", "nim\u00b7mer\u00b7mehr", "zu", "mei\u00b7den", "und", "zu", "flieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist, was mir auch Vernunft und Glaube sagt,", "tokens": ["Ist", ",", "was", "mir", "auch", "Ver\u00b7nunft", "und", "Glau\u00b7be", "sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Weines hier und deines Munds Rubin.", "tokens": ["Des", "Wei\u00b7nes", "hier", "und", "dei\u00b7nes", "Munds", "Ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}