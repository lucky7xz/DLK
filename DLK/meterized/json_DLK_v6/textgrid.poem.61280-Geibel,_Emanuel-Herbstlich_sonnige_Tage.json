{"textgrid.poem.61280": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Herbstlich sonnige Tage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstlich sonnige Tage,", "tokens": ["Herbst\u00b7lich", "son\u00b7ni\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mir beschieden zur Lust,", "tokens": ["Mir", "be\u00b7schie\u00b7den", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Euch mit leiserem Schlage", "tokens": ["Euch", "mit", "lei\u00b7se\u00b7rem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gr\u00fc\u00dft die atmende Brust.", "tokens": ["Gr\u00fc\u00dft", "die", "at\u00b7men\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "O wie waltet die Stunde", "tokens": ["O", "wie", "wal\u00b7tet", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Nun in seliger Ruh'!", "tokens": ["Nun", "in", "se\u00b7li\u00b7ger", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Jede schmerzende Wunde", "tokens": ["Je\u00b7de", "schmer\u00b7zen\u00b7de", "Wun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schlie\u00dfet leise sich zu.", "tokens": ["Schlie\u00b7\u00dfet", "lei\u00b7se", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Nur zu rasten, zu lieben,", "tokens": ["Nur", "zu", "ras\u00b7ten", ",", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Still an sich selber zu baun", "tokens": ["Still", "an", "sich", "sel\u00b7ber", "zu", "baun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "F\u00fchlt sich die Seele getrieben", "tokens": ["F\u00fchlt", "sich", "die", "See\u00b7le", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und mit Liebe zu ", "tokens": ["Und", "mit", "Lie\u00b7be", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und so schreit' ich im Tale,", "tokens": ["Und", "so", "schreit'", "ich", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In den Bergen, am Bach", "tokens": ["In", "den", "Ber\u00b7gen", ",", "am", "Bach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jedem segnenden Strahle,", "tokens": ["Je\u00b7dem", "seg\u00b7nen\u00b7den", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jedem verzehrenden nach.", "tokens": ["Je\u00b7dem", "ver\u00b7zeh\u00b7ren\u00b7den", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Jedem leisen Verf\u00e4rben", "tokens": ["Je\u00b7dem", "lei\u00b7sen", "Ver\u00b7f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Lausch' ich mit stillem Bem\u00fchn,", "tokens": ["Lausch'", "ich", "mit", "stil\u00b7lem", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Jedem Wachsen und Sterben,", "tokens": ["Je\u00b7dem", "Wach\u00b7sen", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jedem Welken und Bl\u00fchn.", "tokens": ["Je\u00b7dem", "Wel\u00b7ken", "und", "Bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Selig lern' ich es sp\u00fcren,", "tokens": ["Se\u00b7lig", "lern'", "ich", "es", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wie die Sch\u00f6pfung entlang", "tokens": ["Wie", "die", "Sch\u00f6p\u00b7fung", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPO"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Geist und Welt sich ber\u00fchren", "tokens": ["Geist", "und", "Welt", "sich", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PRF", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zu harmonischem Klang.", "tokens": ["Zu", "har\u00b7mo\u00b7ni\u00b7schem", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was da webet im Ringe,", "tokens": ["Was", "da", "we\u00b7bet", "im", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Was da bl\u00fcht auf der Flur,", "tokens": ["Was", "da", "bl\u00fcht", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sinnbild ewiger Dinge", "tokens": ["Sinn\u00b7bild", "e\u00b7wi\u00b7ger", "Din\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist's dem Schauenden nur.", "tokens": ["Ist's", "dem", "Schau\u00b7en\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Jede sprossende Pflanze,", "tokens": ["Je\u00b7de", "spros\u00b7sen\u00b7de", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die mit D\u00fcften sich f\u00fcllt,", "tokens": ["Die", "mit", "D\u00fcf\u00b7ten", "sich", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tr\u00e4gt im Kelche das ganze", "tokens": ["Tr\u00e4gt", "im", "Kel\u00b7che", "das", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Weltgeheimnis verh\u00fcllt.", "tokens": ["Welt\u00b7ge\u00b7heim\u00b7nis", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Schweigend blickt's aus der Klippe,", "tokens": ["Schwei\u00b7gend", "blickt's", "aus", "der", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Spricht im Wellengebraus,", "tokens": ["Spricht", "im", "Wel\u00b7len\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch mit heiliger Lippe", "tokens": ["Doch", "mit", "hei\u00b7li\u00b7ger", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deutet die Mus' es aus.", "tokens": ["Deu\u00b7tet", "die", "Mus'", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}