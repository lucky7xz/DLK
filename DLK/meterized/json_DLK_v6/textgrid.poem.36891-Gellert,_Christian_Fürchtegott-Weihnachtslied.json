{"textgrid.poem.36891": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtslied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, schicke dich,", "tokens": ["Auf", ",", "schi\u00b7cke", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Recht feierlich", "tokens": ["Recht", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Heilands Fest mit Danken zu begehen!", "tokens": ["Des", "Hei\u00b7lands", "Fest", "mit", "Dan\u00b7ken", "zu", "be\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lieb ist der Dank,", "tokens": ["Lieb", "ist", "der", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Lobgesang,", "tokens": ["Der", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch den wir ihn, den Gott der Lieb, erh\u00f6hen.", "tokens": ["Durch", "den", "wir", "ihn", ",", "den", "Gott", "der", "Lieb", ",", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sprich dankbar froh:", "tokens": ["Sprich", "dank\u00b7bar", "froh", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Also, also", "tokens": ["Al\u00b7so", ",", "al\u00b7so"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Hat Gott die Welt in seinem Sohn geliebet!", "tokens": ["Hat", "Gott", "die", "Welt", "in", "sei\u00b7nem", "Sohn", "ge\u00b7lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O, wer bin ich,", "tokens": ["O", ",", "wer", "bin", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Herr, da\u00df du mich", "tokens": ["Herr", ",", "da\u00df", "du", "mich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF"], "meter": "---+", "measure": "unknown.measure.single"}, "line.6": {"text": "So herrlich hoch in deinem Sohn geliebet?", "tokens": ["So", "herr\u00b7lich", "hoch", "in", "dei\u00b7nem", "Sohn", "ge\u00b7lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er, unser Freund,", "tokens": ["Er", ",", "un\u00b7ser", "Freund", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit uns vereint,", "tokens": ["Mit", "uns", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zur Zeit, da wir noch seine Feinde waren;", "tokens": ["Zur", "Zeit", ",", "da", "wir", "noch", "sei\u00b7ne", "Fein\u00b7de", "wa\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er wird uns gleich,", "tokens": ["Er", "wird", "uns", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Um Gottes Reich", "tokens": ["Um", "Got\u00b7tes", "Reich"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und seine Lieb im Fleisch zu offenbaren.", "tokens": ["Und", "sei\u00b7ne", "Lieb", "im", "Fleisch", "zu", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An ihm nimm teil,", "tokens": ["An", "ihm", "nimm", "teil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVIMP", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Er ist das Heil;", "tokens": ["Er", "ist", "das", "Heil", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tu t\u00e4glich Bu\u00df und gl\u00e4ub an seinen Namen.", "tokens": ["Tu", "t\u00e4g\u00b7lich", "Bu\u00df", "und", "gl\u00e4ub", "an", "sei\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ehrt ihn nicht,", "tokens": ["Der", "ehrt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wer Herr, Herr, spricht,", "tokens": ["Wer", "Herr", ",", "Herr", ",", "spricht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und doch nicht sucht sein Beispiel nachzuahmen.", "tokens": ["Und", "doch", "nicht", "sucht", "sein", "Bei\u00b7spiel", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Aus Dank will ich", "tokens": ["Aus", "Dank", "will", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Br\u00fcdern dich,", "tokens": ["In", "Br\u00fc\u00b7dern", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dich, Gottessohn, bekleiden, speisen, tr\u00e4nken;", "tokens": ["Dich", ",", "Got\u00b7tes\u00b7sohn", ",", "be\u00b7klei\u00b7den", ",", "spei\u00b7sen", ",", "tr\u00e4n\u00b7ken", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Frommen Herz", "tokens": ["Der", "From\u00b7men", "Herz"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In ihrem Schmerz", "tokens": ["In", "ih\u00b7rem", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit Trost erfreun, und dein dabei gedenken.", "tokens": ["Mit", "Trost", "er\u00b7freun", ",", "und", "dein", "da\u00b7bei", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "KON", "PPOSAT", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Rat, Kraft und Held,", "tokens": ["Rat", ",", "Kraft", "und", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durch den die Welt", "tokens": ["Durch", "den", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und alles ist, im Himmel und auf Erden!", "tokens": ["Und", "al\u00b7les", "ist", ",", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "APPRART", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Christenheit", "tokens": ["Die", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Preist dich erfreut,", "tokens": ["Preist", "dich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und aller Knie soll dir gebeuget werden.", "tokens": ["Und", "al\u00b7ler", "Knie", "soll", "dir", "ge\u00b7beu\u00b7get", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Erhebt den Herrn!", "tokens": ["Er\u00b7hebt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Er hilft uns gern,", "tokens": ["Er", "hilft", "uns", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wer ihn sucht, den wird sein Name tr\u00f6sten.", "tokens": ["Und", "wer", "ihn", "sucht", ",", "den", "wird", "sein", "Na\u00b7me", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Alleluja!", "tokens": ["Al\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Alleluja!", "tokens": ["Al\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Freut euch des Herrn, und jauchzt ihm, ihr Erl\u00f6sten!", "tokens": ["Freut", "euch", "des", "Herrn", ",", "und", "jauchzt", "ihm", ",", "ihr", "Er\u00b7l\u00f6s\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}