{"textgrid.poem.42221": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum dr\u00e4ngst du dich in meine Tr\u00e4ume?", "tokens": ["Wa\u00b7rum", "dr\u00e4ngst", "du", "dich", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum hemmst du meiner Schritte Lauf?", "tokens": ["Wa\u00b7rum", "hemmst", "du", "mei\u00b7ner", "Schrit\u00b7te", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Warum f\u00fcllst du alle Himmelsr\u00e4ume,", "tokens": ["Wa\u00b7rum", "f\u00fcllst", "du", "al\u00b7le", "Him\u00b7mels\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blick ich n\u00e4chtens zu den Sternen auf?", "tokens": ["Blick", "ich", "n\u00e4ch\u00b7tens", "zu", "den", "Ster\u00b7nen", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "St\u00f6r ich deiner Seele heil'gen Frieden,", "tokens": ["St\u00f6r", "ich", "dei\u00b7ner", "See\u00b7le", "heil'\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum machst du, M\u00e4dchen, dich so breit?", "tokens": ["Wa\u00b7rum", "machst", "du", ",", "M\u00e4d\u00b7chen", ",", "dich", "so", "breit", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und \u00bbNicht doch!\u00ab entgegnest du entschieden", "tokens": ["Und", "\u00bb", "Nicht", "doch", "!", "\u00ab", "ent\u00b7geg\u00b7nest", "du", "ent\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "$(", "PTKNEG", "ADV", "$.", "$(", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie der Genius der Enthaltsamkeit.", "tokens": ["Wie", "der", "Ge\u00b7nius", "der", "Ent\u00b7halt\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach, so kann es nicht mehr lange dauern;", "tokens": ["Ach", ",", "so", "kann", "es", "nicht", "mehr", "lan\u00b7ge", "dau\u00b7ern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach, es w\u00e4lzt sich drohend Ach auf Ach;", "tokens": ["Ach", ",", "es", "w\u00e4lzt", "sich", "dro\u00b7hend", "Ach", "auf", "Ach", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PRF", "VVPP", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df dir deine Zimmert\u00fcr vermauern,", "tokens": ["La\u00df", "dir", "dei\u00b7ne", "Zim\u00b7mer\u00b7t\u00fcr", "ver\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder f\u00fcrchte den Zusammenkrach.", "tokens": ["O\u00b7der", "f\u00fcrch\u00b7te", "den", "Zu\u00b7sam\u00b7men\u00b7krach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}}}}