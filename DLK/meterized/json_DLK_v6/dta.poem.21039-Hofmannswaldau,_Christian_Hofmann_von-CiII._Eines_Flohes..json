{"dta.poem.21039": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "CiII.  \n Eines Flohes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein schwartzer Rittersmann fiel durch ein weisses", "tokens": ["Ein", "schwart\u00b7zer", "Rit\u00b7ters\u00b7mann", "fiel", "durch", "ein", "weis\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weib/", "tokens": ["Weib", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In dem er ohne Scheu betrat den zarten Leib.", "tokens": ["In", "dem", "er", "oh\u00b7ne", "Scheu", "be\u00b7trat", "den", "zar\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ist sein alter Ruhm nicht gantz und gar verdor-", "tokens": ["Doch", "ist", "sein", "al\u00b7ter", "Ruhm", "nicht", "gantz", "und", "gar", "ver\u00b7dor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.6": {"text": "In dem er eben so wie ", "tokens": ["In", "dem", "er", "e\u00b7ben", "so", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "KOKOM"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}