{"dta.poem.10406": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mein s\u00fc\u00dfes Lieb, wenn du im Grab,", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Lieb", ",", "wenn", "du", "im", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im dunkeln Grab wirst liegen,", "tokens": ["Im", "dun\u00b7keln", "Grab", "wirst", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann steig' ich langsam zu dir hinab,", "tokens": ["Dann", "steig'", "ich", "lang\u00b7sam", "zu", "dir", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und will mich an dich schmiegen.", "tokens": ["Und", "will", "mich", "an", "dich", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kuss', ich umschlinge, ich presse dich wild,", "tokens": ["Ich", "kuss'", ",", "ich", "um\u00b7schlin\u00b7ge", ",", "ich", "pres\u00b7se", "dich", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du Stille, du Kalte, du Bleiche!", "tokens": ["Du", "Stil\u00b7le", ",", "du", "Kal\u00b7te", ",", "du", "Blei\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich jauchze, ich zitt're, ich weine mild,", "tokens": ["Ich", "jauch\u00b7ze", ",", "ich", "zitt'\u00b7re", ",", "ich", "wei\u00b7ne", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich werde selber zur Leiche.", "tokens": ["Ich", "wer\u00b7de", "sel\u00b7ber", "zur", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Todten stehn auf, die Mitternacht ruft,", "tokens": ["Die", "Tod\u00b7ten", "stehn", "auf", ",", "die", "Mit\u00b7ter\u00b7nacht", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie tanzen im luftigen Schwarme;", "tokens": ["Sie", "tan\u00b7zen", "im", "luf\u00b7ti\u00b7gen", "Schwar\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir beide bleiben in der Gruft,", "tokens": ["Wir", "bei\u00b7de", "blei\u00b7ben", "in", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich liege in deinem Arme.", "tokens": ["Ich", "lie\u00b7ge", "in", "dei\u00b7nem", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Todten stehn auf, der Tag des Gerichts", "tokens": ["Die", "Tod\u00b7ten", "stehn", "auf", ",", "der", "Tag", "des", "Ge\u00b7richts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ruft sie zu Qual und Vergn\u00fcgen;", "tokens": ["Ruft", "sie", "zu", "Qual", "und", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wir beide bek\u00fcmmern uns um nichts,", "tokens": ["Wir", "bei\u00b7de", "be\u00b7k\u00fcm\u00b7mern", "uns", "um", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und bleiben umschlungen liegen.", "tokens": ["Und", "blei\u00b7ben", "um\u00b7schlun\u00b7gen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}