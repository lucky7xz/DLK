{"textgrid.poem.33743": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "23. Einziges", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein eigner Schauer h\u00e4lt mich stets beklommen,", "tokens": ["Ein", "eig\u00b7ner", "Schau\u00b7er", "h\u00e4lt", "mich", "stets", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich an Jene denk', die das gesehen,", "tokens": ["Wenn", "ich", "an", "Je\u00b7ne", "denk'", ",", "die", "das", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDS", "VVFIN", "$,", "PRELS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was niemand mehr, so lang die Welt wird stehen,", "tokens": ["Was", "nie\u00b7mand", "mehr", ",", "so", "lang", "die", "Welt", "wird", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Ein Stern \u2013 nur einmal war sein Licht entglommen,", "tokens": ["Ein", "Stern", "\u2013", "nur", "ein\u00b7mal", "war", "sein", "Licht", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sah f\u00fcr alle Zeit ihn untergehen;", "tokens": ["Man", "sah", "f\u00fcr", "al\u00b7le", "Zeit", "ihn", "un\u00b7ter\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kunstwerk \u2013 das der Zeit Verw\u00fcstungswehen", "tokens": ["Ein", "Kunst\u00b7werk", "\u2013", "das", "der", "Zeit", "Ver\u00b7w\u00fcs\u00b7tungs\u00b7we\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PDS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr immer unserm Blick hinweggenommen.", "tokens": ["F\u00fcr", "im\u00b7mer", "un\u00b7serm", "Blick", "hin\u00b7weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Begl\u00fcckt, wer Sappho's Lieder noch als Ganzes,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", "Sap\u00b7pho's", "Lie\u00b7der", "noch", "als", "Gan\u00b7zes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "NE", "NN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer Phidias' Werke sah, als unzerst\u00fcckte,", "tokens": ["Wer", "Phi\u00b7dias'", "Wer\u00b7ke", "sah", ",", "als", "un\u00b7zer\u00b7st\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$,", "KOUS", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Vinci's Bild in vollem Farbenglanze!", "tokens": ["Da", "Vin\u00b7ci's", "Bild", "in", "vol\u00b7lem", "Far\u00b7ben\u00b7glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und gl\u00fccklicher, wem einst zu schauen gl\u00fcckte", "tokens": ["Und", "gl\u00fcck\u00b7li\u00b7cher", ",", "wem", "einst", "zu", "schau\u00b7en", "gl\u00fcck\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWS", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Helena im Schmuck des Hochzeitkranzes,", "tokens": ["Die", "He\u00b7le\u00b7na", "im", "Schmuck", "des", "Hoch\u00b7zeit\u00b7kran\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wen ein Blick Kleopatra's entz\u00fcckte!", "tokens": ["Und", "wen", "ein", "Blick", "Kle\u00b7o\u00b7pa\u00b7tra's", "ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}