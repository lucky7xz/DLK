{"textgrid.poem.39118": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fcgenm\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst stieg ich einen Berg hinan,", "tokens": ["J\u00fcngst", "stieg", "ich", "ei\u00b7nen", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sah ich da!", "tokens": ["Was", "sah", "ich", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich sah ein allerliebstes Land,", "tokens": ["Ich", "sah", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wein wuchs an der Mauer,", "tokens": ["Der", "Wein", "wuchs", "an", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dicht am Throne, rechter Hand,", "tokens": ["Und", "dicht", "am", "Thro\u00b7ne", ",", "rech\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stand B\u00fcrgersmann und Bauer.", "tokens": ["Stand", "B\u00fcr\u00b7gers\u00b7mann", "und", "Bau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wunder \u00fcber Wunder!", "tokens": ["Wun\u00b7der", "\u00fc\u00b7ber", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Keine Barone", "tokens": ["Kei\u00b7ne", "Ba\u00b7ro\u00b7ne"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Neben dem Throne?", "tokens": ["Ne\u00b7ben", "dem", "Thro\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Unterdessen nimmt mich's wunder.", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "nimmt", "mich's", "wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und weiter stieg ich frisch hinan,", "tokens": ["Und", "wei\u00b7ter", "stieg", "ich", "frisch", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sah ich da!", "tokens": ["Was", "sah", "ich", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Kein Leutnant war, kein F\u00e4hnrich dort", "tokens": ["Kein", "Leut\u00b7nant", "war", ",", "kein", "F\u00e4hn\u00b7rich", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kein Rekrut zu sehen,", "tokens": ["Und", "kein", "Re\u00b7krut", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man wu\u00dfte nicht das kleinste Wort", "tokens": ["Man", "wu\u00df\u00b7te", "nicht", "das", "kleins\u00b7te", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von stehenden Armeen.", "tokens": ["Von", "ste\u00b7hen\u00b7den", "Ar\u00b7me\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Wunder \u00fcber Wunder!", "tokens": ["Wun\u00b7der", "\u00fc\u00b7ber", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Keine Barone", "tokens": ["Kei\u00b7ne", "Ba\u00b7ro\u00b7ne"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Neben dem Throne?", "tokens": ["Ne\u00b7ben", "dem", "Thro\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Gl\u00fcckliche Staaten", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", "Staa\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ohne Soldaten?", "tokens": ["Oh\u00b7ne", "Sol\u00b7da\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Unterdessen nimmt mich's wunder.", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen"