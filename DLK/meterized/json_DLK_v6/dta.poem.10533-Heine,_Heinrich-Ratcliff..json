{"dta.poem.10533": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ratcliff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Traumgott brachte mich in eine Landschaft,", "tokens": ["Der", "Traum\u00b7gott", "brach\u00b7te", "mich", "in", "ei\u00b7ne", "Land\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Trauerweiden mir \u201eWillkommen\u201c winkten,", "tokens": ["Wo", "Trau\u00b7er\u00b7wei\u00b7den", "mir", "\u201e", "Will\u00b7kom\u00b7men", "\u201c", "wink\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihren langen, gr\u00fcnen Armen, wo die Blumen", "tokens": ["Mit", "ih\u00b7ren", "lan\u00b7gen", ",", "gr\u00fc\u00b7nen", "Ar\u00b7men", ",", "wo", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit klugen Schwesteraugen still mich ansah'n,", "tokens": ["Mit", "klu\u00b7gen", "Schwes\u00b7ter\u00b7au\u00b7gen", "still", "mich", "an\u00b7sah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo mir vertraulich klang der V\u00f6gel Zwitschern,", "tokens": ["Wo", "mir", "ver\u00b7trau\u00b7lich", "klang", "der", "V\u00f6\u00b7gel", "Zwit\u00b7schern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo gar der Hunde Bellen mir bekannt schien,", "tokens": ["Wo", "gar", "der", "Hun\u00b7de", "Bel\u00b7len", "mir", "be\u00b7kannt", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Und Stimmen und Gestalten mich begr\u00fc\u00dften,", "tokens": ["Und", "Stim\u00b7men", "und", "Ge\u00b7stal\u00b7ten", "mich", "be\u00b7gr\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einen alten Freund, und wo doch Alles", "tokens": ["Wie", "ei\u00b7nen", "al\u00b7ten", "Freund", ",", "und", "wo", "doch", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "KON", "PWAV", "ADV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So fremd mir schien, so wunderseltsam fremd.", "tokens": ["So", "fremd", "mir", "schien", ",", "so", "wun\u00b7der\u00b7selt\u00b7sam", "fremd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor einem l\u00e4ndlich schmucken Hause stand ich,", "tokens": ["Vor", "ei\u00b7nem", "l\u00e4nd\u00b7lich", "schmu\u00b7cken", "Hau\u00b7se", "stand", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In meiner Brust bewegte sich's, im Kopfe", "tokens": ["In", "mei\u00b7ner", "Brust", "be\u00b7weg\u00b7te", "sich's", ",", "im", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "War's ruhig, ruhig sch\u00fcttelte ich ab", "tokens": ["Wa\u00b7r's", "ru\u00b7hig", ",", "ru\u00b7hig", "sch\u00fct\u00b7tel\u00b7te", "ich", "ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Den Staub von meinen Reisekleidern,", "tokens": ["Den", "Staub", "von", "mei\u00b7nen", "Rei\u00b7se\u00b7klei\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dumpf klang die Klingel, und die Th\u00fcr ging auf.", "tokens": ["Dumpf", "klang", "die", "Klin\u00b7gel", ",", "und", "die", "Th\u00fcr", "ging", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Da waren M\u00e4nner, Frauen, viel bekannte", "tokens": ["Da", "wa\u00b7ren", "M\u00e4n\u00b7ner", ",", "Frau\u00b7en", ",", "viel", "be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesichter. Stiller Kummer lag auf allen", "tokens": ["Ge\u00b7sich\u00b7ter", ".", "Stil\u00b7ler", "Kum\u00b7mer", "lag", "auf", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "NN", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und heimlich scheue Angst. Seltsam verst\u00f6rt,", "tokens": ["Und", "heim\u00b7lich", "scheu\u00b7e", "Angst", ".", "Selt\u00b7sam", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$.", "NE", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Beileidsmienen fast, sah'n sie mich an,", "tokens": ["Mit", "Bei\u00b7leids\u00b7mie\u00b7nen", "fast", ",", "sah'n", "sie", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df es mir selber durch die Seele schauert',", "tokens": ["Da\u00df", "es", "mir", "sel\u00b7ber", "durch", "die", "See\u00b7le", "schau\u00b7ert'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Ahnung eines unbekannten Unheils.", "tokens": ["Wie", "Ah\u00b7nung", "ei\u00b7nes", "un\u00b7be\u00b7kann\u00b7ten", "Un\u00b7heils", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die alte Marg'reth hab' ich gleich erkannt;", "tokens": ["Die", "al\u00b7te", "Mar\u00b7g'\u00b7reth", "hab'", "ich", "gleich", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sah sie forschend an, jedoch sie sprach nicht.", "tokens": ["Ich", "sah", "sie", "for\u00b7schend", "an", ",", "je\u00b7doch", "sie", "sprach", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ADV", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201ewo ist Maria?\u201c fragt' ich, doch sie sprach nicht,", "tokens": ["\u201e", "wo", "ist", "Ma\u00b7ria", "?", "\u201c", "fragt'", "ich", ",", "doch", "sie", "sprach", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "NE", "$.", "$(", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Griff leise meine Hand, und f\u00fchrte mich", "tokens": ["Griff", "lei\u00b7se", "mei\u00b7ne", "Hand", ",", "und", "f\u00fchr\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch viele lange, leuchtende Gem\u00e4cher,", "tokens": ["Durch", "vie\u00b7le", "lan\u00b7ge", ",", "leuch\u00b7ten\u00b7de", "Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Prunk und Pracht und Todtenstille herrschte,", "tokens": ["Wo", "Prunk", "und", "Pracht", "und", "Tod\u00b7ten\u00b7stil\u00b7le", "herrschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und f\u00fchrt' mich endlich in ein d\u00e4mmernd Zimmer,", "tokens": ["Und", "f\u00fchrt'", "mich", "end\u00b7lich", "in", "ein", "d\u00e4m\u00b7mernd", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und zeigt' mit abgewandtem Angesicht',", "tokens": ["Und", "zeigt'", "mit", "ab\u00b7ge\u00b7wand\u00b7tem", "An\u00b7ge\u00b7sicht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nach der Gestalt, die auf dem Sopha sa\u00df.", "tokens": ["Nach", "der", "Ge\u00b7stalt", ",", "die", "auf", "dem", "So\u00b7pha", "sa\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "\u201esind Sie Maria?\u201c fragt' ich. Innerlich", "tokens": ["\u201e", "sind", "Sie", "Ma\u00b7ria", "?", "\u201c", "fragt'", "ich", ".", "In\u00b7ner\u00b7lich"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["$(", "VAFIN", "PPER", "NE", "$.", "$(", "VVFIN", "PPER", "$.", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Erstaunt' ich selber ob der Festigkeit,", "tokens": ["Er\u00b7staunt'", "ich", "sel\u00b7ber", "ob", "der", "Fes\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Womit ich sprach. Und steinern und metalllos", "tokens": ["Wo\u00b7mit", "ich", "sprach", ".", "Und", "stei\u00b7nern", "und", "me\u00b7tall\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "KON", "VVINF", "KON", "NE"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.13": {"text": "Scholl eine Stimm': \u201eSo nennen mich die Leute.\u201c", "tokens": ["Scholl", "ei\u00b7ne", "Stimm'", ":", "\u201e", "So", "nen\u00b7nen", "mich", "die", "Leu\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ein schneidend Weh durchfr\u00f6stelte mich da,", "tokens": ["Ein", "schnei\u00b7dend", "Weh", "durch\u00b7fr\u00f6s\u00b7tel\u00b7te", "mich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Denn jener hohle, kalte Ton war doch \u2014", "tokens": ["Denn", "je\u00b7ner", "hoh\u00b7le", ",", "kal\u00b7te", "Ton", "war", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "$,", "ADJA", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die einst so s\u00fc\u00dfe Stimme von Maria!", "tokens": ["Die", "einst", "so", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", "von", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und jenes Weib im fahlen Lillakleid,", "tokens": ["Und", "je\u00b7nes", "Weib", "im", "fah\u00b7len", "Lil\u00b7lak\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Nachl\u00e4ssig angezogen, Busen schlotternd,", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "an\u00b7ge\u00b7zo\u00b7gen", ",", "Bu\u00b7sen", "schlot\u00b7ternd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die Augen gl\u00e4sern starr, die Wangenmuskeln", "tokens": ["Die", "Au\u00b7gen", "gl\u00e4\u00b7sern", "starr", ",", "die", "Wan\u00b7gen\u00b7mus\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Des wei\u00dfen Angesichtes lederschlaff \u2014", "tokens": ["Des", "wei\u00b7\u00dfen", "An\u00b7ge\u00b7sich\u00b7tes", "le\u00b7der\u00b7schlaff"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ach, jenes Weib war doch die einst so sch\u00f6ne,", "tokens": ["Ach", ",", "je\u00b7nes", "Weib", "war", "doch", "die", "einst", "so", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDAT", "NN", "VAFIN", "ADV", "ART", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die bl\u00fchend holde, liebliche Maria!", "tokens": ["Die", "bl\u00fc\u00b7hend", "hol\u00b7de", ",", "lieb\u00b7li\u00b7che", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u201esie waren lang auf Reisen!\u201c sprach sie laut,", "tokens": ["\u201e", "sie", "wa\u00b7ren", "lang", "auf", "Rei\u00b7sen", "!", "\u201c", "sprach", "sie", "laut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "APPR", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mit kalt unheimlicher Vertraulichkeit,", "tokens": ["Mit", "kalt", "un\u00b7heim\u00b7li\u00b7cher", "Ver\u00b7trau\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "\u201esie schaun nicht mehr so schmachtend, liebster Freund,", "tokens": ["\u201e", "sie", "schaun", "nicht", "mehr", "so", "schmach\u00b7tend", ",", "liebs\u00b7ter", "Freund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sie sind gesund, und pralle Lend' und Wade", "tokens": ["Sie", "sind", "ge\u00b7sund", ",", "und", "pral\u00b7le", "Lend'", "und", "Wa\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ADJA"