{"textgrid.poem.65238": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Teufelsbanner", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Teufel dacht' in seinem Sinn,", "tokens": ["Der", "Teu\u00b7fel", "dacht'", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sollt' ein Fr\u00f6mmler werden,", "tokens": ["Ich", "sollt'", "ein", "Fr\u00f6mm\u00b7ler", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weil ich's nicht geworden bin,", "tokens": ["Und", "weil", "ich's", "nicht", "ge\u00b7wor\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PTKNEG", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So zieht er mir Geberden,", "tokens": ["So", "zieht", "er", "mir", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zeigt Rosenkr\u00e4nz' und Gei\u00dfeln mir", "tokens": ["Zeigt", "Ro\u00b7sen\u00b7kr\u00e4nz'", "und", "Gei\u00b7\u00dfeln", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KON", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und thut sich drehn und b\u00fccken;", "tokens": ["Und", "thut", "sich", "drehn", "und", "b\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich sitze bei dem Glase hier", "tokens": ["Ich", "sit\u00b7ze", "bei", "dem", "Gla\u00b7se", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und spotte seiner T\u00fccken.", "tokens": ["Und", "spot\u00b7te", "sei\u00b7ner", "T\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Teufel fiel es wieder ein,", "tokens": ["Dem", "Teu\u00b7fel", "fiel", "es", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kriechen mir zu lehren;", "tokens": ["Das", "Krie\u00b7chen", "mir", "zu", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er pfiff und lockte grob und fein,", "tokens": ["Er", "pfiff", "und", "lock\u00b7te", "grob", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach von hohen Ehren.", "tokens": ["Und", "sprach", "von", "ho\u00b7hen", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Flugs warf ich in die Brust mich recht", "tokens": ["Flugs", "warf", "ich", "in", "die", "Brust", "mich", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und reckt' empor den Nacken,", "tokens": ["Und", "reckt'", "em\u00b7por", "den", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Trank Pereat dem Wurmgeschlecht:", "tokens": ["Trank", "Pe\u00b7reat", "dem", "Wurm\u00b7ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da wies er mir die Hacken.", "tokens": ["Da", "wies", "er", "mir", "die", "Ha\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da endlich, Br\u00fcder, wollt' er mich", "tokens": ["Da", "end\u00b7lich", ",", "Br\u00fc\u00b7der", ",", "wollt'", "er", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Diplomaten machen,", "tokens": ["Zum", "Dip\u00b7lo\u00b7ma\u00b7ten", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4hnte schon: Jetzt hab' ich dich!", "tokens": ["Und", "w\u00e4hn\u00b7te", "schon", ":", "Jetzt", "hab'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lacht' und lie\u00df ihn lachen.", "tokens": ["Ich", "lacht'", "und", "lie\u00df", "ihn", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er f\u00fchrte mich zu einem Schmaus", "tokens": ["Er", "f\u00fchr\u00b7te", "mich", "zu", "ei\u00b7nem", "Schmaus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit gro\u00dfen Diplomaten:", "tokens": ["Mit", "gro\u00b7\u00dfen", "Dip\u00b7lo\u00b7ma\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich trank die besten Flaschen aus,", "tokens": ["Ich", "trank", "die", "bes\u00b7ten", "Fla\u00b7schen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und a\u00df den feinsten Braten.", "tokens": ["Und", "a\u00df", "den", "feins\u00b7ten", "Bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun will er in Verzweiflung heut'", "tokens": ["Nun", "will", "er", "in", "Ver\u00b7zwei\u00b7flung", "heut'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Dichter mich kreiren,", "tokens": ["Zum", "Dich\u00b7ter", "mich", "krei\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und meint, ich soll aus Dankbarkeit", "tokens": ["Und", "meint", ",", "ich", "soll", "aus", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn weidlich honoriren.", "tokens": ["Ihn", "weid\u00b7lich", "ho\u00b7no\u00b7ri\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber lass' in hellem Ton", "tokens": ["Ich", "a\u00b7ber", "lass'", "in", "hel\u00b7lem", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein frohes Lied erklingen:", "tokens": ["Mein", "fro\u00b7hes", "Lied", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Herr Satanas, ich singe schon!", "tokens": ["Herr", "Sa\u00b7ta\u00b7nas", ",", "ich", "sin\u00b7ge", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jetzt r\u00fchre deine Schwingen!", "tokens": ["Jetzt", "r\u00fch\u00b7re", "dei\u00b7ne", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lustig leben, selig sterben,", "tokens": ["Lus\u00b7tig", "le\u00b7ben", ",", "se\u00b7lig", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hei\u00dft des Teufels Spiel verderben.", "tokens": ["Hei\u00dft", "des", "Teu\u00b7fels", "Spiel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}