{"textgrid.poem.54867": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "81. An Melanien", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst/ warumb ich dich/ ", "tokens": ["Du", "fragst", "/", "wa\u00b7rumb", "ich", "dich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die auff des Vaters Sarg der Mutter Grab belachte?", "tokens": ["Die", "auff", "des", "Va\u00b7ters", "Sarg", "der", "Mut\u00b7ter", "Grab", "be\u00b7lach\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den sch\u00e4tz ich sonder Witz/ der Libens wehrt kan sch\u00e4tzen", "tokens": ["Den", "sch\u00e4tz", "ich", "son\u00b7der", "Witz", "/", "der", "Li\u00b7bens", "wehrt", "kan", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die/ die man anders nicht den sterbend kan ergetzen.", "tokens": ["Die", "/", "die", "man", "an\u00b7ders", "nicht", "den", "ster\u00b7bend", "kan", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PIS", "ADV", "PTKNEG", "ART", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}