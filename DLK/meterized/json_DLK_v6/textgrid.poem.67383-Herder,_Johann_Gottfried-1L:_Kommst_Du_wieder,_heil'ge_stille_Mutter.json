{"textgrid.poem.67383": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommst Du wieder, heil'ge stille Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst Du wieder, heil'ge stille Mutter", "tokens": ["Kommst", "Du", "wie\u00b7der", ",", "heil'\u00b7ge", "stil\u00b7le", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Gestirn' und himmlischer Gedanken,", "tokens": ["Der", "Ge\u00b7stirn'", "und", "himm\u00b7li\u00b7scher", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kommst Du zu uns wieder? Dich erwartet", "tokens": ["Kommst", "Du", "zu", "uns", "wie\u00b7der", "?", "Dich", "er\u00b7war\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lechzend schon die Erd', und ihre Blumen", "tokens": ["Lech\u00b7zend", "schon", "die", "Erd'", ",", "und", "ih\u00b7re", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Beugen matt ihr Haupt, aus Deinem Kelche", "tokens": ["Beu\u00b7gen", "matt", "ihr", "Haupt", ",", "aus", "Dei\u00b7nem", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nur zwei Tropfen Himmelsthau zu kosten;", "tokens": ["Nur", "zwei", "Trop\u00b7fen", "Him\u00b7mel\u00b7sthau", "zu", "kos\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und mit ihnen neiget sich ermattet", "tokens": ["Und", "mit", "ih\u00b7nen", "nei\u00b7get", "sich", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meine bilder\u00fcberf\u00fcllte Seele,", "tokens": ["Mei\u00b7ne", "bil\u00b7de\u00b7r\u00fc\u00b7berf\u00b7\u00fcll\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Harrend, da\u00df Dein sanfter Schwamm sie l\u00f6sche", "tokens": ["Har\u00b7rend", ",", "da\u00df", "Dein", "sanf\u00b7ter", "Schwamm", "sie", "l\u00f6\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und mit Bildern andrer Welten tr\u00e4nke", "tokens": ["Und", "mit", "Bil\u00b7dern", "an\u00b7drer", "Wel\u00b7ten", "tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und mein lechzend Herz mit Ruhe labe.", "tokens": ["Und", "mein", "lech\u00b7zend", "Herz", "mit", "Ru\u00b7he", "la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sternenreiche, goldgekr\u00f6nte G\u00f6ttin,", "tokens": ["Ster\u00b7nen\u00b7rei\u00b7che", ",", "gold\u00b7ge\u00b7kr\u00f6n\u00b7te", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du, auf deren schwarzem, weitem Mantel", "tokens": ["Du", ",", "auf", "de\u00b7ren", "schwar\u00b7zem", ",", "wei\u00b7tem", "Man\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend Welten funkeln, die Du alle", "tokens": ["Tau\u00b7send", "Wel\u00b7ten", "fun\u00b7keln", ",", "die", "Du", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "$,", "PRELS", "PPER", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sanft gebarest und ihr rastlos Wesen,", "tokens": ["Sanft", "ge\u00b7ba\u00b7rest", "und", "ihr", "rast\u00b7los", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihren Feuerschwung, ihr reges Kreisen", "tokens": ["Ih\u00b7ren", "Feu\u00b7er\u00b7schwung", ",", "ihr", "re\u00b7ges", "Krei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem Arm der ew'gen Ruhe festh\u00e4ltst!", "tokens": ["Mit", "dem", "Arm", "der", "ew'\u00b7gen", "Ru\u00b7he", "fest\u00b7h\u00e4ltst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Welch ein Lobgesang ert\u00f6nt in allen", "tokens": ["Welch", "ein", "Lob\u00b7ge\u00b7sang", "er\u00b7t\u00f6nt", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Welten Dir, Du aller Sternench\u00f6re", "tokens": ["Wel\u00b7ten", "Dir", ",", "Du", "al\u00b7ler", "Ster\u00b7nen\u00b7ch\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Leise F\u00fchrerin! Ein hohes Loblied,", "tokens": ["Lei\u00b7se", "F\u00fch\u00b7re\u00b7rin", "!", "Ein", "ho\u00b7hes", "Lob\u00b7lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dem der Sturm verstummet, dem die Sprache,", "tokens": ["Dem", "der", "Sturm", "ver\u00b7stum\u00b7met", ",", "dem", "die", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dem des Herzens Laut, dem alle T\u00f6ne", "tokens": ["Dem", "des", "Her\u00b7zens", "Laut", ",", "dem", "al\u00b7le", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sanft entschlummern in ein heilig Schweigen.", "tokens": ["Sanft", "ent\u00b7schlum\u00b7mern", "in", "ein", "hei\u00b7lig", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Heilig Schweigen, das die Welt jetzt f\u00fcllet,", "tokens": ["Hei\u00b7lig", "Schwei\u00b7gen", ",", "das", "die", "Welt", "jetzt", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sanfter Strom, der in den ew'gen Ufern", "tokens": ["Sanf\u00b7ter", "Strom", ",", "der", "in", "den", "ew'\u00b7gen", "U\u00b7fern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Endeloser Sch\u00f6pfung feiernd hinrollt!", "tokens": ["En\u00b7de\u00b7lo\u00b7ser", "Sch\u00f6p\u00b7fung", "fei\u00b7ernd", "hin\u00b7rollt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und Du herrlicher Gesang der Sterne,", "tokens": ["Und", "Du", "herr\u00b7li\u00b7cher", "Ge\u00b7sang", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Licht aus Licht, des Himmels sanfte Sprache!", "tokens": ["Licht", "aus", "Licht", ",", "des", "Him\u00b7mels", "sanf\u00b7te", "Spra\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Weite Nacht umfasset meine Seele!", "tokens": ["Wei\u00b7te", "Nacht", "um\u00b7fas\u00b7set", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meere der Unendlichkeit umfangen", "tokens": ["Mee\u00b7re", "der", "Un\u00b7end\u00b7lich\u00b7keit", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meinen Geist, die Himmel aller Himmel!", "tokens": ["Mei\u00b7nen", "Geist", ",", "die", "Him\u00b7mel", "al\u00b7ler", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "N\u00e4chtlich still, ein Meer voll lichter Scenen,", "tokens": ["N\u00e4cht\u00b7lich", "still", ",", "ein", "Meer", "voll", "lich\u00b7ter", "Sce\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wie das Weltmeer, voll von Feuerfunken.", "tokens": ["Wie", "das", "Welt\u00b7meer", ",", "voll", "von", "Feu\u00b7er\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hohe Nacht, ich knie vor Deinem Altar!", "tokens": ["Ho\u00b7he", "Nacht", ",", "ich", "knie", "vor", "Dei\u00b7nem", "Al\u00b7tar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Alle Funken des allweiten Aethers", "tokens": ["Al\u00b7le", "Fun\u00b7ken", "des", "all\u00b7wei\u00b7ten", "A\u00b7e\u00b7thers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+-+--", "measure": "anapaest.init"}, "line.3": {"text": "Sind das Stirnband Deiner heil'gen Schl\u00e4fe,", "tokens": ["Sind", "das", "Stirn\u00b7band", "Dei\u00b7ner", "heil'\u00b7gen", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Voll von Gottesschrift. Wer kann sie lesen,", "tokens": ["Voll", "von", "Got\u00b7tes\u00b7schrift", ".", "Wer", "kann", "sie", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Diese Flammenschrift des Unerschaffnen", "tokens": ["Die\u00b7se", "Flam\u00b7men\u00b7schrift", "des", "Un\u00b7er\u00b7schaff\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf der Stirn der Nacht? Sie spricht: \u00bbJehovah", "tokens": ["Auf", "der", "Stirn", "der", "Nacht", "?", "Sie", "spricht", ":", "\u00bb", "Je\u00b7ho\u00b7vah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "$(", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ist nur Einer, und Sein Nam' unendlich,", "tokens": ["Ist", "nur", "Ei\u00b7ner", ",", "und", "Sein", "Nam'", "un\u00b7end\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und Sein Kind die Nacht. Ihr hoher Name", "tokens": ["Und", "Sein", "Kind", "die", "Nacht", ".", "Ihr", "ho\u00b7her", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Hei\u00dft ", "tokens": ["Hei\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Deckte Niemand auf. Sie hat geboren", "tokens": ["Deck\u00b7te", "Nie\u00b7mand", "auf", ".", "Sie", "hat", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Welten, R\u00e4ume, Zeiten. Ihren Kindern", "tokens": ["Wel\u00b7ten", ",", "R\u00e4u\u00b7me", ",", "Zei\u00b7ten", ".", "Ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Stehen ewig vor Gesetz und Ordnung,", "tokens": ["Ste\u00b7hen", "e\u00b7wig", "vor", "Ge\u00b7setz", "und", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Lieb' und strenges Schicksal, Alle leitend,", "tokens": ["Lieb'", "und", "stren\u00b7ges", "Schick\u00b7sal", ",", "Al\u00b7le", "lei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "PIAT", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Alle leitend zum lebend'gen Vater.\u00ab", "tokens": ["Al\u00b7le", "lei\u00b7tend", "zum", "le\u00b7ben\u00b7d'\u00b7gen", "Va\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJD", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "La\u00df den Schleier sinken, heil'ge Mutter,", "tokens": ["La\u00df", "den", "Schlei\u00b7er", "sin\u00b7ken", ",", "heil'\u00b7ge", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlage zu Dein Buch voll Gottesschriften!", "tokens": ["Schla\u00b7ge", "zu", "Dein", "Buch", "voll", "Got\u00b7tes\u00b7schrif\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn ich kann nicht weiter, kann nicht h\u00f6her", "tokens": ["Denn", "ich", "kann", "nicht", "wei\u00b7ter", ",", "kann", "nicht", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,", "VMFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Klimmen in Gedanken. Neige lieber", "tokens": ["Klim\u00b7men", "in", "Ge\u00b7dan\u00b7ken", ".", "Nei\u00b7ge", "lie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Her das F\u00fcllhorn Deiner Ruh und tr\u00e4ufle,", "tokens": ["Her", "das", "F\u00fcll\u00b7horn", "Dei\u00b7ner", "Ruh", "und", "tr\u00e4uf\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tr\u00e4ufle sanft mir zu, o Du, des Schlafes", "tokens": ["Tr\u00e4uf\u00b7le", "sanft", "mir", "zu", ",", "o", "Du", ",", "des", "Schla\u00b7fes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Tr\u00e4ume Mutter, tr\u00e4ufle sanft mir", "tokens": ["Und", "der", "Tr\u00e4u\u00b7me", "Mut\u00b7ter", ",", "tr\u00e4uf\u00b7le", "sanft", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "VVFIN", "ADJD", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu Vergessenheit von meinen Sorgen!", "tokens": ["Zu", "Ver\u00b7ges\u00b7sen\u00b7heit", "von", "mei\u00b7nen", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fchl' ich nicht, wie ihre Schlummerbinde", "tokens": ["F\u00fchl'", "ich", "nicht", ",", "wie", "ih\u00b7re", "Schlum\u00b7mer\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", 