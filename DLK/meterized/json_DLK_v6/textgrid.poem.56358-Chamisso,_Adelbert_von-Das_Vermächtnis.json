{"textgrid.poem.56358": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Verm\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin schon alt, es mahnt der Zeiten Lauf", "tokens": ["Ich", "bin", "schon", "alt", ",", "es", "mahnt", "der", "Zei\u00b7ten", "Lauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich oft an l\u00e4ngst geschehene Geschichten,", "tokens": ["Mich", "oft", "an", "l\u00e4ngst", "ge\u00b7sche\u00b7he\u00b7ne", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die erz\u00e4hl ich, horcht auch niemand auf.", "tokens": ["Und", "die", "er\u00b7z\u00e4hl", "ich", ",", "horcht", "auch", "nie\u00b7mand", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wei\u00df ich aus der Chronik und Gedichten,", "tokens": ["So", "wei\u00df", "ich", "aus", "der", "Chro\u00b7nik", "und", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie bei der Pest es in Ferrara war,", "tokens": ["Wie", "bei", "der", "Pest", "es", "in", "Fer\u00b7ra\u00b7ra", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "APPR", "NE", "VAFIN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und will davon nur einen Zug berichten.", "tokens": ["Und", "will", "da\u00b7von", "nur", "ei\u00b7nen", "Zug", "be\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es scheute wohl sich jeder vor Gefahr,", "tokens": ["Es", "scheu\u00b7te", "wohl", "sich", "je\u00b7der", "vor", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den pesterkrankten Vater floh der Sohn,", "tokens": ["Den", "pes\u00b7ter\u00b7krank\u00b7ten", "Va\u00b7ter", "floh", "der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Mutter selbst das Kind, das sie gebar.", "tokens": ["Die", "Mut\u00b7ter", "selbst", "das", "Kind", ",", "das", "sie", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es war zu hei\u00dfer Sommerzeit; geflohn", "tokens": ["Es", "war", "zu", "hei\u00b7\u00dfer", "Som\u00b7mer\u00b7zeit", ";", "ge\u00b7flohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$.", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von Freunden und Verwandten, weltverlassen", "tokens": ["Von", "Freun\u00b7den", "und", "Ver\u00b7wand\u00b7ten", ",", "welt\u00b7ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Lag Basso della Penna sterbend schon.", "tokens": ["Lag", "Bas\u00b7so", "del\u00b7la", "Pen\u00b7na", "ster\u00b7bend", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sein Testament, das wollt er schreiben lassen;", "tokens": ["Sein", "Tes\u00b7ta\u00b7ment", ",", "das", "wollt", "er", "schrei\u00b7ben", "las\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Es lie\u00df sich endlich ein Notar bewegen,", "tokens": ["Es", "lie\u00df", "sich", "end\u00b7lich", "ein", "No\u00b7tar", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das Dokument rechtskr\u00e4ftig zu verfassen.", "tokens": ["Das", "Do\u00b7ku\u00b7ment", "rechts\u00b7kr\u00e4f\u00b7tig", "zu", "ver\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und er: \u00bbIch will es ihnen auferlegen,", "tokens": ["Und", "er", ":", "\u00bb", "Ich", "will", "es", "ih\u00b7nen", "auf\u00b7er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "$(", "PPER", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich meine meinen Kindern, meinen Erben,", "tokens": ["Ich", "mei\u00b7ne", "mei\u00b7nen", "Kin\u00b7dern", ",", "mei\u00b7nen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Anst\u00e4ndig meine Fliegen zu verpflegen.\u00ab", "tokens": ["An\u00b7st\u00e4n\u00b7dig", "mei\u00b7ne", "Flie\u00b7gen", "zu", "ver\u00b7pfle\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und der Notar: \u00bbIhr lieget schon im Sterben,", "tokens": ["Und", "der", "No\u00b7tar", ":", "\u00bb", "Ihr", "lie\u00b7get", "schon", "im", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wie schickt sich's, Basso, da\u00df Ihr Scherze treibt,", "tokens": ["Wie", "schickt", "sich's", ",", "Bas\u00b7so", ",", "da\u00df", "Ihr", "Scher\u00b7ze", "treibt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "$,", "NE", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Anstatt um Euer Heil Euch zu bewerben.\u00ab", "tokens": ["An\u00b7statt", "um", "Eu\u00b7er", "Heil", "Euch", "zu", "be\u00b7wer\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Drauf dieser: \u00bbSchreibt, wie ich Euch sage, schreibt!", "tokens": ["Drauf", "die\u00b7ser", ":", "\u00bb", "Schreibt", ",", "wie", "ich", "Euch", "sa\u00b7ge", ",", "schreibt", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PDAT", "$.", "$(", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ihr seht mich ja verlassen von den Meinen,", "tokens": ["Ihr", "seht", "mich", "ja", "ver\u00b7las\u00b7sen", "von", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da noch dies Fliegenvolk mir treu verbleibt.", "tokens": ["Da", "noch", "dies", "Flie\u00b7gen\u00b7volk", "mir", "treu", "ver\u00b7bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Nur treu aus Eigennutz, so m\u00f6gt Ihr meinen;", "tokens": ["Nur", "treu", "aus", "Ei\u00b7gen\u00b7nutz", ",", "so", "m\u00f6gt", "Ihr", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ich will's nicht untersuchen, will allein", "tokens": ["Ich", "will's", "nicht", "un\u00b7ter\u00b7su\u00b7chen", ",", "will", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Es wissen, da\u00df die Treusten sie mir scheinen;", "tokens": ["Es", "wis\u00b7sen", ",", "da\u00df", "die", "Treus\u00b7ten", "sie", "mir", "schei\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Bei Gott! ich mu\u00df und will erkenntlich sein,", "tokens": ["Bei", "Gott", "!", "ich", "mu\u00df", "und", "will", "er\u00b7kennt\u00b7lich", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PPER", "VMFIN", "KON", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Drum, schreibt es nieder, so wie ich Euch sage,", "tokens": ["Drum", ",", "schreibt", "es", "nie\u00b7der", ",", "so", "wie", "ich", "Euch", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Denn wohlerwogen ist der Wille mein:", "tokens": ["Denn", "woh\u00b7ler\u00b7wo\u00b7gen", "ist", "der", "Wil\u00b7le", "mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Allj\u00e4hrig sollen sie am Jakobstage", "tokens": ["All\u00b7j\u00e4h\u00b7rig", "sol\u00b7len", "sie", "am", "Ja\u00b7kobs\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Aussetzen einen Scheffel reifer Feigen", "tokens": ["Aus\u00b7set\u00b7zen", "ei\u00b7nen", "Schef\u00b7fel", "rei\u00b7fer", "Fei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Den Fliegen allzumal zum Festgelage.", "tokens": ["Den", "Flie\u00b7gen", "all\u00b7zu\u00b7mal", "zum", "Fest\u00b7ge\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und sollten sie darin sich l\u00e4ssig zeigen,", "tokens": ["Und", "soll\u00b7ten", "sie", "da\u00b7rin", "sich", "l\u00e4s\u00b7sig", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und unterblieb' es nur ein einzig Mal,", "tokens": ["Und", "un\u00b7ter\u00b7blieb'", "es", "nur", "ein", "ein\u00b7zig", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "F\u00e4llt Hab und Gut dem Armenhaus zu eigen.\u00ab \u2013", "tokens": ["F\u00e4llt", "Hab", "und", "Gut", "dem", "Ar\u00b7men\u00b7haus", "zu", "ei\u00b7gen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "PTKA", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und noch geschieht es so, wie er befahl,", "tokens": ["Und", "noch", "ge\u00b7schieht", "es", "so", ",", "wie", "er", "be\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Und am bestimmten Tage zugemessen", "tokens": ["Und", "am", "be\u00b7stimm\u00b7ten", "Ta\u00b7ge", "zu\u00b7ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wird noch den Fliegen ihr bestimmtes Mahl.", "tokens": ["Wird", "noch", "den", "Flie\u00b7gen", "ihr", "be\u00b7stimm\u00b7tes", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Der Fliegen hat kein Erbe je vergessen.", "tokens": ["Der", "Flie\u00b7gen", "hat", "kein", "Er\u00b7be", "je", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}