{"textgrid.poem.24900": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Zaubergarten, wunderbar erbl\u00fchter,", "tokens": ["O", "Zau\u00b7berg\u00b7ar\u00b7ten", ",", "wun\u00b7der\u00b7bar", "er\u00b7bl\u00fch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Erdenw\u00fcste gr\u00fcnendste Oase,", "tokens": ["Der", "Er\u00b7den\u00b7w\u00fcs\u00b7te", "gr\u00fc\u00b7nends\u00b7te", "O\u00b7a\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Riswan stets, der Paradiesesh\u00fcter,", "tokens": ["Die", "Ris\u00b7wan", "stets", ",", "der", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7h\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Tau benetzt aus seiner Himmelsvase,", "tokens": ["Mit", "Tau", "be\u00b7netzt", "aus", "sei\u00b7ner", "Him\u00b7mels\u00b7va\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seh' ich, o Vega, deine freudenhellen,", "tokens": ["Seh'", "ich", ",", "o", "Ve\u00b7ga", ",", "dei\u00b7ne", "freu\u00b7den\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcckschweren Fluren sich vor mir verbreiten,", "tokens": ["Gl\u00fcck\u00b7schwe\u00b7ren", "Flu\u00b7ren", "sich", "vor", "mir", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Meer des reichsten Segens, dessen Wellen", "tokens": ["Ein", "Meer", "des", "reichs\u00b7ten", "Se\u00b7gens", ",", "des\u00b7sen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Silberlicht der Morgensonne gleiten,", "tokens": ["Im", "Sil\u00b7ber\u00b7licht", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seh' ich am Bergeshang die deutsche Eiche", "tokens": ["Seh'", "ich", "am", "Ber\u00b7ges\u00b7hang", "die", "deut\u00b7sche", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich mit der Palme schwesterlich umarmen,", "tokens": ["Sich", "mit", "der", "Pal\u00b7me", "schwes\u00b7ter\u00b7lich", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollte hier, wie in dem Fabelreiche,", "tokens": ["Als", "woll\u00b7te", "hier", ",", "wie", "in", "dem", "Fa\u00b7bel\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Norden an des S\u00fcdens Brust erwarmen,", "tokens": ["Der", "Nor\u00b7den", "an", "des", "S\u00fc\u00b7dens", "Brust", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und h\u00f6r' ich dann von den beeisten Zinnen", "tokens": ["Und", "h\u00f6r'", "ich", "dann", "von", "den", "be\u00b7eis\u00b7ten", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sierra durch die echoreichen Schluchten", "tokens": ["Der", "Sier\u00b7ra", "durch", "die", "ec\u00b7ho\u00b7rei\u00b7chen", "Schluch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die schneegebornen B\u00e4che niederrinnen,", "tokens": ["Die", "schnee\u00b7ge\u00b7bor\u00b7nen", "B\u00e4\u00b7che", "nie\u00b7der\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dich mit ihrem ew'gen Tau befruchten:", "tokens": ["Die", "dich", "mit", "ih\u00b7rem", "ew'\u00b7gen", "Tau", "be\u00b7fruch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dann glaub' ich oft, o herrlichstes der Thale,", "tokens": ["Dann", "glaub'", "ich", "oft", ",", "o", "herr\u00b7lichs\u00b7tes", "der", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "FM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du seist der letzte Rest der jungen Erde,", "tokens": ["Du", "seist", "der", "letz\u00b7te", "Rest", "der", "jun\u00b7gen", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die einst, sich sonnend in dem Morgenstrahle,", "tokens": ["Die", "einst", ",", "sich", "son\u00b7nend", "in", "dem", "Mor\u00b7gen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Nichts enttauchte auf das gro\u00dfe Werde.", "tokens": ["Dem", "Nichts", "ent\u00b7tauch\u00b7te", "auf", "das", "gro\u00b7\u00dfe", "Wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So gl\u00e4nzte die Natur, ein reines Eden,", "tokens": ["So", "gl\u00e4nz\u00b7te", "die", "Na\u00b7tur", ",", "ein", "rei\u00b7nes", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von saft'gem Gr\u00fcn und Fr\u00fchrot \u00fcbergossen,", "tokens": ["Von", "saft'\u00b7gem", "Gr\u00fcn", "und", "Fr\u00fch\u00b7rot", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als erst der Lebensstrom in Silberf\u00e4den", "tokens": ["Als", "erst", "der", "Le\u00b7bens\u00b7strom", "in", "Sil\u00b7ber\u00b7f\u00e4\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gro\u00dfen Himmelsurne kaum entflossen.", "tokens": ["Der", "gro\u00b7\u00dfen", "Him\u00b7mel\u00b7sur\u00b7ne", "kaum", "ent\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zerst\u00f6rt ist jene Welt; nur in Ruinen", "tokens": ["Zer\u00b7st\u00f6rt", "ist", "je\u00b7ne", "Welt", ";", "nur", "in", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PDAT", "NN", "$.", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lebt noch von dem, was einst sie war, die Sage;", "tokens": ["Lebt", "noch", "von", "dem", ",", "was", "einst", "sie", "war", ",", "die", "Sa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aber strahlst, vom goldnen Licht beschienen,", "tokens": ["Du", "a\u00b7ber", "strahlst", ",", "vom", "gold\u00b7nen", "Licht", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch heute wie am ersten Sch\u00f6pfungstage.", "tokens": ["Noch", "heu\u00b7te", "wie", "am", "ers\u00b7ten", "Sch\u00f6p\u00b7fungs\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}