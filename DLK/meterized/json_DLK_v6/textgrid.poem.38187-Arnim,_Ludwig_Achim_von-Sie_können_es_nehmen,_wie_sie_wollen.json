{"textgrid.poem.38187": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sie k\u00f6nnen es nehmen, wie sie wollen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4gdlein jung gef\u00e4llt mir wohl,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "jung", "ge\u00b7f\u00e4llt", "mir", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Jahren alt, weis wie ein Kohl,", "tokens": ["Von", "Jah\u00b7ren", "alt", ",", "weis", "wie", "ein", "Kohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sch\u00f6n wie ein Rab ihr gelbes Haar,", "tokens": ["Sch\u00f6n", "wie", "ein", "Rab", "ihr", "gel\u00b7bes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Triefdunkel sind die Aeuglein klar.", "tokens": ["Trief\u00b7dun\u00b7kel", "sind", "die", "A\u00b7e\u00b7u\u00b7glein", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Stirn rund wie ein Falten Rock,", "tokens": ["Die", "Stirn", "rund", "wie", "ein", "Fal\u00b7ten", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Feist ausged\u00f6rrt die B\u00e4cklein schmuck.", "tokens": ["Feist", "aus\u00b7ge\u00b7d\u00f6rrt", "die", "B\u00e4c\u00b7klein", "schmuck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blauroth ist ihr das M\u00fcndlein wei\u00df,", "tokens": ["Blau\u00b7roth", "ist", "ihr", "das", "M\u00fcnd\u00b7lein", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sch\u00f6n h\u00e4\u00dflich ich sie schelt und prei\u00df.", "tokens": ["Sch\u00f6n", "h\u00e4\u00df\u00b7lich", "ich", "sie", "schelt", "und", "prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schneewei\u00df sind ihre schwarze H\u00e4nd,", "tokens": ["Schnee\u00b7wei\u00df", "sind", "ih\u00b7re", "schwar\u00b7ze", "H\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie eine Schneck ihr Gang behend,", "tokens": ["Wie", "ei\u00b7ne", "Schneck", "ihr", "Gang", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wie ein Kettenhund sie freundlich redt,", "tokens": ["Wie", "ein", "Ket\u00b7ten\u00b7hund", "sie", "freund\u00b7lich", "redt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sauh\u00f6flich, wenn sie geht und steht.", "tokens": ["Sau\u00b7h\u00f6f\u00b7lich", ",", "wenn", "sie", "geht", "und", "steht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein solches M\u00e4gdlein h\u00e4tt ich gern,", "tokens": ["Ein", "sol\u00b7ches", "M\u00e4gd\u00b7lein", "h\u00e4tt", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nah bei ihr zu seyn sehr weit und fern,", "tokens": ["Nah", "bei", "ihr", "zu", "seyn", "sehr", "weit", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VAINF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie oft zu herzen nimmermehr,", "tokens": ["Sie", "oft", "zu", "her\u00b7zen", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott nehm sie bald! ist mein Begehr.", "tokens": ["Gott", "nehm", "sie", "bald", "!", "ist", "mein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}