{"textgrid.poem.32871": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trinkt, trinkt, trinkt,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trinkt, trinkt, trinkt,", "tokens": ["Trinkt", ",", "trinkt", ",", "trinkt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Trinkt, ihr unverdro\u00dfnen Br\u00fcder", "tokens": ["Trinkt", ",", "ihr", "un\u00b7ver\u00b7dro\u00df\u00b7nen", "Br\u00fc\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eures Lebens Sorgen nieder!", "tokens": ["Eu\u00b7res", "Le\u00b7bens", "Sor\u00b7gen", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singt, singt, singt,", "tokens": ["Singt", ",", "singt", ",", "singt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Singt darunter frohe Lieder,", "tokens": ["Singt", "da\u00b7run\u00b7ter", "fro\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trinkt darauf und singet wieder!", "tokens": ["Trinkt", "da\u00b7rauf", "und", "sin\u00b7get", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reich, reich, reich,", "tokens": ["Reich", ",", "reich", ",", "reich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Tr\u00e4ufelt Seegen von Ly\u00e4en,", "tokens": ["Tr\u00e4u\u00b7felt", "See\u00b7gen", "von", "Ly\u00b7\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den weinbepflanzten H\u00f6hen!", "tokens": ["Von", "den", "wein\u00b7be\u00b7pflanz\u00b7ten", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch, euch, euch,", "tokens": ["Euch", ",", "euch", ",", "euch", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "L\u00e4chelt er zuk\u00fcnftge Freuden,", "tokens": ["L\u00e4\u00b7chelt", "er", "zu\u00b7k\u00fcnft\u00b7ge", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die G\u00f6tter euch beneiden.", "tokens": ["Um", "die", "G\u00f6t\u00b7ter", "euch", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt, h\u00f6rt, h\u00f6rt,", "tokens": ["H\u00f6rt", ",", "h\u00f6rt", ",", "h\u00f6rt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "H\u00f6rt der Winzer frohen Willen", "tokens": ["H\u00f6rt", "der", "Win\u00b7zer", "fro\u00b7hen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4sser her! wir m\u00fcssen f\u00fcllen.", "tokens": ["F\u00e4s\u00b7ser", "her", "!", "wir", "m\u00fcs\u00b7sen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leert, leert, leert,", "tokens": ["Leert", ",", "leert", ",", "leert", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Leert die\u00df Fa\u00df mit tapfern Z\u00fcgen,", "tokens": ["Leert", "die\u00df", "Fa\u00df", "mit", "tap\u00b7fern", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Winzer Tonnen kriegen!", "tokens": ["Da\u00df", "die", "Win\u00b7zer", "Ton\u00b7nen", "krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}