{"dta.poem.16130": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "184.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Weisheit th\u00f6richt wird, sucht sie den Stein der Weisen,", "tokens": ["Wenn", "Weis\u00b7heit", "th\u00f6\u00b7richt", "wird", ",", "sucht", "sie", "den", "Stein", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Arzenei, die gleich f\u00fcr jedes Weh zu preisen,", "tokens": ["Die", "Ar\u00b7ze\u00b7nei", ",", "die", "gleich", "f\u00fcr", "je\u00b7des", "Weh", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die allgemeine Sprach' und einen ew'gen Frieden,", "tokens": ["Die", "all\u00b7ge\u00b7mei\u00b7ne", "Sprach'", "und", "ei\u00b7nen", "ew'\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles was nie war, und nie wird seyn hienieden.", "tokens": ["Und", "al\u00b7les", "was", "nie", "war", ",", "und", "nie", "wird", "seyn", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ADV", "VAFIN", "$,", "KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Allgemeine ist beim Ew'gen ewig dort,", "tokens": ["Das", "All\u00b7ge\u00b7mei\u00b7ne", "ist", "beim", "Ew'\u00b7gen", "e\u00b7wig", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPRART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier beim Verg\u00e4nglichen ist des Gemeinen Ort.", "tokens": ["Hier", "beim", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "ist", "des", "Ge\u00b7mei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+--++-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Das Unbedingte ist, wo keine Dinge sind,", "tokens": ["Das", "Un\u00b7be\u00b7ding\u00b7te", "ist", ",", "wo", "kei\u00b7ne", "Din\u00b7ge", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von welchen ist dein Witz bedingt, o Menschenkind;", "tokens": ["Von", "wel\u00b7chen", "ist", "dein", "Witz", "be\u00b7dingt", ",", "o", "Men\u00b7schen\u00b7kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Gutes, Sch\u00f6nes steht, Ein Wahres dort gewis;", "tokens": ["Ein", "Gu\u00b7tes", ",", "Sch\u00f6\u00b7nes", "steht", ",", "Ein", "Wah\u00b7res", "dort", "ge\u00b7wis", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "VVFIN", "$,", "ART", "ADJA", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch macht kein Sternenschein zum Tag die Finsternis.", "tokens": ["Doch", "macht", "kein", "Ster\u00b7nen\u00b7schein", "zum", "Tag", "die", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Kein Gutes hier ist gut, kein Sch\u00f6nes sch\u00f6n f\u00fcr alle,", "tokens": ["Kein", "Gu\u00b7tes", "hier", "ist", "gut", ",", "kein", "Sch\u00f6\u00b7nes", "sch\u00f6n", "f\u00fcr", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "ADJD", "$,", "PIAT", "NN", "ADJD", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewisses selbst gewis nur im gewissen Falle.", "tokens": ["Ge\u00b7wis\u00b7ses", "selbst", "ge\u00b7wis", "nur", "im", "ge\u00b7wis\u00b7sen", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}