{"dta.poem.11645": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Florentine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wildes M\u00e4dchen! schau mir doch", "tokens": ["Wil\u00b7des", "M\u00e4d\u00b7chen", "!", "schau", "mir", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal recht in's Auge!", "tokens": ["Ein\u00b7mal", "recht", "in's", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob so gar nichts dir darin", "tokens": ["Ob", "so", "gar", "nichts", "dir", "da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PIS", "PPER", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein wenig tauge?", "tokens": ["Nur", "ein", "we\u00b7nig", "tau\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar dein liebes Bild hast du", "tokens": ["Zwar", "dein", "lie\u00b7bes", "Bild", "hast", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oefters drin gesehen,", "tokens": ["Oef\u00b7ters", "drin", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freutest auch des Spiegels dich,", "tokens": ["Freu\u00b7test", "auch", "des", "Spie\u00b7gels", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00df'st ihn wieder stehen.", "tokens": ["L\u00e4\u00df'st", "ihn", "wie\u00b7der", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch so mu\u00dft du mehr und mehr", "tokens": ["Doch", "so", "mu\u00dft", "du", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir darin gefallen,", "tokens": ["Dir", "da\u00b7rin", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und am Ende bleibt er dir", "tokens": ["Und", "am", "En\u00b7de", "bleibt", "er", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb und werth vor allen.", "tokens": ["Lieb", "und", "werth", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}