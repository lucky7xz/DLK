{"textgrid.poem.33171": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Cap. 11.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Mosis amt ists also aus, und dies gewohnet sind zu haus, die haltens willig ohn gebot,", "tokens": ["Mit", "Mo\u00b7sis", "amt", "ists", "al\u00b7so", "aus", ",", "und", "dies", "ge\u00b7woh\u00b7net", "sind", "zu", "haus", ",", "die", "hal\u00b7tens", "wil\u00b7lig", "ohn", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAFIN", "ADV", "PTKVZ", "$,", "KON", "PDS", "VVFIN", "VAFIN", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Weils ihnen auch niemand verbot.", "tokens": ["Weils", "ih\u00b7nen", "auch", "nie\u00b7mand", "ver\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Lehrt' keinen abfall vom gesetz, lie\u00df sich selbst reinigen zuletz, aus respect vor die andern Herrn,", "tokens": ["Lehrt'", "kei\u00b7nen", "ab\u00b7fall", "vom", "ge\u00b7setz", ",", "lie\u00df", "sich", "selbst", "rei\u00b7ni\u00b7gen", "zu\u00b7letz", ",", "aus", "res\u00b7pect", "vor", "die", "an\u00b7dern", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$,", "VVFIN", "PRF", "ADV", "VVFIN", "PTKVZ", "$,", "APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Die das gesetz noch hatten gern.", "tokens": ["Die", "das", "ge\u00b7setz", "noch", "hat\u00b7ten", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er wolte angesehen seyn, als ders gesetz f\u00fcr gut und fein, und nicht f\u00fcr b\u00f6se g\u00e4be aus,", "tokens": ["Er", "wol\u00b7te", "an\u00b7ge\u00b7se\u00b7hen", "seyn", ",", "als", "ders", "ge\u00b7setz", "f\u00fcr", "gut", "und", "fein", ",", "und", "nicht", "f\u00fcr", "b\u00f6\u00b7se", "g\u00e4\u00b7be", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "KOUS", "ART", "NN", "APPR", "ADJD", "KON", "ADJD", "$,", "KON", "PTKNEG", "APPR", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und das kam in so weit heraus.", "tokens": ["Und", "das", "kam", "in", "so", "weit", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}