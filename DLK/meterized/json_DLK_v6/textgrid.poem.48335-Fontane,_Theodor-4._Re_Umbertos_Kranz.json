{"textgrid.poem.48335": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4. Re Umbertos Kranz", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bb ... Im alten Dom zu Monza ruht die Krone,", "tokens": ["\u00bb", "...", "Im", "al\u00b7ten", "Dom", "zu", "Mon\u00b7za", "ruht", "die", "Kro\u00b7ne", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "APPRART", "ADJA", "NN", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Bl\u00fcht auch des Lorbeers viel in meinen G\u00e4rten.", "tokens": ["Bl\u00fcht", "auch", "des", "Lor\u00b7beers", "viel", "in", "mei\u00b7nen", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pfl\u00fcckt von dem Lorbeer, und vom dunkelsch\u00f6nsten", "tokens": ["Pfl\u00fcckt", "von", "dem", "Lor\u00b7beer", ",", "und", "vom", "dun\u00b7kel\u00b7sch\u00f6ns\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "APPRART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Schlingt einen Kranz \u2013 ", "tokens": ["Schlingt", "ei\u00b7nen", "Kranz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bis hin zur Ruhst\u00e4tt' meines Martyrfreundes,", "tokens": ["Bis", "hin", "zur", "Ruh\u00b7st\u00e4tt'", "mei\u00b7nes", "Mar\u00b7tyr\u00b7freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis in die Friedenskirche.", "tokens": ["Bis", "in", "die", "Frie\u00b7dens\u00b7kir\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Siegeslorbeer,", "tokens": ["Sie\u00b7ges\u00b7lor\u00b7beer", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nicht Friedenspalmen will ich niederlegen", "tokens": ["Nicht", "Frie\u00b7dens\u00b7pal\u00b7men", "will", "ich", "nie\u00b7der\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf seinen Sarg. Wozu noch Friedenspalmen?", "tokens": ["Auf", "sei\u00b7nen", "Sarg", ".", "Wo\u00b7zu", "noch", "Frie\u00b7dens\u00b7pal\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PWAV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Er hat, was er ersehnt \u2013 er ", "tokens": ["Er", "hat", ",", "was", "er", "er\u00b7sehnt", "\u2013", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$(", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}