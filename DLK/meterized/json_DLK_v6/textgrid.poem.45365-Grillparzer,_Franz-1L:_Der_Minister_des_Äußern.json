{"textgrid.poem.45365": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Minister des \u00c4u\u00dfern", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Minister des \u00c4u\u00dfern", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "des", "\u00c4u\u00b7\u00dfern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kann sich nicht \u00e4u\u00dfern,", "tokens": ["Kann", "sich", "nicht", "\u00e4u\u00b7\u00dfern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Minister des Innern", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "des", "In\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ist schwach im Erinnern,", "tokens": ["Ist", "schwach", "im", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Kriegsminister", "tokens": ["Der", "Kriegs\u00b7mi\u00b7nis\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Tr\u00e4gt Szepter und Kron im Tornister,", "tokens": ["Tr\u00e4gt", "Szep\u00b7ter", "und", "Kron", "im", "Tor\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Minister der Finanzen", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "der", "Fi\u00b7nan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df nach jedes Pfeife tanzen,", "tokens": ["Mu\u00df", "nach", "je\u00b7des", "Pfei\u00b7fe", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der Minister des Handels", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "des", "Han\u00b7dels"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ist unsichtbaren Wandels,", "tokens": ["Ist", "un\u00b7sicht\u00b7ba\u00b7ren", "Wan\u00b7dels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Der Minister der Justiz", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "der", "Jus\u00b7tiz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Hat nicht Stimme, nur Sitz,", "tokens": ["Hat", "nicht", "Stim\u00b7me", ",", "nur", "Sitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Der Minister des Kultus", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", "des", "Kul\u00b7tus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "\u00c4ndert cultus in stultus,", "tokens": ["\u00c4n\u00b7dert", "cul\u00b7tus", "in", "stul\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Der Chef der Polizei", "tokens": ["Der", "Chef", "der", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sch\u00fcttelt den Kopf dabei.", "tokens": ["Sch\u00fct\u00b7telt", "den", "Kopf", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}