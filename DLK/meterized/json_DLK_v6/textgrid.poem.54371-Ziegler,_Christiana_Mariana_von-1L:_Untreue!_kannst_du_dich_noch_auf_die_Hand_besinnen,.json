{"textgrid.poem.54371": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Untreue! kannst du dich noch auf die Hand besinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Untreue! kannst du dich noch auf die Hand besinnen,", "tokens": ["Un\u00b7treu\u00b7e", "!", "kannst", "du", "dich", "noch", "auf", "die", "Hand", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dieses Blat an dich, Meineidige gericht,", "tokens": ["Die", "die\u00b7ses", "Blat", "an", "dich", ",", "Mei\u00b7nei\u00b7di\u00b7ge", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Worauf der Thr\u00e4nen mehr, als Dintentropfen rinnen,", "tokens": ["Wo\u00b7rauf", "der", "Thr\u00e4\u00b7nen", "mehr", ",", "als", "Din\u00b7ten\u00b7trop\u00b7fen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lies, und frage dich, was dein Gewissen spricht.", "tokens": ["So", "lies", ",", "und", "fra\u00b7ge", "dich", ",", "was", "dein", "Ge\u00b7wis\u00b7sen", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Alphonsus schickt es dir, der ehmals dich erhoben,", "tokens": ["Al\u00b7phon\u00b7sus", "schickt", "es", "dir", ",", "der", "eh\u00b7mals", "dich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,", "PRELS", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nun aber statt des Danks mit Kummer und Verdru\u00df", "tokens": ["Nun", "a\u00b7ber", "statt", "des", "Danks", "mit", "Kum\u00b7mer", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bey deiner tollen Wuth, bey Rasen, Fluchen, Toben,", "tokens": ["Bey", "dei\u00b7ner", "tol\u00b7len", "Wuth", ",", "bey", "Ra\u00b7sen", ",", "Flu\u00b7chen", ",", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf einer Insel hier sein Elend bauen mu\u00df.", "tokens": ["Auf", "ei\u00b7ner", "In\u00b7sel", "hier", "sein", "E\u00b7lend", "bau\u00b7en", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie h\u00e4tt ich armer doch wohl \u00fcbler k\u00f6nnen wehlen,", "tokens": ["Wie", "h\u00e4tt", "ich", "ar\u00b7mer", "doch", "wohl", "\u00fcb\u00b7ler", "k\u00f6n\u00b7nen", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJA", "ADV", "ADV", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als doch mein Aug auf dich, du Furie, verfiel?", "tokens": ["Als", "doch", "mein", "Aug", "auf", "dich", ",", "du", "Fu\u00b7rie", ",", "ver\u00b7fiel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$,", "PPER", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Warum gerieth es denn in des Avernus H\u00f6len?", "tokens": ["Wa\u00b7rum", "ge\u00b7rieth", "es", "denn", "in", "des", "A\u00b7ver\u00b7nus", "H\u00f6\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Denn Pluto war gewi\u00df als Henker mit im Spiel.", "tokens": ["Denn", "Plu\u00b7to", "war", "ge\u00b7wi\u00df", "als", "Hen\u00b7ker", "mit", "im", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "KOUS", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Nachwelt wird es nicht, sie kann es auch nicht glauben", "tokens": ["Die", "Nach\u00b7welt", "wird", "es", "nicht", ",", "sie", "kann", "es", "auch", "nicht", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df deine Geilheit mir durch Falschheit, Rank, und List,", "tokens": ["Da\u00df", "dei\u00b7ne", "Geil\u00b7heit", "mir", "durch", "Falschheit", ",", "Rank", ",", "und", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Kann Freyheit Kron und Trohn, wohl gar das Leben rauben,", "tokens": ["Kann", "Frey\u00b7heit", "Kron", "und", "Trohn", ",", "wohl", "gar", "das", "Le\u00b7ben", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NE", "KON", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mir, den du als Gemal, und K\u00f6nig hast gek\u00fc\u00dft.", "tokens": ["Mir", ",", "den", "du", "als", "Ge\u00b7mal", ",", "und", "K\u00f6\u00b7nig", "hast", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "KOUS", "NN", "$,", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es sey der Scheidebrief, das Ungl\u00fccksblat, verfluchet,", "tokens": ["Es", "sey", "der", "Schei\u00b7de\u00b7brief", ",", "das", "Un\u00b7gl\u00fccks\u00b7blat", ",", "ver\u00b7flu\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das du mir zu gestellt, und das dein L\u00e4stermund", "tokens": ["Das", "du", "mir", "zu", "ge\u00b7stellt", ",", "und", "das", "dein", "L\u00e4s\u00b7ter\u00b7mund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "PTKZU", "VVPP", "$,", "KON", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch Vorwand, Lug und Trug noch zu besch\u00f6nen suchet!", "tokens": ["Durch", "Vor\u00b7wand", ",", "Lug", "und", "Trug", "noch", "zu", "be\u00b7sch\u00f6\u00b7nen", "su\u00b7chet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dies macht das Reich emp\u00f6rt, dies bricht den Eyd und Bund.", "tokens": ["Dies", "macht", "das", "Reich", "em\u00b7p\u00f6rt", ",", "dies", "bricht", "den", "Eyd", "und", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was zischest du dem Volck, dem h\u00e4\u00dflichen Geschmeisse,", "tokens": ["Was", "zi\u00b7schest", "du", "dem", "Volck", ",", "dem", "h\u00e4\u00df\u00b7li\u00b7chen", "Ge\u00b7schmeis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "F\u00fcr fabelhaftes Zeug, Betr\u00fcgerinn ins Ohr?", "tokens": ["F\u00fcr", "fa\u00b7bel\u00b7haf\u00b7tes", "Zeug", ",", "Be\u00b7tr\u00fc\u00b7ge\u00b7rinn", "ins", "Ohr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie stellst du, da ich doch die Unschuld selber heisse,", "tokens": ["Wie", "stellst", "du", ",", "da", "ich", "doch", "die", "Un\u00b7schuld", "sel\u00b7ber", "heis\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Welt das Gegentheil von meiner Treue vor?", "tokens": ["Der", "Welt", "das", "Ge\u00b7gen\u00b7theil", "von", "mei\u00b7ner", "Treu\u00b7e", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ists m\u00f6glich, da\u00df du kannst Algarbien bel\u00fcgen,", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "du", "kannst", "Al\u00b7gar\u00b7bi\u00b7en", "be\u00b7l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als ob sich \u00fcberall die Ohnmacht bey mir wies,", "tokens": ["Als", "ob", "sich", "\u00fc\u00b7be\u00b7rall", "die", "Ohn\u00b7macht", "bey", "mir", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nach der ich Lieblingen, um dich nur zu vergn\u00fcgen,", "tokens": ["Nach", "der", "ich", "Lieb\u00b7lin\u00b7gen", ",", "um", "dich", "nur", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "$,", "KOUI", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Zu deinem Schlafgemach den Schl\u00fcssel \u00fcberlie\u00df.", "tokens": ["Zu", "dei\u00b7nem", "Schlaf\u00b7ge\u00b7mach", "den", "Schl\u00fcs\u00b7sel", "\u00fc\u00b7ber\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O Schandmaul! welcher Prinz l\u00e4\u00dft wohl mit eignem Winken", "tokens": ["O", "Schand\u00b7maul", "!", "wel\u00b7cher", "Prinz", "l\u00e4\u00dft", "wohl", "mit", "eig\u00b7nem", "Win\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PWAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den R\u00e4uber, den er sieht, in seinen Garten ein?", "tokens": ["Den", "R\u00e4u\u00b7ber", ",", "den", "er", "sieht", ",", "in", "sei\u00b7nen", "Gar\u00b7ten", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer l\u00e4\u00dft den Frevler wohl aus seinem Becher trinken?", "tokens": ["Wer", "l\u00e4\u00dft", "den", "Frev\u00b7ler", "wohl", "aus", "sei\u00b7nem", "Be\u00b7cher", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Des Throns und Lagers darf kein andrer theilhaft seyn.", "tokens": ["Des", "Throns", "und", "La\u00b7gers", "darf", "kein", "an\u00b7drer", "theil\u00b7haft", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "PIAT", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dies sannst du listig aus, mich nur verha\u00dft zu machen,", "tokens": ["Dies", "sannst", "du", "lis\u00b7tig", "aus", ",", "mich", "nur", "ver\u00b7ha\u00dft", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und gabst dabey noch vor, als wenn noch immer dar,", "tokens": ["Und", "gabst", "da\u00b7bey", "noch", "vor", ",", "als", "wenn", "noch", "im\u00b7mer", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "PTKVZ", "$,", "KOKOM", "KOUS", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wor\u00fcber ich auch mu\u00df in meinem Elend lachen,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ich", "auch", "mu\u00df", "in", "mei\u00b7nem", "E\u00b7lend", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es sollte dich die Welt die andre Vesta nennen,", "tokens": ["Es", "soll\u00b7te", "dich", "die", "Welt", "die", "and\u00b7re", "Ves\u00b7ta", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-----", "measure": "unknown.measure.tetra"}, "line.37": {"text": "Die vormals Lebenslang der Keuschheit sich verschrieb.", "tokens": ["Die", "vor\u00b7mals", "Le\u00b7bens\u00b7lang", "der", "Keuschheit", "sich", "ver\u00b7schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Drum sah man dich verstellt nach einem Kloster rennen,", "tokens": ["Drum", "sah", "man", "dich", "ver\u00b7stellt", "nach", "ei\u00b7nem", "Klos\u00b7ter", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als ob in selbiges dich dein Gewissen trieb.", "tokens": ["Als", "ob", "in", "sel\u00b7bi\u00b7ges", "dich", "dein", "Ge\u00b7wis\u00b7sen", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dies sollte, Heuchlerinn, dir eine Maske leihen,", "tokens": ["Dies", "soll\u00b7te", ",", "Heuch\u00b