 "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Suchst du die heiligen und stillen Mauren auf,", "tokens": ["Suchst", "du", "die", "hei\u00b7li\u00b7gen", "und", "stil\u00b7len", "Mau\u00b7ren", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Don Pedro war dir blo\u00df, ich weis es, im Gehirne,", "tokens": ["Don", "Ped\u00b7ro", "war", "dir", "blo\u00df", ",", "ich", "weis", "es", ",", "im", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "ADV", "$,", "PPER", "PTKVZ", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So listig bahntest du dir deiner Liebe Lauf.", "tokens": ["So", "lis\u00b7tig", "bahn\u00b7test", "du", "dir", "dei\u00b7ner", "Lie\u00b7be", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Was hast du nicht daselbst, f\u00fcr Streiche ausgehecket,", "tokens": ["Was", "hast", "du", "nicht", "da\u00b7selbst", ",", "f\u00fcr", "Strei\u00b7che", "aus\u00b7ge\u00b7he\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "PAV", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wodurch du gegen mich mein treues Volk verhetzt!", "tokens": ["Wo\u00b7durch", "du", "ge\u00b7gen", "mich", "mein", "treu\u00b7es", "Volk", "ver\u00b7hetzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Was Aufruhr hast du nicht im ganzen Reich erwecket,", "tokens": ["Was", "Auf\u00b7ruhr", "hast", "du", "nicht", "im", "gan\u00b7zen", "Reich", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der deinen Buler nun auf meinen Thron gesetzt?", "tokens": ["Der", "dei\u00b7nen", "Bu\u00b7ler", "nun", "auf", "mei\u00b7nen", "Thron", "ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So hat dein geiler Geist sich wider mich emp\u00f6ret,", "tokens": ["So", "hat", "dein", "gei\u00b7ler", "Geist", "sich", "wi\u00b7der", "mich", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Mein Scepter, dessen Macht ich dir mit \u00fcber gab,", "tokens": ["Mein", "Scep\u00b7ter", ",", "des\u00b7sen", "Macht", "ich", "dir", "mit", "\u00fc\u00b7ber", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "PPER", "PPER", "APPR", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Verwandelt sich nunmehr, wie mich die Zeit gelehret,", "tokens": ["Ver\u00b7wan\u00b7delt", "sich", "nun\u00b7mehr", ",", "wie", "mich", "die", "Zeit", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Durch deine Grausamkeit in einen Schlangenstab.", "tokens": ["Durch", "dei\u00b7ne", "Grau\u00b7sam\u00b7keit", "in", "ei\u00b7nen", "Schlan\u00b7gen\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Sag Mammeluckinn doch, was hat dich denn bewogen,", "tokens": ["Sag", "Mam\u00b7me\u00b7lu\u00b7ckinn", "doch", ",", "was", "hat", "dich", "denn", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Da\u00df deine freche Faust mir, deinem Ehgemal,", "tokens": ["Da\u00df", "dei\u00b7ne", "fre\u00b7che", "Faust", "mir", ",", "dei\u00b7nem", "Eh\u00b7ge\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Den K\u00f6nigsmantel hat so sch\u00e4ndlich abgezogen?", "tokens": ["Den", "K\u00f6\u00b7nigs\u00b7man\u00b7tel", "hat", "so", "sch\u00e4nd\u00b7lich", "ab\u00b7ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ich weis wer solches dir, Bundbr\u00fcchige, befahl.", "tokens": ["Ich", "weis", "wer", "sol\u00b7ches", "dir", ",", "Bund\u00b7br\u00fc\u00b7chi\u00b7ge", ",", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKVZ", "PWS", "PIS", "PPER", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.60": {"text": "Du suchtst mit selbigem die Schande zu zu decken,", "tokens": ["Du", "suchtst", "mit", "sel\u00b7bi\u00b7gem", "die", "Schan\u00b7de", "zu", "zu", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ART", "NN", "PTKZU", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die mit Don Pedro dich die Geilheit treiben hei\u00dft.", "tokens": ["Die", "mit", "Don", "Ped\u00b7ro", "dich", "die", "Geil\u00b7heit", "trei\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NE", "PRF", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Des Bruders Lagerstadt so sch\u00e4ndlich zu beflecken,", "tokens": ["Des", "Bru\u00b7ders", "La\u00b7ger\u00b7stadt", "so", "sch\u00e4nd\u00b7lich", "zu", "be\u00b7fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ist etwas das uns auch das Heydenthum nicht weist.", "tokens": ["Ist", "et\u00b7was", "das", "uns", "auch", "das", "Hey\u00b7den\u00b7thum", "nicht", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "An eurem Throne wird der Fluch bekleben bleiben,", "tokens": ["An", "eu\u00b7rem", "Thro\u00b7ne", "wird", "der", "Fluch", "be\u00b7kle\u00b7ben", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Mein Ach und Weh schleicht euch stets, geile Seelen, nach;", "tokens": ["Mein", "Ach", "und", "Weh", "schleicht", "euch", "stets", ",", "gei\u00b7le", "See\u00b7len", ",", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Der Thr\u00e4nen Salz das ihr mir suchet auszutreiben,", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Salz", "das", "ihr", "mir", "su\u00b7chet", "aus\u00b7zu\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "PPER", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und meiner Seufzer Kraft dringt in eur Schlafgemach.", "tokens": ["Und", "mei\u00b7ner", "Seuf\u00b7zer", "Kraft", "dringt", "in", "eur", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Armseliger Alphons! du bist nunmehr verwiesen;", "tokens": ["Arm\u00b7se\u00b7li\u00b7ger", "Al\u00b7phons", "!", "du", "bist", "nun\u00b7mehr", "ver\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.69": {"text": "Dein falsches Ehgemal, der Bruder und das Reich", "tokens": ["Dein", "fal\u00b7sches", "Eh\u00b7ge\u00b7mal", ",", "der", "Bru\u00b7der", "und", "das", "Reich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Die deine Thaten sonst aufs trefflichste gepriesen,", "tokens": ["Die", "dei\u00b7ne", "Tha\u00b7ten", "sonst", "aufs", "treff\u00b7lichs\u00b7te", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Die w\u00fcten allerseits nun wider dich zugleich.", "tokens": ["Die", "w\u00fc\u00b7ten", "al\u00b7ler\u00b7seits", "nun", "wi\u00b7der", "dich", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Statt meines Throns ist mir ein Kerker aufbehalten,", "tokens": ["Statt", "mei\u00b7nes", "Throns", "ist", "mir", "ein", "Ker\u00b7ker", "auf\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"