{"textgrid.poem.62464": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Seiner Herrin", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einer ist mein Herz entz\u00fcndet,", "tokens": ["Von", "ei\u00b7ner", "ist", "mein", "Herz", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die l\u00e4\u00dft mir Tag und Nacht nicht Ruh';", "tokens": ["Die", "l\u00e4\u00dft", "mir", "Tag", "und", "Nacht", "nicht", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hab' ich ewig mich verk\u00fcndet,", "tokens": ["Der", "hab'", "ich", "e\u00b7wig", "mich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr thu' ich alles, was ich thu'.", "tokens": ["Ihr", "thu'", "ich", "al\u00b7les", ",", "was", "ich", "thu'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Sch\u00f6nheit bl\u00fcht sie, gl\u00e4nzt in Ehren,", "tokens": ["In", "Sch\u00f6n\u00b7heit", "bl\u00fcht", "sie", ",", "gl\u00e4nzt", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gottes wundervolle Braut,", "tokens": ["Wie", "Got\u00b7tes", "wun\u00b7der\u00b7vol\u00b7le", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und scheint sich t\u00e4glich zu verkl\u00e4ren,", "tokens": ["Und", "scheint", "sich", "t\u00e4g\u00b7lich", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit ich an ihren Dienst getraut.", "tokens": ["Seit", "ich", "an", "ih\u00b7ren", "Dienst", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So freundlich hat sie mich geladen,", "tokens": ["So", "freund\u00b7lich", "hat", "sie", "mich", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df schier mein Herz in Liebe brach,", "tokens": ["Da\u00df", "schier", "mein", "Herz", "in", "Lie\u00b7be", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn die Mutter aller Gnaden", "tokens": ["Als", "wenn", "die", "Mut\u00b7ter", "al\u00b7ler", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In sel'gen Tr\u00e4umen zu mir sprach.", "tokens": ["In", "sel'\u00b7gen", "Tr\u00e4u\u00b7men", "zu", "mir", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als Knabe hab' ich viel vernommen", "tokens": ["Als", "Kna\u00b7be", "hab'", "ich", "viel", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihrer hohen W\u00fcrdigkeit,", "tokens": ["Von", "ih\u00b7rer", "ho\u00b7hen", "W\u00fcr\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem J\u00fcngling war ein Ruf gekommen", "tokens": ["Dem", "J\u00fcng\u00b7ling", "war", "ein", "Ruf", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrer Schmach und Niedrigkeit.", "tokens": ["Von", "ih\u00b7rer", "Schmach", "und", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da ging ich oft in Eichenhainen", "tokens": ["Da", "ging", "ich", "oft", "in", "Ei\u00b7chen\u00b7hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu suchen die versunkne Pracht;", "tokens": ["Zu", "su\u00b7chen", "die", "ver\u00b7sunk\u00b7ne", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Fall der Herrin zu beweinen,", "tokens": ["Den", "Fall", "der", "Her\u00b7rin", "zu", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu pr\u00fcfen meines Armes Macht.", "tokens": ["Zu", "pr\u00fc\u00b7fen", "mei\u00b7nes", "Ar\u00b7mes", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da betet' ich, la\u00df mich sie retten,", "tokens": ["Da", "be\u00b7tet'", "ich", ",", "la\u00df", "mich", "sie", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PPER", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du, welcher lenkt der Sterne Gang,", "tokens": ["Du", ",", "wel\u00b7cher", "lenkt", "der", "Ster\u00b7ne", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich la\u00df zerbrechen ihre Ketten,", "tokens": ["Mich", "la\u00df", "zer\u00b7bre\u00b7chen", "ih\u00b7re", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sterben froh, wenn das gelang.", "tokens": ["Und", "ster\u00b7ben", "froh", ",", "wenn", "das", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun ist die hohe Zeit gekommen,", "tokens": ["Nun", "ist", "die", "ho\u00b7he", "Zeit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6lle Rotten sind ged\u00e4mpft,", "tokens": ["Der", "H\u00f6l\u00b7le", "Rot\u00b7ten", "sind", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und betend knien die Starken, Frommen,", "tokens": ["Und", "be\u00b7tend", "kni\u00b7en", "die", "Star\u00b7ken", ",", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die k\u00fchn um solchen Lohn gek\u00e4mpft.", "tokens": ["Die", "k\u00fchn", "um", "sol\u00b7chen", "Lohn", "ge\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mu\u00df ich noch immer auf dich warten,", "tokens": ["Mu\u00df", "ich", "noch", "im\u00b7mer", "auf", "dich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meine ganze Seele f\u00fcllt,", "tokens": ["Die", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Ehrenpreis, mein Freudengarten,", "tokens": ["Mein", "Eh\u00b7ren\u00b7preis", ",", "mein", "Freu\u00b7den\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du aller Himmel sch\u00f6nstes Bild?", "tokens": ["Du", "al\u00b7ler", "Him\u00b7mel", "sch\u00f6ns\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was h\u00e4ltst du l\u00e4nger dich verborgen,", "tokens": ["Was", "h\u00e4ltst", "du", "l\u00e4n\u00b7ger", "dich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O s\u00fc\u00dfes wundersames Licht?", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "wun\u00b7der\u00b7sa\u00b7mes", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Treuen stehn in bangen Sorgen,", "tokens": ["Die", "Treu\u00b7en", "stehn", "in", "ban\u00b7gen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entzeuch dich ihnen l\u00e4nger nicht.", "tokens": ["Ent\u00b7zeuch", "dich", "ih\u00b7nen", "l\u00e4n\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O tritt hervor in deiner Sch\u00f6ne,", "tokens": ["O", "tritt", "her\u00b7vor", "in", "dei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von heil'gem Eichenzweig umlaubt,", "tokens": ["Von", "heil'\u00b7gem", "Ei\u00b7chen\u00b7zweig", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dich die Hand des Volkes kr\u00f6ne,", "tokens": ["Da\u00df", "dich", "die", "Hand", "des", "Vol\u00b7kes", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das immerdar an dich geglaubt.", "tokens": ["Das", "im\u00b7mer\u00b7dar", "an", "dich", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein Leuchten ist's aus gro\u00dfen Tagen,", "tokens": ["Ein", "Leuch\u00b7ten", "ist's", "aus", "gro\u00b7\u00dfen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das dich, du Herrliche, umwallt,", "tokens": ["Das", "dich", ",", "du", "Herr\u00b7li\u00b7che", ",", "um\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Zauber schwebt's von alten Sagen", "tokens": ["Wie", "Zau\u00b7ber", "schwebt's", "von", "al\u00b7ten", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um deine selige Gestalt.", "tokens": ["Um", "dei\u00b7ne", "se\u00b7li\u00b7ge", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wer dich nur schauet, mu\u00df entbrennen", "tokens": ["Wer", "dich", "nur", "schau\u00b7et", ",", "mu\u00df", "ent\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Liebesglut und Andacht gleich;", "tokens": ["In", "Lie\u00b7bes\u00b7glut", "und", "An\u00b7dacht", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df mich deinen Namen nennen:", "tokens": ["So", "la\u00df", "mich", "dei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}