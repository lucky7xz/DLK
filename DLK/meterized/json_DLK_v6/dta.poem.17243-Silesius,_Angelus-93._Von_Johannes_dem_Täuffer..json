{"dta.poem.17243": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "93.  Von Johannes dem T\u00e4uffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Johannes a\u00df fast nichts/ er trug ein rauhes Kleid/", "tokens": ["Jo\u00b7han\u00b7nes", "a\u00df", "fast", "nichts", "/", "er", "trug", "ein", "rau\u00b7hes", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PIS", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sa\u00df in der W\u00fcsteney die gantze Lebenszeit.", "tokens": ["Sa\u00df", "in", "der", "W\u00fcs\u00b7te\u00b7ney", "die", "gant\u00b7ze", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er war so from: was fiel er GOtt so hart zu Fusse?", "tokens": ["Er", "war", "so", "from", ":", "was", "fiel", "er", "Gott", "so", "hart", "zu", "Fus\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "NN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gr\u00f6sten Heiligen die thun die gr\u00f6ste Busse.", "tokens": ["Die", "gr\u00f6s\u00b7ten", "Hei\u00b7li\u00b7gen", "die", "thun", "die", "gr\u00f6s\u00b7te", "Bus\u00b7se", "."], "token_i