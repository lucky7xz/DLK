{"dta.poem.13246": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey dieser Rede war der K\u00f6niginn Gesicht", "tokens": ["Bey", "die\u00b7ser", "Re\u00b7de", "war", "der", "K\u00f6\u00b7ni\u00b7ginn", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "190Best\u00e4ndig nach dem Mund des Ehgemahls gericht;", "tokens": ["nach", "dem", "Mund", "des", "Eh\u00b7ge\u00b7mahls", "ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie seines Eifers Thon, wie seine Worte klungen,", "tokens": ["Wie", "sei\u00b7nes", "Ei\u00b7fers", "Thon", ",", "wie", "sei\u00b7ne", "Wor\u00b7te", "klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sah man, da\u00df jeder Saz in ihr Gem\u00fcth gedrungen.", "tokens": ["Sah", "man", ",", "da\u00df", "je\u00b7der", "Saz", "in", "ihr", "Ge\u00b7m\u00fcth", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Lust vermischte sich mit Ernst und Freundlichkeit;", "tokens": ["Die", "Lust", "ver\u00b7mischte", "sich", "mit", "Ernst", "und", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auch bey dem Tugend-Krei\u00df erschien ein solcher Streit.", "tokens": ["Auch", "bey", "dem", "Tu\u00b7gen\u00b7dKrei\u00df", "er\u00b7schien", "ein", "sol\u00b7cher", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "195Mir war die\u00df kaum genug: ich w\u00fcnschte mehr zu h\u00f6ren,", "tokens": ["war", "die\u00df", "kaum", "ge\u00b7nug", ":", "ich", "w\u00fcnschte", "mehr", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "O, dacht\u2019 ich, dieser F\u00fcrst wird unser Heil vermehren.", "tokens": ["O", ",", "dacht'", "ich", ",", "die\u00b7ser", "F\u00fcrst", "wird", "un\u00b7ser", "Heil", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}