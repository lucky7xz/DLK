{"textgrid.poem.30247": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Schnee der Haut kam zu dem Schnee der Haare,", "tokens": ["Der", "jun\u00b7ge", "Schnee", "der", "Haut", "kam", "zu", "dem", "Schnee", "der", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff da\u00df mit jenem der auff eine Zeit sich paare;", "tokens": ["Auff", "da\u00df", "mit", "je\u00b7nem", "der", "auff", "ei\u00b7ne", "Zeit", "sich", "paa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "PDAT", "ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das paaren gieng wol an, doch ward man zeitlich innen:", "tokens": ["Das", "paa\u00b7ren", "gieng", "wol", "an", ",", "doch", "ward", "man", "zeit\u00b7lich", "in\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Haut-Schnee, der war Glut, der Haar-Schnee muste rinnen.", "tokens": ["Der", "Hau\u00b7tSchnee", ",", "der", "war", "Glut", ",", "der", "Haa\u00b7rSchnee", "mus\u00b7te", "rin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "wo