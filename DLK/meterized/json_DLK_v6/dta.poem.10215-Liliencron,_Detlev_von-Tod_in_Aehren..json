{"dta.poem.10215": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Tod in Aehren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Weizenfeld, in Korn und Mohn,             ", "tokens": ["Im", "Wei\u00b7zen\u00b7feld", ",", "in", "Korn", "und", "Mohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt ein Soldat, unaufgefunden,", "tokens": ["Liegt", "ein", "Sol\u00b7dat", ",", "un\u00b7auf\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwei Tage schon, zwei N\u00e4chte schon,", "tokens": ["Zwei", "Ta\u00b7ge", "schon", ",", "zwei", "N\u00e4ch\u00b7te", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit schweren Wunden, unverbunden.", "tokens": ["Mit", "schwe\u00b7ren", "Wun\u00b7den", ",", "un\u00b7ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durst\u00fcberqu\u00e4lt und fieberwild,", "tokens": ["Dur\u00b7st\u00fc\u00b7berq\u00b7u\u00e4lt", "und", "fie\u00b7ber\u00b7wild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Todeskampf den Kopf erhoben.", "tokens": ["Im", "To\u00b7des\u00b7kampf", "den", "Kopf", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein letzter Traum, ein letztes Bild,", "tokens": ["Ein", "letz\u00b7ter", "Traum", ",", "ein", "letz\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein brechend\u2019 Auge schl\u00e4gt nach oben:", "tokens": ["Sein", "bre\u00b7chend'", "Au\u00b7ge", "schl\u00e4gt", "nach", "o\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Sense rauscht im Aehrenfeld,", "tokens": ["Die", "Sen\u00b7se", "rauscht", "im", "A\u00b7eh\u00b7ren\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er sieht sein Dorf im Arbeitsfrieden.", "tokens": ["Er", "sieht", "sein", "Dorf", "im", "Ar\u00b7beits\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, Ade du Heimatwelt \u2014", "tokens": ["A\u00b7de", ",", "A\u00b7de", "du", "Hei\u00b7mat\u00b7welt"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und beugt das Haupt, und ist verschieden.", "tokens": ["Und", "beugt", "das", "Haupt", ",", "und", "ist", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}