{"textgrid.poem.34422": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Liebe Lust in Liedern auszuklagen,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebe Lust in Liedern auszuklagen,", "tokens": ["Der", "Lie\u00b7be", "Lust", "in", "Lie\u00b7dern", "aus\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "scheint heutzutag dem Dichter fast verwehrt.", "tokens": ["scheint", "heut\u00b7zu\u00b7tag", "dem", "Dich\u00b7ter", "fast", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was k\u00f6nnt er Neues auch den Leuten sagen:", "tokens": ["Was", "k\u00f6nnt", "er", "Neu\u00b7es", "auch", "den", "Leu\u00b7ten", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so mancher hat uns schon sein Gl\u00fcck beschert.", "tokens": ["so", "man\u00b7cher", "hat", "uns", "schon", "sein", "Gl\u00fcck", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Glaubt einer gar der Liebe Leid zu tragen,", "tokens": ["Glaubt", "ei\u00b7ner", "gar", "der", "Lie\u00b7be", "Leid", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "l\u00e4sst er uns sicherlich nicht unversehrt:", "tokens": ["l\u00e4sst", "er", "uns", "si\u00b7cher\u00b7lich", "nicht", "un\u00b7ver\u00b7sehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PRF", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Herz reimt noch stets auf Schmerz, auf Liebe Triebe \u2013", "tokens": ["Herz", "reimt", "noch", "stets", "auf", "Schmerz", ",", "auf", "Lie\u00b7be", "Trie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ich reimte mit Genuss auf beide \u2013 Hiebe!", "tokens": ["ich", "reim\u00b7te", "mit", "Ge\u00b7nuss", "auf", "bei\u00b7de", "\u2013", "Hie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PIS", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So weiss denn selbst der traurigste Philister:", "tokens": ["So", "weiss", "denn", "selbst", "der", "trau\u00b7rigs\u00b7te", "Phi\u00b7lis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Liebe sei so eine Himmelsmacht;", "tokens": ["die", "Lie\u00b7be", "sei", "so", "ei\u00b7ne", "Him\u00b7mels\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in illustrierten Wochenbl\u00e4ttern liest er,", "tokens": ["in", "il\u00b7lust\u00b7rier\u00b7ten", "Wo\u00b7chen\u00b7bl\u00e4t\u00b7tern", "liest", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dass man sich oft sogar drum umgebracht.", "tokens": ["dass", "man", "sich", "oft", "so\u00b7gar", "drum", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Kenner aller Leidenschaften ist er,", "tokens": ["Ein", "Ken\u00b7ner", "al\u00b7ler", "Lei\u00b7den\u00b7schaf\u00b7ten", "ist", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wer ihm nichts Neues bringt, wird ausgelacht:", "tokens": ["wer", "ihm", "nichts", "Neu\u00b7es", "bringt", ",", "wird", "aus\u00b7ge\u00b7lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "kurz, was die Lieb angeht \u2013 ", "tokens": ["kurz", ",", "was", "die", "Lieb", "an\u00b7geht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "es l\u00e4sst sich nichts mehr machen drin. O weh!", "tokens": ["es", "l\u00e4sst", "sich", "nichts", "mehr", "ma\u00b7chen", "drin", ".", "O", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ADV", "VVINF", "PTKVZ", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ist man nun aus purem Pech ein Dichter,", "tokens": ["Und", "ist", "man", "nun", "aus", "pu\u00b7rem", "Pech", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dems schlecht behagt, den andern nachzutreten,", "tokens": ["dems", "schlecht", "be\u00b7hagt", ",", "den", "an\u00b7dern", "nach\u00b7zu\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dems nicht gen\u00fcgt, nur manchmal neue Lichter", "tokens": ["dems", "nicht", "ge\u00b7n\u00fcgt", ",", "nur", "manch\u00b7mal", "neu\u00b7e", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "VVFIN", "$,", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zu pflanzen vor ein Bild, zu dem sie beten \u2013", "tokens": ["zu", "pflan\u00b7zen", "vor", "ein", "Bild", ",", "zu", "dem", "sie", "be\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "so wird man fluchen auf das Reimgelichter,", "tokens": ["so", "wird", "man", "flu\u00b7chen", "auf", "das", "Reim\u00b7ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das auch den sch\u00f6nsten Brei schon breitgetreten,", "tokens": ["das", "auch", "den", "sch\u00f6ns\u00b7ten", "Brei", "schon", "breit\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und wird, obwohl die Sache etwas schwierig,", "tokens": ["und", "wird", ",", "ob\u00b7wohl", "die", "Sa\u00b7che", "et\u00b7was", "schwie\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die Liebe g\u00e4nzlich streichen aus der Lyrik.", "tokens": ["die", "Lie\u00b7be", "g\u00e4nz\u00b7lich", "strei\u00b7chen", "aus", "der", "Ly\u00b7rik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Wie hass ich jene, die naiv wie Thiere", "tokens": ["Wie", "hass", "ich", "je\u00b7ne", ",", "die", "naiv", "wie", "Thie\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$,", "PRELS", "ADJD", "KOKOM", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ihr Lieben schmatzend beichten \u2013 ekelhaft!", "tokens": ["ihr", "Lie\u00b7ben", "schmat\u00b7zend", "beich\u00b7ten", "\u2013", "e\u00b7kel\u00b7haft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VVINF", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unreinem Ohre bei unechtem Biere!", "tokens": ["Un\u00b7rei\u00b7nem", "Oh\u00b7re", "bei", "un\u00b7ech\u00b7tem", "Bie\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch ist nicht schlimmer noch die Leidenschaft,", "tokens": ["Doch", "ist", "nicht", "schlim\u00b7mer", "noch", "die", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "auf unverh\u00fclltem, feilem Druckpapiere", "tokens": ["auf", "un\u00b7ver\u00b7h\u00fcll\u00b7tem", ",", "fei\u00b7lem", "Druck\u00b7pa\u00b7pie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schamlos zu k\u00fcnden, was uns Freuden schafft?", "tokens": ["scham\u00b7los", "zu", "k\u00fcn\u00b7den", ",", "was", "uns", "Freu\u00b7den", "schafft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum Heil dem Dichter, der mit sich gerungen", "tokens": ["Drum", "Heil", "dem", "Dich\u00b7ter", ",", "der", "mit", "sich", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "VVPP"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "und als ein Held zum ", "tokens": ["und", "als", "ein", "Held", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}