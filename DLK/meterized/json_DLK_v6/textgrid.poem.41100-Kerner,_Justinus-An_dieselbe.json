{"textgrid.poem.41100": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier kommt das Buch vom Markte schwer verh\u00f6hnt,", "tokens": ["Hier", "kommt", "das", "Buch", "vom", "Mark\u00b7te", "schwer", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum dritten Male durch den Markt gedrungen.", "tokens": ["Zum", "drit\u00b7ten", "Ma\u00b7le", "durch", "den", "Markt", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn mich das Lied mit Blumen hat gekr\u00f6nt,", "tokens": ["Wenn", "mich", "das", "Lied", "mit", "Blu\u00b7men", "hat", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat Dornen mir dies Buch ums Haupt geschlungen.", "tokens": ["Hat", "Dor\u00b7nen", "mir", "dies", "Buch", "ums", "Haupt", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "PDS", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl heilig ist der Blumenkranz f\u00fcrs Lied,", "tokens": ["Wohl", "hei\u00b7lig", "ist", "der", "Blu\u00b7men\u00b7kranz", "f\u00fcrs", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch jenem, der des Innern Tiefen glaubte,", "tokens": ["Doch", "je\u00b7nem", ",", "der", "des", "In\u00b7nern", "Tie\u00b7fen", "glaub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr seinen Glauben k\u00e4mpfte nimmer m\u00fcd',", "tokens": ["F\u00fcr", "sei\u00b7nen", "Glau\u00b7ben", "k\u00e4mpf\u00b7te", "nim\u00b7mer", "m\u00fcd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist heiliger der Dornenkranz im Haupte.", "tokens": ["Ist", "hei\u00b7li\u00b7ger", "der", "Dor\u00b7nen\u00b7kranz", "im", "Haup\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit solchem tret' ich jetzt zum erstenmal", "tokens": ["Mit", "sol\u00b7chem", "tret'", "ich", "jetzt", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "VVFIN", "PPER", "ADV", "APPRART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Des Winzers Jubel schweigt auf Berg und Tal,", "tokens": ["Des", "Win\u00b7zers", "Ju\u00b7bel", "schweigt", "auf", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Feste ruhen in des K\u00f6nigs Hallen.", "tokens": ["Die", "Fes\u00b7te", "ru\u00b7hen", "in", "des", "K\u00f6\u00b7nigs", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sieh! in meines Gartens stillem Raum", "tokens": ["Und", "sieh", "!", "in", "mei\u00b7nes", "Gar\u00b7tens", "stil\u00b7lem", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4llt Blatt um Blatt vom Baum, wo ", "tokens": ["F\u00e4llt", "Blatt", "um", "Blatt", "vom", "Baum", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "APPR", "NN", "APPRART", "NN", "$,", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein lichtes Bild aus einem Fr\u00fchlingstraum,", "tokens": ["Ein", "lich\u00b7tes", "Bild", "aus", "ei\u00b7nem", "Fr\u00fch\u00b7lings\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einmal getr\u00e4umt und nimmer zu vergessen.", "tokens": ["Ein\u00b7mal", "ge\u00b7tr\u00e4umt", "und", "nim\u00b7mer", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Da sang der Vogel, duftete die Flur.", "tokens": ["Da", "sang", "der", "Vo\u00b7gel", ",", "duf\u00b7te\u00b7te", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie war Dein Auge licht, wie licht der Himmel!", "tokens": ["Wie", "war", "Dein", "Au\u00b7ge", "licht", ",", "wie", "licht", "der", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "NN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lenz, Sommer, Herbst verschwanden, die Natur", "tokens": ["Lenz", ",", "Som\u00b7mer", ",", "Herbst", "ver\u00b7schwan\u00b7den", ",", "die", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Innre flieht aus irdischem Get\u00fcmmel.", "tokens": ["Ins", "Inn\u00b7re", "flieht", "aus", "ir\u00b7di\u00b7schem", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch herrlicher ergl\u00e4nzt der Sterne Pracht,", "tokens": ["Doch", "herr\u00b7li\u00b7cher", "er\u00b7gl\u00e4nzt", "der", "Ster\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging Vogelsang und Blumenduft zu Ende.", "tokens": ["Ging", "Vo\u00b7gel\u00b7sang", "und", "Blu\u00b7men\u00b7duft", "zu", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Winter naht, die stille Geisternacht,", "tokens": ["Der", "Win\u00b7ter", "naht", ",", "die", "stil\u00b7le", "Geis\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihr leg' ich dies Buch in Deine H\u00e4nde.", "tokens": ["In", "ihr", "leg'", "ich", "dies", "Buch", "in", "Dei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PDS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}