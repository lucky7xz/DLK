{"textgrid.poem.38498": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit grausem Get\u00fcmmel,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit grausem Get\u00fcmmel,", "tokens": ["Mit", "grau\u00b7sem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entfliehen vom Himmel", "tokens": ["Ent\u00b7flie\u00b7hen", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gew\u00f6lke voll Nacht!", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "voll", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Seht! wie sie, zerrissen,", "tokens": ["Seht", "!", "wie", "sie", ",", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In Regen zerfliessen,", "tokens": ["In", "Re\u00b7gen", "zer\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Sturme gejagt!", "tokens": ["Vom", "Stur\u00b7me", "ge\u00b7jagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nun kehret, voll Wonne,", "tokens": ["Nun", "keh\u00b7ret", ",", "voll", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dein L\u00e4cheln, o Sonne!", "tokens": ["Dein", "L\u00e4\u00b7cheln", ",", "o", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den Fluren zur\u00fck;", "tokens": ["Den", "Flu\u00b7ren", "zu\u00b7r\u00fck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit segnender Milde", "tokens": ["Mit", "seg\u00b7nen\u00b7der", "Mil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Begr\u00fc\u00dft die Gefilde", "tokens": ["Be\u00b7gr\u00fc\u00dft", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dein himlischer Blik!", "tokens": ["Dein", "him\u00b7li\u00b7scher", "Blik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Nun sprossen und keimen", "tokens": ["Nun", "spros\u00b7sen", "und", "kei\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aus B\u00fcschen und B\u00e4umen", "tokens": ["Aus", "B\u00fc\u00b7schen", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Bl\u00e4tter hervor!", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nun rieselt der Quelle", "tokens": ["Nun", "rie\u00b7selt", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hellsch\u00e4umende Welle", "tokens": ["Hell\u00b7sch\u00e4u\u00b7men\u00b7de", "Wel\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch wankendes Rohr!", "tokens": ["Durch", "wan\u00b7ken\u00b7des", "Rohr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Die Bienen umirren,", "tokens": ["Die", "Bie\u00b7nen", "um\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit fr\u00f6hlichem Schwirren,", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Violen voll Thau!", "tokens": ["Vi\u00b7o\u00b7len", "voll", "Thau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sanftathmende L\u00fcfte", "tokens": ["Sanf\u00b7tath\u00b7men\u00b7de", "L\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Entschmeicheln Ged\u00fcfte", "tokens": ["Ent\u00b7schmei\u00b7cheln", "Ge\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Kr\u00e4utern der Au!", "tokens": ["Den", "Kr\u00e4u\u00b7tern", "der", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Horch! wie in den Thalen,", "tokens": ["Horch", "!", "wie", "in", "den", "Tha\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Die bunter sich malen,", "tokens": ["Die", "bun\u00b7ter", "sich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das Wollenvieh bl\u00f6kt;", "tokens": ["Das", "Wol\u00b7len\u00b7vieh", "bl\u00f6kt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und fern in den dichten,", "tokens": ["Und", "fern", "in", "den", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Umd\u00fcsterten Fichten", "tokens": ["Um\u00b7d\u00fcs\u00b7ter\u00b7ten", "Fich\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Wiederhall wekt!", "tokens": ["Den", "Wie\u00b7der\u00b7hall", "wekt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Durch Pappelalleen,", "tokens": ["Durch", "Pap\u00b7pe\u00b7lal\u00b7le\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An bl\u00e4ulichen Seen,", "tokens": ["An", "bl\u00e4u\u00b7li\u00b7chen", "Seen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schallt Liederget\u00f6n!", "tokens": ["Schallt", "Lie\u00b7der\u00b7ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Im rosigen Kleide,", "tokens": ["Im", "ro\u00b7si\u00b7gen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schwebt l\u00e4chelnd die Freude", "tokens": ["Schwebt", "l\u00e4\u00b7chelnd", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von blumichten H\u00f6h'n!", "tokens": ["Von", "blu\u00b7mich\u00b7ten", "H\u00f6h'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Sie winkt, unter K\u00fcssen", "tokens": ["Sie", "winkt", ",", "un\u00b7ter", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Lenz zu begr\u00fcssen,", "tokens": ["Den", "Lenz", "zu", "be\u00b7gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die M\u00e4dchen zum Hain,", "tokens": ["Die", "M\u00e4d\u00b7chen", "zum", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und schlingt sich in gr\u00fcnen", "tokens": ["Und", "schlingt", "sich", "in", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Geb\u00fcschen mit ihnen", "tokens": ["Ge\u00b7b\u00fc\u00b7schen", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im zirkelnden Reihn!", "tokens": ["Im", "zir\u00b7keln\u00b7den", "Reihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Blikt fr\u00f6hlichen Zechern,", "tokens": ["Blikt", "fr\u00f6h\u00b7li\u00b7chen", "Ze\u00b7chern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei funkelnden Bechern,", "tokens": ["Bei", "fun\u00b7keln\u00b7den", "Be\u00b7chern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sokratischen Scherz,", "tokens": ["Sok\u00b7ra\u00b7ti\u00b7schen", "Scherz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und feuriges Sehnen", "tokens": ["Und", "feu\u00b7ri\u00b7ges", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nach l\u00e4chelnden Sch\u00f6nen", "tokens": ["Nach", "l\u00e4\u00b7cheln\u00b7den", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ins gl\u00fchende Herz!", "tokens": ["Ins", "gl\u00fc\u00b7hen\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Da eilen die Stunden,", "tokens": ["Da", "ei\u00b7len", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Rosen umwunden,", "tokens": ["Mit", "Ro\u00b7sen", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit Wonne beschwingt!", "tokens": ["Mit", "Won\u00b7ne", "be\u00b7schwingt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Becher erklingen!", "tokens": ["Die", "Be\u00b7cher", "er\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie scherzen und singen", "tokens": ["Sie", "scher\u00b7zen", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bis Hesperus sinkt!", "tokens": ["Bis", "Hes\u00b7pe\u00b7rus", "sinkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}