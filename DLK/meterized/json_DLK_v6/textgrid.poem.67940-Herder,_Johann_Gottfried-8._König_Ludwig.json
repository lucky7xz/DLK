{"textgrid.poem.67940": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8. K\u00f6nig Ludwig", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen K\u00f6nig wei\u00df ich,", "tokens": ["Ei\u00b7nen", "K\u00f6\u00b7nig", "wei\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Heisset Herr Ludwig,", "tokens": ["Heis\u00b7set", "Herr", "Lud\u00b7wig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der gern Gott dienet,", "tokens": ["Der", "gern", "Gott", "die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weil ers ihm lohnet.", "tokens": ["Weil", "ers", "ihm", "loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kind ward er vaterlos,", "tokens": ["Kind", "ward", "er", "va\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "De\u00df ward ihm sehr bos:", "tokens": ["De\u00df", "ward", "ihm", "sehr", "bos", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hervor holt' ihn Gott,", "tokens": ["Her\u00b7vor", "holt'", "ihn", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ihn selbst erzog.", "tokens": ["Ihn", "selbst", "er\u00b7zog", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Gab ihm t\u00fcgende", "tokens": ["Gab", "ihm", "t\u00fc\u00b7gen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Frone Dienende;", "tokens": ["Fro\u00b7ne", "Die\u00b7nen\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Stuhl hier in Franken:", "tokens": ["Stuhl", "hier", "in", "Fran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Brauch er ihn lange!", "tokens": ["Brauch", "er", "ihn", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Den theilt er dann", "tokens": ["Den", "theilt", "er", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit Karlomann,", "tokens": ["Mit", "Kar\u00b7lo\u00b7mann", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem Bruder sein,", "tokens": ["Dem", "Bru\u00b7der", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ohn allen Wahn.", "tokens": ["Ohn", "al\u00b7len", "Wahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Das war geendet,", "tokens": ["Das", "war", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da wollt Gott pr\u00fcfen:", "tokens": ["Da", "wollt", "Gott", "pr\u00fc\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ob er Arbeiten", "tokens": ["Ob", "er", "Ar\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auch mochte leiden?", "tokens": ["Auch", "moch\u00b7te", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Lie\u00df der Heidenm\u00e4nner", "tokens": ["Lie\u00df", "der", "Hei\u00b7den\u00b7m\u00e4n\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ueber sie kommen;", "tokens": ["Ue\u00b7ber", "sie", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lie\u00df seine Franken", "tokens": ["Lie\u00df", "sei\u00b7ne", "Fran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Heiden dienen.", "tokens": ["Den", "Hei\u00b7den", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Die giengen verloren!", "tokens": ["Die", "gien\u00b7gen", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die wurden erkoren!", "tokens": ["Die", "wur\u00b7den", "er\u00b7ko\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der ward verschm\u00e4het,", "tokens": ["Der", "ward", "ver\u00b7schm\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der ihnen mi\u00dflebt.", "tokens": ["Der", "ih\u00b7nen", "mi\u00df\u00b7lebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Wer da ein Dieb was,", "tokens": ["Wer", "da", "ein", "Dieb", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PWS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der de\u00df gena\u00df,", "tokens": ["Der", "de\u00df", "ge\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Nahm seine Festung,", "tokens": ["Nahm", "sei\u00b7ne", "Fes\u00b7tung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Seit war er Gutmann", "tokens": ["Seit", "war", "er", "Gut\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Der war ein L\u00fcgner,", "tokens": ["Der", "war", "ein", "L\u00fcg\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der war ein R\u00e4uber,", "tokens": ["Der", "war", "ein", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der ein Verr\u00e4ther:", "tokens": ["Der", "ein", "Ver\u00b7r\u00e4\u00b7ther", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und er geberdt sich de\u00df.", "tokens": ["Und", "er", "ge\u00b7berdt", "sich", "de\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "K\u00f6nig war ger\u00fchret,", "tokens": ["K\u00f6\u00b7nig", "war", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das Reich verwirret,", "tokens": ["Das", "Reich", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Erz\u00fcrnt war Christ,", "tokens": ["Er\u00b7z\u00fcrnt", "war", "Christ", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Litt dies Entgeltni\u00df.", "tokens": ["Litt", "dies", "Ent\u00b7gelt\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Da erbarmt es Gott,", "tokens": ["Da", "er\u00b7barmt", "es", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der wust all die Noth,", "tokens": ["Der", "wust", "all", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hie\u00df Herr Ludwig", "tokens": ["Hie\u00df", "Herr", "Lud\u00b7wig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Eilig herbeiziehn.", "tokens": ["Ei\u00b7lig", "her\u00b7bei\u00b7ziehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbludwig, K\u00f6nig mein,", "tokens": ["\u00bb", "lud\u00b7wig", ",", "K\u00f6\u00b7nig", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hilf meinen Leuten!", "tokens": ["Hilf", "mei\u00b7nen", "Leu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es haben sie Normannen", "tokens": ["Es", "ha\u00b7ben", "sie", "Nor\u00b7man\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Harte bezwungen.\u00ab", "tokens": ["Har\u00b7te", "be\u00b7zwun\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Dann sprach Ludwig:", "tokens": ["Dann", "sprach", "Lud\u00b7wig", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "\u00bbherr! so thu ich.", "tokens": ["\u00bb", "herr", "!", "so", "thu", "ich", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Tod nicht rette mir es,", "tokens": ["Tod", "nicht", "ret\u00b7te", "mir", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-++", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Was du gebietest.\u00ab", "tokens": ["Was", "du", "ge\u00b7bie\u00b7test", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Da nahm er Gotts Urlaub,", "tokens": ["Da", "nahm", "er", "Gotts", "Ur\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hob die Kundfahn auf:", "tokens": ["Hob", "die", "Kund\u00b7fahn", "auf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Reitet in Franken", "tokens": ["Rei\u00b7tet", "in", "Fran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Entgegen den Normannen.", "tokens": ["Ent\u00b7ge\u00b7gen", "den", "Nor\u00b7man\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Gotte dankend,", "tokens": ["Got\u00b7te", "dan\u00b7kend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Diesem harrend,", "tokens": ["Die\u00b7sem", "har\u00b7rend", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sprach: \u00bbO Herr mein,", "tokens": ["Sprach", ":", "\u00bb", "O", "Herr", "mein", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "NN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Lange harren wir dein.\u00ab", "tokens": ["Lan\u00b7ge", "har\u00b7ren", "wir", "dein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Sprach dann mit Muthe,", "tokens": ["Sprach", "dann", "mit", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ludwig der Gute:", "tokens": ["Lud\u00b7wig", "der", "Gu\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbtr\u00f6stet euch, Gesellen,", "tokens": ["\u00bb", "tr\u00f6s\u00b7tet", "euch", ",", "Ge\u00b7sel\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die mir in Noth stehn.", "tokens": ["Die", "mir", "in", "Noth", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Her sandte mich Gott!", "tokens": ["Her", "sand\u00b7te", "mich", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Th\u00e4t mir selbst die Gnad,", "tokens": ["Th\u00e4t", "mir", "selbst", "die", "Gnad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob ihr mir Rath thut,", "tokens": ["Ob", "ihr", "mir", "Rath", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df ich euch f\u00fchre.", "tokens": ["Da\u00df", "ich", "euch", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Mich selbst nicht spar' ich,", "tokens": ["Mich", "selbst", "nicht", "spa\u00b7r'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bis ich befrei euch:", "tokens": ["Bis", "ich", "be\u00b7frei", "euch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nu will ich, da\u00df mir folgen", "tokens": ["Nu", "will", "ich", ",", "da\u00df", "mir", "fol\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All Gottes Holden.", "tokens": ["All", "Got\u00b7tes", "Hol\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Beschert ist uns die Hierfrist,", "tokens": ["Be\u00b7schert", "ist", "uns", "die", "Hier\u00b7frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang es will Christ.", "tokens": ["So", "lang", "es", "will", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er wartet unser Gebein,", "tokens": ["Er", "war\u00b7tet", "un\u00b7ser", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wacht selbst darein.", "tokens": ["Wacht", "selbst", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.20": {"line.1": {"text": "Wer nun Gottes Willen", "tokens": ["Wer", "nun", "Got\u00b7tes", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eilig will erf\u00fcllen;", "tokens": ["Ei\u00b7lig", "will", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt er gesund aus,", "tokens": ["Kommt", "er", "ge\u00b7sund", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lohn ich ihm das;", "tokens": ["Lohn", "ich", "ihm", "das", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PDS", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bleibet er drinne,", "tokens": ["Blei\u00b7bet", "er", "drin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Lohn ichs den Seinen.\u00ab", "tokens": ["Lohn", "ichs", "den", "Sei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIS", "ART", "PPOSS", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.21": {"line.1": {"text": "Da nahm er Schild und Speer,", "tokens": ["Da", "nahm", "er", "Schild", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ritt eilig daher,", "tokens": ["Ritt", "ei\u00b7lig", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wollt wahrlich r\u00e4chen", "tokens": ["Wollt", "wahr\u00b7lich", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Seine Widersacher.", "tokens": ["Sei\u00b7ne", "Wi\u00b7der\u00b7sa\u00b7cher", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Das war nicht lange,", "tokens": ["Das", "war", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Fand er die Normannen:", "tokens": ["Fand", "er", "die", "Nor\u00b7man\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gottlob! rief er,", "tokens": ["Gott\u00b7lob", "!", "rief", "er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Seinen Wunsch sah er.", "tokens": ["Sei\u00b7nen", "Wunsch", "sah", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-++-", "measure": "unknown.measure.tri"}}, "stanza.23": {"line.1": {"text": "Der K\u00f6nig reitet k\u00fchn,", "tokens": ["Der", "K\u00f6\u00b7nig", "rei\u00b7tet", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sang lautes Lied,", "tokens": ["Sang", "lau\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und alle sungen:", "tokens": ["Und", "al\u00b7le", "sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kyrie Eleison.", "tokens": ["Ky\u00b7rie", "E\u00b7lei\u00b7son", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.24": {"line.1": {"text": "Sang war gesungen,", "tokens": ["Sang", "war", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schlacht ward begonnen,", "tokens": ["Schlacht", "ward", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Blut schien in den Wangen,", "tokens": ["Blut", "schien", "in", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Spielender Franken.", "tokens": ["Spie\u00b7len\u00b7der", "Fran\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da r\u00e4cht jeder sich.", "tokens": ["Da", "r\u00e4cht", "je\u00b7der", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Keiner wie Ludwig.", "tokens": ["Kei\u00b7ner", "wie", "Lud\u00b7wig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.25": {"line.1": {"text": "Schnell und k\u00fchn,", "tokens": ["Schnell", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "War je sein Sinn.", "tokens": ["War", "je", "sein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jenen durchschlug er,", "tokens": ["Je\u00b7nen", "durch\u00b7schlug", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Diesen durchstach er,", "tokens": ["Die\u00b7sen", "durch\u00b7stach", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.26": {"line.1": {"text": "Schenkte zu Handen", "tokens": ["Schenk\u00b7te", "zu", "Han\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seinen Feinden", "tokens": ["Sei\u00b7nen", "Fein\u00b7den"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Trank bittern Leides,", "tokens": ["Trank", "bit\u00b7tern", "Lei\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So wichen sie Leibes.", "tokens": ["So", "wi\u00b7chen", "sie", "Lei\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.27": {"line.1": {"text": "Gelobt sey Gottes Kraft!", "tokens": ["Ge\u00b7lobt", "sey", "Got\u00b7tes", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ludwig ward sieghaft.", "tokens": ["Lud\u00b7wig", "ward", "sieg\u00b7haft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sagt allen Heiligen Dank!", "tokens": ["Sagt", "al\u00b7len", "Hei\u00b7li\u00b7gen", "Dank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sein ward der Siegkampf.", "tokens": ["Sein", "ward", "der", "Sieg\u00b7kampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.28": {"line.1": {"text": "O wie ward Ludwig", "tokens": ["O", "wie", "ward", "Lud\u00b7wig"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "K\u00f6nig so selig!", "tokens": ["K\u00f6\u00b7nig", "so", "se\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hurtig er war,", "tokens": ["Hur\u00b7tig", "er", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VAFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schwer, wie es noth war!", "tokens": ["Schwer", ",", "wie", "es", "noth", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "NN", "VAFIN", "$."], "meter": "+--++", "measure": "adoneus"}, "line.5": {"text": "Erhalt ihn, Herr Gott!", "tokens": ["Er\u00b7halt", "ihn", ",", "Herr", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bei seinen Rechten.", "tokens": ["Bei", "sei\u00b7nen", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}