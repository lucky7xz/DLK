{"textgrid.poem.50761": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bricht aus mir ein bunter Faschingszug", "tokens": ["Es", "bricht", "aus", "mir", "ein", "bun\u00b7ter", "Fa\u00b7schings\u00b7zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zieht dahin mit t\u00f6nendem Gepr\u00e4nge;", "tokens": ["Und", "zieht", "da\u00b7hin", "mit", "t\u00f6\u00b7nen\u00b7dem", "Ge\u00b7pr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tal\u00fcber wallt im luftigen Gedr\u00e4nge", "tokens": ["Ta\u00b7l\u00fc\u00b7ber", "wallt", "im", "luf\u00b7ti\u00b7gen", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Bilderreigen, mein Gedankenflug!", "tokens": ["Ein", "Bil\u00b7der\u00b7rei\u00b7gen", ",", "mein", "Ge\u00b7dan\u00b7ken\u00b7flug", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie spielend sie die Luft hin\u00fcbertrug,", "tokens": ["Wie", "spie\u00b7lend", "sie", "die", "Luft", "hin\u00b7\u00fc\u00b7bert\u00b7rug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ranken sich, ein \u00fcppig Laubgeh\u00e4nge,", "tokens": ["So", "ran\u00b7ken", "sich", ",", "ein", "\u00fcp\u00b7pig", "Laub\u00b7ge\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis auf zum Giebel, meine Nachtges\u00e4nge", "tokens": ["Bis", "auf", "zum", "Gie\u00b7bel", ",", "mei\u00b7ne", "Nacht\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPR", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rings um ihr Haus, ein zauberischer Trug!", "tokens": ["Rings", "um", "ihr", "Haus", ",", "ein", "zau\u00b7be\u00b7ri\u00b7scher", "Trug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es rauscht und schwillt und bricht ins Schlafgemach", "tokens": ["Es", "rauscht", "und", "schwillt", "und", "bricht", "ins", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und singt und klingt die reine Seele wach,", "tokens": ["Und", "singt", "und", "klingt", "die", "rei\u00b7ne", "See\u00b7le", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bet\u00e4ubt tritt sie in meine Blumenschlingen!", "tokens": ["Be\u00b7t\u00e4ubt", "tritt", "sie", "in", "mei\u00b7ne", "Blu\u00b7men\u00b7schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun ist es Zeit, mein Herz! mach dich hinzu!", "tokens": ["Nun", "ist", "es", "Zeit", ",", "mein", "Herz", "!", "mach", "dich", "hin\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nachtwandelnd wei\u00df sie's nicht und lauscht in Ruh:", "tokens": ["Nacht\u00b7wan\u00b7delnd", "wei\u00df", "sie's", "nicht", "und", "lauscht", "in", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kannst alles, alles ihr zu Ohren bringen!", "tokens": ["Kannst", "al\u00b7les", ",", "al\u00b7les", "ihr", "zu", "Oh\u00b7ren", "brin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PIS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}