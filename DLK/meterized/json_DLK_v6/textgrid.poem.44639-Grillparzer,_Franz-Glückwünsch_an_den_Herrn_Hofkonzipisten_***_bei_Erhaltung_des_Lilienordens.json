{"textgrid.poem.44639": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnsch an den Herrn Hofkonzipisten *** bei Erhaltung des Lilienordens", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie passend schm\u00fcckt dich der Lilie Zier,", "tokens": ["Wie", "pas\u00b7send", "schm\u00fcckt", "dich", "der", "Li\u00b7lie", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie wird zum symbolischen Zeichen an Dir.", "tokens": ["Sie", "wird", "zum", "sym\u00b7bo\u00b7li\u00b7schen", "Zei\u00b7chen", "an", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie \u00e4hnlich seid ihr euch beide!", "tokens": ["Wie", "\u00e4hn\u00b7lich", "seid", "ihr", "euch", "bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPER", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer denkt nicht an das, was die Bibel spricht:", "tokens": ["Wer", "denkt", "nicht", "an", "das", ",", "was", "die", "Bi\u00b7bel", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbdie Lilie, sie ackert und spinnet nicht", "tokens": ["\u00bb", "die", "Li\u00b7lie", ",", "sie", "ac\u00b7kert", "und", "spin\u00b7net", "nicht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und prangt doch in k\u00f6stlichem Kleide\u00ab.", "tokens": ["Und", "prangt", "doch", "in", "k\u00f6st\u00b7li\u00b7chem", "Klei\u00b7de", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$(", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}