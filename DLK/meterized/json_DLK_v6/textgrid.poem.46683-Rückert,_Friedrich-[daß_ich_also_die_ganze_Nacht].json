{"textgrid.poem.46683": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df ich also die ganze Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich also die ganze Nacht,", "tokens": ["Da\u00df", "ich", "al\u00b7so", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Von erquickendem Duft durchthaut", "tokens": ["Von", "er\u00b7qui\u00b7cken\u00b7dem", "Duft", "durcht\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bis ins Mark und die Knochen \u2013", "tokens": ["Bis", "ins", "Mark", "und", "die", "Kno\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "KON", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df ich also die ganze Nacht,", "tokens": ["Da\u00df", "ich", "al\u00b7so", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Unerweckt von des Seufzers Laut,", "tokens": ["Un\u00b7er\u00b7weckt", "von", "des", "Seuf\u00b7zers", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Noch vom Herzen mit Pochen \u2013", "tokens": ["Noch", "vom", "Her\u00b7zen", "mit", "Po\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Da\u00df ich also die ganze Nacht,", "tokens": ["Da\u00df", "ich", "al\u00b7so", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Von Traumseligkeit angeschaut", "tokens": ["Von", "Traum\u00b7se\u00b7lig\u00b7keit", "an\u00b7ge\u00b7schaut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und zur Ruhe gesprochen \u2013", "tokens": ["Und", "zur", "Ru\u00b7he", "ge\u00b7spro\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Da\u00df ich also die ganze Nacht,", "tokens": ["Da\u00df", "ich", "al\u00b7so", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Ueberschattet von bl\u00fchendem Kraut,", "tokens": ["Ue\u00b7ber\u00b7schat\u00b7tet", "von", "bl\u00fc\u00b7hen\u00b7dem", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Nicht von Nesseln gestochen \u2013", "tokens": ["Nicht", "von", "Nes\u00b7seln", "ge\u00b7sto\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVPP", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Da\u00df ich also die ganze Nacht", "tokens": ["Da\u00df", "ich", "al\u00b7so", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.14": {"text": "Schlafen k\u00f6nnte, so lieb und traut,", "tokens": ["Schla\u00b7fen", "k\u00f6nn\u00b7te", ",", "so", "lieb", "und", "traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Still und ununterbrochen,", "tokens": ["Still", "und", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Wie ihr beiden nun Tag und Nacht", "tokens": ["Wie", "ihr", "bei\u00b7den", "nun", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "ADV", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Schlaft, vom k\u00fchlen Gemach umbaut", "tokens": ["Schlaft", ",", "vom", "k\u00fch\u00b7len", "Ge\u00b7mach", "um\u00b7baut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "APPR"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Schlaft seit Monden und Wochen!", "tokens": ["Schlaft", "seit", "Mon\u00b7den", "und", "Wo\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}