{"textgrid.poem.60135": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo war ich, als dich Morgensterne lobten?", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo war ich, als dich Morgensterne lobten?", "tokens": ["Wo", "war", "ich", ",", "als", "dich", "Mor\u00b7gens\u00b7ter\u00b7ne", "lob\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da, wie aus Windeln du gewickelt hast das Meer!", "tokens": ["Da", ",", "wie", "aus", "Win\u00b7deln", "du", "ge\u00b7wi\u00b7ckelt", "hast", "das", "Meer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und als vor dir die Wellen tobten,", "tokens": ["Und", "als", "vor", "dir", "die", "Wel\u00b7len", "tob\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihnen sprachest: kommet, bis hieher!", "tokens": ["Zu", "ih\u00b7nen", "spra\u00b7chest", ":", "kom\u00b7met", ",", "bis", "hie\u00b7her", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "VVFIN", "$,", "KON", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo lag ich, als dein Arm der Erde Gr\u00e4nzen", "tokens": ["Wo", "lag", "ich", ",", "als", "dein", "Arm", "der", "Er\u00b7de", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umher gezogen hat, und ihren Grund gelegt?", "tokens": ["Um\u00b7her", "ge\u00b7zo\u00b7gen", "hat", ",", "und", "ih\u00b7ren", "Grund", "ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als du die Morgenr\u00f6the gl\u00e4nzen", "tokens": ["Als", "du", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Purpur hiessest, den sie um sich tr\u00e4gt?", "tokens": ["Mit", "Pur\u00b7pur", "hies\u00b7sest", ",", "den", "sie", "um", "sich", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In ungeformtem Klumpen noch gelegen", "tokens": ["In", "un\u00b7ge\u00b7form\u00b7tem", "Klum\u00b7pen", "noch", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich, als auf dein Wort der Tag hervor geeilt", "tokens": ["Bin", "ich", ",", "als", "auf", "dein", "Wort", "der", "Tag", "her\u00b7vor", "ge\u00b7eilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Thau gezeugt ward, und der Regen", "tokens": ["Der", "Thau", "ge\u00b7zeugt", "ward", ",", "und", "der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Finsterni\u00df von Lichte ward getheilt!", "tokens": ["Und", "Fins\u00b7ter\u00b7ni\u00df", "von", "Lich\u00b7te", "ward", "ge\u00b7theilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch gleich dem kleinsten Staube, den die Sonne", "tokens": ["Noch", "gleich", "dem", "kleins\u00b7ten", "Stau\u00b7be", ",", "den", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hei\u00dfscheinend an sich zieht von d\u00fcrrer Erde Schoo\u00df,", "tokens": ["Hei\u00df\u00b7schei\u00b7nend", "an", "sich", "zieht", "von", "d\u00fcr\u00b7rer", "Er\u00b7de", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War ich doch schon der Engel Wonne,", "tokens": ["War", "ich", "doch", "schon", "der", "En\u00b7gel", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dir erschaffen, war ich ihnen gro\u00df.", "tokens": ["Von", "dir", "er\u00b7schaf\u00b7fen", ",", "war", "ich", "ih\u00b7nen", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mit Sternenkleidern herrlich angezogen", "tokens": ["Mit", "Ster\u00b7nen\u00b7klei\u00b7dern", "herr\u00b7lich", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du, Gott Sch\u00f6pfer! sie dem Winde gleich gemacht;", "tokens": ["Hast", "du", ",", "Gott", "Sch\u00f6p\u00b7fer", "!", "sie", "dem", "Win\u00b7de", "gleich", "ge\u00b7macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "NN", "$.", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sch\u00f6nfarbigt wie der Regenbogen", "tokens": ["Sch\u00f6n\u00b7far\u00b7bigt", "wie", "der", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sonnenglut, ist ihrer Leiber Pracht.", "tokens": ["Wie", "Son\u00b7nen\u00b7glut", ",", "ist", "ih\u00b7rer", "Lei\u00b7ber", "Pracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zum Dienst erschaffen f\u00fcr die Menschenkinder", "tokens": ["Zum", "Dienst", "er\u00b7schaf\u00b7fen", "f\u00fcr", "die", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind sie; sie eilen, Gott! wenn du Befehle blickst,", "tokens": ["Sind", "sie", ";", "sie", "ei\u00b7len", ",", "Gott", "!", "wenn", "du", "Be\u00b7feh\u00b7le", "blickst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "NN", "$.", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch deinen Himmel viel geschwinder", "tokens": ["Durch", "dei\u00b7nen", "Him\u00b7mel", "viel", "ge\u00b7schwin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als deine Blitze, die du flammigt schickst!", "tokens": ["Als", "dei\u00b7ne", "Blit\u00b7ze", ",", "die", "du", "flam\u00b7migt", "schickst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Aus Aether sind zusammen sie geflossen:", "tokens": ["Aus", "A\u00b7e\u00b7ther", "sind", "zu\u00b7sam\u00b7men", "sie", "ge\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich ward, wie Staub, der auf der Flur zusammen l\u00e4uft,", "tokens": ["Ich", "ward", ",", "wie", "Staub", ",", "der", "auf", "der", "Flur", "zu\u00b7sam\u00b7men", "l\u00e4uft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann deine Wolken ihn begossen", "tokens": ["Wann", "dei\u00b7ne", "Wol\u00b7ken", "ihn", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Klo\u00df an Klo\u00df sich nun zusammen h\u00e4uft.", "tokens": ["Und", "Klo\u00df", "an", "Klo\u00df", "sich", "nun", "zu\u00b7sam\u00b7men", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich ward; dein Sprechen: La\u00dft uns Menschen machen!", "tokens": ["Ich", "ward", ";", "dein", "Spre\u00b7chen", ":", "La\u00dft", "uns", "Men\u00b7schen", "ma\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ri\u00df auch mich hervor, als du des Lebens Th\u00fcr", "tokens": ["Das", "ri\u00df", "auch", "mich", "her\u00b7vor", ",", "als", "du", "des", "Le\u00b7bens", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entriegeltest, und noch der Rachen", "tokens": ["Ent\u00b7rie\u00b7gel\u00b7test", ",", "und", "noch", "der", "Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Grabes nicht er\u00f6fnet war vor dir!", "tokens": ["Das", "Gra\u00b7bes", "nicht", "er\u00b7\u00f6f\u00b7net", "war", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Jahrtausende vergiengen, kurze Tage", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7gien\u00b7gen", ",", "kur\u00b7ze", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor deinem Angesicht! dann kam mein Tag, und du", "tokens": ["Vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "!", "dann", "kam", "mein", "Tag", ",", "und", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gabst mir die H\u00fclle, die ich trage", "tokens": ["Gabst", "mir", "die", "H\u00fcl\u00b7le", ",", "die", "ich", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um diesen Geist von dir geathmet, zu!", "tokens": ["Um", "die\u00b7sen", "Geist", "von", "dir", "ge\u00b7ath\u00b7met", ",", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "APPR", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Von deinem Munde, der mit einem Hauche", "tokens": ["Von", "dei\u00b7nem", "Mun\u00b7de", ",", "der", "mit", "ei\u00b7nem", "Hau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00fcrge bl\u00e4set tief herunter in das Meer,", "tokens": ["Ge\u00b7b\u00fcr\u00b7ge", "bl\u00e4\u00b7set", "tief", "her\u00b7un\u00b7ter", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nahm ich dis Leben zum Gebrauche,", "tokens": ["Nahm", "ich", "dis", "Le\u00b7ben", "zum", "Ge\u00b7brau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinem Ruhm; Herr! mein Gesang sey er!", "tokens": ["Zu", "dei\u00b7nem", "Ruhm", ";", "Herr", "!", "mein", "Ge\u00b7sang", "sey", "er", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}