{"textgrid.poem.57223": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sahst du die Thr\u00e4ne, welche mein Herz vergoss,", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du die Thr\u00e4ne, welche mein Herz vergoss,", "tokens": ["Sahst", "du", "die", "Thr\u00e4\u00b7ne", ",", "wel\u00b7che", "mein", "Herz", "ver\u00b7goss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Ebert? Traurend lehn' ich auf dich mich hin.", "tokens": ["Mein", "E\u00b7bert", "?", "Trau\u00b7rend", "lehn'", "ich", "auf", "dich", "mich", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "VVFIN", "PPER", "APPR", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sing mir begeistert, als vom Dreyfuss,", "tokens": ["Sing", "mir", "be\u00b7geis\u00b7tert", ",", "als", "vom", "Drey\u00b7fuss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Brittischen Ernst, dass ich froh wie du sey!", "tokens": ["Brit\u00b7ti\u00b7schen", "Ernst", ",", "dass", "ich", "froh", "wie", "du", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "ADJD", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+--+-+++-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Doch jetzt auf Einmahl wird mir das Auge hell!", "tokens": ["Doch", "jetzt", "auf", "Ein\u00b7mahl", "wird", "mir", "das", "Au\u00b7ge", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gesichten hell, und hell der Begeisterung!", "tokens": ["Ge\u00b7sich\u00b7ten", "hell", ",", "und", "hell", "der", "Be\u00b7geis\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich seh' in Wingolfs fernen Hallen", "tokens": ["Ich", "seh'", "in", "Win\u00b7golfs", "fer\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in den schweigenden D\u00e4mmerungen,", "tokens": ["Tief", "in", "den", "schwei\u00b7gen\u00b7den", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dort seh' ich langsam heilige Schatten gehn!", "tokens": ["Dort", "seh'", "ich", "lang\u00b7sam", "hei\u00b7li\u00b7ge", "Schat\u00b7ten", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht jene, die sich traurig von Sterbenden", "tokens": ["Nicht", "je\u00b7ne", ",", "die", "sich", "trau\u00b7rig", "von", "Ster\u00b7ben\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDS", "$,", "PRELS", "PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erheben, nein, die, in der Dichtkunst", "tokens": ["Er\u00b7he\u00b7ben", ",", "nein", ",", "die", ",", "in", "der", "Dicht\u00b7kunst"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PTKANT", "$,", "PRELS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stund' und der Freundschaft, um Dichter schweben!", "tokens": ["Stund'", "und", "der", "Freund\u00b7schaft", ",", "um", "Dich\u00b7ter", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,", "KOUI", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Sie f\u00fchret, hoch den Fl\u00fcgel, Begeistrung her!", "tokens": ["Sie", "f\u00fch\u00b7ret", ",", "hoch", "den", "Fl\u00fc\u00b7gel", ",", "Be\u00b7geis\u00b7trung", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ART", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verdeckt dem Auge, welches der Genius", "tokens": ["Ver\u00b7deckt", "dem", "Au\u00b7ge", ",", "wel\u00b7ches", "der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht sch\u00e4rft, siehst du sie, seelenvolles,", "tokens": ["Nicht", "sch\u00e4rft", ",", "siehst", "du", "sie", ",", "see\u00b7len\u00b7vol\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ahndendes Auge des Dichters, du nur!", "tokens": ["Ahn\u00b7den\u00b7des", "Au\u00b7ge", "des", "Dich\u00b7ters", ",", "du", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Drey Schatten kommen! neben den Schatten t\u00f6nts", "tokens": ["Drey", "Schat\u00b7ten", "kom\u00b7men", "!", "ne\u00b7ben", "den", "Schat\u00b7ten", "t\u00f6nts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Mimers Quelle droben vom Eichenhain", "tokens": ["Wie", "Mi\u00b7mers", "Quel\u00b7le", "dro\u00b7ben", "vom", "Ei\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Ungest\u00fcm herrauscht, und Weisheit", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", "her\u00b7rauscht", ",", "und", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehret die horchenden Wiederhalle!", "tokens": ["Leh\u00b7ret", "die", "hor\u00b7chen\u00b7den", "Wie\u00b7der\u00b7hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wie aus der hohen Dr\u00fcden Versamlungen,", "tokens": ["Wie", "aus", "der", "ho\u00b7hen", "Dr\u00fc\u00b7den", "Ver\u00b7sam\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Braga's Telyn, nieder vom Opferfels,", "tokens": ["Nach", "Bra\u00b7ga's", "Te\u00b7lyn", ",", "nie\u00b7der", "vom", "Op\u00b7fer\u00b7fels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ins lange tiefe Thal der Waldschlacht,", "tokens": ["Ins", "lan\u00b7ge", "tie\u00b7fe", "Thal", "der", "Wald\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Satzungenlos sich der Barden Lied st\u00fcrzt!", "tokens": ["Sat\u00b7zun\u00b7gen\u00b7los", "sich", "der", "Bar\u00b7den", "Lied", "st\u00fcrzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Der du dort wandelst, ernstvoll und heiter doch,", "tokens": ["Der", "du", "dort", "wan\u00b7delst", ",", "ernst\u00b7voll", "und", "hei\u00b7ter", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das Auge voll von weiser Zufriedenheit,", "tokens": ["Das", "Au\u00b7ge", "voll", "von", "wei\u00b7ser", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Lippe voll von Scherz; (Es horchen", "tokens": ["Die", "Lip\u00b7pe", "voll", "von", "Scherz", ";(", "Es", "hor\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "emoticon", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$(", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm die Bemerkungen deiner Freunde,", "tokens": ["Ihm", "die", "Be\u00b7mer\u00b7kun\u00b7gen", "dei\u00b7ner", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ihm horcht entz\u00fcckt die feinere Sch\u00e4ferin,)", "tokens": ["Ihm", "horcht", "ent\u00b7z\u00fcckt", "die", "fei\u00b7ne\u00b7re", "Sch\u00e4\u00b7fe\u00b7rin", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer bist du, Schatten? Ebert! er neiget sich", "tokens": ["Wer", "bist", "du", ",", "Schat\u00b7ten", "?", "E\u00b7bert", "!", "er", "nei\u00b7get", "sich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "NE", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu mir, und l\u00e4chelt. Ja er ist es!", "tokens": ["Zu", "mir", ",", "und", "l\u00e4\u00b7chelt", ".", "Ja", "er", "ist", "es", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "$.", "PTKANT", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Siehe der Schatten ist unser G\u00e4rtner!", "tokens": ["Sie\u00b7he", "der", "Schat\u00b7ten", "ist", "un\u00b7ser", "G\u00e4rt\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Uns werth, wie Flakkus war sein Quintilius,", "tokens": ["Uns", "werth", ",", "wie", "Flak\u00b7kus", "war", "sein", "Quin\u00b7ti\u00b7lius", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PWAV", "NE", "VAFIN", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der unverh\u00fcllten Wahrheit Vertraulichster,", "tokens": ["Der", "un\u00b7ver\u00b7h\u00fcll\u00b7ten", "Wahr\u00b7heit", "Ver\u00b7trau\u00b7lichs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach kehre, G\u00e4rtner, deinen Freunden", "tokens": ["Ach", "keh\u00b7re", ",", "G\u00e4rt\u00b7ner", ",", "dei\u00b7nen", "Freun\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig zur\u00fcck! Doch du fliehest fern weg!", "tokens": ["E\u00b7wig", "zu\u00b7r\u00fcck", "!", "Doch", "du", "flie\u00b7hest", "fern", "weg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Fleuch nicht, mein G\u00e4rtner, fleuch nicht! du flohst ja nicht,", "tokens": ["Fleuch", "nicht", ",", "mein", "G\u00e4rt\u00b7ner", ",", "fleuch", "nicht", "!", "du", "flohst", "ja", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als wir an jenen traurigen Abenden,", "tokens": ["Als", "wir", "an", "je\u00b7nen", "trau\u00b7ri\u00b7gen", "A\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um dich voll Wehmuth still versammelt,", "tokens": ["Um", "dich", "voll", "Weh\u00b7muth", "still", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ADJD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da dich umarmten, und Abschied nahmen!", "tokens": ["Da", "dich", "um\u00b7arm\u00b7ten", ",", "und", "Ab\u00b7schied", "nah\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Die letzten Stunden, welche du Abschied nahmst,", "tokens": ["Die", "letz\u00b7ten", "Stun\u00b7den", ",", "wel\u00b7che", "du", "Ab\u00b7schied", "nahmst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Abend soll mir festlich auf immer seyn!", "tokens": ["Der", "A\u00b7bend", "soll", "mir", "fest\u00b7lich", "auf", "im\u00b7mer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "APPR", "ADV", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Da lernt' ich, voll von ihrem Schmerze,", "tokens": ["Da", "lernt'", "ich", ",", "voll", "von", "ih\u00b7rem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich die wenigen Edlen liebten!", "tokens": ["Wie", "sich", "die", "we\u00b7ni\u00b7gen", "Ed\u00b7len", "lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Viel Mittern\u00e4chte werden noch einst entfliehn.", "tokens": ["Viel", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "wer\u00b7den", "noch", "einst", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lebt sie nicht einsam, Enkel, und heiligt sie", "tokens": ["Lebt", "sie", "nicht", "ein\u00b7sam", ",", "En\u00b7kel", ",", "und", "hei\u00b7ligt", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Freundschaft, wie sie eure V\u00e4ter", "tokens": ["Der", "Freund\u00b7schaft", ",", "wie", "sie", "eu\u00b7re", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiligten, und euch Exempel wurden!", "tokens": ["Hei\u00b7lig\u00b7ten", ",", "und", "euch", "Ex\u00b7em\u00b7pel", "wur\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "NN", "VAFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}