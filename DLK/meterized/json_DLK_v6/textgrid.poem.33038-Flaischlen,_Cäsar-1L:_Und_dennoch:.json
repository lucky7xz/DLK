{"textgrid.poem.33038": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und dennoch:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und dennoch:", "tokens": ["Und", "den\u00b7noch", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nein, ich beneid euch nicht! ...", "tokens": ["Nein", ",", "ich", "be\u00b7neid", "euch", "nicht", "!", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hell ist mein Herz und hell mein Blick", "tokens": ["Hell", "ist", "mein", "Herz", "und", "hell", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und hell in goldener Sonne liegt", "tokens": ["und", "hell", "in", "gol\u00b7de\u00b7ner", "Son\u00b7ne", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die Welt, so sommerklar und sch\u00f6n,", "tokens": ["die", "Welt", ",", "so", "som\u00b7mer\u00b7klar", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "leuchtende Wolken \u00fcber den H\u00f6hn ...", "tokens": ["leuch\u00b7ten\u00b7de", "Wol\u00b7ken", "\u00fc\u00b7ber", "den", "H\u00f6hn", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "und immer tiefer sinkt das Tal", "tokens": ["und", "im\u00b7mer", "tie\u00b7fer", "sinkt", "das", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und sein Gew\u00fchl", "tokens": ["und", "sein", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "und alle Angst und alles Enge,", "tokens": ["und", "al\u00b7le", "Angst", "und", "al\u00b7les", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "alle Schwere, alles Gedr\u00e4nge ...", "tokens": ["al\u00b7le", "Schwe\u00b7re", ",", "al\u00b7les", "Ge\u00b7dr\u00e4n\u00b7ge", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "und immer h\u00f6her, immer breiter,", "tokens": ["und", "im\u00b7mer", "h\u00f6\u00b7her", ",", "im\u00b7mer", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "immer lichter, immer weiter", "tokens": ["im\u00b7mer", "lich\u00b7ter", ",", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "wird der Himmel,", "tokens": ["wird", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "wird mein Ziel!", "tokens": ["wird", "mein", "Ziel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}