{"textgrid.poem.66921": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gen Himmel gro\u00df durch Winterland", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Himmel gro\u00df durch Winterland", "tokens": ["Gen", "Him\u00b7mel", "gro\u00df", "durch", "Win\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00dfe Sternennacht,", "tokens": ["Und", "wei\u00b7\u00dfe", "Ster\u00b7nen\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein S\u00e4mann schreitet bis zum Rand", "tokens": ["Ein", "S\u00e4\u00b7mann", "schrei\u00b7tet", "bis", "zum", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der fernsten Wehr und Wacht.", "tokens": ["Der", "ferns\u00b7ten", "Wehr", "und", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schreitet mit gewaltigem Schritt", "tokens": ["Er", "schrei\u00b7tet", "mit", "ge\u00b7wal\u00b7ti\u00b7gem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den Riesenacker ab", "tokens": ["Den", "Rie\u00b7sen\u00b7a\u00b7cker", "ab"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mi\u00dft den blutigen Ernteschnitt", "tokens": ["Und", "mi\u00dft", "den", "blu\u00b7ti\u00b7gen", "Ern\u00b7te\u00b7schnitt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Helm und Kreuz und Grab.", "tokens": ["An", "Helm", "und", "Kreuz", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vor manchem H\u00fcgel schneeumh\u00fcllt,", "tokens": ["Vor", "man\u00b7chem", "H\u00fc\u00b7gel", "schnee\u00b7um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stockt des S\u00e4manns Fu\u00df,", "tokens": ["Da", "stockt", "des", "S\u00e4\u00b7manns", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er neigt das Haupt, von Schmerz erf\u00fcllt,", "tokens": ["Er", "neigt", "das", "Haupt", ",", "von", "Schmerz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beut der Ehrfurcht Gru\u00df.", "tokens": ["Und", "beut", "der", "Ehr\u00b7furcht", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann richtet sich sein R\u00fccken fest,", "tokens": ["Dann", "rich\u00b7tet", "sich", "sein", "R\u00fc\u00b7cken", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Gottes Ruf und Rat", "tokens": ["Nach", "Got\u00b7tes", "Ruf", "und", "Rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwingt er den Arm, und fallen l\u00e4\u00dft", "tokens": ["Schwingt", "er", "den", "Arm", ",", "und", "fal\u00b7len", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVINF", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Er segnend seine Saat.", "tokens": ["Er", "seg\u00b7nend", "sei\u00b7ne", "Saat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn ein Saatkorn f\u00e4llt, so sprie\u00dft", "tokens": ["Und", "wenn", "ein", "Saat\u00b7korn", "f\u00e4llt", ",", "so", "sprie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In freier Heimat Grund,", "tokens": ["In", "frei\u00b7er", "Hei\u00b7mat", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kampf und Not zusammenschlie\u00dft,", "tokens": ["Die", "Kampf", "und", "Not", "zu\u00b7sam\u00b7men\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neuer Menschenbund.", "tokens": ["Ein", "neu\u00b7er", "Men\u00b7schen\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wo zu Kindesweisen hell", "tokens": ["Und", "wo", "zu", "Kin\u00b7des\u00b7wei\u00b7sen", "hell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwacht der Lichter Schein,", "tokens": ["Er\u00b7wacht", "der", "Lich\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tief springt im Herzen auf ein Quell,", "tokens": ["Tief", "springt", "im", "Her\u00b7zen", "auf", "ein", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der l\u00f6scht der M\u00fctter Pein.", "tokens": ["Der", "l\u00f6scht", "der", "M\u00fct\u00b7ter", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wo sich heimlich um Verlust", "tokens": ["Und", "wo", "sich", "heim\u00b7lich", "um", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele sorgt und m\u00fcht,", "tokens": ["Die", "See\u00b7le", "sorgt", "und", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da geht ein Stern auf in der Brust,", "tokens": ["Da", "geht", "ein", "Stern", "auf", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stern der Zukunft gl\u00fcht.", "tokens": ["Der", "Stern", "der", "Zu\u00b7kunft", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das wirkt des gro\u00dfen S\u00e4manns Hand", "tokens": ["Das", "wirkt", "des", "gro\u00b7\u00dfen", "S\u00e4\u00b7manns", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wei\u00dfer Weihenacht,", "tokens": ["In", "wei\u00b7\u00dfer", "Wei\u00b7he\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wirft die heilige Saat ins Land", "tokens": ["Er", "wirft", "die", "hei\u00b7li\u00b7ge", "Saat", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der deutschen Liebesmacht.", "tokens": ["Der", "deut\u00b7schen", "Lie\u00b7bes\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}