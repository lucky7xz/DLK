{"dta.poem.14654": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Kannst Du rechte Liebe fassen,", "tokens": ["Kannst", "Du", "rech\u00b7te", "Lie\u00b7be", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so frage, zaudre nicht!", "tokens": ["Nun", "so", "fra\u00b7ge", ",", "zaud\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlo\u00df und Garten mu\u00dft Du lassen", "tokens": ["Schlo\u00df", "und", "Gar\u00b7ten", "mu\u00dft", "Du", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Aeltern Angesicht \u2014", "tokens": ["Und", "der", "A\u00b7el\u00b7tern", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf der Fluth mit mir alleine,", "tokens": ["Auf", "der", "Fluth", "mit", "mir", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da erst, Liebchen, bist Du meine!\u201c", "tokens": ["Da", "erst", ",", "Lieb\u00b7chen", ",", "bist", "Du", "mei\u00b7ne", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}