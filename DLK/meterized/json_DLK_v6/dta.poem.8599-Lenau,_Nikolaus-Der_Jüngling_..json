{"dta.poem.8599": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00fcngling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der J\u00fcngling sto\u00dft vom Strand im leichten Kahne,", "tokens": ["Der", "J\u00fcng\u00b7ling", "sto\u00dft", "vom", "Strand", "im", "leich\u00b7ten", "Kah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sehnsucht hat die Segel ihm gebreitet;", "tokens": ["Die", "Sehn\u00b7sucht", "hat", "die", "Se\u00b7gel", "ihm", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie rasch im Fantasieenoceane,", "tokens": ["Wie", "rasch", "im", "Fan\u00b7ta\u00b7si\u00b7e\u00b7e\u00b7no\u00b7ce\u00b7a\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Westen fortgekost, dahin er gleitet!", "tokens": ["Von", "Wes\u00b7ten", "fort\u00b7ge\u00b7kost", ",", "da\u00b7hin", "er", "glei\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schon weht auf neuen Welten seine Fahne,", "tokens": ["Schon", "weht", "auf", "neu\u00b7en", "Wel\u00b7ten", "sei\u00b7ne", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo selig er durch Paradiese schreitet,", "tokens": ["Wo", "se\u00b7lig", "er", "durch", "Pa\u00b7ra\u00b7die\u00b7se", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Blumen pfl\u00fcckt, wie nimmer sie geboren", "tokens": ["Und", "Blu\u00b7men", "pfl\u00fcckt", ",", "wie", "nim\u00b7mer", "sie", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "PWAV", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im reichsten Lenz die heimatlichen Horen.", "tokens": ["Im", "reichs\u00b7ten", "Lenz", "die", "hei\u00b7mat\u00b7li\u00b7chen", "Ho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201ewillkommen J\u00fcngling, von der fernen Reise!\u201c", "tokens": ["\u201e", "will\u00b7kom\u00b7men", "J\u00fcng\u00b7ling", ",", "von", "der", "fer\u00b7nen", "Rei\u00b7se", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begr\u00fc\u00dft ihn t\u00fcckisch wieder nun das Leben,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ihn", "t\u00fc\u00b7ckisch", "wie\u00b7der", "nun", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kosend naht ein Weib, unmerklich leise", "tokens": ["Und", "ko\u00b7send", "naht", "ein", "Weib", ",", "un\u00b7mer\u00b7klich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe Gaukelmacht um ihn zu weben.", "tokens": ["Der", "Lie\u00b7be", "Gau\u00b7kel\u00b7macht", "um", "ihn", "zu", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie h\u00e4lt ihn festgebannt in ihrem Kreise", "tokens": ["Sie", "h\u00e4lt", "ihn", "fest\u00b7ge\u00b7bannt", "in", "ih\u00b7rem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Seufzerformeln, heuchelndem Ergeben.", "tokens": ["Mit", "Seuf\u00b7zer\u00b7for\u00b7meln", ",", "heu\u00b7cheln\u00b7dem", "Er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Froh schm\u00fcckt er ihr mit seinen Traumes-Bl\u00fcthen", "tokens": ["Froh", "schm\u00fcckt", "er", "ihr", "mit", "sei\u00b7nen", "Trau\u00b7mes\u00b7B\u00b7l\u00fcthen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Brust, um welche Todes-L\u00fcfte br\u00fcten.", "tokens": ["Die", "Brust", ",", "um", "wel\u00b7che", "To\u00b7des\u00b7L\u00fcf\u00b7te", "br\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PWAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}