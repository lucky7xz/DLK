{"dta.poem.333": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie ist zu strenge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Celindo stirb! dein Leben will es haben", "tokens": ["Ce\u00b7lin\u00b7do", "stirb", "!", "dein", "Le\u00b7ben", "will", "es", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jhr Zorn hat dich Elender schon begraben.", "tokens": ["Ihr", "Zorn", "hat", "dich", "E\u00b7len\u00b7der", "schon", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach Leben fleuch/ ich mag nicht lebhafft seyn/", "tokens": ["Ach", "Le\u00b7ben", "fleuch", "/", "ich", "mag", "nicht", "leb\u00b7hafft", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Leben scharrt mich in das Grab hinein.", "tokens": ["Mein", "Le\u00b7ben", "scharrt", "mich", "in", "das", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jhr Geister flieht/ verla\u00dft die Jammer-H\u00fctten/", "tokens": ["Ihr", "Geis\u00b7ter", "flieht", "/", "ver\u00b7la\u00dft", "die", "Jam\u00b7mer\u00b7H\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jhr hofft umsonst/ sie l\u00e4st sich nicht erbitten.", "tokens": ["Ihr", "hofft", "um\u00b7sonst", "/", "sie", "l\u00e4st", "sich", "nicht", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf s\u00fcsser Tod/ ich warte mit Verlangen/", "tokens": ["Auf", "s\u00fcs\u00b7ser", "Tod", "/", "ich", "war\u00b7te", "mit", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will dich gern zu meiner Ruh umfangen", "tokens": ["Ich", "will", "dich", "gern", "zu", "mei\u00b7ner", "Ruh", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich liebe dich und k\u00fcsse deinen Pfeil", "tokens": ["Ich", "lie\u00b7be", "dich", "und", "k\u00fcs\u00b7se", "dei\u00b7nen", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du bist mein Trost und allerletztes Heyl.", "tokens": ["Du", "bist", "mein", "Trost", "und", "al\u00b7ler\u00b7letz\u00b7tes", "Heyl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach sterbet doch ihr halb erstorbnen Sinnen!", "tokens": ["Ach", "ster\u00b7bet", "doch", "ihr", "halb", "ers\u00b7torb\u00b7nen", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jhr sucht umsonst die Harte zugewinnen.", "tokens": ["Ihr", "sucht", "um\u00b7sonst", "die", "Har\u00b7te", "zu\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Rei\u00df mit Gewalt des Lebens-Pfeiler nieder/", "tokens": ["Rei\u00df", "mit", "Ge\u00b7walt", "des", "Le\u00b7bens\u00b7Pfei\u00b7ler", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ach t\u00f6dte doch die abgematten Glieder!", "tokens": ["Ach", "t\u00f6d\u00b7te", "doch", "die", "ab\u00b7ge\u00b7mat\u00b7ten", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Grausahm-seyn betr\u00fcbt mich allzusehr/", "tokens": ["Ihr", "Grau\u00b7sahm\u00b7seyn", "be\u00b7tr\u00fcbt", "mich", "all\u00b7zu\u00b7sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jm Sterben f\u00fchlt man keine Marter mehr.", "tokens": ["Jm", "Ster\u00b7ben", "f\u00fchlt", "man", "kei\u00b7ne", "Mar\u00b7ter", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tyrannin sag/ was n\u00fctzet dir mein Qu\u00e4hlen?", "tokens": ["Ty\u00b7ran\u00b7nin", "sag", "/", "was", "n\u00fct\u00b7zet", "dir", "mein", "Qu\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hab\u2019 ich die Schuld so magst du mich entseelen?", "tokens": ["Hab'", "ich", "die", "Schuld", "so", "magst", "du", "mich", "ent\u00b7see\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Leb wohl! leb wohl/ du strenge ", "tokens": ["Leb", "wohl", "!", "leb", "wohl", "/", "du", "stren\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "ADV", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist so hart und grausahm als du sch\u00f6ne.", "tokens": ["Du", "bist", "so", "hart", "und", "grau\u00b7sahm", "als", "du", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Celindo stirbt/ dein Hart-seyn hats gemacht.", "tokens": ["Ce\u00b7lin\u00b7do", "stirbt", "/", "dein", "Har\u00b7tseyn", "hats", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Adjeu zu letzt/ leb wohl zu guter Nacht.", "tokens": ["Ad\u00b7jeu", "zu", "letzt", "/", "leb", "wohl", "zu", "gu\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "$(", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Reu und die Zeit wird noch die Sinnen zwingen/", "tokens": ["Reu", "und", "die", "Zeit", "wird", "noch", "die", "Sin\u00b7nen", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lieb wen du wilt/ mein Wunsch wird jetzt gelingen.", "tokens": ["Lieb", "wen", "du", "wilt", "/", "mein", "Wunsch", "wird", "jetzt", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VMFIN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}