{"dta.poem.3336": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Denn diese grauenvollen H\u00f6hlen", "tokens": ["Denn", "die\u00b7se", "grau\u00b7en\u00b7vol\u00b7len", "H\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind abgeschiednen strafbarn Seelen", "tokens": ["Sind", "ab\u00b7ge\u00b7schied\u00b7nen", "straf\u00b7barn", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ihrem Aufenthalt ernannt.", "tokens": ["Zu", "ih\u00b7rem", "Auf\u00b7ent\u00b7halt", "er\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier schw\u00e4rmen unter bangen Klagen", "tokens": ["Hier", "schw\u00e4r\u00b7men", "un\u00b7ter", "ban\u00b7gen", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Werkzeug\u2019 allgemeiner Plagen,", "tokens": ["Die", "Werk\u00b7zeug'", "all\u00b7ge\u00b7mei\u00b7ner", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die euch die H\u00f6lle zugesandt:", "tokens": ["Die", "euch", "die", "H\u00f6l\u00b7le", "zu\u00b7ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verr\u00e4ther, Wuchrer, Ungerechte,", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", ",", "Wuch\u00b7rer", ",", "Un\u00b7ge\u00b7rech\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die keinen Gott, kein Vaterland,", "tokens": ["Die", "kei\u00b7nen", "Gott", ",", "kein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als ihren Eigennutz, gekannt:", "tokens": ["Als", "ih\u00b7ren", "Ei\u00b7gen\u00b7nutz", ",", "ge\u00b7kannt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der schwarzen Habsucht schlaue Knechte,", "tokens": ["Der", "schwar\u00b7zen", "Hab\u00b7sucht", "schlau\u00b7e", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die auch ein Meineyd nicht erschreckt,", "tokens": ["Die", "auch", "ein", "Mei\u00b7neyd", "nicht", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sobald sich ein Gewinn entdeckt:", "tokens": ["So\u00b7bald", "sich", "ein", "Ge\u00b7winn", "ent\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Heuchler, derer fromme Zungen", "tokens": ["Die", "Heuch\u00b7ler", ",", "de\u00b7rer", "from\u00b7me", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Bald andachtvolle Lieder sungen,", "tokens": ["Bald", "an\u00b7dacht\u00b7vol\u00b7le", "Lie\u00b7der", "sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und bald, o heiliges Bem\u00fchn!", "tokens": ["Und", "bald", ",", "o", "hei\u00b7li\u00b7ges", "Be\u00b7m\u00fchn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den Gift verg\u00e4llter L\u00e4sterungen", "tokens": ["Den", "Gift", "ver\u00b7g\u00e4ll\u00b7ter", "L\u00e4s\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auf ihren bessern N\u00e4chsten spien:", "tokens": ["Auf", "ih\u00b7ren", "bes\u00b7sern", "N\u00e4chs\u00b7ten", "spien", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Harte, der sich nie erbarmet,", "tokens": ["Der", "Har\u00b7te", ",", "der", "sich", "nie", "er\u00b7bar\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Nie auf den Armen h\u00fclfreich blickt:", "tokens": ["Nie", "auf", "den", "Ar\u00b7men", "h\u00fclf\u00b7reich", "blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Falsche, der den Freund umarmet,", "tokens": ["Der", "Fal\u00b7sche", ",", "der", "den", "Freund", "um\u00b7ar\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und ihm den Dolch ins Herze dr\u00fcckt:", "tokens": ["Und", "ihm", "den", "Dolch", "ins", "Her\u00b7ze", "dr\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der giftigen Verl\u00e4umdung Freunde,", "tokens": ["Der", "gif\u00b7ti\u00b7gen", "Ver\u00b7l\u00e4um\u00b7dung", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die gl\u00e4nzender Verdienste Feinde,", "tokens": ["Die", "gl\u00e4n\u00b7zen\u00b7der", "Ver\u00b7diens\u00b7te", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Verfolger aller Tugend sind;", "tokens": ["Ver\u00b7fol\u00b7ger", "al\u00b7ler", "Tu\u00b7gend", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und jene plaudernde Sibyllen,", "tokens": ["Und", "je\u00b7ne", "plau\u00b7dern\u00b7de", "Si\u00b7byl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die iedes Haus mit Zwist erf\u00fcllen,", "tokens": ["Die", "ie\u00b7des", "Haus", "mit", "Zwist", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wo ihr Geschw\u00e4tz ein Ohr gewinnt;", "tokens": ["Wo", "ihr", "Ge\u00b7schw\u00e4tz", "ein", "Ohr", "ge\u00b7winnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Verlebte m\u00fcssige Matronen,", "tokens": ["Ver\u00b7leb\u00b7te", "m\u00fcs\u00b7si\u00b7ge", "Mat\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Geisseln, ja die Pest der Strassen, wo sie wohnen.", "tokens": ["Die", "Geis\u00b7seln", ",", "ja", "die", "Pest", "der", "Stras\u00b7sen", ",", "wo", "sie", "woh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}