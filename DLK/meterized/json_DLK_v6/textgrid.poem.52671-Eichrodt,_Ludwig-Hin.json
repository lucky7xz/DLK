{"textgrid.poem.52671": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Hin", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es braust der Zug, es donnern hin die Wagen", "tokens": ["Es", "braust", "der", "Zug", ",", "es", "don\u00b7nern", "hin", "die", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch \u00f6den Wald,", "tokens": ["Durch", "\u00f6\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Dampfes volle wei\u00dfe Wolken jagen \u2013", "tokens": ["Des", "Damp\u00b7fes", "vol\u00b7le", "wei\u00b7\u00dfe", "Wol\u00b7ken", "ja\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich komme bald!", "tokens": ["Ich", "kom\u00b7me", "bald", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie seid ihr gr\u00fcnen sommerlichen R\u00e4ume", "tokens": ["Wie", "seid", "ihr", "gr\u00fc\u00b7nen", "som\u00b7mer\u00b7li\u00b7chen", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun todt, entstellt!", "tokens": ["Nun", "todt", ",", "ent\u00b7stellt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In kalte Nebel tauchen Berg und B\u00e4ume,", "tokens": ["In", "kal\u00b7te", "Ne\u00b7bel", "tau\u00b7chen", "Berg", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Grau liegt die Welt.", "tokens": ["Grau", "liegt", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du harrest mein, du z\u00e4hlest die Sekunde,", "tokens": ["Du", "har\u00b7rest", "mein", ",", "du", "z\u00e4h\u00b7lest", "die", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich erschein \u2013", "tokens": ["Da", "ich", "er\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Entgegen zittre ich dem hei\u00dfen Munde,", "tokens": ["Ent\u00b7ge\u00b7gen", "zitt\u00b7re", "ich", "dem", "hei\u00b7\u00dfen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O s\u00fc\u00dfe Pein!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Pein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kein lieblich L\u00fcftchen soll erquicken d\u00fcrfen", "tokens": ["Kein", "lieb\u00b7lich", "L\u00fcft\u00b7chen", "soll", "er\u00b7qui\u00b7cken", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "VMFIN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich wonniglich \u2013", "tokens": ["Mich", "won\u00b7nig\u00b7lich", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur deinen s\u00fc\u00dfen Athem will ich schl\u00fcrfen;", "tokens": ["Nur", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "A\u00b7them", "will", "ich", "schl\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er bl\u00fcht f\u00fcr mich.", "tokens": ["Er", "bl\u00fcht", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Mich gr\u00fc\u00dfen keine Blumen und die Sonne", "tokens": ["Mich", "gr\u00fc\u00b7\u00dfen", "kei\u00b7ne", "Blu\u00b7men", "und", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie gr\u00fc\u00dft mich nicht,", "tokens": ["Sie", "gr\u00fc\u00dft", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es soll mir sein des Gru\u00dfes erste Wonne", "tokens": ["Es", "soll", "mir", "sein", "des", "Gru\u00b7\u00dfes", "ers\u00b7te", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Augenlicht.", "tokens": ["Dein", "Au\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "An deinem Herzen werd ich heut erwarmen", "tokens": ["An", "dei\u00b7nem", "Her\u00b7zen", "werd", "ich", "heut", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach G\u00f6tterschlu\u00df \u2013", "tokens": ["Nach", "G\u00f6t\u00b7ter\u00b7schlu\u00df", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Liebesfeuer str\u00f6mt aus deinen Armen,", "tokens": ["Ein", "Lie\u00b7bes\u00b7feu\u00b7er", "str\u00f6mt", "aus", "dei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus deinem Ku\u00df.", "tokens": ["Aus", "dei\u00b7nem", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Rollt hin, rollt hin! es bringt mich nicht zur Stelle", "tokens": ["Rollt", "hin", ",", "rollt", "hin", "!", "es", "bringt", "mich", "nicht", "zur", "Stel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tr\u00e4ge Fahrt.", "tokens": ["Die", "tr\u00e4\u00b7ge", "Fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr Wagen braust! ihr seid mir nicht zu schnelle;", "tokens": ["Ihr", "Wa\u00b7gen", "braust", "!", "ihr", "seid", "mir", "nicht", "zu", "schnel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebste harrt.", "tokens": ["Die", "Liebs\u00b7te", "harrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}