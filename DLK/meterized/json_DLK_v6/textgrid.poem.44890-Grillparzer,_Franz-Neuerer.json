{"textgrid.poem.44890": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Neuerer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schwatzt ihr mir von eurer neuen Zeit?", "tokens": ["Was", "schwatzt", "ihr", "mir", "von", "eu\u00b7rer", "neu\u00b7en", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeiten hatten sich, es ist nicht lang, erneut.", "tokens": ["Die", "Zei\u00b7ten", "hat\u00b7ten", "sich", ",", "es", "ist", "nicht", "lang", ",", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was aber jetzt f\u00fcr neue Zeit wir halten,", "tokens": ["Was", "a\u00b7ber", "jetzt", "f\u00fcr", "neu\u00b7e", "Zeit", "wir", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist weiter nichts, als leise Wiederkehr zur alten.", "tokens": ["Ist", "wei\u00b7ter", "nichts", ",", "als", "lei\u00b7se", "Wie\u00b7der\u00b7kehr", "zur", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "KOUS", "ADJA", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}