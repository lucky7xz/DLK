, "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der den Kaiserschmarrn?", "tokens": ["Der", "den", "Kai\u00b7ser\u00b7schmarrn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Warum geht's uns meistens dreckig?", "tokens": ["Wa\u00b7rum", "geht's", "uns", "meis\u00b7tens", "dre\u00b7ckig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Weshalb schreib ich dies Gedicht?", "tokens": ["We\u00b7shalb", "schreib", "ich", "dies", "Ge\u00b7dicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum ist das Zebra fleckig", "tokens": ["Wa\u00b7rum", "ist", "das", "Zeb\u00b7ra", "flec\u00b7kig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und Mariechen nicht?", "tokens": ["Und", "Ma\u00b7rie\u00b7chen", "nicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dennoch ahnt man irgendwie", "tokens": ["Den\u00b7noch", "ahnt", "man", "ir\u00b7gend\u00b7wie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Qualverwandschaft,", "tokens": ["Got\u00b7tes", "Qual\u00b7ver\u00b7wand\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Trifft man unerwartet sie", "tokens": ["Trifft", "man", "un\u00b7er\u00b7war\u00b7tet", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drau\u00dfen in der Landschaft.", "tokens": ["Drau\u00b7\u00dfen", "in", "der", "Land\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}