{"textgrid.poem.50230": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "8. Auslegung des Bildes der Gerechtigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist die ", "tokens": ["Ist", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die gleiche ", "tokens": ["Die", "glei\u00b7che"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ob die Dukaten ", "tokens": ["Ob", "die", "Du\u00b7ka\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die man ihr zusteckt; und sie h\u00e4lt", "tokens": ["Die", "man", "ihr", "zu\u00b7steckt", ";", "und", "sie", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "PPER", "VVPP", "$.", "KON", "PPER", "VVFIN"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Dass ihr die Diebe nicht das Geld,", "tokens": ["Dass", "ihr", "die", "Die\u00b7be", "nicht", "das", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ihr ", "tokens": ["Das", "ihr"], "token_info": ["word", "word"], "pos": ["PDS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}}}}}