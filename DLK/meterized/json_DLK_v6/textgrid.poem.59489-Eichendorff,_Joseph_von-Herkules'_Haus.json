{"textgrid.poem.59489": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herkules' Haus", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Rodrich in Toledo,", "tokens": ["K\u00f6\u00b7nig", "Rod\u00b7rich", "in", "To\u00b7le\u00b7do", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Seiner Krone Glanz zu mehren,", "tokens": ["Sei\u00b7ner", "Kro\u00b7ne", "Glanz", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df ein gro\u00df Turnier verk\u00fcnden.", "tokens": ["Lie\u00df", "ein", "gro\u00df", "Tur\u00b7nier", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Hell schon die Trompeten schmettern,", "tokens": ["Hell", "schon", "die", "Trom\u00b7pe\u00b7ten", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sechzigtausend Ritter kamen,", "tokens": ["Sech\u00b7zig\u00b7tau\u00b7send", "Rit\u00b7ter", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zu k\u00e4mpfen dort begehrten.", "tokens": ["Die", "zu", "k\u00e4mp\u00b7fen", "dort", "be\u00b7gehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch, bevor der Kampf begonnen,", "tokens": ["Doch", ",", "be\u00b7vor", "der", "Kampf", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu ihm die Toleder treten", "tokens": ["Zu", "ihm", "die", "To\u00b7le\u00b7der", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Bittend, da\u00df er Tor und Riegel", "tokens": ["Bit\u00b7tend", ",", "da\u00df", "er", "Tor", "und", "Rie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Woll mit neuem Schlo\u00df versehen", "tokens": ["Woll", "mit", "neu\u00b7em", "Schlo\u00df", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "An des Herkules Palaste,", "tokens": ["An", "des", "Her\u00b7ku\u00b7les", "Pa\u00b7las\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Wie's bisher der Brauch gewesen.", "tokens": ["Wie's", "bis\u00b7her", "der", "Brauch", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aber in dem alten Hause", "tokens": ["A\u00b7ber", "in", "dem", "al\u00b7ten", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dacht er, reichen Schatz zu heben,", "tokens": ["Dacht", "er", ",", "rei\u00b7chen", "Schatz", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Lie\u00df die Riegel all zerbrechen", "tokens": ["Lie\u00df", "die", "Rie\u00b7gel", "all", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und des Tempels Tore sprengen.", "tokens": ["Und", "des", "Tem\u00b7pels", "To\u00b7re", "spren\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als er eintrat, war's so still drin,", "tokens": ["Als", "er", "ein\u00b7trat", ",", "wa\u00b7r's", "so", "still", "drin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Nur ein Spruch gl\u00e4nzt' ihm entgegen:", "tokens": ["Nur", "ein", "Spruch", "gl\u00e4nzt'", "ihm", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbweh dir, Rodrich, denn der K\u00f6nig,", "tokens": ["\u00bb", "weh", "dir", ",", "Rod\u00b7rich", ",", "denn", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "NE", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der betreten diese Schwelle,", "tokens": ["Der", "be\u00b7tre\u00b7ten", "die\u00b7se", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der gebrochen diese Stille,", "tokens": ["Der", "ge\u00b7bro\u00b7chen", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird Hispanien versengen!\u00ab", "tokens": ["Wird", "His\u00b7pa\u00b7ni\u00b7en", "ver\u00b7sen\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Seitw\u00e4rts hinter einem Pfeiler", "tokens": ["Seit\u00b7w\u00e4rts", "hin\u00b7ter", "ei\u00b7nem", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War ein pr\u00e4cht'ger Schrank zu sehen,", "tokens": ["War", "ein", "pr\u00e4cht'\u00b7ger", "Schrank", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drinnen lagen fremde Banner", "tokens": ["Drin\u00b7nen", "la\u00b7gen", "frem\u00b7de", "Ban\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit Figuren zum Erschrecken,", "tokens": ["Mit", "Fi\u00b7gu\u00b7ren", "zum", "Er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und Araber, hoch zu Rosse,", "tokens": ["Und", "A\u00b7ra\u00b7ber", ",", "hoch", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Funkelnd mit gez\u00fcckten Schwertern,", "tokens": ["Fun\u00b7kelnd", "mit", "ge\u00b7z\u00fcck\u00b7ten", "Schwer\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hielten an dem Schrein die Wache,", "tokens": ["Hiel\u00b7ten", "an", "dem", "Schrein", "die", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Lautlos, ohne sich zu regen. \u2013", "tokens": ["Laut\u00b7los", ",", "oh\u00b7ne", "sich", "zu", "re\u00b7gen", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "KOUI", "PRF", "APPR", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Rodrich wandt sich vor Entsetzen,", "tokens": ["Rod\u00b7rich", "wandt", "sich", "vor", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wollt fortan nichts weiter sehen,", "tokens": ["Wollt", "for\u00b7tan", "nichts", "wei\u00b7ter", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und ein Blitzstrahl zuckt' vom Himmel", "tokens": ["Und", "ein", "Blitz\u00b7strahl", "zuckt'", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und verbrannt den Zaubertempel.", "tokens": ["Und", "ver\u00b7brannt", "den", "Zau\u00b7ber\u00b7tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00dcbers Meer wohl sandt er Kriegsvolk,", "tokens": ["\u00dc\u00b7bers", "Meer", "wohl", "sandt", "er", "Kriegs\u00b7volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollten Afrika erwerben,", "tokens": ["Soll\u00b7ten", "Af\u00b7ri\u00b7ka", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wetter stiegen, wo sie fuhren,", "tokens": ["Wet\u00b7ter", "stie\u00b7gen", ",", "wo", "sie", "fuh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dften all im Meer verderben.", "tokens": ["Mu\u00df\u00b7ten", "all", "im", "Meer", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}