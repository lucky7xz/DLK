{"dta.poem.15618": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr meine Theueren, wo seid ihr hin gekommen?", "tokens": ["Ihr", "mei\u00b7ne", "Theu\u00b7e\u00b7ren", ",", "wo", "seid", "ihr", "hin", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort in die Ewigkeit verewigt aufgenommen.", "tokens": ["Dort", "in", "die", "E\u00b7wig\u00b7keit", "ve\u00b7re\u00b7wigt", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch in der Zeitlichkeit ist eure Spur verschwunden?", "tokens": ["Doch", "in", "der", "Zeit\u00b7lich\u00b7keit", "ist", "eu\u00b7re", "Spur", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, tief in meinem Seyn, in meinem Sinn gebunden.", "tokens": ["Nein", ",", "tief", "in", "mei\u00b7nem", "Seyn", ",", "in", "mei\u00b7nem", "Sinn", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bedeutend innere Denkmale meines Lebens!", "tokens": ["Be\u00b7deu\u00b7tend", "in\u00b7ne\u00b7re", "Denk\u00b7ma\u00b7le", "mei\u00b7nes", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4rt ihr auch dieses nur, ihr w\u00e4ret nicht vergebens.", "tokens": ["W\u00e4rt", "ihr", "auch", "die\u00b7ses", "nur", ",", "ihr", "w\u00e4\u00b7ret", "nicht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PDAT", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was wirkend nun mein Sinn nach au\u00dfen mag entfalten,", "tokens": ["Was", "wir\u00b7kend", "nun", "mein", "Sinn", "nach", "au\u00b7\u00dfen", "mag", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "PPOSAT", "NN", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So seid ihr mit darinn, wie in mir selbst, enthalten.", "tokens": ["So", "seid", "ihr", "mit", "da\u00b7rinn", ",", "wie", "in", "mir", "selbst", ",", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PAV", "$,", "PWAV", "APPR", "PPER", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}