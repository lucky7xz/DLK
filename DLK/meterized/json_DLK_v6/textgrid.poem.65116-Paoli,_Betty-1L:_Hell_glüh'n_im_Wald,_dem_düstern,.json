{"textgrid.poem.65116": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hell gl\u00fch'n im Wald, dem d\u00fcstern,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell gl\u00fch'n im Wald, dem d\u00fcstern,", "tokens": ["Hell", "gl\u00fch'n", "im", "Wald", ",", "dem", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Abendlichtes Br\u00e4nde,", "tokens": ["Des", "A\u00b7bend\u00b7lich\u00b7tes", "Br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Bl\u00e4tter rauschen, fl\u00fcstern \u2013", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", ",", "fl\u00fcs\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, wer sie doch verst\u00e4nde!", "tokens": ["O", ",", "wer", "sie", "doch", "ver\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Empor aus dichten Zweigen,", "tokens": ["Em\u00b7por", "aus", "dich\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich einer Opferspende,", "tokens": ["Gleich", "ei\u00b7ner", "Op\u00b7fer\u00b7spen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der V\u00f6gel Lieder steigen \u2013", "tokens": ["Der", "V\u00f6\u00b7gel", "Lie\u00b7der", "stei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, wer sie doch verst\u00e4nde!", "tokens": ["O", ",", "wer", "sie", "doch", "ver\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Bach zieht seine Kreise", "tokens": ["Der", "Bach", "zieht", "sei\u00b7ne", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch gr\u00fcne Uferw\u00e4nde,", "tokens": ["Durch", "gr\u00fc\u00b7ne", "U\u00b7fer\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wellen murmeln leise \u2013", "tokens": ["Die", "Wel\u00b7len", "mur\u00b7meln", "lei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, wer sie doch verst\u00e4nde!", "tokens": ["O", ",", "wer", "sie", "doch", "ver\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In all den Wechselreden,", "tokens": ["In", "all", "den", "Wech\u00b7sel\u00b7re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob nicht ein Gru\u00df sich f\u00e4nde", "tokens": ["Ob", "nicht", "ein", "Gru\u00df", "sich", "f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus dem verlornen Eden? \u2013", "tokens": ["Aus", "dem", "ver\u00b7lor\u00b7nen", "E\u00b7den", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, wer sie doch verst\u00e4nde!", "tokens": ["O", ",", "wer", "sie", "doch", "ver\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}