{"textgrid.poem.41183": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traurig ist der Tag!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig ist der Tag!", "tokens": ["Trau\u00b7rig", "ist", "der", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Von der Himmelstochter", "tokens": ["Von", "der", "Him\u00b7mel\u00b7stoch\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blicken ungetr\u00f6stet", "tokens": ["Bli\u00b7cken", "un\u00b7ge\u00b7tr\u00f6s\u00b7tet"], "token_info": ["word", "word"], "pos": ["NN", "APPO"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "D\u00e4mmert er dahin.", "tokens": ["D\u00e4m\u00b7mert", "er", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Graue Nebels\u00e4ulen", "tokens": ["Grau\u00b7e", "Ne\u00b7bel\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Steigen von Gebirgen.", "tokens": ["Stei\u00b7gen", "von", "Ge\u00b7bir\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Endlos ist der Wolken Zug. \u2013", "tokens": ["End\u00b7los", "ist", "der", "Wol\u00b7ken", "Zug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ha, du bist von meinem Herzen,", "tokens": ["Ha", ",", "du", "bist", "von", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tag, ein Bild!", "tokens": ["Tag", ",", "ein", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Traurig ist mein Herz. \u2013", "tokens": ["Trau\u00b7rig", "ist", "mein", "Herz", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hat im deutschen Vaterlande", "tokens": ["Hat", "im", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Je sein Volk ein Barde", "tokens": ["Je", "sein", "Volk", "ein", "Bar\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mehr geliebt, als ich,", "tokens": ["Mehr", "ge\u00b7liebt", ",", "als", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O so rei\u00dfe diese Saiten,", "tokens": ["O", "so", "rei\u00b7\u00dfe", "die\u00b7se", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Schwermuth schlaff gelassen,", "tokens": ["Von", "der", "Schwer\u00b7muth", "schlaff", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter meinen Griffen", "tokens": ["Un\u00b7ter", "mei\u00b7nen", "Grif\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Unsichtbare Kraft entzwei!", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Kraft", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und dennoch \u2013 ach der Zeiten! \u2013", "tokens": ["Und", "den\u00b7noch", "\u2013", "ach", "der", "Zei\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "XY", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Emp\u00f6rt sich mir ein Lied im Herzen,", "tokens": ["Em\u00b7p\u00f6rt", "sich", "mir", "ein", "Lied", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und greif' ich nach dem Harfenspiele,", "tokens": ["Und", "greif'", "ich", "nach", "dem", "Har\u00b7fen\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sey's in der Winterhalle,", "tokens": ["Sey's", "in", "der", "Win\u00b7ter\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sey's in der Eiche Schatten,", "tokens": ["Sey's", "in", "der", "Ei\u00b7che", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und steh'n die Kinder meines Volkes", "tokens": ["Und", "steh'n", "die", "Kin\u00b7der", "mei\u00b7nes", "Vol\u00b7kes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Liede lauschend her um mich;", "tokens": ["Dem", "Lie\u00b7de", "lau\u00b7schend", "her", "um", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hat das Lied nun ausgequollen,", "tokens": ["Und", "hat", "das", "Lied", "nun", "aus\u00b7ge\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dann seh' ich manches Nackensch\u00fctteln.", "tokens": ["Dann", "seh'", "ich", "man\u00b7ches", "Na\u00b7cken\u00b7sch\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dann h\u00f6r' ich manches Hohngefl\u00fcster:", "tokens": ["Dann", "h\u00f6r'", "ich", "man\u00b7ches", "Hohn\u00b7ge\u00b7fl\u00fcs\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbdie Fremden singen besser.", "tokens": ["\u00bb", "die", "Frem\u00b7den", "sin\u00b7gen", "bes\u00b7ser", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wer mag sein Lied versteh'n!\u00ab", "tokens": ["Wer", "mag", "sein", "Lied", "ver\u00b7steh'n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Dann sinkt mein Haupt auf's Harfenspiel.", "tokens": ["Dann", "sinkt", "mein", "Haupt", "auf's", "Har\u00b7fen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Seufzer rei\u00dft sich aus der Brust,", "tokens": ["Ein", "Seuf\u00b7zer", "rei\u00dft", "sich", "aus", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und jede Saite winselt", "tokens": ["Und", "je\u00b7de", "Sai\u00b7te", "win\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ihm leise leise nach.", "tokens": ["Ihm", "lei\u00b7se", "lei\u00b7se", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kinder meines Volkes!", "tokens": ["Kin\u00b7der", "mei\u00b7nes", "Vol\u00b7kes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Soll ein deutscher Barde singen,", "tokens": ["Soll", "ein", "deut\u00b7scher", "Bar\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein Herz w\u00fc\u00dfte nichts davon?", "tokens": ["Und", "sein", "Herz", "w\u00fc\u00df\u00b7te", "nichts", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist die Wolke nicht des Blitzes,", "tokens": ["Ist", "die", "Wol\u00b7ke", "nicht", "des", "Blit\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Herz nicht der Lieder Sitz?", "tokens": ["Und", "das", "Herz", "nicht", "der", "Lie\u00b7der", "Sitz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und wie geu\u00dft ein volles Herz sich aus?", "tokens": ["Und", "wie", "geu\u00dft", "ein", "vol\u00b7les", "Herz", "sich", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Str\u00f6mt es nicht dr\u00e4ngend und gewaltig,", "tokens": ["Str\u00f6mt", "es", "nicht", "dr\u00e4n\u00b7gend", "und", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Unaufhaltsam, seelenfassend", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "see\u00b7len\u00b7fas\u00b7send"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seinen Inhalt fort?", "tokens": ["Sei\u00b7nen", "In\u00b7halt", "fort", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Spricht es nicht Heldensprache, Geistersprache? \u2013", "tokens": ["Spricht", "es", "nicht", "Hel\u00b7den\u00b7spra\u00b7che", ",", "Geis\u00b7ter\u00b7spra\u00b7che", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,", "NN", "$.", "$("], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ha! \u00bbWer mag sein Lied versteh'n!\u00ab", "tokens": ["Ha", "!", "\u00bb", "Wer", "mag", "sein", "Lied", "ver\u00b7steh'n", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "$(", "PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kinder meines Volkes! jener,", "tokens": ["Kin\u00b7der", "mei\u00b7nes", "Vol\u00b7kes", "!", "je\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Allvaters hohe Gabe,", "tokens": ["Der", "All\u00b7va\u00b7ters", "ho\u00b7he", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Der den Funken des Gef\u00fchles,", "tokens": ["Der", "den", "Fun\u00b7ken", "des", "Ge\u00b7f\u00fch\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der den Saamen der Empfindung,", "tokens": ["Der", "den", "Saa\u00b7men", "der", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als er ward in ihn geleget,", "tokens": ["Als", "er", "ward", "in", "ihn", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht ersticket hat.", "tokens": ["Nicht", "er\u00b7sti\u00b7cket", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kinder meines Volkes!", "tokens": ["Kin\u00b7der", "mei\u00b7nes", "Vol\u00b7kes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ein verderblich Wort", "tokens": ["Ein", "ver\u00b7derb\u00b7lich", "Wort"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Haben euch die Fremden angehauchet.", "tokens": ["Ha\u00b7ben", "euch", "die", "Frem\u00b7den", "an\u00b7ge\u00b7hau\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dieses Wort kannten eure V\u00e4ter nicht.", "tokens": ["Die\u00b7ses", "Wort", "kann\u00b7ten", "eu\u00b7re", "V\u00e4\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Barde! sprachen eure V\u00e4ter:", "tokens": ["Bar\u00b7de", "!", "spra\u00b7chen", "eu\u00b7re", "V\u00e4\u00b7ter", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gib uns Liebe, gib uns Klagen,", "tokens": ["Gib", "uns", "Lie\u00b7be", ",", "gib", "uns", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gib uns Lust zu gro\u00dfen Thaten,", "tokens": ["Gib", "uns", "Lust", "zu", "gro\u00b7\u00dfen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gib uns Muth f\u00fcr's Vaterland zu sterben!", "tokens": ["Gib", "uns", "Muth", "f\u00fcr's", "Va\u00b7ter\u00b7land", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Keiner sprach: Gib uns Witz!", "tokens": ["Kei\u00b7ner", "sprach", ":", "Gib", "uns", "Witz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "VVIMP", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Witz ist eine kalte Wasserblase,", "tokens": ["Witz", "ist", "e