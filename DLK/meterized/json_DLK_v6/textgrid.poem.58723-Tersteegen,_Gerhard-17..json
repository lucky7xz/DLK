{"textgrid.poem.58723": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ge\u00e4ngstet und zerschlagen Herz wirst du, Gott, nicht verachten. Psalm 51, 19", "tokens": ["Ein", "ge\u00b7\u00e4ngs\u00b7tet", "und", "zer\u00b7schla\u00b7gen", "Herz", "wirst", "du", ",", "Gott", ",", "nicht", "ver\u00b7ach\u00b7ten", ".", "Psalm", "51", ",", "19"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["ART", "VVPP", "KON", "ADJA", "NN", "VAFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "VVFIN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Du, Liebe, willst mein ganzes Herze haben \u2013", "tokens": ["Du", ",", "Lie\u00b7be", ",", "willst", "mein", "gan\u00b7zes", "Her\u00b7ze", "ha\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier bring' ich's dir statt andrer Gaben,", "tokens": ["Hier", "bring'", "ich's", "dir", "statt", "an\u00b7drer", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, nimm's aus meinen H\u00e4nden an!", "tokens": ["Ach", ",", "nim\u00b7m's", "aus", "mei\u00b7nen", "H\u00e4n\u00b7den", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es ist geschehn, dir sei es \u00fcberlassen", "tokens": ["Es", "ist", "ge\u00b7schehn", ",", "dir", "sei", "es", "\u00fc\u00b7ber\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und eingeh\u00e4ndigt besterma\u00dfen,", "tokens": ["Und", "ein\u00b7ge\u00b7h\u00e4n\u00b7digt", "bes\u00b7ter\u00b7ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein h\u00f6hers Gl\u00fcck ihm werden kann.", "tokens": ["Kein", "h\u00f6\u00b7hers", "Gl\u00fcck", "ihm", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nimmst du mein Opfer an, ach, la\u00df es deinen H\u00e4nden", "tokens": ["Nimmst", "du", "mein", "Op\u00b7fer", "an", ",", "ach", ",", "la\u00df", "es", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nimmer wiederum entwenden!", "tokens": ["Doch", "nim\u00b7mer", "wie\u00b7de\u00b7rum", "ent\u00b7wen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es schl\u00e4get mir die Liebe wohl nicht ab,", "tokens": ["Es", "schl\u00e4\u00b7get", "mir", "die", "Lie\u00b7be", "wohl", "nicht", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich hiemit gebeten hab'.", "tokens": ["Was", "ich", "hie\u00b7mit", "ge\u00b7be\u00b7ten", "hab'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil du mein Herz nimmst an, geh\u00f6rt es g\u00e4nzlich dir,", "tokens": ["Weil", "du", "mein", "Herz", "nimmst", "an", ",", "ge\u00b7h\u00f6rt", "es", "g\u00e4nz\u00b7lich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Eigentum doch nie verlier;", "tokens": ["Dein", "Ei\u00b7gen\u00b7tum", "doch", "nie", "ver\u00b7lier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df lieber auf mein Haupt all' deine Fluten schlagen,", "tokens": ["La\u00df", "lie\u00b7ber", "auf", "mein", "Haupt", "all'", "dei\u00b7ne", "Flu\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Herzensk\u00f6nig, Gottes Lamm,", "tokens": ["Mein", "Her\u00b7zens\u00b7k\u00f6\u00b7nig", ",", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Leiden sind mir s\u00fc\u00df, ich will die h\u00e4rtsten tragen,", "tokens": ["Die", "Lei\u00b7den", "sind", "mir", "s\u00fc\u00df", ",", "ich", "will", "die", "h\u00e4rts\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn du nur wirst mein Br\u00e4utigam!", "tokens": ["Wenn", "du", "nur", "wirst", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}