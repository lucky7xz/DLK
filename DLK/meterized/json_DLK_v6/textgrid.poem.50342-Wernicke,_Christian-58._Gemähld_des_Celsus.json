{"textgrid.poem.50342": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "58. Gem\u00e4hld des Celsus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer preisst den ", "tokens": ["Wer", "preisst", "den"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "So wenig sind an ", "tokens": ["So", "we\u00b7nig", "sind", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der keinen ", "tokens": ["Der", "kei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und keinen ", "tokens": ["Und", "kei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So sehr ", "tokens": ["So", "sehr"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dass seine ", "tokens": ["Dass", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Dass seinen ", "tokens": ["Dass", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Ein F\u00fcrst, der Lohn und Schutz bedr\u00e4ngter Tugend giebet,", "tokens": ["Ein", "F\u00fcrst", ",", "der", "Lohn", "und", "Schutz", "be\u00b7dr\u00e4ng\u00b7ter", "Tu\u00b7gend", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sich mit ", "tokens": ["Und", "sich", "mit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "So dass er neulich noch diss Lob davon gebracht:", "tokens": ["So", "dass", "er", "neu\u00b7lich", "noch", "diss", "Lob", "da\u00b7von", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}