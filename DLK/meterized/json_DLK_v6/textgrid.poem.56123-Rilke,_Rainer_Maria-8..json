{"textgrid.poem.56123": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur im Raum der R\u00fchmung darf die Klage", "tokens": ["Nur", "im", "Raum", "der", "R\u00fch\u00b7mung", "darf", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gehn, die Nymphe des geweinten Quells,", "tokens": ["gehn", ",", "die", "Nym\u00b7phe", "des", "ge\u00b7wein\u00b7ten", "Quells", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wachend \u00fcber unserm Niederschlage,", "tokens": ["wa\u00b7chend", "\u00fc\u00b7ber", "un\u00b7serm", "Nie\u00b7der\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "da\u00df er klar sei an demselben Fels,", "tokens": ["da\u00df", "er", "klar", "sei", "an", "dem\u00b7sel\u00b7ben", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "der die Tore tr\u00e4gt und die Alt\u00e4re. \u2013", "tokens": ["der", "die", "To\u00b7re", "tr\u00e4gt", "und", "die", "Al\u00b7t\u00e4\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh, um ihre stillen Schultern fr\u00fcht", "tokens": ["Sieh", ",", "um", "ih\u00b7re", "stil\u00b7len", "Schul\u00b7tern", "fr\u00fcht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "das Gef\u00fchl, da\u00df sie die j\u00fcngste w\u00e4re", "tokens": ["das", "Ge\u00b7f\u00fchl", ",", "da\u00df", "sie", "die", "j\u00fcngs\u00b7te", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "unter den Geschwistern im Gem\u00fct.", "tokens": ["un\u00b7ter", "den", "Ge\u00b7schwis\u00b7tern", "im", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jubel ", "tokens": ["Ju\u00b7bel"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "nur die Klage lernt noch; m\u00e4dchenh\u00e4ndig", "tokens": ["nur", "die", "Kla\u00b7ge", "lernt", "noch", ";", "m\u00e4d\u00b7chen\u00b7h\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$.", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "z\u00e4hlt sie n\u00e4chtelang das alte Schlimme.", "tokens": ["z\u00e4hlt", "sie", "n\u00e4ch\u00b7tel\u00b7ang", "das", "al\u00b7te", "Schlim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber pl\u00f6tzlich, schr\u00e4g und unge\u00fcbt,", "tokens": ["A\u00b7ber", "pl\u00f6tz\u00b7lich", ",", "schr\u00e4g", "und", "un\u00b7ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "h\u00e4lt sie doch ein Sternbild unsrer Stimme", "tokens": ["h\u00e4lt", "sie", "doch", "ein", "Stern\u00b7bild", "uns\u00b7rer", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in den Himmel, den ihr Hauch nicht tr\u00fcbt.", "tokens": ["in", "den", "Him\u00b7mel", ",", "den", "ihr", "Hauch", "nicht", "tr\u00fcbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}