{"textgrid.poem.48750": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "37. Mons. Bernhard Ostermannen, F\u00fcrstl. Holsteinischer Gesandten Pandoristen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df heute bringen her dein trefliches Pandor,", "tokens": ["La\u00df", "heu\u00b7te", "brin\u00b7gen", "her", "dein", "tref\u00b7li\u00b7ches", "Pan\u00b7dor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVINF", "APZR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf dem du Meister bist. Sie, deine Kameraden,", "tokens": ["auf", "dem", "du", "Meis\u00b7ter", "bist", ".", "Sie", ",", "dei\u00b7ne", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VAFIN", "$.", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die kommen auf dein Fest, l\u00e4\u00dft du sie auch nicht laden.", "tokens": ["die", "kom\u00b7men", "auf", "dein", "Fest", ",", "l\u00e4\u00dft", "du", "sie", "auch", "nicht", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir wollen ingesamt bestimmen einen Chor", "tokens": ["Wir", "wol\u00b7len", "in\u00b7ge\u00b7samt", "be\u00b7stim\u00b7men", "ei\u00b7nen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "durch Saiten und Gesang. Ihr andern f\u00fchrt ein Tor", "tokens": ["durch", "Sai\u00b7ten", "und", "Ge\u00b7sang", ".", "Ihr", "an\u00b7dern", "f\u00fchrt", "ein", "Tor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "PPOSAT", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von gr\u00fcnen Str\u00e4uchern auf f\u00fcr dieser Hitze Schaden.", "tokens": ["von", "gr\u00fc\u00b7nen", "Str\u00e4u\u00b7chern", "auf", "f\u00fcr", "die\u00b7ser", "Hit\u00b7ze", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir wollen unsern Mund und Magen heute baden,", "tokens": ["Wir", "wol\u00b7len", "un\u00b7sern", "Mund", "und", "Ma\u00b7gen", "heu\u00b7te", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "bis uns die schwarze Nacht verh\u00fcllt in ihren Flor.", "tokens": ["bis", "uns", "die", "schwar\u00b7ze", "Nacht", "ver\u00b7h\u00fcllt", "in", "ih\u00b7ren", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Trag Alles, was du hast in K\u00fcch' und Keller f\u00fcr,", "tokens": ["Trag", "Al\u00b7les", ",", "was", "du", "hast", "in", "K\u00fc\u00b7ch'", "und", "Kel\u00b7ler", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWS", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "APPR", "$,"], "meter": "+----+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gott wei\u00df auf Morgen mehr, du hast uns heute hier.", "tokens": ["Gott", "wei\u00df", "auf", "Mor\u00b7gen", "mehr", ",", "du", "hast", "uns", "heu\u00b7te", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir wollen frisch vertun, ein Andrer mags erwerben.", "tokens": ["Wir", "wol\u00b7len", "frisch", "ver\u00b7tun", ",", "ein", "A\u00b7ndrer", "mags", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich halte nichts von dem, der allzeit Grillen scheu\u00dft,", "tokens": ["Ich", "hal\u00b7te", "nichts", "von", "dem", ",", "der", "all\u00b7zeit", "Gril\u00b7len", "scheu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und sich die ganze Zeit mit tollen Sorgen schmei\u00dft.", "tokens": ["und", "sich", "die", "gan\u00b7ze", "Zeit", "mit", "tol\u00b7len", "Sor\u00b7gen", "schmei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will noch lustig sein, solt' ich auch heute sterben.", "tokens": ["Ich", "will", "noch", "lus\u00b7tig", "sein", ",", "solt'", "ich", "auch", "heu\u00b7te", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}