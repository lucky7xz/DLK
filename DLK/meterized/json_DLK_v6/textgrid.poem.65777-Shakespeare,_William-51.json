{"textgrid.poem.65777": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "51", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebreich entschuld'gen, tr\u00e4gt er mich von dir:", "tokens": ["Lieb\u00b7reich", "ent\u00b7schuld'\u00b7gen", ",", "tr\u00e4gt", "er", "mich", "von", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was soll die Eil als bei der R\u00fcckkehr mir?", "tokens": ["Was", "soll", "die", "Eil", "als", "bei", "der", "R\u00fcck\u00b7kehr", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O, wie wird dann mein Tier Entschuld'gung finden,", "tokens": ["O", ",", "wie", "wird", "dann", "mein", "Tier", "Ent\u00b7schuld'\u00b7gung", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wenn schnellste Schnelligkeit nur scheint Verzug?", "tokens": ["Wenn", "schnells\u00b7te", "Schnel\u00b7lig\u00b7keit", "nur", "scheint", "Ver\u00b7zug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann m\u00fc\u00dft' ich spornen, s\u00e4\u00df ich auf den Winden;", "tokens": ["Dann", "m\u00fc\u00dft'", "ich", "spor\u00b7nen", ",", "s\u00e4\u00df", "ich", "auf", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bewegungslos schien mir des Fittigs Flug:", "tokens": ["Be\u00b7we\u00b7gungs\u00b7los", "schien", "mir", "des", "Fit\u00b7tigs", "Flug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Dann h\u00e4lt kein Ro\u00df mit meiner Sehnsucht Schritt;", "tokens": ["Dann", "h\u00e4lt", "kein", "Ro\u00df", "mit", "mei\u00b7ner", "Sehn\u00b7sucht", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und Sehnsucht, die vollkommner Lieb' entspro\u00df,", "tokens": ["Und", "Sehn\u00b7sucht", ",", "die", "voll\u00b7komm\u00b7ner", "Lieb'", "ent\u00b7spro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nicht tr\u00e4ges Fleisch, wieh'rt feueratmend mit,", "tokens": ["Nicht", "tr\u00e4\u00b7ges", "Fleisch", ",", "wieh'rt", "feu\u00b7e\u00b7rat\u00b7mend", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und Lieb' um Lieb' entschuldigt ", "tokens": ["Und", "Lieb'", "um", "Lieb'", "ent\u00b7schul\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Weil ich's von dir hinweg freiwillig z\u00f6gern sehn,", "tokens": ["Weil", "ich's", "von", "dir", "hin\u00b7weg", "frei\u00b7wil\u00b7lig", "z\u00f6\u00b7gern", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Will ich zu dir nun laufen, es mag gehn.", "tokens": ["Will", "ich", "zu", "dir", "nun", "lau\u00b7fen", ",", "es", "mag", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}