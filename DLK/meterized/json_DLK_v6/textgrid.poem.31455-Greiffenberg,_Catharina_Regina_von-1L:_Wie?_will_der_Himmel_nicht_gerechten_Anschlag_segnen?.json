{"textgrid.poem.31455": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? will der Himmel nicht gerechten Anschlag segnen?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? will der Himmel nicht gerechten Anschlag segnen?", "tokens": ["Wie", "?", "will", "der", "Him\u00b7mel", "nicht", "ge\u00b7rech\u00b7ten", "An\u00b7schlag", "seg\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist denn nicht / wie zuvor / der seelig / der ihm traut?", "tokens": ["ist", "denn", "nicht", "/", "wie", "zu\u00b7vor", "/", "der", "see\u00b7lig", "/", "der", "ihm", "traut", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "$(", "KOKOM", "ADV", "$(", "ART", "ADJD", "$(", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie da\u00df das Widerspiel man hier so kl\u00e4rlich schaut /", "tokens": ["wie", "da\u00df", "das", "Wi\u00b7der\u00b7spiel", "man", "hier", "so", "kl\u00e4r\u00b7lich", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df dem / des Hoffnung Gott / alls Vngl\u00fcck mu\u00df begegnen /", "tokens": ["da\u00df", "dem", "/", "des", "Hoff\u00b7nung", "Gott", "/", "alls", "Vn\u00b7gl\u00fcck", "mu\u00df", "be\u00b7geg\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ART", "NN", "NN", "$(", "KOUS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ganzes Jammer-Meer die Sternen auf ihn regnen;", "tokens": ["Ein", "gan\u00b7zes", "Jam\u00b7mer\u00b7Meer", "die", "Ster\u00b7nen", "auf", "ihn", "reg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "je mehr in Schlamm er sinkt / je mehr er auf ihn baut /", "tokens": ["je", "mehr", "in", "Schlamm", "er", "sinkt", "/", "je", "mehr", "er", "auf", "ihn", "baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PPER", "VVFIN", "$(", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "und sich nur mehr und mehr im Vngl\u00fccks Pa\u00df verhaut.", "tokens": ["und", "sich", "nur", "mehr", "und", "mehr", "im", "Vn\u00b7gl\u00fccks", "Pa\u00df", "ver\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADV", "KON", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Viel besser geht es / Ach! den Gottlos-und verwegnen.", "tokens": ["Viel", "bes\u00b7ser", "geht", "es", "/", "Ach", "!", "den", "Gott\u00b7los\u00b7\u00b7und", "ver\u00b7weg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$(", "ITJ", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein / nein / mein Sinn / du irrst! schweig' und bedenk' ihr Ende.", "tokens": ["Nein", "/", "nein", "/", "mein", "Sinn", "/", "du", "irrst", "!", "schweig'", "und", "be\u00b7denk'", "ihr", "En\u00b7de", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPOSAT", "NN", "$(", "PPER", "ADV", "$.", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn / auf das schl\u00fcpfrig' Ei\u00df hat sie der Herr gesetzt.", "tokens": ["Denn", "/", "auf", "das", "schl\u00fcpf\u00b7rig'", "Ei\u00df", "hat", "sie", "der", "Herr", "ge\u00b7setzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Ruhe-bringend Streit viel sicherer ergetzt /", "tokens": ["Der", "Ru\u00b7he\u00b7brin\u00b7gend", "Streit", "viel", "si\u00b7che\u00b7rer", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "als wann vom Nectar ich zum Acheron anl\u00e4nde.", "tokens": ["als", "wann", "vom", "Nec\u00b7tar", "ich", "zum", "A\u00b7che\u00b7ron", "an\u00b7l\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das gute / da\u00df das h\u00f6chst Gut nicht bef\u00f6rdern will /", "tokens": ["Das", "gu\u00b7te", "/", "da\u00df", "das", "h\u00f6chst", "Gut", "nicht", "be\u00b7f\u00f6r\u00b7dern", "will", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "KOUS", "PDS", "ADV", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ist nicht gut / oder hat noch nicht erreicht sein Ziel.", "tokens": ["ist", "nicht", "gut", "/", "o\u00b7der", "hat", "noch", "nicht", "er\u00b7reicht", "sein", "Ziel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$(", "KON", "VAFIN", "ADV", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}