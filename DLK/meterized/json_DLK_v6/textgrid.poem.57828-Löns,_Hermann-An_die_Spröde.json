{"textgrid.poem.57828": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "An die Spr\u00f6de", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gertrude, wei\u00dfe Blume,", "tokens": ["Ger\u00b7tru\u00b7de", ",", "wei\u00b7\u00dfe", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was bist du so stolz;", "tokens": ["Was", "bist", "du", "so", "stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es w\u00e4chst kein gr\u00fcnes Bl\u00e4ttlein", "tokens": ["Es", "w\u00e4chst", "kein", "gr\u00fc\u00b7nes", "Bl\u00e4tt\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am trockenen Holz.", "tokens": ["Am", "tro\u00b7cke\u00b7nen", "Holz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall singt nicht", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Schnee und in Eis;", "tokens": ["In", "Schnee", "und", "in", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Liebe schmeckt am sch\u00f6nsten,", "tokens": ["Die", "Lie\u00b7be", "schmeckt", "am", "sch\u00f6ns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn niemand es wei\u00df.", "tokens": ["Wenn", "nie\u00b7mand", "es", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Gertrude, wei\u00dfe Blume,", "tokens": ["Ger\u00b7tru\u00b7de", ",", "wei\u00b7\u00dfe", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Flieder der bl\u00fcht;", "tokens": ["Der", "Flie\u00b7der", "der", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Nachtigall im Walde", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die singet ihr Lied.", "tokens": ["Die", "sin\u00b7get", "ihr", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Sie singet von Liebe,", "tokens": ["Sie", "sin\u00b7get", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie singet von Gl\u00fcck;", "tokens": ["Sie", "sin\u00b7get", "von", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Zeit, die verpa\u00dft ist,", "tokens": ["Die", "Zeit", ",", "die", "ver\u00b7pa\u00dft", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die kommt nicht zur\u00fcck.", "tokens": ["Die", "kommt", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Gertrude, wei\u00dfe Blume,", "tokens": ["Ger\u00b7tru\u00b7de", ",", "wei\u00b7\u00dfe", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kurz ist der Mai;", "tokens": ["Und", "kurz", "ist", "der", "Mai", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was willst du noch warten \u2013", "tokens": ["Was", "willst", "du", "noch", "war\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bald ist es vorbei.", "tokens": ["Bald", "ist", "es", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}