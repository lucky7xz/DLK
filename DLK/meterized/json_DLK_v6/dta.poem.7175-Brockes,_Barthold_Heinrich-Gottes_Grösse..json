{"dta.poem.7175": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich aber endlich dencke,", "tokens": ["Wenn", "ich", "a\u00b7ber", "end\u00b7lich", "den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der S\u00fcd- und Norder-Pol", "tokens": ["Was", "der", "S\u00fcd", "und", "Nor\u00b7der\u00b7Pol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wol f\u00fcr einen Raum verschrencke,", "tokens": ["Wol", "f\u00fcr", "ei\u00b7nen", "Raum", "ver\u00b7schren\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und was dieser Abgrund wol", "tokens": ["Und", "was", "die\u00b7ser", "Ab\u00b7grund", "wol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PDAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr ein Sternen-Heer bedecke;", "tokens": ["F\u00fcr", "ein", "Ster\u00b7nen\u00b7Heer", "be\u00b7de\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So erstaun\u2019 ich, zittr\u2019, erschrecke.", "tokens": ["So", "er\u00b7staun'", "ich", ",", "zit\u00b7tr'", ",", "er\u00b7schre\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Mein so gar verschlung\u2019ner Sinn", "tokens": ["Mein", "so", "gar", "ver\u00b7schlung'\u00b7ner", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wei\u00df nicht, ob und was ich bin.", "tokens": ["Wei\u00df", "nicht", ",", "ob", "und", "was", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "KON", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}