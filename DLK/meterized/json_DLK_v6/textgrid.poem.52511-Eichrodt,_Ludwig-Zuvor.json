{"textgrid.poem.52511": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Zuvor", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wellensch\u00e4ume,", "tokens": ["Wel\u00b7len\u00b7sch\u00e4u\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wolkens\u00e4ume,", "tokens": ["Wol\u00b7ken\u00b7s\u00e4u\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "W\u00fcnsche, Tr\u00e4ume,", "tokens": ["W\u00fcn\u00b7sche", ",", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Im Entfalten,", "tokens": ["Im", "Ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Im Zerflie\u00dfen festgehalten;", "tokens": ["Im", "Zer\u00b7flie\u00b7\u00dfen", "fest\u00b7ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Manch Erlebtes", "tokens": ["Manch", "Er\u00b7leb\u00b7tes"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "L\u00e4ngst Entschwebtes,", "tokens": ["L\u00e4ngst", "Ent\u00b7schweb\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Mit Gestalten", "tokens": ["Mit", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Leicht Verwebtes,", "tokens": ["Leicht", "Ver\u00b7web\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Wie sie kommen, wie sie fliehn", "tokens": ["Wie", "sie", "kom\u00b7men", ",", "wie", "sie", "fliehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u2013 Launekinder, Phantasien,", "tokens": ["\u2013", "Lau\u00b7ne\u00b7kin\u00b7der", ",", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Bilder im Vor\u00fcberziehn,", "tokens": ["Bil\u00b7der", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Liebespoesien!", "tokens": ["Lie\u00b7be\u00b7spo\u00b7e\u00b7si\u00b7en", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}