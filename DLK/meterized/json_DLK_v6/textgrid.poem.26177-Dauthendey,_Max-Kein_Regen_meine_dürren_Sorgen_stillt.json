{"textgrid.poem.26177": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Kein Regen meine d\u00fcrren Sorgen stillt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regennebel dampft und Waldgew\u00fchl sich bl\u00e4ht,", "tokens": ["Der", "Re\u00b7gen\u00b7ne\u00b7bel", "dampft", "und", "Wald\u00b7ge\u00b7w\u00fchl", "sich", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Riese schwer in Wasserstiefeln stampft,", "tokens": ["Ein", "Rie\u00b7se", "schwer", "in", "Was\u00b7ser\u00b7stie\u00b7feln", "stampft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hat das Blau vom Himmel fortgem\u00e4ht.", "tokens": ["Er", "hat", "das", "Blau", "vom", "Him\u00b7mel", "fort\u00b7ge\u00b7m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der m\u00fcde Abend nimmt den Nebel sich als Pf\u00fchl,", "tokens": ["Der", "m\u00fc\u00b7de", "A\u00b7bend", "nimmt", "den", "Ne\u00b7bel", "sich", "als", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Waldholz steht prunkend wie ein Chorgest\u00fchl", "tokens": ["Wald\u00b7holz", "steht", "prun\u00b7kend", "wie", "ein", "Chor\u00b7ge\u00b7st\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "In einem Dom, der seit Jahrhunderten ergraut und schw\u00fcl.", "tokens": ["In", "ei\u00b7nem", "Dom", ",", "der", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "er\u00b7graut", "und", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+---+---+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Dem Walde schwillt wie nebeltrunken jeder Knorren.", "tokens": ["Dem", "Wal\u00b7de", "schwillt", "wie", "ne\u00b7bel\u00b7trun\u00b7ken", "je\u00b7der", "Knor\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur mir kein Regen meine d\u00fcrren Sorgen stillt,", "tokens": ["Nur", "mir", "kein", "Re\u00b7gen", "mei\u00b7ne", "d\u00fcr\u00b7ren", "Sor\u00b7gen", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es mu\u00df die Hand erst im Gebet verdorren.", "tokens": ["Es", "mu\u00df", "die", "Hand", "erst", "im", "Ge\u00b7bet", "ver\u00b7dor\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}