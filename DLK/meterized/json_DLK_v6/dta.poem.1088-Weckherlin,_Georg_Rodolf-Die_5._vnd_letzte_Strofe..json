{"dta.poem.1088": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 5. vnd letzte Strofe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zweck/ welchen ich mir f\u00fcrnem/", "tokens": ["Der", "Zweck", "/", "wel\u00b7chen", "ich", "mir", "f\u00fcr\u00b7nem", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "PPER", "PPER", "ADJA", "$("], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Darnach begihriglich zuzihlen/", "tokens": ["Dar\u00b7nach", "be\u00b7gih\u00b7rig\u00b7lich", "zu\u00b7zih\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist allein/ das ich w\u00fcnsch zu fihlen/", "tokens": ["Ist", "al\u00b7lein", "/", "das", "ich", "w\u00fcnsch", "zu", "fih\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "PRELS", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das dir dise gab angenem.", "tokens": ["Das", "dir", "di\u00b7se", "gab", "an\u00b7ge\u00b7nem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PDS", "VVFIN", "NE", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "O du/ vnder dem sich mein gl\u00fcck", "tokens": ["O", "du", "/", "vn\u00b7der", "dem", "sich", "mein", "gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "APPR", "PRELS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor keinem ansto\u00df darf befahren/", "tokens": ["Vor", "kei\u00b7nem", "an\u00b7sto\u00df", "darf", "be\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mir mit einem frewden-blick", "tokens": ["La\u00df", "mir", "mit", "ei\u00b7nem", "fre\u00b7wden\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Deiner gunst zaichen widerfahren:", "tokens": ["Dei\u00b7ner", "gunst", "zai\u00b7chen", "wi\u00b7der\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Vnd wan dises so ich dir bring", "tokens": ["Vnd", "wan", "di\u00b7ses", "so", "ich", "dir", "bring"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PDS", "ADV", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dein verlangen nicht solt erf\u00fcllen/", "tokens": ["Dein", "ver\u00b7lan\u00b7gen", "nicht", "solt", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Gedenck/ das keine gaab gering/", "tokens": ["Ge\u00b7denck", "/", "das", "kei\u00b7ne", "ga\u00b7ab", "ge\u00b7ring", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wan Sie geschicht mit reichem willen.", "tokens": ["Wan", "Sie", "ge\u00b7schicht", "mit", "rei\u00b7chem", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}