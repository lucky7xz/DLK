{"textgrid.poem.47305": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser Haus hat viele Th\u00fcren,", "tokens": ["Un\u00b7ser", "Haus", "hat", "vie\u00b7le", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die hinein zum Herren f\u00fchren.", "tokens": ["Die", "hin\u00b7ein", "zum", "Her\u00b7ren", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer den Herrn sieht, mu\u00df anbetend", "tokens": ["Wer", "den", "Herrn", "sieht", ",", "mu\u00df", "an\u00b7be\u00b7tend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VMFIN", "VVPP"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit der Stirn den Boden r\u00fchren.", "tokens": ["Mit", "der", "Stirn", "den", "Bo\u00b7den", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viel' im Haus sind blind geboren,", "tokens": ["Viel'", "im", "Haus", "sind", "blind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die des Herrn Gebot doch sp\u00fcren.", "tokens": ["Die", "des", "Herrn", "Ge\u00b7bot", "doch", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch den Lahmen sind gegeben", "tokens": ["Auch", "den", "Lah\u00b7men", "sind", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hausgesch\u00e4fte zu vollf\u00fchren.", "tokens": ["Haus\u00b7ge\u00b7sch\u00e4f\u00b7te", "zu", "voll\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Selbst der Wind mit kaltem Atem", "tokens": ["Selbst", "der", "Wind", "mit", "kal\u00b7tem", "A\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mu\u00df des Hauses Feuer sch\u00fcren.", "tokens": ["Mu\u00df", "des", "Hau\u00b7ses", "Feu\u00b7er", "sch\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Thun mu\u00df jeder, was ihm obliegt,", "tokens": ["Thun", "mu\u00df", "je\u00b7der", ",", "was", "ihm", "ob\u00b7liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.12": {"text": "Wahl hat keiner, selbst zu k\u00fcren.", "tokens": ["Wahl", "hat", "kei\u00b7ner", ",", "selbst", "zu", "k\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mancher w\u00e4hnt sich frei und siehet", "tokens": ["Man\u00b7cher", "w\u00e4hnt", "sich", "frei", "und", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nicht die Bande, die ihn schn\u00fcren.", "tokens": ["Nicht", "die", "Ban\u00b7de", ",", "die", "ihn", "schn\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Tr\u00e4gest du dein Band in Demut,", "tokens": ["Tr\u00e4\u00b7gest", "du", "dein", "Band", "in", "De\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wird es dir zu Blumenschn\u00fcren.", "tokens": ["Wird", "es", "dir", "zu", "Blu\u00b7men\u00b7schn\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schw\u00f6re Treu'! und Gnad' antwortet", "tokens": ["Schw\u00f6\u00b7re", "Treu'", "!", "und", "Gnad'", "ant\u00b7wor\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dir mit h\u00f6chsten Liebeschw\u00fcren.", "tokens": ["Dir", "mit", "h\u00f6chs\u00b7ten", "Lie\u00b7be\u00b7schw\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Knecht im Hause! gegen deinen", "tokens": ["Knecht", "im", "Hau\u00b7se", "!", "ge\u00b7gen", "dei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPRART", "NN", "$.", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Mitknecht will kein Stolz geb\u00fchren.", "tokens": ["Mit\u00b7knecht", "will", "kein", "Stolz", "ge\u00b7b\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sei vertr\u00e4glich! denn der Herr hat", "tokens": ["Sei", "ver\u00b7tr\u00e4g\u00b7lich", "!", "denn", "der", "Herr", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$.", "KON", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Keine Freud' an Ungeb\u00fchren.", "tokens": ["Kei\u00b7ne", "Freud'", "an", "Un\u00b7ge\u00b7b\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wer darf trotzig Einla\u00df fordern,", "tokens": ["Wer", "darf", "trot\u00b7zig", "Ein\u00b7la\u00df", "for\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Den nicht Er ein l\u00e4sset f\u00fchren?", "tokens": ["Den", "nicht", "Er", "ein", "l\u00e4s\u00b7set", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wer kann mit dem Hausherrn hadern,", "tokens": ["Wer", "kann", "mit", "dem", "Haus\u00b7herrn", "ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Den er st\u00f6\u00dft aus seinen Th\u00fcren?", "tokens": ["Den", "er", "st\u00f6\u00dft", "aus", "sei\u00b7nen", "Th\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}