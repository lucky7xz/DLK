{"textgrid.poem.54086": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Stationen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst gehst du umher und suchst an der Frau", "tokens": ["Erst", "gehst", "du", "um\u00b7her", "und", "suchst", "an", "der", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das, was man anfassen kann.", "tokens": ["das", ",", "was", "man", "an\u00b7fas\u00b7sen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollkn\u00e4ul, Spielzeug und K\u00e4tzchen \u2013 Miau \u2013", "tokens": ["Woll\u00b7kn\u00e4ul", ",", "Spiel\u00b7zeug", "und", "K\u00e4tz\u00b7chen", "\u2013", "Mi\u00b7au", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$(", "NE", "$("], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "du bist noch kein richtiger Mann.", "tokens": ["du", "bist", "noch", "kein", "rich\u00b7ti\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Du willst eine lustig bewegte Ruh:", "tokens": ["Du", "willst", "ei\u00b7ne", "lus\u00b7tig", "be\u00b7weg\u00b7te", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "sie soll anders sein, aber sonst wie du . . .", "tokens": ["sie", "soll", "an\u00b7ders", "sein", ",", "a\u00b7ber", "sonst", "wie", "du", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAINF", "$,", "ADV", "ADV", "KOKOM", "PPER", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Herz sagt:", "tokens": ["Dein", "Herz", "sagt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Max und Moritz!", "tokens": ["Max", "und", "Mo\u00b7ritz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Das verw\u00e4chst du. Dann langts nicht mit dem Verstand.", "tokens": ["Das", "ver\u00b7w\u00e4chst", "du", ".", "Dann", "langts", "nicht", "mit", "dem", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Karriere! Es ist Zeit . . . !", "tokens": ["Die", "Kar\u00b7rie\u00b7re", "!", "Es", "ist", "Zeit", ".", ".", ".", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine kluge Frau nimmt dich an die Hand", "tokens": ["Ei\u00b7ne", "klu\u00b7ge", "Frau", "nimmt", "dich", "an", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "in tyrannischer M\u00fctterlichkeit.", "tokens": ["in", "ty\u00b7ran\u00b7ni\u00b7scher", "M\u00fct\u00b7ter\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sie pa\u00dft auf dich auf. Sie wartet zu Haus.", "tokens": ["Sie", "pa\u00dft", "auf", "dich", "auf", ".", "Sie", "war\u00b7tet", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du weinst dich an ihren Br\u00fcsten aus . . .", "tokens": ["Du", "weinst", "dich", "an", "ih\u00b7ren", "Br\u00fcs\u00b7ten", "aus", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dein Herz sagt:", "tokens": ["Dein", "Herz", "sagt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Mutter.", "tokens": ["Mut\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Das verw\u00e4chst du. Nun bist du ein reifer Mann.", "tokens": ["Das", "ver\u00b7w\u00e4chst", "du", ".", "Nun", "bist", "du", "ein", "rei\u00b7fer", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Dir wird etwas sanft im Gem\u00fct.", "tokens": ["Dir", "wird", "et\u00b7was", "sanft", "im", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du m\u00f6chtest, da\u00df im Bett nebenan", "tokens": ["Du", "m\u00f6ch\u00b7test", ",", "da\u00df", "im", "Bett", "ne\u00b7be\u00b7nan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPRART", "NN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "eine fremde Jugend gl\u00fcht.", "tokens": ["ei\u00b7ne", "frem\u00b7de", "Ju\u00b7gend", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dumm kann sie sein. Du willst: junges Tier,", "tokens": ["Dumm", "kann", "sie", "sein", ".", "Du", "willst", ":", "jun\u00b7ges", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$.", "PPER", "VMFIN", "$.", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "ein Reh, eine Wilde, ein Elixier.", "tokens": ["ein", "Reh", ",", "ei\u00b7ne", "Wil\u00b7de", ",", "ein", "E\u00b7li\u00b7xier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Dein Herz sagt:", "tokens": ["Dein", "Herz", "sagt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Erde.", "tokens": ["Er\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Und dann bist du alt.", "tokens": ["Und", "dann", "bist", "du", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und ist es soweit,", "tokens": ["Und", "ist", "es", "so\u00b7weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "da\u00df ihr an der Verdauung leidet \u2013:", "tokens": ["da\u00df", "ihr", "an", "der", "Ver\u00b7dau\u00b7ung", "lei\u00b7det", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "dann sitzt ihr auf einem B\u00e4nkchen zu zweit,", "tokens": ["dann", "sitzt", "ihr", "auf", "ei\u00b7nem", "B\u00e4nk\u00b7chen", "zu", "zweit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "als Philemon und Baucis verkleidet.", "tokens": ["als", "Phi\u00b7le\u00b7mon", "und", "Bau\u00b7cis", "ver\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "VVPP", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie sagt nichts. Du sagst nichts, denn ihr wi\u00dft,", "tokens": ["Sie", "sagt", "nichts", ".", "Du", "sagst", "nichts", ",", "denn", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "PIS", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "wie es im menschlichen Leben ist . . .", "tokens": ["wie", "es", "im", "menschli\u00b7chen", "Le\u00b7ben", "ist", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "VAFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Herz, das so viele Frauen besang,", "tokens": ["Dein", "Herz", ",", "das", "so", "vie\u00b7le", "Frau\u00b7en", "be\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "dein Herz sagt: \u00bbNa, Alte . . . ?\u00ab", "tokens": ["dein", "Herz", "sagt", ":", "\u00bb", "Na", ",", "Al\u00b7te", ".", ".", ".", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "ITJ", "$,", "NN", "$.", "$.", "$.", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Dein Herz sagt: Dank.", "tokens": ["Dein", "Herz", "sagt", ":", "Dank", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}