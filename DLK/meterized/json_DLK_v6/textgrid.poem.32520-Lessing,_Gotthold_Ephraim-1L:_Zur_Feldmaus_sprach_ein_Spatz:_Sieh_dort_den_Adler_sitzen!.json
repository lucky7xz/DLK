{"textgrid.poem.32520": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zur Feldmaus sprach ein Spatz: Sieh dort den Adler sitzen!", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Feldmaus sprach ein Spatz: Sieh dort den Adler sitzen!", "tokens": ["Zur", "Feld\u00b7maus", "sprach", "ein", "Spatz", ":", "Sieh", "dort", "den", "Ad\u00b7ler", "sit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "NE", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, weil du ihn noch siehst! er wiegt den K\u00f6rper schon;", "tokens": ["Sieh", ",", "weil", "du", "ihn", "noch", "siehst", "!", "er", "wiegt", "den", "K\u00f6r\u00b7per", "schon", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bereit zum k\u00fchnen Flug, bekannt mit Sonn' und Blitzen,", "tokens": ["Be\u00b7reit", "zum", "k\u00fch\u00b7nen", "Flug", ",", "be\u00b7kannt", "mit", "Sonn'", "und", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zielt er nach Jovis Thron.", "tokens": ["Zielt", "er", "nach", "Jo\u00b7vis", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch wette, - seh' ich schon nicht adlerm\u00e4\u00dfig aus -", "tokens": ["Doch", "wet\u00b7te", ",", "seh'", "ich", "schon", "nicht", "ad\u00b7ler\u00b7m\u00e4\u00b7\u00dfig", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich flieg' ihm gleich. - Fleug, Prahler! rief die Maus.", "tokens": ["Ich", "flieg'", "ihm", "gleich", ".", "Fleug", ",", "Prah\u00b7ler", "!", "rief", "die", "Maus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "NN", "$,", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Indes flog jener auf, k\u00fchn auf gepr\u00fcfte Schwingen;", "tokens": ["In\u00b7des", "flog", "je\u00b7ner", "auf", ",", "k\u00fchn", "auf", "ge\u00b7pr\u00fcf\u00b7te", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "PTKVZ", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dieser wagts, ihm nachzudringen.", "tokens": ["Und", "die\u00b7ser", "wagts", ",", "ihm", "nach\u00b7zu\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch kaum, da\u00df ihr ungleicher Flug", "tokens": ["Doch", "kaum", ",", "da\u00df", "ihr", "un\u00b7glei\u00b7cher", "Flug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie beide bis zur H\u00f6h' gemeiner B\u00e4ume trug,", "tokens": ["Sie", "bei\u00b7de", "bis", "zur", "H\u00f6h'", "ge\u00b7mei\u00b7ner", "B\u00e4u\u00b7me", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als beide sich dem Blick der bl\u00f6den Maus entzogen,", "tokens": ["Als", "bei\u00b7de", "sich", "dem", "Blick", "der", "bl\u00f6\u00b7den", "Maus", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und beide, wie sie schlo\u00df, gleich unerme\u00dflich flogen.", "tokens": ["Und", "bei\u00b7de", ",", "wie", "sie", "schlo\u00df", ",", "gleich", "un\u00b7er\u00b7me\u00df\u00b7lich", "flo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein unbiegsamer F* will k\u00fchn wie Milton singen.", "tokens": ["Ein", "un\u00b7bieg\u00b7sa\u00b7mer", "F", "*", "will", "k\u00fchn", "wie", "Mil\u00b7ton", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "XY", "XY", "VMFIN", "ADJD", "KOKOM", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem er Richter w\u00e4hlt, nach dem wirds ihm gelingen.", "tokens": ["Nach", "dem", "er", "Rich\u00b7ter", "w\u00e4hlt", ",", "nach", "dem", "wirds", "ihm", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$,", "APPR", "ART", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}