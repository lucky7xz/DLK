{"textgrid.poem.32014": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Perle wird vom Tau in einer Muschelh\u00f6hle", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Perle wird vom Tau in einer Muschelh\u00f6hle", "tokens": ["Die", "Per\u00b7le", "wird", "vom", "Tau", "in", "ei\u00b7ner", "Mu\u00b7schel\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gezeuget und geborn und dies ist bald beweist,", "tokens": ["Ge\u00b7zeu\u00b7get", "und", "ge\u00b7born", "und", "dies", "ist", "bald", "be\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "KON", "PDS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo dus nicht glauben willst: der Tau ist Gottes Geist,", "tokens": ["Wo", "dus", "nicht", "glau\u00b7ben", "willst", ":", "der", "Tau", "ist", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PTKNEG", "VVINF", "VMFIN", "$.", "ART", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Perle Jesus Christ, die Muschel meine Seele.", "tokens": ["Die", "Per\u00b7le", "Je\u00b7sus", "Christ", ",", "die", "Mu\u00b7schel", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-"