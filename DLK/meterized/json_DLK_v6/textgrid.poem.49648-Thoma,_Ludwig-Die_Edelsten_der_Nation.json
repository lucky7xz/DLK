{"textgrid.poem.49648": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Edelsten der Nation", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sagtet ihr nur damals von den Polen,", "tokens": ["Was", "sag\u00b7tet", "ihr", "nur", "da\u00b7mals", "von", "den", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich meine damals, als ihr keck gestohlen,", "tokens": ["Ich", "mei\u00b7ne", "da\u00b7mals", ",", "als", "ihr", "keck", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was sonst als Eigentum geheiligt ist?", "tokens": ["Was", "sonst", "als", "Ei\u00b7gen\u00b7tum", "ge\u00b7hei\u00b7ligt", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Ostmark wolltet ihr uns deutsch erhalten,", "tokens": ["Die", "Ost\u00b7mark", "woll\u00b7tet", "ihr", "uns", "deutsch", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht dulden, da\u00df dort fremde M\u00e4chte walten,", "tokens": ["Nicht", "dul\u00b7den", ",", "da\u00df", "dort", "frem\u00b7de", "M\u00e4ch\u00b7te", "wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die immerdar entfachen neuen Zwist!", "tokens": ["Die", "im\u00b7mer\u00b7dar", "ent\u00b7fa\u00b7chen", "neu\u00b7en", "Zwist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie doch hie\u00df es von den argen Pfaffen?", "tokens": ["Und", "wie", "doch", "hie\u00df", "es", "von", "den", "ar\u00b7gen", "Pfaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Vorhand sollten sie an sich zu raffen", "tokens": ["Die", "Vor\u00b7hand", "soll\u00b7ten", "sie", "an", "sich", "zu", "raf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PRF", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch euren Widerstand verhindert sein?", "tokens": ["Durch", "eu\u00b7ren", "Wi\u00b7der\u00b7stand", "ver\u00b7hin\u00b7dert", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Rom und Papst des Reiches Schicksal lenken,", "tokens": ["Da\u00df", "Rom", "und", "Papst", "des", "Rei\u00b7ches", "Schick\u00b7sal", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei unertr\u00e4glich euerm deutschen Denken", "tokens": ["Sei", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "eu\u00b7erm", "deut\u00b7schen", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schaffe eurem Herzen grimme Pein?", "tokens": ["Und", "schaf\u00b7fe", "eu\u00b7rem", "Her\u00b7zen", "grim\u00b7me", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verwundert sieht nun, wer euch arglos traute", "tokens": ["Ver\u00b7wun\u00b7dert", "sieht", "nun", ",", "wer", "euch", "arg\u00b7los", "trau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf das Wort von Preu\u00dfens Junkern baute,", "tokens": ["Und", "auf", "das", "Wort", "von", "Preu\u00b7\u00dfens", "Jun\u00b7kern", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verwundert sieht er, was ihr heute treibt.", "tokens": ["Ver\u00b7wun\u00b7dert", "sieht", "er", ",", "was", "ihr", "heu\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sieht alle Flammen eurer Wut erloschen,", "tokens": ["Sieht", "al\u00b7le", "Flam\u00b7men", "eu\u00b7rer", "Wut", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sieht, wie ihr neuerdings um Judasgroschen", "tokens": ["Sieht", ",", "wie", "ihr", "neu\u00b7er\u00b7dings", "um", "Ju\u00b7das\u00b7gro\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Feinden Deutschlands euren Dienst verschreibt.", "tokens": ["Den", "Fein\u00b7den", "Deutschlands", "eu\u00b7ren", "Dienst", "ver\u00b7schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch jene l\u00e4cheln, die euch nie gepriesen;", "tokens": ["Doch", "je\u00b7ne", "l\u00e4\u00b7cheln", ",", "die", "euch", "nie", "ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein alter Spruch nur hat sich wahr erwiesen:", "tokens": ["Ein", "al\u00b7ter", "Spruch", "nur", "hat", "sich", "wahr", "er\u00b7wie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schl\u00e4gt sich und vertr\u00e4gt sich schlechtes Pack", "tokens": ["Es", "schl\u00e4gt", "sich", "und", "ver\u00b7tr\u00e4gt", "sich", "schlech\u00b7tes", "Pack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun eilt, euch fromme Masken vorzubinden", "tokens": ["Nun", "eilt", ",", "euch", "from\u00b7me", "Mas\u00b7ken", "vor\u00b7zu\u00b7bin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und neu gef\u00fcgte Phrasen zu erfinden", "tokens": ["Und", "neu", "ge\u00b7f\u00fcg\u00b7te", "Phra\u00b7sen", "zu", "er\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Gott und Vaterland f\u00fcr euren Sack!", "tokens": ["Von", "Gott", "und", "Va\u00b7ter\u00b7land", "f\u00fcr", "eu\u00b7ren", "Sack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}