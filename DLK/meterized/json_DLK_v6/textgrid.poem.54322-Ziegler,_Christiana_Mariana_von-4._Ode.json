{"textgrid.poem.54322": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "4. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Eltern sagt, wie schlug das Herz", "tokens": ["Ihr", "El\u00b7tern", "sagt", ",", "wie", "schlug", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey eurer Kinder letzten Reise!", "tokens": ["Bey", "eu\u00b7rer", "Kin\u00b7der", "letz\u00b7ten", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts hemmte Thr\u00e4nen, Angst, und Schmerz,", "tokens": ["Nichts", "hemm\u00b7te", "Thr\u00e4\u00b7nen", ",", "Angst", ",", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mengten sich in Trank und Speise.", "tokens": ["Sie", "meng\u00b7ten", "sich", "in", "Trank", "und", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr rieft mit grossem Vorbedacht:", "tokens": ["Ihr", "rieft", "mit", "gros\u00b7sem", "Vor\u00b7be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nehmt euch, ihr S\u00f6hne, wohl in acht.", "tokens": ["Nehmt", "euch", ",", "ihr", "S\u00f6h\u00b7ne", ",", "wohl", "in", "acht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Zuruf kam aus zarten Trieben", "tokens": ["Der", "Zu\u00b7ruf", "kam", "aus", "zar\u00b7ten", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von der die sie getragen hat;", "tokens": ["Von", "der", "die", "sie", "ge\u00b7tra\u00b7gen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn die wird niemahls m\u00fcd und satt,", "tokens": ["Denn", "die", "wird", "nie\u00b7mahls", "m\u00fcd", "und", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Kind so als sich selbst zu lieben.", "tokens": ["Das", "Kind", "so", "als", "sich", "selbst", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Wagen rauscht, das Posthorn schallt:", "tokens": ["Der", "Wa\u00b7gen", "rauscht", ",", "das", "Post\u00b7horn", "schallt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das waren rechte Schreckensth\u00f6ne.", "tokens": ["Das", "wa\u00b7ren", "rech\u00b7te", "Schre\u00b7ckenst\u00b7h\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Vaters Blut in Adern wallt", "tokens": ["Des", "Va\u00b7ters", "Blut", "in", "A\u00b7dern", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dem Verreisen seiner S\u00f6hne.", "tokens": ["Bey", "dem", "Ver\u00b7rei\u00b7sen", "sei\u00b7ner", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Kinder Herz stimmt \u00fcberein;", "tokens": ["Der", "Kin\u00b7der", "Herz", "stimmt", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die Blicke m\u00fcssen Redner seyn:", "tokens": ["Die", "Bli\u00b7cke", "m\u00fcs\u00b7sen", "Red\u00b7ner", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Abschied wird durch sie genommen:", "tokens": ["Der", "Ab\u00b7schied", "wird", "durch", "sie", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein jeder eilet schnelle fort;", "tokens": ["Ein", "je\u00b7der", "ei\u00b7let", "schnel\u00b7le", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Eltern bleiben an dem Ort,", "tokens": ["Die", "El\u00b7tern", "blei\u00b7ben", "an", "dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis sie aus ihren Augen kommen.", "tokens": ["Bis", "sie", "aus", "ih\u00b7ren", "Au\u00b7gen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie freuen sich schon im voraus", "tokens": ["Sie", "freu\u00b7en", "sich", "schon", "im", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihrer Kinder Wohlergehen.", "tokens": ["Auf", "ih\u00b7rer", "Kin\u00b7der", "Woh\u00b7ler\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kehren ganz getrost nach Haus", "tokens": ["Und", "keh\u00b7ren", "ganz", "ge\u00b7trost", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie dieselben auch nicht sehen.", "tokens": ["Wenn", "sie", "die\u00b7sel\u00b7ben", "auch", "nicht", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hoffen dennoch auf ein Blat,", "tokens": ["Sie", "hof\u00b7fen", "den\u00b7noch", "auf", "ein", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das die vergn\u00fcgte Wirkung hat,", "tokens": ["Das", "die", "ver\u00b7gn\u00fcg\u00b7te", "Wir\u00b7kung", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Kummers Macht zu unterdr\u00fccken.", "tokens": ["Des", "Kum\u00b7mers", "Macht", "zu", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kinder Gl\u00fcck ist ihre Lust,", "tokens": ["Der", "Kin\u00b7der", "Gl\u00fcck", "ist", "ih\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dis labet die beklemmte Brust,", "tokens": ["Dis", "la\u00b7bet", "die", "be\u00b7klemm\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und kann den matten Geist erquicken.", "tokens": ["Und", "kann", "den", "mat\u00b7ten", "Geist", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum ist die erste Angst vorbey,", "tokens": ["Kaum", "ist", "die", "ers\u00b7te", "Angst", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sehen sie die Wolken th\u00fcrmen.", "tokens": ["So", "se\u00b7hen", "sie", "die", "Wol\u00b7ken", "th\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird die Sorge doppelt neu,", "tokens": ["Da", "wird", "die", "Sor\u00b7ge", "dop\u00b7pelt", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn die erboosten Winde st\u00fcrmen.", "tokens": ["Wenn", "die", "er\u00b7boos\u00b7ten", "Win\u00b7de", "st\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Furcht bet\u00e4ubet Sinn und Ohr;", "tokens": ["Die", "Furcht", "be\u00b7t\u00e4u\u00b7bet", "Sinn", "und", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie stellen sich im Geiste vor", "tokens": ["Sie", "stel\u00b7len", "sich", "im", "Geis\u00b7te", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Meeres Wuth, der Wellen Rasen.", "tokens": ["Des", "Mee\u00b7res", "Wuth", ",", "der", "Wel\u00b7len", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unm\u00f6glich kann das Schiff bestehn;", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kann", "das", "Schiff", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es mu\u00df zu Grund und Tr\u00fcmmern gehn.", "tokens": ["Es", "mu\u00df", "zu", "Grund", "und", "Tr\u00fcm\u00b7mern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "H\u00f6rt Aeol nicht bald auf zu blasen.", "tokens": ["H\u00f6rt", "A\u00b7e\u00b7ol", "nicht", "bald", "auf", "zu", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PTKNEG", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Der Ruf nimmt Stadt und Mauren ein:", "tokens": ["Der", "Ruf", "nimmt", "Stadt", "und", "Mau\u00b7ren", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schiff sey durch den Sturm verschlagen.", "tokens": ["Das", "Schiff", "sey", "durch", "den", "Sturm", "ver\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Wort durchdringet Mark und Bein,", "tokens": ["Dies", "Wort", "durch\u00b7drin\u00b7get", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch h\u00f6rt man sie nicht klagen.", "tokens": ["Und", "den\u00b7noch", "h\u00f6rt", "man", "sie", "nicht", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie waren wie vom Schlag ger\u00fchrt", "tokens": ["Sie", "wa\u00b7ren", "wie", "vom", "Schlag", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Odem wurde mehr versp\u00fchrt", "tokens": ["Kein", "O\u00b7dem", "wur\u00b7de", "mehr", "ver\u00b7sp\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie wollten auch zugleich verderben.", "tokens": ["Sie", "woll\u00b7ten", "auch", "zu\u00b7gleich", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jedoch in einem Augenblick", "tokens": ["Je\u00b7doch", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kam der entflohne Geist zur\u00fcck:", "tokens": ["Kam", "der", "ent\u00b7floh\u00b7ne", "Geist", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ihr Jammer ward ein t\u00e4glich Sterben.", "tokens": ["Ihr", "Jam\u00b7mer", "ward", "ein", "t\u00e4g\u00b7lich", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch endlich trat der Mann ins Spiel", "tokens": ["Doch", "end\u00b7lich", "trat", "der", "Mann", "ins", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wind und Meer Gehorsam leisten.", "tokens": ["Dem", "Wind", "und", "Meer", "Ge\u00b7hor\u00b7sam", "leis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der stellte gleich ein ander Ziel", "tokens": ["Der", "stell\u00b7te", "gleich", "ein", "an\u00b7der", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie begl\u00fcckt zur\u00fccke reisten.", "tokens": ["Da\u00df", "sie", "be\u00b7gl\u00fcckt", "zu\u00b7r\u00fc\u00b7cke", "reis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum gr\u00fc\u00dfen sie das sichre Land,", "tokens": ["Kaum", "gr\u00fc\u00b7\u00dfen", "sie", "das", "sich\u00b7re", "