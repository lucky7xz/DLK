{"dta.poem.10375": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Aus meinen Thr\u00e4nen sprie\u00dfen", "tokens": ["Aus", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel bl\u00fchende Blumen hervor,", "tokens": ["Viel", "bl\u00fc\u00b7hen\u00b7de", "Blu\u00b7men", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und meine Seufzer werden", "tokens": ["Und", "mei\u00b7ne", "Seuf\u00b7zer", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Nachtigallenchor.", "tokens": ["Ein", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7chor", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn du mich lieb hast, Kindchen,", "tokens": ["Und", "wenn", "du", "mich", "lieb", "hast", ",", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VAFIN", "$,", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schenk' ich dir die Blumen all',", "tokens": ["Schenk'", "ich", "dir", "die", "Blu\u00b7men", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor deinem Fenster soll klingen", "tokens": ["Und", "vor", "dei\u00b7nem", "Fens\u00b7ter", "soll", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Lied der Nachtigall.", "tokens": ["Das", "Lied", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}