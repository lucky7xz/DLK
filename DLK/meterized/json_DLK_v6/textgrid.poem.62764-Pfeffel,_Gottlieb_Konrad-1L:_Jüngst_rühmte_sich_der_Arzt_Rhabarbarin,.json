{"textgrid.poem.62764": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst r\u00fchmte sich der Arzt Rhabarbarin,", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst r\u00fchmte sich der Arzt Rhabarbarin,", "tokens": ["J\u00fcngst", "r\u00fchm\u00b7te", "sich", "der", "Arzt", "Rha\u00b7bar\u00b7ba\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sey durch ihn von Gicht und Pest genesen.", "tokens": ["Ich", "sey", "durch", "ihn", "von", "Gicht", "und", "Pest", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Probe, da\u00df er nie mein Arzt gewesen,", "tokens": ["Die", "Pro\u00b7be", ",", "da\u00df", "er", "nie", "mein", "Arzt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist, weil ich noch am Leben bin.", "tokens": ["Ist", ",", "weil", "ich", "noch", "am", "Le\u00b7ben", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}