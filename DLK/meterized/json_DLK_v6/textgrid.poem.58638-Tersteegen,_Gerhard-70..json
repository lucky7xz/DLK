{"textgrid.poem.58638": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr ist ein rechter Gott, ein lebendiger Gott, ein ewiger K\u00f6nig. Jerem. 10, 10", "tokens": ["Der", "Herr", "ist", "ein", "rech\u00b7ter", "Gott", ",", "ein", "le\u00b7ben\u00b7di\u00b7ger", "Gott", ",", "ein", "e\u00b7wi\u00b7ger", "K\u00f6\u00b7nig", ".", "Je\u00b7rem", ".", "10", ",", "10"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+--+-+--+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Gott ist wahrhaftig nur, die Welt hat Schein und L\u00fcgen;", "tokens": ["Gott", "ist", "wahr\u00b7haf\u00b7tig", "nur", ",", "die", "Welt", "hat", "Schein", "und", "L\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "$,", "ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, trau auf Gottes Treu, er wird dich nie betr\u00fcgen!", "tokens": ["Ach", ",", "trau", "auf", "Got\u00b7tes", "Treu", ",", "er", "wird", "dich", "nie", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "NN", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur Gott lebendig ist, dein Geld und Gut ist tot;", "tokens": ["Nur", "Gott", "le\u00b7ben\u00b7dig", "ist", ",", "dein", "Geld", "und", "Gut", "ist", "tot", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lebt Gott, so wird er dir auch helfen aus der Not.", "tokens": ["Lebt", "Gott", ",", "so", "wird", "er", "dir", "auch", "hel\u00b7fen", "aus", "der", "Not", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was sichtbar ist, vergeht, nur Gott wird ewig bleiben,", "tokens": ["Was", "sicht\u00b7bar", "ist", ",", "ver\u00b7geht", ",", "nur", "Gott", "wird", "e\u00b7wig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "$,", "ADV", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir, ew'ger K\u00f6nig, will ich ewig mich verschreiben;", "tokens": ["Dir", ",", "ew'\u00b7ger", "K\u00f6\u00b7nig", ",", "will", "ich", "e\u00b7wig", "mich", "ver\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach, nimm mein Herz nur hin und mach es dir zum Thron,", "tokens": ["Ach", ",", "nimm", "mein", "Herz", "nur", "hin", "und", "mach", "es", "dir", "zum", "Thron", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPOSAT", "NN", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vertreib die Feinde gar und ewig drinnen wohn!", "tokens": ["Ver\u00b7treib", "die", "Fein\u00b7de", "gar", "und", "e\u00b7wig", "drin\u00b7nen", "wohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}