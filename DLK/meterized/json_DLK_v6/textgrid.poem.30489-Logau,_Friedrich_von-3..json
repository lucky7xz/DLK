{"textgrid.poem.30489": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Au\u00df Deutschland zeucht der Krieg ietzund in Franckreich hin;", "tokens": ["Au\u00df", "Deutschland", "zeucht", "der", "Krieg", "ie\u00b7tzund", "in", "Fran\u00b7ck\u00b7reich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er wil das deutsche Volck dort auff die mode ziehn;", "tokens": ["Er", "wil", "das", "deut\u00b7sche", "Volck", "dort", "auff", "die", "mo\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sollen nicht die Deutschen wie sonst daf\u00fcr spendiren;", "tokens": ["Doch", "sol\u00b7len", "nicht", "die", "Deut\u00b7schen", "wie", "sonst", "da\u00b7f\u00fcr", "spen\u00b7di\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "KOKOM", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Deutschen solln von ihnen den Sack gev\u00f6llet f\u00fchren.", "tokens": ["Die", "Deut\u00b7schen", "solln", "von", "ih\u00b7nen", "den", "Sack", "ge\u00b7v\u00f6l\u00b7let", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}