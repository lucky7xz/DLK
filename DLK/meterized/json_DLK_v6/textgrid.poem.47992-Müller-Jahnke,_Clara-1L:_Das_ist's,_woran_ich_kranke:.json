{"textgrid.poem.47992": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist's, woran ich kranke:", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist's, woran ich kranke:", "tokens": ["Das", "ist's", ",", "wo\u00b7ran", "ich", "kran\u00b7ke", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie fest auch Sinn und Rat \u2013", "tokens": ["Wie", "fest", "auch", "Sinn", "und", "Rat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ach, stets ist der Gedanke", "tokens": ["ach", ",", "stets", "ist", "der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["XY", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "mir gr\u00f6\u00dfer als die Tat! \u2013", "tokens": ["mir", "gr\u00f6\u00b7\u00dfer", "als", "die", "Tat", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mag ich in Tr\u00e4umen schauen", "tokens": ["Mag", "ich", "in", "Tr\u00e4u\u00b7men", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mir S\u00f6ller und Prunkgemach,", "tokens": ["mir", "S\u00f6l\u00b7ler", "und", "Prunk\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "das goldene Schlo\u00df zu bauen,", "tokens": ["das", "gol\u00b7de\u00b7ne", "Schlo\u00df", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ist meine Hand zu schwach.", "tokens": ["ist", "mei\u00b7ne", "Hand", "zu", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und was mit Lenzgewalten", "tokens": ["Und", "was", "mit", "Lenz\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "durch meine Seele zieht,", "tokens": ["durch", "mei\u00b7ne", "See\u00b7le", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "zu halten und zu gestalten,", "tokens": ["zu", "hal\u00b7ten", "und", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "versagt mir Wort und Lied . . . ...", "tokens": ["ver\u00b7sagt", "mir", "Wort", "und", "Lied", ".", ".", ".", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$.", "$.", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}