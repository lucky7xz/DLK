{"textgrid.poem.31570": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Qvelle mir / mein Himmel-Nectar / unersch\u00f6pffter Wei\u00dfheits-Brunn!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Qvelle mir / mein Himmel-Nectar / unersch\u00f6pffter Wei\u00dfheits-Brunn!", "tokens": ["Qvel\u00b7le", "mir", "/", "mein", "Him\u00b7mel\u00b7\u00b7Nec\u00b7tar", "/", "un\u00b7er\u00b7sch\u00f6pff\u00b7ter", "Wei\u00df\u00b7heits\u00b7Brunn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPOSAT", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "wann ich nur ein Tr\u00f6pflein k\u00f6nd von des Geistes Einflu\u00df sp\u00fchren /", "tokens": ["wann", "ich", "nur", "ein", "Tr\u00f6pf\u00b7lein", "k\u00f6nd", "von", "des", "Geis\u00b7tes", "Ein\u00b7flu\u00df", "sp\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "wann die Herzen Geister sich m\u00f6chten durch sein Regen r\u00fchren /", "tokens": ["wann", "die", "Her\u00b7zen", "Geis\u00b7ter", "sich", "m\u00f6ch\u00b7ten", "durch", "sein", "Re\u00b7gen", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PRF", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "wann ich nur noch einmal h\u00e4tte solch' ein viel begl\u00fccktes Nun!", "tokens": ["wann", "ich", "nur", "noch", "ein\u00b7mal", "h\u00e4t\u00b7te", "solch'", "ein", "viel", "be\u00b7gl\u00fcck\u00b7tes", "Nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VAFIN", "PIAT", "ART", "PIAT", "ADJA", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ach da\u00df mir das sch\u00f6ne Bild also eilend doch entrunn!", "tokens": ["Ach", "da\u00df", "mir", "das", "sch\u00f6\u00b7ne", "Bild", "al\u00b7so", "ei\u00b7lend", "doch", "ent\u00b7runn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "KON", "NE", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "wann wird mich dein Gnaden-Trieb mehr in diesen Lust-Wald f\u00fchren /", "tokens": ["wann", "wird", "mich", "dein", "Gna\u00b7den\u00b7Trieb", "mehr", "in", "die\u00b7sen", "Lust\u00b7Wald", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "wo die Himmlisch Nachtigal lieblich pflegt zu tireliren /", "tokens": ["wo", "die", "Himm\u00b7lisch", "Nach\u00b7ti\u00b7gal", "lieb\u00b7lich", "pflegt", "zu", "ti\u00b7re\u00b7li\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "wo der Wei\u00dfheit-Safft so s\u00fcsse schnell und hell vor\u00fcber runn /", "tokens": ["wo", "der", "Wei\u00df\u00b7heit\u00b7\u00b7S\u00b7afft", "so", "s\u00fcs\u00b7se", "schnell", "und", "hell", "vor\u00b7\u00fc\u00b7ber", "runn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$("], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "da\u00df ich auf ein Hoffnungs- Gras mich fein sanfft k\u00f6nt niderlassen /", "tokens": ["da\u00df", "ich", "auf", "ein", "Hoff\u00b7nungs", "Gras", "mich", "fein", "sanfft", "k\u00f6nt", "ni\u00b7der\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "TRUNC", "NN", "PPER", "ADJD", "VVFIN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "macht von Trost- und Freuden-Blumen einen Wunder bunten Kranz /", "tokens": ["macht", "von", "Trost", "und", "Freu\u00b7den\u00b7Blu\u00b7men", "ei\u00b7nen", "Wun\u00b7der", "bun\u00b7ten", "Kranz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "TRUNC", "KON", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "sch\u00f6pffte aus dem klaren Brunn k\u00fchlen Safft und Krafft dermassen /", "tokens": ["sch\u00f6pff\u00b7te", "aus", "dem", "kla\u00b7ren", "Brunn", "k\u00fch\u00b7len", "Safft", "und", "Krafft", "der\u00b7mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "PDAT", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "da\u00df von s\u00fcssen Lieblichkeiten ich in Lust verzucket ganz", "tokens": ["da\u00df", "von", "s\u00fcs\u00b7sen", "Lieb\u00b7lich\u00b7kei\u00b7ten", "ich", "in", "Lust", "ver\u00b7zu\u00b7cket", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "APPR", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "schl\u00fc\u00df die Sorgen-Augen zu / und entschlieff in vollen Freuden!", "tokens": ["schl\u00fc\u00df", "die", "Sor\u00b7gen\u00b7Au\u00b7gen", "zu", "/", "und", "ent\u00b7schlieff", "in", "vol\u00b7len", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "wollst so dein verlohrnes Schaf / Edler Sch\u00f6pffer Sch\u00e4fer weiden!", "tokens": ["wollst", "so", "dein", "ver\u00b7lohr\u00b7nes", "Schaf", "/", "Ed\u00b7ler", "Sch\u00f6pf\u00b7fer", "Sch\u00e4\u00b7fer", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$(", "ADJA", "NN", "NE", "VVINF", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}}}}}