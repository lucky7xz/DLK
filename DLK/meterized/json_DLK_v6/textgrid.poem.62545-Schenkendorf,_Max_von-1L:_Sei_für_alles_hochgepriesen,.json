{"textgrid.poem.62545": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei f\u00fcr alles hochgepriesen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei f\u00fcr alles hochgepriesen,", "tokens": ["Sei", "f\u00fcr", "al\u00b7les", "hoch\u00b7ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du Gutes mir erwiesen,", "tokens": ["Was", "du", "Gu\u00b7tes", "mir", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute, und von Anbeginn!", "tokens": ["Heu\u00b7te", ",", "und", "von", "An\u00b7be\u00b7ginn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur aus Gnaden, nicht mit Rechte", "tokens": ["Nur", "aus", "Gna\u00b7den", ",", "nicht", "mit", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gabst du mir, dem tr\u00e4gen Knechte,", "tokens": ["Gabst", "du", "mir", ",", "dem", "tr\u00e4\u00b7gen", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Viel, de\u00df ich nicht w\u00fcrdig bin.", "tokens": ["Viel", ",", "de\u00df", "ich", "nicht", "w\u00fcr\u00b7dig", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wie mancher mag nicht haben,", "tokens": ["Ach", ",", "wie", "man\u00b7cher", "mag", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PIS", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen d\u00fcrren Gaum zu laben,", "tokens": ["Sei\u00b7nen", "d\u00fcr\u00b7ren", "Gaum", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen matten Leib zu ruhn!", "tokens": ["Sei\u00b7nen", "mat\u00b7ten", "Leib", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wer bin denn ich auf Erden,", "tokens": ["Und", "wer", "bin", "denn", "ich", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "KON", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df du nie willst m\u00fcde werden,", "tokens": ["Da\u00df", "du", "nie", "willst", "m\u00fc\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So viel Gutes mir zu thun?", "tokens": ["So", "viel", "Gu\u00b7tes", "mir", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}