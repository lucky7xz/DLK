{"textgrid.poem.61762": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo der Mosel dunkle Wellen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo der Mosel dunkle Wellen", "tokens": ["Wo", "der", "Mo\u00b7sel", "dunk\u00b7le", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ihr felsig Ufer schwellen,", "tokens": ["Um", "ihr", "fel\u00b7sig", "U\u00b7fer", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigt zum drittenmal die Schlacht,", "tokens": ["Schweigt", "zum", "drit\u00b7ten\u00b7mal", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die feuchten Winde tragen", "tokens": ["Und", "die", "feuch\u00b7ten", "Win\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobgesang und Totenklagen", "tokens": ["Lob\u00b7ge\u00b7sang", "und", "To\u00b7ten\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fernverhallend durch die Nacht.", "tokens": ["Fern\u00b7ver\u00b7hal\u00b7lend", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unsre Siegesbanner wogen,", "tokens": ["Uns\u00b7re", "Sie\u00b7ges\u00b7ban\u00b7ner", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Bahn, die sie durchflogen,", "tokens": ["Doch", "die", "Bahn", ",", "die", "sie", "durch\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ist von teurem Blute rot;", "tokens": ["Ist", "von", "teu\u00b7rem", "Blu\u00b7te", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Eisenregen spr\u00fchte,", "tokens": ["Wo", "der", "Ei\u00b7sen\u00b7re\u00b7gen", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sank in Garben, ach, die Bl\u00fcte", "tokens": ["Sank", "in", "Gar\u00b7ben", ",", "ach", ",", "die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "ITJ", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsrer Jugend in den Tod.", "tokens": ["Uns\u00b7rer", "Ju\u00b7gend", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie viel verwaiste Herzen", "tokens": ["O", "wie", "viel", "ver\u00b7wais\u00b7te", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nennen euch hinfort mit Schmerzen,", "tokens": ["Nen\u00b7nen", "euch", "hin\u00b7fort", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mars-la-Tour und Gravelotte!", "tokens": ["Mar\u00b7sla\u00b7Tour", "und", "Gra\u00b7ve\u00b7lot\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiche Fraun, zum Tod bek\u00fcmmert,", "tokens": ["Blei\u00b7che", "Fraun", ",", "zum", "Tod", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Br\u00e4ute, deren Gl\u00fcck zertr\u00fcmmert,", "tokens": ["Br\u00e4u\u00b7te", ",", "de\u00b7ren", "Gl\u00fcck", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Greise M\u00fctter, tr\u00f6st' euch Gott!", "tokens": ["Grei\u00b7se", "M\u00fct\u00b7ter", ",", "tr\u00f6st'", "euch", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber euch, ihr treuen Toten,", "tokens": ["A\u00b7ber", "euch", ",", "ihr", "treu\u00b7en", "To\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei der Br\u00fcder Schwur entboten,", "tokens": ["Sei", "der", "Br\u00fc\u00b7der", "Schwur", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zorn'ge Tr\u00e4nen rinnen drein:", "tokens": ["Zorn'\u00b7ge", "Tr\u00e4\u00b7nen", "rin\u00b7nen", "drein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer soll, das ihr vergossen,", "tokens": ["Nim\u00b7mer", "soll", ",", "das", "ihr", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Euer Blut umsonst geflossen,", "tokens": ["Eu\u00b7er", "Blut", "um\u00b7sonst", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer soll's vergessen sein!", "tokens": ["Nim\u00b7mer", "soll's", "ver\u00b7ges\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eures heil'gen Willens Erben", "tokens": ["Eu\u00b7res", "heil'\u00b7gen", "Wil\u00b7lens", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schw\u00f6ren wir auf Sieg und Sterben", "tokens": ["Schw\u00f6\u00b7ren", "wir", "auf", "Sieg", "und", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu zu stehn in Wacht und Schlacht:", "tokens": ["Treu", "zu", "stehn", "in", "Wacht", "und", "Schlacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner soll der Rast gedenken", "tokens": ["Kei\u00b7ner", "soll", "der", "Rast", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch das Schwert zur Scheide senken,", "tokens": ["Noch", "das", "Schwert", "zur", "Schei\u00b7de", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis das gro\u00dfe Werk vollbracht;", "tokens": ["Bis", "das", "gro\u00b7\u00dfe", "Werk", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bis des Erbfeinds Trutz vernichtet,", "tokens": ["Bis", "des", "Erb\u00b7feinds", "Trutz", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis das Bollwerk aufgerichtet,", "tokens": ["Bis", "das", "Boll\u00b7werk", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Zukunft schirmt der Welt,", "tokens": ["Das", "die", "Zu\u00b7kunft", "schirmt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit rauschendem Gefieder", "tokens": ["Und", "mit", "rau\u00b7schen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber euren Gr\u00e4bern wieder", "tokens": ["\u00dc\u00b7ber", "eu\u00b7ren", "Gr\u00e4\u00b7bern", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutschlands Aar die Grenzwacht h\u00e4lt.", "tokens": ["Deutschlands", "Aar", "die", "Grenz\u00b7wacht", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}