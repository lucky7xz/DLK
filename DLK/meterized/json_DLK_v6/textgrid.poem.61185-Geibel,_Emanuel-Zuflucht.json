{"textgrid.poem.61185": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Zuflucht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit Tau und Sonnenschein ern\u00e4hrst die Lilien auf dem Feld,", "tokens": ["Der", "du", "mit", "Tau", "und", "Son\u00b7nen\u00b7schein", "er\u00b7n\u00e4hrst", "die", "Li\u00b7li\u00b7en", "auf", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Der du der jungen Raben nicht vergissest unterm Himmelszelt,", "tokens": ["Der", "du", "der", "jun\u00b7gen", "Ra\u00b7ben", "nicht", "ver\u00b7gis\u00b7sest", "un\u00b7term", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Der du zu Wasserb\u00e4chen f\u00fchrst den Hirsch, der durstig auf den Tod,", "tokens": ["Der", "du", "zu", "Was\u00b7ser\u00b7b\u00e4\u00b7chen", "f\u00fchrst", "den", "Hirsch", ",", "der", "durs\u00b7tig", "auf", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "O gib, du Allbarmherziger, auch unsrer Zeit, was ihr so not!", "tokens": ["O", "gib", ",", "du", "All\u00b7barm\u00b7her\u00b7zi\u00b7ger", ",", "auch", "uns\u00b7rer", "Zeit", ",", "was", "ihr", "so", "not", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "$,", "PPER", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "PWS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Um Frieden, Frieden flehen wir, nicht jenen, der des Sturms entbehrt,", "tokens": ["Um", "Frie\u00b7den", ",", "Frie\u00b7den", "fle\u00b7hen", "wir", ",", "nicht", "je\u00b7nen", ",", "der", "des", "Sturms", "ent\u00b7behrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der sicher in der Scheide Haft gefesselt h\u00e4lt das scharfe Schwert,", "tokens": ["Der", "si\u00b7cher", "in", "der", "Schei\u00b7de", "Haft", "ge\u00b7fes\u00b7selt", "h\u00e4lt", "das", "schar\u00b7fe", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN", "VVPP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Nein, um den Frieden in der Brust, den's mitten in der Schlacht nicht graut,", "tokens": ["Nein", ",", "um", "den", "Frie\u00b7den", "in", "der", "Brust", ",", "den's", "mit\u00b7ten", "in", "der", "Schlacht", "nicht", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Weil auf den Felsen deines Worts mit festen Pfeilern er gebaut.", "tokens": ["Weil", "auf", "den", "Fel\u00b7sen", "dei\u00b7nes", "Worts", "mit", "fes\u00b7ten", "Pfei\u00b7lern", "er", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Gib uns die Hoffnung, Herr, zu dir, die nie zuschanden werden l\u00e4\u00dft,", "tokens": ["Gib", "uns", "die", "Hoff\u00b7nung", ",", "Herr", ",", "zu", "dir", ",", "die", "nie", "zu\u00b7schan\u00b7den", "wer\u00b7den", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "NN", "$,", "APPR", "PPER", "$,", "PRELS", "ADV", "VVPP", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Gib uns die Liebe, die im Tod und \u00fcberm Tode noch h\u00e4lt fest,", "tokens": ["Gib", "uns", "die", "Lie\u00b7be", ",", "die", "im", "Tod", "und", "\u00fc\u00b7berm", "To\u00b7de", "noch", "h\u00e4lt", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "KON", "APPRART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Gib uns den Glauben l\u00f6wenstark, den Glauben, der die Welt bezwingt", "tokens": ["Gib", "uns", "den", "Glau\u00b7ben", "l\u00f6\u00b7wen\u00b7stark", ",", "den", "Glau\u00b7ben", ",", "der", "die", "Welt", "be\u00b7zwingt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und auf dem Scheiterhaufen noch dir helle Jubelpsalmen singt.", "tokens": ["Und", "auf", "dem", "Schei\u00b7ter\u00b7hau\u00b7fen", "noch", "dir", "hel\u00b7le", "Ju\u00b7be\u00b7lpsal\u00b7men", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wohl sind wir s\u00fcndig, arm und schwach und nimmer solcher Gnaden wert,", "tokens": ["Wohl", "sind", "wir", "s\u00fcn\u00b7dig", ",", "arm", "und", "schwach", "und", "nim\u00b7mer", "sol\u00b7cher", "Gna\u00b7den", "wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Doch du erbarmst dich, wo ein Herz voll Angst und Sehnsucht dein begehrt;", "tokens": ["Doch", "du", "er\u00b7barmst", "dich", ",", "wo", "ein", "Herz", "voll", "Angst", "und", "Sehn\u00b7sucht", "dein", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADJD", "NN", "KON", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "So h\u00f6r' uns denn gleich Israel, da er dich ringend hielt umfa\u00dft:", "tokens": ["So", "h\u00f6r'", "uns", "denn", "gleich", "Is\u00b7rael", ",", "da", "er", "dich", "rin\u00b7gend", "hielt", "um\u00b7fa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NE", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "\u00bbich lass' dich nicht, ich lass' dich nicht, Herr, bis du mich gesegnet hast.\u00ab", "tokens": ["\u00bb", "ich", "lass'", "dich", "nicht", ",", "ich", "lass'", "dich", "nicht", ",", "Herr", ",", "bis", "du", "mich", "ge\u00b7seg\u00b7net", "hast", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Nein! Du verst\u00f6\u00dfest nimmermehr den, der da fl\u00fcchtet in dein Haus,", "tokens": ["Nein", "!", "Du", "ver\u00b7st\u00f6\u00b7\u00dfest", "nim\u00b7mer\u00b7mehr", "den", ",", "der", "da", "fl\u00fcch\u00b7tet", "in", "dein", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "ART", "$,", "PRELS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zerbrichst nicht das zerknickte Rohr und l\u00f6schst den matten Docht nicht aus,", "tokens": ["Zer\u00b7brichst", "nicht", "das", "zer\u00b7knick\u00b7te", "Rohr", "und", "l\u00f6schst", "den", "mat\u00b7ten", "Docht", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die Arme tust du auf und sprichst auch zu den Herzen unsrer Zeit:", "tokens": ["Die", "Ar\u00b7me", "tust", "du", "auf", "und", "sprichst", "auch", "zu", "den", "Her\u00b7zen", "uns\u00b7rer", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "\u00bbkommt her zu mir, die ihr im Geist m\u00fchselig und beladen seid.\u00ab", "tokens": ["\u00bb", "kommt", "her", "zu", "mir", ",", "die", "ihr", "im", "Geist", "m\u00fch\u00b7se\u00b7lig", "und", "be\u00b7la\u00b7den", "seid", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "ADJD", "KON", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "So kommt denn all', in deren Ohr die hohe Freudenbotschaft klang,", "tokens": ["So", "kommt", "denn", "all'", ",", "in", "de\u00b7ren", "Ohr", "die", "ho\u00b7he", "Freu\u00b7den\u00b7bot\u00b7schaft", "klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$,", "APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die einst den Hirten auf dem Feld der Chor der Engelstimmen sang;", "tokens": ["Die", "einst", "den", "Hir\u00b7ten", "auf", "dem", "Feld", "der", "Chor", "der", "En\u00b7gel\u00b7stim\u00b7men", "sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Kommt! S\u00fc\u00dfer Frieden ist in ihm und Licht, das keinem Dunkel weicht,", "tokens": ["Kommt", "!", "S\u00fc\u00b7\u00dfer", "Frie\u00b7den", "ist", "in", "ihm", "und", "Licht", ",", "das", "kei\u00b7nem", "Dun\u00b7kel", "weicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "NN", "VAFIN", "APPR", "PPER", "KON", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Das Leben ist er, und sein Joch ist sanft, und seine Last ist leicht.", "tokens": ["Das", "Le\u00b7ben", "ist", "er", ",", "und", "sein", "Joch", "ist", "sanft", ",", "und", "sei\u00b7ne", "Last", "ist", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}