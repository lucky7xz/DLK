{"textgrid.poem.55684": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch Deinem Mund ist scheu und dumpf entglitten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Deinem Mund ist scheu und dumpf entglitten,", "tokens": ["Auch", "Dei\u00b7nem", "Mund", "ist", "scheu", "und", "dumpf", "ent\u00b7glit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Jeder, Jeder schmerzlich noch erkannt,", "tokens": ["Was", "Je\u00b7der", ",", "Je\u00b7der", "schmerz\u00b7lich", "noch", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$,", "PIS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nachzudenken nur die Mu\u00dfe fand:", "tokens": ["Der", "nach\u00b7zu\u00b7den\u00b7ken", "nur", "die", "Mu\u00b7\u00dfe", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben wird ertragen und gelitten.", "tokens": ["Das", "Le\u00b7ben", "wird", "er\u00b7tra\u00b7gen", "und", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast es einst voll Leidenschaft bestritten,", "tokens": ["Du", "hast", "es", "einst", "voll", "Lei\u00b7den\u00b7schaft", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast Lug und Trug, Kom\u00f6dienspiel genannt", "tokens": ["Hast", "Lug", "und", "Trug", ",", "Ko\u00b7m\u00f6\u00b7di\u00b7en\u00b7spiel", "ge\u00b7nannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NE", "KON", "NN", "$,", "NN", "VVPP"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das Heimweh nach der Toten stillem Land;", "tokens": ["Das", "Heim\u00b7weh", "nach", "der", "To\u00b7ten", "stil\u00b7lem", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Heut' aber brauch' ich leise nur zu bitten,", "tokens": ["Heut'", "a\u00b7ber", "brauch'", "ich", "lei\u00b7se", "nur", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wir verreisen traulich Hand in Hand.", "tokens": ["Und", "wir", "ver\u00b7rei\u00b7sen", "trau\u00b7lich", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}