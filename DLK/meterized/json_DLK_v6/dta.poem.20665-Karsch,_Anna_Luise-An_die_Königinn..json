{"dta.poem.20665": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die K\u00f6niginn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O K\u00f6niginn! an allen Enden                 ", "tokens": ["O", "K\u00f6\u00b7ni\u00b7ginn", "!", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde wirst Du nun die Gr\u00f6\u00dfeste genannt,", "tokens": ["Der", "Er\u00b7de", "wirst", "Du", "nun", "die", "Gr\u00f6\u00b7\u00dfes\u00b7te", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So weit sich Ph\u00f6bus kann auf goldnem Wagen wenden,", "tokens": ["So", "weit", "sich", "Ph\u00f6\u00b7bus", "kann", "auf", "gold\u00b7nem", "Wa\u00b7gen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "NE", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weit ist ", "tokens": ["So", "weit", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Der Wilde schnitzt Sein Bild aus Holze,", "tokens": ["Der", "Wil\u00b7de", "schnitzt", "Sein", "Bild", "aus", "Hol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Cocosbaum, und nennet ihn den ersten Sohn", "tokens": ["Vom", "Co\u00b7cos\u00b7baum", ",", "und", "nen\u00b7net", "ihn", "den", "ers\u00b7ten", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Jupiters; und zeigt mit einem edlen Stolze,", "tokens": ["Des", "Ju\u00b7pi\u00b7ters", ";", "und", "zeigt", "mit", "ei\u00b7nem", "ed\u00b7len", "Stol\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Altar seiner Nation.", "tokens": ["Den", "Al\u00b7tar", "sei\u00b7ner", "Na\u00b7tion", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da tanzen Hand in Hand geschlungen", "tokens": ["Da", "tan\u00b7zen", "Hand", "in", "Hand", "ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Muschelschaalen und mit Perlen ausgeschm\u00fcckt", "tokens": ["Mit", "Mu\u00b7schel\u00b7schaa\u00b7len", "und", "mit", "Per\u00b7len", "aus\u00b7ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um ihren neuen Gott die Greise mit den Jungen;", "tokens": ["Um", "ih\u00b7ren", "neu\u00b7en", "Gott", "die", "Grei\u00b7se", "mit", "den", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind bis zur Raserey entz\u00fcckt;", "tokens": ["Sind", "bis", "zur", "Ra\u00b7se\u00b7rey", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und werfen ihre Blumeng\u00fcrte", "tokens": ["Und", "wer\u00b7fen", "ih\u00b7re", "Blu\u00b7men\u00b7g\u00fcr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ihm, und singen: \u201eda\u00df er L\u00f6w und Hyder schlug,", "tokens": ["Nach", "ihm", ",", "und", "sin\u00b7gen", ":", "\u201e", "da\u00df", "er", "L\u00f6w", "und", "Hy\u00b7der", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "$.", "$(", "KOUS", "PPER", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u201eund da\u00df im Kriege selbst sein gl\u00fccklich Volk die Myrthe", "tokens": ["\u201e", "und", "da\u00df", "im", "Krie\u00b7ge", "selbst", "sein", "gl\u00fcck\u00b7lich", "Volk", "die", "Myr\u00b7the"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "KOUS", "APPRART", "NN", "ADV", "PPOSAT", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ebeym Gastmahl um die Schl\u00e4fe trug.\u201e", "tokens": ["\u201e", "beym", "Gast\u00b7mahl", "um", "die", "Schl\u00e4\u00b7fe", "trug", ".", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So feyren ihn durch ihre T\u00e4nze", "tokens": ["So", "fey\u00b7ren", "ihn", "durch", "ih\u00b7re", "T\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Inseln und der Strand des weiten Oceans;", "tokens": ["Die", "In\u00b7seln", "und", "der", "Strand", "des", "wei\u00b7ten", "O\u00b7ceans", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Indianer bringt die Beute vieler Lenze,", "tokens": ["Der", "In\u00b7di\u00b7a\u00b7ner", "bringt", "die", "Beu\u00b7te", "vie\u00b7ler", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Opferdufte dieses Pans,", "tokens": ["Zum", "Op\u00b7fer\u00b7duf\u00b7te", "die\u00b7ses", "Pans", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ruft ihn an, eh er den Bogen", "tokens": ["Und", "ruft", "ihn", "an", ",", "eh", "er", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und volle K\u00f6cher nimmt: \u201everleihe mir den Sieg!", "tokens": ["Und", "vol\u00b7le", "K\u00f6\u00b7cher", "nimmt", ":", "\u201e", "ver\u00b7lei\u00b7he", "mir", "den", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eo du, vor welchem stets das Schrecken hergezogen,", "tokens": ["\u201e", "o", "du", ",", "vor", "wel\u00b7chem", "stets", "das", "Schre\u00b7cken", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "$,", "APPR", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edein Gl\u00fcck zieh mit mir in den Krieg.\u201e", "tokens": ["\u201e", "dein", "Gl\u00fcck", "zieh", "mit", "mir", "in", "den", "Krieg", ".", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So betet, mit der Stirn zur Erde,", "tokens": ["So", "be\u00b7tet", ",", "mit", "der", "Stirn", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der wilde Canibal von ", "tokens": ["Der", "wil\u00b7de", "Ca\u00b7ni\u00b7bal", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR"], "meter": "-+-----", "measure": "dactylic.init"}, "line.3": {"text": "Und w\u00fcnscht sich Fl\u00fcgel, w\u00fcnscht sich Titans Purpur-", "tokens": ["Und", "w\u00fcnscht", "sich", "Fl\u00fc\u00b7gel", ",", "w\u00fcnscht", "sich", "Ti\u00b7tans", "Pur\u00b7pur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "NN", "$,", "VVFIN", "PRF", "NE", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Pferde,", "tokens": ["Pfer\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Den Mars zu sehn, und den Apoll,", "tokens": ["Den", "Mars", "zu", "sehn", ",", "und", "den", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Von welchen aller V\u00f6lker Zungen", "tokens": ["Von", "wel\u00b7chen", "al\u00b7ler", "V\u00f6l\u00b7ker", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschichterz\u00e4hler sind; der G\u00f6tterlieder h\u00f6rt", "tokens": ["Ge\u00b7schich\u00b7ter\u00b7z\u00e4h\u00b7ler", "sind", ";", "der", "G\u00f6t\u00b7ter\u00b7lie\u00b7der", "h\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In allen Sprachen; und von Dichtern wird besungen,", "tokens": ["In", "al\u00b7len", "Spra\u00b7chen", ";", "und", "von", "Dich\u00b7tern", "wird", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "KON", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis einst der Weltbau wird zerst\u00f6rt.", "tokens": ["Bis", "einst", "der", "Welt\u00b7bau", "wird", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Auch Dich mit Glanz Umgebne! werden", "tokens": ["Auch", "Dich", "mit", "Glanz", "Um\u00b7geb\u00b7ne", "!", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "NN", "$.", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Virgile singen, bis die Felsen nicht mehr stehn,", "tokens": ["Vir\u00b7gi\u00b7le", "sin\u00b7gen", ",", "bis", "die", "Fel\u00b7sen", "nicht", "mehr", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und keine wollenreiche wei\u00dfe L\u00e4mmerheerden", "tokens": ["Und", "kei\u00b7ne", "wol\u00b7len\u00b7rei\u00b7che", "wei\u00b7\u00dfe", "L\u00e4m\u00b7mer\u00b7heer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In blumenvollen Th\u00e4lern gehn.", "tokens": ["In", "blu\u00b7men\u00b7vol\u00b7len", "Th\u00e4\u00b7lern", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du bist, um Deines Herzens wegen,", "tokens": ["Du", "bist", ",", "um", "Dei\u00b7nes", "Her\u00b7zens", "we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesangesw\u00fcrdiger, als Berenice war,", "tokens": ["Ge\u00b7san\u00b7ges\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "als", "Be\u00b7re\u00b7ni\u00b7ce", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die f\u00fcr den Ptolom\u00e4us erbat den Waffensegen,", "tokens": ["Die", "f\u00fcr", "den", "Pto\u00b7lo\u00b7m\u00e4us", "er\u00b7bat", "den", "Waf\u00b7fen\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und angelobt ihr lockig Haar", "tokens": ["Und", "an\u00b7ge\u00b7lobt", "ihr", "lo\u00b7ckig", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dem Gott des Kriegs, und als der ferne", "tokens": ["Dem", "Gott", "des", "Kriegs", ",", "und", "als", "der", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Siegreiche K\u00f6nig kam, den Hauptschmuck dargebracht,", "tokens": ["Sieg\u00b7rei\u00b7che", "K\u00f6\u00b7nig", "kam", ",", "den", "Haupt\u00b7schmuck", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der zum Olymp entr\u00fcckt zu einem neuen Sterne,", "tokens": ["Der", "zum", "O\u00b7lymp", "ent\u00b7r\u00fcckt", "zu", "ei\u00b7nem", "neu\u00b7en", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ihr zur Belohnung ward gemacht.", "tokens": ["Ihr", "zur", "Be\u00b7loh\u00b7nung", "ward", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}