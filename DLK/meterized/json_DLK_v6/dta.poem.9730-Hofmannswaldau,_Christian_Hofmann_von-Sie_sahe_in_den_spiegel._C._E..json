{"dta.poem.9730": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie sahe in den spiegel.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah Auroren sich nach ihrem spiegel drehen/", "tokens": ["Ich", "sah", "Au\u00b7ro\u00b7ren", "sich", "nach", "ih\u00b7rem", "spie\u00b7gel", "dre\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fand in selbigem ihr antlitz wundersch\u00f6n/", "tokens": ["Und", "fand", "in", "sel\u00b7bi\u00b7gem", "ihr", "ant\u00b7litz", "wun\u00b7der\u00b7sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aurora! rieff ich drauff/ mustu nicht selbst gestehn/", "tokens": ["Au\u00b7ro\u00b7ra", "!", "rieff", "ich", "drauff", "/", "mus\u00b7tu", "nicht", "selbst", "ge\u00b7stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PAV", "$(", "VMFIN", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man noch nie ein gla\u00df hat sch\u00f6ner spiegeln sehen?", "tokens": ["Da\u00df", "man", "noch", "nie", "ein", "gla\u00df", "hat", "sch\u00f6\u00b7ner", "spie\u00b7geln", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "NN", "VAFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}