{"textgrid.poem.26797": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Worte nenn ich euch, inhaltschwer,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Worte nenn ich euch, inhaltschwer,", "tokens": ["Drei", "Wor\u00b7te", "nenn", "ich", "euch", ",", "in\u00b7halt\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie gehen von Munde zu Munde,", "tokens": ["Sie", "ge\u00b7hen", "von", "Mun\u00b7de", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch stammen sie nicht von au\u00dfen her,", "tokens": ["Doch", "stam\u00b7men", "sie", "nicht", "von", "au\u00b7\u00dfen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Herz nur gibt davon Kunde.", "tokens": ["Das", "Herz", "nur", "gibt", "da\u00b7von", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PAV", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Menschen ist aller Wert geraubt,", "tokens": ["Dem", "Men\u00b7schen", "ist", "al\u00b7ler", "Wert", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn er nicht mehr an die drei Worte glaubt.", "tokens": ["Wenn", "er", "nicht", "mehr", "an", "die", "drei", "Wor\u00b7te", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Mensch ist frei geschaffen, ist frei,", "tokens": ["Der", "Mensch", "ist", "frei", "ge\u00b7schaf\u00b7fen", ",", "ist", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und w\u00fcrd er in Ketten geboren,", "tokens": ["Und", "w\u00fcrd", "er", "in", "Ket\u00b7ten", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "La\u00dft euch nicht irren des P\u00f6bels Geschrei,", "tokens": ["La\u00dft", "euch", "nicht", "ir\u00b7ren", "des", "P\u00f6\u00b7bels", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Nicht den Mi\u00dfbrauch rasender Toren.", "tokens": ["Nicht", "den", "Mi\u00df\u00b7brauch", "ra\u00b7sen\u00b7der", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Vor dem Sklaven, wenn er die Kette bricht,", "tokens": ["Vor", "dem", "Skla\u00b7ven", ",", "wenn", "er", "die", "Ket\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Vor dem freien Menschen erzittert nicht.", "tokens": ["Vor", "dem", "frei\u00b7en", "Men\u00b7schen", "er\u00b7zit\u00b7tert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und die Tugend, sie ist kein leerer Schall,", "tokens": ["Und", "die", "Tu\u00b7gend", ",", "sie", "ist", "kein", "lee\u00b7rer", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Mensch kann sie \u00fcben im Leben,", "tokens": ["Der", "Mensch", "kann", "sie", "\u00fc\u00b7ben", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und sollt er auch straucheln \u00fcberall,", "tokens": ["Und", "sollt", "er", "auch", "strau\u00b7cheln", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er kann nach der g\u00f6ttlichen streben,", "tokens": ["Er", "kann", "nach", "der", "g\u00f6tt\u00b7li\u00b7chen", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und was kein Verstand der Verst\u00e4ndigen sieht,", "tokens": ["Und", "was", "kein", "Ver\u00b7stand", "der", "Ver\u00b7st\u00e4n\u00b7di\u00b7gen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das \u00fcbet in Einfalt ein kindlich Gem\u00fct.", "tokens": ["Das", "\u00fc\u00b7bet", "in", "Ein\u00b7falt", "ein", "kind\u00b7lich", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und ein Gott ist, ein heiliger Wille lebt,", "tokens": ["Und", "ein", "Gott", "ist", ",", "ein", "hei\u00b7li\u00b7ger", "Wil\u00b7le", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wie auch der menschliche wanke,", "tokens": ["Wie", "auch", "der", "menschli\u00b7che", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber der Zeit und dem Raume webt", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Zeit", "und", "dem", "Rau\u00b7me", "webt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lebendig der h\u00f6chste Gedanke,", "tokens": ["Le\u00b7ben\u00b7dig", "der", "h\u00f6chs\u00b7te", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Und ob alles in ewigem Wechsel kreist,", "tokens": ["Und", "ob", "al\u00b7les", "in", "e\u00b7wi\u00b7gem", "Wech\u00b7sel", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Es beharret im Wechsel ein ruhiger Geist.", "tokens": ["Es", "be\u00b7har\u00b7ret", "im", "Wech\u00b7sel", "ein", "ru\u00b7hi\u00b7ger", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Die drei Worte bewahret euch, inhaltschwer,", "tokens": ["Die", "drei", "Wor\u00b7te", "be\u00b7wah\u00b7ret", "euch", ",", "in\u00b7halt\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sie pflanzet von Munde zu Munde,", "tokens": ["Sie", "pflan\u00b7zet", "von", "Mun\u00b7de", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und stammen sie gleich nicht von au\u00dfen her,", "tokens": ["Und", "stam\u00b7men", "sie", "gleich", "nicht", "von", "au\u00b7\u00dfen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Euer Innres gibt davon Kunde,", "tokens": ["Eu\u00b7er", "Inn\u00b7res", "gibt", "da\u00b7von", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dem Menschen ist nimmer sein Wert geraubt,", "tokens": ["Dem", "Men\u00b7schen", "ist", "nim\u00b7mer", "sein", "Wert", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Solang er noch an die drei Worte glaubt.", "tokens": ["So\u00b7lang", "er", "noch", "an", "die", "drei", "Wor\u00b7te", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}