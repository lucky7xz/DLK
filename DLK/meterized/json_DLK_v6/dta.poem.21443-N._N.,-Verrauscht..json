{"dta.poem.21443": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Verrauscht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Sturmwolken mir zu H\u00e4upten zieh\u2019n,", "tokens": ["Sturm\u00b7wol\u00b7ken", "mir", "zu", "H\u00e4up\u00b7ten", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verweht der V\u00f6gel Melodien", "tokens": ["Ver\u00b7weht", "der", "V\u00f6\u00b7gel", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach S\u00fcdens Zauberlande;", "tokens": ["Nach", "S\u00fc\u00b7dens", "Zau\u00b7berl\u00b7an\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur einige Blumen einsam bl\u00fch\u2019n,", "tokens": ["Nur", "ei\u00b7ni\u00b7ge", "Blu\u00b7men", "ein\u00b7sam", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im Sonnenstrahl sie nicht ergl\u00fch\u2019n,", "tokens": ["Im", "Son\u00b7nen\u00b7strahl", "sie", "nicht", "er\u00b7gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun welken sie im Sande.", "tokens": ["Nun", "wel\u00b7ken", "sie", "im", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wirr braut der Nebel auf dem Flu\u00df,", "tokens": ["Wirr", "braut", "der", "Ne\u00b7bel", "auf", "dem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Verrauscht ist l\u00e4ngst der Liebe Ku\u00df,", "tokens": ["Ver\u00b7rauscht", "ist", "l\u00e4ngst", "der", "Lie\u00b7be", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie schwand der Lenz so balde!", "tokens": ["Wie", "schwand", "der", "Lenz", "so", "bal\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Rauh breitet nun sein Leichentuch", "tokens": ["Rauh", "brei\u00b7tet", "nun", "sein", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Winter. Und ein d\u00fcst\u2019rer Fluch", "tokens": ["Der", "Win\u00b7ter", ".", "Und", "ein", "d\u00fcst'\u00b7rer", "Fluch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Legt sich auf Flur und Halde.", "tokens": ["Legt", "sich", "auf", "Flur", "und", "Hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}