{"textgrid.poem.65094": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, du hast Recht! Es mu\u00df das Gl\u00fcck zerfallen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, du hast Recht! Es mu\u00df das Gl\u00fcck zerfallen,", "tokens": ["Ja", ",", "du", "hast", "Recht", "!", "Es", "mu\u00df", "das", "Gl\u00fcck", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll sich des Geistes k\u00fchner Bau erheben,", "tokens": ["Soll", "sich", "des", "Geis\u00b7tes", "k\u00fch\u00b7ner", "Bau", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kaufpreis f\u00fcr Unsterblichkeit hei\u00dft ", "tokens": ["Der", "Kauf\u00b7preis", "f\u00fcr", "U\u00b7nsterb\u00b7lich\u00b7keit", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Gr\u00e4bern ruh'n der Tempel stolze Hallen!", "tokens": ["Auf", "Gr\u00e4\u00b7bern", "ruh'n", "der", "Tem\u00b7pel", "stol\u00b7ze", "Hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Lorbeer wird nur jene Stirn' umwallen,", "tokens": ["Der", "Lor\u00b7beer", "wird", "nur", "je\u00b7ne", "Stirn'", "um\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich des Blumenkranzes hat begeben,", "tokens": ["Die", "sich", "des", "Blu\u00b7men\u00b7kran\u00b7zes", "hat", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach keinem Erdengl\u00fccke darf mehr streben", "tokens": ["Nach", "kei\u00b7nem", "Er\u00b7den\u00b7gl\u00fc\u00b7cke", "darf", "mehr", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer sich dem Ruhm geschworen zum Vasallen.", "tokens": ["Wer", "sich", "dem", "Ruhm", "ge\u00b7schwo\u00b7ren", "zum", "Va\u00b7sal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbich dulde keine G\u00f6tter neben mir!\u00ab", "tokens": ["\u00bb", "ich", "dul\u00b7de", "kei\u00b7ne", "G\u00f6t\u00b7ter", "ne\u00b7ben", "mir", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So spricht der Gott, und willst du treu ihm dienen,", "tokens": ["So", "spricht", "der", "Gott", ",", "und", "willst", "du", "treu", "ihm", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirf rasch hinweg der G\u00f6tzendiener Zier.", "tokens": ["Wirf", "rasch", "hin\u00b7weg", "der", "G\u00f6t\u00b7zen\u00b7die\u00b7ner", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und schreite starken Muth's aus den Ruinen", "tokens": ["Und", "schrei\u00b7te", "star\u00b7ken", "Muth's", "aus", "den", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des eingest\u00fcrzten Gl\u00fcckes, welche dir", "tokens": ["Des", "ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Gl\u00fc\u00b7ckes", ",", "wel\u00b7che", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kerker waren und Palast nur ", "tokens": ["Ein", "Ker\u00b7ker", "wa\u00b7ren", "und", "Pa\u00b7last", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}