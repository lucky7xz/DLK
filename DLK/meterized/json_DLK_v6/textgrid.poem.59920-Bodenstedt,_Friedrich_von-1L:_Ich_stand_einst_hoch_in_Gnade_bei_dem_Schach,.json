{"textgrid.poem.59920": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand einst hoch in Gnade bei dem Schach,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand einst hoch in Gnade bei dem Schach,", "tokens": ["Ich", "stand", "einst", "hoch", "in", "Gna\u00b7de", "bei", "dem", "Schach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der oftmals bitter sich bei mir beklagte,", "tokens": ["Der", "oft\u00b7mals", "bit\u00b7ter", "sich", "bei", "mir", "be\u00b7klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ihm kein Mensch so recht die Wahrheit sagte.", "tokens": ["Da\u00df", "ihm", "kein", "Mensch", "so", "recht", "die", "Wahr\u00b7heit", "sag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich dachte ob dem Sinn der Worte nach", "tokens": ["Ich", "dach\u00b7te", "ob", "dem", "Sinn", "der", "Wor\u00b7te", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fand, da\u00df er mit gutem Grunde klagte;", "tokens": ["Und", "fand", ",", "da\u00df", "er", "mit", "gu\u00b7tem", "Grun\u00b7de", "klag\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch als ich ihm so recht die Wahrheit sagte,", "tokens": ["Doch", "als", "ich", "ihm", "so", "recht", "die", "Wahr\u00b7heit", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verbannte mich von seinem Hof der Schach.", "tokens": ["Ver\u00b7bann\u00b7te", "mich", "von", "sei\u00b7nem", "Hof", "der", "Schach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl gibt es F\u00fcrsten,", "tokens": ["Wohl", "gibt", "es", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die nach Wahrheit d\u00fcrsten,", "tokens": ["Die", "nach", "Wahr\u00b7heit", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch wenigen ward ein so gesunder Magen,", "tokens": ["Doch", "we\u00b7ni\u00b7gen", "ward", "ein", "so", "ge\u00b7sun\u00b7der", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie zu vertragen.", "tokens": ["Sie", "zu", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}