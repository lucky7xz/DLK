{"textgrid.poem.67051": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hero", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange schlummern ruhig all die Meinen,", "tokens": ["Lan\u00b7ge", "schlum\u00b7mern", "ru\u00b7hig", "all", "die", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stille atmet durch die Mitternacht;", "tokens": ["Stil\u00b7le", "at\u00b7met", "durch", "die", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf dann! Hero! auf und la\u00df das Weinen!", "tokens": ["Auf", "dann", "!", "He\u00b7ro", "!", "auf", "und", "la\u00df", "das", "Wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NE", "$.", "PTKVZ", "KON", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dank euch, G\u00f6tter! Heros Mut erwacht.", "tokens": ["Dank", "euch", ",", "G\u00f6t\u00b7ter", "!", "He\u00b7ros", "Mut", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "NE", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fort ans Meer! ans Meer! es sch\u00e4ume die Welle,", "tokens": ["Fort", "ans", "Meer", "!", "ans", "Meer", "!", "es", "sch\u00e4u\u00b7me", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Brause der Sturm mir immer ins Angesicht!", "tokens": ["Brau\u00b7se", "der", "Sturm", "mir", "im\u00b7mer", "ins", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Fort ans Meer! ohn ihn ist alles H\u00f6lle \u2013", "tokens": ["Fort", "ans", "Meer", "!", "ohn", "ihn", "ist", "al\u00b7les", "H\u00f6l\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Liebe \u00e4ngstet mich Arme \u2013 Sturm und Welle nicht.", "tokens": ["Lie\u00b7be", "\u00e4ngs\u00b7tet", "mich", "Ar\u00b7me", "\u2013", "Sturm", "und", "Wel\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ruhig will ich da hin\u00fcberlauschen,", "tokens": ["Ru\u00b7hig", "will", "ich", "da", "hin\u00b7\u00fc\u00b7berl\u00b7au\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo sein H\u00fcttchen \u00fcber Felsen h\u00e4ngt,", "tokens": ["Wo", "sein", "H\u00fctt\u00b7chen", "\u00fc\u00b7ber", "Fel\u00b7sen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rufen will ichs in der Woge Rauschen,", "tokens": ["Ru\u00b7fen", "will", "ichs", "in", "der", "Wo\u00b7ge", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sein Zaudern seine Hero kr\u00e4nkt.", "tokens": ["Wie", "sein", "Zau\u00b7dern", "sei\u00b7ne", "He\u00b7ro", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ha! da wird er sich mutig von seinem Gestade", "tokens": ["Ha", "!", "da", "wird", "er", "sich", "mu\u00b7tig", "von", "sei\u00b7nem", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "St\u00fcrzen, Posidaons Kraft ihm Liebe verleihn,", "tokens": ["St\u00fcr\u00b7zen", ",", "Po\u00b7sid\u00b7aons", "Kraft", "ihm", "Lie\u00b7be", "ver\u00b7leihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Lieb ihn leiten des Meeres furchtbare Pfade,", "tokens": ["Lieb", "ihn", "lei\u00b7ten", "des", "Mee\u00b7res", "furcht\u00b7ba\u00b7re", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "G\u00f6tter! wie wird \u2013 wie wird uns wieder sein?", "tokens": ["G\u00f6t\u00b7ter", "!", "wie", "wird", "\u2013", "wie", "wird", "uns", "wie\u00b7der", "sein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}