{"textgrid.poem.50662": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Leutseligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verachte Niemand nicht, obgleich du gr\u00f6\u00dfer bist,", "tokens": ["Ver\u00b7ach\u00b7te", "Nie\u00b7mand", "nicht", ",", "ob\u00b7gleich", "du", "gr\u00f6\u00b7\u00dfer", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit sich Niemand nicht vor deinem Anspruch scheu';", "tokens": ["Da\u00b7mit", "sich", "Nie\u00b7mand", "nicht", "vor", "dei\u00b7nem", "An\u00b7spruch", "scheu'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lieb' Einen, ", "tokens": ["Lieb'", "Ei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ART", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den Andern, ", "tokens": ["Den", "An\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}