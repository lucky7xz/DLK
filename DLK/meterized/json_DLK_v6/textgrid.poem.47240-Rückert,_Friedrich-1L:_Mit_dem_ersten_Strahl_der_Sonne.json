{"textgrid.poem.47240": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit dem ersten Strahl der Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem ersten Strahl der Sonne", "tokens": ["Mit", "dem", "ers\u00b7ten", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du weislich aufgestanden,", "tokens": ["Bist", "du", "weis\u00b7lich", "auf\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von deines Tages Wonne", "tokens": ["Da\u00df", "von", "dei\u00b7nes", "Ta\u00b7ges", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir kein Teilchen komm' abhanden,", "tokens": ["Dir", "kein", "Teil\u00b7chen", "komm'", "ab\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fl\u00fcchtigste vom Stamm der Fliegen,", "tokens": ["Fl\u00fcch\u00b7tigs\u00b7te", "vom", "Stamm", "der", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichtbeschwingtes Eintagskind!", "tokens": ["Leicht\u00b7be\u00b7schwing\u00b7tes", "Ein\u00b7tags\u00b7kind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Morgens Duft gestiegen", "tokens": ["Aus", "des", "Mor\u00b7gens", "Duft", "ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verweht vom Abendwind.", "tokens": ["Und", "ver\u00b7weht", "vom", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil bestimmt zu deinem Leben", "tokens": ["Weil", "be\u00b7stimmt", "zu", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Geschick ein Tag dir war,", "tokens": ["Vom", "Ge\u00b7schick", "ein", "Tag", "dir", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat es milde dir gegeben", "tokens": ["Hat", "es", "mil\u00b7de", "dir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen l\u00e4ngsten Tag im Jahr.", "tokens": ["Die\u00b7sen", "l\u00e4ngs\u00b7ten", "Tag", "im", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei der Tag dir still und helle,", "tokens": ["Sei", "der", "Tag", "dir", "still", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKVZ", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil du keinen zweiten hast;", "tokens": ["Weil", "du", "kei\u00b7nen", "zwei\u00b7ten", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unversiegt des Taues Quelle,", "tokens": ["Un\u00b7ver\u00b7siegt", "des", "Tau\u00b7es", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wind und Sonne nicht zur Last!", "tokens": ["Wind", "und", "Son\u00b7ne", "nicht", "zur", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Keine Schwalb' im Flug dich hasche!", "tokens": ["Kei\u00b7ne", "Schwalb'", "im", "Flug", "dich", "ha\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stelle dir kein Netz die Spinne!", "tokens": ["Stel\u00b7le", "dir", "kein", "Netz", "die", "Spin\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh, im Duft der Bl\u00fcten nasche", "tokens": ["Geh", ",", "im", "Duft", "der", "Bl\u00fc\u00b7ten", "na\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Abend drein zerrinne!", "tokens": ["Und", "am", "A\u00b7bend", "drein", "zer\u00b7rin\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}