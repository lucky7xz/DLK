{"textgrid.poem.62848": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Theanor sa\u00df am Eingang seiner Zelle,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Theanor sa\u00df am Eingang seiner Zelle,", "tokens": ["Thea\u00b7nor", "sa\u00df", "am", "Ein\u00b7gang", "sei\u00b7ner", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die vor Jerusalem auf einem Berge stand,", "tokens": ["Die", "vor", "Je\u00b7ru\u00b7sa\u00b7lem", "auf", "ei\u00b7nem", "Ber\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und sah den greisen Tag im lichten Sterbgewand", "tokens": ["Und", "sah", "den", "grei\u00b7sen", "Tag", "im", "lich\u00b7ten", "Sterb\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Abendroths, wie er die graue Schwelle", "tokens": ["Des", "A\u00b7ben\u00b7droths", ",", "wie", "er", "die", "grau\u00b7e", "Schwel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Horizonts mit feyerlichem Schritt", "tokens": ["Des", "Ho\u00b7ri\u00b7zonts", "mit", "fey\u00b7er\u00b7li\u00b7chem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am Arm der Zeit hinunter glitt.", "tokens": ["Am", "Arm", "der", "Zeit", "hin\u00b7un\u00b7ter", "glitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So gleitet nach dem stillen Meere", "tokens": ["So", "glei\u00b7tet", "nach", "dem", "stil\u00b7len", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ewigkeit des Menschen Leben hin.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "des", "Men\u00b7schen", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mein Lenz entfloh, mein Winter wird entfliehn;", "tokens": ["Mein", "Lenz", "ent\u00b7floh", ",", "mein", "Win\u00b7ter", "wird", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O! wenn ich nur schon durchgedrungen w\u00e4re", "tokens": ["O", "!", "wenn", "ich", "nur", "schon", "durch\u00b7ge\u00b7drun\u00b7gen", "w\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Durch deine Halle, schauervolles Grab!", "tokens": ["Durch", "dei\u00b7ne", "Hal\u00b7le", ",", "schau\u00b7er\u00b7vol\u00b7les", "Grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So seufzet er und eine bittre Z\u00e4hre", "tokens": ["So", "seuf\u00b7zet", "er", "und", "ei\u00b7ne", "bitt\u00b7re", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Rollt \u00fcber sein Gesicht herab.", "tokens": ["Rollt", "\u00fc\u00b7ber", "sein", "Ge\u00b7sicht", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sie rollte noch, als, gleich dem Strahle", "tokens": ["Sie", "roll\u00b7te", "noch", ",", "als", ",", "gleich", "dem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Des matten Blitzes, ihn ein Silberstrom", "tokens": ["Des", "mat\u00b7ten", "Blit\u00b7zes", ",", "ihn", "ein", "Sil\u00b7ber\u00b7strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Umflo\u00df, und eine Stimm aus dem saphirnen Dohm", "tokens": ["Um\u00b7flo\u00df", ",", "und", "ei\u00b7ne", "Stimm", "aus", "dem", "sa\u00b7phir\u00b7nen", "Dohm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihm zurief: blicke nach dem Thale!", "tokens": ["Ihm", "zu\u00b7rief", ":", "bli\u00b7cke", "nach", "dem", "Tha\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Er blickt ins Thal; auf einem dunkeln Pfad", "tokens": ["Er", "blickt", "ins", "Thal", ";", "auf", "ei\u00b7nem", "dun\u00b7keln", "Pfad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Schlich ein geb\u00fcckter Greis getrost an seinem Stecken,", "tokens": ["Schlich", "ein", "ge\u00b7b\u00fcck\u00b7ter", "Greis", "ge\u00b7trost", "an", "sei\u00b7nem", "Ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als pl\u00f6zlich der Monarch der Schrecken", "tokens": ["Als", "pl\u00f6z\u00b7lich", "der", "Mon\u00b7arch", "der", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Aus einer Wolke vor ihn trat.", "tokens": ["Aus", "ei\u00b7ner", "Wol\u00b7ke", "vor", "ihn", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Greis erkennet ihn: Sey dreymal mir gegr\u00fc\u00dfet,", "tokens": ["Der", "Greis", "er\u00b7ken\u00b7net", "ihn", ":", "Sey", "drey\u00b7mal", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du Bote der Unsterblichkeit!", "tokens": ["Du", "Bo\u00b7te", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "So redet er ihn an und neiget sich, und k\u00fcsset", "tokens": ["So", "re\u00b7det", "er", "ihn", "an", "und", "nei\u00b7get", "sich", ",", "und", "k\u00fcs\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "KON", "VVFIN", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den schwarzen Saum an seinem Kleid.", "tokens": ["Den", "schwar\u00b7zen", "Saum", "an", "sei\u00b7nem", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie, sprach der alte Sohn der S\u00fcnde:", "tokens": ["Wie", ",", "sprach", "der", "al\u00b7te", "Sohn", "der", "S\u00fcn\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Erschrickst du nicht vor dem, der jedem Adamskinde", "tokens": ["Er\u00b7schrickst", "du", "nicht", "vor", "dem", ",", "der", "je\u00b7dem", "A\u00b7dams\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So furchtbar ist? ... \u00bbNur den erschreckt dein Bild,", "tokens": ["So", "furcht\u00b7bar", "ist", "?", "...", "\u00bb", "Nur", "den", "er\u00b7schreckt", "dein", "Bild", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$.", "$(", "$(", "ADV", "ART", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der vor sich selbst erschrickt.\u00ab So schaudre vor den Seuchen,", "tokens": ["Der", "vor", "sich", "selbst", "er\u00b7schrickt", ".", "\u00ab", "So", "schaud\u00b7re", "vor", "den", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVPP", "$.", "$(", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die vor mir her im Finstern schleichen", "tokens": ["Die", "vor", "mir", "her", "im", "Fins\u00b7tern", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und vor dem kalten Schwei\u00df, der mir vom Fittig quillt!", "tokens": ["Und", "vor", "dem", "kal\u00b7ten", "Schwei\u00df", ",", "der", "mir", "vom", "Fit\u00b7tig", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u00bbich schaudre nicht.\u00ab \u2013 Warum nicht? \u2013 \u00bbFreund, ich werde", "tokens": ["\u00bb", "ich", "schaud\u00b7re", "nicht", ".", "\u00ab", "\u2013", "Wa\u00b7rum", "nicht", "?", "\u2013", "\u00bb", "Freund", ",", "ich", "wer\u00b7de"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$.", "$(", "$(", "PWAV", "PTKNEG", "$.", "$(", "$(", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Durch sie gewahr, da\u00df du mir nahe bist.\u00ab \u2013", "tokens": ["Durch", "sie", "ge\u00b7wahr", ",", "da\u00df", "du", "mir", "na\u00b7he", "bist", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wer bist du denn, du Mensch von Erde,", "tokens": ["Wer", "bist", "du", "denn", ",", "du", "Mensch", "von", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Der Freund mich nennt? \u2013 \u00bbIch bin ein Christ.\u00ab", "tokens": ["Der", "Freund", "mich", "nennt", "?", "\u2013", "\u00bb", "Ich", "bin", "ein", "Christ", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "$(", "$(", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Schnell hauchet ihn der ernste Seraph an", "tokens": ["Schnell", "hau\u00b7chet", "ihn", "der", "erns\u00b7te", "Se\u00b7raph", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und Tod und Sterblicher verschwanden. \u2013", "tokens": ["Und", "Tod", "und", "Sterb\u00b7li\u00b7cher", "ver\u00b7schwan\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Ein unterirdischer Orkan", "tokens": ["Ein", "un\u00b7ter\u00b7ir\u00b7di\u00b7scher", "Or\u00b7kan"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Er\u00f6ffnet an dem Ort, wo sie gestanden,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "an", "dem", "Ort", ",", "wo", "sie", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Mit einem dumpfen Donnerschlag", "tokens": ["Mit", "ei\u00b7nem", "dum\u00b7pfen", "Don\u00b7ner\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ein tiefes Grab. Theanor bebte.", "tokens": ["Ein", "tie\u00b7fes", "Grab", ".", "Thea\u00b7nor", "beb\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Er sah da\u00df etwas in der H\u00f6le lag,", "tokens": ["Er", "sah", "da\u00df", "et\u00b7was", "in", "der", "H\u00f6\u00b7le", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Darauf ein dunkler Nebel schwebte.", "tokens": ["Da\u00b7rauf", "ein", "dunk\u00b7ler", "Ne\u00b7bel", "schweb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Doch pl\u00f6tzlich hallt in sein bet\u00e4ubtes Ohr", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "hallt", "in", "sein", "be\u00b7t\u00e4ub\u00b7tes", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Ein leiser Harfenklang von des Olympus K\u00fcsten:", "tokens": ["Ein", "lei\u00b7ser", "Har\u00b7fen\u00b7klang", "von", "des", "O\u00b7lym\u00b7pus", "K\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Er blickt hinauf und sieht den Christen", "tokens": ["Er", "blickt", "hin\u00b7auf", "und", "sieht", "den", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Umringt von einem hehren Chor", "tokens": ["Um\u00b7ringt", "von", "ei\u00b7nem", "heh\u00b7ren", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Verkl\u00e4rter Geister, das mit s\u00fc\u00dfen Psalmen", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ter", "Geis\u00b7ter", ",", "d