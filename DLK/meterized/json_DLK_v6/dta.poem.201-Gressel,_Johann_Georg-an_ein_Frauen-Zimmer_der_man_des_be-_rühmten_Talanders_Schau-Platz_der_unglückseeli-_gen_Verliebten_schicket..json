{"dta.poem.201": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "an ein Frauen-Zimmer/ der man des be-  \n r\u00fchmten  Talanders  Schau-Platz der ungl\u00fcckseeli-  \n gen Verliebten schicket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schicksahl ist so schlimm als derer wohl gewesen/", "tokens": ["Mein", "Schick\u00b7sahl", "ist", "so", "schlimm", "als", "de\u00b7rer", "wohl", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "PDS", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hier die witzge Schrifft von einer klugen Hand", "tokens": ["So", "hier", "die", "witz\u00b7ge", "Schrifft", "von", "ei\u00b7ner", "klu\u00b7gen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In netter Zierlichkeit den Teutschen macht bekandt/", "tokens": ["In", "net\u00b7ter", "Zier\u00b7lich\u00b7keit", "den", "Teut\u00b7schen", "macht", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Franckreichs Ebentheur in ihrem Buch l\u00e4st lesen/", "tokens": ["Die", "Fran\u00b7ck\u00b7reichs", "E\u00b7bent\u00b7heur", "in", "ih\u00b7rem", "Buch", "l\u00e4st", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie zeiget/ da\u00df der Neid stets bey der Liebe-Tresen", "tokens": ["Sie", "zei\u00b7get", "/", "da\u00df", "der", "Neid", "stets", "bey", "der", "Lie\u00b7be\u00b7Tre\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den K\u00e4uffern giebt Allaun vor s\u00fcsses Zuckerkand/", "tokens": ["Den", "K\u00e4uf\u00b7fern", "giebt", "Al\u00b7laun", "vor", "s\u00fcs\u00b7ses", "Zu\u00b7cker\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der offt das rohte Hertz in Spaden hat verwand/", "tokens": ["Der", "offt", "das", "roh\u00b7te", "Hertz", "in", "Spa\u00b7den", "hat", "ver\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denns Gl\u00fccke geht in Bley/ das Ungl\u00fcck f\u00e4hrt in ", "tokens": ["Denns", "Gl\u00fc\u00b7cke", "geht", "in", "Bley", "/", "das", "Un\u00b7gl\u00fcck", "f\u00e4hrt", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Lernt nun ", "tokens": ["Lernt", "nun"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "So kan ich nicht der Schrifft dar\u00fcber b\u00f6se seyn/", "tokens": ["So", "kan", "ich", "nicht", "der", "Schrifft", "da\u00b7r\u00fc\u00b7ber", "b\u00f6\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "PAV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur auf das Schicksahl werd ich nicht zum besten sprechen/", "tokens": ["Nur", "auf", "das", "Schick\u00b7sahl", "werd", "ich", "nicht", "zum", "bes\u00b7ten", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil es ", "tokens": ["Weil", "es"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Doch tr\u00f6st ich mich damit trotz aller Neider Rasen.", "tokens": ["Doch", "tr\u00f6st", "ich", "mich", "da\u00b7mit", "trotz", "al\u00b7ler", "Nei\u00b7der", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PAV", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df endlich das Gel\u00fcck ins Seegelm\u00fcssen blasen.", "tokens": ["Da\u00df", "end\u00b7lich", "das", "Ge\u00b7l\u00fcck", "ins", "See\u00b7gel\u00b7m\u00fcs\u00b7sen", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}