{"textgrid.poem.45997": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Erde war gestorben]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde war gestorben", "tokens": ["Die", "Er\u00b7de", "war", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lebte ganz allein,", "tokens": ["Ich", "leb\u00b7te", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne war verdorben,", "tokens": ["Die", "Son\u00b7ne", "war", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwei Augen gaben Schein,", "tokens": ["Zwei", "Au\u00b7gen", "ga\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da bot sie mir zu trinken", "tokens": ["Da", "bot", "sie", "mir", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blickte mich nicht an,", "tokens": ["Und", "blick\u00b7te", "mich", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lie\u00df die Augen sinken,", "tokens": ["Sie", "lie\u00df", "die", "Au\u00b7gen", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es war um mich getan.", "tokens": ["Es", "war", "um", "mich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Reg' Fr\u00fchling nur die Schwingen", "tokens": ["Reg'", "Fr\u00fch\u00b7ling", "nur", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehn' nur, du Erde, dich,", "tokens": ["Sehn'", "nur", ",", "du", "Er\u00b7de", ",", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann nichts anders singen,", "tokens": ["Ich", "kann", "nichts", "an\u00b7ders", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als, Jesus schau auf mich.", "tokens": ["Als", ",", "Je\u00b7sus", "schau", "auf", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NE", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}