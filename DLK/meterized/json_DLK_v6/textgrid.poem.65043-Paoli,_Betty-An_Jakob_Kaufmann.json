{"textgrid.poem.65043": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "An Jakob Kaufmann", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Heimath trauter Haft", "tokens": ["In", "der", "Hei\u00b7math", "trau\u00b7ter", "Haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolltest du nicht weilen;", "tokens": ["Woll\u00b7test", "du", "nicht", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "M\u00f6g' auf deiner Wanderschaft", "tokens": ["M\u00f6g'", "auf", "dei\u00b7ner", "Wan\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich mein Gru\u00df ereilen.", "tokens": ["Dich", "mein", "Gru\u00df", "er\u00b7ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schmerzlich, freudig, m\u00f6g' er dich", "tokens": ["Schmerz\u00b7lich", ",", "freu\u00b7dig", ",", "m\u00f6g'", "er", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "VMFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An die Zeit gemahnen,", "tokens": ["An", "die", "Zeit", "ge\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo f\u00fcr fl\u00fccht'ge Stunden sich", "tokens": ["Wo", "f\u00fcr", "fl\u00fccht'\u00b7ge", "Stun\u00b7den", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuzten unsre Bahnen.", "tokens": ["Kreuz\u00b7ten", "uns\u00b7re", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O wie froh lie\u00df ich dein Wort", "tokens": ["O", "wie", "froh", "lie\u00df", "ich", "dein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Geist umranken,", "tokens": ["Mei\u00b7nen", "Geist", "um\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und empfing von dir den Hort", "tokens": ["Und", "emp\u00b7fing", "von", "dir", "den", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ew'ger Lenzgedanken!", "tokens": ["Ew'\u00b7ger", "Lenz\u00b7ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Einen Strom von Poesie", "tokens": ["Ei\u00b7nen", "Strom", "von", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fchlt' ich mich umschwellen,", "tokens": ["F\u00fchlt'", "ich", "mich", "um\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Seele l\u00e4uternd, wie", "tokens": ["Mei\u00b7ne", "See\u00b7le", "l\u00e4u\u00b7ternd", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil'gen Stromes Wellen! \u2013", "tokens": ["Heil'\u00b7gen", "Stro\u00b7mes", "Wel\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bruder! gr\u00fc\u00dfe ich dich leis'", "tokens": ["Bru\u00b7der", "!", "gr\u00fc\u00b7\u00dfe", "ich", "dich", "leis'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in meinem Sange,", "tokens": ["Hier", "in", "mei\u00b7nem", "San\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weil ich keinen Namen wei\u00df,", "tokens": ["Weil", "ich", "kei\u00b7nen", "Na\u00b7men", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der von s\u00fc\u00dfrem Klange.", "tokens": ["Der", "von", "s\u00fc\u00df\u00b7rem", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "O gewi\u00df! dein Herz verkennt", "tokens": ["O", "ge\u00b7wi\u00df", "!", "dein", "Herz", "ver\u00b7kennt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht was meines fl\u00fcstert:", "tokens": ["Nicht", "was", "mei\u00b7nes", "fl\u00fcs\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir, ob f\u00fcr stets getrennt,", "tokens": ["Da\u00df", "wir", ",", "ob", "f\u00fcr", "stets", "ge\u00b7trennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch f\u00fcr stets verschwistert.", "tokens": ["Doch", "f\u00fcr", "stets", "ver\u00b7schwis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}