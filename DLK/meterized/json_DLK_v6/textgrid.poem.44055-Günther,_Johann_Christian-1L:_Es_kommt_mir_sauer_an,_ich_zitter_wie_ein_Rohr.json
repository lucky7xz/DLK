{"textgrid.poem.44055": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kommt mir sauer an, ich zitter wie ein Rohr", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt mir sauer an, ich zitter wie ein Rohr", "tokens": ["Es", "kommt", "mir", "sau\u00b7er", "an", ",", "ich", "zit\u00b7ter", "wie", "ein", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bin mehr Schein als Mensch. Allein wer kan davor?", "tokens": ["Und", "bin", "mehr", "Schein", "als", "Mensch", ".", "Al\u00b7lein", "wer", "kan", "da\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "KOUS", "NN", "$.", "ADV", "PWS", "VMFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du, Herr, verstellst dich mir und magst dich auch verstellen.", "tokens": ["Du", ",", "Herr", ",", "ver\u00b7stellst", "dich", "mir", "und", "magst", "dich", "auch", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ehre deinen Grimm bey noch so schweren F\u00e4llen;", "tokens": ["Ich", "eh\u00b7re", "dei\u00b7nen", "Grimm", "bey", "noch", "so", "schwe\u00b7ren", "F\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NE", "APPR", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vermehr auch Schlag und Zorn, du strafst mich nie zu hart,", "tokens": ["Ver\u00b7mehr", "auch", "Schlag", "und", "Zorn", ",", "du", "strafst", "mich", "nie", "zu", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich seh gleichwohl daraus des Vaters Gegenwart,", "tokens": ["Ich", "seh", "gleich\u00b7wohl", "da\u00b7raus", "des", "Va\u00b7ters", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Vaters, der mich nicht aus blindem Eifer schl\u00e4get,", "tokens": ["Des", "Va\u00b7ters", ",", "der", "mich", "nicht", "aus", "blin\u00b7dem", "Ei\u00b7fer", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nein, sondern nur dadurch zu Reu und Leid beweget.", "tokens": ["Nein", ",", "son\u00b7dern", "nur", "da\u00b7durch", "zu", "Reu", "und", "Leid", "be\u00b7we\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "ADV", "PAV", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer weis, zu was es dient! Ich bitte dich um nichts", "tokens": ["Wer", "weis", ",", "zu", "was", "es", "dient", "!", "Ich", "bit\u00b7te", "dich", "um", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als dann und wann um Trost des holden Angesichts.", "tokens": ["Als", "dann", "und", "wann", "um", "Trost", "des", "hol\u00b7den", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "PWAV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sonst gieb mir, was du wilst, ich bin zu blind zum Wehlen;", "tokens": ["Sonst", "gieb", "mir", ",", "was", "du", "wilst", ",", "ich", "bin", "zu", "blind", "zum", "Weh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du, Vater, wirst mich doch nicht \u00fcber Kr\u00e4fte qu\u00e4len.", "tokens": ["Du", ",", "Va\u00b7ter", ",", "wirst", "mich", "doch", "nicht", "\u00fc\u00b7ber", "Kr\u00e4f\u00b7te", "qu\u00e4\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Spott, Hunger, Fluch und Neid und gar ver\u00e4chtlich seyn", "tokens": ["Spott", ",", "Hun\u00b7ger", ",", "Fluch", "und", "Neid", "und", "gar", "ver\u00b7\u00e4cht\u00b7lich", "seyn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Geht freylich, wer es f\u00fchlt, dem Fleische bitter ein;", "tokens": ["Geht", "frey\u00b7lich", ",", "wer", "es", "f\u00fchlt", ",", "dem", "Flei\u00b7sche", "bit\u00b7ter", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch wenn mich auch dein Zorn bis in die H\u00f6lle triebe,", "tokens": ["Doch", "wenn", "mich", "auch", "dein", "Zorn", "bis", "in", "die", "H\u00f6l\u00b7le", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So predigt ich auch dort die Wollust deiner Liebe.", "tokens": ["So", "pre\u00b7digt", "ich", "auch", "dort", "die", "Wol\u00b7lust", "dei\u00b7ner", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}