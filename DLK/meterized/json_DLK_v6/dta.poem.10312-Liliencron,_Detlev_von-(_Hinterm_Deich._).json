{"dta.poem.10312": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Hinterm Deich. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal rechts und links den Blick geschwind,", "tokens": ["Noch", "ein\u00b7mal", "rechts", "und", "links", "den", "Blick", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann in das kleine K\u00e4thnerhaus hinein.", "tokens": ["Dann", "in", "das", "klei\u00b7ne", "K\u00e4\u00b7th\u00b7ner\u00b7haus", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und vor mir steht ein schlankes, blondes Kind", "tokens": ["Und", "vor", "mir", "steht", "ein", "schlan\u00b7kes", ",", "blon\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Madonnenhaft im Winterabendschein.", "tokens": ["Ma\u00b7don\u00b7nen\u00b7haft", "im", "Win\u00b7ter\u00b7a\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zwei Jahrmarktspudel schaun vom Kleiderspind,", "tokens": ["Zwei", "Jahr\u00b7markt\u00b7spu\u00b7del", "schaun", "vom", "Klei\u00b7der\u00b7spind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und weinen Glas, und sind so h\u00fcbsch und fein.", "tokens": ["Und", "wei\u00b7nen", "Glas", ",", "und", "sind", "so", "h\u00fcbsch", "und", "fein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Purpursonne schickt den Westerwind", "tokens": ["Die", "Pur\u00b7pur\u00b7son\u00b7ne", "schickt", "den", "Wes\u00b7ter\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit letzten Gr\u00fc\u00dfen unserm Stelldichein.", "tokens": ["Mit", "letz\u00b7ten", "Gr\u00fc\u00b7\u00dfen", "un\u00b7serm", "Stell\u00b7dich\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}