{"dta.poem.14202": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Leid und Lust .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und Busen und Wangen", "tokens": ["Und", "Bu\u00b7sen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "D\u00fcrft' Ihr da seh'n. \u2014", "tokens": ["D\u00fcrft'", "Ihr", "da", "seh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich brenn' vor Verlangen,", "tokens": ["Ich", "brenn'", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und mu\u00df hier steh'n!", "tokens": ["Und", "mu\u00df", "hier", "steh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}