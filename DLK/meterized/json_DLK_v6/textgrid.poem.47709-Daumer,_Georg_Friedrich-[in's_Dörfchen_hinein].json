{"textgrid.poem.47709": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in's D\u00f6rfchen hinein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In's D\u00f6rfchen hinein", "tokens": ["In's", "D\u00f6rf\u00b7chen", "hin\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APZR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom Quelle daher", "tokens": ["Vom", "Quel\u00b7le", "da\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hinwallt' ich und trug", "tokens": ["Hin\u00b7wallt'", "ich", "und", "trug"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN"], "meter": "+---+", "measure": "dactylic.init"}, "line.4": {"text": "Die Welle so schwer,", "tokens": ["Die", "Wel\u00b7le", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ging sacht und sinnend", "tokens": ["Ging", "sacht", "und", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Am Stalle hin,", "tokens": ["Am", "Stal\u00b7le", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Aufwieherte helle", "tokens": ["Auf\u00b7wie\u00b7her\u00b7te", "hel\u00b7le"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Der Braune drin.", "tokens": ["Der", "Brau\u00b7ne", "drin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mir d\u00e4uchte zugleich,", "tokens": ["Mir", "d\u00e4uch\u00b7te", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Als h\u00f6ret' ich einen", "tokens": ["Als", "h\u00f6\u00b7ret'", "ich", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "In ringender Qual", "tokens": ["In", "rin\u00b7gen\u00b7der", "Qual"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Aufseufzen und weinen.", "tokens": ["Auf\u00b7seuf\u00b7zen", "und", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und hin den Einer", "tokens": ["Und", "hin", "den", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Zur Erde gesetzt,", "tokens": ["Zur", "Er\u00b7de", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Aufthat ich leise", "tokens": ["Auf\u00b7that", "ich", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die Th\u00fcre jetzt.", "tokens": ["Die", "Th\u00fc\u00b7re", "jetzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Ihn, dem ich hold,", "tokens": ["Ihn", ",", "dem", "ich", "hold", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "In Thr\u00e4nen erblickt' ich,", "tokens": ["In", "Thr\u00e4\u00b7nen", "er\u00b7blickt'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Um seinen Hals", "tokens": ["Um", "sei\u00b7nen", "Hals"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Die Arme strickt' ich.", "tokens": ["Die", "Ar\u00b7me", "strickt'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "\u00bbwas weinst du, mein Lieb,", "tokens": ["\u00bb", "was", "weinst", "du", ",", "mein", "Lieb", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Was st\u00f6hnst du, mein Reiter?", "tokens": ["Was", "st\u00f6hnst", "du", ",", "mein", "Rei\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Was meinst du, mein Trieb,", "tokens": ["Was", "meinst", "du", ",", "mein", "Trieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Er trage mich weiter?", "tokens": ["Er", "tra\u00b7ge", "mich", "wei\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Dein ist, wie es war,", "tokens": ["Dein", "ist", ",", "wie", "es", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "Mein Herze noch immer,", "tokens": ["Mein", "Her\u00b7ze", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Untreue befleckt", "tokens": ["Un\u00b7treu\u00b7e", "be\u00b7fleckt"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "Das redliche nimmer.", "tokens": ["Das", "red\u00b7li\u00b7che", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Wie Fluthen im Quelle,", "tokens": ["Wie", "Flut\u00b7hen", "im", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "So lauter und licht,", "tokens": ["So", "lau\u00b7ter", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "So ist mein Lieben;", "tokens": ["So", "ist", "mein", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "O zweifle nicht!\u00ab", "tokens": ["O", "zweif\u00b7le", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}