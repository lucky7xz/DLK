{"dta.poem.4082": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Halt!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den Erlen heraus,", "tokens": ["Aus", "den", "Er\u00b7len", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durch Rauschen und Singen", "tokens": ["Durch", "Rau\u00b7schen", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bricht R\u00e4dergebraus.", "tokens": ["Bricht", "R\u00e4\u00b7der\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ei willkommen, ei willkommen,", "tokens": ["Ei", "will\u00b7kom\u00b7men", ",", "ei", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NE", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer M\u00fchlengesang!", "tokens": ["S\u00fc\u00b7\u00dfer", "M\u00fch\u00b7len\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und das Haus, wie so traulich!", "tokens": ["Und", "das", "Haus", ",", "wie", "so", "trau\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Fenster, wie blank!", "tokens": ["Und", "die", "Fens\u00b7ter", ",", "wie", "blank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und die Sonne, wie helle", "tokens": ["Und", "die", "Son\u00b7ne", ",", "wie", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vom Himmel sie scheint!", "tokens": ["Vom", "Him\u00b7mel", "sie", "scheint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ei, B\u00e4chlein, liebes B\u00e4chlein,", "tokens": ["Ei", ",", "B\u00e4ch\u00b7lein", ",", "lie\u00b7bes", "B\u00e4ch\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War es also gemeint?", "tokens": ["War", "es", "al\u00b7so", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}