{"textgrid.poem.56552": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00f6llerbur\u00df so flink un keit,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6llerbur\u00df so flink un keit,", "tokens": ["M\u00f6l\u00b7ler\u00b7bur\u00df", "so", "flink", "un", "keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wa he springt un dreiht!", "tokens": ["Wa", "he", "springt", "un", "dreiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "FM", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sin Haar is so plusti,", "tokens": ["Sin", "Haar", "is", "so", "plus\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sin Bart is so dusti,", "tokens": ["Sin", "Bart", "is", "so", "dus\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "B\u0119tn Klister op de Backen,", "tokens": ["B\u0119tn", "Klis\u00b7ter", "op", "de", "Ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Un'n Spitzbov inn Nacken,", "tokens": ["Un'n", "Spitz\u00b7bov", "inn", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Fl\u00fcggt rum mank den Mehlstuff,", "tokens": ["Fl\u00fcggt", "rum", "mank", "den", "Mehl\u00b7stuff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Kridewitt as en Duv.", "tokens": ["Kri\u00b7de\u00b7witt", "as", "en", "Duv", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "S\u00fcnnabnds mit min Achendeel", "tokens": ["S\u00fcn\u00b7nabnds", "mit", "min", "A\u00b7chen\u00b7deel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ik rop na M\u0153l.", "tokens": ["Kam", "ik", "rop", "na", "M\u0153l", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn geit se un klappert,", "tokens": ["Denn", "geit", "se", "un", "klap\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn steit he un plappert:", "tokens": ["Denn", "steit", "he", "un", "plap\u00b7pert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wa is he bepudert!", "tokens": ["Wa", "is", "he", "be\u00b7pu\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wa spa\u00dft he un sludert!", "tokens": ["Wa", "spa\u00dft", "he", "un", "slu\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Un wenn 'k em den Sch\u00fcllnk g\u0119v,", "tokens": ["Un", "wenn", "'k", "em", "den", "Sch\u00fcllnk", "g\u0119v", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wa kikt he verlevt!", "tokens": ["Wa", "kikt", "he", "ver\u00b7levt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Awer keem he mi to neeg,", "tokens": ["A\u00b7wer", "keem", "he", "mi", "to", "neeg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sett ik em torech!", "tokens": ["Sett", "ik", "em", "to\u00b7rech", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wa wull ik em pulen!", "tokens": ["Wa", "wull", "ik", "em", "pu\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wa wull ik em ulen!", "tokens": ["Wa", "wull", "ik", "em", "u\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ik klopp em de Jack ut,", "tokens": ["Ik", "klopp", "em", "de", "Jack", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "As st\u00f6v ik en Sack ut:", "tokens": ["As", "st\u00f6v", "ik", "en", "Sack", "ut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sunst kunn' je all L\u00fcd sehn,", "tokens": ["Sunst", "kunn'", "je", "all", "L\u00fcd", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "To M\u0153l weer ik w\u0119n.", "tokens": ["To", "M\u0153l", "weer", "ik", "w\u0119n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}