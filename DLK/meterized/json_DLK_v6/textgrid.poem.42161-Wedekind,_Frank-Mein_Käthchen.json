{"textgrid.poem.42161": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Mein K\u00e4thchen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein K\u00e4thchen fordert zum Lohne", "tokens": ["Mein", "K\u00e4th\u00b7chen", "for\u00b7dert", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von mir ein Liebesgedicht.", "tokens": ["Von", "mir", "ein", "Lie\u00b7bes\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich sage: Mein K\u00e4thchen verschone", "tokens": ["Ich", "sa\u00b7ge", ":", "Mein", "K\u00e4th\u00b7chen", "ver\u00b7scho\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mich damit, ich kann das nicht.", "tokens": ["Mich", "da\u00b7mit", ",", "ich", "kann", "das", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "$,", "PPER", "VMFIN", "PDS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob \u00fcberhaupt ich dich liebe,", "tokens": ["Ob", "\u00fc\u00b7ber\u00b7haupt", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das wei\u00df ich nicht so genau.", "tokens": ["Das", "wei\u00df", "ich", "nicht", "so", "ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zwar sagst du ganz richtig, das bliebe", "tokens": ["Zwar", "sagst", "du", "ganz", "rich\u00b7tig", ",", "das", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PDS", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gleichg\u00fcltig; doch, K\u00e4thchen, schau:", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", ";", "doch", ",", "K\u00e4th\u00b7chen", ",", "schau", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "$,", "NE", "$,", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn ich die Liebe bedichte,", "tokens": ["Wenn", "ich", "die", "Lie\u00b7be", "be\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bedicht ich sie immer vorher,", "tokens": ["Be\u00b7dicht", "ich", "sie", "im\u00b7mer", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Denn wenn vorbei die Geschichte,", "tokens": ["Denn", "wenn", "vor\u00b7bei", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird mir das Dichten zu schwer.", "tokens": ["Wird", "mir", "das", "Dich\u00b7ten", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}