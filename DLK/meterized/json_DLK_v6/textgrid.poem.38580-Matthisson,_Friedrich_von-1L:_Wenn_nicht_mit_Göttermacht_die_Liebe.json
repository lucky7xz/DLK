{"textgrid.poem.38580": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn nicht mit G\u00f6ttermacht die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht mit G\u00f6ttermacht die Liebe", "tokens": ["Wenn", "nicht", "mit", "G\u00f6t\u00b7ter\u00b7macht", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Dunkelheiten unser Herz", "tokens": ["Aus", "Dun\u00b7kel\u00b7hei\u00b7ten", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu lichten Himmelsh\u00f6h'n erh\u00fcbe,", "tokens": ["Zu", "lich\u00b7ten", "Him\u00b7mels\u00b7h\u00f6h'n", "er\u00b7h\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer tr\u00fcge dann des Lebens Schmerz?", "tokens": ["Wer", "tr\u00fc\u00b7ge", "dann", "des", "Le\u00b7bens", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00e4nkt den Geist mit Seligkeiten,", "tokens": ["Sie", "tr\u00e4nkt", "den", "Geist", "mit", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die selbst Petrarka's Lied nicht singt,", "tokens": ["Die", "selbst", "Pe\u00b7trar\u00b7ka's", "Lied", "nicht", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie folgt dem Fluge des Geweihten,", "tokens": ["Sie", "folgt", "dem", "Flu\u00b7ge", "des", "Ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann er dem Staube sich entschwingt!", "tokens": ["Wann", "er", "dem", "Stau\u00b7be", "sich", "ent\u00b7schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und st\u00fcrzt', umdonnert von den Flammen", "tokens": ["Und", "st\u00fcrzt'", ",", "um\u00b7don\u00b7nert", "von", "den", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des schreckenvollen Weltgerichts,", "tokens": ["Des", "schre\u00b7cken\u00b7vol\u00b7len", "Welt\u00b7ge\u00b7richts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erdkreis unter ihr zusammen,", "tokens": ["Der", "Erd\u00b7kreis", "un\u00b7ter", "ihr", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe bleibt und f\u00fcrchtet nichts!", "tokens": ["Die", "Lie\u00b7be", "bleibt", "und", "f\u00fcrch\u00b7tet", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}