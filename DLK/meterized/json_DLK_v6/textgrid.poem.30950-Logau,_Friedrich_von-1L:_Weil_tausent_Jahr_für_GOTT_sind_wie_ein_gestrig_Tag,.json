{"textgrid.poem.30950": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil tausent Jahr f\u00fcr GOTT sind wie ein gestrig Tag,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil tausent Jahr f\u00fcr GOTT sind wie ein gestrig Tag,", "tokens": ["Weil", "tau\u00b7sent", "Jahr", "f\u00fcr", "GoTT", "sind", "wie", "ein", "ge\u00b7strig", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "APPR", "NE", "VAFIN", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So einer hundert Jahr der Welt geniessen mag,", "tokens": ["So", "ei\u00b7ner", "hun\u00b7dert", "Jahr", "der", "Welt", "ge\u00b7nies\u00b7sen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "CARD", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So rechne, wieviel Zeit er lebt f\u00fcr seinem GOTT,", "tokens": ["So", "rech\u00b7ne", ",", "wie\u00b7viel", "Zeit", "er", "lebt", "f\u00fcr", "sei\u00b7nem", "GoTT", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAT", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lebt aber solche Zeit o, in wie vieler Noth!", "tokens": ["Lebt", "a\u00b7ber", "sol\u00b7che", "Zeit", "o", ",", "in", "wie", "vie\u00b7ler", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "FM", "$,", "APPR", "KO