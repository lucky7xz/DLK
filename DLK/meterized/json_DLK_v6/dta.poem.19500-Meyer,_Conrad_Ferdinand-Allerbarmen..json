{"dta.poem.19500": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Allerbarmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "An dem Bauerhaus vor\u00fcber", "tokens": ["An", "dem", "Bau\u00b7er\u00b7haus", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schritt ich eilig, weil mir grauste,", "tokens": ["Schritt", "ich", "ei\u00b7lig", ",", "weil", "mir", "graus\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil im dumpfen Hof ein tr\u00fcber,", "tokens": ["Weil", "im", "dum\u00b7pfen", "Hof", "ein", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fctender Cretine hauste.", "tokens": ["Br\u00fc\u00b7ten\u00b7der", "Cre\u00b7ti\u00b7ne", "haus\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Schaudernd warf ich einen halben", "tokens": ["Schau\u00b7dernd", "warf", "ich", "ei\u00b7nen", "hal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick in seinen feuchten Kerker \u2014", "tokens": ["Blick", "in", "sei\u00b7nen", "feuch\u00b7ten", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eben war die Zeit der Schwalben,", "tokens": ["E\u00b7ben", "war", "die", "Zeit", "der", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie baun an Dach und Erker.", "tokens": ["Wo", "sie", "baun", "an", "Dach", "und", "Er\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Enterbten sah ich kauern,", "tokens": ["Den", "En\u00b7terb\u00b7ten", "sah", "ich", "kau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber seiner Lagerst\u00e4tte", "tokens": ["Ue\u00b7ber", "sei\u00b7ner", "La\u00b7ger\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzten Schwalben um die Mauern,", "tokens": ["Blitz\u00b7ten", "Schwal\u00b7ben", "um", "die", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nester bauend in die Wette.", "tokens": ["Nes\u00b7ter", "bau\u00b7end", "in", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der erloschne Blick erfreute", "tokens": ["Der", "er\u00b7lo\u00b7schne", "Blick", "er\u00b7freu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich, in einem kleinen blauen", "tokens": ["Sich", ",", "in", "ei\u00b7nem", "klei\u00b7nen", "blau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "$,", "APPR", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Raum das Werk der Schwalben heute,", "tokens": ["Raum", "das", "Werk", "der", "Schwal\u00b7ben", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses kluge Werk zu schauen.", "tokens": ["Die\u00b7ses", "klu\u00b7ge", "Werk", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blitzend kreiste das Geschwirre", "tokens": ["Blit\u00b7zend", "kreis\u00b7te", "das", "Ge\u00b7schwir\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem engen Horizonte,", "tokens": ["An", "dem", "en\u00b7gen", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und das Lachen klang, das irre,", "tokens": ["Und", "das", "La\u00b7chen", "klang", ",", "das", "ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin sich doch der Himmel sonnte.", "tokens": ["Drin", "sich", "doch", "der", "Him\u00b7mel", "sonn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}