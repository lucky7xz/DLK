{"textgrid.poem.32859": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du weinest, sch\u00f6ne Caroline,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du weinest, sch\u00f6ne Caroline,", "tokens": ["Du", "wei\u00b7nest", ",", "sch\u00f6\u00b7ne", "Ca\u00b7ro\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich dies V\u00f6gelchen hier in dem Sprenkel qu\u00e4lt?", "tokens": ["Da\u00df", "sich", "dies", "V\u00f6\u00b7gel\u00b7chen", "hier", "in", "dem", "Spren\u00b7kel", "qu\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PDS", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch siehst du l\u00e4ngst an mir die bange d\u00fcstre Mine,", "tokens": ["Doch", "siehst", "du", "l\u00e4ngst", "an", "mir", "die", "ban\u00b7ge", "d\u00fcst\u00b7re", "Mi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und fragst nicht einmal, was mir fehlt?", "tokens": ["Und", "fragst", "nicht", "ein\u00b7mal", ",", "was", "mir", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du w\u00fcnschst die Freyheit ihm zu geben?", "tokens": ["Du", "w\u00fcnschst", "die", "Frey\u00b7heit", "ihm", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan! es flattre hin! sein Gl\u00fcck verdank es dir!", "tokens": ["Wo\u00b7hlan", "!", "es", "flatt\u00b7re", "hin", "!", "sein", "Gl\u00fcck", "ver\u00b7dank", "es", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sieh! vom n\u00e4chsten Ast dankt dir sein Lied das Leben,", "tokens": ["Da", "sieh", "!", "vom", "n\u00e4chs\u00b7ten", "Ast", "dankt", "dir", "sein", "Lied", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein das Meine raubst du mir. \u2013 \u2013", "tokens": ["Al\u00b7lein", "das", "Mei\u00b7ne", "raubst", "du", "mir", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "PPOSAT", "VVFIN", "PPER", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich scheint der Vorwurf zu betr\u00fcben,", "tokens": ["Dich", "scheint", "der", "Vor\u00b7wurf", "zu", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du rufst: \u00bbso grausam! ich? und wie? wenn raub ichs dir?\u00ab", "tokens": ["Du", "rufst", ":", "\u00bb", "so", "grau\u00b7sam", "!", "ich", "?", "und", "wie", "?", "wenn", "raub", "ichs", "dir", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "ADJD", "$.", "PPER", "$.", "KON", "PWAV", "$.", "KOUS", "VVFIN", "PIS", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, Sch\u00f6nste, raubst du mir die Freyheit dich zu lieben,", "tokens": ["Ja", ",", "Sch\u00f6ns\u00b7te", ",", "raubst", "du", "mir", "die", "Frey\u00b7heit", "dich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Raubst du da nicht das Leben mir?", "tokens": ["Raubst", "du", "da", "nicht", "das", "Le\u00b7ben", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}