{"textgrid.poem.30789": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlesien, da\u00df dein Zabothus worden ist f\u00fcr wenig Jahren,", "tokens": ["Schle\u00b7si\u00b7en", ",", "da\u00df", "dein", "Za\u00b7bot\u00b7hus", "wor\u00b7den", "ist", "f\u00fcr", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "VAPP", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Was den Grichen ihr Parnassus, Helicon und Pindus waren,", "tokens": ["Was", "den", "Gri\u00b7chen", "ihr", "Par\u00b7nas\u00b7sus", ",", "He\u00b7li\u00b7con", "und", "Pin\u00b7dus", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPOSAT", "NN", "$,", "NE", "KON", "NE", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df dein Opitz ist Apollo, da\u00df die andren klugen Sinnen", "tokens": ["Da\u00df", "dein", "O\u00b7pitz", "ist", "A\u00b7pol\u00b7lo", ",", "da\u00df", "die", "an\u00b7dren", "klu\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "NE", "$,", "KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Deiner Kinder dieses worden, was sonst sind die Castalinnen,", "tokens": ["Dei\u00b7ner", "Kin\u00b7der", "die\u00b7ses", "wor\u00b7den", ",", "was", "sonst", "sind", "die", "Cas\u00b7ta\u00b7lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "VAPP", "$,", "PRELS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dieses ist dir ewig r\u00fchmlich. Glaube, was die Griechen tichten,", "tokens": ["Die\u00b7ses", "ist", "dir", "e\u00b7wig", "r\u00fchm\u00b7lich", ".", "Glau\u00b7be", ",", "was", "die", "Grie\u00b7chen", "tich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "ADJD", "$.", "NN", "$,", "PRELS", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wer da wil. Uns kan zum Zeugn\u00fc\u00df Ort und Tag es selbsten richten.", "tokens": ["Wer", "da", "wil", ".", "Uns", "kan", "zum", "Zeug\u00b7n\u00fc\u00df", "Ort", "und", "Tag", "es", "selbs\u00b7ten", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "$.", "PPER", "VMFIN", "APPRART", "NN", "NN", "KON", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}}}}}