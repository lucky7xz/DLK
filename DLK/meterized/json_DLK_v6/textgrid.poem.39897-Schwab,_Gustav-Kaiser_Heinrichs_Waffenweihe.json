{"textgrid.poem.39897": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Heinrichs Waffenweihe", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge K\u00f6nig Heinrich schlief", "tokens": ["Der", "jun\u00b7ge", "K\u00f6\u00b7nig", "Hein\u00b7rich", "schlief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Go\u00dflar in der Kammer tief,", "tokens": ["Zu", "Go\u00df\u00b7lar", "in", "der", "Kam\u00b7mer", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlossen waren alle Th\u00fcren,", "tokens": ["Ver\u00b7schlos\u00b7sen", "wa\u00b7ren", "al\u00b7le", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es durfte sich kein Leben r\u00fchren,", "tokens": ["Es", "durf\u00b7te", "sich", "kein", "Le\u00b7ben", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Hall den langen Gang durchlief,", "tokens": ["Kein", "Hall", "den", "lan\u00b7gen", "Gang", "durch\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der junge K\u00f6nig Heinrich schlief.", "tokens": ["Der", "jun\u00b7ge", "K\u00f6\u00b7nig", "Hein\u00b7rich", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wenn der Herr im Himmel spricht,", "tokens": ["Doch", "wenn", "der", "Herr", "im", "Him\u00b7mel", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hilft ein Gebot zu schweigen nicht;", "tokens": ["Hilft", "ein", "Ge\u00b7bot", "zu", "schwei\u00b7gen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Winde durch die Hallen pfeifen,", "tokens": ["Die", "Win\u00b7de", "durch", "die", "Hal\u00b7len", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tropfen an das Fenster streifen,", "tokens": ["Die", "Trop\u00b7fen", "an", "das", "Fens\u00b7ter", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In manchem rauhen Donnerschlag", "tokens": ["In", "man\u00b7chem", "rau\u00b7hen", "Don\u00b7ner\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entlastet sich der hei\u00dfe Tag.", "tokens": ["Ent\u00b7las\u00b7tet", "sich", "der", "hei\u00b7\u00dfe", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Diener schleichen auf den Zeh'n,", "tokens": ["Die", "Die\u00b7ner", "schlei\u00b7chen", "auf", "den", "Zeh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wagen nicht herein zu seh'n:", "tokens": ["Sie", "wa\u00b7gen", "nicht", "her\u00b7ein", "zu", "seh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will er das Wetter \u00fcberh\u00f6ren \u2013", "tokens": ["Will", "er", "das", "Wet\u00b7ter", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht wollen ", "tokens": ["Nicht", "wol\u00b7len"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Bis da\u00df ein Knall das Haus durchdringt,", "tokens": ["Bis", "da\u00df", "ein", "Knall", "das", "Haus", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit Geklirr die Kammer klingt.", "tokens": ["Und", "mit", "Ge\u00b7klirr", "die", "Kam\u00b7mer", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da flieget bei des Herrn Gefahr", "tokens": ["Da", "flie\u00b7get", "bei", "des", "Herrn", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herbei der bleichen Knechte Schar,", "tokens": ["Her\u00b7bei", "der", "blei\u00b7chen", "Knech\u00b7te", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man h\u00f6rt nicht mehr den Regen fallen.", "tokens": ["Man", "h\u00f6rt", "nicht", "mehr", "den", "Re\u00b7gen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rt nicht mehr den Donner hallen,", "tokens": ["Man", "h\u00f6rt", "nicht", "mehr", "den", "Don\u00b7ner", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man h\u00f6ret nur der F\u00fc\u00dfe Tritt", "tokens": ["Man", "h\u00f6\u00b7ret", "nur", "der", "F\u00fc\u00b7\u00dfe", "Tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwerer M\u00e4nner Eisenschritt.", "tokens": ["Und", "schwe\u00b7rer", "M\u00e4n\u00b7ner", "Ei\u00b7sen\u00b7schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie \u00f6ffnen scheu das Fl\u00fcgelthor \u2013", "tokens": ["Sie", "\u00f6ff\u00b7nen", "scheu", "das", "Fl\u00fc\u00b7gelt\u00b7hor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlossen ist des K\u00f6nigs Ohr,", "tokens": ["Ver\u00b7schlos\u00b7sen", "ist", "des", "K\u00f6\u00b7nigs", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Auge schl\u00e4ft noch schlummertrunken;", "tokens": ["Sein", "Au\u00b7ge", "schl\u00e4ft", "noch", "schlum\u00b7mer\u00b7trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie es auf den Pf\u00fchl gesunken,", "tokens": ["Und", "wie", "es", "auf", "den", "Pf\u00fchl", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So liegt sein junges Haupt in Ruh',", "tokens": ["So", "liegt", "sein", "jun\u00b7ges", "Haupt", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die gelben Locken decken's zu.", "tokens": ["Die", "gel\u00b7ben", "Lo\u00b7cken", "de\u00b7cken's", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch \u00fcber'm Bette Schwert und Schild \u2013", "tokens": ["Doch", "\u00fc\u00b7ber'm", "Bet\u00b7te", "Schwert", "und", "Schild", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie h\u00e4ngen, der Zerst\u00f6rung Bild,", "tokens": ["Sie", "h\u00e4n\u00b7gen", ",", "der", "Zer\u00b7st\u00f6\u00b7rung", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Stahl geschmelzt wie in der Esse,", "tokens": ["Der", "Stahl", "ge\u00b7schmelzt", "wie", "in", "der", "Es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schild zerdr\u00fcckt, wie von der Presse,", "tokens": ["Der", "Schild", "zer\u00b7dr\u00fcckt", ",", "wie", "von", "der", "Pres\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Leder und durch Eisen fuhr", "tokens": ["Durch", "Le\u00b7der", "und", "durch", "Ei\u00b7sen", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Blitz und lie\u00df die hei\u00dfe Spur.", "tokens": ["Der", "Blitz", "und", "lie\u00df", "die", "hei\u00b7\u00dfe", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Diener starren; jetzt erwacht", "tokens": ["Die", "Die\u00b7ner", "star\u00b7ren", ";", "jetzt", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr K\u00f6nig aus des Schlafes Nacht,", "tokens": ["Ihr", "K\u00f6\u00b7nig", "aus", "des", "Schla\u00b7fes", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fliegt sein Blick nach seinen Waffen", "tokens": ["Es", "fliegt", "sein", "Blick", "nach", "sei\u00b7nen", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieht sie staunend umgeschaffen;", "tokens": ["Und", "sieht", "sie", "stau\u00b7nend", "um\u00b7ge\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gar bald err\u00e4t er was geschah,", "tokens": ["Gar", "bald", "er\u00b7r\u00e4t", "er", "was", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Spricht: \u00bbGro\u00dfer Meister, warst ", "tokens": ["Spricht", ":", "\u00bb", "Gro\u00b7\u00dfer", "Meis\u00b7ter", ",", "warst"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$.", "$(", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mir d\u00e4ucht, ich h\u00f6rte doch dein Lied,", "tokens": ["Mir", "d\u00e4ucht", ",", "ich", "h\u00f6r\u00b7te", "doch", "dein", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6rte h\u00e4mmern dich, o Schmied!", "tokens": ["Ich", "h\u00f6r\u00b7te", "h\u00e4m\u00b7mern", "dich", ",", "o", "Schmied", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah deine Loh das Leder gerben,", "tokens": ["Sah", "dei\u00b7ne", "Loh", "das", "Le\u00b7der", "ger\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah deine Glut das Eisen f\u00e4rben,", "tokens": ["Sah", "dei\u00b7ne", "Glut", "das", "Ei\u00b7sen", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu stehen meint' in k\u00fchnem Traum", "tokens": ["Zu", "ste\u00b7hen", "meint'", "in", "k\u00fch\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hoch in deiner Werkstatt Raum!\u00ab", "tokens": ["Ich", "hoch", "in", "dei\u00b7ner", "Werk\u00b7statt", "Raum", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der K\u00f6nig von dem Lager sprang,", "tokens": ["Der", "K\u00f6\u00b7nig", "von", "dem", "La\u00b7ger", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald in der Hand den Hammer schwang,", "tokens": ["Bald", "in", "der", "Hand", "den", "Ham\u00b7mer", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4\u00dft ihn auf dem Schwerte klingen,", "tokens": ["Er", "l\u00e4\u00dft", "ihn", "auf", "dem", "Schwer\u00b7te", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will selbst, was Gott begann, vollbringen,", "tokens": ["Will", "selbst", ",", "was", "Gott", "be\u00b7gann", ",", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PRELS", "NN", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Eisen, warm noch, schmiedet er,", "tokens": ["Das", "Ei\u00b7sen", ",", "warm", "noch", ",", "schmie\u00b7det", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stellt den Schild aus Falten her.", "tokens": ["Und", "stellt", "den", "Schild", "aus", "Fal\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos"