{"dta.poem.11392": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschung der universit\u00e4t Witten-  \n berg an Jhro  Excellen tz Herrn H. A. von  \n Sch\u00f6ning, rc. bey antritt der von Seiner  \n Churf\u00fcrstl. Durchl. zu Sachsen ihm gn\u00e4-  \n digst aufgetragenen General-  \n Feld-Marschalls- Charge.  \n J. G. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tapffrer feldherr unsrer zeit! der sich l\u00e4st vom himmel treiben,", "tokens": ["Tapf\u00b7frer", "feld\u00b7herr", "uns\u00b7rer", "zeit", "!", "der", "sich", "l\u00e4st", "vom", "him\u00b7mel", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$.", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "---+--+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und, als wie ein helles licht, nirgend kan verschlossen bleiben:", "tokens": ["Und", ",", "als", "wie", "ein", "hel\u00b7les", "licht", ",", "nir\u00b7gend", "kan", "ver\u00b7schlos\u00b7sen", "blei\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der bey regen, sturm und wetter seinen fu\u00df zu felde lenckt,", "tokens": ["Der", "bey", "re\u00b7gen", ",", "sturm", "und", "wet\u00b7ter", "sei\u00b7nen", "fu\u00df", "zu", "fel\u00b7de", "lenckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "$,", "ADJD", "KON", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und die angereitzten schwerder in der feinde blute tr\u00e4nckt!", "tokens": ["Und", "die", "an\u00b7ge\u00b7reitz\u00b7ten", "schwer\u00b7der", "in", "der", "fein\u00b7de", "blu\u00b7te", "tr\u00e4nckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Teutschland kennet deinen muth, wie du in so vielen z\u00fcgen", "tokens": ["Teutschland", "ken\u00b7net", "dei\u00b7nen", "muth", ",", "wie", "du", "in", "so", "vie\u00b7len", "z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.6": {"text": "Bey der unerschrocknen welt l\u00e4ngsthin einen ruhm bestiegen:", "tokens": ["Bey", "der", "un\u00b7er\u00b7schrock\u00b7nen", "welt", "l\u00e4ngst\u00b7hin", "ei\u00b7nen", "ruhm", "be\u00b7stie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Von so manchen donner-schl\u00e4gen ist dein frisches hertz erweckt,", "tokens": ["Von", "so", "man\u00b7chen", "don\u00b7ner\u00b7schl\u00e4\u00b7gen", "ist", "dein", "fri\u00b7sches", "hertz", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Eisen, pulver, spie\u00df und feuer haben dich niemahls erschreckt.", "tokens": ["Ei\u00b7sen", ",", "pul\u00b7ver", ",", "spie\u00df", "und", "feu\u00b7er", "ha\u00b7ben", "dich", "nie\u00b7mahls", "er\u00b7schreckt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VVFIN", "KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Norden blies auf s\u00fcden los, da\u00df die st\u00e4rcksten mauren bebten,", "tokens": ["Nor\u00b7den", "blies", "auf", "s\u00fc\u00b7den", "los", ",", "da\u00df", "die", "st\u00e4rcks\u00b7ten", "mau\u00b7ren", "beb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und die l\u00e4nder allbereit zwischen th\u00fcr und angel schwebten;", "tokens": ["Und", "die", "l\u00e4n\u00b7der", "all\u00b7be\u00b7reit", "zwi\u00b7schen", "th\u00fcr", "und", "an\u00b7gel", "schweb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Doch der nord-wind wich mit schrecken von den steilen felsen ab,", "tokens": ["Doch", "der", "nord\u00b7wind", "wich", "mit", "schre\u00b7cken", "von", "den", "stei\u00b7len", "fel\u00b7sen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Da sich deinem aufgebote sund und insul untergab.", "tokens": ["Da", "sich", "dei\u00b7nem", "auf\u00b7ge\u00b7bo\u00b7te", "sund", "und", "in\u00b7sul", "un\u00b7ter\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Preussen neunt dich Hannibal; Denn die auf und nieder strichen", "tokens": ["Preus\u00b7sen", "neunt", "dich", "Han\u00b7ni\u00b7bal", ";", "Denn", "die", "auf", "und", "nie\u00b7der", "stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "NE", "$.", "KON", "ART", "APPR", "KON", "PTKVZ", "VVFIN"], "meter": "+-+----+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Sind vor deinem paucken-schall bi\u00df an ihren ort gewichen:", "tokens": ["Sind", "vor", "dei\u00b7nem", "pau\u00b7cken\u00b7schall", "bi\u00df", "an", "ih\u00b7ren", "ort", "ge\u00b7wi\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Der bi\u00dfher noch freche sieger lie\u00df dir selbst die sieges-fahn,", "tokens": ["Der", "bi\u00df\u00b7her", "noch", "fre\u00b7che", "sie\u00b7ger", "lie\u00df", "dir", "selbst", "die", "sie\u00b7ges\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Denn du hattest in dem treffen als ein Hercules gethan.", "tokens": ["Denn", "du", "hat\u00b7test", "in", "dem", "tref\u00b7fen", "als", "ein", "Her\u00b7cu\u00b7les", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "VVINF", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Also hat dein starcker blitz auch in Ofen eingeschlagen,", "tokens": ["Al\u00b7so", "hat", "dein", "star\u00b7cker", "blitz", "auch", "in", "O\u00b7fen", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Und, was ehmahls hat geherrscht, mu\u00df anitzo fessel tragen;", "tokens": ["Und", ",", "was", "eh\u00b7mahls", "hat", "ge\u00b7herrscht", ",", "mu\u00df", "a\u00b7nit\u00b7zo", "fes\u00b7sel", "tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Sehet! sprach der grosse kayser, was allhier ein Teutscher thut,", "tokens": ["Se\u00b7het", "!", "sprach", "der", "gros\u00b7se", "kay\u00b7ser", ",", "was", "all\u00b7hier", "ein", "Teut\u00b7scher", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Denn sein tropffen \u00fcberwieget hundert tonnen T\u00fcrcken-blut.", "tokens": ["Denn", "sein", "tropf\u00b7fen", "\u00fc\u00b7berw\u00b7ie\u00b7get", "hun\u00b7dert", "ton\u00b7nen", "T\u00fcr\u00b7cken\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Drauf erfolgte der triumph, da die feld-trompeten klungen:", "tokens": ["Drauf", "er\u00b7folg\u00b7te", "der", "tri\u00b7umph", ",", "da", "die", "feld\u00b7trom\u00b7pe\u00b7ten", "klun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Da vor deiner sieges-pracht die erhitzten hengste sprungen:", "tokens": ["Da", "vor", "dei\u00b7ner", "sie\u00b7ges\u00b7pracht", "die", "er\u00b7hitz\u00b7ten", "hengs\u00b7te", "sprun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Da die sclaven deinen f\u00fcssen nur ein schemmel wolten seyn:", "tokens": ["Da", "die", "scla\u00b7ven", "dei\u00b7nen", "f\u00fcs\u00b7sen", "nur", "ein", "schem\u00b7mel", "wol\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPOSAT", "VVFIN", "ADV", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Hiemit zog der sch\u00f6ne sieger, als ein welt-bezwinger ein.", "tokens": ["Hie\u00b7mit", "zog", "der", "sch\u00f6\u00b7ne", "sie\u00b7ger", ",", "als", "ein", "welt\u00b7be\u00b7zwin\u00b7ger", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Darum liebt der Sachsen Mars dein so ritterliches wesen,", "tokens": ["Da\u00b7rum", "liebt", "der", "Sach\u00b7sen", "Mars", "dein", "so", "rit\u00b7ter\u00b7li\u00b7ches", "we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Da\u00df er dich vor unser heyl hat zum werckzeug auserlesen:", "tokens": ["Da\u00df", "er", "dich", "vor", "un\u00b7ser", "heyl", "hat", "zum", "wer\u00b7ck\u00b7zeug", "au\u00b7ser\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Diesem bleibst du Agamemnon, der vor Troja triumphirt:", "tokens": ["Die\u00b7sem", "bleibst", "du", "A\u00b7ga\u00b7mem\u00b7non", ",", "der", "vor", "Tro\u00b7ja", "tri\u00b7um\u00b7phirt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NE", "$,", "PRELS", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Und bist wie ein ander Pyrrhus, der Achillens waffen f\u00fchrt.", "tokens": ["Und", "bist", "wie", "ein", "an\u00b7der", "Pyrr\u00b7hus", ",", "der", "A\u00b7chil\u00b7lens", "waf\u00b7fen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ART", "ADJD", "NE", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+--+-+---+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Zeuch mit deinem David hin, da\u00df des milden himmels g\u00fcte", "tokens": ["Zeuch", "mit", "dei\u00b7nem", "Da\u00b7vid", "hin", ",", "da\u00df", "des", "mil\u00b7den", "him\u00b7mels", "g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NE", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Deinen k\u00fcra\u00df, helm und stahl stets mit segen \u00fcbersch\u00fctte!", "tokens": ["Dei\u00b7nen", "k\u00fc\u00b7ra\u00df", ",", "helm", "und", "stahl", "stets", "mit", "se\u00b7gen", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+---+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Unser trost, Johann George, schaffe diesen grentzen ruh!", "tokens": ["Un\u00b7ser", "trost", ",", "Jo\u00b7hann", "Ge\u00b7or\u00b7ge", ",", "schaf\u00b7fe", "die\u00b7sen", "grent\u00b7zen", "ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "NE", "$,", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Ja er schliesse, wie Augustus, dreymahl Janus tempel zu!", "tokens": ["Ja", "er", "schlies\u00b7se", ",", "wie", "Au\u00b7gus\u00b7tus", ",", "drey\u00b7mahl", "Ja\u00b7nus", "tem\u00b7pel", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PWAV", "NE", "$,", "ADV", "NE", "NE", "PTKVZ", "$."], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Leucoris wird ihren sohn hinfort ihren vater nennen,", "tokens": ["Leu\u00b7co\u00b7ris", "wird", "ih\u00b7ren", "sohn", "hin\u00b7fort", "ih\u00b7ren", "va\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Und als eine dienerin gegen ihn aus liebe brennen;", "tokens": ["Und", "als", "ei\u00b7ne", "die\u00b7ne\u00b7rin", "ge\u00b7gen", "ihn", "aus", "lie\u00b7be", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Gnung, da\u00df diese werthe mutter einen adler hat gezeugt,", "tokens": ["Gnung", ",", "da\u00df", "die\u00b7se", "wert\u00b7he", "mut\u00b7ter", "ei\u00b7nen", "ad\u00b7ler", "hat", "ge\u00b7zeugt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PDS", "VVFIN", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Der nunmehr den S\u00e4chschen him\u0303el wieder zu uns kindern neigt.", "tokens": ["Der", "nun\u00b7mehr", "den", "S\u00e4ch\u00b7schen", "him\u0303el", "wie\u00b7der", "zu", "uns", "kin\u00b7dern", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}