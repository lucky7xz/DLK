{"dta.poem.19575": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der trunkene Gott.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe Marmorstufen steigen", "tokens": ["Wei\u00b7\u00dfe", "Mar\u00b7mor\u00b7stu\u00b7fen", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der G\u00e4rten laub'ge Nacht,", "tokens": ["Durch", "der", "G\u00e4r\u00b7ten", "laub'\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlanke Palmenf\u00e4cher neigen", "tokens": ["Schlan\u00b7ke", "Pal\u00b7men\u00b7f\u00e4\u00b7cher", "nei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Himmels blaue Pracht.", "tokens": ["In", "des", "Him\u00b7mels", "blau\u00b7e", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber Tempeln, Hainen, Gr\u00fcften", "tokens": ["Ue\u00b7ber", "Tem\u00b7peln", ",", "Hai\u00b7nen", ",", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zecht in abendweichen L\u00fcften", "tokens": ["Zecht", "in", "ab\u00b7end\u00b7wei\u00b7chen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alexander's Lieblingsschaar;", "tokens": ["A\u00b7lex\u00b7an\u00b7der's", "Lieb\u00b7lings\u00b7schaar", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der Erde Herr sich labe,", "tokens": ["Da\u00df", "der", "Er\u00b7de", "Herr", "sich", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bietet ihm ein sch\u00f6ner Knabe", "tokens": ["Bie\u00b7tet", "ihm", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wein in goldner Schaale dar.", "tokens": ["Wein", "in", "gold\u00b7ner", "Schaa\u00b7le", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kleitos neben Philipp's Sohne", "tokens": ["Klei\u00b7tos", "ne\u00b7ben", "Phil\u00b7ipp's", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Furcht die Stirne kummervoll,", "tokens": ["Furcht", "die", "Stir\u00b7ne", "kum\u00b7mer\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der benarbte Macedone", "tokens": ["Der", "be\u00b7narb\u00b7te", "Ma\u00b7ce\u00b7do\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00fcrft im Weine Zorn und Groll:", "tokens": ["Schl\u00fcrft", "im", "Wei\u00b7ne", "Zorn", "und", "Groll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gedenkt der Heergenossen,", "tokens": ["Er", "ge\u00b7denkt", "der", "Heer\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die die erste Phalanx schlossen", "tokens": ["Die", "die", "ers\u00b7te", "Pha\u00b7lanx", "schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den Bergen k\u00fchl und fern \u2014", "tokens": ["In", "den", "Ber\u00b7gen", "k\u00fchl", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen dunkeln Muth zu kr\u00e4nken", "tokens": ["Sei\u00b7nen", "dun\u00b7keln", "Muth", "zu", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00fcstet es den jungen Schenken", "tokens": ["L\u00fcs\u00b7tet", "es", "den", "jun\u00b7gen", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lagernd an dem Knie des Herrn.", "tokens": ["La\u00b7gernd", "an", "dem", "Knie", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die erhabne Stirn und Braue", "tokens": ["Die", "er\u00b7hab\u00b7ne", "Stirn", "und", "Brau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umt den Zug ins Inderland,", "tokens": ["Tr\u00e4umt", "den", "Zug", "ins", "In\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauschend liest den Traum das schlaue", "tokens": ["Lau\u00b7schend", "liest", "den", "Traum", "das", "schlau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kind, den Blick emporgewandt:", "tokens": ["Kind", ",", "den", "Blick", "em\u00b7por\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ebacchus bist du, der belaubte,", "tokens": ["\u201e", "bac\u00b7chus", "bist", "du", ",", "der", "be\u00b7laub\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem schw\u00e4rmerischen Haupte,", "tokens": ["Mit", "dem", "schw\u00e4r\u00b7me\u00b7ri\u00b7schen", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der ins Land der Sonne zieht!", "tokens": ["Der", "ins", "Land", "der", "Son\u00b7ne", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Heer kannst du bezwingen,", "tokens": ["Oh\u00b7ne", "Heer", "kannst", "du", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur den Thyrsus darfst du schwingen,", "tokens": ["Nur", "den", "Thyr\u00b7sus", "darfst", "du", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Winke nur und Indien kniet!\u201c", "tokens": ["Win\u00b7ke", "nur", "und", "In\u00b7di\u00b7en", "kniet", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "NE", "VVFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Finster grollt der tapfre Streiter:", "tokens": ["Fins\u00b7ter", "grollt", "der", "tapf\u00b7re", "Strei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201edurch der W\u00fcste hei\u00dfen Sand?", "tokens": ["\u201e", "durch", "der", "W\u00fcs\u00b7te", "hei\u00b7\u00dfen", "Sand", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer ferner, immer weiter?", "tokens": ["Im\u00b7mer", "fer\u00b7ner", ",", "im\u00b7mer", "wei\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach des Indus Fabelstrand?", "tokens": ["Nach", "des", "In\u00b7dus", "Fa\u00b7bel\u00b7strand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siegst du mit der Wimper Winken,", "tokens": ["Siegst", "du", "mit", "der", "Wim\u00b7per", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum fechten wir und sinken", "tokens": ["Wa\u00b7rum", "fech\u00b7ten", "wir", "und", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir f\u00fcr dich? Zum Schein und Spott?", "tokens": ["Wir", "f\u00fcr", "dich", "?", "Zum", "Schein", "und", "Spott", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "$.", "APPRART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebende kannst du belohnen,", "tokens": ["Le\u00b7ben\u00b7de", "kannst", "du", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Deine todten Macedonen,", "tokens": ["Dei\u00b7ne", "tod\u00b7ten", "Ma\u00b7ce\u00b7do\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wecke sie, bist du ein Gott!\u201c", "tokens": ["We\u00b7cke", "sie", ",", "bist", "du", "ein", "Gott", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u2014 \u201eWelchen dampfenden Altares", "tokens": ["\u201e", "Wel\u00b7chen", "damp\u00b7fen\u00b7den", "Al\u00b7ta\u00b7res"], "token_info": ["punct", "punct", "word", "word", "word"], "pos": ["$(", "$(", "PWAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freust du auf der Erde dich?", "tokens": ["Freust", "du", "auf", "der", "Er\u00b7de", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du die Gewalt des Ares,", "tokens": ["Bist", "du", "die", "Ge\u00b7walt", "des", "A\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Helmumflattert, f\u00fcrchterlich?", "tokens": ["Hel\u00b7mum\u00b7flat\u00b7tert", ",", "f\u00fcrch\u00b7ter\u00b7lich", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, bevor den niedern Thalen", "tokens": ["Herr", ",", "be\u00b7vor", "den", "nie\u00b7dern", "Tha\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du dich nahtest ohne Strahlen,", "tokens": ["Du", "dich", "nah\u00b7test", "oh\u00b7ne", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welches war dein himmlisch Amt?", "tokens": ["Wel\u00b7ches", "war", "dein", "himm\u00b7lisch", "Amt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bist du Zeus? Bist du ein Andrer?", "tokens": ["Bist", "du", "Zeus", "?", "Bist", "du", "ein", "A\u00b7ndrer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bist du Helios, der Wandrer,", "tokens": ["Bist", "du", "He\u00b7li\u00b7os", ",", "der", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dessen Stirne sonnig flammt?\u201c \u2014", "tokens": ["Des\u00b7sen", "Stir\u00b7ne", "son\u00b7nig", "flammt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Traulich neigt der graue Fechter", "tokens": ["Trau\u00b7lich", "neigt", "der", "grau\u00b7e", "Fech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zum Ohr des Gottes hin,", "tokens": ["Sich", "zum", "Ohr", "des", "Got\u00b7tes", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit unseligem Gel\u00e4chter", "tokens": ["Mit", "un\u00b7se\u00b7li\u00b7gem", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.4": {"text": "R\u00fchrt er an der Schulter ihn:", "tokens": ["R\u00fchrt", "er", "an", "der", "Schul\u00b7ter", "ihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201egast des Himmels, merklich sinken", "tokens": ["\u201e", "gast", "des", "Him\u00b7mels", ",", "merk\u00b7lich", "sin\u00b7ken"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haupt und Schulter dir zur Linken", "tokens": ["Haupt", "und", "Schul\u00b7ter", "dir", "zur", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lastet dir der Erde Raub?", "tokens": ["Las\u00b7tet", "dir", "der", "Er\u00b7de", "Raub", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht der Knabe dich zum Gotte,", "tokens": ["Macht", "der", "Kna\u00b7be", "dich", "zum", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dein Gebrechen schreit mit Spotte:", "tokens": ["Dein", "Ge\u00b7bre\u00b7chen", "schreit", "mit", "Spot\u00b7te", ":"], "token_info": ["word", "word", "wo