{"textgrid.poem.43651": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein liebes G\u00e4rtchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer hat alle Welt begl\u00fcckt", "tokens": ["Der", "Som\u00b7mer", "hat", "al\u00b7le", "Welt", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Jedem eine Freude gebracht;", "tokens": ["Und", "Je\u00b7dem", "ei\u00b7ne", "Freu\u00b7de", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er hat mein liebes G\u00e4rtchen geschm\u00fcckt", "tokens": ["Er", "hat", "mein", "lie\u00b7bes", "G\u00e4rt\u00b7chen", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch sch\u00f6ner als ich je gedacht,", "tokens": ["Noch", "sch\u00f6\u00b7ner", "als", "ich", "je", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein liebes G\u00e4rtchen hinter'm Haus", "tokens": ["Mein", "lie\u00b7bes", "G\u00e4rt\u00b7chen", "hin\u00b7ter'm", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich so gern geh' ein und aus.", "tokens": ["Wo", "ich", "so", "gern", "geh'", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Alles d'rin von Blumen prangt!", "tokens": ["Wie", "Al\u00b7les", "d'\u00b7rin", "von", "Blu\u00b7men", "prangt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie Alles d'rin von Fr\u00fcchten hangt!", "tokens": ["Wie", "Al\u00b7les", "d'\u00b7rin", "von", "Fr\u00fcch\u00b7ten", "hangt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Erdbeeren l\u00e4cheln aus dunklem Gr\u00fcn,", "tokens": ["Erd\u00b7bee\u00b7ren", "l\u00e4\u00b7cheln", "aus", "dunk\u00b7lem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Und daneben Rosen und Lilien bl\u00fchn.", "tokens": ["Und", "da\u00b7ne\u00b7ben", "Ro\u00b7sen", "und", "Li\u00b7li\u00b7en", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Doch hat uns auch keine M\u00fche verdrossen:", "tokens": ["Doch", "hat", "uns", "auch", "kei\u00b7ne", "M\u00fc\u00b7he", "ver\u00b7dros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wir haben ges\u00e4et, gepflanzt und begossen,", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7s\u00e4et", ",", "ge\u00b7pflanzt", "und", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und flei\u00dfig gej\u00e4tet mit eigener Hand", "tokens": ["Und", "flei\u00b7\u00dfig", "ge\u00b7j\u00e4\u00b7tet", "mit", "ei\u00b7ge\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Und die Wege bestreut mit frischem Sand.", "tokens": ["Und", "die", "We\u00b7ge", "be\u00b7streut", "mit", "fri\u00b7schem", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Du liebes G\u00e4rtchen, f\u00fcr alle die M\u00fchn", "tokens": ["Du", "lie\u00b7bes", "G\u00e4rt\u00b7chen", ",", "f\u00fcr", "al\u00b7le", "die", "M\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PIS", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da l\u00e4ssest du deine Blumen bl\u00fchn", "tokens": ["Da", "l\u00e4s\u00b7sest", "du", "dei\u00b7ne", "Blu\u00b7men", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und s\u00fc\u00dfe Fr\u00fcchte reichst du uns auch", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", "reichst", "du", "uns", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Von manchem Baum und manchem Strauch.", "tokens": ["Von", "man\u00b7chem", "Baum", "und", "man\u00b7chem", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "F\u00fcr all das Lieb' und Gut' empfang'", "tokens": ["F\u00fcr", "all", "das", "Lieb'", "und", "Gut'", "emp\u00b7fang'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nun unsern Dank in Sang und Klang!", "tokens": ["Nun", "un\u00b7sern", "Dank", "in", "Sang", "und", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}