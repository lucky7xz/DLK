{"dta.poem.14092": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das alte M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mohrenritter, Mohrenritter!", "tokens": ["Moh\u00b7ren\u00b7rit\u00b7ter", ",", "Moh\u00b7ren\u00b7rit\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier gefangen auf dem Schlosse,", "tokens": ["Hier", "ge\u00b7fan\u00b7gen", "auf", "dem", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steh' ich einsam an dem Gitter,", "tokens": ["Steh'", "ich", "ein\u00b7sam", "an", "dem", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warte wohl der s\u00fc\u00dfen Rosen,", "tokens": ["War\u00b7te", "wohl", "der", "s\u00fc\u00b7\u00dfen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau' in's Thal beim Klang der Zitter;", "tokens": ["Schau'", "in's", "Thal", "beim", "Klang", "der", "Zit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob Du nah'st im Glanz des Morgens, \u2014", "tokens": ["Ob", "Du", "nah'st", "im", "Glanz", "des", "Mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "ART", "ADV", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch geschlossen bleibt das Gitter,", "tokens": ["Doch", "ge\u00b7schlos\u00b7sen", "bleibt", "das", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es fliegen Stunden, Wolken \u2014", "tokens": ["Und", "es", "flie\u00b7gen", "Stun\u00b7den", ",", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mohrenritter, Mohrenritter!", "tokens": ["Moh\u00b7ren\u00b7rit\u00b7ter", ",", "Moh\u00b7ren\u00b7rit\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und es sinken Lenz und Rosen \u2014", "tokens": ["Und", "es", "sin\u00b7ken", "Lenz", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}