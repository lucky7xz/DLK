{"textgrid.poem.36488": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "10. Der Sperber und die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kleine Lerche sah den blauen Himmel an,", "tokens": ["Die", "klei\u00b7ne", "Ler\u00b7che", "sah", "den", "blau\u00b7en", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schwebte, singend, hin und wieder,", "tokens": ["Und", "schweb\u00b7te", ",", "sin\u00b7gend", ",", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df auf ihrer Flur sich langsam, singend, nieder;", "tokens": ["Und", "lie\u00df", "auf", "ih\u00b7rer", "Flur", "sich", "lang\u00b7sam", ",", "sin\u00b7gend", ",", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "ADJD", "$,", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! aber pl\u00f6tzlich scho\u00df, mit schlagendem Gefieder,", "tokens": ["Ach", "!", "a\u00b7ber", "pl\u00f6tz\u00b7lich", "scho\u00df", ",", "mit", "schla\u00b7gen\u00b7dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus seinem Busch hervor ein Sperber, ein Tyrann;", "tokens": ["Aus", "sei\u00b7nem", "Busch", "her\u00b7vor", "ein", "Sper\u00b7ber", ",", "ein", "Ty\u00b7rann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, grausam sie verzehrend, sprach er: H\u00f6ren", "tokens": ["Und", ",", "grau\u00b7sam", "sie", "ver\u00b7zeh\u00b7rend", ",", "sprach", "er", ":", "H\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADJD", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Konnt' ich sie l\u00e4nger nicht; ich mu\u00dfte sie verzehren,", "tokens": ["Konnt'", "ich", "sie", "l\u00e4n\u00b7ger", "nicht", ";", "ich", "mu\u00df\u00b7te", "sie", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich, wie sie, nicht singen kann!", "tokens": ["Weil", "ich", ",", "wie", "sie", ",", "nicht", "sin\u00b7gen", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}