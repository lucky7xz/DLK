{"textgrid.poem.34507": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1. Die Flucht vor dem Herren", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geschah des Herren Wort zu Jona:", "tokens": ["Es", "ge\u00b7schah", "des", "Her\u00b7ren", "Wort", "zu", "Jo\u00b7na", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mach dich auf und wandre in die grosse", "tokens": ["Mach", "dich", "auf", "und", "wand\u00b7re", "in", "die", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ninive und predige darinnen", "tokens": ["Ni\u00b7ni\u00b7ve", "und", "pre\u00b7di\u00b7ge", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "von dem heiligen Zorne deines Gottes,", "tokens": ["von", "dem", "hei\u00b7li\u00b7gen", "Zor\u00b7ne", "dei\u00b7nes", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "denn es ist vor mich heraufgekommen", "tokens": ["denn", "es", "ist", "vor", "mich", "her\u00b7auf\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "ihre Bosheit, und ich will sie strafen.", "tokens": ["ih\u00b7re", "Bos\u00b7heit", ",", "und", "ich", "will", "sie", "stra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber Jona traute nicht dem Zorne", "tokens": ["A\u00b7ber", "Jo\u00b7na", "trau\u00b7te", "nicht", "dem", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "seines Herrn und h\u00f6rte nicht auf seine", "tokens": ["sei\u00b7nes", "Herrn", "und", "h\u00f6r\u00b7te", "nicht", "auf", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stimme, sondern floh hinab zum Meere.", "tokens": ["Stim\u00b7me", ",", "son\u00b7dern", "floh", "hin\u00b7ab", "zum", "Mee\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und da er ein Schiff fand, das bereit war,", "tokens": ["Und", "da", "er", "ein", "Schiff", "fand", ",", "das", "be\u00b7reit", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "auf die hohe See hinaus zu steuern,", "tokens": ["auf", "die", "ho\u00b7he", "See", "hin\u00b7aus", "zu", "steu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "gab er F\u00e4hrgeld und bestieg es eilends \u2013", "tokens": ["gab", "er", "F\u00e4hr\u00b7geld", "und", "be\u00b7stieg", "es", "ei\u00b7lends", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "vor dem Herren auf das Meer zu fl\u00fcchten!", "tokens": ["vor", "dem", "Her\u00b7ren", "auf", "das", "Meer", "zu", "fl\u00fcch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch da sandte Zebaoth die St\u00fcrme", "tokens": ["Doch", "da", "sand\u00b7te", "Ze\u00b7bao\u00b7th", "die", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcbers Meer, dass sich ein Ungewitter", "tokens": ["\u00fc\u00b7bers", "Meer", ",", "dass", "sich", "ein", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hochgewaltig aufhob aus der Tiefe,", "tokens": ["hoch\u00b7ge\u00b7wal\u00b7tig", "auf\u00b7hob", "aus", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "weisse Wellen rings das Schiff umst\u00fcrzten \u2013", "tokens": ["weis\u00b7se", "Wel\u00b7len", "rings", "das", "Schiff", "um\u00b7st\u00fcrz\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "also, dass sie glaubten, es zerbr\u00e4che.", "tokens": ["al\u00b7so", ",", "dass", "sie", "glaub\u00b7ten", ",", "es", "zer\u00b7br\u00e4\u00b7che", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Furcht und Graun ergriff das ganze Schiffsvolk,", "tokens": ["Furcht", "und", "Graun", "er\u00b7griff", "das", "gan\u00b7ze", "Schiffs\u00b7volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und es schrie ein jeglicher zu seinem", "tokens": ["und", "es", "schrie", "ein", "jeg\u00b7li\u00b7cher", "zu", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIAT", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gott. Sie warfen alle Lasten, G\u00fcter,", "tokens": ["Gott", ".", "Sie", "war\u00b7fen", "al\u00b7le", "Las\u00b7ten", ",", "G\u00fc\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das Ger\u00e4te selber, das im Schiff war,", "tokens": ["das", "Ge\u00b7r\u00e4\u00b7te", "sel\u00b7ber", ",", "das", "im", "Schiff", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00fcber Bord. \u2013 Nur Jona war hinunter", "tokens": ["\u00fc\u00b7ber", "Bord", ".", "\u2013", "Nur", "Jo\u00b7na", "war", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "ADV", "NE", "VAFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "in das Schiff gestiegen \u2013 lag und schlief.", "tokens": ["in", "das", "Schiff", "ge\u00b7stie\u00b7gen", "\u2013", "lag", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Doch ihn weckte jetzt der Herr des Schiffes,", "tokens": ["Doch", "ihn", "weck\u00b7te", "jetzt", "der", "Herr", "des", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "trat zu ihm und sprach: Was schl\u00e4fst du? Stehe", "tokens": ["trat", "zu", "ihm", "und", "sprach", ":", "Was", "schl\u00e4fst", "du", "?", "Ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "auf und ruf auch du jetzt deinen Gott an:", "tokens": ["auf", "und", "ruf", "auch", "du", "jetzt", "dei\u00b7nen", "Gott", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "NN", "ADV", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "ob vielleicht er unser denken m\u00f6chte,", "tokens": ["ob", "viel\u00b7leicht", "er", "un\u00b7ser", "den\u00b7ken", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "dass wir nicht verd\u00fcrben. \u2013 Aber Jona", "tokens": ["dass", "wir", "nicht", "ver\u00b7d\u00fcr\u00b7ben", ".", "\u2013", "A\u00b7ber", "Jo\u00b7na"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "$.", "$(", "KON", "NE"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "wusste kein Gebet \u2013 er beugte schweigend", "tokens": ["wuss\u00b7te", "kein", "Ge\u00b7bet", "\u2013", "er", "beug\u00b7te", "schwei\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "seine Stirn und dachte seiner S\u00fcnde.", "tokens": ["sei\u00b7ne", "Stirn", "und", "dach\u00b7te", "sei\u00b7ner", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lasst uns losen! riefen da die Schiffer.", "tokens": ["Lasst", "uns", "lo\u00b7sen", "!", "rie\u00b7fen", "da", "die", "Schif\u00b7fer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lasst uns losen, dass wir so erfahren,", "tokens": ["Lasst", "uns", "lo\u00b7sen", ",", "dass", "wir", "so", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wer von uns es sei, um dessentwillen", "tokens": ["wer", "von", "uns", "es", "sei", ",", "um", "des\u00b7sent\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "PPER", "PPER", "VAFIN", "$,", "KOUI", "PAV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "solches \u00dcbel unser Schiff betroffen,", "tokens": ["sol\u00b7ches", "\u00dc\u00b7bel", "un\u00b7ser", "Schiff", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "wem das Unheil gilt, das uns vernichtet.", "tokens": ["wem", "das", "Un\u00b7heil", "gilt", ",", "das", "uns", "ver\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und es fiel das Los und fiel auf Jona.", "tokens": ["Und", "es", "fiel", "das", "Los", "und", "fiel", "auf", "Jo\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Scheu zur Seite wichen da die Leute,", "tokens": ["Scheu", "zur", "Sei\u00b7te", "wi\u00b7chen", "da", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und sie fragten: Sprich, warum geschieht uns", "tokens": ["und", "sie", "frag\u00b7ten", ":", "Sprich", ",", "wa\u00b7rum", "ge\u00b7schieht", "uns"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "VVIMP", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Solches? Was ist dein Gewerbe? Woher", "tokens": ["Sol\u00b7ches", "?", "Was", "ist", "dein", "Ge\u00b7wer\u00b7be", "?", "Wo\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "PWAV"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "kommst du, und von welchem Volke stammst du?", "tokens": ["kommst", "du", ",", "und", "von", "wel\u00b7chem", "Vol\u00b7ke", "stammst", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "PWAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Jona richtete sich auf und sprach:", "tokens": ["Jo\u00b7na", "rich\u00b7te\u00b7te", "sich", "auf", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ein Ebr\u00e4er bin ich und den Herren", "tokens": ["Ein", "Eb\u00b7r\u00e4\u00b7er", "bin", "ich", "und", "den", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "KON", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zebaoth, den Gott der Himmel f\u00fcrcht ich,", "tokens": ["Ze\u00b7bao\u00b7th", ",", "den", "Gott", "der", "Him\u00b7mel", "f\u00fcrcht", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "der das Meer gemacht und alles Trockne.", "tokens": ["der", "das", "Meer", "ge\u00b7macht", "und", "al\u00b7les", "Trock\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Seines Wortes heiliger Diener war ich", "tokens": ["Sei\u00b7nes", "Wor\u00b7tes", "hei\u00b7li\u00b7ger", "Die\u00b7ner", "war", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VAFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "bis hierher. Er ist es, der uns heimsucht,", "tokens": ["bis", "hier\u00b7her", ".", "Er", "ist", "es", ",", "der", "uns", "heim\u00b7sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$.", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und ich bin es, der vor ihm gefrevelt,", "tokens": ["und", "ich", "bin", "es", ",", "der", "vor", "ihm", "ge\u00b7fre\u00b7velt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"te