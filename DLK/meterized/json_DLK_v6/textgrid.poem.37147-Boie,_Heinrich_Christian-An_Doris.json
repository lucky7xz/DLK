{"textgrid.poem.37147": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Doris", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was stehst du da und marterst dich,", "tokens": ["Was", "stehst", "du", "da", "und", "mar\u00b7terst", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer deine Gunst verdient? O Doris, w\u00e4hle mich!", "tokens": ["Wer", "dei\u00b7ne", "Gunst", "ver\u00b7dient", "?", "O", "Do\u00b7ris", ",", "w\u00e4h\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$.", "NE", "NE", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ich bin jung genug mich deiner werth zu zeigen", "tokens": ["Denn", "ich", "bin", "jung", "ge\u00b7nug", "mich", "dei\u00b7ner", "werth", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADV", "PPER", "PPOSAT", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alt genug mein Gl\u00fcck zu schweigen.", "tokens": ["Und", "alt", "ge\u00b7nug", "mein", "Gl\u00fcck", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}