{"dta.poem.7791": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inzwischen kommt der Wag der dreyen Printze\u00dfinnen,", "tokens": ["I\u00b7nzwi\u00b7schen", "kommt", "der", "Wag", "der", "drey\u00b7en", "Print\u00b7ze\u00b7\u00dfin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "CARD", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem sie, als sie die H\u00f6l zu ihrem Schutz gewinnen,", "tokens": ["Dem", "sie", ",", "als", "sie", "die", "H\u00f6l", "zu", "ih\u00b7rem", "Schutz", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befohlen, in dem Wald zu harren aus die Nacht,", "tokens": ["Be\u00b7foh\u00b7len", ",", "in", "dem", "Wald", "zu", "har\u00b7ren", "aus", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machet sein Get\u00f6n, da\u00df Michal bald erwacht.", "tokens": ["Und", "ma\u00b7chet", "sein", "Ge\u00b7t\u00f6n", ",", "da\u00df", "Mic\u00b7hal", "bald", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sie Thalmais sieht in Merobs Armen liegen,", "tokens": ["Wie", "sie", "Thal\u00b7mais", "sieht", "in", "Me\u00b7robs", "Ar\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVFIN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Steht sie behende auf, um dieser ihr Vergn\u00fcgen", "tokens": ["Steht", "sie", "be\u00b7hen\u00b7de", "auf", ",", "um", "die\u00b7ser", "ihr", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "$,", "KOUI", "PDAT", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Zu st\u00f6hren nicht so bald, und gehet aus der H\u00f6l", "tokens": ["Zu", "st\u00f6h\u00b7ren", "nicht", "so", "bald", ",", "und", "ge\u00b7het", "aus", "der", "H\u00f6l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PTKNEG", "ADV", "ADV", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum Wagen, da\u00df sie da dem F\u00fchrer was befehl.", "tokens": ["Zum", "Wa\u00b7gen", ",", "da\u00df", "sie", "da", "dem", "F\u00fch\u00b7rer", "was", "be\u00b7fehl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "PWS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "David, der Michal sieht, eilt gleich sie zu begr\u00fcssen,", "tokens": ["Da\u00b7vid", ",", "der", "Mic\u00b7hal", "sieht", ",", "eilt", "gleich", "sie", "zu", "be\u00b7gr\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NE", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "K\u00fc\u00dft der Printzessin Rock, wirfft sich zu ihren F\u00fcssen.", "tokens": ["K\u00fc\u00dft", "der", "Print\u00b7zes\u00b7sin", "Rock", ",", "wirfft", "sich", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Woher, redt er sie an, kommt diese hohe Gnad,", "tokens": ["Wo\u00b7her", ",", "redt", "er", "sie", "an", ",", "kommt", "die\u00b7se", "ho\u00b7he", "Gnad", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die meine Sch\u00e4ferh\u00fctt anitzt empfangen hat,", "tokens": ["Die", "mei\u00b7ne", "Sch\u00e4\u00b7fer\u00b7h\u00fctt", "a\u00b7nitzt", "emp\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df ich des Hofes Zier darf sehn in meinen Auen,", "tokens": ["Da\u00df", "ich", "des", "Ho\u00b7fes", "Zier", "darf", "sehn", "in", "mei\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer bringet unserm Ort die\u00df edle Gut zu schauen?", "tokens": ["Wer", "brin\u00b7get", "un\u00b7serm", "Ort", "die\u00df", "ed\u00b7le", "Gut", "zu", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PDS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Des K\u00f6nigs Tochter hie? Weist du noch, wer ich bin,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "hie", "?", "Weist", "du", "noch", ",", "wer", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$.", "VAFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sagt Michal, ich vermeint, dein freyer Sch\u00e4fersinn", "tokens": ["Sagt", "Mic\u00b7hal", ",", "ich", "ver\u00b7meint", ",", "dein", "frey\u00b7er", "Sch\u00e4\u00b7fer\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ged\u00e4cht nicht mehr nach Hof? Ich bin froh dich zu sehen", "tokens": ["Ge\u00b7d\u00e4cht", "nicht", "mehr", "nach", "Hof", "?", "Ich", "bin", "froh", "dich", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "NE", "$.", "PPER", "VAFIN", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zu h\u00f6ren deinen Stand, der Deinen Wohlergehen.", "tokens": ["Zu", "h\u00f6\u00b7ren", "dei\u00b7nen", "Stand", ",", "der", "Dei\u00b7nen", "Woh\u00b7ler\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was ist der David doch, derselbe wiedersagt,", "tokens": ["Was", "ist", "der", "Da\u00b7vid", "doch", ",", "der\u00b7sel\u00b7be", "wie\u00b7der\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NE", "ADV", "$,", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df meines K\u00f6nigs Kind nach meinem Wesen fragt?", "tokens": ["Da\u00df", "mei\u00b7nes", "K\u00f6\u00b7nigs", "Kind", "nach", "mei\u00b7nem", "We\u00b7sen", "fragt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Davids Wohlergehn, sprach sie, mu\u00df uns erfreuen,", "tokens": ["Des", "Da\u00b7vids", "Woh\u00b7ler\u00b7gehn", ",", "sprach", "sie", ",", "mu\u00df", "uns", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn wen mein Vater liebt, werd ich niemahlen scheuen.", "tokens": ["Denn", "wen", "mein", "Va\u00b7ter", "liebt", ",", "werd", "ich", "nie\u00b7mah\u00b7len", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So lieb mir diese Gnad, sagt er, die mich beehrt,", "tokens": ["So", "lieb", "mir", "die\u00b7se", "Gnad", ",", "sagt", "er", ",", "die", "mich", "be\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDAT", "NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So leid ist mir was ich f\u00fcrher hab angeh\u00f6rt,", "tokens": ["So", "leid", "ist", "mir", "was", "ich", "f\u00fcr\u00b7her", "hab", "an\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PWS", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df da die Michal meint, ich sey so grober Sinnen,", "tokens": ["Da\u00df", "da", "die", "Mic\u00b7hal", "meint", ",", "ich", "sey", "so", "gro\u00b7ber", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df zu vergessen ich die Gnade k\u00f6nnt beginnen,", "tokens": ["Da\u00df", "zu", "ver\u00b7ges\u00b7sen", "ich", "die", "Gna\u00b7de", "k\u00f6nnt", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVFIN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die ehmals ich bey Hof zu Gibea erlangt,", "tokens": ["Die", "eh\u00b7mals", "ich", "bey", "Hof", "zu", "Gi\u00b7bea", "er\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "APPR", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Von der ohn Unterla\u00df mein stoltzer Muth noch prangt.", "tokens": ["Von", "der", "ohn", "Un\u00b7ter\u00b7la\u00df", "mein", "stolt\u00b7zer", "Muth", "noch", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es wissen es die B\u00fcsch, die Wiesen, Berg und W\u00e4lder,", "tokens": ["Es", "wis\u00b7sen", "es", "die", "B\u00fcsch", ",", "die", "Wie\u00b7sen", ",", "Berg", "und", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die B\u00e4che hie umher, die Th\u00e4ler und die Felder,", "tokens": ["Die", "B\u00e4\u00b7che", "hie", "um\u00b7her", ",", "die", "Th\u00e4\u00b7ler", "und", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was ich von Michals Sch\u00f6n sing ihnen t\u00e4glich f\u00fcr.", "tokens": ["Was", "ich", "von", "Mic\u00b7hals", "Sch\u00f6n", "sing", "ih\u00b7nen", "t\u00e4g\u00b7lich", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "NE", "VVFIN", "PPER", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ja wann es m\u00f6glich w\u00e4r, da\u00df diese meine Thier", "tokens": ["Ja", "wann", "es", "m\u00f6g\u00b7lich", "w\u00e4r", ",", "da\u00df", "die\u00b7se", "mei\u00b7ne", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Verm\u00f6chten mit der Sprach zu zeugen, was sie h\u00f6ren,", "tokens": ["Ver\u00b7m\u00f6ch\u00b7ten", "mit", "der", "Sprach", "zu", "zeu\u00b7gen", ",", "was", "sie", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie w\u00fcrden itzt mein Wort mit ihrem Ja bew\u00e4hren,", "tokens": ["Sie", "w\u00fcr\u00b7den", "itzt", "mein", "Wort", "mit", "ih\u00b7rem", "Ja", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und weil nun, wie gesagt, ich so gl\u00fcckselig bin,", "tokens": ["Und", "weil", "nun", ",", "wie", "ge\u00b7sagt", ",", "ich", "so", "gl\u00fcck\u00b7se\u00b7lig", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "PWAV", "VVPP", "$,", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.36": {"text": "Erwart ich den Befehl, was ich soll, und worinn", "tokens": ["Er\u00b7wart", "ich", "den", "Be\u00b7fehl", ",", "was", "ich", "soll", ",", "und", "wo\u00b7rinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWS", "PPER", "VMFIN", "$,", "KON", "PWAV"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Ich dienen kan allhie; was kan der Landmann geben,", "tokens": ["Ich", "die\u00b7nen", "kan", "all\u00b7hie", ";", "was", "kan", "der", "Land\u00b7mann", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ADV", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bieth ich gehorsamst an. Das schlechte Hirtenleben", "tokens": ["Bieth", "ich", "ge\u00b7hor\u00b7samst", "an", ".", "Das", "schlech\u00b7te", "Hir\u00b7ten\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vermag nicht gar zu viel. Die Michal wird err\u00f6th,", "tokens": ["Ver\u00b7mag", "nicht", "gar", "zu", "viel", ".", "Die", "Mic\u00b7hal", "wird", "er\u00b7r\u00f6th", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKA", "PIS", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und schweiget hiezu still, das schier den David t\u00f6dt,", "tokens": ["Und", "schwei\u00b7get", "hie\u00b7zu", "still", ",", "das", "schier", "den", "Da\u00b7vid", "t\u00f6dt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKVZ", "$,", "PRELS", "ADJD", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Weil er denckt, seine Wort die haben sie betr\u00fcbet,", "tokens": ["Weil", "er", "denckt", ",", "sei\u00b7ne", "Wort", "die", "ha\u00b7ben", "sie", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ART", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und da\u00df es diese sey, die Adrielen liebet,", "tokens": ["Und", "da\u00df", "es", "die\u00b7se", "sey", ",", "die", "Ad\u00b7rie\u00b7len", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Dahero seine Wort nicht wohl genommen an;", "tokens": ["Da\u00b7he\u00b7ro", "sei\u00b7ne", "Wort", "nicht", "wohl", "ge\u00b7nom\u00b7men", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Er liebt sie allbereits, ohn da\u00df er hoffen kan.", "tokens": ["Er", "liebt", "sie", "all\u00b7be\u00b7reits", ",", "ohn", "da\u00df", "er", "hof\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUI", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Indem sagt sie zu ihm: Ich bin hieher gekommen", "tokens": ["In\u00b7dem", "sagt", "sie", "zu", "ihm", ":", "Ich", "bin", "hie\u00b7her", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VAFIN", "PAV", "VVPP"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.46": {"text": "Wohl recht von ungefehr, dann wir uns f\u00fcrgenommen,", "tokens": ["Wohl", "recht", "von", "un\u00b7ge\u00b7fehr", ",", "dann", "wir", "uns", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJD", "$,", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Noch gestern wiederum in Gibea zu seyn;", "tokens": ["Noch", "ge\u00b7stern", "wie\u00b7de\u00b7rum", "in", "Gi\u00b7bea", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NE", "PTKZU", "VAINF", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.48": {"text": "Doch ward es uns zu sp\u00e4t, und fiel ein Wetter ein,", "tokens": ["Doch", "ward", "es", "uns", "zu", "sp\u00e4t", ",", "und", "fiel", "ein", "Wet\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "PTKA", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Das also heftig war, da\u00df uns hier zu verbleiben", "tokens": ["Das", "al\u00b7so", "hef\u00b7tig", "war", ",", "da\u00df", "uns", "hier", "zu", "ver\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die h\u00f6chste Noth befahl. Ich werd den Tag anschreiben,", "tokens": ["Die", "h\u00f6chs\u00b7te", "Noth", "be\u00b7fahl", ".", "Ich", "werd", "den", "Tag", "an\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sprach David, der mir bringt so unverdiente Gnad,", "tokens": ["Sprach", "Da\u00b7vid", ",", "der", "mir", "bringt", "so", "un\u00b7ver\u00b7dien\u00b7te", "Gnad", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da\u00df ich nun sagen kan, wie da geruhet hat", "tokens": ["Da\u00df", "ich", "nun", "sa\u00b7gen", "kan", ",", "wie", "da", "ge\u00b7ru\u00b7het", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PWAV", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "In meiner H\u00f6l die Zierd von Benjamins Geschlechte,", "tokens": ["In", "mei\u00b7ner", "H\u00f6l", "die", "Zierd", "von", "Ben\u00b7ja\u00b7mins", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ja von gantz Jsrael; Ich prei\u00df mit allem Rechte", "tokens": ["Ja", "von", "gantz", "Js\u00b7rael", ";", "Ich", "prei\u00df", "mit", "al\u00b7lem", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ADV", "NE", "$.", "PPER", "VVFIN", "APPR", "PIS", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.55": {"text": "Hochseelig diesen Tag, f\u00fcr andern ist er hoch.", "tokens": ["Hoch\u00b7see\u00b7lig", "die\u00b7sen", "Tag", ",", "f\u00fcr", "an\u00b7dern", "ist", "er", "hoch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$,", "APPR", "PIS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Der Michal Wangen Zier ein neue R\u00f6th umzog,", "tokens": ["Der", "Mic\u00b7hal", "Wan\u00b7gen", "Zier", "ein", "neu\u00b7e", "R\u00f6th", "um\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Als sie h\u00f6rt dieses Lob, das sie nicht wollte h\u00f6ren,", "tokens": ["Als", "sie", "h\u00f6rt", "die\u00b7ses", "Lob", ",", "das", "sie", "nicht", "woll\u00b7te", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.58": {"text": "Drum bricht sie ab die Red, und thut von ihm begehren,", "tokens": ["Drum", "bricht", "sie", "ab", "die", "Red", ",", "und", "thut", "von", "ihm", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Da\u00df er ihr sagen soll, was das sey f\u00fcr ein Hau\u00df,", "tokens": ["Da\u00df", "er", "ihr", "sa\u00b7gen", "soll", ",", "was", "das", "sey", "f\u00fcr", "ein", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "PWS", "PDS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Das da an jenem Berg f\u00fcr andern schien heraus?", "tokens": ["Das", "da", "an", "je\u00b7nem", "Berg", "f\u00fcr", "an\u00b7dern", "schien", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PDAT", "NN", "APPR", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Der Suri wohnet da, spricht David, dem gegeben", "tokens": ["Der", "Su\u00b7ri", "woh\u00b7net", "da", ",", "spricht", "Da\u00b7vid", ",", "dem", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "$,", "VVFIN", "NE", "$,", "PRELS", "VVPP"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "Zeruja ist zum Weib. Wie? fraget sie daneben,", "tokens": ["Ze\u00b7ru\u00b7ja", "ist", "zum", "Weib", ".", "Wie", "?", "fra\u00b7get", "sie", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "$.", "PWAV", "$.", "VVFIN", "PPER", "PAV", "$,"], "meter": "+--+-+-+---+-", "measure": "iambic.penta.invert"}, "line.63": {"text": "Ist Davids Schwester nicht Zeruja, als ich meyn?", "tokens": ["Ist", "Da\u00b7vids", "Schwes\u00b7ter", "nicht", "Ze\u00b7ru\u00b7ja", ",", "als", "ich", "meyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "PTKNEG", "NE", "$,", "KOUS", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Ja, sprach der Hirt. Sie fragt, ob sie beerbet seyn?", "tokens": ["Ja", ",", "sprach", "der", "Hirt", ".", "Sie", "fragt", ",", "ob", "sie", "be\u00b7er\u00b7bet", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Er saget wieder: Ja. Jhr Wunsch ist sie zu sehen,", "tokens": ["Er", "sa\u00b7get", "wie\u00b7der", ":", "Ja", ".", "Ihr", "Wunsch", "ist", "sie", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Weil sie sie f\u00fcr der Zeit gekannt. Das kan geschehen,", "tokens": ["Weil", "sie", "sie", "f\u00fcr", "der", "Zeit", "ge\u00b7kannt", ".", "Das", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "PDS", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Sprach David, ohne M\u00fch, ich will gleich zu ihr hin.", "tokens": ["Sprach", "Da\u00b7vid", ",", "oh\u00b7ne", "M\u00fch", ",", "ich", "will", "gleich", "zu", "ihr", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUI", "NN", "$,", "PPER", "VMFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Wann ich da, wandt sie ein, nur nicht beschwerlich bin,", "tokens": ["Wann", "ich", "da", ",", "wandt", "sie", "ein", ",", "nur", "nicht", "be\u00b7schwer\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Will ich den Weg mit thun. Das war zu grosse Ehre,", "tokens": ["Will", "ich", "den", "Weg", "mit", "thun", ".", "Das", "war", "zu", "gros\u00b7se", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "VVINF", "$.", "PDS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Spricht David. Nein, sagt sie, wann ich es so degehre,", "tokens": ["Spricht", "Da\u00b7vid", ".", "Nein", ",", "sagt", "sie", ",", "wann", "ich", "es", "so", "de\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "PTKANT", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.71": {"text": "Die Schwestern schlaffen noch, und eh sie sind erwacht,", "tokens": ["Die", "Schwes\u00b7tern", "schlaf\u00b7fen", "noch", ",", "und", "eh", "sie", "sind", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Soll diese kurtze Reis schon seyn zu End gebracht.", "tokens": ["Soll", "die\u00b7se", "kurt\u00b7ze", "Reis", "schon", "seyn", "zu", "End", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "ADV", "VAINF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Erwachen sie dann gleich, will ich ob ihrem Schrecken,", "tokens": ["Er\u00b7wa\u00b7chen", "sie", "dann", "gleich", ",", "will", "ich", "ob", "ih\u00b7rem", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "VMFIN", "PPER", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Da\u00df sie mich finden nicht, nur eine Freud erwecken.", "tokens": ["Da\u00df", "sie", "mich", "fin\u00b7den", "nicht", ",", "nur", "ei\u00b7ne", "Freud", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKNEG", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und weil Zeruja ist der Merob sonders werth,", "tokens": ["Und", "weil", "Ze\u00b7ru\u00b7ja", "ist", "der", "Me\u00b7rob", "son\u00b7ders", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Will ich sie holen selbst. Hiemit der Kutscher f\u00e4hrt.", "tokens": ["Will", "ich", "sie", "ho\u00b7len", "selbst", ".", "Hie\u00b7mit", "der", "Kut\u00b7scher", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$.", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Der David der bey ihr in ihrem Wagen sitzet,", "tokens": ["Der", "Da\u00b7vid", "der", "bey", "ihr", "in", "ih\u00b7rem", "Wa\u00b7gen", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "APPR", "PPOSAT", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.78": {"text": "Wird von so hoher G\u00fct je mehr und mehr erhitzet,", "tokens": ["Wird", "von", "so", "ho\u00b7her", "G\u00fct", "je", "mehr", "und", "mehr", "er\u00b7hit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Doch d\u00e4mpft er diese Flamm, die eh nicht soll entstehn,", "tokens": ["Doch", "d\u00e4mpft", "er", "die\u00b7se", "Flamm", ",", "die", "eh", "nicht", "soll", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "KOUS", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Bis da\u00df er wei\u00df von wem er sich geliebt wird sehn.", "tokens": ["Bis", "da\u00df", "er", "wei\u00df", "von", "wem", "er", "sich", "ge\u00b7liebt", "wird", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPR", "PWS", "PPER", "PRF", "VVPP", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}