{"textgrid.poem.65854": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "128", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft, o meine Muse! wenn dein Finger", "tokens": ["Wie", "oft", ",", "o", "mei\u00b7ne", "Mu\u00b7se", "!", "wenn", "dein", "Fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "FM", "PPOSAT", "NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dem begl\u00fcckten Holz Musik entspann", "tokens": ["Aus", "dem", "be\u00b7gl\u00fcck\u00b7ten", "Holz", "Mu\u00b7sik", "ent\u00b7spann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jenen Wohllaut, meines Ohrs Bezwinger,", "tokens": ["Und", "je\u00b7nen", "Wohl\u00b7laut", ",", "mei\u00b7nes", "Ohrs", "Be\u00b7zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit s\u00fc\u00dfem Griff den Saiten abgewann,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Griff", "den", "Sai\u00b7ten", "ab\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beneidet' ich die Tasten, wie zu nippen", "tokens": ["Be\u00b7nei\u00b7det'", "ich", "die", "Tas\u00b7ten", ",", "wie", "zu", "nip\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie deinen zarten H\u00e4nden eilig nah'n,", "tokens": ["Sie", "dei\u00b7nen", "zar\u00b7ten", "H\u00e4n\u00b7den", "ei\u00b7lig", "nah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Indes err\u00f6tend meine armen Lippen", "tokens": ["In\u00b7des", "er\u00b7r\u00f6\u00b7tend", "mei\u00b7ne", "ar\u00b7men", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An k\u00fchnes Holz ihr Recht verschwendet sah'n.", "tokens": ["An", "k\u00fch\u00b7nes", "Holz", "ihr", "Recht", "ver\u00b7schwen\u00b7det", "sah'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie m\u00f6chten sie um solch Ber\u00fchren tauschen", "tokens": ["Wie", "m\u00f6ch\u00b7ten", "sie", "um", "solch", "Be\u00b7r\u00fch\u00b7ren", "tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit jedem Sp\u00e4nlein, das sich tanzend b\u00fcckt,", "tokens": ["Mit", "je\u00b7dem", "Sp\u00e4n\u00b7lein", ",", "das", "sich", "tan\u00b7zend", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn deiner Wanderfinger leises Rauschen", "tokens": ["Wenn", "dei\u00b7ner", "Wan\u00b7der\u00b7fin\u00b7ger", "lei\u00b7ses", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mehr totes Holz als roten Mund begl\u00fcckt!", "tokens": ["Mehr", "to\u00b7tes", "Holz", "als", "ro\u00b7ten", "Mund", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KOUS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn kecke Tasten denn so schwelgen m\u00fcssen,", "tokens": ["Wenn", "ke\u00b7cke", "Tas\u00b7ten", "denn", "so", "schwel\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "La\u00df sie die Hand, la\u00df mich die Lippen k\u00fcssen.", "tokens": ["La\u00df", "sie", "die", "Hand", ",", "la\u00df", "mich", "die", "Lip\u00b7pen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}