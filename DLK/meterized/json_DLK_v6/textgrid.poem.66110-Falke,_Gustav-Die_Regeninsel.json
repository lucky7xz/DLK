{"textgrid.poem.66110": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Regeninsel", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus eines fernen Ozeans grauen Wassern,", "tokens": ["Aus", "ei\u00b7nes", "fer\u00b7nen", "O\u00b7ze\u00b7ans", "grau\u00b7en", "Was\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die nie ein Sturm aus ihrer Ruhe r\u00fcttelt,", "tokens": ["Die", "nie", "ein", "Sturm", "aus", "ih\u00b7rer", "Ru\u00b7he", "r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ragt unter schwerem, ewig tr\u00fcbem Himmel", "tokens": ["Ragt", "un\u00b7ter", "schwe\u00b7rem", ",", "e\u00b7wig", "tr\u00fc\u00b7bem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In flachem Anstieg eine stille Insel.", "tokens": ["In", "fla\u00b7chem", "An\u00b7stieg", "ei\u00b7ne", "stil\u00b7le", "In\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "So lang des Meeres schl\u00e4frig tr\u00e4ge Wellen", "tokens": ["So", "lang", "des", "Mee\u00b7res", "schl\u00e4f\u00b7rig", "tr\u00e4\u00b7ge", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit schmutzig gelbem Schaum den Strand umkr\u00e4nzen,", "tokens": ["Mit", "schmut\u00b7zig", "gel\u00b7bem", "Schaum", "den", "Strand", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Seit tausenden von Jahren, rieselt endlos", "tokens": ["Seit", "tau\u00b7sen\u00b7den", "von", "Jah\u00b7ren", ",", "rie\u00b7selt", "end\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "APPR", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Derselbe sanfte Regen aus den Wolken", "tokens": ["Der\u00b7sel\u00b7be", "sanf\u00b7te", "Re\u00b7gen", "aus", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und n\u00e4\u00dft den Boden, dessen \u00fcppige Wildnis", "tokens": ["Und", "n\u00e4\u00dft", "den", "Bo\u00b7den", ",", "des\u00b7sen", "\u00fcp\u00b7pi\u00b7ge", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die Feuchte trinkt mit immer durstigem Mund.", "tokens": ["Die", "Feuch\u00b7te", "trinkt", "mit", "im\u00b7mer", "durs\u00b7ti\u00b7gem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Und ewig plauscht und plantscht und plitscht und platscht es.", "tokens": ["Und", "e\u00b7wig", "plauscht", "und", "plantscht", "und", "plitscht", "und", "platscht", "es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eint\u00f6nig, rhythmenlos, tropfts von den Zweigen,", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", ",", "rhyth\u00b7men\u00b7los", ",", "tropfts", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gluckst seufzend von den Ranken, f\u00e4llt von Halmen", "tokens": ["Gluckst", "seuf\u00b7zend", "von", "den", "Ran\u00b7ken", ",", "f\u00e4llt", "von", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Tr\u00e4nen ab und klatscht in tausend T\u00fcmpel,", "tokens": ["Wie", "Tr\u00e4\u00b7nen", "ab", "und", "klatscht", "in", "tau\u00b7send", "T\u00fcm\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lehmfarbige Lachen, und verspritzt, zerst\u00e4ubt.", "tokens": ["Lehm\u00b7far\u00b7bi\u00b7ge", "La\u00b7chen", ",", "und", "ver\u00b7spritzt", ",", "zer\u00b7st\u00e4ubt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Baumriesen, deren nasse, blanke \u00c4ste", "tokens": ["Baum\u00b7rie\u00b7sen", ",", "de\u00b7ren", "nas\u00b7se", ",", "blan\u00b7ke", "\u00c4s\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "$,", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Schlammfransen schm\u00fccken, als ob gestern erst", "tokens": ["Schlamm\u00b7fran\u00b7sen", "schm\u00fc\u00b7cken", ",", "als", "ob", "ge\u00b7stern", "erst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KOKOM", "KOUS", "ADV", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Insel aus den Fluten sich erhoben,", "tokens": ["Die", "In\u00b7sel", "aus", "den", "Flu\u00b7ten", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beschatten m\u00e4chtige Farrenwedelw\u00e4lder", "tokens": ["Be\u00b7schat\u00b7ten", "m\u00e4ch\u00b7ti\u00b7ge", "Far\u00b7ren\u00b7we\u00b7del\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und dicke, fleischige, tellerf\u00f6rmige Bl\u00e4tter", "tokens": ["Und", "di\u00b7cke", ",", "flei\u00b7schi\u00b7ge", ",", "tel\u00b7ler\u00b7f\u00f6r\u00b7mi\u00b7ge", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Von Sumpfgew\u00e4chsen rings und hochgestielte", "tokens": ["Von", "Sumpf\u00b7ge\u00b7w\u00e4ch\u00b7sen", "rings", "und", "hoch\u00b7ges\u00b7tiel\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Farblose Blumen, die in schwammigen Kelchen", "tokens": ["Farb\u00b7lo\u00b7se", "Blu\u00b7men", ",", "die", "in", "schwam\u00b7mi\u00b7gen", "Kel\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Den Regen fangen, der in feinen B\u00e4chen", "tokens": ["Den", "Re\u00b7gen", "fan\u00b7gen", ",", "der", "in", "fei\u00b7nen", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der schwanken Becken R\u00e4nder \u00fcberrinnt,", "tokens": ["Der", "schwan\u00b7ken", "Be\u00b7cken", "R\u00e4n\u00b7der", "\u00fc\u00b7berr\u00b7innt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und ewig plauscht und plantscht und plitscht und platscht es.", "tokens": ["Und", "e\u00b7wig", "plauscht", "und", "plantscht", "und", "plitscht", "und", "platscht", "es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fremdartige V\u00f6gel horsten auf den B\u00e4umen", "tokens": ["Fremd\u00b7ar\u00b7ti\u00b7ge", "V\u00f6\u00b7gel", "hors\u00b7ten", "auf", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit fettigem, \u00f6lgl\u00e4nzendem Gefieder", "tokens": ["Mit", "fet\u00b7ti\u00b7gem", ",", "\u00f6l\u00b7gl\u00e4n\u00b7zen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schwarzem, abgestumpftem Entenschnabel.", "tokens": ["Und", "schwar\u00b7zem", ",", "ab\u00b7ge\u00b7stumpf\u00b7tem", "En\u00b7ten\u00b7schna\u00b7bel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus lehmiger Erde bauen sie die Nester", "tokens": ["Aus", "leh\u00b7mi\u00b7ger", "Er\u00b7de", "bau\u00b7en", "sie", "die", "Nes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Den schlick- und schlammumh\u00fcllten Waldkolossen", "tokens": ["Den", "schlick", "und", "schlam\u00b7mum\u00b7h\u00fcll\u00b7ten", "Wald\u00b7ko\u00b7los\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihre breiten Arme. Klagend klingen,", "tokens": ["In", "ih\u00b7re", "brei\u00b7ten", "Ar\u00b7me", ".", "Kla\u00b7gend", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gebrochen, schrill, die wunderlichen Rufe", "tokens": ["Ge\u00b7bro\u00b7chen", ",", "schrill", ",", "die", "wun\u00b7der\u00b7li\u00b7chen", "Ru\u00b7fe"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der gro\u00dfen grauen Tiere, die mit leisem,", "tokens": ["Der", "gro\u00b7\u00dfen", "grau\u00b7en", "Tie\u00b7re", ",", "die", "mit", "lei\u00b7sem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Fast regungslosem Flug die weite \u00d6de", "tokens": ["Fast", "re\u00b7gungs\u00b7lo\u00b7sem", "Flug", "die", "wei\u00b7te", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der See bestreichen und nach Fischen fahnden.", "tokens": ["Der", "See", "be\u00b7strei\u00b7chen", "und", "nach", "Fi\u00b7schen", "fahn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seltsame, stumme Stelzenv\u00f6gel jagen", "tokens": ["Selt\u00b7sa\u00b7me", ",", "stum\u00b7me", "Stel\u00b7zen\u00b7v\u00f6\u00b7gel", "ja\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Im Sumpf nach feisten, plumpen Riesenfr\u00f6schen,", "tokens": ["Im", "Sumpf", "nach", "feis\u00b7ten", ",", "plum\u00b7pen", "Rie\u00b7sen\u00b7fr\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fabelhafte Wesen, halb der Otter,", "tokens": ["Und", "fa\u00b7bel\u00b7haf\u00b7te", "We\u00b7sen", ",", "halb", "der", "Ot\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Halb einem Eichhorn gleich, mit Flatterfl\u00fcgeln", "tokens": ["Halb", "ei\u00b7nem", "Eich\u00b7horn", "gleich", ",", "mit", "Flat\u00b7ter\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie eine Fledermaus, nur gr\u00f6\u00dfer, f\u00fchren", "tokens": ["Wie", "ei\u00b7ne", "Fle\u00b7der\u00b7maus", ",", "nur", "gr\u00f6\u00b7\u00dfer", ",", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein wunderliches, drolliges Doppelleben,", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7ches", ",", "drol\u00b7li\u00b7ges", "Dop\u00b7pel\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Halb Vogel und halb Fisch, in all dem Na\u00df.", "tokens": ["Halb", "Vo\u00b7gel", "und", "halb", "Fisch", ",", "in", "all", "dem", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "NN", "$,", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ewig plauscht und plantscht und plitscht und platscht es.", "tokens": ["Und", "e\u00b7wig", "plauscht", "und", "plantscht", "und", "plitscht", "und", "platscht", "es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch m\u00e4rchenhafter noch als diese Tiere", "tokens": ["Doch", "m\u00e4r\u00b7chen\u00b7haf\u00b7ter", "noch", "als", "die\u00b7se", "Tie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind hier die Menschen. Klein, breitm\u00e4ulig, schielend,", "tokens": ["Sind", "hier", "die", "Men\u00b7schen", ".", "Klein", ",", "breit\u00b7m\u00e4u\u00b7lig", ",", "schie\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Karpfenaugen unter wulstigen Lidern,", "tokens": ["Mit", "Kar\u00b7pfen\u00b7au\u00b7gen", "un\u00b7ter", "wuls\u00b7ti\u00b7gen", "Li\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und fischgeschw\u00e4nzt, Schwimmh\u00e4ute an den H\u00e4nden,", "tokens": ["Und", "fischge\u00b7schw\u00e4nzt", ",", "Schwimm\u00b7h\u00e4u\u00b7te", "an", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", 