{"textgrid.poem.31692": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte wandern an die Lahn,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wan\u00b7dern", "an", "die", "Lahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl an die Lahn zur Stunde,", "tokens": ["Wohl", "an", "die", "Lahn", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo auf den Wellen kreist der Schwan,", "tokens": ["Wo", "auf", "den", "Wel\u00b7len", "kreist", "der", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wo auf den D\u00f6rfern singt der Hahn", "tokens": ["Wo", "auf", "den", "D\u00f6r\u00b7fern", "singt", "der", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit hoch prophet'schem Munde.", "tokens": ["Mit", "hoch", "pro\u00b7phet'\u00b7schem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo lind die Luft, wo klar die Flut,", "tokens": ["Wo", "lind", "die", "Luft", ",", "wo", "klar", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo tr\u00e4umerisch das falbe,", "tokens": ["Wo", "tr\u00e4u\u00b7me\u00b7risch", "das", "fal\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das falbe Ro\u00df im Grase ruht,", "tokens": ["Das", "fal\u00b7be", "Ro\u00df", "im", "Gra\u00b7se", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo oft den Flug ins Freie tut", "tokens": ["Wo", "oft", "den", "Flug", "ins", "Frei\u00b7e", "tut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Taube und die Schwalbe.", "tokens": ["Die", "Tau\u00b7be", "und", "die", "Schwal\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo mittags aus dem Laubgewind", "tokens": ["Wo", "mit\u00b7tags", "aus", "dem", "Laub\u00b7ge\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die braunen Hirsche sehen,", "tokens": ["Die", "brau\u00b7nen", "Hir\u00b7sche", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo still die weiten T\u00e4ler sind,", "tokens": ["Wo", "still", "die", "wei\u00b7ten", "T\u00e4\u00b7ler", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo fl\u00fcsternd wohl im Abendwind", "tokens": ["Wo", "fl\u00fcs\u00b7ternd", "wohl", "im", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die jungen Buchen wehen.", "tokens": ["Die", "jun\u00b7gen", "Bu\u00b7chen", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo hoch vom Berg die Rebe sieht", "tokens": ["Wo", "hoch", "vom", "Berg", "die", "Re\u00b7be", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ranken gr\u00fcn und krause,", "tokens": ["Mit", "Ran\u00b7ken", "gr\u00fcn", "und", "krau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo wild die rote Rose gl\u00fcht", "tokens": ["Wo", "wild", "die", "ro\u00b7te", "Ro\u00b7se", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo die sch\u00f6nste Tochter bl\u00fcht", "tokens": ["Und", "wo", "die", "sch\u00f6ns\u00b7te", "Toch\u00b7ter", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In einem wei\u00dfen Hause.", "tokens": ["In", "ei\u00b7nem", "wei\u00b7\u00dfen", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}