{"dta.poem.20755": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ueber ein Gem\u00e4lde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist in ihrem vollen Reitze               ", "tokens": ["Dies", "ist", "in", "ih\u00b7rem", "vol\u00b7len", "Reit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flora, die mit Liebesgeitze", "tokens": ["Die", "Flo\u00b7ra", ",", "die", "mit", "Lie\u00b7bes\u00b7geit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich Pompejus von ihr ri\u00df", "tokens": ["Wenn", "sich", "Pom\u00b7pe\u00b7jus", "von", "ihr", "ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NE", "APPR", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihn feurig in die Lippen bi\u00df.", "tokens": ["Ihn", "feu\u00b7rig", "in", "die", "Lip\u00b7pen", "bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lieblich aufgeschwollen lachte", "tokens": ["So", "lieb\u00b7lich", "auf\u00b7ge\u00b7schwol\u00b7len", "lach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVIZU", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Mund; so schwamm ihr Augenpaar", "tokens": ["Ihr", "Mund", ";", "so", "schwamm", "ihr", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Wollust, so erhub ihr goldgelocktes Haar", "tokens": ["In", "Wol\u00b7lust", ",", "so", "er\u00b7hub", "ihr", "gold\u00b7ge\u00b7lock\u00b7tes", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich um die Schultern, wenn der Ku\u00df sie trunken machte", "tokens": ["Sich", "um", "die", "Schul\u00b7tern", ",", "wenn", "der", "Ku\u00df", "sie", "trun\u00b7ken", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ihr Gef\u00fchl Entz\u00fcckung war.", "tokens": ["Und", "ihr", "Ge\u00b7f\u00fchl", "Ent\u00b7z\u00fc\u00b7ckung", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}