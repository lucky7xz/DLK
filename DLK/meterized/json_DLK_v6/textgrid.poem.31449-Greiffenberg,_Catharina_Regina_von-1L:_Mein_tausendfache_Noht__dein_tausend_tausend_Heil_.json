{"textgrid.poem.31449": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein tausendfache Noht / dein tausend tausend Heil /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein tausendfache Noht / dein tausend tausend Heil /", "tokens": ["Mein", "tau\u00b7send\u00b7fa\u00b7che", "Noht", "/", "dein", "tau\u00b7send", "tau\u00b7send", "Heil", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "CARD", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O \u00fcber-guter Gott / dem\u00fctiglich anflehet.", "tokens": ["O", "\u00fc\u00b7ber\u00b7gu\u00b7ter", "Gott", "/", "de\u00b7m\u00fc\u00b7tig\u00b7lich", "an\u00b7fle\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein' Elends-tieffe bey dem gnaden-Sandberg stehet:", "tokens": ["Mein'", "E\u00b7lends\u00b7tief\u00b7fe", "bey", "dem", "gna\u00b7den\u00b7Sand\u00b7berg", "ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "w\u00fcrd nur ein K\u00f6rnlein gro\u00df von diesem mir zu theil!", "tokens": ["w\u00fcrd", "nur", "ein", "K\u00f6rn\u00b7lein", "gro\u00df", "von", "die\u00b7sem", "mir", "zu", "theil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "APPR", "PDAT", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "doch ist die Allheit hie um nichts (O wunder) feil:", "tokens": ["doch", "ist", "die", "All\u00b7heit", "hie", "um", "nichts", "(", "O", "wun\u00b7der", ")", "feil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "PIS", "$(", "NE", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der Glaub' all ihre Krafft / ja selbsten sie / empf\u00e4het.", "tokens": ["der", "Glaub'", "all", "ih\u00b7re", "Krafft", "/", "ja", "selbs\u00b7ten", "sie", "/", "empf\u00b7\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "$(", "VVFIN", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die Seeligkeit der Geist in diesen Segel wehet /", "tokens": ["Die", "See\u00b7lig\u00b7keit", "der", "Geist", "in", "die\u00b7sen", "Se\u00b7gel", "we\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der in den Hafen bringt das Schiff mit Pfeiles-eil.", "tokens": ["der", "in", "den", "Ha\u00b7fen", "bringt", "das", "Schiff", "mit", "Pfei\u00b7les\u00b7eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin bereit im Port / und mein Port ist in mir /", "tokens": ["Ich", "bin", "be\u00b7reit", "im", "Port", "/", "und", "mein", "Port", "ist", "in", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NE", "$(", "KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "auch mitten in dem Meer: was darf die Flut mich scherzen?", "tokens": ["auch", "mit\u00b7ten", "in", "dem", "Meer", ":", "was", "darf", "die", "Flut", "mich", "scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich hab' an Jesu Christ das Land und Strand im Herzen.", "tokens": ["Ich", "hab'", "an", "Je\u00b7su", "Christ", "das", "Land", "und", "Strand", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NE", "ART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Schiffbruch f\u00fcrcht ich nicht / gesch\u00e4h' er auch nun schier.", "tokens": ["Den", "Schiff\u00b7bruch", "f\u00fcrcht", "ich", "nicht", "/", "ge\u00b7sch\u00e4\u00b7h'", "er", "auch", "nun", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "in meines Jesus Scho\u00df / in Gott des Vatters H\u00e4nde /", "tokens": ["in", "mei\u00b7nes", "Je\u00b7sus", "Scho\u00df", "/", "in", "Gott", "des", "Vat\u00b7ters", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "APPR", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und in des Geistes freud' / ich mit dem Geist anl\u00e4nde.", "tokens": ["und", "in", "des", "Geis\u00b7tes", "freud'", "/", "ich", "mit", "dem", "Geist", "an\u00b7l\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$(", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}