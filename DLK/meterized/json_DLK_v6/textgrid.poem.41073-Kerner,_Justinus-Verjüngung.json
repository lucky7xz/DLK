{"textgrid.poem.41073": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Verj\u00fcngung", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich wieder mich verj\u00fcngen!", "tokens": ["K\u00f6nnt'", "ich", "wie\u00b7der", "mich", "ver\u00b7j\u00fcn\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich wachsen wie ein Kind!", "tokens": ["K\u00f6nnt'", "ich", "wach\u00b7sen", "wie", "ein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugend! wie bist du verflogen!", "tokens": ["Ju\u00b7gend", "!", "wie", "bist", "du", "ver\u00b7flo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Alter, wie kamst du geschwind!", "tokens": ["Al\u00b7ter", ",", "wie", "kamst", "du", "ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Hab' Geduld, wirst dich verj\u00fcngen!", "tokens": ["Hab'", "Ge\u00b7duld", ",", "wirst", "dich", "ver\u00b7j\u00fcn\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der allerletzten Stund'", "tokens": ["In", "der", "al\u00b7ler\u00b7letz\u00b7ten", "Stund'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich verj\u00fcngen oft die Z\u00fcge", "tokens": ["Sich", "ver\u00b7j\u00fcn\u00b7gen", "oft", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbender noch um den Mund.", "tokens": ["Ster\u00b7ben\u00b7der", "noch", "um", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Hab' Geduld, du wirst noch wachsen!", "tokens": ["Hab'", "Ge\u00b7duld", ",", "du", "wirst", "noch", "wach\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Geist verl\u00e4\u00dft sein Haus,", "tokens": ["Wenn", "der", "Geist", "ver\u00b7l\u00e4\u00dft", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strecken sich der Leiche Glieder", "tokens": ["Stre\u00b7cken", "sich", "der", "Lei\u00b7che", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft noch wie zum Wachstum aus.", "tokens": ["Oft", "noch", "wie", "zum", "Wachs\u00b7tum", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}