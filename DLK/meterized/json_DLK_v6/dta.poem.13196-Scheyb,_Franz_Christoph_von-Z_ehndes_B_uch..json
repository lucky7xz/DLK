{"dta.poem.13196": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df, als h\u00e4tte die\u00df den guten Mann gest\u00f6rt;", "tokens": ["Es", "lie\u00df", ",", "als", "h\u00e4t\u00b7te", "die\u00df", "den", "gu\u00b7ten", "Mann", "ge\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PDS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil wir von seinem Mund hierauf nichts mehr geh\u00f6rt.", "tokens": ["Weil", "wir", "von", "sei\u00b7nem", "Mund", "hier\u00b7auf", "nichts", "mehr", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "155Er neigte sich, wie vor, und gab sich auf die Seite,", "tokens": ["neig\u00b7te", "sich", ",", "wie", "vor", ",", "und", "gab", "sich", "auf", "die", "Sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWAV", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Indem es schien, da\u00df sie zum reden sich bereite.", "tokens": ["In\u00b7dem", "es", "schien", ",", "da\u00df", "sie", "zum", "re\u00b7den", "sich", "be\u00b7rei\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja! sie erhob das Haupt mit freundlichem Gesicht,", "tokens": ["Ja", "!", "sie", "er\u00b7hob", "das", "Haupt", "mit", "freund\u00b7li\u00b7chem", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihre Rede war zum Tugend-Chor gericht:", "tokens": ["Und", "ih\u00b7re", "Re\u00b7de", "war", "zum", "Tu\u00b7gen\u00b7dChor", "ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}