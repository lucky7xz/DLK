{"textgrid.poem.27645": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Musa hat kaum einen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Musa hat kaum einen,", "tokens": ["Mei\u00b7ne", "Mu\u00b7sa", "hat", "kaum", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ihr Ph\u00f6bus wil erscheinen.", "tokens": ["Der", "ihr", "Ph\u00f6\u00b7bus", "wil", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar genug! Sie hat alleine,", "tokens": ["Gar", "ge\u00b7nug", "!", "Sie", "hat", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was f\u00fcr sich sonst in gemeine", "tokens": ["Was", "f\u00fcr", "sich", "sonst", "in", "ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PRF", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle dreimal drei G\u00f6ttinnen", "tokens": ["Al\u00b7le", "drei\u00b7mal", "drei", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser Zeit kaum haben k\u00f6nnen.", "tokens": ["Die\u00b7ser", "Zeit", "kaum", "ha\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VAINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}