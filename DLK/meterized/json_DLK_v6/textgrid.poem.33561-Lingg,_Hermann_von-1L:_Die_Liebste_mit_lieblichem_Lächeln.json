{"textgrid.poem.33561": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebste mit lieblichem L\u00e4cheln", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebste mit lieblichem L\u00e4cheln", "tokens": ["Die", "Liebs\u00b7te", "mit", "lieb\u00b7li\u00b7chem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hat meinen Schlummer bewacht.", "tokens": ["Hat", "mei\u00b7nen", "Schlum\u00b7mer", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O hellgestirnter \u00c4ther,", "tokens": ["O", "hell\u00b7ges\u00b7tirn\u00b7ter", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O einzig sch\u00f6ne Nacht!", "tokens": ["O", "ein\u00b7zig", "sch\u00f6\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah sich \u00fcber mich neigen", "tokens": ["Ich", "sah", "sich", "\u00fc\u00b7ber", "mich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Traum ihr holdes Gesicht,", "tokens": ["Im", "Traum", "ihr", "hol\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das sorgende, sinnende Schweigen", "tokens": ["Das", "sor\u00b7gen\u00b7de", ",", "sin\u00b7nen\u00b7de", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erschien ihr so s\u00fc\u00dfe Pflicht.", "tokens": ["Er\u00b7schien", "ihr", "so", "s\u00fc\u00b7\u00dfe", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich bin an einem Verr\u00e4ter,", "tokens": ["Ich", "bin", "an", "ei\u00b7nem", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An ihrem Ku\u00df erwacht \u2013", "tokens": ["An", "ih\u00b7rem", "Ku\u00df", "er\u00b7wacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O hellgestirnter \u00c4ther,", "tokens": ["O", "hell\u00b7ges\u00b7tirn\u00b7ter", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O einzig sch\u00f6ne Nacht!", "tokens": ["O", "ein\u00b7zig", "sch\u00f6\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}