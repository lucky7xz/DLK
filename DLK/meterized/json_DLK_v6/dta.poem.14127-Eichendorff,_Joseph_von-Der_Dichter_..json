{"dta.poem.14127": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh', schon ist die Sonn' gesunken", "tokens": ["Sieh'", ",", "schon", "ist", "die", "Sonn'", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der dunkelblauen Schw\u00fcle,", "tokens": ["Aus", "der", "dun\u00b7kel\u00b7blau\u00b7en", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zerspringt in tausend Funken", "tokens": ["Und", "zer\u00b7springt", "in", "tau\u00b7send", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Felsen rings und B\u00e4umen,", "tokens": ["An", "den", "Fel\u00b7sen", "rings", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis sie alle seelig tr\u00e4umen.", "tokens": ["Bis", "sie", "al\u00b7le", "see\u00b7lig", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Sternen in der K\u00fchle", "tokens": ["Mit", "den", "Ster\u00b7nen", "in", "der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fch'n da W\u00fcnsche, steigen Lieder", "tokens": ["Bl\u00fch'n", "da", "W\u00fcn\u00b7sche", ",", "stei\u00b7gen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus des Herzens Himmelsgrund,", "tokens": ["Aus", "des", "Her\u00b7zens", "Him\u00b7mels\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und ich f\u00fchle alles wieder:", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "al\u00b7les", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alte Freuden, junges Wagen! \u2014", "tokens": ["Al\u00b7te", "Freu\u00b7den", ",", "jun\u00b7ges", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach! so viel m\u00f6cht' ich Dir sagen,", "tokens": ["Ach", "!", "so", "viel", "m\u00f6cht'", "ich", "Dir", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sagen recht aus Herzensgrund,", "tokens": ["Sa\u00b7gen", "recht", "aus", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "In dem Rauschen, in dem Wehen", "tokens": ["In", "dem", "Rau\u00b7schen", ",", "in", "dem", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "M\u00f6cht' ich fr\u00f6hlich mit Dir gehen,", "tokens": ["M\u00f6cht'", "ich", "fr\u00f6h\u00b7lich", "mit", "Dir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Plaudern in der lauen Nacht,", "tokens": ["Plau\u00b7dern", "in", "der", "lau\u00b7en", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Bis der Morgenstern erwacht! \u2014", "tokens": ["Bis", "der", "Mor\u00b7gens\u00b7tern", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+---+", "measure": "anapaest.init"}}}}}