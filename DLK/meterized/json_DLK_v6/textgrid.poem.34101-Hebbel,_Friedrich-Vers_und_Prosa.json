{"textgrid.poem.34101": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vers und Prosa", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leichter w\u00e4re auf einmal der Vers, als die Prosa, geworden?", "tokens": ["Leich\u00b7ter", "w\u00e4\u00b7re", "auf", "ein\u00b7mal", "der", "Vers", ",", "als", "die", "Pro\u00b7sa", ",", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "VAPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schwerer ist er wofern ihr ihn vortrefflich verlangt,", "tokens": ["Schwe\u00b7rer", "ist", "er", "wo\u00b7fern", "ihr", "ihn", "vor\u00b7treff\u00b7lich", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn mit jeglichem Reiz der Prosa mu\u00df er sich schm\u00fccken,", "tokens": ["Denn", "mit", "jeg\u00b7li\u00b7chem", "Reiz", "der", "Pro\u00b7sa", "mu\u00df", "er", "sich", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und mit dem h\u00f6heren noch, den man an ihr nicht vermi\u00dft.", "tokens": ["Und", "mit", "dem", "h\u00f6\u00b7he\u00b7ren", "noch", ",", "den", "man", "an", "ihr", "nicht", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "$,", "PRELS", "PIS", "APPR", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Wenn ihr ihm einen erla\u00dft, so wird's euch der Dichter nicht danken,", "tokens": ["Wenn", "ihr", "ihm", "ei\u00b7nen", "er\u00b7la\u00dft", ",", "so", "wird's", "euch", "der", "Dich\u00b7ter", "nicht", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Denn ihr ebnet dadurch einzig dem St\u00fcmper die Bahn.", "tokens": ["Denn", "ihr", "eb\u00b7net", "da\u00b7durch", "ein\u00b7zig", "dem", "St\u00fcm\u00b7per", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aber, so seid ihr, ihr setzt, damit nur Jeder ein K\u00fcnstler", "tokens": ["A\u00b7ber", ",", "so", "seid", "ihr", ",", "ihr", "setzt", ",", "da\u00b7mit", "nur", "Je\u00b7der", "ein", "K\u00fcnst\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "KOUS", "ADV", "PIS", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Hei\u00dfe, ruhig die Kunst unter sich selbst erst herab,", "tokens": ["Hei\u00b7\u00dfe", ",", "ru\u00b7hig", "die", "Kunst", "un\u00b7ter", "sich", "selbst", "erst", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJD", "ART", "NN", "APPR", "PRF", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Und da pfeifende Knaben das Nichts nun zu leisten verm\u00f6gen,", "tokens": ["Und", "da", "pfei\u00b7fen\u00b7de", "Kna\u00b7ben", "das", "Nichts", "nun", "zu", "leis\u00b7ten", "ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "ART", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Das ihr fordert, so denkt ihr sie euch selbst als ein Nichts!", "tokens": ["Das", "ihr", "for\u00b7dert", ",", "so", "denkt", "ihr", "sie", "euch", "selbst", "als", "ein", "Nichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PPER", "ADV", "KOUS", "ART", "PIS", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}}}}