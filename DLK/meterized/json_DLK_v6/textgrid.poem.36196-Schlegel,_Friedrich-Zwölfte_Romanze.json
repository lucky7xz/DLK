{"textgrid.poem.36196": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zw\u00f6lfte Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigend durch des Waldes Dunkel,", "tokens": ["Schwei\u00b7gend", "durch", "des", "Wal\u00b7des", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Morgen kaum noch graute,", "tokens": ["Da", "der", "Mor\u00b7gen", "kaum", "noch", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Zogen alle sie gewaffnet,", "tokens": ["Zo\u00b7gen", "al\u00b7le", "sie", "ge\u00b7waff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Kriegsgenossen trauernd,", "tokens": ["Um", "die", "Kriegs\u00b7ge\u00b7nos\u00b7sen", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem Roncisvaller Grunde,", "tokens": ["Nach", "dem", "Ron\u00b7cis\u00b7val\u00b7ler", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Toten sie in Haufen,", "tokens": ["Wo", "die", "To\u00b7ten", "sie", "in", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einige noch lebend \u00e4chzen", "tokens": ["Ei\u00b7ni\u00b7ge", "noch", "le\u00b7bend", "\u00e4ch\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Durch einander sahn mit Grausen.", "tokens": ["Durch", "ein\u00b7an\u00b7der", "sahn", "mit", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oliverus, dessen Seele", "tokens": ["O\u00b7li\u00b7ve\u00b7rus", ",", "des\u00b7sen", "See\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fern schon war vom Erdenraume,", "tokens": ["Fern", "schon", "war", "vom", "Er\u00b7den\u00b7rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seinen Leichnam fanden gr\u00e4\u00dflich", "tokens": ["Sei\u00b7nen", "Leich\u00b7nam", "fan\u00b7den", "gr\u00e4\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da am Boden ihre Augen,", "tokens": ["Da", "am", "Bo\u00b7den", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Kreuzweis an vier hohe Pf\u00e4hle", "tokens": ["Kreuz\u00b7weis", "an", "vier", "ho\u00b7he", "Pf\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ausgestreckt mit starken Tauen,", "tokens": ["Aus\u00b7ge\u00b7streckt", "mit", "star\u00b7ken", "Tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Von der Scheitel bis zur Sohle", "tokens": ["Von", "der", "Schei\u00b7tel", "bis", "zur", "Soh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ganz zerrissen und zerhauen,", "tokens": ["Ganz", "zer\u00b7ris\u00b7sen", "und", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "So mit Lanzen, Schwertern, Messern,", "tokens": ["So", "mit", "Lan\u00b7zen", ",", "Schwer\u00b7tern", ",", "Mes\u00b7sern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wie von grimmer Drachen Klauen.", "tokens": ["Wie", "von", "grim\u00b7mer", "Dra\u00b7chen", "Klau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O, was war da f\u00fcr ein Klagen,", "tokens": ["O", ",", "was", "war", "da", "f\u00fcr", "ein", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Schreien und Geheul der Trauer.", "tokens": ["Schrei\u00b7en", "und", "Ge\u00b7heul", "der", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.21": {"text": "Jeder wehklagt um die Seinen,", "tokens": ["Je\u00b7der", "weh\u00b7klagt", "um", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die der herbe Tod ihm raubte;", "tokens": ["Die", "der", "her\u00b7be", "Tod", "ihm", "raub\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wiederklangen aus dem Tale", "tokens": ["Wie\u00b7der\u00b7klan\u00b7gen", "aus", "dem", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Durch den Wald die Klagelaute.", "tokens": ["Durch", "den", "Wald", "die", "Kla\u00b7ge\u00b7lau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Kaiser Karol schmerzentbrannter,", "tokens": ["Kai\u00b7ser", "Ka\u00b7rol", "schmer\u00b7zent\u00b7brann\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Leidvoll sich die Haare raufend,", "tokens": ["Leid\u00b7voll", "sich", "die", "Haa\u00b7re", "rau\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Schwur bei dem allm\u00e4cht'gen Gotte:", "tokens": ["Schwur", "bei", "dem", "all\u00b7m\u00e4cht'\u00b7gen", "Got\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.28": {"text": "Nichts soll hemmen ihn im Laufe,", "tokens": ["Nichts", "soll", "hem\u00b7men", "ihn", "im", "Lau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Nimmer will er irgend rasten,", "tokens": ["Nim\u00b7mer", "will", "er", "ir\u00b7gend", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Bis sein Schwert im Blute rauche", "tokens": ["Bis", "sein", "Schwert", "im", "Blu\u00b7te", "rau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Jener heidnischen Verr\u00e4ter,", "tokens": ["Je\u00b7ner", "heid\u00b7ni\u00b7schen", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Die so manchen Mann ihm raubten. \u2013", "tokens": ["Die", "so", "man\u00b7chen", "Mann", "ihm", "raub\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Da den Heiden sie nun folgen", "tokens": ["Da", "den", "Hei\u00b7den", "sie", "nun", "fol\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ward erh\u00f6ret sein Vertrauen;", "tokens": ["Ward", "er\u00b7h\u00f6\u00b7ret", "sein", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Unbeweglich stand die Sonne", "tokens": ["Un\u00b7be\u00b7weg\u00b7lich", "stand", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wohl an dreier Tage Dauer,", "tokens": ["Wohl", "an", "drei\u00b7er", "Ta\u00b7ge", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Bis bei Saragossas Burgen,", "tokens": ["Bis", "bei", "Sa\u00b7ra\u00b7gos\u00b7sas", "Bur\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "An der Ebra Uferauen,", "tokens": ["An", "der", "Eb\u00b7ra", "U\u00b7fe\u00b7rau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Karol sie hat \u00fcberfallen,", "tokens": ["Ka\u00b7rol", "sie", "hat", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Ganz in Freud' und Fest berauschet.", "tokens": ["Ganz", "in", "Freud'", "und", "Fest", "be\u00b7rau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Da die Rache nun vollzogen,", "tokens": ["Da", "die", "Ra\u00b7che", "nun", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Lie\u00df er hin zu jenem Baume", "tokens": ["Lie\u00df", "er", "hin", "zu", "je\u00b7nem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Alle Kranken, Schwerverwund'ten,", "tokens": ["Al\u00b7le", "Kran\u00b7ken", ",", "Schwer\u00b7ver\u00b7wun\u00b7d'\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.44": {"text": "Dort wo Roland schlo\u00df die Augen,", "tokens": ["Dort", "wo", "Ro\u00b7land", "schlo\u00df", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "F\u00fchren, um sie streng zu fragen,", "tokens": ["F\u00fch\u00b7ren", ",", "um", "sie", "streng", "zu", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Weil im Heere war der Glaube,", "tokens": ["Weil", "im", "Hee\u00b7re", "war", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Durch Verrat sei es geschehen,", "tokens": ["Durch", "Ver\u00b7rat", "sei", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.48": {"text": "Weil er Ganelon vertraute.", "tokens": ["Weil", "er", "Ga\u00b7ne\u00b7lon", "ver\u00b7trau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Um das klarer zu erkunden,", "tokens": ["Um", "das", "kla\u00b7rer", "zu", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Soll im Zweikampf nach dem Brauche", "tokens": ["Soll", "im", "Zwei\u00b7kampf", "nach", "dem", "Brau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Dietrich f\u00fcr den Kaiser streiten,", "tokens": ["Diet\u00b7rich", "f\u00fcr", "den", "Kai\u00b7ser", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Da\u00df man Gottes Urteil schaue,", "tokens": ["Da\u00df", "man", "Got\u00b7tes", "Ur\u00b7teil", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Pinabell f\u00fcr den Verr\u00e4ter,", "tokens": ["Pi\u00b7na\u00b7bell", "f\u00fcr", "den", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.54": {"text": "Einer seiner Freund' und Trauten.", "tokens": ["Ei\u00b7ner", "sei\u00b7ner", "Freund'", "und", "Trau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Doch als Pinabell erschlagen,", "tokens": ["Doch", "als", "Pi\u00b7na\u00b7bell", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "L\u00e4\u00dft der Kaiser ohne Zaudern,", "tokens": ["L\u00e4\u00dft", "der", "Kai\u00b7ser", "oh\u00b7ne", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Da die Schuld nun liegt am Tage,", "tokens": ["Da", "die", "Schuld", "nun", "liegt", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Keines Zeugen es mehr brauchte,", "tokens": ["Kei\u00b7nes", "Zeu\u00b7gen", "es", "mehr", "brauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "An vier wilde Rosse binden", "tokens": ["An", "vier", "wil\u00b7de", "Ros\u00b7se", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Vor des ganzen Heeres Augen", "tokens": ["Vor", "des", "gan\u00b7zen", "Hee\u00b7res", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Ganelone, den Verr\u00e4ter,", "tokens": ["Ga\u00b7ne\u00b7lo\u00b7ne", ",", "den", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Die ihn so zerrissen grausend,", "tokens": ["Die", "ihn", "so", "zer\u00b7ris\u00b7sen", "grau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Ihn zerst\u00fcckten in vier Teile", "tokens": ["Ihn", "zer\u00b7st\u00fcck\u00b7ten", "in", "vier", "Tei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Nach den Enden des Weltraumes.", "tokens": ["Nach", "den", "En\u00b7den", "des", "Welt\u00b7rau\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.65": {"text": "Diesen Tod mu\u00dft' er erleiden,", "tokens": ["Die\u00b7sen", "Tod", "mu\u00dft'", "er", "er\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Bis er einstens klagt noch lauter,", "tokens": ["Bis", "er", "eins\u00b7tens", "klagt", "noch", "lau\u00b7ter", ","