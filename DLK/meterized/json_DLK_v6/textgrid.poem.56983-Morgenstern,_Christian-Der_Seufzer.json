{"textgrid.poem.56983": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Seufzer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Seufzer lief Schlittschuh auf n\u00e4chtlichem Eis", "tokens": ["Ein", "Seuf\u00b7zer", "lief", "Schlitt\u00b7schuh", "auf", "n\u00e4cht\u00b7li\u00b7chem", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und tr\u00e4umte von Liebe und Freude.", "tokens": ["und", "tr\u00e4um\u00b7te", "von", "Lie\u00b7be", "und", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es war an dem Stadtwall, und schneewei\u00df", "tokens": ["Es", "war", "an", "dem", "Stadt\u00b7wall", ",", "und", "schnee\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "gl\u00e4nzten die Stadtwallgeb\u00e4ude.", "tokens": ["gl\u00e4nz\u00b7ten", "die", "Stadt\u00b7wall\u00b7ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Der Seufzer dacht an ein Maidelein", "tokens": ["Der", "Seuf\u00b7zer", "dacht", "an", "ein", "Mai\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und blieb ergl\u00fchend stehen.", "tokens": ["und", "blieb", "er\u00b7gl\u00fc\u00b7hend", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da schmolz die Eisbahn unter ihm ein \u2013", "tokens": ["Da", "schmolz", "die", "Eis\u00b7bahn", "un\u00b7ter", "ihm", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und er sank \u2013 und ward nimmer gesehen.", "tokens": ["und", "er", "sank", "\u2013", "und", "ward", "nim\u00b7mer", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}