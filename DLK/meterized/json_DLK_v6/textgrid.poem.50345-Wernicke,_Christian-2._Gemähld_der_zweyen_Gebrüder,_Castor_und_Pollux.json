{"textgrid.poem.50345": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2. Gem\u00e4hld der zweyen Gebr\u00fcder, Castor und Pollux", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Pinsel ist zu schwach zu mahlen", "tokens": ["Mein", "Pin\u00b7sel", "ist", "zu", "schwach", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine ", "tokens": ["Der", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Des andern ", "tokens": ["Des", "an\u00b7dern"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Es stammen voller Glantz beyd' aus der ", "tokens": ["Es", "stam\u00b7men", "vol\u00b7ler", "Glantz", "beyd'", "aus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich ", "tokens": ["Gleich"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Doch d\u00fcncket mich, wenn ich den einen recht betracht',", "tokens": ["Doch", "d\u00fcn\u00b7cket", "mich", ",", "wenn", "ich", "den", "ei\u00b7nen", "recht", "be\u00b7tracht'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dass man vor ", "tokens": ["Dass", "man", "vor"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und wenn den andern ich mit jenem will vergleichen,", "tokens": ["Und", "wenn", "den", "an\u00b7dern", "ich", "mit", "je\u00b7nem", "will", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "PPER", "APPR", "PDAT", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es sey der ", "tokens": ["Es", "sey", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Und beyder wunderw\u00fcrdig Licht,", "tokens": ["Und", "bey\u00b7der", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das t\u00e4glich heller scheint,", "tokens": ["Das", "t\u00e4g\u00b7lich", "hel\u00b7ler", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}