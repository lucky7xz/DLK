{"textgrid.poem.48829": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "54. An den Mon", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die du standhaft bist in deinem Unbestande,", "tokens": ["Du", ",", "die", "du", "stand\u00b7haft", "bist", "in", "dei\u00b7nem", "Un\u00b7be\u00b7stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "steig', Hekate, herab; ich singe dir ein Lied,", "tokens": ["steig'", ",", "He\u00b7ka\u00b7te", ",", "her\u00b7ab", ";", "ich", "sin\u00b7ge", "dir", "ein", "Lied", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ADV", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "ein Lied von meiner Zier, die itzt auch nach dir sieht,", "tokens": ["ein", "Lied", "von", "mei\u00b7ner", "Zier", ",", "die", "itzt", "auch", "nach", "dir", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ob ich schon bin sehr weit von ihr und ihrem Lande.", "tokens": ["ob", "ich", "schon", "bin", "sehr", "weit", "von", "ihr", "und", "ih\u00b7rem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Komm, Berezynthie, zu dieses Stromes Rande,", "tokens": ["Komm", ",", "Be\u00b7re\u00b7zynt\u00b7hie", ",", "zu", "die\u00b7ses", "Stro\u00b7mes", "Ran\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "an dem ich geh' herum, da meine Hoffnung bl\u00fcht,", "tokens": ["an", "dem", "ich", "geh'", "he\u00b7rum", ",", "da", "mei\u00b7ne", "Hoff\u00b7nung", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du wei\u00dft es, Delie, was itzt mit ihr geschicht,", "tokens": ["du", "wei\u00dft", "es", ",", "De\u00b7lie", ",", "was", "itzt", "mit", "ihr", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "du wei\u00dft es, wie es steht um meine ", "tokens": ["du", "wei\u00dft", "es", ",", "wie", "es", "steht", "um", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, Ph\u00f6be, Tag der Nacht, Diane, Borgelicht,", "tokens": ["Komm", ",", "Ph\u00f6\u00b7be", ",", "Tag", "der", "Nacht", ",", "Di\u00b7a\u00b7ne", ",", "Bor\u00b7ge\u00b7licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "ART", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wars\u00e4grin, Liederfreund; komm, Lune, s\u00e4ume nicht;", "tokens": ["War\u00b7s\u00e4g\u00b7rin", ",", "Lie\u00b7der\u00b7freund", ";", "komm", ",", "Lu\u00b7ne", ",", "s\u00e4u\u00b7me", "nicht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die ganze Welt, die schl\u00e4ft. Ich wache dich zu loben.", "tokens": ["die", "gan\u00b7ze", "Welt", ",", "die", "schl\u00e4ft", ".", "Ich", "wa\u00b7che", "dich", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Stromf\u00fcrstin, J\u00e4gerfrau, Nachtauge, Horngesicht',", "tokens": ["Strom\u00b7f\u00fcrs\u00b7tin", ",", "J\u00e4\u00b7ger\u00b7frau", ",", "Nach\u00b7tau\u00b7ge", ",", "Horn\u00b7ge\u00b7sicht'", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "herab! itzt fang' ich an das s\u00fc\u00dfe Lobgedicht'.", "tokens": ["her\u00b7ab", "!", "itzt", "fang'", "ich", "an", "das", "s\u00fc\u00b7\u00dfe", "Lob\u00b7ge\u00b7dicht'", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und k\u00f6mst du nicht herab, so h\u00f6r es nur dort oben!", "tokens": ["Und", "k\u00f6mst", "du", "nicht", "her\u00b7ab", ",", "so", "h\u00f6r", "es", "nur", "dort", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}