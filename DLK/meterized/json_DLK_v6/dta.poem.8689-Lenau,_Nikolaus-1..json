{"dta.poem.8689": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der dunkle Wald umrauscht den Wiesengrund,", "tokens": ["Der", "dunk\u00b7le", "Wald", "um\u00b7rauscht", "den", "Wie\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gar d\u00fcster liegt der graue Berg dahinter,", "tokens": ["Gar", "d\u00fcs\u00b7ter", "liegt", "der", "grau\u00b7e", "Berg", "da\u00b7hin\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das d\u00fcrre Laub, der Windhauch gibt es kund,", "tokens": ["Das", "d\u00fcr\u00b7re", "Laub", ",", "der", "Wind\u00b7hauch", "gibt", "es", "kund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geschritten kommt allm\u00e4hlig schon der Winter.", "tokens": ["Ge\u00b7schrit\u00b7ten", "kommt", "all\u00b7m\u00e4h\u00b7lig", "schon", "der", "Win\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sonne ging, umh\u00fcllt von Wolken dicht,", "tokens": ["Die", "Son\u00b7ne", "ging", ",", "um\u00b7h\u00fcllt", "von", "Wol\u00b7ken", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unfreundlich, ohne Scheideblick von hinnen,", "tokens": ["Un\u00b7freund\u00b7lich", ",", "oh\u00b7ne", "Schei\u00b7de\u00b7blick", "von", "hin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die Natur verstummt, im D\u00e4mmerlicht", "tokens": ["Und", "die", "Na\u00b7tur", "ver\u00b7stummt", ",", "im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schwerm\u00fcthig ihrem Tode nachzusinnen.", "tokens": ["Schwer\u00b7m\u00fct\u00b7hig", "ih\u00b7rem", "To\u00b7de", "nach\u00b7zu\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort, wo die Eiche rauscht am Bergesfu\u00df,", "tokens": ["Dort", ",", "wo", "die", "Ei\u00b7che", "rauscht", "am", "Ber\u00b7ges\u00b7fu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo bang vor\u00fcberklagt des Baches Welle,", "tokens": ["Wo", "bang", "vor\u00b7\u00fc\u00b7ber\u00b7klagt", "des", "Ba\u00b7ches", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort winket, wie aus alter Zeit ein Gru\u00df,", "tokens": ["Dort", "win\u00b7ket", ",", "wie", "aus", "al\u00b7ter", "Zeit", "ein", "Gru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die l\u00e4ngst verlassne, stille Waldkapelle.", "tokens": ["Die", "l\u00e4ngst", "ver\u00b7lass\u00b7ne", ",", "stil\u00b7le", "Wald\u00b7ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo sind sie, deren Lied aus deinem Schoo\u00df,", "tokens": ["Wo", "sind", "sie", ",", "de\u00b7ren", "Lied", "aus", "dei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PRELAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Kirchlein, einst zu Gott emporgeflogen,", "tokens": ["O", "Kirch\u00b7lein", ",", "einst", "zu", "Gott", "em\u00b7por\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergessend all' ihr tr\u00fcbes Erdenloos? \u2014", "tokens": ["Ver\u00b7ges\u00b7send", "all'", "ihr", "tr\u00fc\u00b7bes", "Er\u00b7den\u00b7loos", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PIAT", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo sind sie? \u2014 ihrem Liede nachgezogen!", "tokens": ["Wo", "sind", "sie", "?", "ih\u00b7rem", "Lie\u00b7de", "nach\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "$(", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}