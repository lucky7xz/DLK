{"dta.poem.12389": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Prinzenraub .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir wolln ein Liedel heben an,               ", "tokens": ["Wir", "wolln", "ein", "Lie\u00b7del", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sich hat angespunnen,", "tokens": ["Was", "sich", "hat", "an\u00b7ge\u00b7spun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie's im Plei\u00dfnerland gar schlecht war bestallt,", "tokens": ["Wie's", "im", "Plei\u00df\u00b7ner\u00b7land", "gar", "schlecht", "war", "be\u00b7stallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Als den jungen F\u00fcrst'n geschah Gewalt,", "tokens": ["Als", "den", "jun\u00b7gen", "F\u00fcr\u00b7st'n", "ge\u00b7schah", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Durch Kuntzen von Kauffungen.", "tokens": ["Durch", "Kunt\u00b7zen", "von", "Kauf\u00b7fun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Adler hat auf'm Fels gebaut", "tokens": ["Der", "Ad\u00b7ler", "hat", "auf'm", "Fels", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6nes Nest mit Jungen,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Nest", "mit", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie er einst geflogen aus,", "tokens": ["Und", "wie", "er", "einst", "ge\u00b7flo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holt ein Geyer die Jungen heraus,", "tokens": ["Holt", "ein", "Ge\u00b7yer", "die", "Jun\u00b7gen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Drauf ward's Nest leer gefunden.", "tokens": ["Drauf", "ward's", "Nest", "leer", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo der Geyer auf'm Dache sitzt,", "tokens": ["Wo", "der", "Ge\u00b7yer", "auf'm", "Da\u00b7che", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedeihen die K\u00fcchlein selten,", "tokens": ["Ge\u00b7dei\u00b7hen", "die", "K\u00fcch\u00b7lein", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es war da ein seltsam Narrenspiel,", "tokens": ["Es", "war", "da", "ein", "selt\u00b7sam", "Nar\u00b7ren\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welcher F\u00fcrst seinen R\u00e4then traut zu viel,", "tokens": ["Wel\u00b7cher", "F\u00fcrst", "sei\u00b7nen", "R\u00e4\u00b7then", "traut", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "PIS", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mu\u00df oft es selber entgelten.", "tokens": ["Mu\u00df", "oft", "es", "sel\u00b7ber", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Altenburg, du feine Stadt,", "tokens": ["Al\u00b7ten\u00b7burg", ",", "du", "fei\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich th\u00e4t er mit Untreu meinen,", "tokens": ["Dich", "th\u00e4t", "er", "mit", "Un\u00b7treu", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da in dir war'n all' Hofleut voll,", "tokens": ["Da", "in", "dir", "wa\u00b7r'n", "all'", "Hof\u00b7leut", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kam Kunz mit Leitern und Buben toll,", "tokens": ["Kam", "Kunz", "mit", "Lei\u00b7tern", "und", "Bu\u00b7ben", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und holt die F\u00fcrsten so kleine.", "tokens": ["Und", "holt", "die", "F\u00fcrs\u00b7ten", "so", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Was blast dich, Kunz, f\u00fcr Unlust an,", "tokens": ["Was", "blast", "dich", ",", "Kunz", ",", "f\u00fcr", "Un\u00b7lust", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NE", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du ins Schlo\u00df einsteigest?", "tokens": ["Da\u00df", "du", "ins", "Schlo\u00df", "ein\u00b7stei\u00b7gest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stiehlst die zarten Herren heraus,", "tokens": ["Und", "stiehlst", "die", "zar\u00b7ten", "Her\u00b7ren", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als der Kurf\u00fcrst eben nit war zu Haus,", "tokens": ["Als", "der", "Kur\u00b7f\u00fcrst", "e\u00b7ben", "nit", "war", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die zarten F\u00fcrsten-Zweige.", "tokens": ["Die", "zar\u00b7ten", "F\u00fcrs\u00b7ten\u00b7Zwei\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war wohl als ein Wunderding,", "tokens": ["Es", "war", "wohl", "als", "ein", "Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich das Land beweget,", "tokens": ["Wie", "sich", "das", "Land", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was da auf'n Stra\u00dfen war'n f\u00fcr Leut',", "tokens": ["Was", "da", "auf'n", "Stra\u00b7\u00dfen", "wa\u00b7r'n", "f\u00fcr", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die den R\u00e4ubern folgten nach in Zeit,", "tokens": ["Die", "den", "R\u00e4u\u00b7bern", "folg\u00b7ten", "nach", "in", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "All's wibbelt, kribbelt, sich beweget.", "tokens": ["All's", "wib\u00b7belt", ",", "krib\u00b7belt", ",", "sich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}