{"textgrid.poem.48230": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Was mir gef\u00e4llt", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst: ob mir in dieser Welt", "tokens": ["Du", "fragst", ":", "ob", "mir", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcberhaupt noch was gef\u00e4llt?", "tokens": ["\u00dc\u00b7ber\u00b7haupt", "noch", "was", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du fragst es und l\u00e4chelst sp\u00f6ttisch dabei.", "tokens": ["Du", "fragst", "es", "und", "l\u00e4\u00b7chelst", "sp\u00f6t\u00b7tisch", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bblieber Freund, mir gef\u00e4llt noch allerlei:", "tokens": ["\u00bb", "lie\u00b7ber", "Freund", ",", "mir", "ge\u00b7f\u00e4llt", "noch", "al\u00b7ler\u00b7lei", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "PPER", "VVFIN", "ADV", "PIAT", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jedes Fr\u00fchjahr das erste Tiergartengr\u00fcn,", "tokens": ["Je\u00b7des", "Fr\u00fch\u00b7jahr", "das", "ers\u00b7te", "Tier\u00b7gar\u00b7ten\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oder wenn in Werder die Kirschen bl\u00fchn,", "tokens": ["O\u00b7der", "wenn", "in", "Wer\u00b7der", "die", "Kir\u00b7schen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu Pfingsten Kalmus und Birkenreiser,", "tokens": ["Zu", "Pfings\u00b7ten", "Kal\u00b7mus", "und", "Bir\u00b7ken\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der alte Moltke, der alte Kaiser,", "tokens": ["Der", "al\u00b7te", "Molt\u00b7ke", ",", "der", "al\u00b7te", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und dann zu Pferd, eine Stunde sp\u00e4ter,", "tokens": ["Und", "dann", "zu", "Pferd", ",", "ei\u00b7ne", "Stun\u00b7de", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit dem gelben Streifen der \u203aHalberst\u00e4dter\u2039;", "tokens": ["Mit", "dem", "gel\u00b7ben", "Strei\u00b7fen", "der", "\u203a", "Hal\u00b7ber\u00b7st\u00e4d\u00b7ter", "\u2039", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$(", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Kuckucksrufen, im Wald ein Reh,", "tokens": ["Ku\u00b7ckucks\u00b7ru\u00b7fen", ",", "im", "Wald", "ein", "Reh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Ein Spaziergang durch die L\u00e4ster-Allee,", "tokens": ["Ein", "Spa\u00b7zier\u00b7gang", "durch", "die", "L\u00e4s\u00b7ter\u00b7Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Paraden, der Schapersche Goethekopf", "tokens": ["Pa\u00b7ra\u00b7den", ",", "der", "Scha\u00b7per\u00b7sche", "Goe\u00b7the\u00b7kopf"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und ein Backfisch mit einem Mozartzopf.\u00ab", "tokens": ["Und", "ein", "Back\u00b7fisch", "mit", "ei\u00b7nem", "Mo\u00b7zart\u00b7zopf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}