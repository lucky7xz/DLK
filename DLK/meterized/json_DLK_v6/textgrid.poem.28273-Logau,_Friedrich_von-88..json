{"textgrid.poem.28273": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Be\u00dfres was solt euren Ehren,", "tokens": ["Be\u00df\u00b7res", "was", "solt", "eu\u00b7ren", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edles Bild, zu dienen kummen,", "tokens": ["Ed\u00b7les", "Bild", ",", "zu", "die\u00b7nen", "kum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wo wird das genummen,", "tokens": ["A\u00b7ber", "wo", "wird", "das", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df sie m\u00f6cht um etwas mehren?", "tokens": ["Da\u00df", "sie", "m\u00f6cht", "um", "et\u00b7was", "meh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das, was kummt, ist kaum zu nennen;", "tokens": ["Das", ",", "was", "kummt", ",", "ist", "kaum", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "VVFIN", "$,", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Der es schickt, ist drum zu schelten,", "tokens": ["Der", "es", "schickt", ",", "ist", "drum", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df auch billich solches gelten,", "tokens": ["Mu\u00df", "auch", "bil\u00b7lich", "sol\u00b7ches", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Darff sich auch nicht lassen kennen.", "tokens": ["Darff", "sich", "auch", "nicht", "las\u00b7sen", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}