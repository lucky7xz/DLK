{"textgrid.poem.62540": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sanft ist dein Joch, leicht deine Last,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft ist dein Joch, leicht deine Last,", "tokens": ["Sanft", "ist", "dein", "Joch", ",", "leicht", "dei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch \u2013 wer tr\u00e4gt sie gern?", "tokens": ["Und", "doch", "\u2013", "wer", "tr\u00e4gt", "sie", "gern", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt, die du erl\u00f6set hast,", "tokens": ["Die", "Welt", ",", "die", "du", "er\u00b7l\u00f6\u00b7set", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dient einem andern Herrn.", "tokens": ["Dient", "ei\u00b7nem", "an\u00b7dern", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, Heiland! allen Erdentand", "tokens": ["Ach", ",", "Hei\u00b7land", "!", "al\u00b7len", "Er\u00b7den\u00b7tand"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rei\u00df' selbst aus meiner Brust,", "tokens": ["Rei\u00df'", "selbst", "aus", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmel sei mein Vaterland,", "tokens": ["Der", "Him\u00b7mel", "sei", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und du sei meine Lust.", "tokens": ["Und", "du", "sei", "mei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}