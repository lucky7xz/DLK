{"textgrid.poem.40467": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00f6chte Jedem gern die Stelle zeigen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chte Jedem gern die Stelle zeigen,", "tokens": ["M\u00f6ch\u00b7te", "Je\u00b7dem", "gern", "die", "Stel\u00b7le", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo mein Herz so schwer verwundet worden:", "tokens": ["Wo", "mein", "Herz", "so", "schwer", "ver\u00b7wun\u00b7det", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber dir m\u00f6cht' ich mein Leid verschweigen,", "tokens": ["A\u00b7ber", "dir", "m\u00f6cht'", "ich", "mein", "Leid", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch nur dir! denn du allein", "tokens": ["Doch", "nur", "dir", "!", "denn", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "$.", "KON", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast den Dolch, der mich vermag zu morden.", "tokens": ["Hast", "den", "Dolch", ",", "der", "mich", "ver\u00b7mag", "zu", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00f6chte Keinem meine Leiden klagen,", "tokens": ["M\u00f6ch\u00b7te", "Kei\u00b7nem", "mei\u00b7ne", "Lei\u00b7den", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber dir enth\u00fcllen alle Wunden,", "tokens": ["A\u00b7ber", "dir", "ent\u00b7h\u00fcl\u00b7len", "al\u00b7le", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die gar tief mein Herz sich hat geschlagen;", "tokens": ["Die", "gar", "tief", "mein", "Herz", "sich", "hat", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "PRF", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch nur dir! denn du allein", "tokens": ["Doch", "nur", "dir", "!", "denn", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "$.", "KON", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast den Balsam, der mich macht gesunden.", "tokens": ["Hast", "den", "Bal\u00b7sam", ",", "der", "mich", "macht", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}