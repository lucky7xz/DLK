{"textgrid.poem.53830": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Pariser Dankgebet", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "dann sagt keiner: \u00bbDir hol ick jleich runter vom Bock, du oller Mistkutschenfahrer, pass mal uff \u2013!\u00ab", "tokens": ["dann", "sagt", "kei\u00b7ner", ":", "\u00bb", "Dir", "hol", "ick", "jleich", "run\u00b7ter", "vom", "Bock", ",", "du", "ol\u00b7ler", "Mist\u00b7kut\u00b7schen\u00b7fah\u00b7rer", ",", "pass", "mal", "uff", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NE", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "$(", "$.", "$("], "meter": "--+-+---+--+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier geht man fr\u00fchmorgens heiter und pfeifend aus seinem Haus.", "tokens": ["Hier", "geht", "man", "fr\u00fch\u00b7mor\u00b7gens", "hei\u00b7ter", "und", "pfei\u00b7fend", "aus", "sei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "ein Mensch zu sein, aufzuatmen allein und zu zweit.", "tokens": ["ein", "Mensch", "zu", "sein", ",", "auf\u00b7zu\u00b7at\u00b7men", "al\u00b7lein", "und", "zu", "zweit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "VVFIN", "ADV", "KON", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rekonvaleszenten sehen aus den Fenstern der Krankenh\u00e4user, ungehetzt und voll Ruh.", "tokens": ["Re\u00b7kon\u00b7va\u00b7les\u00b7zen\u00b7ten", "se\u00b7hen", "aus", "den", "Fens\u00b7tern", "der", "Kran\u00b7ken\u00b7h\u00e4u\u00b7ser", ",", "un\u00b7ge\u00b7hetzt", "und", "voll", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+--+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Alte Herren gucken im Luxembourg den Spielen der Kinder zu.", "tokens": ["Al\u00b7te", "Her\u00b7ren", "gu\u00b7cken", "im", "Lu\u00b7xem\u00b7bourg", "den", "Spie\u00b7len", "der", "Kin\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hier ist Wolke noch Wolke und Stein noch Stein.", "tokens": ["Hier", "ist", "Wol\u00b7ke", "noch", "Wol\u00b7ke", "und", "Stein", "noch", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "NN", "KON", "NN", "ADV", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": " hier hat es noch einen Sinn, am Leben zu sein.", "tokens": ["hier", "hat", "es", "noch", "ei\u00b7nen", "Sinn", ",", "am", "Le\u00b7ben", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": " amen.", "tokens": ["a\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Hier sind die Professoren keine Staatssklaven und die Studenten keine Hausknechte.", "tokens": ["Hier", "sind", "die", "Pro\u00b7fes\u00b7so\u00b7ren", "kei\u00b7ne", "Staatss\u00b7kla\u00b7ven", "und", "die", "Stu\u00b7den\u00b7ten", "kei\u00b7ne", "Haus\u00b7knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+--+--+-++-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Hier mu\u00df man gebildet und kultiviert sein \u2013 das verlangt die Linke wie die Rechte.", "tokens": ["Hier", "mu\u00df", "man", "ge\u00b7bil\u00b7det", "und", "kul\u00b7ti\u00b7viert", "sein", "\u2013", "das", "ver\u00b7langt", "die", "Lin\u00b7ke", "wie", "die", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVPP", "KON", "VVFIN", "PPOSAT", "$(", "PDS", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+-+-+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hier k\u00f6nnen sogar die Royalisten gute Artikel schreiben \u2013 es sind Leute von Witz und Esprit.", "tokens": ["Hier", "k\u00f6n\u00b7nen", "so\u00b7gar", "die", "Ro\u00b7ya\u00b7lis\u00b7ten", "gu\u00b7te", "Ar\u00b7ti\u00b7kel", "schrei\u00b7ben", "\u2013", "es", "sind", "Leu\u00b7te", "von", "Witz", "und", "E\u00b7sprit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$(", "PPER", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+--+---+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Und von schimmernder Wehr und Gro\u00dffrankreich sprechen die Bl\u00e4tter fast nie.", "tokens": ["Und", "von", "schim\u00b7mern\u00b7der", "Wehr", "und", "Gro\u00df\u00b7fran\u00b7kreich", "spre\u00b7chen", "die", "Bl\u00e4t\u00b7ter", "fast", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "--+--+-+--+--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn sich hier die siegreichen Generale so benehmen wollten wie unsre geschlagenen zu Haus . . . !", "tokens": ["Wenn", "sich", "hier", "die", "sieg\u00b7rei\u00b7chen", "Ge\u00b7ne\u00b7ra\u00b7le", "so", "be\u00b7neh\u00b7men", "woll\u00b7ten", "wie", "uns\u00b7re", "ge\u00b7schla\u00b7ge\u00b7nen", "zu", "Haus", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "KOKOM", "PPOSAT", "ADJA", "APPR", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Ein ganzes Land streckt friedfertig die Arme aus \u2013", "tokens": ["Ein", "gan\u00b7zes", "Land", "streckt", "fried\u00b7fer\u00b7tig", "die", "Ar\u00b7me", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Wie sch\u00f6n ist es, hier zu leben: ohne diese Gesichter, die keine sind;", "tokens": ["Wie", "sch\u00f6n", "ist", "es", ",", "hier", "zu", "le\u00b7ben", ":", "oh\u00b7ne", "die\u00b7se", "Ge\u00b7sich\u00b7ter", ",", "die", "kei\u00b7ne", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$.", "APPR", "PDAT", "NN", "$,", "PRELS", "PIAT", "VAFIN", "$."], "meter": "-+--+-+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "ohne Krach und Krakeel \u2013 ohne den staubigen berliner Sommerwind.", "tokens": ["oh\u00b7ne", "Krach", "und", "Kra\u00b7keel", "\u2013", "oh\u00b7ne", "den", "stau\u00b7bi\u00b7gen", "ber\u00b7li\u00b7ner", "Som\u00b7mer\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Zehn Jahre zu sp\u00e4t! Und doch darf ich nicht klagen.", "tokens": ["Zehn", "Jah\u00b7re", "zu", "sp\u00e4t", "!", "Und", "doch", "darf", "ich", "nicht", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKA", "ADJD", "$.", "KON", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": " es tut so wohl, auch einmal Ja zu sagen.", "tokens": ["es", "tut", "so", "wohl", ",", "auch", "ein\u00b7mal", "Ja", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "PTKANT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}