{"dta.poem.19049": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxX.  Auf des Herrn von Watteville Ver-  \n l\u00f6bni\u00df mit der Fr\u00e4ulein Joh. Sophia  \n von Zetzschwitz.   Am 7. Septembr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du Quell der ewgen Ehe,", "tokens": ["Du", "Quell", "der", "ew\u00b7gen", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und du der Seelen-Mann!", "tokens": ["Und", "du", "der", "See\u00b7len\u00b7Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Geist der Tief und H\u00f6he!", "tokens": ["Du", "Geist", "der", "Tief", "und", "H\u00f6\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schau her: wir beten an,", "tokens": ["Schau", "her", ":", "wir", "be\u00b7ten", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wir r\u00fchmen deine Liebe,", "tokens": ["Wir", "r\u00fch\u00b7men", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir freun uns deiner Treu;", "tokens": ["Wir", "freun", "uns", "dei\u00b7ner", "Treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn ihrer beyder Triebe", "tokens": ["Denn", "ih\u00b7rer", "bey\u00b7der", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind alle Morgen neu.", "tokens": ["Sind", "al\u00b7le", "Mor\u00b7gen", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie werden doch die Deinen", "tokens": ["Wie", "wer\u00b7den", "doch", "die", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So seliglich gef\u00fchrt?", "tokens": ["So", "se\u00b7lig\u00b7lich", "ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wird auf bl\u00f6des Weinen", "tokens": ["Wie", "wird", "auf", "bl\u00f6\u00b7des", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald muntre Lust versp\u00fcrt!", "tokens": ["Bald", "mun\u00b7tre", "Lust", "ver\u00b7sp\u00fcrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jtzt setzen die Gerechten", "tokens": ["Jtzt", "set\u00b7zen", "die", "Ge\u00b7rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Satan Sto\u00df auf Sto\u00df,", "tokens": ["Und", "Sa\u00b7tan", "Sto\u00df", "auf", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn rufst du deinen Knechten", "tokens": ["Denn", "rufst", "du", "dei\u00b7nen", "Knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Ruh in deinen Schoo\u00df.", "tokens": ["Zur", "Ruh", "in", "dei\u00b7nen", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jtzt sehn die bl\u00f6den Augen", "tokens": ["Jtzt", "sehn", "die", "bl\u00f6\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der menschlichen Natur,", "tokens": ["Der", "menschli\u00b7chen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die nimmer f\u00fcr dir taugen,", "tokens": ["Die", "nim\u00b7mer", "f\u00fcr", "dir", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich weder Bahn noch Spur;", "tokens": ["Sich", "we\u00b7der", "Bahn", "noch", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald zieht dein seligs Wincken", "tokens": ["Bald", "zieht", "dein", "se\u00b7ligs", "Win\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Decke wieder weg:", "tokens": ["Die", "De\u00b7cke", "wie\u00b7der", "weg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir dachten zu versincken;", "tokens": ["Wir", "dach\u00b7ten", "zu", "ver\u00b7sin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun zeigt sich Spur und Steg.", "tokens": ["Nun", "zeigt", "sich", "Spur", "und", "Steg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du allzu treue Liebe!", "tokens": ["Du", "all\u00b7zu", "treu\u00b7e", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was sollen wir dir thun?", "tokens": ["Was", "sol\u00b7len", "wir", "dir", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer f\u00f6rdert unsre Triebe?", "tokens": ["Wer", "f\u00f6r\u00b7dert", "uns\u00b7re", "Trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie k\u00f6nnen ja nicht ruhn;", "tokens": ["Sie", "k\u00f6n\u00b7nen", "ja", "nicht", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach w\u00e4ren sie verm\u00f6gend,", "tokens": ["Ach", "w\u00e4\u00b7ren", "sie", "ver\u00b7m\u00f6\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich also zu erh\u00f6hn,", "tokens": ["Dich", "al\u00b7so", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df unsre gantze Gegend", "tokens": ["Da\u00df", "uns\u00b7re", "gant\u00b7ze", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von deinem Ruhm erth\u00f6n!", "tokens": ["Von", "dei\u00b7nem", "Ruhm", "er\u00b7th\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sey gn\u00e4diglich zufrieden", "tokens": ["Sey", "gn\u00e4\u00b7di\u00b7glich", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit unserm gantzen Seyn.", "tokens": ["Mit", "un\u00b7serm", "gant\u00b7zen", "Seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir habens dir beschieden", "tokens": ["Wir", "ha\u00b7bens", "dir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Tempel, nimm es ein,", "tokens": ["Zum", "Tem\u00b7pel", ",", "nimm", "es", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und stimme deiner Ehre", "tokens": ["Und", "stim\u00b7me", "dei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Christo deinem Sohn", "tokens": ["In", "Chris\u00b7to", "dei\u00b7nem", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch alle Geistes Ch\u00f6re", "tokens": ["Durch", "al\u00b7le", "Geis\u00b7tes", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den allerreinsten Thon.", "tokens": ["Den", "al\u00b7ler\u00b7reins\u00b7ten", "Thon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die neu-verbundnen Beyde,", "tokens": ["Die", "neu\u00b7ver\u00b7bund\u00b7nen", "Bey\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die du erst dein gemacht,", "tokens": ["Die", "du", "erst", "dein", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun zu Freud und Leide", "tokens": ["Und", "nun", "zu", "Freud", "und", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einander zugebracht,", "tokens": ["Ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die sind zu uns getreten", "tokens": ["Die", "sind", "zu", "uns", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In gleicher Harmonie,", "tokens": ["In", "glei\u00b7cher", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu singen und zu beten,", "tokens": ["Zu", "sin\u00b7gen", "und", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herab! und h\u00f6re sie.", "tokens": ["Her\u00b7ab", "!", "und", "h\u00f6\u00b7re", "sie", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Z\u00fcnd allen ihren Wandel", "tokens": ["Z\u00fcnd", "al\u00b7len", "ih\u00b7ren", "Wan\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deiner Liebe an,", "tokens": ["Mit", "dei\u00b7ner", "Lie\u00b7be", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bi\u00df man in ihrem Handel", "tokens": ["Bi\u00df", "man", "in", "ih\u00b7rem", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Gleichni\u00df sehen kan:", "tokens": ["Dein", "Gleich\u00b7ni\u00df", "se\u00b7hen", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr\u2019 Augen mache lichte,", "tokens": ["Ih\u00b7r'", "Au\u00b7gen", "ma\u00b7che", "lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Damit ihr gantzer Leib", "tokens": ["Da\u00b7mit", "ihr", "gant\u00b7zer", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von deinem Angesichte", "tokens": ["Von", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein klarer Spiegel bleib.", "tokens": ["Ein", "kla\u00b7rer", "Spie\u00b7gel", "bleib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du bist bereits entschlossen,", "tokens": ["Du", "bist", "be\u00b7reits", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um ihnen Guts zu thun,", "tokens": ["Um", "ih\u00b7nen", "Guts", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hat die Welt verdrossen,", "tokens": ["Das", "hat", "die", "Welt", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Satan will nicht ruhn,", "tokens": ["Der", "Sa\u00b7tan", "will", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Streit ist angegangen,", "tokens": ["Der", "Streit", "ist", "an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch hat sich Jonathan", "tokens": ["Auch", "hat", "sich", "Jo\u00b7na\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schon an den Feind gehangen:", "tokens": ["Schon", "an", "den", "Feind", "ge\u00b7han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir h\u00e4ngen uns mit an.", "tokens": ["Wir", "h\u00e4n\u00b7gen", "uns", "mit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie siegt bereits, die Liebe;", "tokens": ["Sie", "siegt", "be\u00b7reits", ",", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum sehet, was ihr thut.", "tokens": ["Drum", "se\u00b7het", ",", "was", "ihr", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gebt jenen Streich und Hiebe", "tokens": ["Gebt", "je\u00b7nen", "Streich", "und", "Hie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Gut und Blut.", "tokens": ["Der", "Lie\u00b7be", "Gut", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn sonst bekommt ihr Wunden,", "tokens": ["Denn", "sonst", "be\u00b7kommt", "ihr", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werdet ausgelacht;", "tokens": ["Und", "wer\u00b7det", "aus\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat euch der HErr verbunden,", "tokens": ["Hat", "euch", "der", "Herr", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So zeigt nun eure Macht.", "tokens": ["So", "zeigt", "nun", "eu\u00b7re", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gedeyt in Zions Mauren,", "tokens": ["Ge\u00b7deyt", "in", "Zi\u00b7ons", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die eine Friedens-Stadt:", "tokens": ["Die", "ei\u00b7ne", "Frie\u00b7dens\u00b7Stadt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00fcsse ewig dauren,", "tokens": ["Es", "m\u00fcs\u00b7se", "e\u00b7wig", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was GOtt gegr\u00fcndet hat;", "tokens": ["Was", "Gott", "ge\u00b7gr\u00fcn\u00b7det", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch m\u00fcssens sehn und h\u00f6ren,", "tokens": ["Auch", "m\u00fcs\u00b7sens", "sehn", "und", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die seine Hasser sind,", "tokens": ["Die", "sei\u00b7ne", "Has\u00b7ser", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df bey der Liebe Ch\u00f6ren", "tokens": ["Da\u00df", "bey", "der", "Lie\u00b7be", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch alles lieb gewinnt.", "tokens": ["Euch", "al\u00b7les", "lieb", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gott lasse unser Flehen", "tokens": ["Gott", "las\u00b7se", "un\u00b7ser", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey ihm erh\u00f6ret seyn,", "tokens": ["Bey", "ihm", "er\u00b7h\u00f6\u00b7ret", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df wir euch wachsen sehen", "tokens": ["Da\u00df", "wir", "euch", "wach\u00b7sen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Christi Creutz-Gemein,", "tokens": ["In", "Chris\u00b7ti", "Creutz\u00b7Ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und unter uns erbauen", "tokens": ["Und", "un\u00b7ter", "uns", "er\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Hau\u00df des HErrn bequem,", "tokens": ["Ein", "Hau\u00df", "des", "Herrn", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bi\u00df da\u00df wir alle schauen", "tokens": ["Bi\u00df", "da\u00df", "wir", "al\u00b7le", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Gl\u00fcck Jerusal