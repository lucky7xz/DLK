{"textgrid.poem.64793": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Elend unterdr\u00fcckt, und \u00fcberschwemmt mit Blut,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Elend unterdr\u00fcckt, und \u00fcberschwemmt mit Blut,", "tokens": ["Von", "E\u00b7lend", "un\u00b7ter\u00b7dr\u00fcckt", ",", "und", "\u00fc\u00b7bersc\u00b7hwemmt", "mit", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Feinde, die es rief, und seiner S\u00f6hne Wuth,", "tokens": ["Durch", "Fein\u00b7de", ",", "die", "es", "rief", ",", "und", "sei\u00b7ner", "S\u00f6h\u00b7ne", "Wuth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sangst du Germanien; es f\u00fchlt in deinen Klagen", "tokens": ["Sangst", "du", "Ger\u00b7ma\u00b7ni\u00b7en", ";", "es", "f\u00fchlt", "in", "dei\u00b7nen", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sp\u00e4te Nachwelt noch das Weh von unsern Tagen.", "tokens": ["Die", "sp\u00e4\u00b7te", "Nach\u00b7welt", "noch", "das", "Weh", "von", "un\u00b7sern", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Richter! wenn hinauf, wo deine Freundinn lebt,", "tokens": ["O", "Rich\u00b7ter", "!", "wenn", "hin\u00b7auf", ",", "wo", "dei\u00b7ne", "Freun\u00b7dinn", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fast unzufrieden sich dein thr\u00e4nend Aug' erhebt,", "tokens": ["Fast", "un\u00b7zu\u00b7frie\u00b7den", "sich", "dein", "thr\u00e4\u00b7nend", "Aug'", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn dir kein Tag so schwarz, als jener Tag erscheinet,", "tokens": ["Wenn", "dir", "kein", "Tag", "so", "schwarz", ",", "als", "je\u00b7ner", "Tag", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "ADJD", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seit dem dein Mitleid nicht bey ihren Schmerzen weinet,", "tokens": ["Seit", "dem", "dein", "Mit\u00b7leid", "nicht", "bey", "ih\u00b7ren", "Schmer\u00b7zen", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So denk': Wie manchen Tag die n\u00e4chste Zeit erblickt,", "tokens": ["So", "denk'", ":", "Wie", "man\u00b7chen", "Tag", "die", "n\u00e4chs\u00b7te", "Zeit", "er\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PWAV", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Seelen schaarenweis aus unsrer Welt geschickt,", "tokens": ["Der", "See\u00b7len", "schaa\u00b7ren\u00b7weis", "aus", "uns\u00b7rer", "Welt", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Um den der Gottheit hier entweyhtes Lob erklungen,", "tokens": ["Um", "den", "der", "Got\u00b7theit", "hier", "ent\u00b7weyh\u00b7tes", "Lob", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch nur der V\u00f6lker Ach vor ihren Thron gedrungen.", "tokens": ["Doch", "nur", "der", "V\u00f6l\u00b7ker", "Ach", "vor", "ih\u00b7ren", "Thron", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denk' an so manche Noth, an jenes arme Land,", "tokens": ["Denk'", "an", "so", "man\u00b7che", "Noth", ",", "an", "je\u00b7nes", "ar\u00b7me", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ADV", "PIAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo St\u00e4dte freundschaftlich ein helfend Heer verbrannt.", "tokens": ["Wo", "St\u00e4d\u00b7te", "freund\u00b7schaft\u00b7lich", "ein", "hel\u00b7fend", "Heer", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gef\u00fchl von fremder Qual wird eignen Kummer schw\u00e4chen,", "tokens": ["Ge\u00b7f\u00fchl", "von", "frem\u00b7der", "Qual", "wird", "eig\u00b7nen", "Kum\u00b7mer", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Mensch, der Patriot, mehr als der Wittwer sprechen.", "tokens": ["Der", "Mensch", ",", "der", "Pat\u00b7ri\u00b7ot", ",", "mehr", "als", "der", "Witt\u00b7wer", "spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PIAT", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch kennt des Leidens Maa\u00df, das Er dir zugetheilt,", "tokens": ["Auch", "kennt", "des", "Lei\u00b7dens", "Maa\u00df", ",", "das", "Er", "dir", "zu\u00b7ge\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er, der den Kriegen steu'rt, der Erde Br\u00fcche heilt,", "tokens": ["Er", ",", "der", "den", "Krie\u00b7gen", "steu'rt", ",", "der", "Er\u00b7de", "Br\u00fc\u00b7che", "heilt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, wenn Geduld sich nur, wie Er befiehlt, gew\u00f6hnet,", "tokens": ["Und", ",", "wenn", "Ge\u00b7duld", "sich", "nur", ",", "wie", "Er", "be\u00b7fiehlt", ",", "ge\u00b7w\u00f6h\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "NN", "PRF", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie tr\u00f6stend hier erquickt, und dorten gn\u00e4dig kr\u00f6net.", "tokens": ["Sie", "tr\u00f6s\u00b7tend", "hier", "er\u00b7quickt", ",", "und", "dor\u00b7ten", "gn\u00e4\u00b7dig", "kr\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVPP", "$,", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}