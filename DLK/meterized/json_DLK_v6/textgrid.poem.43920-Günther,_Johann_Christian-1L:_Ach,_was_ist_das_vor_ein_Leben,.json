{"textgrid.poem.43920": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, was ist das vor ein Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was ist das vor ein Leben,", "tokens": ["Ach", ",", "was", "ist", "das", "vor", "ein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemahls recht verliebt zu seyn!", "tokens": ["Nie\u00b7mahls", "recht", "ver\u00b7liebt", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts kan Trost im Ungl\u00fcck geben", "tokens": ["Nichts", "kan", "Trost", "im", "Un\u00b7gl\u00fcck", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Ku\u00df voll s\u00fc\u00dfer Pein.", "tokens": ["Als", "ein", "Ku\u00df", "voll", "s\u00fc\u00b7\u00dfer", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reizt mich nicht an gro\u00dfe Tittel", "tokens": ["Reizt", "mich", "nicht", "an", "gro\u00b7\u00dfe", "Tit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder r\u00fchmt mir etwan Geld;", "tokens": ["O\u00b7der", "r\u00fchmt", "mir", "et\u00b7wan", "Geld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Redligkeit im Kittel", "tokens": ["Sch\u00f6\u00b7ne", "Red\u00b7lig\u00b7keit", "im", "Kit\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mein h\u00f6chstes Gut der Welt.", "tokens": ["Ist", "mein", "h\u00f6chs\u00b7tes", "Gut", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neider fluchen, Sp\u00f6tter kr\u00e4ncken,", "tokens": ["Nei\u00b7der", "flu\u00b7chen", ",", "Sp\u00f6t\u00b7ter", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles hof ich auszustehn,", "tokens": ["Al\u00b7les", "hof", "ich", "aus\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4st mich nur dein Angedencken", "tokens": ["L\u00e4st", "mich", "nur", "dein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Hofnungsrosen gehn.", "tokens": ["Auf", "den", "Hof\u00b7nungs\u00b7ro\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem Hauche deiner Lippen", "tokens": ["Nach", "dem", "Hau\u00b7che", "dei\u00b7ner", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht der Sehnsucht schneller Kahn:", "tokens": ["Geht", "der", "Sehn\u00b7sucht", "schnel\u00b7ler", "Kahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Lieb ein Meer voll Klippen,", "tokens": ["Ist", "die", "Lieb", "ein", "Meer", "voll", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm nur mich zum Ancker an!", "tokens": ["Nimm", "nur", "mich", "zum", "An\u00b7cker", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}