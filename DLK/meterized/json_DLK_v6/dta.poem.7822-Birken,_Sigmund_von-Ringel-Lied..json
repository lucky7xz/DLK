{"dta.poem.7822": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Ringel-Lied.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Am Leyta-Strand/ wo er die Donau gr\u00fcsset", "tokens": ["Am", "Ley\u00b7ta\u00b7Strand", "/", "wo", "er", "die", "Do\u00b7nau", "gr\u00fcs\u00b7set"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "PWAV", "PPER", "ART", "NE", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "von ferne schon/ das gr\u00fcne Ufer k\u00fcsset", "tokens": ["von", "fer\u00b7ne", "schon", "/", "das", "gr\u00fc\u00b7ne", "U\u00b7fer", "k\u00fcs\u00b7set"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Trautmans-Burg/ im alten Pan\u0304ons-", "tokens": ["der", "Traut\u00b7mans\u00b7Burg", "/", "im", "al\u00b7ten", "Pan\u0304\u00b7ons"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "lie\u00df\u2019 Artaban\u2019/ ein Edler Held/ erklingen", "tokens": ["lie\u00df'", "A\u00b7rtaban'", "/", "ein", "Ed\u00b7ler", "Held", "/", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "sein Lied von Leid. Ihn h\u00f6rt\u2019/ ein Hirte/ singe\u0304/", "tokens": ["sein", "Lied", "von", "Leid", ".", "Ihn", "h\u00f6rt'", "/", "ein", "Hir\u00b7te", "/", "sing\u0113", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "$(", "ART", "NN", "$(", "XY", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "(de", "tokens": ["(", "de"], "token_info": ["punct", "word"], "pos": ["$(", "NE"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Mein S\u00e4itenspiel! hang du an dieser Wei", "tokens": ["Mein", "S\u00e4i\u00b7ten\u00b7spiel", "!", "hang", "du", "an", "die\u00b7ser", "Wei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mach/ wie du pflegst/ itzund mir keine Freud", "tokens": ["mach", "/", "wie", "du", "pflegst", "/", "it\u00b7zund", "mir", "kei\u00b7ne", "Freud"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ADV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Unmut nur mein Schmerz sich tr\u00f6-", "tokens": ["Mit", "Un\u00b7mut", "nur", "mein", "Schmerz", "sich", "tr\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nein! kom\u0304 her! du must mir helfen sag", "tokens": ["Doch", "nein", "!", "kom\u0304", "her", "!", "du", "must", "mir", "hel\u00b7fen", "sag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "von meinem Leid/ du must mir helfen klagen", "tokens": ["von", "mei\u00b7nem", "Leid", "/", "du", "must", "mir", "hel\u00b7fen", "kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Emilie ist todt! und ich kan leben?", "tokens": ["E\u00b7mi\u00b7lie", "ist", "todt", "!", "und", "ich", "kan", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ward nicht/ mein Geist/ mit ihrem hingegebe\u0304", "tokens": ["ward", "nicht", "/", "mein", "Geist", "/", "mit", "ih\u00b7rem", "hin\u00b7ge\u00b7ge\u00b7b\u0113"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "wie da\u00df ich noch/ da sie gefallen/ steb?", "tokens": ["wie", "da\u00df", "ich", "noch", "/", "da", "sie", "ge\u00b7fal\u00b7len", "/", "steb", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "$(", "KOUS", "PPER", "VVPP", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Faunen ihr/ in diesen W\u00e4ldereyen!", "tokens": ["Ihr", "Fau\u00b7nen", "ihr", "/", "in", "die\u00b7sen", "W\u00e4l\u00b7de\u00b7re\u00b7yen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "ihr Satyren! ihr Hirten! helft mir schreyen", "tokens": ["ihr", "Sa\u00b7ty\u00b7ren", "!", "ihr", "Hir\u00b7ten", "!", "helft", "mir", "schre\u00b7yen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr thut Gewalt/ ihr Fata! du Gesetze", "tokens": ["Ihr", "thut", "Ge\u00b7walt", "/", "ihr", "Fa\u00b7ta", "!", "du", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "PPOSAT", "NN", "$.", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Sterblichkeit! und du des Todes Netze!", "tokens": ["der", "Sterb\u00b7lich\u00b7keit", "!", "und", "du", "des", "To\u00b7des", "Net\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "di\u00df euer Thun/ hat keine Rechts-gestalt.", "tokens": ["di\u00df", "eu\u00b7er", "Thun", "/", "hat", "kei\u00b7ne", "Rechts\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$(", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr t\u00f6dt: was lebt zu aller Welt Behagen/", "tokens": ["Ihr", "t\u00f6dt", ":", "was", "lebt", "zu", "al\u00b7ler", "Welt", "Be\u00b7ha\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wz liebt und labt. Ich mu\u00df noch einmal sage\u0304:", "tokens": ["wz", "liebt", "und", "labt", ".", "Ich", "mu\u00df", "noch", "ein\u00b7mal", "sag\u0113", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gern wolt ich ja/ O Himmel/ deinem Willen", "tokens": ["Gern", "wolt", "ich", "ja", "/", "O", "Him\u00b7mel", "/", "dei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$(", "NE", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "itzt geben Ehr: das Herz ist nicht zustillen.", "tokens": ["itzt", "ge\u00b7ben", "Ehr", ":", "das", "Herz", "ist", "nicht", "zu\u00b7stil\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "NN", "$.", "ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch ist vo\u0304 Ihr/ dz Schmerz-andenken da.", "tokens": ["Noch", "ist", "v\u014d", "Ihr", "/", "dz", "Schmer\u00b7zan\u00b7den\u00b7ken", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "$(", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Leib/ sie starb: der Liebe Sie noch lebet.", "tokens": ["Dem", "Leib", "/", "sie", "starb", ":", "der", "Lie\u00b7be", "Sie", "noch", "le\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "$.", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach! die Vernunft der Angst nit widerstrebet:", "tokens": ["Ach", "!", "die", "Ver\u00b7nunft", "der", "Angst", "nit", "wi\u00b7der\u00b7stre\u00b7bet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ach helft mir doch dem hartem Himmel flehen/", "tokens": ["Ach", "helft", "mir", "doch", "dem", "har\u00b7tem", "Him\u00b7mel", "fle\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ihr Saiten ihr! ihn so erweicht zusehen:", "tokens": ["ihr", "Sai\u00b7ten", "ihr", "!", "ihn", "so", "er\u00b7weicht", "zu\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$.", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df er mir g\u00f6nn die Sch\u00f6nste l\u00e4nger noch.", "tokens": ["da\u00df", "er", "mir", "g\u00f6nn", "die", "Sch\u00f6ns\u00b7te", "l\u00e4n\u00b7ger", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ob ihr zu euch/ ihr Sternen/ Sie genommen", "tokens": ["Ob", "ihr", "zu", "euch", "/", "ihr", "Ster\u00b7nen", "/", "Sie", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "in eure Zahl: lasst Sie mir widerkommen!", "tokens": ["in", "eu\u00b7re", "Zahl", ":", "lasst", "Sie", "mir", "wi\u00b7der\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es ist noch Zeit. Kein Mensch kan euch ent-\nlaufen/ ", "tokens": ["Es", "ist", "noch", "Zeit", ".", "Kein", "Mensch", "kan", "euch", "ent", "lau\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "PIAT", "NN", "VMFIN", "PPER", "TRUNC", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(fen", "tokens": ["(", "fen"], "token_info": ["punct", "word"], "pos": ["$(", "VVINF"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "ihr Parcen ihr! man komt dem Todten-hau-", "tokens": ["ihr", "Par\u00b7cen", "ihr", "!", "man", "komt", "dem", "Tod\u00b7ten\u00b7hau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "$.", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "doch endlich zu. Hier ist kein\u2019 Ewigkeit.", "tokens": ["doch", "end\u00b7lich", "zu", ".", "Hier", "ist", "kein'", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lass du Sie nur mich noch ein Zeitlang laben!", "tokens": ["Lass", "du", "Sie", "nur", "mich", "noch", "ein", "Zeit\u00b7lang", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "du solst sie doch/ O Himmel! einmal haben:", "tokens": ["du", "solst", "sie", "doch", "/", "O", "Him\u00b7mel", "!", "ein\u00b7mal", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$(", "NE", "NN", "$.", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Erlaubt es nicht dein Raht/ sie mir zu geben", "tokens": ["Er\u00b7laubt", "es", "nicht", "dein", "Raht", "/", "sie", "mir", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$(", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "so nim\u0304 auch mir das nun als-todte Leben.", "tokens": ["so", "nim\u0304", "auch", "mir", "das", "nun", "als\u00b7tod\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wan\u0304 meine Klag dir nicht dz Herze bricht:", "tokens": ["Wan\u0304", "mei\u00b7ne", "Klag", "dir", "nicht", "dz", "Her\u00b7ze", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PTKNEG", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mag der Tod hier di\u00df/ das meine/ brechen", "tokens": ["So", "mag", "der", "Tod", "hier", "di\u00df", "/", "das", "mei\u00b7ne", "/", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PDS", "$(", "PDS", "VVFIN", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch du bist ja so hart nit! dein Versprechen", "tokens": ["Doch", "du", "bist", "ja", "so", "hart", "nit", "!", "dein", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So sehnlich sang/ in s\u00fcssen Klang der S\u00e4item/", "tokens": ["So", "sehn\u00b7lich", "sang", "/", "in", "s\u00fcs\u00b7sen", "Klang", "der", "S\u00e4i\u00b7tem", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$(", "APPR", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Edle Held. Es h\u00f6rten Ihm/ von weiten/", "tokens": ["der", "Ed\u00b7le", "Held", ".", "Es", "h\u00f6r\u00b7ten", "Ihm", "/", "von", "wei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$(", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Nymfen zu. Das Wild/ zu Ihm sich", "tokens": ["die", "Nym\u00b7fen", "zu", ".", "Das", "Wild", "/", "zu", "Ihm", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "$(", "APPR", "PPER", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es merkten auf/ die sch