"token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tausendmal an dich!", "tokens": ["Tau\u00b7send\u00b7mal", "an", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Phyllis, so mich zu vergessen?", "tokens": ["Phyl\u00b7lis", ",", "so", "mich", "zu", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Lieber hasse mich!", "tokens": ["Lie\u00b7ber", "has\u00b7se", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}