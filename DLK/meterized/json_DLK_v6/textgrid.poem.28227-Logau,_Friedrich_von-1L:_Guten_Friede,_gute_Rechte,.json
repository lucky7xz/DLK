{"textgrid.poem.28227": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Guten Friede, gute Rechte,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten Friede, gute Rechte,", "tokens": ["Gu\u00b7ten", "Frie\u00b7de", ",", "gu\u00b7te", "Rech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Tage, gute N\u00e4chte,", "tokens": ["Gu\u00b7te", "Ta\u00b7ge", ",", "gu\u00b7te", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gut Gewitter, gute Zeiten,", "tokens": ["Gut", "Ge\u00b7wit\u00b7ter", ",", "gu\u00b7te", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gut zu melcken, gut zu reiten,", "tokens": ["Gut", "zu", "mel\u00b7cken", ",", "gut", "zu", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lauter G\u00fct und gute Gaben", "tokens": ["Lau\u00b7ter", "G\u00fct", "und", "gu\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wolln die Menschen h\u00e4uffig haben,", "tokens": ["Wolln", "die", "Men\u00b7schen", "h\u00e4uf\u00b7fig", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denen doch an Leib und Mute", "tokens": ["De\u00b7nen", "doch", "an", "Leib", "und", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbsten mangelt alles gute.", "tokens": ["Selbs\u00b7ten", "man\u00b7gelt", "al\u00b7les", "gu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}