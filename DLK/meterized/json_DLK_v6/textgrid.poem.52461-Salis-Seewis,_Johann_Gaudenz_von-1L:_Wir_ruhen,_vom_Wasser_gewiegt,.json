{"textgrid.poem.52461": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir ruhen, vom Wasser gewiegt,", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir ruhen, vom Wasser gewiegt,", "tokens": ["Wir", "ru\u00b7hen", ",", "vom", "Was\u00b7ser", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Im Kreise vertraulich und enge;", "tokens": ["Im", "Krei\u00b7se", "ver\u00b7trau\u00b7lich", "und", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch Eintracht wie Blumengeh\u00e4nge", "tokens": ["Durch", "Ein\u00b7tracht", "wie", "Blu\u00b7men\u00b7ge\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verkn\u00fcpft und in Reihen gef\u00fcgt;", "tokens": ["Ver\u00b7kn\u00fcpft", "und", "in", "Rei\u00b7hen", "ge\u00b7f\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Uns sondert von l\u00e4stiger Menge", "tokens": ["Uns", "son\u00b7dert", "von", "l\u00e4s\u00b7ti\u00b7ger", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Flut, die den Nachen umschmiegt.", "tokens": ["Die", "Flut", ",", "die", "den", "Na\u00b7chen", "um\u00b7schmiegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "So gleiten, im Raume vereint,", "tokens": ["So", "glei\u00b7ten", ",", "im", "Rau\u00b7me", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wir auf der Verg\u00e4nglichkeit Wellen,", "tokens": ["Wir", "auf", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wo Freunde sich innig gesellen", "tokens": ["Wo", "Freun\u00b7de", "sich", "in\u00b7nig", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "ADJD", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zum Freunde, der redlich es meint!", "tokens": ["Zum", "Freun\u00b7de", ",", "der", "red\u00b7lich", "es", "meint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Getrost, weil die dunkelsten Stellen", "tokens": ["Ge\u00b7trost", ",", "weil", "die", "dun\u00b7kels\u00b7ten", "Stel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein Glanz aus der H\u00f6he bescheint.", "tokens": ["Ein", "Glanz", "aus", "der", "H\u00f6\u00b7he", "be\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ach! tr\u00fcg' uns die f\u00e4hrliche Flut", "tokens": ["Ach", "!", "tr\u00fcg'", "uns", "die", "f\u00e4hr\u00b7li\u00b7che", "Flut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Des Lebens so friedlich und leise!", "tokens": ["Des", "Le\u00b7bens", "so", "fried\u00b7lich", "und", "lei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O drohte nie Trennung dem Kreise,", "tokens": ["O", "droh\u00b7te", "nie", "Tren\u00b7nung", "dem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der, sorglos um Zukunft, hier ruht!", "tokens": ["Der", ",", "sorg\u00b7los", "um", "Zu\u00b7kunft", ",", "hier", "ruht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "O n\u00e4hm' uns am Ziele der Reise", "tokens": ["O", "n\u00e4hm'", "uns", "am", "Zie\u00b7le", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Elysiums Busen in Hut!", "tokens": ["E\u00b7ly\u00b7si\u00b7ums", "Bu\u00b7sen", "in", "Hut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Verhallen mag unser Gesang,", "tokens": ["Ver\u00b7hal\u00b7len", "mag", "un\u00b7ser", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie Fl\u00f6tenhauch schwinden das Leben:", "tokens": ["Wie", "Fl\u00f6\u00b7ten\u00b7hauch", "schwin\u00b7den", "das", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit Jubel und Seufzern verschweben", "tokens": ["Mit", "Ju\u00b7bel", "und", "Seuf\u00b7zern", "ver\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Des Daseins zerflie\u00dfender Klang!", "tokens": ["Des", "Da\u00b7seins", "zer\u00b7flie\u00b7\u00dfen\u00b7der", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der Geist wird verkl\u00e4rt sich erheben,", "tokens": ["Der", "Geist", "wird", "ver\u00b7kl\u00e4rt", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wenn Lethe sein Fahrzeug verschlang.", "tokens": ["Wenn", "Le\u00b7the", "sein", "Fahr\u00b7zeug", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}