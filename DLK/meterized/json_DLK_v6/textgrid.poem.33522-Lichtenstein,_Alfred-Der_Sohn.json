{"textgrid.poem.33522": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, halte mich nicht,", "tokens": ["Mut\u00b7ter", ",", "hal\u00b7te", "mich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mutter, dein Streicheln tut weh,", "tokens": ["Mut\u00b7ter", ",", "dein", "Strei\u00b7cheln", "tut", "weh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sieh durch mein Gesicht,", "tokens": ["Sieh", "durch", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ich gl\u00fch und vergeh.", "tokens": ["Wie", "ich", "gl\u00fch", "und", "ver\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gib den letzten Ku\u00df. La\u00df mich frei.", "tokens": ["Gib", "den", "letz\u00b7ten", "Ku\u00df", ".", "La\u00df", "mich", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schick mir Gebete nach.", "tokens": ["Schick", "mir", "Ge\u00b7be\u00b7te", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df ich dein Leben zerbrach,", "tokens": ["Da\u00df", "ich", "dein", "Le\u00b7ben", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mutter, verzeih.", "tokens": ["Mut\u00b7ter", ",", "ver\u00b7zeih", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}