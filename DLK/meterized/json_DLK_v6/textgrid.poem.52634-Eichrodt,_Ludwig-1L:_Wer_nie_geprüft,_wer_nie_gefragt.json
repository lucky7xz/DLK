{"textgrid.poem.52634": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer nie gepr\u00fcft, wer nie gefragt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nie gepr\u00fcft, wer nie gefragt", "tokens": ["Wer", "nie", "ge\u00b7pr\u00fcft", ",", "wer", "nie", "ge\u00b7fragt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "$,", "PWS", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach aller Dinge Grund,", "tokens": ["Nach", "al\u00b7ler", "Din\u00b7ge", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nie um Wahrheit sich geplagt,", "tokens": ["Wer", "nie", "um", "Wahr\u00b7heit", "sich", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob sie ihm werde kund?", "tokens": ["Ob", "sie", "ihm", "wer\u00b7de", "kund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer nicht den freien Blick bewahrt", "tokens": ["Wer", "nicht", "den", "frei\u00b7en", "Blick", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vorwelt und der Gegenwart,", "tokens": ["Der", "Vor\u00b7welt", "und", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die er genie\u00dft, begreift \u2013", "tokens": ["Die", "er", "ge\u00b7nie\u00dft", ",", "be\u00b7greift", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ist nicht zum Mann gereift.", "tokens": ["Ist", "nicht", "zum", "Mann", "ge\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer nie an einer That der Kraft", "tokens": ["Wer", "nie", "an", "ei\u00b7ner", "That", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den frischen Geist gelabt,", "tokens": ["Den", "fri\u00b7schen", "Geist", "ge\u00b7labt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer niemals eine Leidenschaft", "tokens": ["Wer", "nie\u00b7mals", "ei\u00b7ne", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lebenlang gehabt,", "tokens": ["Sein", "Le\u00b7ben\u00b7lang", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nie ein \u00fcberschw\u00e4nglich Gl\u00fcck,", "tokens": ["Und", "nie", "ein", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und keinen gro\u00dfen Augenblick,", "tokens": ["Und", "kei\u00b7nen", "gro\u00b7\u00dfen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der l\u00e4utert und erhebt \u2013", "tokens": ["Der", "l\u00e4u\u00b7tert", "und", "er\u00b7hebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Umsonst hat er gelebt!", "tokens": ["Um\u00b7sonst", "hat", "er", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wer nie ein liebes Weib umarmt,", "tokens": ["Wer", "nie", "ein", "lie\u00b7bes", "Weib", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Becher nie gek\u00fc\u00dft,", "tokens": ["Den", "Be\u00b7cher", "nie", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist an Leib und Seel verarmt,", "tokens": ["Er", "ist", "an", "Leib", "und", "Seel", "ver\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem Stern gegr\u00fc\u00dft;", "tokens": ["Von", "kei\u00b7nem", "Stern", "ge\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem hat sich auch kein Freund gesellt,", "tokens": ["Dem", "hat", "sich", "auch", "kein", "Freund", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der schleichet durch die sch\u00f6ne Welt", "tokens": ["Der", "schlei\u00b7chet", "durch", "die", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Trostverla\u00dfner hin \u2013", "tokens": ["Ein", "Trost\u00b7ver\u00b7la\u00df\u00b7ner", "hin", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er hat es nicht Gewinn.", "tokens": ["Er", "hat", "es", "nicht", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}