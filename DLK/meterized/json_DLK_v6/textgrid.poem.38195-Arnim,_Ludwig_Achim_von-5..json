{"textgrid.poem.38195": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach hartes Herz, la\u00df dich doch eins erweichen,", "tokens": ["Ach", "har\u00b7tes", "Herz", ",", "la\u00df", "dich", "doch", "eins", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mich zu deiner Huld doch noch gereichen;", "tokens": ["La\u00df", "mich", "zu", "dei\u00b7ner", "Huld", "doch", "noch", "ge\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wen sollt doch nicht erbarmen,", "tokens": ["Wen", "sollt", "doch", "nicht", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich mu\u00df als erarmen.", "tokens": ["Da\u00df", "ich", "mu\u00df", "als", "er\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach starker Fels, la\u00df dich doch eins bewegen,", "tokens": ["Ach", "star\u00b7ker", "Fels", ",", "la\u00df", "dich", "doch", "eins", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thu dein gewohnte H\u00e4rt eins von dir legen,", "tokens": ["Thu", "dein", "ge\u00b7wohn\u00b7te", "H\u00e4rt", "eins", "von", "dir", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wen sollt doch nicht erbarmen,", "tokens": ["Wen", "sollt", "doch", "nicht", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich mu\u00df als erarmen.", "tokens": ["Da\u00df", "ich", "mu\u00df", "als", "er\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach veste Burg, la\u00df dich doch eins gewinnen,", "tokens": ["Ach", "ves\u00b7te", "Burg", ",", "la\u00df", "dich", "doch", "eins", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach reicher Brunn, la\u00df mich nicht gar verbrinnen;", "tokens": ["Ach", "rei\u00b7cher", "Brunn", ",", "la\u00df", "mich", "nicht", "gar", "ver\u00b7brin\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3"