{"textgrid.poem.42754": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Logik", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war kalt und sternenklar,", "tokens": ["Die", "Nacht", "war", "kalt", "und", "ster\u00b7nen\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da trieb im Meer bei Norderney", "tokens": ["Da", "trieb", "im", "Meer", "bei", "Nor\u00b7der\u00b7ney"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Suahelischnurrbarthaar. \u2013", "tokens": ["Ein", "Su\u00b7a\u00b7he\u00b7lischnurr\u00b7bart\u00b7haar", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die n\u00e4chste Schiffsuhr wies auf drei.", "tokens": ["Die", "n\u00e4chs\u00b7te", "Schiff\u00b7suhr", "wies", "auf", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir scheint da mancherlei nicht klar,", "tokens": ["Mir", "scheint", "da", "man\u00b7cher\u00b7lei", "nicht", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man fragt doch, wenn man Logik hat,", "tokens": ["Man", "fragt", "doch", ",", "wenn", "man", "Lo\u00b7gik", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "PIS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sucht ein Suahelihaar", "tokens": ["Was", "sucht", "ein", "Su\u00b7a\u00b7he\u00b7li\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn nachts um drei am Kattegatt?", "tokens": ["Denn", "nachts", "um", "drei", "am", "Kat\u00b7te\u00b7gatt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}