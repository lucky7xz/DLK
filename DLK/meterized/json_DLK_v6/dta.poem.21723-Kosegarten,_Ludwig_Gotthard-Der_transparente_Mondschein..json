{"dta.poem.21723": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der transparente Mondschein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gegr&#252;sst, sanftd&#228;mmernde Landschaft, im", "tokens": ["Sey", "mir", "ge\u00b7gr", "&#252;", "sst", ",", "sanftd", "&#228;", "mmern\u00b7de", "Land\u00b7schaft", ",", "im"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "XML_entity", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "NE", "$(", "VVFIN", "$,", "ADJD", "$(", "ADJA", "NN", "$,", "APPRART"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "t\u00e4uschenden Zwielicht!", "tokens": ["t\u00e4u\u00b7schen\u00b7den", "Zwie\u00b7licht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Siehe, wie rollet der Mond \u00fcber den Bergen", "tokens": ["Sie\u00b7he", ",", "wie", "rol\u00b7let", "der", "Mond", "\u00fc\u00b7ber", "den", "Ber\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "daher.", "tokens": ["da\u00b7her", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Gl\u00e4nzende Wolken verschleyern des Wandelnden", "tokens": ["Gl\u00e4n\u00b7zen\u00b7de", "Wol\u00b7ken", "ver\u00b7schley\u00b7ern", "des", "Wan\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.6": {"text": "freundliches Antlitz,", "tokens": ["freund\u00b7li\u00b7ches", "Ant\u00b7litz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Siehe, sie wallen hinweg, gl\u00e4nzender wandelt", "tokens": ["Sie\u00b7he", ",", "sie", "wal\u00b7len", "hin\u00b7weg", ",", "gl\u00e4n\u00b7zen\u00b7der", "wan\u00b7delt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "er hin.", "tokens": ["er", "hin", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Rings erschimmern die H\u00e4upter der Berge, die", "tokens": ["Rings", "er\u00b7schim\u00b7mern", "die", "H\u00e4up\u00b7ter", "der", "Ber\u00b7ge", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Wipfel des Waldes,", "tokens": ["Wip\u00b7fel", "des", "Wal\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Silberne Strahlen durchspr\u00fchn flitternd das s\u00e4u-", "tokens": ["Sil\u00b7ber\u00b7ne", "Strah\u00b7len", "durch\u00b7spr\u00fchn", "flit\u00b7ternd", "das", "s\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "ART", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Schau, es gl\u00e4nzet der Bach, und jegliche zitternde", "tokens": ["Schau", ",", "es", "gl\u00e4n\u00b7zet", "der", "Bach", ",", "und", "jeg\u00b7li\u00b7che", "zit\u00b7tern\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Welle", "tokens": ["Wel\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Spiegelt dein leuchtendes Bild, freundliche Lu-", "tokens": ["Spie\u00b7gelt", "dein", "leuch\u00b7ten\u00b7des", "Bild", ",", "freund\u00b7li\u00b7che", "Lu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Komm, Geliebte, mit mir in die traulich d\u00e4mmernde", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7te", ",", "mit", "mir", "in", "die", "trau\u00b7lich", "d\u00e4m\u00b7mern\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPER", "APPR", "ART", "ADJD", "ADJA"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Gegend,", "tokens": ["Ge\u00b7gend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Hier durch die thauige Flur, dort durch das", "tokens": ["Hier", "durch", "die", "thau\u00b7i\u00b7ge", "Flur", ",", "dort", "durch", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "grasichte Ried,", "tokens": ["gra\u00b7sich\u00b7te", "Ried", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Hier an den blumigen Rand des kalmusduftenden", "tokens": ["Hier", "an", "den", "blu\u00b7mi\u00b7gen", "Rand", "des", "kal\u00b7mus\u00b7duf\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Sees,", "tokens": ["Sees", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Dort in den d\u00e4mmernden Hayn, drinnen die", "tokens": ["Dort", "in", "den", "d\u00e4m\u00b7mern\u00b7den", "Hayn", ",", "drin\u00b7nen", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ART"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Nachtigal schl\u00e4gt.", "tokens": ["Nach\u00b7ti\u00b7gal", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Horch, wie sie schl\u00e4gt! Wie d\u00e4mmerts im Hayn!", "tokens": ["Horch", ",", "wie", "sie", "schl\u00e4gt", "!", "Wie", "d\u00e4m\u00b7merts", "im", "Hayn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$.", "PWAV", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Hellsilberne Tropfen", "tokens": ["Hell\u00b7sil\u00b7ber\u00b7ne", "Trop\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Regnen die Wipfel herab. K\u00fchl ists und schau-", "tokens": ["Reg\u00b7nen", "die", "Wip\u00b7fel", "her\u00b7ab", ".", "K\u00fchl", "ists", "und", "schau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$.", "NN", "VAFIN", "KON", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Inniger, meine Geliebte, umflicht mich, noch inniger,", "tokens": ["In\u00b7ni\u00b7ger", ",", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", ",", "um\u00b7flicht", "mich", ",", "noch", "in\u00b7ni\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "enger.", "tokens": ["en\u00b7ger", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Schaurig und k\u00fchl ist der Hayn, einsam und", "tokens": ["Schau\u00b7rig", "und", "k\u00fchl", "ist", "der", "Hayn", ",", "ein\u00b7sam", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "KON"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.29": {"text": "schaurig die Nacht \u2014 \u2014", "tokens": ["schau\u00b7rig", "die", "Nacht"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "NN", "$(", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "Bey den Schatten des Hayns, bey des Waldes hei-", "tokens": ["Bey", "den", "Schat\u00b7ten", "des", "Hayns", ",", "bey", "des", "Wal\u00b7des", "hei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Bey den Gestalten, die blass wanken im ra-", "tokens": ["Bey", "den", "Ge\u00b7stal\u00b7ten", ",", "die", "blass", "wan\u00b7ken", "im", "ra"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.32": {"text": "Bey den Schauern der Nacht, bey jenem rollenden", "tokens": ["Bey", "den", "Schau\u00b7ern", "der", "Nacht", ",", "bey", "je\u00b7nem", "rol\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PDAT", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Monde,", "tokens": ["Mon\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "Welcher dein Antlitz bestrahlt, welcher dein", "tokens": ["Wel\u00b7cher", "dein", "Ant\u00b7litz", "be\u00b7strahlt", ",", "wel\u00b7cher", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPOSAT"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.35": {"text": "Auge verkl\u00e4rt \u2014", "tokens": ["Au\u00b7ge", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "Wie ich dich liebe, Geliebte, so liebte dich keiner,", "tokens": ["Wie", "ich", "dich", "lie\u00b7be", ",", "Ge\u00b7lieb\u00b7te", ",", "so", "lieb\u00b7te", "dich", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "so liebet", "tokens": ["so", "lie\u00b7bet"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Keiner in Ewigkeit dich, ewiglich lieb ich", "tokens": ["Kei\u00b7ner", "in", "E\u00b7wig\u00b7keit", "dich", ",", "e\u00b7wig\u00b7lich", "lieb", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "APPR", "NN", "PPER", "$,", "ADV", "ADJD", "PPER"], "meter": "+--+-+-+-++-", "measure": "iambic.hexa.invert"}, "line.39": {"text": "dich so.", "tokens": ["dich", "so", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Bey den D\u00fcften des Hayns, bey dem Ambraathem", "tokens": ["Bey", "den", "D\u00fcf\u00b7ten", "des", "Hayns", ",", "bey", "dem", "A\u00b7mbraa\u00b7them"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "der Matten,", "tokens": ["der", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Bey den Stimmen, die rings fl\u00fcstern im s\u00e4useln-", "tokens": ["Bey", "den", "Stim\u00b7men", ",", "die", "rings", "fl\u00fcs\u00b7tern", "im", "s\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPRART", "TRUNC"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.43": {"text": "Bey den Gewalten der Nacht, bey jenem rollenden", "tokens": ["Bey", "den", "Ge\u00b7wal\u00b7ten", "der", "Nacht", ",", "bey", "je\u00b7nem", "rol\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PDAT", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.44": {"text": "Monde,", "tokens": ["Mon\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Der in der Thr\u00e4ne gl\u00e4nzt, die in den Wimpern", "tokens": ["Der", "in", "der", "Thr\u00e4\u00b7ne", "gl\u00e4nzt", ",", "die", "in", "den", "Wim\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "dir bebt \u2014 \u2014", "tokens": ["dir", "bebt"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "$("], "meter": "-+", "measure": "iambic.single"}, "line.47": {"text": "Liebe, liebe auch mich, wie ich dich liebe, Ge-", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", "auch", "mich", ",", "wie", "ich", "dich", "lie\u00b7be", ",", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "TRUNC"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "Herzlich und schmerzlich und wahr liebe, Ge-", "tokens": ["Herz\u00b7lich", "und", "schmerz\u00b7lich", "und", "wahr", "lie\u00b7be", ",", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VVFIN", "$,", "TRUNC"], "meter": "+--+-+++-+", "measure": "iambic.hexa.invert"}, "line.49": {"text": "Inniger, Traute, umflicht mich, noch br\u00fcnstiger,", "tokens": ["In\u00b7ni\u00b7ger", ",", "Trau\u00b7te", ",", "um\u00b7flicht", "mich", ",", "noch", "br\u00fcns\u00b7ti\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.50": {"text": "inniger, enger!", "tokens": ["in\u00b7ni\u00b7ger", ",", "en\u00b7ger", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "Beben Entz\u00fcckungen nicht rings durch das Herz", "tokens": ["Be\u00b7ben", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "nicht", "rings", "durch", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.52": {"text": "der Natur?", "tokens": ["der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.53": {"text": "Zittern nicht funkelnde Thr\u00e4nen auf Gr\u00e4sern und", "tokens": ["Zit\u00b7tern", "nicht", "fun\u00b7keln\u00b7de", "Thr\u00e4\u00b7nen", "auf", "Gr\u00e4\u00b7sern", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.54": {"text": "Bl\u00e4ttern und Blumen,", "tokens": ["Bl\u00e4t\u00b7tern", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "Str\u00f6mt nicht edenischer Glanz, magische Hel-", "tokens": ["Str\u00f6mt", "nicht", "e\u00b7de\u00b7ni\u00b7scher", "Glanz", ",", "ma\u00b7gi\u00b7sche", "Hel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.56": {"text": "Weinet nicht l\u00e4chelnd der Mond? erschauert nicht", "tokens"