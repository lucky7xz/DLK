{"textgrid.poem.61480": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wider den Schmerz dich zu vermauern,", "tokens": ["Wi\u00b7der", "den", "Schmerz", "dich", "zu", "ver\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ist so verkehrt wie ma\u00dflos Trauern;", "tokens": ["Ist", "so", "ver\u00b7kehrt", "wie", "ma\u00df\u00b7los", "Trau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sollst von ihm dich mahnen lassen,", "tokens": ["Du", "sollst", "von", "ihm", "dich", "mah\u00b7nen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dir dein H\u00f6chstes doppelt fest zu fassen.", "tokens": ["In", "dir", "dein", "H\u00f6chs\u00b7tes", "dop\u00b7pelt", "fest", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}