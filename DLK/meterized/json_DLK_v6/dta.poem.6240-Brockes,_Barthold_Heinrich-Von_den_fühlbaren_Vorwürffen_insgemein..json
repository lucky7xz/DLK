{"dta.poem.6240": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Theilchen wird bewegt, ein C\u00f6rper legt sich an,", "tokens": ["Ein", "Theil\u00b7chen", "wird", "be\u00b7wegt", ",", "ein", "C\u00f6r\u00b7per", "legt", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es dr\u00fccket uns ein Ding, es st\u00f6sst uns, oder sticht;", "tokens": ["Es", "dr\u00fc\u00b7cket", "uns", "ein", "Ding", ",", "es", "st\u00f6sst", "uns", ",", "o\u00b7der", "sticht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So liegt doch alles, was geschicht,", "tokens": ["So", "liegt", "doch", "al\u00b7les", ",", "was", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An der Bewegung und Figur.", "tokens": ["An", "der", "Be\u00b7we\u00b7gung", "und", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}