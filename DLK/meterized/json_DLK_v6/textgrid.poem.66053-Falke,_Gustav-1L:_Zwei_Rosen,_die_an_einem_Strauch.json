{"textgrid.poem.66053": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Rosen, die an einem Strauch", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Rosen, die an einem Strauch", "tokens": ["Zwei", "Ro\u00b7sen", ",", "die", "an", "ei\u00b7nem", "Strauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen aufgebl\u00fcht,", "tokens": ["Zu\u00b7sam\u00b7men", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem knospenhaften Hauch", "tokens": ["Von", "ei\u00b7nem", "knos\u00b7pen\u00b7haf\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch lieblich \u00fcbergl\u00fcht,", "tokens": ["Noch", "lieb\u00b7lich", "\u00fc\u00b7berg\u00b7l\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4dchen brach wohl \u00fcber Tag", "tokens": ["Ein", "M\u00e4d\u00b7chen", "brach", "wohl", "\u00fc\u00b7ber", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schwesterliche Paar:", "tokens": ["Das", "schwes\u00b7ter\u00b7li\u00b7che", "Paar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mutter, die im Sterben lag,", "tokens": ["Der", "Mut\u00b7ter", ",", "die", "im", "Ster\u00b7ben", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bracht sie die eine dar,", "tokens": ["Bracht", "sie", "die", "ei\u00b7ne", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ART", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die andre aber legte dann", "tokens": ["Die", "and\u00b7re", "a\u00b7ber", "leg\u00b7te", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem ersten Schmerz", "tokens": ["Mit", "ih\u00b7rem", "ers\u00b7ten", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie weinend dem geliebten Mann,", "tokens": ["Sie", "wei\u00b7nend", "dem", "ge\u00b7lieb\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trostheischend, an das Herz,", "tokens": ["Trost\u00b7hei\u00b7schend", ",", "an", "das", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und gl\u00fchte selig auf und stund,", "tokens": ["Und", "gl\u00fch\u00b7te", "se\u00b7lig", "auf", "und", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch halb den Tod im Sinn,", "tokens": ["Noch", "halb", "den", "Tod", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bot den jungen Rosenmund", "tokens": ["Und", "bot", "den", "jun\u00b7gen", "Ro\u00b7sen\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem warmen Leben hin.", "tokens": ["Dem", "war\u00b7men", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}