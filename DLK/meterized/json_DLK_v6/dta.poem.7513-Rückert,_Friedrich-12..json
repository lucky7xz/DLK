{"dta.poem.7513": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe hab' ich einst die Frucht am Baum gesehn,", "tokens": ["Als", "Kna\u00b7be", "hab'", "ich", "einst", "die", "Frucht", "am", "Baum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sehe nun als Greis die Bl\u00fctenknospen stehn.", "tokens": ["Und", "se\u00b7he", "nun", "als", "Greis", "die", "Bl\u00fc\u00b7ten\u00b7knos\u00b7pen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vom Menschen wird nur das, was er nicht hat, gesucht,", "tokens": ["Vom", "Men\u00b7schen", "wird", "nur", "das", ",", "was", "er", "nicht", "hat", ",", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PDS", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Bl\u00fctentrieb vom Greis, vom Kind die reife Frucht.", "tokens": ["Der", "Bl\u00fc\u00b7ten\u00b7trieb", "vom", "Greis", ",", "vom", "Kind", "die", "rei\u00b7fe", "Frucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum nach reifer Frucht das Kind begierig greift?", "tokens": ["Wa\u00b7rum", "nach", "rei\u00b7fer", "Frucht", "das", "Kind", "be\u00b7gie\u00b7rig", "greift", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es die Bl\u00fct' ist, die der Frucht entgegen reift.", "tokens": ["Weil", "es", "die", "Bl\u00fct'", "ist", ",", "die", "der", "Frucht", "ent\u00b7ge\u00b7gen", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Warum das alte Herz an jungen Trieben h\u00e4ngt?", "tokens": ["Wa\u00b7rum", "das", "al\u00b7te", "Herz", "an", "jun\u00b7gen", "Trie\u00b7ben", "h\u00e4ngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil die getriebne Frucht zu neuen Trieben dr\u00e4ngt.", "tokens": ["Weil", "die", "ge\u00b7trieb\u00b7ne", "Frucht", "zu", "neu\u00b7en", "Trie\u00b7ben", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo tr\u00e4gt die Gegenwart der Zukunft Bl\u00fctenkrone?", "tokens": ["Wo", "tr\u00e4gt", "die", "Ge\u00b7gen\u00b7wart", "der", "Zu\u00b7kunft", "Bl\u00fc\u00b7ten\u00b7kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich ein Vater sieht verj\u00fcngt in seinem Sohne.", "tokens": ["Wo", "sich", "ein", "Va\u00b7ter", "sieht", "ver\u00b7j\u00fcngt", "in", "sei\u00b7nem", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der G\u00e4rtner sei gelobt, der diesen Baum begie\u00dft,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "sei", "ge\u00b7lobt", ",", "der", "die\u00b7sen", "Baum", "be\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Frucht aus Bl\u00fct' und Bl\u00fct' aus Frucht unendlich sprie\u00dft.", "tokens": ["Wo", "Frucht", "aus", "Bl\u00fct'", "und", "Bl\u00fct'", "aus", "Frucht", "un\u00b7end\u00b7lich", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}