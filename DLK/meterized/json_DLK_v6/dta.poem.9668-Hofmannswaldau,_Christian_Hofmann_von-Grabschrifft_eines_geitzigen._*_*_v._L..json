{"dta.poem.9668": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines geitzigen.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wundre dich du leser nicht/ was die ursach m\u00fcsse/ seyn/", "tokens": ["Wund\u00b7re", "dich", "du", "le\u00b7ser", "nicht", "/", "was", "die", "ur\u00b7sach", "m\u00fcs\u00b7se", "/", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$(", "PWS", "ART", "NN", "VMFIN", "$(", "VAINF", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df mit eisen dieses grab \u00fcberdeckt ist/ nicht mit stein.", "tokens": ["Da\u00df", "mit", "ei\u00b7sen", "die\u00b7ses", "grab", "\u00fc\u00b7berd\u00b7eckt", "ist", "/", "nicht", "mit", "stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PDAT", "NN", "VVPP", "VAFIN", "$(", "PTKNEG", "APPR", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der/ so drinnen ruhen soll/ war wie eisern in dem leben/", "tokens": ["Der", "/", "so", "drin\u00b7nen", "ru\u00b7hen", "soll", "/", "war", "wie", "ei\u00b7sern", "in", "dem", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ADV", "VVINF", "VMFIN", "$(", "VAFIN", "KOKOM", "ADJD", "APPR", "ART", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Drum hat eisen nach dem todt eine deck\u2019 ihm m\u00fcssen geben.", "tokens": ["Drum", "hat", "ei\u00b7sen", "nach", "dem", "todt", "ei\u00b7ne", "deck'", "ihm", "m\u00fcs\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "APPR", "ART", "ADJD", "ART", "NE", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}