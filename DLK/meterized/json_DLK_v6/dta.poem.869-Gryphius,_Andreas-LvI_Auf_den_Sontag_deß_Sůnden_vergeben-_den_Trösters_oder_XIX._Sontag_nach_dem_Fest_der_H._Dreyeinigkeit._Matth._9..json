{"dta.poem.869": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LvI  Auf den Sontag de\u00df S\u016fnden vergeben-  \n den Tr\u00f6sters/ oder  XIX.  Sontag nach dem Fest  \n der H. Dreyeinigkeit. Matth. 9.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dvnckts jemand frembde da\u00df ich in d' angst verschwinde? ", "tokens": ["Dvnckts", "je\u00b7mand", "fremb\u00b7de", "da\u00df", "ich", "in", "d'", "angst", "ver\u00b7schwin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KOUS", "PPER", "APPR", "NE", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df tewrer mittelflei\u00df/ v\u00f1 werther Kr\u00e4utter macht/", "tokens": ["Da\u00df", "tew\u00b7rer", "mit\u00b7tel\u00b7flei\u00df", "/", "v\u00f1", "wert\u00b7her", "Kr\u00e4ut\u00b7ter", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$(", "NE", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df weiser Aerzte kunst mir noch nicht wieder bracht", "tokens": ["Da\u00df", "wei\u00b7ser", "A\u00b7erz\u00b7te", "kunst", "mir", "noch", "nicht", "wie\u00b7der", "bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN"], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Was Sucht vnd Angst verzehrt? die grim\u0303e pein die S\u00fcnde", "tokens": ["Was", "Sucht", "vnd", "Angst", "ver\u00b7zehrt", "?", "die", "grim\u0303e", "pein", "die", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Greifft mich von innen an. Mein ", "tokens": ["Greifft", "mich", "von", "in\u00b7nen", "an", ".", "Mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PRF", "APPR", "ADV", "PTKVZ", "$.", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da\u00df alles nur vmbsonst nach dem ein ", "tokens": ["Da\u00df", "al\u00b7les", "nur", "vmbsonst", "nach", "dem", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "ART", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weil diese Gifft noch wehr\u2019t. Kom\u2019 eh\u2019 ich gantz ver-", "tokens": ["Weil", "die\u00b7se", "Gifft", "noch", "wehr'", "t.", "Kom'", "eh'", "ich", "gantz", "ver"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADJD", "ADJA", "NN", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schmacht", "tokens": ["schmacht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "O S\u00fcnden tilger komm/ komm eilendts vnd entbinde", "tokens": ["O", "S\u00fcn\u00b7den", "til\u00b7ger", "komm", "/", "komm", "ei\u00b7lendts", "vnd", "ent\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJA", "VVFIN", "$(", "VVFIN", "NE", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein fest vestricktes Hertz das so voll Bo\u00dfheit steckt/ ", "tokens": ["Mein", "fest", "vest\u00b7rick\u00b7tes", "Hertz", "das", "so", "voll", "Bo\u00df\u00b7heit", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "ART", "ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da rohe Sicherheit/ Seuch vber Seuchen heckt.", "tokens": ["Da", "ro\u00b7he", "Si\u00b7cher\u00b7heit", "/", "Seuch", "vber", "Seu\u00b7chen", "heckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$(", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sprich: Sey getrost mein Kind! Ich habe dir vergeben", "tokens": ["Sprich", ":", "Sey", "ge\u00b7trost", "mein", "Kind", "!", "Ich", "ha\u00b7be", "dir", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "VAIMP", "ADJD", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wormit du mich erz\u00fcrnt: Ich habe deine Noth ", "tokens": ["Wor\u00b7mit", "du", "mich", "er\u00b7z\u00fcrnt", ":", "Ich", "ha\u00b7be", "dei\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$.", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewendet: ja dein Crentz geendet; vnd den Tod", "tokens": ["Ge\u00b7wen\u00b7det", ":", "ja", "dein", "Crentz", "ge\u00b7en\u00b7det", ";", "vnd", "den", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "ADV", "PPOSAT", "NN", "VVPP", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschlungen/ da\u00df du magst vnendlich f\u00fcr mir leben.", "tokens": ["Ver\u00b7schlun\u00b7gen", "/", "da\u00df", "du", "magst", "vnend\u00b7lich", "f\u00fcr", "mir", "le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}