{"textgrid.poem.62002": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Sinn der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erd' und Himmel rollen in einander,", "tokens": ["Erd'", "und", "Him\u00b7mel", "rol\u00b7len", "in", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "APPR", "PRF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur ein einzig Sternlein blinket noch,", "tokens": ["Nur", "ein", "ein\u00b7zig", "Stern\u00b7lein", "blin\u00b7ket", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein blaues Aug' im dunklen Wetter", "tokens": ["Wie", "ein", "blau\u00b7es", "Aug'", "im", "dunk\u00b7len", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Strahlt es an dem Himmelszelte hoch.", "tokens": ["Strahlt", "es", "an", "dem", "Him\u00b7mels\u00b7zel\u00b7te", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jenes Sternlein birget ferne Welten,", "tokens": ["Je\u00b7nes", "Stern\u00b7lein", "bir\u00b7get", "fer\u00b7ne", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Dein Blick, er tr\u00e4gt Dich sonnenweit:", "tokens": ["Und", "Dein", "Blick", ",", "er", "tr\u00e4gt", "Dich", "son\u00b7nen\u00b7weit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer rief jenen Stern und jenen Sinn der Ferne", "tokens": ["Wer", "rief", "je\u00b7nen", "Stern", "und", "je\u00b7nen", "Sinn", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "KON", "PDAT", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In das Leben unsrer Wirklichkeit?", "tokens": ["In", "das", "Le\u00b7ben", "uns\u00b7rer", "Wirk\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mast und Segel schwimmen auf dem Meere,", "tokens": ["Mast", "und", "Se\u00b7gel", "schwim\u00b7men", "auf", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer schafft dieses Ungewitters Sturm?", "tokens": ["Wer", "schafft", "die\u00b7ses", "Un\u00b7ge\u00b7wit\u00b7ters", "Sturm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und die Schlange in den schwarzen Wolken,", "tokens": ["Und", "die", "Schlan\u00b7ge", "in", "den", "schwar\u00b7zen", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den kleinen roten Totenwurm?", "tokens": ["Und", "den", "klei\u00b7nen", "ro\u00b7ten", "To\u00b7ten\u00b7wurm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Menschheit unter W\u00fcrmern, steh' mir Rede,", "tokens": ["Menschheit", "un\u00b7ter", "W\u00fcr\u00b7mern", ",", "steh'", "mir", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Armes undankbar \u2013 verw\u00f6hntes Kind.", "tokens": ["Ar\u00b7mes", "un\u00b7dank\u00b7bar", "\u2013", "ver\u00b7w\u00f6hn\u00b7tes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4gt der Zufall meilenweit die Blicke,", "tokens": ["Tr\u00e4gt", "der", "Zu\u00b7fall", "mei\u00b7len\u00b7weit", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist's nur Zufall, da\u00df wir sterblich sind?", "tokens": ["Ist's", "nur", "Zu\u00b7fall", ",", "da\u00df", "wir", "sterb\u00b7lich", "sind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Unser Jammer b\u00fcrgt f\u00fcr Ewigkeiten \u2013", "tokens": ["Un\u00b7ser", "Jam\u00b7mer", "b\u00fcrgt", "f\u00fcr", "E\u00b7wig\u00b7kei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das offne, nimmersatte Grab!", "tokens": ["Und", "das", "off\u00b7ne", ",", "nim\u00b7mer\u00b7sat\u00b7te", "Grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch ein Gott erschuf den Sinn der Ferne,", "tokens": ["Doch", "ein", "Gott", "er\u00b7schuf", "den", "Sinn", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und wir sinken drum getrost hinab. \u2013", "tokens": ["Und", "wir", "sin\u00b7ken", "drum", "ge\u00b7trost", "hin\u00b7ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}