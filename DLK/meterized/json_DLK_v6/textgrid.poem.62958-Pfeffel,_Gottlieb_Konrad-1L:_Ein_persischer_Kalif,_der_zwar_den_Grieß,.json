{"textgrid.poem.62958": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein persischer Kalif, der zwar den Grie\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein persischer Kalif, der zwar den Grie\u00df,", "tokens": ["Ein", "per\u00b7si\u00b7scher", "Ka\u00b7lif", ",", "der", "zwar", "den", "Grie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Zipperlein und eine Fistel hatte,", "tokens": ["Das", "Zip\u00b7per\u00b7lein", "und", "ei\u00b7ne", "Fis\u00b7tel", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich aber doch als Gott verehren lie\u00df,", "tokens": ["Sich", "a\u00b7ber", "doch", "als", "Gott", "ver\u00b7eh\u00b7ren", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "KOUS", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verlor sich auf der Jagd. Auf einer gr\u00fcnen Matte,", "tokens": ["Ver\u00b7lor", "sich", "auf", "der", "Jagd", ".", "Auf", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Mat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die tief im Holze lag, sah er mit leisem Graus,", "tokens": ["Die", "tief", "im", "Hol\u00b7ze", "lag", ",", "sah", "er", "mit", "lei\u00b7sem", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wenig F\u00fcrstenaugen sahen,", "tokens": ["Was", "we\u00b7nig", "F\u00fcrs\u00b7ten\u00b7au\u00b7gen", "sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein angef\u00fclltes Beinerhaus.", "tokens": ["Ein", "an\u00b7ge\u00b7f\u00fcll\u00b7tes", "Bei\u00b7ner\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er wagt es muthig hinzunahen", "tokens": ["Er", "wagt", "es", "mut\u00b7hig", "hin\u00b7zu\u00b7na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und findet einen hagern Mann", "tokens": ["Und", "fin\u00b7det", "ei\u00b7nen", "ha\u00b7gern", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit alten Lumpen angethan,", "tokens": ["Mit", "al\u00b7ten", "Lum\u00b7pen", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der in den Todtensch\u00e4deln st\u00f6rte,", "tokens": ["Der", "in", "den", "Tod\u00b7ten\u00b7sch\u00e4\u00b7deln", "st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bald einen, bald den andern griff,", "tokens": ["Bald", "ei\u00b7nen", ",", "bald", "den", "an\u00b7dern", "griff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihn forschend auf und abw\u00e4rts kehrte,", "tokens": ["Ihn", "for\u00b7schend", "auf", "und", "ab\u00b7w\u00e4rts", "kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dann auf den Haufen schmi\u00df. He, Freund, rief der Kalif,", "tokens": ["Dann", "auf", "den", "Hau\u00b7fen", "schmi\u00df", ".", "He", ",", "Freund", ",", "rief", "der", "Ka\u00b7lif", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$.", "ITJ", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was machst du hier? Der Mann schien nicht auf ihn zu h\u00f6ren.", "tokens": ["Was", "machst", "du", "hier", "?", "Der", "Mann", "schien", "nicht", "auf", "ihn", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Sultan reitet n\u00e4her hin,", "tokens": ["Der", "Sul\u00b7tan", "rei\u00b7tet", "n\u00e4\u00b7her", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Allein der Mann lie\u00df sich nicht st\u00f6ren.", "tokens": ["Al\u00b7lein", "der", "Mann", "lie\u00df", "sich", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Verwegner! wei\u00dft du nicht, da\u00df ich der K\u00f6nig bin:", "tokens": ["Ver\u00b7weg\u00b7ner", "!", "wei\u00dft", "du", "nicht", ",", "da\u00df", "ich", "der", "K\u00f6\u00b7nig", "bin", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So br\u00fcllt er und durchbohrt mit Blicken ihn,", "tokens": ["So", "br\u00fcllt", "er", "und", "durch\u00b7bohrt", "mit", "Bli\u00b7cken", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die, wie sein Herz, von wildem Zorn entbrennen,", "tokens": ["Die", ",", "wie", "sein", "Herz", ",", "von", "wil\u00b7dem", "Zorn", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sprich, ehe dir mein Speer das Hundeleben raubt,", "tokens": ["Sprich", ",", "e\u00b7he", "dir", "mein", "Speer", "das", "Hun\u00b7de\u00b7le\u00b7ben", "raubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was suchst du hier? \u00bbHerr, meines Vaters Haupt", "tokens": ["Was", "suchst", "du", "hier", "?", "\u00bb", "Herr", ",", "mei\u00b7nes", "Va\u00b7ters", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und deines Vaters Haupt; ich kann sie nicht erkennen.\u00ab", "tokens": ["Und", "dei\u00b7nes", "Va\u00b7ters", "Haupt", ";", "ich", "kann", "sie", "nicht", "er\u00b7ken\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hier schwang der Schach die mordgewohnte Hand;", "tokens": ["Hier", "schwang", "der", "Schach", "die", "mord\u00b7ge\u00b7wohn\u00b7te", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sie sank \u2013 und das Phantom verschwand.", "tokens": ["Sie", "sank", "\u2013", "und", "das", "Phan\u00b7tom", "ver\u00b7schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}