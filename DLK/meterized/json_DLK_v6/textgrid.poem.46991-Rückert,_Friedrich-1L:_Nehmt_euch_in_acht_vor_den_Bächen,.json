{"textgrid.poem.46991": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nehmt euch in acht vor den B\u00e4chen,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmt euch in acht vor den B\u00e4chen,", "tokens": ["Nehmt", "euch", "in", "acht", "vor", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die da von Tieren sprechen,", "tokens": ["Die", "da", "von", "Tie\u00b7ren", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt und hernach!", "tokens": ["Jetzt", "und", "her\u00b7nach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dort bei Ro\u00dfbach! dort bei Ro\u00dfbach!", "tokens": ["Dort", "bei", "Ro\u00df\u00b7bach", "!", "dort", "bei", "Ro\u00df\u00b7bach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort von eueren Rossen", "tokens": ["Dort", "von", "eu\u00b7e\u00b7ren", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat man euch einst geschossen,", "tokens": ["Hat", "man", "euch", "einst", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist das Blut geflossen", "tokens": ["Ist", "das", "Blut", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "In rechtem Bach.", "tokens": ["In", "rech\u00b7tem", "Bach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nehmt euch in acht vor den B\u00e4chen,", "tokens": ["Nehmt", "euch", "in", "acht", "vor", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die da von Tieren sprechen,", "tokens": ["Die", "da", "von", "Tie\u00b7ren", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt und hernach!", "tokens": ["Jetzt", "und", "her\u00b7nach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "An der Katzbach! An der Katzbach!", "tokens": ["An", "der", "Katz\u00b7bach", "!", "An", "der", "Katz\u00b7bach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da haben wir den Katzen", "tokens": ["Da", "ha\u00b7ben", "wir", "den", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Abgehaun die Tatzen,", "tokens": ["Ab\u00b7ge\u00b7haun", "die", "Tat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df sie nicht mehr kratzen;", "tokens": ["Da\u00df", "sie", "nicht", "mehr", "krat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Kein Hieb ging flach!", "tokens": ["Kein", "Hieb", "ging", "flach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}