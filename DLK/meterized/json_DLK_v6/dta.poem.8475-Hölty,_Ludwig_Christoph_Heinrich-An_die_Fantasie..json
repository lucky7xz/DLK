{"dta.poem.8475": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Fantasie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Aetherblumen um deine Stirn',", "tokens": ["A\u00b7e\u00b7ther\u00b7blu\u00b7men", "um", "dei\u00b7ne", "Stirn'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und erhelle der Nacht Schatten, die schlummerlos", "tokens": ["Und", "er\u00b7hel\u00b7le", "der", "Nacht", "Schat\u00b7ten", ",", "die", "schlum\u00b7mer\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "ADJD"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um mein einsames Lager h\u00e4ngt.", "tokens": ["Um", "mein", "ein\u00b7sa\u00b7mes", "La\u00b7ger", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein unsterblicher Fuss weilet, o K\u00f6nigin,", "tokens": ["Dein", "uns\u00b7terb\u00b7li\u00b7cher", "Fuss", "wei\u00b7let", ",", "o", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "An den Quellen des Morgenroths;", "tokens": ["An", "den", "Quel\u00b7len", "des", "Mor\u00b7gen\u00b7roths", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du entsch\u00f6pfest dem Quell liebliches Rosenlicht,", "tokens": ["Du", "ent\u00b7sch\u00f6p\u00b7fest", "dem", "Quell", "lieb\u00b7li\u00b7ches", "Ro\u00b7sen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und bestralest die Erdenwelt.", "tokens": ["Und", "be\u00b7stra\u00b7lest", "die", "Er\u00b7den\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Eine Grazie h\u00fcpft, leicht wie ein Rosenblatt,", "tokens": ["Ei\u00b7ne", "Gra\u00b7zie", "h\u00fcpft", ",", "leicht", "wie", "ein", "Ro\u00b7sen\u00b7blatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Liebel\u00e4chelnd an deiner Hand,", "tokens": ["Lie\u00b7be\u00b7l\u00e4\u00b7chelnd", "an", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schlingt sich mir um den Arm, wandelt im Abendglanz", "tokens": ["Schlingt", "sich", "mir", "um", "den", "Arm", ",", "wan\u00b7delt", "im", "A\u00b7bend\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-++-+---+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Durch die thauigen Blumen hin;", "tokens": ["Durch", "die", "thau\u00b7i\u00b7gen", "Blu\u00b7men", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Durch den s\u00e4uselnden Hain, durch das Geb\u00fcsch von Gold,", "tokens": ["Durch", "den", "s\u00e4u\u00b7seln\u00b7den", "Hain", ",", "durch", "das", "Ge\u00b7b\u00fcsch", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch das schlummernde Mondenlicht;", "tokens": ["Durch", "das", "schlum\u00b7mern\u00b7de", "Mon\u00b7den\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und aus Rosengew\u00f6lk schimmert der Abendstern", "tokens": ["Und", "aus", "Ro\u00b7sen\u00b7ge\u00b7w\u00f6lk", "schim\u00b7mert", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Meiner Wallerin ins Gesicht.", "tokens": ["Mei\u00b7ner", "Wal\u00b7le\u00b7rin", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Horch, die Nachtigall singt! Seze dich hier am Bach!", "tokens": ["Horch", ",", "die", "Nach\u00b7ti\u00b7gall", "singt", "!", "Se\u00b7ze", "dich", "hier", "am", "Bach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sch\u00fcchtern sinkt sie auf meinen Schooss,", "tokens": ["Sch\u00fcch\u00b7tern", "sinkt", "sie", "auf", "mei\u00b7nen", "Scho\u00b7oss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und ich k\u00fcss' ihr den Schleir von der gehobnen Brust,", "tokens": ["Und", "ich", "k\u00fcss'", "ihr", "den", "Schleir", "von", "der", "ge\u00b7hob\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schweb' in Tr\u00e4umen Elisiums.", "tokens": ["Schweb'", "in", "Tr\u00e4u\u00b7men", "E\u00b7lis\u00b7i\u00b7ums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NE", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}}}}