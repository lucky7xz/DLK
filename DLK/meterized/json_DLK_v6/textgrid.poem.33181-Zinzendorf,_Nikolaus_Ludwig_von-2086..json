{"textgrid.poem.33181": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2086.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesu, lieber Meister! meines herzens Herr! beut auf deine geister f\u00fcr die wanderer.", "tokens": ["Je\u00b7su", ",", "lie\u00b7ber", "Meis\u00b7ter", "!", "mei\u00b7nes", "her\u00b7zens", "Herr", "!", "beut", "auf", "dei\u00b7ne", "geis\u00b7ter", "f\u00fcr", "die", "wan\u00b7de\u00b7rer", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NN", "$.", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+-+--", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Pilger-Erb-Marschalle, Mahanaim, auf! f\u00fchret ein und alle in den zeugen-lauff.", "tokens": ["Pil\u00b7ger\u00b7Er\u00b7bMar\u00b7schal\u00b7le", ",", "Ma\u00b7ha\u00b7naim", ",", "auf", "!", "f\u00fch\u00b7ret", "ein", "und", "al\u00b7le", "in", "den", "zeu\u00b7gen\u00b7lauff", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PTKVZ", "$.", "VVFIN", "PTKVZ", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-+-+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Fliegt mit l\u00f6wen-glauben aus dem tauben-haus, o ihr kirchen-tauben! \u00fcbers Welt-meer naus.", "tokens": ["Fliegt", "mit", "l\u00f6\u00b7wen\u00b7glau\u00b7ben", "aus", "dem", "tau\u00b7ben\u00b7haus", ",", "o", "ihr", "kir\u00b7chen\u00b7tau\u00b7ben", "!", "\u00fc\u00b7bers", "Welt\u00b7meer", "naus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,", "FM", "PPOSAT", "NN", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Und bis ihr zur stelle, seyd wie tauben-flug; geht erschrekt die h\u00f6lle mit dem pilger-zug.", "tokens": ["Und", "bis", "ihr", "zur", "stel\u00b7le", ",", "seyd", "wie", "tau\u00b7ben\u00b7flug", ";", "geht", "er\u00b7schrekt", "die", "h\u00f6l\u00b7le", "mit", "dem", "pil\u00b7ger\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "ADJA", "$,", "VAFIN", "KOKOM", "ADJD", "$.", "VVFIN", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}