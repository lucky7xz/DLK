{"textgrid.poem.37177": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die trinkende Doris", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Doris trinkt, steht Bachus tief entz\u00fcckt,", "tokens": ["Wenn", "Do\u00b7ris", "trinkt", ",", "steht", "Ba\u00b7chus", "tief", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,", "VVFIN", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn zu seinem Ruhm es w\u00e4re.", "tokens": ["Als", "wenn", "zu", "sei\u00b7nem", "Ruhm", "es", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Amor, der indess bescheiden nieder blickt,", "tokens": ["Doch", "A\u00b7mor", ",", "der", "in\u00b7dess", "be\u00b7schei\u00b7den", "nie\u00b7der", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat ganz allein davon die Ehre:", "tokens": ["Hat", "ganz", "al\u00b7lein", "da\u00b7von", "die", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wenn sie trinkt,", "tokens": ["Denn", "wenn", "sie", "trinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Macht sie der Wein so sch\u00f6n, da\u00df der beseelte Zecher", "tokens": ["Macht", "sie", "der", "Wein", "so", "sch\u00f6n", ",", "da\u00df", "der", "be\u00b7seel\u00b7te", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Vielmehr aus seinem Becher", "tokens": ["Viel\u00b7mehr", "aus", "sei\u00b7nem", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als ihren Augen sich betrinkt.", "tokens": ["Als", "ih\u00b7ren", "Au\u00b7gen", "sich", "be\u00b7trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}