{"textgrid.poem.33662": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Alte Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alte Tr\u00e4ume kommen wieder", "tokens": ["Al\u00b7te", "Tr\u00e4u\u00b7me", "kom\u00b7men", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem fernen fremden Land,", "tokens": ["In", "dem", "fer\u00b7nen", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die alten lieben Lieder", "tokens": ["Und", "die", "al\u00b7ten", "lie\u00b7ben", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehm' ich wieder in die Hand.", "tokens": ["Nehm'", "ich", "wie\u00b7der", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O ihr sch\u00f6nen Jugendtage,", "tokens": ["O", "ihr", "sch\u00f6\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundervolle Fr\u00fchlingszeit,", "tokens": ["Wun\u00b7der\u00b7vol\u00b7le", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfe Schmerzen, teure Klage,", "tokens": ["S\u00fc\u00b7\u00dfe", "Schmer\u00b7zen", ",", "teu\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jugend \u2013 o du Herrlichkeit!", "tokens": ["Ju\u00b7gend", "\u2013", "o", "du", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar, was damals uns durchgl\u00fchte,", "tokens": ["Zwar", ",", "was", "da\u00b7mals", "uns", "durch\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward zur Tat aus Wunsch und Traum,", "tokens": ["Ward", "zur", "Tat", "aus", "Wunsch", "und", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber lieblich wie die Bl\u00fcte", "tokens": ["A\u00b7ber", "lieb\u00b7lich", "wie", "die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00e4ucht die reife Frucht uns kaum.", "tokens": ["D\u00e4ucht", "die", "rei\u00b7fe", "Frucht", "uns", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6ner war die tr\u00fcbe Schw\u00fcle,", "tokens": ["Sch\u00f6\u00b7ner", "war", "die", "tr\u00fc\u00b7be", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die helle K\u00fchle jetzt;", "tokens": ["Als", "die", "hel\u00b7le", "K\u00fch\u00b7le", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene fr\u00fchen Vollgef\u00fchle,", "tokens": ["Je\u00b7ne", "fr\u00fc\u00b7hen", "Voll\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kennst du was, das sie ersetzt?", "tokens": ["Kennst", "du", "was", ",", "das", "sie", "er\u00b7setzt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}