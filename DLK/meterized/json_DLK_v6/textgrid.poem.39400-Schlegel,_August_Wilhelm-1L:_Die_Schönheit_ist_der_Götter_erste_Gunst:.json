{"textgrid.poem.39400": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sch\u00f6nheit ist der G\u00f6tter erste Gunst:", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit ist der G\u00f6tter erste Gunst:", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "der", "G\u00f6t\u00b7ter", "ers\u00b7te", "Gunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Bl\u00fcthen der Natur erw\u00e4chst die Kunst.", "tokens": ["Aus", "Bl\u00fc\u00b7then", "der", "Na\u00b7tur", "er\u00b7w\u00e4chst", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Geiste Klarheit, tief Gef\u00fchl im Herzen;", "tokens": ["Im", "Geis\u00b7te", "Klar\u00b7heit", ",", "tief", "Ge\u00b7f\u00fchl", "im", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sitten Adel, wie im Ernst, in Scherzen;", "tokens": ["Der", "Sit\u00b7ten", "A\u00b7del", ",", "wie", "im", "Ernst", ",", "in", "Scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Sprache Wohllaut aus beseeltem Mund:", "tokens": ["Der", "Spra\u00b7che", "Wohl\u00b7laut", "aus", "be\u00b7seel\u00b7tem", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die\u00df macht nur halb Charlottens Wesen kund.", "tokens": ["Die\u00df", "macht", "nur", "halb", "Char\u00b7lot\u00b7tens", "We\u00b7sen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}