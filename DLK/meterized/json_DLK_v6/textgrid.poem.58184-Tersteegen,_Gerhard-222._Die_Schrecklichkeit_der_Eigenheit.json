{"textgrid.poem.58184": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "222. Die Schrecklichkeit der Eigenheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je mehr ich untergeh', je mehr wird Gott erh\u00f6het,", "tokens": ["Je", "mehr", "ich", "un\u00b7ter\u00b7geh'", ",", "je", "mehr", "wird", "Gott", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werd' ich in mir gering, wird er in mir geehrt,", "tokens": ["Werd'", "ich", "in", "mir", "ge\u00b7ring", ",", "wird", "er", "in", "mir", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADJD", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wer sich vergi\u00dft, verliert, in Gott am besten stehet,", "tokens": ["Wer", "sich", "ver\u00b7gi\u00dft", ",", "ver\u00b7liert", ",", "in", "Gott", "am", "bes\u00b7ten", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "$,", "VVFIN", "$,", "APPR", "NN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die mind'ste Eigenheit ist wohl der H\u00f6lle wert.", "tokens": ["Die", "min\u00b7d'\u00b7ste", "Ei\u00b7gen\u00b7heit", "ist", "wohl", "der", "H\u00f6l\u00b7le", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}