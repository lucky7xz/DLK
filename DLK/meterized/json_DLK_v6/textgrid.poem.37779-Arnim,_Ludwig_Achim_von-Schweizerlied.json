{"textgrid.poem.37779": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schweizerlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebet ihr Schweizer hoch,", "tokens": ["Le\u00b7bet", "ihr", "Schwei\u00b7zer", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sterbe die Feindesbrut,", "tokens": ["Ster\u00b7be", "die", "Fein\u00b7des\u00b7brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die uns belog,", "tokens": ["Die", "uns", "be\u00b7log", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Streitet in Himmelshuth,", "tokens": ["Strei\u00b7tet", "in", "Him\u00b7mels\u00b7huth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Trinket der Feinde Blut,", "tokens": ["Trin\u00b7ket", "der", "Fein\u00b7de", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Heute befreit zu sein", "tokens": ["Heu\u00b7te", "be\u00b7freit", "zu", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "PTKZU", "VAINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Schlingt den Verein.", "tokens": ["Schlingt", "den", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Schw\u00fchl ist die Sommerluft,", "tokens": ["Schw\u00fchl", "ist", "die", "Som\u00b7mer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "K\u00fchl ist die Sternennacht,", "tokens": ["K\u00fchl", "ist", "die", "Ster\u00b7nen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Decket die Kluft!", "tokens": ["De\u00b7cket", "die", "Kluft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Euer ist Leibes-Macht,", "tokens": ["Eu\u00b7er", "ist", "Lei\u00b7bes\u00b7Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Br\u00fccken sind leicht gemacht", "tokens": ["Br\u00fc\u00b7cken", "sind", "leicht", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "VVPP"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "\u00dcber sie schreitet hin", "tokens": ["\u00dc\u00b7ber", "sie", "schrei\u00b7tet", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sieg und Gewinn.", "tokens": ["Sieg", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Jubelnd zum wilden Rhein", "tokens": ["Ju\u00b7belnd", "zum", "wil\u00b7den", "Rhein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hallet der K\u00fche-Reihn", "tokens": ["Hal\u00b7let", "der", "K\u00fc\u00b7he\u00b7Reihn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Muthig allein! \u2013", "tokens": ["Mut\u00b7hig", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "ADV", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "W\u00e4r es das weite Meer", "tokens": ["W\u00e4r", "es", "das", "wei\u00b7te", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimmer verweilt das Heer,", "tokens": ["Nim\u00b7mer", "ver\u00b7weilt", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Suchet mit Augenblitz", "tokens": ["Su\u00b7chet", "mit", "Au\u00b7gen\u00b7blitz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Waffenbesitz.", "tokens": ["Waf\u00b7fen\u00b7be\u00b7sitz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt ihr den tiefen Klang", "tokens": ["H\u00f6rt", "ihr", "den", "tie\u00b7fen", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schauert euch Feinden bang?", "tokens": ["Schau\u00b7ert", "euch", "Fein\u00b7den", "bang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Herz aus uns sang!", "tokens": ["Herz", "aus", "uns", "sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Blut in die Wangen drang,", "tokens": ["Blut", "in", "die", "Wan\u00b7gen", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wuth in den Sehnen rang,", "tokens": ["Wuth", "in", "den", "Seh\u00b7nen", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wonne es st\u00fcrmt das Horn", "tokens": ["Won\u00b7ne", "es", "st\u00fcrmt", "das", "Horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Gegen euch Zorn.", "tokens": ["Ge\u00b7gen", "euch", "Zorn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ordnet die schnellen Reihn,", "tokens": ["Ord\u00b7net", "die", "schnel\u00b7len", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schauet im Mondenschein", "tokens": ["Schau\u00b7et", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Geister sich freun:", "tokens": ["Geis\u00b7ter", "sich", "freun", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Weihet den Sch\u00fctzen ein,", "tokens": ["Wei\u00b7het", "den", "Sch\u00fct\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Decket mit Kranzes Schein,", "tokens": ["De\u00b7cket", "mit", "Kran\u00b7zes", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Einen der Feind er traf", "tokens": ["Ei\u00b7nen", "der", "Feind", "er", "traf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sinkend zum Schlaf.", "tokens": ["Sin\u00b7kend", "zum", "Schlaf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Dr\u00e4nget die L\u00fccke voll,", "tokens": ["Dr\u00e4n\u00b7get", "die", "L\u00fc\u00b7cke", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Jubelnd mein Tod erscholl", "tokens": ["Ju\u00b7belnd", "mein", "Tod", "er\u00b7scholl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kugel nun roll;", "tokens": ["Ku\u00b7gel", "nun", "roll", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Grase am Boden toll,", "tokens": ["Gra\u00b7se", "am", "Bo\u00b7den", "toll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Tod ist des Lebens Zoll", "tokens": ["Tod", "ist", "des", "Le\u00b7bens", "Zoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Zahlet des Vaterland's", "tokens": ["Zah\u00b7let", "des", "Va\u00b7ter\u00b7lan\u00b7d's"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "K\u00fchlenden Sand.", "tokens": ["K\u00fch\u00b7len\u00b7den", "Sand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Dringt mit dem Morgenstern", "tokens": ["Dringt", "mit", "dem", "Mor\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "K\u00fchn in die Spie\u00dfe ein.", "tokens": ["K\u00fchn", "in", "die", "Spie\u00b7\u00dfe", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut nur den Stern.", "tokens": ["Schaut", "nur", "den", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schlucket die Spie\u00dfe ein,", "tokens": ["Schluc\u00b7ket", "die", "Spie\u00b7\u00dfe", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Krankheit ist b\u00f6se Pein,", "tokens": ["Krank\u00b7heit", "ist", "b\u00f6\u00b7se", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wer von den Spie\u00dfen sinkt", "tokens": ["Wer", "von", "den", "Spie\u00b7\u00dfen", "sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Himmelthau trinkt.", "tokens": ["Him\u00b7melt\u00b7hau", "trinkt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Schauet beim Morgenschein", "tokens": ["Schau\u00b7et", "beim", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ruhend im D\u00e4mmerhayn", "tokens": ["Ru\u00b7hend", "im", "D\u00e4m\u00b7mer\u00b7hayn"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Feindes Gebein:", "tokens": ["Fein\u00b7des", "Ge\u00b7bein", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "H\u00f6ret den Siegsgesang", "tokens": ["H\u00f6\u00b7ret", "den", "Siegs\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der von den Br\u00fcdern klang! \u2013", "tokens": ["Der", "von", "den", "Br\u00fc\u00b7dern", "klang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Feinde nur klagen bang,", "tokens": ["Fein\u00b7de", "nur", "kla\u00b7gen", "bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wonnegesang.", "tokens": ["Won\u00b7ne\u00b7ge\u00b7sang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}