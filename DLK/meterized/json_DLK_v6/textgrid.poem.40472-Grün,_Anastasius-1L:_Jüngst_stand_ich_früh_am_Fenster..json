{"textgrid.poem.40472": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst stand ich fr\u00fch am Fenster.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst stand ich fr\u00fch am Fenster.", "tokens": ["J\u00fcngst", "stand", "ich", "fr\u00fch", "am", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor\u00fcber trugen schwarze M\u00e4nner ernst", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "tru\u00b7gen", "schwar\u00b7ze", "M\u00e4n\u00b7ner", "ernst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Morgenzwielicht einen off'nen Sarg.", "tokens": ["Im", "Mor\u00b7gen\u00b7zwie\u00b7licht", "ei\u00b7nen", "off'\u00b7nen", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da flammt' empor das Fr\u00fchroth.", "tokens": ["Da", "flammt'", "em\u00b7por", "das", "Fr\u00fch\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Leiche Antlitz glomm nun rosigroth,", "tokens": ["Der", "Lei\u00b7che", "Ant\u00b7litz", "glomm", "nun", "ro\u00b7si\u00b7groth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sei nach kurzer Wand'rung r\u00fcckgekehrt", "tokens": ["Als", "sei", "nach", "kur\u00b7zer", "Wan\u00b7d'\u00b7rung", "r\u00fcck\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das Leben ins vorschnell verlass'ne Haus.", "tokens": ["Das", "Le\u00b7ben", "ins", "vor\u00b7schnell", "ver\u00b7lass'\u00b7ne", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kalt strich des Fr\u00fchroths Odem.", "tokens": ["Kalt", "strich", "des", "Fr\u00fch\u00b7roths", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da h\u00fcllten sich, vor K\u00e4lte leichenbla\u00df,", "tokens": ["Da", "h\u00fcll\u00b7ten", "sich", ",", "vor", "K\u00e4l\u00b7te", "lei\u00b7chen\u00b7bla\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00e4nner in die schwarzen M\u00e4ntel tief,", "tokens": ["Die", "M\u00e4n\u00b7ner", "in", "die", "schwar\u00b7zen", "M\u00e4n\u00b7tel", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als wickle sie der Tod ins Leichentuch.", "tokens": ["Als", "wick\u00b7le", "sie", "der", "Tod", "ins", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O wundervolles Fr\u00fchroth!", "tokens": ["O", "wun\u00b7der\u00b7vol\u00b7les", "Fr\u00fch\u00b7roth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Tode hauchst du Gluth ins welke Antlitz,", "tokens": ["Dem", "To\u00b7de", "hauchst", "du", "Gluth", "ins", "wel\u00b7ke", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Leben hauchst du Eis in gl\u00fch'nde Pulse!", "tokens": ["Dem", "Le\u00b7ben", "hauchst", "du", "Eis", "in", "gl\u00fch'n\u00b7de", "Pul\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wundervolle Liebe!", "tokens": ["O", "wun\u00b7der\u00b7vol\u00b7le", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du hauchest Eis ins wunde Herz des Lebens,", "tokens": ["Du", "hau\u00b7chest", "Eis", "ins", "wun\u00b7de", "Herz", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df es vor Frost zu Tode m\u00f6cht' erstarren!", "tokens": ["Da\u00df", "es", "vor", "Frost", "zu", "To\u00b7de", "m\u00f6cht'", "er\u00b7star\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein sch\u00f6nstes Diadem schm\u00fcckt oft erst Leichen,", "tokens": ["Dein", "sch\u00f6ns\u00b7tes", "Di\u00b7a\u00b7dem", "schm\u00fcckt", "oft", "erst", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dein w\u00e4rmster Ku\u00df schwelgt auf des Todes Lippen!", "tokens": ["Dein", "w\u00e4rms\u00b7ter", "Ku\u00df", "schwelgt", "auf", "des", "To\u00b7des", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}