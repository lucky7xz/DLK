{"textgrid.poem.52878": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Droben ist Tee, droben ist Ball,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben ist Tee, droben ist Ball,", "tokens": ["Dro\u00b7ben", "ist", "Tee", ",", "dro\u00b7ben", "ist", "Ball", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gesellschaft, Spiel und Tanz.", "tokens": ["Ge\u00b7sell\u00b7schaft", ",", "Spiel", "und", "Tanz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei, \u00fcber die schmucken M\u00e4nnlein all',", "tokens": ["Ei", ",", "\u00fc\u00b7ber", "die", "schmu\u00b7cken", "M\u00e4nn\u00b7lein", "all'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00dcber den Lichterglanz!", "tokens": ["\u00dc\u00b7ber", "den", "Lich\u00b7ter\u00b7glanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Hier unten, wo die Kutschen stehn,", "tokens": ["Hier", "un\u00b7ten", ",", "wo", "die", "Kut\u00b7schen", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Harr' ich auch einen Augenblick;", "tokens": ["Harr'", "ich", "auch", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will nach den hellen Fenstern sehn", "tokens": ["Will", "nach", "den", "hel\u00b7len", "Fens\u00b7tern", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lauschen auf die Musik.", "tokens": ["Und", "lau\u00b7schen", "auf", "die", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Nur dann und wann ein grober Klang", "tokens": ["Nur", "dann", "und", "wann", "ein", "gro\u00b7ber", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Brummba\u00df trifft mein Ohr,", "tokens": ["Vom", "Brumm\u00b7ba\u00df", "trifft", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An den Gardinen ellenlang", "tokens": ["An", "den", "Gar\u00b7di\u00b7nen", "el\u00b7len\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tauchen Schatten empor.", "tokens": ["Tau\u00b7chen", "Schat\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Drehen sich, b\u00fccken sich, schneuzen sich,", "tokens": ["Dre\u00b7hen", "sich", ",", "b\u00fc\u00b7cken", "sich", ",", "schneu\u00b7zen", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Flistern und trippeln, Paar f\u00fcr Paar,", "tokens": ["Flis\u00b7tern", "und", "trip\u00b7peln", ",", "Paar", "f\u00fcr", "Paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nippen am Gl\u00e4schen j\u00fcngferlich,", "tokens": ["Nip\u00b7pen", "am", "Gl\u00e4s\u00b7chen", "j\u00fcng\u00b7fer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "G\u00e4hnen und w\u00fchlen sich wild im Haar.", "tokens": ["G\u00e4h\u00b7nen", "und", "w\u00fch\u00b7len", "sich", "wild", "im", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Das ist mir auch ein rares Pl\u00e4sier,", "tokens": ["Das", "ist", "mir", "auch", "ein", "ra\u00b7res", "Pl\u00e4\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz nach meinem Geschmack;", "tokens": ["Ganz", "nach", "mei\u00b7nem", "Ge\u00b7schmack", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nein, da lob' ich mein Solo mir,", "tokens": ["Nein", ",", "da", "lob'", "ich", "mein", "So\u00b7lo", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mein Bier und meinen Tabak.", "tokens": ["Mein", "Bier", "und", "mei\u00b7nen", "Ta\u00b7bak", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Tr\u00e4t' ich in diesem Rockelor", "tokens": ["Tr\u00e4t'", "ich", "in", "die\u00b7sem", "Ro\u00b7cke\u00b7lor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So pl\u00f6tzlich in den Saal hinein,", "tokens": ["So", "pl\u00f6tz\u00b7lich", "in", "den", "Saal", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der Hand Laterne, Spie\u00df und Rohr,", "tokens": ["In", "der", "Hand", "La\u00b7ter\u00b7ne", ",", "Spie\u00df", "und", "Rohr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Unter die Schatten mitten drein,", "tokens": ["Un\u00b7ter", "die", "Schat\u00b7ten", "mit\u00b7ten", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Wei\u00dfe Flocken auf meinem Hut,", "tokens": ["Wei\u00b7\u00dfe", "Flo\u00b7cken", "auf", "mei\u00b7nem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Den Bart voll Reif und Frost,", "tokens": ["Den", "Bart", "voll", "Reif", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die braune Wange in frischer Glut,", "tokens": ["Die", "brau\u00b7ne", "Wan\u00b7ge", "in", "fri\u00b7scher", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Glieder steif vom Ost:", "tokens": ["Die", "Glie\u00b7der", "steif", "vom", "Ost", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie hielten es f\u00fcr 'nen Mummenschanz,", "tokens": ["Sie", "hiel\u00b7ten", "es", "f\u00fcr", "'nen", "Mum\u00b7men\u00b7schanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich f\u00fcr ein Gespenst der Nacht,", "tokens": ["Mich", "f\u00fcr", "ein", "Ge\u00b7spenst", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wette, der jungen Fante Tanz", "tokens": ["Und", "ich", "wet\u00b7te", ",", "der", "jun\u00b7gen", "Fan\u00b7te", "Tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zerst\u00f6be, f\u00fcrsichtig-sacht.", "tokens": ["Zer\u00b7st\u00f6\u00b7be", ",", "f\u00fcr\u00b7sich\u00b7tig\u00b7sacht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Es ist in der Welt nach meinem Sinn", "tokens": ["Es", "ist", "in", "der", "Welt", "nach", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein n\u00e4rrischer Schabernack ...", "tokens": ["Ein", "n\u00e4r\u00b7ri\u00b7scher", "Scha\u00b7ber\u00b7nack", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob ich gerad' so ein Mensch wohl bin,", "tokens": ["Ob", "ich", "ge\u00b7rad'", "so", "ein", "Mensch", "wohl", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie das feine, vornehme Pack?", "tokens": ["Wie", "das", "fei\u00b7ne", ",", "vor\u00b7neh\u00b7me", "Pack", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}