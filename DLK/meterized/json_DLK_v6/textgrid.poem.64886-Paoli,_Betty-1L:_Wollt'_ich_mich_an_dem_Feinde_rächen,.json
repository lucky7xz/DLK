{"textgrid.poem.64886": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wollt' ich mich an dem Feinde r\u00e4chen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt' ich mich an dem Feinde r\u00e4chen,", "tokens": ["Wollt'", "ich", "mich", "an", "dem", "Fein\u00b7de", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der t\u00fcckisch durch mein Leben schleicht,", "tokens": ["Der", "t\u00fc\u00b7ckisch", "durch", "mein", "Le\u00b7ben", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00fc\u00dft ich die Kronenzier zerbrechen,", "tokens": ["M\u00fc\u00dft", "ich", "die", "Kro\u00b7nen\u00b7zier", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die er mir widerstrebend reicht.", "tokens": ["Die", "er", "mir", "wi\u00b7der\u00b7stre\u00b7bend", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn, wenn er nicht genau bed\u00e4chte,", "tokens": ["Denn", ",", "wenn", "er", "nicht", "ge\u00b7nau", "be\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n mein Herz, wie gro\u00df und gut:", "tokens": ["Wie", "sch\u00f6n", "mein", "Herz", ",", "wie", "gro\u00df", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht wagte es der Feige, Schlechte,", "tokens": ["Nicht", "wag\u00b7te", "es", "der", "Fei\u00b7ge", ",", "Schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich zu verletzen, wie er's thut.", "tokens": ["Mich", "zu", "ver\u00b7let\u00b7zen", ",", "wie", "er's", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er wei\u00df, ich k\u00f6nnte mich vertheid'gen,", "tokens": ["Er", "wei\u00df", ",", "ich", "k\u00f6nn\u00b7te", "mich", "vert\u00b7hei\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vernichten ihn mit einem Laut,", "tokens": ["Ver\u00b7nich\u00b7ten", "ihn", "mit", "ei\u00b7nem", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wagt es doch, mich zu beleid'gen,", "tokens": ["Und", "wagt", "es", "doch", ",", "mich", "zu", "be\u00b7lei\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Weil er auf meine Gro\u00dfmuth baut.", "tokens": ["Weil", "er", "auf", "mei\u00b7ne", "Gro\u00df\u00b7muth", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wollt' ich in rascher Seelenwendni\u00df", "tokens": ["Wollt'", "ich", "in", "ra\u00b7scher", "See\u00b7len\u00b7wend\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Rache nehmen an dem Feind,", "tokens": ["Nun", "Ra\u00b7che", "neh\u00b7men", "an", "dem", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was w\u00e4re sie, als ein Gest\u00e4ndni\u00df:", "tokens": ["Was", "w\u00e4\u00b7re", "sie", ",", "als", "ein", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbich bin so gut nicht, wie er meint!\u00ab", "tokens": ["\u00bb", "ich", "bin", "so", "gut", "nicht", ",", "wie", "er", "meint", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Kiesel niedern Zorns zu bringen", "tokens": ["Den", "Kie\u00b7sel", "nie\u00b7dern", "Zorns", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Herzens Demantschrein,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "De\u00b7mant\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll keinem Feinde je gelingen!", "tokens": ["Soll", "kei\u00b7nem", "Fein\u00b7de", "je", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will so schlecht, wie er, nicht sein.", "tokens": ["Ich", "will", "so", "schlecht", ",", "wie", "er", ",", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}