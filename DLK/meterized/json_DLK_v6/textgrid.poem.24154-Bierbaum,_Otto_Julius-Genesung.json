{"textgrid.poem.24154": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Genesung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange lag ich krank im Haus", "tokens": ["Lan\u00b7ge", "lag", "ich", "krank", "im", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter dicken Decken,", "tokens": ["Un\u00b7ter", "di\u00b7cken", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heut zum ersten Mal hinaus", "tokens": ["Heut", "zum", "ers\u00b7ten", "Mal", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf den Kopf ich stecken.", "tokens": ["Darf", "den", "Kopf", "ich", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Vor dem Fenster Wipfelgr\u00fcn,", "tokens": ["Vor", "dem", "Fens\u00b7ter", "Wip\u00b7fel\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, wie ist das helle,", "tokens": ["Ach", ",", "wie", "ist", "das", "hel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und es treibt mich fr\u00fchlingsk\u00fchn", "tokens": ["Und", "es", "treibt", "mich", "fr\u00fch\u00b7lings\u00b7k\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zu Thor und Schwelle.", "tokens": ["Bis", "zu", "Thor", "und", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Fliegt mein Blick sehns\u00fcchtig weit", "tokens": ["Fliegt", "mein", "Blick", "sehn\u00b7s\u00fcch\u00b7tig", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Bl\u00fchewonnen,", "tokens": ["Ue\u00b7ber", "Bl\u00fc\u00b7he\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist Gedenken zager Zeit", "tokens": ["Ist", "Ge\u00b7den\u00b7ken", "za\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Dunst zerronnen.", "tokens": ["Wie", "ein", "Dunst", "zer\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In mein Auge schwillt ein Schein", "tokens": ["In", "mein", "Au\u00b7ge", "schwillt", "ein", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelheller Reine:", "tokens": ["Him\u00b7mel\u00b7hel\u00b7ler", "Rei\u00b7ne", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leben! Leben! Bist du mein?", "tokens": ["Le\u00b7ben", "!", "Le\u00b7ben", "!", "Bist", "du", "mein", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich weine, weine ...", "tokens": ["Und", "ich", "wei\u00b7ne", ",", "wei\u00b7ne", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}