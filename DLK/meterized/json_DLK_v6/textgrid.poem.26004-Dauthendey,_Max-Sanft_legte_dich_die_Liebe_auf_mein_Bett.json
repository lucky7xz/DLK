{"textgrid.poem.26004": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sanft legte dich die Liebe auf mein Bett", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft legte dich die Liebe auf mein Bett", "tokens": ["Sanft", "leg\u00b7te", "dich", "die", "Lie\u00b7be", "auf", "mein", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deinem sch\u00f6nsten Kleid aus Scham und Bl\u00f6\u00dfe,", "tokens": ["In", "dei\u00b7nem", "sch\u00f6ns\u00b7ten", "Kleid", "aus", "Scham", "und", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drau\u00dfen kam die Nacht auf atemlosen Schnee,", "tokens": ["Und", "drau\u00b7\u00dfen", "kam", "die", "Nacht", "auf", "a\u00b7tem\u00b7lo\u00b7sen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auch Gottvater kam in atemloser Gr\u00f6\u00dfe.", "tokens": ["Und", "auch", "Gott\u00b7va\u00b7ter", "kam", "in", "a\u00b7tem\u00b7lo\u00b7ser", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit vollem Auge hat der Gott geweint, gelacht.", "tokens": ["Mit", "vol\u00b7lem", "Au\u00b7ge", "hat", "der", "Gott", "ge\u00b7weint", ",", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du hast dein Herz und deinen Leib", "tokens": ["Du", "hast", "dein", "Herz", "und", "dei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Krone dieser Nacht gemacht.", "tokens": ["Zur", "Kro\u00b7ne", "die\u00b7ser", "Nacht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}