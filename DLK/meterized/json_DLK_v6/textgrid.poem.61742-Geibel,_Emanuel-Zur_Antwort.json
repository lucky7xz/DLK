{"textgrid.poem.61742": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Zur Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn von au\u00dfen der Feind uns droht,", "tokens": ["Wenn", "von", "au\u00b7\u00dfen", "der", "Feind", "uns", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wohl mit klingenden Saiten", "tokens": ["Wohl", "mit", "klin\u00b7gen\u00b7den", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Im gewappneten Aufgebot", "tokens": ["Im", "ge\u00b7wapp\u00b7ne\u00b7ten", "Auf\u00b7ge\u00b7bot"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ziemt's dem Dichter zu schreiten.", "tokens": ["Ziemt's", "dem", "Dich\u00b7ter", "zu", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Eisern wie ein geschwungenes Schwert", "tokens": ["Ei\u00b7sern", "wie", "ein", "ge\u00b7schwun\u00b7ge\u00b7nes", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Soll sein Hymnus ert\u00f6nen,", "tokens": ["Soll", "sein", "Hym\u00b7nus", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bis ihm gn\u00e4dig ein Gott beschert,", "tokens": ["Bis", "ihm", "gn\u00e4\u00b7dig", "ein", "Gott", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Siegerstirnen zu kr\u00f6nen.", "tokens": ["Sie\u00b7ger\u00b7stir\u00b7nen", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber wo mit Gewalt und List", "tokens": ["A\u00b7ber", "wo", "mit", "Ge\u00b7walt", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Haupt feindselig und Glieder", "tokens": ["Haupt", "feind\u00b7se\u00b7lig", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sich befehden im innern Zwist,", "tokens": ["Sich", "be\u00b7feh\u00b7den", "im", "in\u00b7nern", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Da verstummen die Lieder.", "tokens": ["Da", "ver\u00b7stum\u00b7men", "die", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Eh' sie diente, der Volkspartein", "tokens": ["Eh'", "sie", "dien\u00b7te", ",", "der", "Volks\u00b7par\u00b7tein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Zwietracht weiterzutragen,", "tokens": ["Zwiet\u00b7racht", "wei\u00b7ter\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Lieber wollt' ich am n\u00e4chsten Stein", "tokens": ["Lie\u00b7ber", "wollt'", "ich", "am", "n\u00e4chs\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Diese Harfe zerschlagen.", "tokens": ["Die\u00b7se", "Har\u00b7fe", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}