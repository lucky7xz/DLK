{"dta.poem.20704": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Wassersnoth bei Frankfurth an der Oder  \n  im April 1785 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Gebirge str\u00f6mte das Verderben                 ", "tokens": ["Vom", "Ge\u00b7bir\u00b7ge", "str\u00f6m\u00b7te", "das", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ins Gefilde weit und breit,", "tokens": ["Ins", "Ge\u00b7fil\u00b7de", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Saat und Blumenkeime wollten sterben", "tokens": ["Saat", "und", "Blu\u00b7men\u00b7kei\u00b7me", "woll\u00b7ten", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unterm Wasserwogenstreit,", "tokens": ["Un\u00b7term", "Was\u00b7ser\u00b7wo\u00b7gen\u00b7streit", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zarte L\u00e4mmer, junge Busenkinder", "tokens": ["Zar\u00b7te", "L\u00e4m\u00b7mer", ",", "jun\u00b7ge", "Bu\u00b7sen\u00b7kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Heischten Rettung aus der Fluth \u2014", "tokens": ["Heischten", "Ret\u00b7tung", "aus", "der", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hungerleiden br\u00fcllten magre Rinder,", "tokens": ["Hun\u00b7ger\u00b7lei\u00b7den", "br\u00fcll\u00b7ten", "mag\u00b7re", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die des Landmanns einzig Gut", "tokens": ["Die", "des", "Land\u00b7manns", "ein\u00b7zig", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In der niedern Armuthsh\u00fctte waren.", "tokens": ["In", "der", "nie\u00b7dern", "Ar\u00b7muths\u00b7h\u00fct\u00b7te", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Gr\u00f6\u00dfer schien die Wassersnoth", "tokens": ["Gr\u00f6\u00b7\u00dfer", "schien", "die", "Was\u00b7sers\u00b7noth"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Als ein Feldzug fremder Kriegesschaaren,", "tokens": ["Als", "ein", "Feld\u00b7zug", "frem\u00b7der", "Krie\u00b7ges\u00b7schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Der mit Schwerd und Feuer droht", "tokens": ["Der", "mit", "Schwerd", "und", "Feu\u00b7er", "droht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und mit Pl\u00fcnderung dem platten Lande,", "tokens": ["Und", "mit", "Pl\u00fcn\u00b7de\u00b7rung", "dem", "plat\u00b7ten", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Das sein Rauschen h\u00f6rt und zagt,", "tokens": ["Das", "sein", "Rau\u00b7schen", "h\u00f6rt", "und", "zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn der Zug vom \u00e4u\u00dfern Gr\u00e4nzenrande", "tokens": ["Wenn", "der", "Zug", "vom", "\u00e4u\u00b7\u00dfern", "Gr\u00e4n\u00b7zen\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Schrecken vor sich hergejagt \u2014", "tokens": ["Schre\u00b7cken", "vor", "sich", "her\u00b7ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Jenem Waffenrasseln widerstehet", "tokens": ["Je\u00b7nem", "Waf\u00b7fen\u00b7ras\u00b7seln", "wi\u00b7der\u00b7ste\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Heldenklugheit, Heldenmuth;", "tokens": ["Hel\u00b7den\u00b7klug\u00b7heit", ",", "Hel\u00b7den\u00b7muth", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Aber wenn sich f\u00fcrchterlich erh\u00f6het", "tokens": ["A\u00b7ber", "wenn", "sich", "f\u00fcrch\u00b7ter\u00b7lich", "er\u00b7h\u00f6\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Ausgetretner Str\u00f6me Wuth,", "tokens": ["Aus\u00b7ge\u00b7tret\u00b7ner", "Str\u00f6\u00b7me", "Wuth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Kann der K\u00f6nig selber nicht gebieten,", "tokens": ["Kann", "der", "K\u00f6\u00b7nig", "sel\u00b7ber", "nicht", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Der mit siegesreicher Hand", "tokens": ["Der", "mit", "sie\u00b7ges\u00b7rei\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Sieben Jahre lang dem Waffenw\u00fcten", "tokens": ["Sie\u00b7ben", "Jah\u00b7re", "lang", "dem", "Waf\u00b7fen\u00b7w\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Vieler Feinde widerstand.", "tokens": ["Vie\u00b7ler", "Fein\u00b7de", "wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Rettung nur war m\u00f6glich, war zu wagen,", "tokens": ["Ret\u00b7tung", "nur", "war", "m\u00f6g\u00b7lich", ",", "war", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADJD", "$,", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Und wenn sie gelang, alsdann", "tokens": ["Und", "wenn", "sie", "ge\u00b7lang", ",", "als\u00b7dann"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "War kein Dichter stark genug, zu sagen", "tokens": ["War", "kein", "Dich\u00b7ter", "stark", "ge\u00b7nug", ",", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "ADV", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Wonne, die der Held gewann.", "tokens": ["Won\u00b7ne", ",", "die", "der", "Held", "ge\u00b7wann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leopold, ein junger, Menschenlieber", "tokens": ["Leo\u00b7pold", ",", "ein", "jun\u00b7ger", ",", "Men\u00b7schen\u00b7lie\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Guelfensohn, hat es gewagt \u2014", "tokens": ["Guel\u00b7fen\u00b7sohn", ",", "hat", "es", "ge\u00b7wagt"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Menschlich Mitleid ri\u00df ihn m\u00e4chtig \u00fcber", "tokens": ["Menschlich", "Mit\u00b7leid", "ri\u00df", "ihn", "m\u00e4ch\u00b7tig", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alle Warnung laut gesagt.", "tokens": ["Al\u00b7le", "War\u00b7nung", "laut", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber alle Todesfurcht erhaben,", "tokens": ["Ue\u00b7ber", "al\u00b7le", "To\u00b7des\u00b7furcht", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sprang er in den Kahn, und sprach:", "tokens": ["Sprang", "er", "in", "den", "Kahn", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201erudert r\u00fcstig fort, ihr Schifferknaben,", "tokens": ["\u201e", "ru\u00b7dert", "r\u00fcs\u00b7tig", "fort", ",", "ihr", "Schif\u00b7fer\u00b7kna\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u201efolgt der Jammerstimme nach,", "tokens": ["\u201e", "folgt", "der", "Jam\u00b7mers\u00b7tim\u00b7me", "nach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u201edie so kl\u00e4glich H\u00fclfe fodert dr\u00fcben,", "tokens": ["\u201e", "die", "so", "kl\u00e4g\u00b7lich", "H\u00fcl\u00b7fe", "fo\u00b7dert", "dr\u00fc\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ADJD", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "\u201eh\u00f6rt die Todesangst und eilt!", "tokens": ["\u201e", "h\u00f6rt", "die", "To\u00b7de\u00b7sangst", "und", "eilt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u201eschon zu lange seyd ihr kalt geblieben,", "tokens": ["\u201e", "schon", "zu", "lan\u00b7ge", "seyd", "ihr", "kalt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKA", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u201ehabt zu lange schon geweilt,", "tokens": ["\u201e", "habt", "zu", "lan\u00b7ge", "schon", "ge\u00b7weilt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "\u201ehabt nur hier die Wellen angegaffet,", "tokens": ["\u201e", "habt", "nur", "hier", "die", "Wel\u00b7len", "an\u00b7ge\u00b7gaf\u00b7fet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "\u201edie der Br\u00fccke Halbtheil schon", "tokens": ["\u201e", "die", "der", "Br\u00fc\u00b7cke", "Halb\u00b7theil", "schon"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "\u201eangegriffen und hinweggeraffet \u2014", "tokens": ["\u201e", "an\u00b7ge\u00b7grif\u00b7fen", "und", "hin\u00b7weg\u00b7ge\u00b7raf\u00b7fet"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "KON", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "\u201ef\u00fcrchtet nicht dies Wasserd", "tokens": ["\u201e", "f\u00fcrch\u00b7tet", "nicht", "dies", "Was\u00b7serd"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKNEG", "PDS", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "\u201eich bin Mensch, wie ihr zur Welt gekommen,", "tokens": ["\u201e", "ich", "bin", "Mensch", ",", "wie", "ihr", "zur", "Welt", "ge\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "\u201ewagt doch, was ich wagen kann,", "tokens": ["\u201e", "wagt", "doch", ",", "was", "ich", "wa\u00b7gen", "kann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "\u201eseht, da wo die H\u00e4user weggeschwommen,", "tokens": ["\u201e", "seht", ",", "da", "wo", "die", "H\u00e4u\u00b7ser", "weg\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u201ekommts auf Menschenrettung an \u2014", "tokens": ["\u201e", "kommts", "auf", "Men\u00b7schen\u00b7ret\u00b7tung", "an"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also sprach der F\u00fcrstensohn und brannte", "tokens": ["Al\u00b7so", "sprach", "der", "F\u00fcrs\u00b7ten\u00b7sohn", "und", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von Begierde, da zu seyn,", "tokens": ["Von", "Be\u00b7gier\u00b7de", ",", "da", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich zu dem Sturmgebieter wandte", "tokens": ["Wo", "sich", "zu", "dem", "Sturm\u00b7ge\u00b7bie\u00b7ter", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nothgedr\u00e4ngter Menschen Schreyn.", "tokens": ["Noth\u00b7ge\u00b7dr\u00e4ng\u00b7ter", "Men\u00b7schen", "Schreyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald hin\u00fcber war die Fahrt gelungen,", "tokens": ["Bald", "hin\u00b7\u00fc\u00b7ber", "war", "die", "Fahrt", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als ein Windsto\u00df sie ergriff,", "tokens": ["Als", "ein", "Wind\u00b7sto\u00df", "sie", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, von einer Welle Wuth gedrungen,", "tokens": ["Ach", ",", "von", "ei\u00b7ner", "Wel\u00b7le", "Wuth", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["