{"textgrid.poem.25943": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Windenbl\u00fcten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgens stehn der Windenbl\u00fcten", "tokens": ["Mor\u00b7gens", "stehn", "der", "Win\u00b7den\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feine T\u00fcten an dem Rain,", "tokens": ["Fei\u00b7ne", "T\u00fc\u00b7ten", "an", "dem", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wie Augen voll von Frische", "tokens": ["Sind", "wie", "Au\u00b7gen", "voll", "von", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "NN", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Am grasgr\u00fcnen Lebenstische.", "tokens": ["Am", "gras\u00b7gr\u00fc\u00b7nen", "Le\u00b7bens\u00b7ti\u00b7sche", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Abends liegen sie daneben,", "tokens": ["A\u00b7bends", "lie\u00b7gen", "sie", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar nichts kann sie mehr beleben.", "tokens": ["Gar", "nichts", "kann", "sie", "mehr", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wie Zecher, die genossen,", "tokens": ["Sind", "wie", "Ze\u00b7cher", ",", "die", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Becher umgesto\u00dfen.", "tokens": ["Ih\u00b7re", "Be\u00b7cher", "um\u00b7ge\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner kann mehr n\u00fcchtern stehn,", "tokens": ["Kei\u00b7ner", "kann", "mehr", "n\u00fcch\u00b7tern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer der Lieb' ins Glas gesehn.", "tokens": ["Wer", "der", "Lieb'", "ins", "Glas", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}