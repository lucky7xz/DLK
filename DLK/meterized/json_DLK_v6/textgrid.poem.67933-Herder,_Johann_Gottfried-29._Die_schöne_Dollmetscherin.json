{"textgrid.poem.67933": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "29. Die sch\u00f6ne Dollmetscherin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Gravo fiel der Bascha Mustaj,", "tokens": ["Ue\u00b7ber", "Gra\u00b7vo", "fiel", "der", "Ba\u00b7scha", "Mu\u00b7staj", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NE", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und ringsum die hohe Mauer sanken", "tokens": ["Und", "ring\u00b7sum", "die", "ho\u00b7he", "Mau\u00b7er", "san\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel von seinen Edeln. Als die T\u00fcrken", "tokens": ["Viel", "von", "sei\u00b7nen", "E\u00b7deln", ".", "Als", "die", "T\u00fcr\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Abends nun im Hause des Nikolo,", "tokens": ["A\u00b7bends", "nun", "im", "Hau\u00b7se", "des", "Ni\u00b7ko\u00b7lo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Des Gebieters \u00fcber Gravo assen,", "tokens": ["Des", "Ge\u00b7bie\u00b7ters", "\u00fc\u00b7ber", "Gra\u00b7vo", "as\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Baten sie um frisches Wasser. Niemand", "tokens": ["Ba\u00b7ten", "sie", "um", "fri\u00b7sches", "Was\u00b7ser", ".", "Nie\u00b7mand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "War der Sprache kundig, als die sch\u00f6ne", "tokens": ["War", "der", "Spra\u00b7che", "kun\u00b7dig", ",", "als", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tochter des Nikolo, und zur Mutter", "tokens": ["Toch\u00b7ter", "des", "Ni\u00b7ko\u00b7lo", ",", "und", "zur", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NE", "$,", "KON", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Rief sie: liebe Mutter, auf die F\u00fcsse!", "tokens": ["Rief", "sie", ":", "lie\u00b7be", "Mut\u00b7ter", ",", "auf", "die", "F\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Frisches Wasser fodern diese T\u00fcrken.", "tokens": ["Fri\u00b7sches", "Was\u00b7ser", "fo\u00b7dern", "die\u00b7se", "T\u00fcr\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Stand die Mutter auf und brachte Wasser.", "tokens": ["Stand", "die", "Mut\u00b7ter", "auf", "und", "brach\u00b7te", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle tranken, doch der J\u00fcngling Muza", "tokens": ["Al\u00b7le", "tran\u00b7ken", ",", "doch", "der", "J\u00fcng\u00b7ling", "Mu\u00b7za"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ART", "NN", "NE"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Trank nicht; bittend sprach er zu der Mutter:", "tokens": ["Trank", "nicht", ";", "bit\u00b7tend", "sprach", "er", "zu", "der", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbedle Frau, der Himmel sey euch g\u00fcnstig!", "tokens": ["\u00bb", "ed\u00b7le", "Frau", ",", "der", "Him\u00b7mel", "sey", "euch", "g\u00fcns\u00b7tig", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aber gebt, o gebt mir eure Tochter", "tokens": ["A\u00b7ber", "gebt", ",", "o", "gebt", "mir", "eu\u00b7re", "Toch\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "FM", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zur getreuen Gattin.\u00ab \u00bbScherze nicht so,", "tokens": ["Zur", "ge\u00b7treu\u00b7en", "Gat\u00b7tin", ".", "\u00ab", "\u00bb", "Scher\u00b7ze", "nicht", "so", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$(", "$(", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Spricht die Mutter, du des Bascha Krieger,", "tokens": ["Spricht", "die", "Mut\u00b7ter", ",", "du", "des", "Ba\u00b7scha", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Lang verm\u00e4hlet ist schon meine Tochter", "tokens": ["Lang", "ver\u00b7m\u00e4h\u00b7let", "ist", "schon", "mei\u00b7ne", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "An Zikolo, an des stolzen Janko", "tokens": ["An", "Zi\u00b7ko\u00b7lo", ",", "an", "des", "stol\u00b7zen", "Jan\u00b7ko"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Neffen. Er gab ihr von rother Seide", "tokens": ["Nef\u00b7fen", ".", "Er", "gab", "ihr", "von", "ro\u00b7ther", "Sei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Drei gar aus der Maassen sch\u00f6ne Kleider,", "tokens": ["Drei", "gar", "aus", "der", "Maas\u00b7sen", "sch\u00f6\u00b7ne", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und von feinem Golde drei Agraffen,", "tokens": ["Und", "von", "fei\u00b7nem", "Gol\u00b7de", "drei", "A\u00b7graf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und drei Diamanten, also pr\u00e4chtig,", "tokens": ["Und", "drei", "Di\u00b7a\u00b7man\u00b7ten", ",", "al\u00b7so", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da\u00df an ihrem Glanz man Abends speisen", "tokens": ["Da\u00df", "an", "ih\u00b7rem", "Glanz", "man", "A\u00b7bends", "spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PIS", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und in Mitternacht, als w\u00e4r es Mittag,", "tokens": ["Und", "in", "Mit\u00b7ter\u00b7nacht", ",", "als", "w\u00e4r", "es", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Zehen Pferd' behufen k\u00f6nnte. Also", "tokens": ["Ze\u00b7hen", "Pferd'", "be\u00b7hu\u00b7fen", "k\u00f6nn\u00b7te", ".", "Al\u00b7so"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "VVINF", "VMFIN", "$.", "XY"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Ist f\u00fcr dich, o Krieger, nicht das M\u00e4dchen.\u00ab", "tokens": ["Ist", "f\u00fcr", "dich", ",", "o", "Krie\u00b7ger", ",", "nicht", "das", "M\u00e4d\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Traurig sa\u00df auf dieses Wort der J\u00fcngling,", "tokens": ["Trau\u00b7rig", "sa\u00df", "auf", "die\u00b7ses", "Wort", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprach nicht mehr und schlo\u00df die Nacht kein Auge,", "tokens": ["Sprach", "nicht", "mehr", "und", "schlo\u00df", "die", "Nacht", "kein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "KON", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und nach langer Nacht bei Tages Anbruch", "tokens": ["Und", "nach", "lan\u00b7ger", "Nacht", "bei", "Ta\u00b7ges", "An\u00b7bruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sprang er auf, auf seine wackern F\u00fcsse,", "tokens": ["Sprang", "er", "auf", ",", "auf", "sei\u00b7ne", "wa\u00b7ckern", "F\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ging zum Zelt des Bascha und mit tiefen", "tokens": ["Ging", "zum", "Zelt", "des", "Ba\u00b7scha", "und", "mit", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NE", "KON", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Worten sprach er also: Hoher Bascha,", "tokens": ["Wor\u00b7ten", "sprach", "er", "al\u00b7so", ":", "Ho\u00b7her", "Ba\u00b7scha", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$.", "NE", "NE", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Unter allen Sch\u00f6nen, die dein weites", "tokens": ["Un\u00b7ter", "al\u00b7len", "Sch\u00f6\u00b7nen", ",", "die", "dein", "wei\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Land dir zollet, ist von Himmelssch\u00f6nheit", "tokens": ["Land", "dir", "zol\u00b7let", ",", "ist", "von", "Him\u00b7mels\u00b7sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Hier ein M\u00e4dchen, unsrer Sprache kundig,", "tokens": ["Hier", "ein", "M\u00e4d\u00b7chen", ",", "uns\u00b7rer", "Spra\u00b7che", "kun\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Tochter des Nikolo, Herrn von Gravo.", "tokens": ["Toch\u00b7ter", "des", "Ni\u00b7ko\u00b7lo", ",", "Herrn", "von", "Gra\u00b7vo", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$,", "NN", "APPR", "NE", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Und der Bascha lie\u00df den Grafen rufen,", "tokens": ["Und", "der", "Ba\u00b7scha", "lie\u00df", "den", "Gra\u00b7fen", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sprach vertraulich zu ihm: \u00bbist es Wahrheit,", "tokens": ["Sprach", "ver\u00b7trau\u00b7lich", "zu", "ihm", ":", "\u00bb", "ist", "es", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "$.", "$(", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was die Rede saget? deine Tochter", "tokens": ["Was", "die", "Re\u00b7de", "sa\u00b7get", "?", "dei\u00b7ne", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sey so sch\u00f6n und lieblich aus der Maassen?", "tokens": ["Sey", "so", "sch\u00f6n", "und", "lieb\u00b7lich", "aus", "der", "Maas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wolltest du sie mir zur Gattin geben?\u00ab", "tokens": ["Woll\u00b7test", "du", "sie", "mir", "zur", "Gat\u00b7tin", "ge\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Unver\u00e4ndert sprach der edle Vater:", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "sprach", "der", "ed\u00b7le", "Va\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbsch\u00f6n ist meine Tochter, hold und lieblich;", "tokens": ["\u00bb", "sch\u00f6n", "ist", "mei\u00b7ne", "Toch\u00b7ter", ",", "hold", "und", "lieb\u00b7lich", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber l\u00e4ngst ist sie zur Braut verm\u00e4hlet.", "tokens": ["A\u00b7ber", "l\u00e4ngst", "ist", "sie", "zur", "Braut", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zekulo, des stolzen Janko Neffe,", "tokens": ["Ze\u00b7ku\u00b7lo", ",", "des", "stol\u00b7zen", "Jan\u00b7ko", "Nef\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gab von rother Seide ihr drei Kleider,", "tokens": ["Gab", "von", "ro\u00b7ther", "Sei\u00b7de", "ihr", "drei", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und von feinem Golde drei Agraffen,", "tokens": ["Und", "von", "fei\u00b7nem", "Gol\u00b7de", "drei", "A\u00b7graf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und drei Diamanten.\u00ab", "tokens": ["Und", "drei", "D