{"textgrid.poem.26845": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Aachen in seiner Kaiserpracht,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Aachen in seiner Kaiserpracht,", "tokens": ["Zu", "Aa\u00b7chen", "in", "sei\u00b7ner", "Kai\u00b7ser\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im altert\u00fcmlichen Saale,", "tokens": ["Im", "al\u00b7ter\u00b7t\u00fcm\u00b7li\u00b7chen", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sa\u00df K\u00f6nig Rudolfs heilige Macht", "tokens": ["Sa\u00df", "K\u00f6\u00b7nig", "Ru\u00b7dolfs", "hei\u00b7li\u00b7ge", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Beim festlichen Kr\u00f6nungsmahle.", "tokens": ["Beim", "fest\u00b7li\u00b7chen", "Kr\u00f6\u00b7nungs\u00b7mah\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Speisen trug der Pfalzgraf des Rheins,", "tokens": ["Die", "Spei\u00b7sen", "trug", "der", "Pfalz\u00b7graf", "des", "Rheins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Es schenkte der B\u00f6hme des perlenden Weins,", "tokens": ["Es", "schenk\u00b7te", "der", "B\u00f6h\u00b7me", "des", "per\u00b7len\u00b7den", "Weins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und alle die W\u00e4hler, die sieben,", "tokens": ["Und", "al\u00b7le", "die", "W\u00e4h\u00b7ler", ",", "die", "sie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "$,", "PRELS", "CARD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wie der Sterne Chor um die Sonne sich stellt,", "tokens": ["Wie", "der", "Ster\u00b7ne", "Chor", "um", "die", "Son\u00b7ne", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Umstanden gesch\u00e4ftig den Herrscher der Welt,", "tokens": ["Um\u00b7stan\u00b7den", "ge\u00b7sch\u00e4f\u00b7tig", "den", "Herr\u00b7scher", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die W\u00fcrde des Amtes zu \u00fcben.", "tokens": ["Die", "W\u00fcr\u00b7de", "des", "Am\u00b7tes", "zu", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und rings erf\u00fcllte den hohen Balkon", "tokens": ["Und", "rings", "er\u00b7f\u00fcll\u00b7te", "den", "ho\u00b7hen", "Bal\u00b7kon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Volk in freudgem Gedr\u00e4nge,", "tokens": ["Das", "Volk", "in", "freud\u00b7gem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Laut mischte sich in der Posaunen Ton", "tokens": ["Laut", "mischte", "sich", "in", "der", "Po\u00b7sau\u00b7nen", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Das jauchzende Rufen der Menge.", "tokens": ["Das", "jauch\u00b7zen\u00b7de", "Ru\u00b7fen", "der", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Denn geendigt nach langem verderblichen Streit", "tokens": ["Denn", "ge\u00b7en\u00b7digt", "nach", "lan\u00b7gem", "ver\u00b7derb\u00b7li\u00b7chen", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "War die kaiserlose, die schreckliche Zeit,", "tokens": ["War", "die", "kai\u00b7ser\u00b7lo\u00b7se", ",", "die", "schreck\u00b7li\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und ein Richter war wieder auf Erden.", "tokens": ["Und", "ein", "Rich\u00b7ter", "war", "wie\u00b7der", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Nicht blind mehr waltet der eiserne Speer,", "tokens": ["Nicht", "blind", "mehr", "wal\u00b7tet", "der", "ei\u00b7ser\u00b7ne", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Nicht f\u00fcrchtet der Schwache, der Friedliche mehr,", "tokens": ["Nicht", "f\u00fcrch\u00b7tet", "der", "Schwa\u00b7che", ",", "der", "Fried\u00b7li\u00b7che", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Des M\u00e4chtigen Beute zu werden.", "tokens": ["Des", "M\u00e4ch\u00b7ti\u00b7gen", "Beu\u00b7te", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und der Kaiser ergreift den goldnen Pokal", "tokens": ["Und", "der", "Kai\u00b7ser", "er\u00b7greift", "den", "gold\u00b7nen", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und spricht mit zufriedenen Blicken:", "tokens": ["Und", "spricht", "mit", "zu\u00b7frie\u00b7de\u00b7nen", "Bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbwohl gl\u00e4nzet das Fest, wohl pranget das Mahl,", "tokens": ["\u00bb", "wohl", "gl\u00e4n\u00b7zet", "das", "Fest", ",", "wohl", "pran\u00b7get", "das", "Mahl", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein k\u00f6niglich Herz zu entz\u00fccken;", "tokens": ["Mein", "k\u00f6\u00b7nig\u00b7lich", "Herz", "zu", "ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Doch den S\u00e4nger vermi\u00df ich, den Bringer der Lust,", "tokens": ["Doch", "den", "S\u00e4n\u00b7ger", "ver\u00b7mi\u00df", "ich", ",", "den", "Brin\u00b7ger", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Der mit s\u00fc\u00dfem Klang mir bewege die Brust", "tokens": ["Der", "mit", "s\u00fc\u00b7\u00dfem", "Klang", "mir", "be\u00b7we\u00b7ge", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und mit g\u00f6ttlich erhabenen Lehren.", "tokens": ["Und", "mit", "g\u00f6tt\u00b7lich", "er\u00b7ha\u00b7be\u00b7nen", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "So hab ichs gehalten von Jugend an,", "tokens": ["So", "hab", "ichs", "ge\u00b7hal\u00b7ten", "von", "Ju\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und was ich als Ritter gepflegt und getan,", "tokens": ["Und", "was", "ich", "als", "Rit\u00b7ter", "ge\u00b7pflegt", "und", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "KOUS", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Nicht will ichs als Kaiser entbehren.\u00ab", "tokens": ["Nicht", "will", "ichs", "als", "Kai\u00b7ser", "ent\u00b7beh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VMFIN", "PIS", "KOKOM", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und sieh! in der F\u00fcrsten umgebenden Kreis", "tokens": ["Und", "sieh", "!", "in", "der", "F\u00fcrs\u00b7ten", "um\u00b7ge\u00b7ben\u00b7den", "Kreis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trat der S\u00e4nger im langen Talare,", "tokens": ["Trat", "der", "S\u00e4n\u00b7ger", "im", "lan\u00b7gen", "Ta\u00b7la\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihm gl\u00e4nzte die Locke silberwei\u00df,", "tokens": ["Ihm", "gl\u00e4nz\u00b7te", "die", "Lo\u00b7cke", "sil\u00b7ber\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gebleicht von der F\u00fclle der Jahre.", "tokens": ["Ge\u00b7bleicht", "von", "der", "F\u00fcl\u00b7le", "der", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u00bbs\u00fc\u00dfer Wohllaut schl\u00e4ft in der Saiten Gold,", "tokens": ["\u00bb", "s\u00fc\u00b7\u00dfer", "Wohl\u00b7laut", "schl\u00e4ft", "in", "der", "Sai\u00b7ten", "Gold", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der S\u00e4nger singt von der Minne Sold,", "tokens": ["Der", "S\u00e4n\u00b7ger", "singt", "von", "der", "Min\u00b7ne", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er preiset das H\u00f6chste, das Beste,", "tokens": ["Er", "prei\u00b7set", "das", "H\u00f6chs\u00b7te", ",", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Was das Herz sich w\u00fcnscht, was der Sinn begehrt,", "tokens": ["Was", "das", "Herz", "sich", "w\u00fcnscht", ",", "was", "der", "Sinn", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Doch sage, was ist des Kaisers wert", "tokens": ["Doch", "sa\u00b7ge", ",", "was", "ist", "des", "Kai\u00b7sers", "wert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "An seinem herrlichsten Feste?\u00ab", "tokens": ["An", "sei\u00b7nem", "herr\u00b7lichs\u00b7ten", "Fes\u00b7te", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbnicht gebieten werd ich dem S\u00e4nger\u00ab, spricht", "tokens": ["\u00bb", "nicht", "ge\u00b7bie\u00b7ten", "werd", "ich", "dem", "S\u00e4n\u00b7ger", "\u00ab", ",", "spricht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "PTKNEG", "VVINF", "VAFIN", "PPER", "ART", "NN", "$(", "$,", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Herrscher mit l\u00e4chelndem Munde,", "tokens": ["Der", "Herr\u00b7scher", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bber steht in des gr\u00f6\u00dferen Herren Pflicht,", "tokens": ["\u00bb", "er", "steht", "in", "des", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Her\u00b7ren", "Pflicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er gehorcht der gebietenden Stunde:", "tokens": ["Er", "ge\u00b7horcht", "der", "ge\u00b7bie\u00b7ten\u00b7den", "Stun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wie in den L\u00fcften der Sturmwind saust,", "tokens": ["Wie", "in", "den", "L\u00fcf\u00b7ten", "der", "Sturm\u00b7wind", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Man wei\u00df nicht, von wannen er kommt und braust,", "tokens": ["Man", "wei\u00df", "nicht", ",", "von", "wan\u00b7nen", "er", "kommt", "und", "braust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "APPR", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wie der Quell aus verborgenen Tiefen,", "tokens": ["Wie", "der", "Quell", "aus", "ver\u00b7bor\u00b7ge\u00b7nen", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "So des S\u00e4ngers Lied aus dem Innern schallt", "tokens": ["So", "des", "S\u00e4n\u00b7gers", "Lied", "aus", "dem", "In\u00b7nern", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und wecket der dunkeln Gef\u00fchle Gewalt,", "tokens": ["Und", "we\u00b7cket", "der", "dun\u00b7keln", "Ge\u00b7f\u00fch\u00b7le", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVF