, "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Drum da\u00df dem Lechzenden werde sein Heil,", "tokens": ["Drum", "da\u00df", "dem", "Lech\u00b7zen\u00b7den", "wer\u00b7de", "sein", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "So will ich das W\u00e4sserlein jetzt in Eil", "tokens": ["So", "will", "ich", "das", "W\u00e4s\u00b7ser\u00b7lein", "jetzt", "in", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Durchwaten mit nackenden F\u00fc\u00dfen.\u2039", "tokens": ["Durch\u00b7wa\u00b7ten", "mit", "na\u00b7cken\u00b7den", "F\u00fc\u00b7\u00dfen", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Da setzt ihn der Graf auf sein ritterlich Pferd", "tokens": ["Da", "setzt", "ihn", "der", "Graf", "auf", "sein", "rit\u00b7ter\u00b7lich", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und reicht ihm die pr\u00e4chtigen Z\u00e4ume,", "tokens": ["Und", "reicht", "ihm", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "Z\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da\u00df er labe den Kranken, der sein begehrt,", "tokens": ["Da\u00df", "er", "la\u