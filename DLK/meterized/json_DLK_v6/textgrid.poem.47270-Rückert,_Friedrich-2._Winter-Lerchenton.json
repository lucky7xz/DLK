{"textgrid.poem.47270": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Winter-Lerchenton", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In L\u00fcften h\u00e4ngt ein Lerchenton,", "tokens": ["In", "L\u00fcf\u00b7ten", "h\u00e4ngt", "ein", "Ler\u00b7chen\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Ohr hat staunend ihn vernommen.", "tokens": ["Mein", "Ohr", "hat", "stau\u00b7nend", "ihn", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's eine, die noch nicht entflohn?", "tokens": ["Ist's", "ei\u00b7ne", ",", "die", "noch", "nicht", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's eine, die zur\u00fcckgekommen,", "tokens": ["Ist's", "ei\u00b7ne", ",", "die", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gelockt von Fr\u00fchling schon,", "tokens": ["Ge\u00b7lockt", "von", "Fr\u00fch\u00b7ling", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da rings die Sch\u00f6pfung noch von Winter ist beklommen?", "tokens": ["Da", "rings", "die", "Sch\u00f6p\u00b7fung", "noch", "von", "Win\u00b7ter", "ist", "be\u00b7klom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch meine Seele zieht ein Schwung,", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "zieht", "ein", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den jener Ton hat angeschlagen.", "tokens": ["Den", "je\u00b7ner", "Ton", "hat", "an\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's Ahnung, ist's Erinnerung", "tokens": ["Ist's", "Ah\u00b7nung", ",", "ist's", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Von k\u00fcnftigen, von vor'gen Tagen?", "tokens": ["Von", "k\u00fcnf\u00b7ti\u00b7gen", ",", "von", "vor'\u00b7gen", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchle nur mich jung,", "tokens": ["Ich", "f\u00fch\u00b7le", "nur", "mich", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ob wie ich's war, ob wie ich sein werd'? ist zu fragen.", "tokens": ["Ob", "wie", "ich's", "war", ",", "ob", "wie", "ich", "sein", "werd'", "?", "ist", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PIS", "VAFIN", "$,", "KOUS", "KOKOM", "PPER", "VAINF", "VAFIN", "$.", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verklungen ist die Melodie,", "tokens": ["Ver\u00b7klun\u00b7gen", "ist", "die", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlungen von Schneewolkenherden;", "tokens": ["Ver\u00b7schlun\u00b7gen", "von", "Schnee\u00b7wol\u00b7ken\u00b7her\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und Winter ist's im Herzen, wie", "tokens": ["Und", "Win\u00b7ter", "ist's", "im", "Her\u00b7zen", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Himmel Winter und auf Erden,", "tokens": ["Am", "Him\u00b7mel", "Win\u00b7ter", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So Winter, als ob nie", "tokens": ["So", "Win\u00b7ter", ",", "als", "ob", "nie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "KOKOM", "KOUS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gewesen Fr\u00fchling sei und nimmer sollte werden.", "tokens": ["Ge\u00b7we\u00b7sen", "Fr\u00fch\u00b7ling", "sei", "und", "nim\u00b7mer", "soll\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "KON", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}