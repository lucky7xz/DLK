{"textgrid.poem.65446": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "19. K\u00fcsse und Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage nicht, da\u00df in des Menschen Brust nur ", "tokens": ["Sa\u00b7ge", "nicht", ",", "da\u00df", "in", "des", "Men\u00b7schen", "Brust", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fchl' ich doch, da\u00df eine Seele dir mit jedem Ku\u00df ich gebe.", "tokens": ["F\u00fchl'", "ich", "doch", ",", "da\u00df", "ei\u00b7ne", "See\u00b7le", "dir", "mit", "je\u00b7dem", "Ku\u00df", "ich", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und wie oft ich dich auch k\u00fcsse, alle K\u00fcsse haben Seelen,", "tokens": ["Und", "wie", "oft", "ich", "dich", "auch", "k\u00fcs\u00b7se", ",", "al\u00b7le", "K\u00fcs\u00b7se", "ha\u00b7ben", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "PRF", "ADV", "VVFIN", "$,", "PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und mir werden eher K\u00fcsse, als den K\u00fcssen Seelen fehlen.", "tokens": ["Und", "mir", "wer\u00b7den", "e\u00b7her", "K\u00fcs\u00b7se", ",", "als", "den", "K\u00fcs\u00b7sen", "See\u00b7len", "feh\