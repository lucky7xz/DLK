{"dta.poem.5451": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob  GOTTES .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht ein eintziges Gestirn, in des Himmels tieffen H\u00f6hen,", "tokens": ["Nicht", "ein", "eint\u00b7zi\u00b7ges", "Ge\u00b7stirn", ",", "in", "des", "Him\u00b7mels", "tief\u00b7fen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wird es gleich in dunckler Nacht nicht von Menschen", "tokens": ["Wird", "es", "gleich", "in", "dunck\u00b7ler", "Nacht", "nicht", "von", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "angesehen,", "tokens": ["an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Flammt und gl\u00e4ntzt darum umsonst. Lieber Mensch, ge-", "tokens": ["Flammt", "und", "gl\u00e4ntzt", "da\u00b7rum", "um\u00b7sonst", ".", "Lie\u00b7ber", "Mensch", ",", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PAV", "ADV", "$.", "ADJD", "NN", "$,", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "dencke nicht,", "tokens": ["den\u00b7cke", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ob es gleich von Menschen Seelen", "tokens": ["Ob", "es", "gleich", "von", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht geschicht;", "tokens": ["Nicht", "ge\u00b7schicht", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Da\u00df Bewunderer dem Himmel, und GOtt Lob-Ges\u00e4nge,", "tokens": ["Da\u00df", "Be\u00b7wun\u00b7de\u00b7rer", "dem", "Him\u00b7mel", ",", "und", "Gott", "Lob\u00b7Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "fehlen.", "tokens": ["feh\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Millionen Creaturen, welche geistig, leben, schweben,", "tokens": ["Mil\u00b7lion\u00b7en", "Crea\u00b7tu\u00b7ren", ",", "wel\u00b7che", "geis\u00b7tig", ",", "le\u00b7ben", ",", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Unsichtbar um unser\u2019 Erde, die des Sch\u00f6pfers Lob erheben.", "tokens": ["Un\u00b7sicht\u00b7bar", "um", "un\u00b7ser'", "Er\u00b7de", ",", "die", "des", "Sch\u00f6p\u00b7fers", "Lob", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ja, so wol dann, wann wir schlaffen, als wann unser", "tokens": ["Ja", ",", "so", "wol", "dann", ",", "wann", "wir", "schlaf\u00b7fen", ",", "als", "wann", "un\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PWAV", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Auge wacht,", "tokens": ["Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Mit nie unterbrochnem Lobe, so bey Tag\u2019, als bey der Nacht,", "tokens": ["Mit", "nie", "un\u00b7ter\u00b7broch\u00b7nem", "Lo\u00b7be", ",", "so", "bey", "Tag'", ",", "als", "bey", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Sehen und bewundern sie seiner Wercke Wunder-Pracht.", "tokens": ["Se\u00b7hen", "und", "be\u00b7wun\u00b7dern", "sie", "sei\u00b7ner", "Wer\u00b7cke", "Wun\u00b7der\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}