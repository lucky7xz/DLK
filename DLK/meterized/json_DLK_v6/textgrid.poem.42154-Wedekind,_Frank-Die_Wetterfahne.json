{"textgrid.poem.42154": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Die Wetterfahne", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du auf deinem h\u00f6chsten Dach,", "tokens": ["Du", "auf", "dei\u00b7nem", "h\u00f6chs\u00b7ten", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich in n\u00e4chster N\u00e4he;", "tokens": ["Ich", "in", "n\u00e4chs\u00b7ter", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch die wahre Liebe, ach,", "tokens": ["Doch", "die", "wah\u00b7re", "Lie\u00b7be", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ITJ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwankt in solcher H\u00f6he,", "tokens": ["Schwankt", "in", "sol\u00b7cher", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Du in deinem Herzen leer,", "tokens": ["Du", "in", "dei\u00b7nem", "Her\u00b7zen", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich in blindem Wahne \u2013", "tokens": ["Ich", "in", "blin\u00b7dem", "Wah\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dreh dich hin, dreh dich her,", "tokens": ["Dreh", "dich", "hin", ",", "dreh", "dich", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sch\u00f6ne Wetterfahne!", "tokens": ["Sch\u00f6\u00b7ne", "Wet\u00b7ter\u00b7fah\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unterhaltend pfeift der Wind,", "tokens": ["Un\u00b7ter\u00b7hal\u00b7tend", "pfeift", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Saust uns um die Ohren;", "tokens": ["Saust", "uns", "um", "die", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Von des Himmels Freuden sind", "tokens": ["Von", "des", "Him\u00b7mels", "Freu\u00b7den", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine noch verloren!", "tokens": ["Kei\u00b7ne", "noch", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Glaubst du, da\u00df verliebt ich bin,", "tokens": ["Glaubst", "du", ",", "da\u00df", "ver\u00b7liebt", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil ich dich ermahne?", "tokens": ["Weil", "ich", "dich", "er\u00b7mah\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dreh dich her, dreh dich hin,", "tokens": ["Dreh", "dich", "her", ",", "dreh", "dich", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sch\u00f6ne Wetterfahne!", "tokens": ["Sch\u00f6\u00b7ne", "Wet\u00b7ter\u00b7fah\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Drehn wir uns auf hohem Turm", "tokens": ["Drehn", "wir", "uns", "auf", "ho\u00b7hem", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer frisch und munter!", "tokens": ["Im\u00b7mer", "frisch", "und", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, der erste Wintersturm", "tokens": ["Ach", ",", "der", "ers\u00b7te", "Win\u00b7ter\u00b7sturm"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleudert dich hinunter.", "tokens": ["Schleu\u00b7dert", "dich", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn dann auch verflogen w\u00e4r,", "tokens": ["Wenn", "dann", "auch", "ver\u00b7flo\u00b7gen", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich jetzt noch ahne ...", "tokens": ["Was", "ich", "jetzt", "noch", "ah\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dreh dich hin, dreh dich her,", "tokens": ["Dreh", "dich", "hin", ",", "dreh", "dich", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sch\u00f6ne Wetterfahne!", "tokens": ["Sch\u00f6\u00b7ne", "Wet\u00b7ter\u00b7fah\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}