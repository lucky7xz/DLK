{"textgrid.poem.46216": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein anschlag ist zu frech, zu schwach dein widerstand,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein anschlag ist zu frech, zu schwach dein widerstand,", "tokens": ["Dein", "an\u00b7schlag", "ist", "zu", "frech", ",", "zu", "schwach", "dein", "wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o torecht stolzer feind. Die warheit dir zu sagen", "tokens": ["o", "to\u00b7recht", "stol\u00b7zer", "feind", ".", "Die", "war\u00b7heit", "dir", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM", "ADJD", "ADJA", "NN", "$.", "ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mu\u00df dises helden lieb und dein leid dir behagen,", "tokens": ["mu\u00df", "di\u00b7ses", "hel\u00b7den", "lieb", "und", "dein", "leid", "dir", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADJD", "KON", "PPOSAT", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil seine gnad dein trost, wie sein zorn deine schand.", "tokens": ["weil", "sei\u00b7ne", "gnad", "dein", "trost", ",", "wie", "sein", "zorn", "dei\u00b7ne", "schand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit weisheit, mut und macht sein wertes haupt, herz, hand", "tokens": ["Mit", "weis\u00b7heit", ",", "mut", "und", "macht", "sein", "wer\u00b7tes", "haupt", ",", "herz", ",", "hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "gezieret, krieg und ruh, sig und gnad zugleich tragen;", "tokens": ["ge\u00b7zie\u00b7ret", ",", "krieg", "und", "ruh", ",", "sig", "und", "gnad", "zu\u00b7gleich", "tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "KON", "NN", "$,", "ADJD", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-++-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "mit ihm ist nur ein werk zu sigen und zu schlagen", "tokens": ["mit", "ihm", "ist", "nur", "ein", "werk", "zu", "si\u00b7gen", "und", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "nichts kan unm\u00fcglich sein f\u00fcr sein schwert und verstand.", "tokens": ["nichts", "kan", "un\u00b7m\u00fcg\u00b7lich", "sein", "f\u00fcr", "sein", "schwert", "und", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "APPR", "PPOSAT", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "So soll nu billich sich das reich mit Brissach neigen", "tokens": ["So", "soll", "nu", "bil\u00b7lich", "sich", "das", "reich", "mit", "Bris\u00b7sach", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADJD", "PRF", "ART", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit wahrer huld und bit: O du der tugend sohn", "tokens": ["mit", "wah\u00b7rer", "huld", "und", "bit", ":", "O", "du", "der", "tu\u00b7gend", "sohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "$.", "NE", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "nem uns (gl\u00fcckselig) auf, dieweil wir nu dein eigen!", "tokens": ["nem", "uns", "(", "gl\u00fcck\u00b7se\u00b7lig", ")", "auf", ",", "die\u00b7weil", "wir", "nu", "dein", "ei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "ADJD", "$(", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du bist ja unser herr, wie der h\u00f6chst dein patron,", "tokens": ["Du", "bist", "ja", "un\u00b7ser", "herr", ",", "wie", "der", "h\u00f6chst", "dein", "pat\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und da ist, da mu\u00df sich, wa du dich wilt, erzeigen,", "tokens": ["und", "da", "ist", ",", "da", "mu\u00df", "sich", ",", "wa", "du", "dich", "wilt", ",", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "ADV", "VMFIN", "PRF", "$,", "PWAV", "PPER", "PPER", "VMFIN", "$,", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "der welt wohn, des rechts thron, des reichs und Teutschlands kron.", "tokens": ["der", "welt", "wohn", ",", "des", "rechts", "thron", ",", "des", "reichs", "und", "Teutschlands", "kron", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADV", "PTKVZ", "$,", "ART", "ADJA", "KON", "NE", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}