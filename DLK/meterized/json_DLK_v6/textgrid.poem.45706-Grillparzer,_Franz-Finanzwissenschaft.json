{"textgrid.poem.45706": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Finanzwissenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anlehnen! Anlehnen!", "tokens": ["An\u00b7leh\u00b7nen", "!", "An\u00b7leh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wer von selbst nicht stehen kann,", "tokens": ["Wer", "von", "selbst", "nicht", "ste\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber bricht einmal die Lehne,", "tokens": ["A\u00b7ber", "bricht", "ein\u00b7mal", "die", "Leh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was alsdann? Was alsdann?", "tokens": ["Was", "als\u00b7dann", "?", "Was", "als\u00b7dann", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "PWS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}