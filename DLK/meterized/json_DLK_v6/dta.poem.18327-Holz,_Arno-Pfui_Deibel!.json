{"dta.poem.18327": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Pfui Deibel!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin des Zeitgeists Stra\u00dfenkehrer;", "tokens": ["Ich", "bin", "des", "Zeit\u00b7geists", "Stra\u00b7\u00dfen\u00b7keh\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch protzgere Kerle sah ich noch nie,", "tokens": ["Doch", "protz\u00b7ge\u00b7re", "Ker\u00b7le", "sah", "ich", "noch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als die Schlotbarone der Plutokratie!", "tokens": ["Als", "die", "Schlot\u00b7ba\u00b7ro\u00b7ne", "der", "Plu\u00b7to\u00b7kra\u00b7tie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}