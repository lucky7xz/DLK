{"textgrid.poem.39209": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin nicht immer so frei und treu", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht immer so frei und treu", "tokens": ["Ich", "bin", "nicht", "im\u00b7mer", "so", "frei", "und", "treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie hier in Eurer Mitt',", "tokens": ["Wie", "hier", "in", "Eu\u00b7rer", "Mitt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft hab' ich geh\u00f6rt mit bebender Scheu", "tokens": ["Oft", "hab'", "ich", "ge\u00b7h\u00f6rt", "mit", "be\u00b7ben\u00b7der", "Scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Schicksals trotzigen Tritt.", "tokens": ["Des", "Schick\u00b7sals", "trot\u00b7zi\u00b7gen", "Tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich habe verloren manchen Stein", "tokens": ["Ich", "ha\u00b7be", "ver\u00b7lo\u00b7ren", "man\u00b7chen", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der g\u00fcldenen Krone der Lust", "tokens": ["Aus", "der", "g\u00fcl\u00b7de\u00b7nen", "Kro\u00b7ne", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und habe geh\u00f6rt mit blutiger Pein", "tokens": ["Und", "ha\u00b7be", "ge\u00b7h\u00f6rt", "mit", "blu\u00b7ti\u00b7ger", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das st\u00e4hlerne Wort: Du mu\u00dft!", "tokens": ["Das", "st\u00e4h\u00b7ler\u00b7ne", "Wort", ":", "Du", "mu\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich habe gesungen das alte Lied", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7sun\u00b7gen", "das", "al\u00b7te", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der gemordeten Seligkeit", "tokens": ["Der", "ge\u00b7mor\u00b7de\u00b7ten", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und habe gek\u00e4mpft mit krankem Gem\u00fct", "tokens": ["Und", "ha\u00b7be", "ge\u00b7k\u00e4mpft", "mit", "kran\u00b7kem", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das w\u00fcste Gefecht der Zeit.", "tokens": ["Das", "w\u00fcs\u00b7te", "Ge\u00b7fecht", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gezwungen zu dem, was ich stets geha\u00dft,", "tokens": ["Ge\u00b7zwun\u00b7gen", "zu", "dem", ",", "was", "ich", "stets", "ge\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der bl\u00fchendste Wunsch zerst\u00f6rt,", "tokens": ["Der", "bl\u00fc\u00b7hends\u00b7te", "Wunsch", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So hab' ich die Schicksalsstunde verpa\u00dft", "tokens": ["So", "hab'", "ich", "die", "Schick\u00b7sals\u00b7stun\u00b7de", "ver\u00b7pa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fruchtlos mich emp\u00f6rt.", "tokens": ["Und", "frucht\u00b7los", "mich", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch wer an Eurem Herde ruht,", "tokens": ["Doch", "wer", "an", "Eu\u00b7rem", "Her\u00b7de", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den l\u00e4\u00dft die Jagd der Qual,", "tokens": ["Den", "l\u00e4\u00dft", "die", "Jagd", "der", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seid so sch\u00f6n, Ihr seid so gut,", "tokens": ["Ihr", "seid", "so", "sch\u00f6n", ",", "Ihr", "seid", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott segn' Euch tausendmal.", "tokens": ["Gott", "segn'", "Euch", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}