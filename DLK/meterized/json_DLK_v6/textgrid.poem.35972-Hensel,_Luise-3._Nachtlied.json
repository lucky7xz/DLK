{"textgrid.poem.35972": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "3. Nachtlied", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du schlafen gangen,", "tokens": ["Bist", "du", "schla\u00b7fen", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hast genug gewacht?", "tokens": ["Hast", "ge\u00b7nug", "ge\u00b7wacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4lt dich Traum umfangen?", "tokens": ["H\u00e4lt", "dich", "Traum", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Liebe, gute Nacht!", "tokens": ["Lie\u00b7be", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4lt der Traum umfangen", "tokens": ["H\u00e4lt", "der", "Traum", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "D\u00e4mmernd, still und sacht,", "tokens": ["D\u00e4m\u00b7mernd", ",", "still", "und", "sacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Rosenwangen?", "tokens": ["Dei\u00b7ne", "Ro\u00b7sen\u00b7wan\u00b7gen", "?"], "token_info": ["word", "word", "punc