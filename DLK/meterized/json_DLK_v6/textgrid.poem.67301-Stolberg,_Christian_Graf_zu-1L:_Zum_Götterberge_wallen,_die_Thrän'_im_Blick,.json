{"textgrid.poem.67301": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum G\u00f6tterberge wallen, die Thr\u00e4n' im Blick,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum G\u00f6tterberge wallen, die Thr\u00e4n' im Blick,", "tokens": ["Zum", "G\u00f6t\u00b7ter\u00b7ber\u00b7ge", "wal\u00b7len", ",", "die", "Thr\u00e4n'", "im", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Weihrauchsperlen z\u00fcndend am S\u00fchnaltar,", "tokens": ["Und", "Weih\u00b7rauchs\u00b7per\u00b7len", "z\u00fcn\u00b7dend", "am", "S\u00fch\u00b7nal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zeus Zwillingst\u00f6chter: ", "tokens": ["Zeus", "Zwil\u00b7lings\u00b7t\u00f6ch\u00b7ter", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "M\u00e4hlig entschwebt's, wenn auf G\u00f6tterstirnen", "tokens": ["M\u00e4h\u00b7lig", "ent\u00b7schwebt's", ",", "wenn", "auf", "G\u00f6t\u00b7ters\u00b7tir\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ein W\u00f6lkchen d\u00e4mmernd weilte; willkommener", "tokens": ["Ein", "W\u00f6lk\u00b7chen", "d\u00e4m\u00b7mernd", "weil\u00b7te", ";", "will\u00b7kom\u00b7me\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$.", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfang begr\u00fc\u00dfet tr\u00f6stend die S\u00fchnenden;", "tokens": ["Emp\u00b7fang", "be\u00b7gr\u00fc\u00b7\u00dfet", "tr\u00f6s\u00b7tend", "die", "S\u00fch\u00b7nen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Am Fu\u00df der Thronensitze deutet", "tokens": ["Am", "Fu\u00df", "der", "Thro\u00b7nen\u00b7sit\u00b7ze", "deu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihnen ein Wink den bestinmten Sessel \u2013", "tokens": ["Ih\u00b7nen", "ein", "Wink", "den", "bes\u00b7tinm\u00b7ten", "Ses\u00b7sel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wir Erdens\u00f6hne, brechliches Machwerk, taub", "tokens": ["Wir", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ",", "brech\u00b7li\u00b7ches", "Mach\u00b7werk", ",", "taub"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der innern Stimme, meidend das Bessere,", "tokens": ["Der", "in\u00b7nern", "Stim\u00b7me", ",", "mei\u00b7dend", "das", "Bes\u00b7se\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns links hin wendend, unser Pf\u00f6rtchen", "tokens": ["Uns", "links", "hin", "wen\u00b7dend", ",", "un\u00b7ser", "Pf\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wollten wir schlie\u00dfen, wenn bittend anklopft", "tokens": ["Woll\u00b7ten", "wir", "schlie\u00b7\u00dfen", ",", "wenn", "bit\u00b7tend", "an\u00b7klopft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Nachbars Reue? \u2013 Edler, Du z\u00fcrnest nicht", "tokens": ["Des", "Nach\u00b7bars", "Reu\u00b7e", "?", "\u2013", "Ed\u00b7ler", ",", "Du", "z\u00fcr\u00b7nest", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "$(", "NE", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem stummen Freund', und bietet mein T\u00e4ubchen Dir", "tokens": ["Dem", "stum\u00b7men", "Freund'", ",", "und", "bie\u00b7tet", "mein", "T\u00e4ub\u00b7chen", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dies Friedens\u00f6lblatt, o ich schau, wie", "tokens": ["Dies", "Frie\u00b7den\u00b7s\u00f6l\u00b7blatt", ",", "o", "ich", "schau", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDS", "NN", "$,", "FM", "PPER", "PTKVZ", "$,", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich Du streichelst die Abgesandtinn!", "tokens": ["Freund\u00b7lich", "Du", "strei\u00b7chelst", "die", "Ab\u00b7ge\u00b7sand\u00b7tinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Mir sandtest Gru\u00df und T\u00e4flein, und sandtest Du", "tokens": ["Mir", "sand\u00b7test", "Gru\u00df", "und", "T\u00e4\u00b7flein", ",", "und", "sand\u00b7test", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Feiersanges Adeler, dienstbar Dir", "tokens": ["Des", "Fei\u00b7er\u00b7san\u00b7ges", "A\u00b7de\u00b7ler", ",", "dienst\u00b7bar", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In hohen L\u00fcften, treu wie Deinen", "tokens": ["In", "ho\u00b7hen", "L\u00fcf\u00b7ten", ",", "treu", "wie", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KOKOM", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rittern ihr Falk bei dem Federspiele.", "tokens": ["Rit\u00b7tern", "ihr", "Falk", "bei", "dem", "Fe\u00b7der\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ich horchte hoch auf, ahnend den Fittigschlag", "tokens": ["Ich", "horch\u00b7te", "hoch", "auf", ",", "ah\u00b7nend", "den", "Fit\u00b7tig\u00b7schlag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Wohlbekannten, schmeichelnd umkreis't er mich,", "tokens": ["Des", "Wohl\u00b7be\u00b7kann\u00b7ten", ",", "schmei\u00b7chelnd", "um\u00b7kreis't", "er", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Flamm' im Auge, sie gefacht von", "tokens": ["Die", "Flamm'", "im", "Au\u00b7ge", ",", "sie", "ge\u00b7facht", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PPER", "VVPP", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, an dem heiligen Freundschaftsheerde.", "tokens": ["Dir", ",", "an", "dem", "hei\u00b7li\u00b7gen", "Freund\u00b7schafts\u00b7heer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Doch Deines Freundes Leyer und Griffel schwieg!", "tokens": ["Doch", "Dei\u00b7nes", "Freun\u00b7des", "Le\u00b7yer", "und", "Grif\u00b7fel", "schwieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verzeih's in Wehmuth! Wei\u00dfest ja, welcher Gram", "tokens": ["Ver\u00b7zeih's", "in", "Weh\u00b7muth", "!", "Wei\u00b7\u00dfest", "ja", ",", "wel\u00b7cher", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "NN", "$.", "VVFIN", "ADV", "$,", "PWAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich abzehrt, kennst ja ", "tokens": ["Mich", "ab\u00b7zehrt", ",", "kennst", "ja"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "B\u00fcrde nicht d\u00e4mpfet des Geistes Fackel.", "tokens": ["B\u00fcr\u00b7de", "nicht", "d\u00e4mp\u00b7fet", "des", "Geis\u00b7tes", "Fa\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}