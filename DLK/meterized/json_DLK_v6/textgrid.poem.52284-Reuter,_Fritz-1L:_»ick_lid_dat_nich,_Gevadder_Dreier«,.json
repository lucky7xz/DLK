{"textgrid.poem.52284": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbick lid dat nich, Gevadder Dreier\u00ab,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbick lid dat nich, Gevadder Dreier\u00ab,", "tokens": ["\u00bb", "ick", "lid", "dat", "nich", ",", "Ge\u00b7vad\u00b7der", "Drei\u00b7er", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "CARD", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seggt de Stadtspreker Snider Meier,", "tokens": ["Seggt", "de", "Stadt\u00b7spre\u00b7ker", "Sni\u00b7der", "Mei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00bbwenn h\u00fct de Burgemeister Lisch", "tokens": ["\u00bb", "wenn", "h\u00fct", "de", "Bur\u00b7ge\u00b7meis\u00b7ter", "Lisch"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "NE", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will wedder pachten unse Bullenwisch;", "tokens": ["Will", "wed\u00b7der", "pach\u00b7ten", "un\u00b7se", "Bul\u00b7len\u00b7wisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ick slag ganz patzig v\u00f6r em up den Disch,", "tokens": ["Ick", "slag", "ganz", "pat\u00b7zig", "v\u00f6r", "em", "up", "den", "Disch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dat geiht jo rein ut Rand un Band!", "tokens": ["Dat", "geiht", "jo", "rein", "ut", "Rand", "un", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADJD", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fiw Daler \u2013 un dat man Courant?\u00ab", "tokens": ["Fiw", "Da\u00b7ler", "\u2013", "un", "dat", "man", "Cou\u00b7rant", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei g\u00fcngen taum Termin.", "tokens": ["Sei", "g\u00fcn\u00b7gen", "taum", "Ter\u00b7min", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hir stunn de Herr Burmeister Lisch,", "tokens": ["Hir", "stunn", "de", "Herr", "Burm\u00b7eis\u00b7ter", "Lisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "As wir de Bullenwisch all sin,", "tokens": ["As", "wir", "de", "Bul\u00b7len\u00b7wisch", "all", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NE", "NE", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ganz ruhig achter'n gr\u00e4unen Disch,", "tokens": ["Ganz", "ru\u00b7hig", "ach\u00b7ter'n", "gr\u00e4u\u00b7nen", "Disch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Set't sick de Brill up sine N\u00e4sen,", "tokens": ["Set't", "sick", "de", "Brill", "up", "si\u00b7ne", "N\u00e4\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u00dcm de Bedingung af tau lesen.", "tokens": ["\u00dcm", "de", "Be\u00b7din\u00b7gung", "af", "tau", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NE", "NE", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Dunn sprak hei so: \u00bbMin leiwen Fr\u00fcn'n,", "tokens": ["Dunn", "sprak", "hei", "so", ":", "\u00bb", "Min", "lei\u00b7wen", "Fr\u00fcn'n", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "ADV", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ick dauh binah 'ne wohre S\u00fcn'n", "tokens": ["Ick", "dauh", "bi\u00b7nah", "'ne", "woh\u00b7re", "S\u00fcn'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "An Fru un Kind, wenn ick up't Frisch", "tokens": ["An", "Fru", "un", "Kind", ",", "wenn", "ick", "up't", "Frisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "FM", "NN", "$,", "KOUS", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "V\u00f6r't negste Johr mi pacht de Bullenwisch.", "tokens": ["V\u00f6r't", "negs\u00b7te", "Johr", "mi", "pacht", "de", "Bul\u00b7len\u00b7wisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Fiw Daler! F\u00f6r so'n sures Fauder!", "tokens": ["Fiw", "Da\u00b7ler", "!", "F\u00f6r", "so'n", "su\u00b7res", "Fau\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Fiw Daler! \u2013 Ja, un wir't min liwlich Brauder,", "tokens": ["Fiw", "Da\u00b7ler", "!", "\u2013", "Ja", ",", "un", "wir't", "min", "liw\u00b7lich", "Brau\u00b7der", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "$(", "PTKANT", "$,", "FM", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Un wir't uns' Herrgott s\u00fclwst in'n Hewen,", "tokens": ["Un", "wir't", "un\u00b7s'", "Herr\u00b7gott", "s\u00fclwst", "in'n", "He\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Ick k\u00fcnn nich mihr f\u00f6r so 'ne Wisch em gewen.", "tokens": ["Ick", "k\u00fcnn", "nich", "mihr", "f\u00f6r", "so", "'ne", "Wisch", "em", "ge\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Indessen as Burmeister von de Stadt", "tokens": ["In\u00b7des\u00b7sen", "as", "Burm\u00b7eis\u00b7ter", "von", "de", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Will ick up't Frisch en Hart mi faten", "tokens": ["Will", "ick", "up't", "Frisch", "en", "Hart", "mi", "fa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "NE", "NE", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Un will mi h\u00fct nich lumpen laten,", "tokens": ["Un", "will", "mi", "h\u00fct", "nich", "lum\u00b7pen", "la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "VVFIN", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ick will de Wisch mi wedder meiden", "tokens": ["Ick", "will", "de", "Wisch", "mi", "wed\u00b7der", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NE", "NE", "NE", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Un will fiw Daler wedder beiden.", "tokens": ["Un", "will", "fiw", "Da\u00b7ler", "wed\u00b7der", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "NE", "KON", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Also \u2013 fiw Daler beid' ick an!", "tokens": ["Al\u00b7so", "\u2013", "fiw", "Da\u00b7ler", "beid'", "ick", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NE", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Fiw Daler! \u2013 Wer b\u00fctt wider?\u00ab", "tokens": ["Fiw", "Da\u00b7ler", "!", "\u2013", "Wer", "b\u00fctt", "wi\u00b7der", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "$(", "PWS", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "\u00bbun noch acht Gr\u00f6schen\u00ab, s\u00e4d de Snider.", "tokens": ["\u00bb", "un", "noch", "acht", "Gr\u00f6\u00b7schen", "\u00ab", ",", "s\u00e4d", "de", "Sni\u00b7der", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADV", "CARD", "NN", "$(", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "De Herr Burmeister denkt, hei hett sick man verh\u00fcrt;", "tokens": ["De", "Herr", "Burm\u00b7eis\u00b7ter", "denkt", ",", "hei", "hett", "sick", "man", "ver\u00b7h\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "$,", "FM", "FM", "FM", "PIS", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Ganz argerlich, dat ein em st\u00fcrt,", "tokens": ["Ganz", "ar\u00b7ger\u00b7lich", ",", "dat", "ein", "em", "st\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Schriggt hei noch drister as v\u00f6rher:", "tokens": ["Schriggt", "hei", "noch", "dris\u00b7ter", "as", "v\u00f6r\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbf\u00fcnf Taler! sag ich. Wer gibt mehr?\u00ab", "tokens": ["\u00bb", "f\u00fcnf", "Ta\u00b7ler", "!", "sag", "ich", ".", "Wer", "gibt", "mehr", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "CARD", "NN", "$.", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Un s\u00fch! de zackermentsche Snider", "tokens": ["Un", "s\u00fch", "!", "de", "za\u00b7cker\u00b7ment\u00b7sche", "Sni\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "B\u00fctt noch mit s\u00f6\u00dfteihn Gr\u00f6schen wider.", "tokens": ["B\u00fctt", "noch", "mit", "s\u00f6\u00df\u00b7teihn", "Gr\u00f6\u00b7schen", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "De Herr Burmeister richt't sick hoch in En'n,", "tokens": ["De", "Herr", "Burm\u00b7eis\u00b7ter", "richt't", "sick", "hoch", "in", "En'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "PRF", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Leggt \u00e4wer sine Ogen beide H\u00e4n'n,", "tokens": ["Leggt", "\u00e4\u00b7wer", "si\u00b7ne", "O\u00b7gen", "bei\u00b7de", "H\u00e4n'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Dormit hei beter kiken k\u00fcnn,", "tokens": ["Dor\u00b7mit", "hei", "be\u00b7ter", "ki\u00b7ken", "k\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Un kickt dorhen, wo unse Snider st\u00fcnn.", "tokens": ["Un", "kickt", "dor\u00b7hen", ",", "wo", "un\u00b7se", "Sni\u00b7der", "st\u00fcnn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Den Snider bewt dat Hart in sinen Liw,", "tokens": ["Den", "Sni\u00b7der", "bewt", "dat", "Hart", "in", "si\u00b7nen", "Liw", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "De Herr Burmeister b\u00fctt noch mal sin \u00bbfiw\u00ab,", "tokens": ["De", "Herr", "Burm\u00b7eis\u00b7ter", "b\u00fctt", "noch", "mal", "sin", "\u00bb", "fiw", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "ADV", "ADV", "VAFIN", "$(", "FM.la", "$(", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Gevadder Dreier st\u00f6tt den Snider an:", "tokens": ["Ge\u00b7vad\u00b7der", "Drei\u00b7er", "st\u00f6tt", "den", "Sni\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "\u00bblat di nich lumpen, Vaddermann!\u00ab", "tokens": ["\u00bb", "lat", "di", "nich", "lum\u00b7pen", ",", "Vad\u00b7der\u00b7mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NE", "PTKNEG", "VVINF", "$,", "NE", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.44": {"text": "\u00bbs\u00f6\u00df Daler\u00ab, r\u00f6ppt de Snider, \u00bbgew' ick Meid'!\u00ab", "tokens": ["\u00bb", "s\u00f6\u00df", "Da\u00b7ler", "\u00ab", ",", "r\u00f6ppt", "de", "Sni\u00b7der", ",", "\u00bb", "ge\u00b7w'", "ick", "Meid'", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "VVIMP", "PPER", "NE", "$.", "$("], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Doch uns' Burmeister, rasch entslaten,", "tokens": ["Doch", "un\u00b7s'", "Burm\u00b7eis\u00b7ter", ",", "rasch", "ents\u00b7la\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Will sick de Wisch nich nehmen laten:", "tokens": ["Will", "sick", "de", "Wisch", "nich", "neh\u00b7men", "la\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NE", "NE", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "\u00bbwat is denn dat f\u00f6r 'n d\u00e4mliches Gebeid'? \u2013", "tokens": ["\u00bb", "wat", "is", "denn", "dat", "f\u00f6r", "'", "n", "d\u00e4m\u00b7li\u00b7ches", "Ge\u00b7beid", "'", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "FM", "KON", "ART", "NE", "$(", "XY", "ADJA", "NN", "$(", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.48": {"text": "Fiw Daler! \u2013 Wat sall dat bed\u00fcden?", "tokens": ["Fiw", "Da\u00b7ler", "!", "\u2013", "Wat", "sall", "dat", "be\u00b7d\u00fc\u00b7den", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos