{"dta.poem.10918": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Marder und Wiesel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der so sch\u00f6n - als wilden Landschaft, sind kleine b\u00f6se\nB\u00fcrger hier,", "tokens": ["In", "der", "so", "sch\u00f6n", "als", "wil\u00b7den", "Land\u00b7schaft", ",", "sind", "klei\u00b7ne", "b\u00f6\u00b7se", "B\u00fcr\u00b7ger", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "$(", "KOUS", "ADJA", "NN", "$,", "VAFIN", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Von unterschiedner Art, zu sehn. So Stein-als Edel-Marder zeigen", "tokens": ["Von", "un\u00b7ter\u00b7schied\u00b7ner", "Art", ",", "zu", "sehn", ".", "So", "Stein\u00b7als", "E\u00b7del\u00b7Mar\u00b7der", "zei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$.", "ADV", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die r\u00e4uberische Fertigkeit, hier in den H\u00f6hlen, dort auf Zweigen,", "tokens": ["Die", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Fer\u00b7tig\u00b7keit", ",", "hier", "in", "den", "H\u00f6h\u00b7len", ",", "dort", "auf", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Sammt ihrer kleinen wilder Brust. Wie sch\u00e4dlich nun das", "tokens": ["Sammt", "ih\u00b7rer", "klei\u00b7nen", "wil\u00b7der", "Brust", ".", "Wie", "sch\u00e4d\u00b7lich", "nun", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$.", "PWAV", "ADJD", "ADV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So n\u00fctzt es uns dennoch nicht minder. Die B\u00e4lge decken uns", "tokens": ["So", "n\u00fctzt", "es", "uns", "den\u00b7noch", "nicht", "min\u00b7der", ".", "Die", "B\u00e4l\u00b7ge", "de\u00b7cken", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Die Losung riechet, recht wie Biesam, die M\u00e4use dienen ihm", "tokens": ["Die", "Lo\u00b7sung", "rie\u00b7chet", ",", "recht", "wie", "Bie\u00b7sam", ",", "die", "M\u00e4u\u00b7se", "die\u00b7nen", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KOKOM", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Auch schleicht die Wiesel hier herum, die auch, mit ihren", "tokens": ["Auch", "schleicht", "die", "Wie\u00b7sel", "hier", "he\u00b7rum", ",", "die", "auch", ",", "mit", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "PRELS", "ADV", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht sonderlich beliebt sich macht, je dennoch ist von ihr zu merken,", "tokens": ["Nicht", "son\u00b7der\u00b7lich", "be\u00b7liebt", "sich", "macht", ",", "je", "den\u00b7noch", "ist", "von", "ihr", "zu", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PRF", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-++--++--+-", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Da\u00df sie, nicht nur der M\u00e4us und Ratzen, sie ist zugleich der", "tokens": ["Da\u00df", "sie", ",", "nicht", "nur", "der", "M\u00e4us", "und", "Rat\u00b7zen", ",", "sie", "ist", "zu\u00b7gleich", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PTKNEG", "ADV", "ART", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Jhr Balg heilt selber ihren Bi\u00df, der giftig seyn soll, und man", "tokens": ["Ihr", "Balg", "heilt", "sel\u00b7ber", "ih\u00b7ren", "Bi\u00df", ",", "der", "gif\u00b7tig", "seyn", "soll", ",", "und", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VAINF", "VMFIN", "$,", "KON", "PIS"], "meter": "-+-+-+-+-+-+-++", "measure": "unknown.measure.octa.plus"}, "line.11": {"text": "Ob heil ihr Blut die b\u00f6se Sucht. Hieraus nun wird aufs", "tokens": ["Ob", "heil", "ihr", "Blut", "die", "b\u00f6\u00b7se", "Sucht", ".", "Hier\u00b7aus", "nun", "wird", "aufs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN", "$.", "PAV", "ADV", "VAFIN", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie auch in Thieren, welche sch\u00e4dlich, ein noch viel gr\u00f6\u00dfrer", "tokens": ["Wie", "auch", "in", "Thie\u00b7ren", ",", "wel\u00b7che", "sch\u00e4d\u00b7lich", ",", "ein", "noch", "viel", "gr\u00f6\u00df\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "PRELS", "ADJD", "$,", "ART", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}