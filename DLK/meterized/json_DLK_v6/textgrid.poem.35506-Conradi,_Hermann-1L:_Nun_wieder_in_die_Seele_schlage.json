{"textgrid.poem.35506": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wieder in die Seele schlage", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wieder in die Seele schlage", "tokens": ["Nun", "wie\u00b7der", "in", "die", "See\u00b7le", "schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem Zauber, Fr\u00fchling, ein!", "tokens": ["Mit", "dei\u00b7nem", "Zau\u00b7ber", ",", "Fr\u00fch\u00b7ling", ",", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lichtflut deiner Sonnentage", "tokens": ["Die", "Licht\u00b7flut", "dei\u00b7ner", "Son\u00b7nen\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll sie verj\u00fcngen, soll sie weihn! ...", "tokens": ["Soll", "sie", "ver\u00b7j\u00fcn\u00b7gen", ",", "soll", "sie", "weihn", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich gedacht in D\u00e4mmerungen,", "tokens": ["Was", "ich", "ge\u00b7dacht", "in", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich getr\u00e4umt in Nebelgrau:", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", "in", "Ne\u00b7bel\u00b7grau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Sonnenband umschlungen", "tokens": ["Von", "dei\u00b7nem", "Son\u00b7nen\u00b7band", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4r' es sich zu leuchtend Blau!", "tokens": ["Ver\u00b7kl\u00e4r'", "es", "sich", "zu", "leuch\u00b7tend", "Blau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umhallt von deiner S\u00e4nger Zunge,", "tokens": ["Um\u00b7hallt", "von", "dei\u00b7ner", "S\u00e4n\u00b7ger", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinem Farbenspiel umgl\u00e4nzt:", "tokens": ["Von", "dei\u00b7nem", "Far\u00b7ben\u00b7spiel", "um\u00b7gl\u00e4nzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich mich freu'n wie ein Bauernjunge,", "tokens": ["Will", "ich", "mich", "freu'n", "wie", "ein", "Bau\u00b7ern\u00b7jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der seine finstre Schule schw\u00e4nzt! \u2013", "tokens": ["Der", "sei\u00b7ne", "finst\u00b7re", "Schu\u00b7le", "schw\u00e4nzt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}