{"textgrid.poem.44236": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[man strafte nechsten Tag den jungen Prediger]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man strafte nechsten Tag den jungen Prediger,", "tokens": ["Man", "straf\u00b7te", "nechs\u00b7ten", "Tag", "den", "jun\u00b7gen", "Pre\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der vor ein fettes Amt viel Beuthel hingeschmi\u00dfen.", "tokens": ["Der", "vor", "ein", "fet\u00b7tes", "Amt", "viel", "Beut\u00b7hel", "hin\u00b7ge\u00b7schmi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er aber sprach: Die Schrift beruhigt mein Gewi\u00dfen.", "tokens": ["Er", "a\u00b7ber", "sprach", ":", "Die", "Schrift", "be\u00b7ru\u00b7higt", "mein", "Ge\u00b7wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn kauft des H\u00f6chsten Sohn, mein Meister und mein Herr,", "tokens": ["Denn", "kauft", "des", "H\u00f6chs\u00b7ten", "Sohn", ",", "mein", "Meis\u00b7ter", "und", "mein", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein armes Hirtendienst vor Marter, Blut und Leben,", "tokens": ["Sein", "ar\u00b7mes", "Hir\u00b7ten\u00b7dienst", "vor", "Mar\u00b7ter", ",", "Blut", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan ich wohl vor dies den kahlen Mammon geben.", "tokens": ["So", "kan", "ich", "wohl", "vor", "dies", "den", "kah\u00b7len", "Mam\u00b7mon", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PDS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}