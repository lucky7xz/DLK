{"textgrid.poem.48267": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gulbrandsdal", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Felsen sind steil, die Schlucht ist schmal,", "tokens": ["Die", "Fel\u00b7sen", "sind", "steil", ",", "die", "Schlucht", "ist", "schmal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Sn\u00f6h\u00e4ttan blickt auf Gulbrandsdal.", "tokens": ["Der", "Sn\u00f6\u00b7h\u00e4t\u00b7tan", "blickt", "auf", "Gulb\u00b7rands\u00b7dal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und weht es im Sommer hei\u00df und schw\u00fcl,", "tokens": ["Und", "weht", "es", "im", "Som\u00b7mer", "hei\u00df", "und", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So halten die Felsen den Talgrund k\u00fchl,", "tokens": ["So", "hal\u00b7ten", "die", "Fel\u00b7sen", "den", "Tal\u00b7grund", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und starrt es im Winter hoch oben von Eis,", "tokens": ["Und", "starrt", "es", "im", "Win\u00b7ter", "hoch", "o\u00b7ben", "von", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So sprudeln unten die Quellen hei\u00df,", "tokens": ["So", "spru\u00b7deln", "un\u00b7ten", "die", "Quel\u00b7len", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Herbstst\u00fcrme ziehen dr\u00fcber hin,", "tokens": ["Herbsts\u00b7t\u00fcr\u00b7me", "zie\u00b7hen", "dr\u00fc\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Fr\u00fchling und Friede wohnen darin,", "tokens": ["Nur", "Fr\u00fch\u00b7ling", "und", "Frie\u00b7de", "woh\u00b7nen", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Kein Fieber schleicht, keine Krankheit geht um,", "tokens": ["Kein", "Fie\u00b7ber", "schleicht", ",", "kei\u00b7ne", "Krank\u00b7heit", "geht", "um", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u00bbtal des Lebens\u00ab hei\u00dft es drum.", "tokens": ["\u00bb", "tal", "des", "Le\u00b7bens", "\u00ab", "hei\u00dft", "es", "drum", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$(", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Menschen im Tal verlassen es nie,", "tokens": ["Und", "die", "Men\u00b7schen", "im", "Tal", "ver\u00b7las\u00b7sen", "es", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu hohen Jahren kommen sie,", "tokens": ["Zu", "ho\u00b7hen", "Jah\u00b7ren", "kom\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Keine Last, keine Sorge beugt ihre Gestalt,", "tokens": ["Kei\u00b7ne", "Last", ",", "kei\u00b7ne", "Sor\u00b7ge", "beugt", "ih\u00b7re", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sie werden wei\u00df, aber sie werden nicht alt.", "tokens": ["Sie", "wer\u00b7den", "wei\u00df", ",", "a\u00b7ber", "sie", "wer\u00b7den", "nicht", "alt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und drei Lebelang sehen dem Leben sie zu,", "tokens": ["Und", "drei", "Le\u00b7be\u00b7lang", "se\u00b7hen", "dem", "Le\u00b7ben", "sie", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sind sie m\u00fcd' und verlangen nach Ruh',", "tokens": ["Da", "sind", "sie", "m\u00fcd'", "und", "ver\u00b7lan\u00b7gen", "nach", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sie rufen den Tod, der aber spricht:", "tokens": ["Und", "sie", "ru\u00b7fen", "den", "Tod", ",", "der", "a\u00b7ber", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.5": {"line.1": {"text": "Und sie steigen hinauf. Und zum ersten Mal,", "tokens": ["Und", "sie", "stei\u00b7gen", "hin\u00b7auf", ".", "Und", "zum", "ers\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Um zu ", "tokens": ["Um", "zu"], "token_info": ["word", "word"], "pos": ["KOUI", "PTKZU"], "meter": "+-", "measure": "trochaic.single"}}}}}