{"textgrid.poem.36529": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Die Milchfrau", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf leichten F\u00fc\u00dfen lief ein artig Bauerweib,", "tokens": ["Auf", "leich\u00b7ten", "F\u00fc\u00b7\u00dfen", "lief", "ein", "ar\u00b7tig", "Bau\u00b7er\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geliebt von ihrem Mann, gesund an Seel und Leib,", "tokens": ["Ge\u00b7liebt", "von", "ih\u00b7rem", "Mann", ",", "ge\u00b7sund", "an", "Seel", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fr\u00fchmorgens in die Stadt, und trug auf ihrem Kopf", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "in", "die", "Stadt", ",", "und", "trug", "auf", "ih\u00b7rem", "Kopf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Vier St\u00fcbchen s\u00fc\u00dfe Milch, in einem gro\u00dfen Topfe;", "tokens": ["Vier", "St\u00fcb\u00b7chen", "s\u00fc\u00b7\u00dfe", "Milch", ",", "in", "ei\u00b7nem", "gro\u00b7\u00dfen", "Top\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lief, wollte gar zu gern: Kauft Milch! am ersten schrein;", "tokens": ["Lief", ",", "woll\u00b7te", "gar", "zu", "gern", ":", "Kauft", "Milch", "!", "am", "ers\u00b7ten", "schrein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "ADV", "PTKA", "ADV", "$.", "NN", "NN", "$.", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die erste, dachte sie, die erste Milch ist teuer;", "tokens": ["Die", "ers\u00b7te", ",", "dach\u00b7te", "sie", ",", "die", "ers\u00b7te", "Milch", "ist", "teu\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will's Gott, so nehm' ich heut sechs baare Groschen ein!", "tokens": ["Will's", "Gott", ",", "so", "nehm'", "ich", "heut", "sechs", "baa\u00b7re", "Gro\u00b7schen", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Daf\u00fcr kauf ich mir dann ein halbes Hundert Eier;", "tokens": ["Da\u00b7f\u00fcr", "kauf", "ich", "mir", "dann", "ein", "hal\u00b7bes", "Hun\u00b7dert", "Ei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Mein H\u00fchnchen br\u00fctet sie mir all' auf einmal aus;", "tokens": ["Mein", "H\u00fchn\u00b7chen", "br\u00fc\u00b7tet", "sie", "mir", "all'", "auf", "ein\u00b7mal", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PIS", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gras, eine Menge, steht um unser kleines Haus;", "tokens": ["Gras", ",", "ei\u00b7ne", "Men\u00b7ge", ",", "steht", "um", "un\u00b7ser", "klei\u00b7nes", "Haus", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.11": {"text": "Die kleinen K\u00fcchelchen, die meine Stimme h\u00f6ren,", "tokens": ["Die", "klei\u00b7nen", "K\u00fc\u00b7chel\u00b7chen", ",", "die", "mei\u00b7ne", "Stim\u00b7me", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die werden herrlich da sich letzen, und sich n\u00e4hren;", "tokens": ["Die", "wer\u00b7den", "herr\u00b7lich", "da", "sich", "let\u00b7zen", ",", "und", "sich", "n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADV", "PRF", "VVINF", "$,", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und, ganz gewi\u00df! der Fuchs, der m\u00fc\u00dfte listig sein,", "tokens": ["Und", ",", "ganz", "ge\u00b7wi\u00df", "!", "der", "Fuchs", ",", "der", "m\u00fc\u00df\u00b7te", "lis\u00b7tig", "sein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "$.", "ART", "NE", "$,", "PRELS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Lie\u00df er mir nicht so viel, da\u00df ich ein kleines Schwein", "tokens": ["Lie\u00df", "er", "mir", "nicht", "so", "viel", ",", "da\u00df", "ich", "ein", "klei\u00b7nes", "Schwein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Daf\u00fcr ertauschen k\u00f6nnte! Seht nur an.", "tokens": ["Da\u00b7f\u00fcr", "er\u00b7tau\u00b7schen", "k\u00f6nn\u00b7te", "!", "Seht", "nur", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "$.", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wenn ich mich etwa schon darauf im Geiste freue,", "tokens": ["Wenn", "ich", "mich", "et\u00b7wa", "schon", "da\u00b7rauf", "im", "Geis\u00b7te", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "PAV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So denk' ich nur dabei an meinen lieben Mann!", "tokens": ["So", "denk'", "ich", "nur", "da\u00b7bei", "an", "mei\u00b7nen", "lie\u00b7ben", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zu m\u00e4sten kostet's mir ja nur ein wenig Kleie!", "tokens": ["Zu", "m\u00e4s\u00b7ten", "kos\u00b7tet's", "mir", "ja", "nur", "ein", "we\u00b7nig", "Klei\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "Hab' ich das Schweinchen fett, dann kauf ich eine Kuh", "tokens": ["Hab'", "ich", "das", "Schwein\u00b7chen", "fett", ",", "dann", "kauf", "ich", "ei\u00b7ne", "Kuh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In meinen kleinen Stall, ein K\u00e4lbchen wohl dazu;", "tokens": ["In", "mei\u00b7nen", "klei\u00b7nen", "Stall", ",", "ein", "K\u00e4lb\u00b7chen", "wohl", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das K\u00e4lbchen will ich dann auf meine Weide bringen,", "tokens": ["Das", "K\u00e4lb\u00b7chen", "will", "ich", "dann", "auf", "mei\u00b7ne", "Wei\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und munter h\u00fcpft's und springt's, wie da die L\u00e4mmer springen!", "tokens": ["Und", "mun\u00b7ter", "h\u00fcpft's", "und", "springt's", ",", "wie", "da", "die", "L\u00e4m\u00b7mer", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hei! sagt sie, und springt auf! Und von dem Kopfe f\u00e4llt", "tokens": ["Hei", "!", "sagt", "sie", ",", "und", "springt", "auf", "!", "Und", "von", "dem", "Kop\u00b7fe", "f\u00e4llt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Topf; das baare Geld,", "tokens": ["Der", "Topf", ";", "das", "baa\u00b7re", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Kalb und Kuh und Reichtum und Vergn\u00fcgen", "tokens": ["Und", "Kalb", "und", "Kuh", "und", "Reich\u00b7tum", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieht nun das arme Weib vor sich in Scherben liegen!", "tokens": ["Sieht", "nun", "das", "ar\u00b7me", "Weib", "vor", "sich", "in", "Scher\u00b7ben", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erschrocken bleibt sie stehn, und sieht die Scherben an.", "tokens": ["Er\u00b7schro\u00b7cken", "bleibt", "sie", "stehn", ",", "und", "sieht", "die", "Scher\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sch\u00f6ne wei\u00dfe Milch, sagt sie, auf schwarzer Erde!", "tokens": ["Die", "sch\u00f6\u00b7ne", "wei\u00b7\u00dfe", "Milch", ",", "sagt", "sie", ",", "auf", "schwar\u00b7zer", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weint, geht nach Haus, erz\u00e4hlt's dem lieben Mann,", "tokens": ["Weint", ",", "geht", "nach", "Haus", ",", "er\u00b7z\u00e4hlt's", "dem", "lie\u00b7ben", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der ihr entgegen kommt, mit ernstlicher Geberde.", "tokens": ["Der", "ihr", "ent\u00b7ge\u00b7gen", "kommt", ",", "mit", "ernst\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kind, sagt der Mann, schon gut! Bau' nur ein andermal", "tokens": ["Kind", ",", "sagt", "der", "Mann", ",", "schon", "gut", "!", "Bau'", "nur", "ein", "an\u00b7der\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$.", "NE", "ADV", "ART", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht Schl\u00f6sser in die Luft! Man bauet seine Qual!", "tokens": ["Nicht", "Schl\u00f6s\u00b7ser", "in", "die", "Luft", "!", "Man", "bau\u00b7et", "sei\u00b7ne", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "ART", "NN", "$.", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geschwinder drehet sich um sich kein Wagenrad,", "tokens": ["Ge\u00b7schwin\u00b7der", "dre\u00b7het", "sich", "um", "sich", "kein", "Wa\u00b7gen\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Als sie verschwinden in den Wind!", "tokens": ["Als", "sie", "ver\u00b7schwin\u00b7den", "in", "den", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wir haben all' das Gl\u00fcck, das unser Junker hat,", "tokens": ["Wir", "ha\u00b7ben", "all'", "das", "Gl\u00fcck", ",", "das", "un\u00b7ser", "Jun\u00b7ker", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn wir zufrieden sind!", "tokens": ["Wenn", "wir", "zu\u00b7frie\u00b7den", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}