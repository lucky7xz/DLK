{"textgrid.poem.25891": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[stille weht in das Haus]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille weht in das Haus,", "tokens": ["Stil\u00b7le", "weht", "in", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "F\u00fchlst du den Atem des Mondes,", "tokens": ["F\u00fchlst", "du", "den", "A\u00b7tem", "des", "Mon\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "L\u00f6se dein Haar,", "tokens": ["L\u00f6\u00b7se", "dein", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Lege dein Haupt in den Blauschein hinaus.", "tokens": ["Le\u00b7ge", "dein", "Haupt", "in", "den", "Blau\u00b7schein", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "H\u00f6rst du, das Meer unten am Strand", "tokens": ["H\u00f6rst", "du", ",", "das", "Meer", "un\u00b7ten", "am", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Wirft dir Sch\u00e4tze ans Land;", "tokens": ["Wirft", "dir", "Sch\u00e4t\u00b7ze", "ans", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sonst wuchsen im Mond W\u00fcnsche, ein Heer,", "tokens": ["Sonst", "wuch\u00b7sen", "im", "Mond", "W\u00fcn\u00b7sche", ",", "ein", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Seit ich dein Auge gesehn, ist die Mondnacht wunschleer.", "tokens": ["Seit", "ich", "dein", "Au\u00b7ge", "ge\u00b7sehn", ",", "ist", "die", "Mond\u00b7nacht", "wun\u00b7schleer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}