{"dta.poem.6119": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die so d\u00fcnne Lufft. Nun diese Fertigkeit", "tokens": ["Durch", "die", "so", "d\u00fcn\u00b7ne", "Lufft", ".", "Nun", "die\u00b7se", "Fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$.", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich zu bewegen, wird zur Leichtigkeit. So weichet,", "tokens": ["Sich", "zu", "be\u00b7we\u00b7gen", ",", "wird", "zur", "Leich\u00b7tig\u00b7keit", ".", "So", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "VAFIN", "APPRART", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man sie Stuffenweis vergleichet,", "tokens": ["Wenn", "man", "sie", "Stuf\u00b7fen\u00b7weis", "ver\u00b7glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich die Materie einander allezeit.", "tokens": ["Sich", "die", "Ma\u00b7te\u00b7rie", "ein\u00b7an\u00b7der", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Ob gleich die Lufft sich schwer und langsam nur beweget,", "tokens": ["Ob", "gleich", "die", "Lufft", "sich", "schwer", "und", "lang\u00b7sam", "nur", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vergleichet man sie mit der Himmels-Lufft;", "tokens": ["Ver\u00b7glei\u00b7chet", "man", "sie", "mit", "der", "Him\u00b7mels\u00b7Lufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist sie doch bey der Fluht und Erd\u2019 ein leichter Dufft.", "tokens": ["Ist", "sie", "doch", "bey", "der", "Fluht", "und", "Erd'", "ein", "leich\u00b7ter", "Dufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach seiner Schwere, die er heget,", "tokens": ["Nach", "sei\u00b7ner", "Schwe\u00b7re", ",", "die", "er", "he\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Senckt jeder C\u00f6rper sich. Diejenigen, so mehr", "tokens": ["Senckt", "je\u00b7der", "C\u00f6r\u00b7per", "sich", ".", "Die\u00b7je\u00b7ni\u00b7gen", ",", "so", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "$.", "PDS", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verwickelt und vereint, auch nicht so schnelle gehn,", "tokens": ["Ver\u00b7wi\u00b7ckelt", "und", "ver\u00b7eint", ",", "auch", "nicht", "so", "schnel\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "ADV", "PTKNEG", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind, da die andern sie starck drucken, anzusehn,", "tokens": ["Sind", ",", "da", "die", "an\u00b7dern", "sie", "starck", "dru\u00b7cken", ",", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "ADJA", "PPER", "ADJD", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als w\u00e4ren sie nach ihrem Wesen schwer.", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "nach", "ih\u00b7rem", "We\u00b7sen", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sie sincken unterwerts. Die\u00df aber nun geschicht", "tokens": ["Sie", "sin\u00b7cken", "un\u00b7ter\u00b7werts", ".", "Die\u00df", "a\u00b7ber", "nun", "ge\u00b7schicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PDS", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch eine Gegen-Last und schwereres Gewicht.", "tokens": ["Durch", "ei\u00b7ne", "Ge\u00b7gen\u00b7Last", "und", "schwe\u00b7re\u00b7res", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein C\u00f6rper sinckt, wenn sich ein andrer C\u00f6rper hebet;", "tokens": ["Ein", "C\u00f6r\u00b7per", "sinckt", ",", "wenn", "sich", "ein", "an\u00b7drer", "C\u00f6r\u00b7per", "he\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es nehmen einige der andern Stellen ein.", "tokens": ["Es", "neh\u00b7men", "ei\u00b7ni\u00b7ge", "der", "an\u00b7dern", "Stel\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es treibet die Figur den Lauff,", "tokens": ["Es", "trei\u00b7bet", "die", "Fi\u00b7gur", "den", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Es h\u00e4lt auch die Figur ihn auf.", "tokens": ["Es", "h\u00e4lt", "auch", "die", "Fi\u00b7gur", "ihn", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Man sieht den Unterscheid, nachdem sie dichte seyn.", "tokens": ["Man", "sieht", "den", "Un\u00b7ter\u00b7scheid", ",", "nach\u00b7dem", "sie", "dich\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus einer eintzigen Bewegung fleusst", "tokens": ["Aus", "ei\u00b7ner", "eint\u00b7zi\u00b7gen", "Be\u00b7we\u00b7gung", "fleusst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die dopple W\u00fcrckung her, weswegen man", "tokens": ["Die", "dopp\u00b7le", "W\u00fcr\u00b7ckung", "her", ",", "wes\u00b7we\u00b7gen", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die C\u00f6rper schwer und leichte heist.", "tokens": ["Die", "C\u00f6r\u00b7per", "schwer", "und", "leich\u00b7te", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}