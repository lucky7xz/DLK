{"textgrid.poem.60861": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Canossa", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo im Palaste den Papst herbergte die stolze Mathildis,", "tokens": ["Wo", "im", "Pa\u00b7las\u00b7te", "den", "Papst", "her\u00b7berg\u00b7te", "die", "stol\u00b7ze", "Ma\u00b7thil\u00b7dis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+----+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Konnte mir kein Obdach bieten der Pfarrer des Orts,", "tokens": ["Konn\u00b7te", "mir", "kein", "Ob\u00b7dach", "bie\u00b7ten", "der", "Pfar\u00b7rer", "des", "Orts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Welcher am Fu\u00df des zertr\u00fcmmerten Schlosses in \u00e4rmlicher H\u00fctte", "tokens": ["Wel\u00b7cher", "am", "Fu\u00df", "des", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "Schlos\u00b7ses", "in", "\u00e4rm\u00b7li\u00b7cher", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPRART", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Haust; doch bot er ein Glas herben lombardischen Weins.", "tokens": ["Haust", ";", "doch", "bot", "er", "ein", "Glas", "her\u00b7ben", "lom\u00b7bar\u00b7di\u00b7schen", "Weins", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "VVPP", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "So denn mu\u00dft ich die neblige Nacht durchfrieren, wie Heinrich,", "tokens": ["So", "denn", "mu\u00dft", "ich", "die", "neb\u00b7li\u00b7ge", "Nacht", "durch\u00b7frie\u00b7ren", ",", "wie", "Hein\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Mit der Laterne den Pfad suchen im steilen Geb\u00fcrg.", "tokens": ["Mit", "der", "La\u00b7ter\u00b7ne", "den", "Pfad", "su\u00b7chen", "im", "stei\u00b7len", "Ge\u00b7b\u00fcrg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}