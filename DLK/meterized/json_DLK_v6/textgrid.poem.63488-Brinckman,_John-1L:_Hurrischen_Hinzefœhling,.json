{"textgrid.poem.63488": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hurrischen Hinzef\u0153hling,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hurrischen Hinzef\u0153hling,", "tokens": ["Hur\u00b7ri\u00b7schen", "Hin\u00b7ze\u00b7f\u0153h\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "wat b\u00fcst du schier un glatt!", "tokens": ["wat", "b\u00fcst", "du", "schier", "un", "glatt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dor wa\u00dft woll sch\u00f6nen Klewer", "tokens": ["Dor", "wa\u00dft", "woll", "sch\u00f6\u00b7nen", "Kle\u00b7wer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hier in de Koppel, wat?", "tokens": ["hier", "in", "de", "Kop\u00b7pel", ",", "wat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$,", "PWS", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hurrischen Hinzef\u0153hling,", "tokens": ["Hur\u00b7ri\u00b7schen", "Hin\u00b7ze\u00b7f\u0153h\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "wur dull sleihst achter ut,", "tokens": ["wur", "dull", "sleihst", "ach\u00b7ter", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wat b\u00fcst so kr\u0153nsch un w\u00e4hlig!", "tokens": ["wat", "b\u00fcst", "so", "kr\u0153nsch", "un", "w\u00e4h\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "FM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du dr\u00f6ppst noch gor min Snut.", "tokens": ["Du", "dr\u00f6ppst", "noch", "gor", "min", "Snut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hurrischen Hinzef\u0153hling,", "tokens": ["Hur\u00b7ri\u00b7schen", "Hin\u00b7ze\u00b7f\u0153h\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "du springst so dull un blind", "tokens": ["du", "springst", "so", "dull", "un", "blind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "FM", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "jo \u0153wer Schleet un Graben furt", "tokens": ["jo", "\u0153wer", "Schleet", "un", "Gra\u00b7ben", "furt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "so lichting as de Wind.", "tokens": ["so", "lich\u00b7ting", "as", "de", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hurrischen Hinzef\u0153hling,", "tokens": ["Hur\u00b7ri\u00b7schen", "Hin\u00b7ze\u00b7f\u0153h\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "man sacht, wu bald, wu bald,", "tokens": ["man", "sacht", ",", "wu", "bald", ",", "wu", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "s\u00fch, denn k\u00fcmmt Harwst un Winter,", "tokens": ["s\u00fch", ",", "denn", "k\u00fcmmt", "Harwst", "un", "Win\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "NE", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "s\u00fch, denn warst up du stallt.", "tokens": ["s\u00fch", ",", "denn", "warst", "up", "du", "stallt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VAFIN", "NE", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Denn kamt se mit de \u00c4gen,", "tokens": ["Denn", "kamt", "se", "mit", "de", "\u00c4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "denn kamens mit den Plog,", "tokens": ["denn", "ka\u00b7mens", "mit", "den", "Plog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Pitsch un Tom un S\u00e4len,", "tokens": ["mit", "Pitsch", "un", "Tom", "un", "S\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "denn warst du fram genog.", "tokens": ["denn", "warst", "du", "fram", "ge\u00b7nog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn m\u00f6\u00dft du d\u00fcchtig slawen,", "tokens": ["Denn", "m\u00f6\u00dft", "du", "d\u00fcch\u00b7tig", "sla\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "denn gifft'n str\u00e4ken Fatt,", "tokens": ["denn", "gifft'n", "str\u00e4\u00b7ken", "Fatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "denn kamen Gall un Schiebel,", "tokens": ["denn", "ka\u00b7men", "Gall", "un", "Schie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "denn kamen Kropp un Spatt.", "tokens": ["denn", "ka\u00b7men", "Kropp", "un", "Spatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}