{"dta.poem.12631": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  \n  Die Helden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Aerzte Haupt, die sich zu Pferde zeigen,", "tokens": ["Der", "A\u00b7erz\u00b7te", "Haupt", ",", "die", "sich", "zu", "Pfer\u00b7de", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Chiron sprach zum durstigen Achill:", "tokens": ["Ein", "Chi\u00b7ron", "sprach", "zum", "durs\u00b7ti\u00b7gen", "A\u00b7chill", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Thetis sey das Wasser-Trinken eigen;", "tokens": ["Der", "The\u00b7tis", "sey", "das", "Was\u00b7ser\u00b7Trin\u00b7ken", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jhr Sohn trinkt Wein, wenn er mir folgen will.", "tokens": ["Ihr", "Sohn", "trinkt", "Wein", ",", "wenn", "er", "mir", "fol\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jhm folgt\u2019 Achill und leerte ganze Schl\u00e4uche", "tokens": ["Jhm", "folgt'", "A\u00b7chill", "und", "leer\u00b7te", "gan\u00b7ze", "Schl\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Br\u00fcderschaft mit andern Helden aus.", "tokens": ["Auf", "Br\u00fc\u00b7der\u00b7schaft", "mit", "an\u00b7dern", "Hel\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geweihter Wein flo\u00df auf Patroclus Leiche,", "tokens": ["Ge\u00b7weih\u00b7ter", "Wein", "flo\u00df", "auf", "Pat\u00b7ro\u00b7clus", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch bessrer Wein flo\u00df beym Begr\u00e4bni\u00df-Schmau\u00df.", "tokens": ["Noch", "bess\u00b7rer", "Wein", "flo\u00df", "beym", "Be\u00b7gr\u00e4b\u00b7ni\u00df\u00b7Schmau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "War Calchas nicht ein hocherfahrner Zecher", "tokens": ["War", "Cal\u00b7chas", "nicht", "ein", "ho\u00b7cher\u00b7fahr\u00b7ner", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, halb berauscht, ein Held im Prophezeyn?", "tokens": ["Und", ",", "halb", "be\u00b7rauscht", ",", "ein", "Held", "im", "Pro\u00b7phe\u00b7zeyn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er trank, er rieth, er weissagt\u2019 aus dem Becher", "tokens": ["Er", "trank", ",", "er", "rieth", ",", "er", "weis\u00b7sagt'", "aus", "dem", "Be\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fand, wie wir, die Wahrheit in dem Wein.", "tokens": ["Und", "fand", ",", "wie", "wir", ",", "die", "Wahr\u00b7heit", "in", "dem", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was that Uly\u00df, der durch ein Abentheuer,", "tokens": ["Was", "that", "U\u00b7ly\u00df", ",", "der", "durch", "ein", "A\u00b7bent\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Alcinous! zu deinem Jahr-Schmau\u00df kam?", "tokens": ["Al\u00b7ci\u00b7nous", "!", "zu", "dei\u00b7nem", "Jahr\u00b7Schmau\u00df", "kam", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der weise Mann erw\u00e4rmte sich am Feuer,", "tokens": ["Der", "wei\u00b7se", "Mann", "er\u00b7w\u00e4rm\u00b7te", "sich", "am", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis man auch ihn an deine Tafel nahm.", "tokens": ["Bis", "man", "auch", "ihn", "an", "dei\u00b7ne", "Ta\u00b7fel", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als Telemach, den Vater aufzusuchen,", "tokens": ["Als", "Te\u00b7le\u00b7mach", ",", "den", "Va\u00b7ter", "auf\u00b7zu\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Nestor kam, und diesen r\u00e4uchern sah,", "tokens": ["Zum", "Nes\u00b7tor", "kam", ",", "und", "die\u00b7sen", "r\u00e4u\u00b7chern", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "PDS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sprach Pylos F\u00fcrst: Trinkt zu den Opfer-Kuchen", "tokens": ["Sprach", "Py\u00b7los", "F\u00fcrst", ":", "Trinkt", "zu", "den", "Op\u00b7fer\u00b7Ku\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "NN", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Priester-Wein, aufs Wohl von Jthaka!", "tokens": ["Den", "Pries\u00b7ter\u00b7Wein", ",", "aufs", "Wohl", "von", "Jtha\u00b7ka", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kaum hatt\u2019 er sich nach Sparta hinbegeben,", "tokens": ["Kaum", "hatt'", "er", "sich", "nach", "Spar\u00b7ta", "hin\u00b7be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So redte dort ihn Menelaus an:", "tokens": ["So", "red\u00b7te", "dort", "ihn", "Me\u00b7ne\u00b7laus", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Willkommen, Prinz! versucht von unsern Reben:", "tokens": ["Will\u00b7kom\u00b7men", ",", "Prinz", "!", "ver\u00b7sucht", "von", "un\u00b7sern", "Re\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herrscht v\u00e4terlich und trinkt als ein Tyrann.", "tokens": ["Herrscht", "v\u00e4\u00b7ter\u00b7lich", "und", "trinkt", "als", "ein", "Ty\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Minerva rieth mit warnenden Geberden", "tokens": ["Mi\u00b7ner\u00b7va", "rieth", "mit", "war\u00b7nen\u00b7den", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem Telemach die wilde Trinksucht ab,", "tokens": ["Dem", "Te\u00b7le\u00b7mach", "die", "wil\u00b7de", "Trink\u00b7sucht", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und trank doch selbst, um nicht erkannt zu werden,", "tokens": ["Und", "trank", "doch", "selbst", ",", "um", "nicht", "er\u00b7kannt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUI", "PTKNEG", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stutzer aus, die ihr Atrides gab.", "tokens": ["Die", "Stut\u00b7zer", "aus", ",", "die", "ihr", "At\u00b7ri\u00b7des", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Cambyses dankt und opfert dir, o Sonne!", "tokens": ["Cam\u00b7by\u00b7ses", "dankt", "und", "op\u00b7fert", "dir", ",", "o", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht, weil dein Lauf durch Stier und Wage streift;", "tokens": ["Nicht", ",", "weil", "dein", "Lauf", "durch", "Stier", "und", "Wa\u00b7ge", "streift", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er nannte dich die Stifterinn der Wonne,", "tokens": ["Er", "nann\u00b7te", "dich", "die", "Stif\u00b7te\u00b7rinn", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur weil durch dich die edle Traube reift.", "tokens": ["Nur", "weil", "durch", "dich", "die", "ed\u00b7le", "Trau\u00b7be", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In Spanien blieb, bey der Liebe Winken,", "tokens": ["In", "Spa\u00b7ni\u00b7en", "blieb", ",", "bey", "der", "Lie\u00b7be", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Scipio dem s\u00fcssen Wein getreu,", "tokens": ["Ein", "Sci\u00b7pio", "dem", "s\u00fcs\u00b7sen", "Wein", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und gab gar bald, ihn ungest\u00f6rt zu trinken,", "tokens": ["Und", "gab", "gar", "bald", ",", "ihn", "un\u00b7ge\u00b7st\u00f6rt", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sch\u00f6nste Kind der Kriegs-Gefangnen frey.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Kind", "der", "Kriegs\u00b7Ge\u00b7fang\u00b7nen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Roms Phocion, das Muster alter Strenge,", "tokens": ["Roms", "Pho\u00b7ci\u00b7on", ",", "das", "Mus\u00b7ter", "al\u00b7ter", "Stren\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch Cato hat zu seinem Trunk gelacht.", "tokens": ["Auch", "Ca\u00b7to", "hat", "zu", "sei\u00b7nem", "Trunk", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er heiligte, bey der Gesch\u00e4ffte Menge,", "tokens": ["Er", "hei\u00b7lig\u00b7te", ",", "bey", "der", "Ge\u00b7sch\u00e4ff\u00b7te", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Tag dem Staat und seinem Wein die Nacht.", "tokens": ["Den", "Tag", "dem", "Staat", "und", "sei\u00b7nem", "Wein", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "F\u00fcrst Herrmann trank, wie deutsche Helden pflegen,", "tokens": ["F\u00fcrst", "Herr\u00b7mann", "trank", ",", "wie", "deut\u00b7sche", "Hel\u00b7den", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann Land und Hof und auch Thu\u00dfnelde schlief,", "tokens": ["Wann", "Land", "und", "Hof", "und", "auch", "Thu\u00df\u00b7nel\u00b7de", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Morgen-Stern aus seinem Helm entgegen,", "tokens": ["Dem", "Mor\u00b7gen\u00b7S\u00b7tern", "aus", "sei\u00b7nem", "Helm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh ihn der Tag in Feld und Lager rief.", "tokens": ["Eh", "ihn", "der", "Tag", "in", "Feld", "und", "La\u00b7ger", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Ritterschaft des Artus zu verbinden,", "tokens": ["Die", "Rit\u00b7ter\u00b7schaft", "des", "Ar\u00b7tus", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ersann er selbst Getr\u00e4nke voller Kraft;", "tokens": ["Er\u00b7sann", "er", "selbst", "Ge\u00b7tr\u00e4n\u00b7ke", "vol\u00b7ler", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00f6niginn, um gleichfalls zu erfinden,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ginn", ",", "um", "gleich\u00b7falls", "zu", "er\u00b7fin\u00b7den", ","], "token_info": ["w