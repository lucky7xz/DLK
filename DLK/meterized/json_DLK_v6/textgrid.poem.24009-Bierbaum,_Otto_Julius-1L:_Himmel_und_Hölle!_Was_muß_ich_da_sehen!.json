{"textgrid.poem.24009": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Himmel und H\u00f6lle! Was mu\u00df ich da sehen!", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel und H\u00f6lle! Was mu\u00df ich da sehen!", "tokens": ["Him\u00b7mel", "und", "H\u00f6l\u00b7le", "!", "Was", "mu\u00df", "ich", "da", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Meine kleine Pollichinelle,", "tokens": ["Mei\u00b7ne", "klei\u00b7ne", "Pol\u00b7li\u00b7chi\u00b7nel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u2013 Himmel und H\u00f6lle! \u2013", "tokens": ["\u2013", "Him\u00b7mel", "und", "H\u00f6l\u00b7le", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "KON", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hingeschmiegt in lustigem Drehen", "tokens": ["Hin\u00b7ge\u00b7schmiegt", "in", "lus\u00b7ti\u00b7gem", "Dre\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "An die breite Brust eines langen", "tokens": ["An", "die", "brei\u00b7te", "Brust", "ei\u00b7nes", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Russen, mit lauter Pistolen