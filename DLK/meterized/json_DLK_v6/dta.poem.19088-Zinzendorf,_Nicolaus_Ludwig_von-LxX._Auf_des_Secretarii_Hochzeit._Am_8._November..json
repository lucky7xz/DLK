{"dta.poem.19088": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxX.  Auf des  Secretarii  Hochzeit.   Am 8. November.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Geht hin zu GOtt, von dem ihr hergekommen,", "tokens": ["Geht", "hin", "zu", "Gott", ",", "von", "dem", "ihr", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Christi Tod und Leben eingenommen:", "tokens": ["In", "Chris\u00b7ti", "Tod", "und", "Le\u00b7ben", "ein\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht hin, versucht des H\u00fcters Meisterschaft,", "tokens": ["Geht", "hin", ",", "ver\u00b7sucht", "des", "H\u00fc\u00b7ters", "Meis\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geht ein und aus in Geist von Kraft zu Kraft!", "tokens": ["Geht", "ein", "und", "aus", "in", "Geist", "von", "Kraft", "zu", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "APPR", "APPR", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}