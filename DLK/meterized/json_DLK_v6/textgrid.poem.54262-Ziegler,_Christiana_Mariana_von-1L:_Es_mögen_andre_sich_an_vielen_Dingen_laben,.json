{"textgrid.poem.54262": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es m\u00f6gen andre sich an vielen Dingen laben,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00f6gen andre sich an vielen Dingen laben,", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "sich", "an", "vie\u00b7len", "Din\u00b7gen", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem sie ihr Plaisir und viel Vergn\u00fcgen haben;", "tokens": ["An", "dem", "sie", "ihr", "Plai\u00b7sir", "und", "viel", "Ver\u00b7gn\u00fc\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lobe mir davor mein kleines liebes Vieh,", "tokens": ["Ich", "lo\u00b7be", "mir", "da\u00b7vor", "mein", "klei\u00b7nes", "lie\u00b7bes", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich meyne meinen Hund, denn dieser heist: Plaisir.", "tokens": ["Ich", "mey\u00b7ne", "mei\u00b7nen", "Hund", ",", "denn", "die\u00b7ser", "heist", ":", "Plai\u00b7sir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PDS", "VAFIN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}