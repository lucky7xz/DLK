{"textgrid.poem.38041": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Reue", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seit mich die S\u00fcnd thut scheiden", "tokens": ["Seit", "mich", "die", "S\u00fcnd", "thut", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von dir mein Gott, du h\u00f6chste Freud,", "tokens": ["Von", "dir", "mein", "Gott", ",", "du", "h\u00f6chs\u00b7te", "Freud", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daf\u00fcr mu\u00df ich viel leiden.", "tokens": ["Da\u00b7f\u00fcr", "mu\u00df", "ich", "viel", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Leiden ist gro\u00df jeder Frist", "tokens": ["Mein", "Lei\u00b7den", "ist", "gro\u00df", "je\u00b7der", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird mir lang mit Schmerzen,", "tokens": ["Und", "wird", "mir", "lang", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich oft klag,", "tokens": ["Da\u00df", "ich", "oft", "klag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Es scheint kein Tag,", "tokens": ["Es", "scheint", "kein", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mein S\u00fcnd mich reut von Herzen.", "tokens": ["Mein", "S\u00fcnd", "mich", "reut", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}