{"textgrid.poem.25509": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ruhet aus, ihr stumpf gejagten F\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ruhet aus, ihr stumpf gejagten F\u00fc\u00dfe,", "tokens": ["Nun", "ru\u00b7het", "aus", ",", "ihr", "stumpf", "ge\u00b7jag\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis einst ich im verdienten Myrtenkranz'", "tokens": ["Bis", "einst", "ich", "im", "ver\u00b7dien\u00b7ten", "Myr\u00b7ten\u00b7kranz'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Br\u00e4utigam bei Fl\u00f6tenton umschlie\u00dfe:", "tokens": ["Den", "Br\u00e4u\u00b7ti\u00b7gam", "bei", "Fl\u00f6\u00b7ten\u00b7ton", "um\u00b7schlie\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinweg mit andern Tanz!", "tokens": ["Hin\u00b7weg", "mit", "an\u00b7dern", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Darfst du den Arm um meinen Nacken schlagen,", "tokens": ["Darfst", "du", "den", "Arm", "um", "mei\u00b7nen", "Na\u00b7cken", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du, den nur erst mein Auge heut' erblickt?", "tokens": ["Du", ",", "den", "nur", "erst", "mein", "Au\u00b7ge", "heut'", "er\u00b7blickt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und du die Hand mir hei\u00df zu dr\u00fccken wagen,", "tokens": ["Und", "du", "die", "Hand", "mir", "hei\u00df", "zu", "dr\u00fc\u00b7cken", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den meine Ann' entz\u00fcckt?", "tokens": ["Den", "mei\u00b7ne", "Ann'", "ent\u00b7z\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ha! kommt heraus aus dem berauschten Saale,", "tokens": ["Ha", "!", "kommt", "he\u00b7raus", "aus", "dem", "be\u00b7rauschten", "Saa\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Hieher mit Euch, wo Papchen Eurer lacht,", "tokens": ["Hie\u00b7her", "mit", "Euch", ",", "wo", "Pap\u00b7chen", "Eu\u00b7rer", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PWAV", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wo nicht der Pauken Wirbel, nicht die Schaale", "tokens": ["Wo", "nicht", "der", "Pau\u00b7ken", "Wir\u00b7bel", ",", "nicht", "die", "Schaa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Voll Rum, die Helden macht.", "tokens": ["Voll", "Rum", ",", "die", "Hel\u00b7den", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie sollt ihr da zur Erde sehn, ihr Herren!", "tokens": ["Wie", "sollt", "ihr", "da", "zur", "Er\u00b7de", "sehn", ",", "ihr", "Her\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die ihr so k\u00fchn durch Busenschleier seht,", "tokens": ["Die", "ihr", "so", "k\u00fchn", "durch", "Bu\u00b7sen\u00b7schlei\u00b7er", "seht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dem Sch\u00fcler gleich, an den Manschetten zerren,", "tokens": ["Dem", "Sch\u00fc\u00b7ler", "gleich", ",", "an", "den", "Man\u00b7schet\u00b7ten", "zer\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Der vor dem Rector steht!", "tokens": ["Der", "vor", "dem", "Rec\u00b7tor", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Empfinden lassen will ich Euch, da\u00df Herzen,", "tokens": ["Emp\u00b7fin\u00b7den", "las\u00b7sen", "will", "ich", "Euch", ",", "da\u00df", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Bewacht wie Nantens Herz, nicht so geschwind,", "tokens": ["Be\u00b7wacht", "wie", "Nan\u00b7tens", "Herz", ",", "nicht", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Als ihm beliebt darum herum zu scherzen,", "tokens": ["Als", "ihm", "be\u00b7liebt", "da\u00b7rum", "he\u00b7rum", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PAV", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Des Narren Beute sind.", "tokens": ["Des", "Nar\u00b7ren", "Beu\u00b7te", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die Eitelkeit wird, Euch zu tr\u00f6sten, l\u00fcgen:", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "wird", ",", "Euch", "zu", "tr\u00f6s\u00b7ten", ",", "l\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich sey Stat\u00fce! Ha! dann soll mit einmal", "tokens": ["Ich", "sey", "Sta\u00b7t\u00fce", "!", "Ha", "!", "dann", "soll", "mit", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "ITJ", "$.", "ADV", "VMFIN", "APPR", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Ein Blick der Lieb' auf Amaranten fliegen,", "tokens": ["Ein", "Blick", "der", "Lieb'", "auf", "A\u00b7ma\u00b7ran\u00b7ten", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "F\u00fcr Euch ein Wetterstrahl!", "tokens": ["F\u00fcr", "Euch", "ein", "Wet\u00b7ter\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}