{"textgrid.poem.67468": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die B\u00e4ume im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten B\u00e4ume, die Ihr die starren, entbl\u00e4tterten Arme", "tokens": ["Gu\u00b7ten", "B\u00e4u\u00b7me", ",", "die", "Ihr", "die", "star\u00b7ren", ",", "ent\u00b7bl\u00e4t\u00b7ter\u00b7ten", "Ar\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Reckt zum Himmel und fleht wieder den Fr\u00fchling herab!", "tokens": ["Reckt", "zum", "Him\u00b7mel", "und", "fleht", "wie\u00b7der", "den", "Fr\u00fch\u00b7ling", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ach, Ihr m\u00fc\u00dft noch harren, Ihr armen S\u00f6hne der Erde,", "tokens": ["Ach", ",", "Ihr", "m\u00fc\u00dft", "noch", "har\u00b7ren", ",", "Ihr", "ar\u00b7men", "S\u00f6h\u00b7ne", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Manche st\u00fcrmige Nacht, manchen erstarrenden Tag!", "tokens": ["Man\u00b7che", "st\u00fcr\u00b7mi\u00b7ge", "Nacht", ",", "man\u00b7chen", "er\u00b7star\u00b7ren\u00b7den", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber dann kommt wieder die Sonne mit gr\u00fcnendem Fr\u00fchling", "tokens": ["A\u00b7ber", "dann", "kommt", "wie\u00b7der", "die", "Son\u00b7ne", "mit", "gr\u00fc\u00b7nen\u00b7dem", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+---+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Euch; nur kehret auch mir Fr\u00fchling und Sonne zur\u00fcck?", "tokens": ["Euch", ";", "nur", "keh\u00b7ret", "auch", "mir", "Fr\u00fch\u00b7ling", "und", "Son\u00b7ne", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ADV", "VVFIN", "ADV", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Harre geduldig, Herz, und birg in die Wurzel den Saft Dir!", "tokens": ["Har\u00b7re", "ge\u00b7dul\u00b7dig", ",", "Herz", ",", "und", "birg", "in", "die", "Wur\u00b7zel", "den", "Saft", "Dir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NN", "$,", "KON", "NE", "APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Unvermuthet vielleicht treibt ihn das Schicksal empor.", "tokens": ["Un\u00b7ver\u00b7mu\u00b7thet", "viel\u00b7leicht", "treibt", "ihn", "das", "Schick\u00b7sal", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}