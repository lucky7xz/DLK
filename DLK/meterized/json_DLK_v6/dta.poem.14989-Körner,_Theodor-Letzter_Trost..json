{"dta.poem.14989": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Was starrt ihr wild in die Nacht hinaus,", "tokens": ["Was", "starrt", "ihr", "wild", "in", "die", "Nacht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr freien, ihr m\u00e4nnlichen Seelen?", "tokens": ["Ihr", "frei\u00b7en", ",", "ihr", "m\u00e4nn\u00b7li\u00b7chen", "See\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Jetzt heult der Sturm, jetzt brau\u00dft das Meer,", "tokens": ["Jetzt", "heult", "der", "Sturm", ",", "jetzt", "brau\u00dft", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt zittert das Erdreich um uns her,", "tokens": ["Jetzt", "zit\u00b7tert", "das", "Er\u00b7dreich", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wir woll'n uns die Noth nicht verhehlen.", "tokens": ["Wir", "woll'n", "uns", "die", "Noth", "nicht", "ver\u00b7heh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die H\u00f6lle brau\u00dft auf in neuer Gluth.", "tokens": ["Die", "H\u00f6l\u00b7le", "brau\u00dft", "auf", "in", "neu\u00b7er", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umsonst ist geflossen viel edles Blut,", "tokens": ["Um\u00b7sonst", "ist", "ge\u00b7flos\u00b7sen", "viel", "ed\u00b7les", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch triumphiren die B\u00f6sen.", "tokens": ["Noch", "tri\u00b7um\u00b7phi\u00b7ren", "die", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch nicht an der Rache des Himmels verzagt,", "tokens": ["Doch", "nicht", "an", "der", "Ra\u00b7che", "des", "Him\u00b7mels", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es hat nicht vergebens blutig getagt,", "tokens": ["Es", "hat", "nicht", "ver\u00b7ge\u00b7bens", "blu\u00b7tig", "ge\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Roth mu\u00df ja der Morgen sich l\u00f6sen.", "tokens": ["Roth", "mu\u00df", "ja", "der", "Mor\u00b7gen", "sich", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und galt es fr\u00fcherhin Muth und Kraft,", "tokens": ["Und", "galt", "es", "fr\u00fc\u00b7her\u00b7hin", "Muth", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jetzt alle Kr\u00e4fte zusammengerafft,", "tokens": ["Jetzt", "al\u00b7le", "Kr\u00e4f\u00b7te", "zu\u00b7sam\u00b7men\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sonst scheitert das Schiff noch im Hafen.", "tokens": ["Sonst", "schei\u00b7tert", "das", "Schiff", "noch", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erhebe dich Jugend, der Tieger dr\u00e4ut!", "tokens": ["Er\u00b7he\u00b7be", "dich", "Ju\u00b7gend", ",", "der", "Tie\u00b7ger", "dr\u00e4ut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Bewaffne dich Landsturm, jetzt kommt deine Zeit!", "tokens": ["Be\u00b7waff\u00b7ne", "dich", "Land\u00b7sturm", ",", "jetzt", "kommt", "dei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Erwache du Volk, das geschlafen!", "tokens": ["Er\u00b7wa\u00b7che", "du", "Volk", ",", "das", "ge\u00b7schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die wir hier r\u00fcstig zusammenstehn,", "tokens": ["Und", "die", "wir", "hier", "r\u00fcs\u00b7tig", "zu\u00b7sam\u00b7men\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und keck dem Tod in die Augen sehn,", "tokens": ["Und", "keck", "dem", "Tod", "in", "die", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Woll'n nicht vom Rechte lassen.", "tokens": ["Woll'n", "nicht", "vom", "Rech\u00b7te", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Freiheit retten, das Vaterland,", "tokens": ["Die", "Frei\u00b7heit", "ret\u00b7ten", ",", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Oder freudig sterben das Schwerdt in der Hand,", "tokens": ["O\u00b7der", "freu\u00b7dig", "ster\u00b7ben", "das", "Schwerdt", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und Knechtschaft und W\u00fcthriche hassen.", "tokens": ["Und", "Knecht\u00b7schaft", "und", "W\u00fcth\u00b7ri\u00b7che", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Das Leben gilt nichts, wo die Freiheit f\u00e4llt.", "tokens": ["Das", "Le\u00b7ben", "gilt", "nichts", ",", "wo", "die", "Frei\u00b7heit", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was giebt uns die weite unendliche Welt", "tokens": ["Was", "giebt", "uns", "die", "wei\u00b7te", "un\u00b7end\u00b7li\u00b7che", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "F\u00fcr des Vaterlands heiligen Boden? \u2014", "tokens": ["F\u00fcr", "des", "Va\u00b7ter\u00b7lands", "hei\u00b7li\u00b7gen", "Bo\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Frei woll'n wir das Vaterland wiedersehn,", "tokens": ["Frei", "woll'n", "wir", "das", "Va\u00b7ter\u00b7land", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Oder frei zu den gl\u00fccklichen V\u00e4tern gehn,", "tokens": ["O\u00b7der", "frei", "zu", "den", "gl\u00fcck\u00b7li\u00b7chen", "V\u00e4\u00b7tern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ja! gl\u00fccklich und frei sind die Todten.", "tokens": ["Ja", "!", "gl\u00fcck\u00b7lich", "und", "frei", "sind", "die", "Tod\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Drum heule, du Sturm, drum brause du Meer,", "tokens": ["Drum", "heu\u00b7le", ",", "du", "Sturm", ",", "drum", "brau\u00b7se", "du", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPER", "NN", "$,", "PAV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drum zittre, du Erdreich, um uns her,", "tokens": ["Drum", "zitt\u00b7re", ",", "du", "Er\u00b7dreich", ",", "um", "uns", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPER", "NN", "$,", "KOUI", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ihr sollt uns die Seele nicht z\u00fcgeln!", "tokens": ["Ihr", "sollt", "uns", "die", "See\u00b7le", "nicht", "z\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Erde kann neben uns untergehn", "tokens": ["Die", "Er\u00b7de", "kann", "ne\u00b7ben", "uns", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir woll'n als freie M\u00e4nner bestehn,", "tokens": ["Wir", "woll'n", "als", "frei\u00b7e", "M\u00e4n\u00b7ner", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und den Bund mit dem Blute besiegeln.", "tokens": ["Und", "den", "Bund", "mit", "dem", "Blu\u00b7te", "be\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}