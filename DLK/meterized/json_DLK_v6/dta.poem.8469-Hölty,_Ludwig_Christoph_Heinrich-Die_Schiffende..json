{"dta.poem.8469": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schiffende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Apfelbl\u00fcten zu;", "tokens": ["Ihr", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gelein, so ihre Gottheit f\u00fchlen,", "tokens": ["Die", "V\u00f6\u00b7ge\u00b7lein", ",", "so", "ih\u00b7re", "Got\u00b7theit", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwachen aus der Ruh.", "tokens": ["Er\u00b7wa\u00b7chen", "aus", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ihr Gewand im Morgenglanze flittert,", "tokens": ["Wie", "ihr", "Ge\u00b7wand", "im", "Mor\u00b7gen\u00b7glan\u00b7ze", "flit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihres Busens Flor!", "tokens": ["Und", "ih\u00b7res", "Bu\u00b7sens", "Flor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wankt dahin; der helle Vollmond zittert", "tokens": ["Sie", "wankt", "da\u00b7hin", ";", "der", "hel\u00b7le", "Voll\u00b7mond", "zit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus jeder Well' hervor.", "tokens": ["Aus", "je\u00b7der", "Well'", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da rauscht der Kahn durch hangende Gestr\u00e4uche,", "tokens": ["Da", "rauscht", "der", "Kahn", "durch", "han\u00b7gen\u00b7de", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Birgt mir das Engelbild,", "tokens": ["Birgt", "mir", "das", "En\u00b7gel\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwankt izt hervor, tanzt wieder auf dem Teiche,", "tokens": ["Schwankt", "izt", "her\u00b7vor", ",", "tanzt", "wie\u00b7der", "auf", "dem", "Tei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ihre Gottheit f\u00fcllt.", "tokens": ["Den", "ih\u00b7re", "Got\u00b7theit", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verdeckt mir nicht, ihr hangenden Gestr\u00e4uche,", "tokens": ["Ver\u00b7deckt", "mir", "nicht", ",", "ihr", "han\u00b7gen\u00b7den", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr l\u00e4chelndes Gesicht;", "tokens": ["Ihr", "l\u00e4\u00b7cheln\u00b7des", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tanzt so sch\u00f6n auf ihrem Silberteiche:", "tokens": ["Sie", "tanzt", "so", "sch\u00f6n", "auf", "ih\u00b7rem", "Sil\u00b7ber\u00b7tei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Erlen, bergt sie nicht.", "tokens": ["Ihr", "Er\u00b7len", ",", "bergt", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Weht, Winde, weht, o fl\u00fcgelt sie, ihr Winde,", "tokens": ["Weht", ",", "Win\u00b7de", ",", "weht", ",", "o", "fl\u00fc\u00b7gelt", "sie", ",", "ihr", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "FM", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An diese Laub' heran,", "tokens": ["An", "die\u00b7se", "Laub'", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dass ich mich ihr im Schauer dieser Linde", "tokens": ["Dass", "ich", "mich", "ihr", "im", "Schau\u00b7er", "die\u00b7ser", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu F\u00fcssen werfen kann.", "tokens": ["Zu", "F\u00fcs\u00b7sen", "wer\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}