{"dta.poem.5484": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es will f\u00fcr alle seine Gaben,", "tokens": ["Es", "will", "f\u00fcr", "al\u00b7le", "sei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die er dir schenckte, GOtt der HErr", "tokens": ["Die", "er", "dir", "schenck\u00b7te", ",", "Gott", "der", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht lange Complimenten haben;", "tokens": ["Nicht", "lan\u00b7ge", "Com\u00b7pli\u00b7men\u00b7ten", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er heischt kein \u00e4ngstlich Wort-Gepl\u00e4rr:", "tokens": ["Er", "heischt", "kein", "\u00e4ngst\u00b7lich", "Wor\u00b7tGe\u00b7pl\u00e4rr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man darf des Sch\u00f6pfers Dienst nicht scheuen;", "tokens": ["Man", "darf", "des", "Sch\u00f6p\u00b7fers", "Dienst", "nicht", "scheu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man ist belohnt, wenn man ihn ehrt.", "tokens": ["Man", "ist", "be\u00b7lohnt", ",", "wenn", "man", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich sein in seinen Wercken freuen", "tokens": ["Sich", "sein", "in", "sei\u00b7nen", "Wer\u00b7cken", "freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist aller Danck, den er begehrt.", "tokens": ["Ist", "al\u00b7ler", "Danck", ",", "den", "er", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}