{"textgrid.poem.39978": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "6. Anderhalde's Traum", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit gekr\u00fcmmtem R\u00fccken sitzt", "tokens": ["Mit", "ge\u00b7kr\u00fcmm\u00b7tem", "R\u00fc\u00b7cken", "sitzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Stuhl Herr Anderhalde,", "tokens": ["In", "dem", "Stuhl", "Herr", "An\u00b7der\u00b7hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah von ferne, wie es blitzt',", "tokens": ["Sah", "von", "fer\u00b7ne", ",", "wie", "es", "blitzt'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hirtenschwert im Speicherwalde;", "tokens": ["Hir\u00b7ten\u00b7schwert", "im", "Spei\u00b7cher\u00b7wal\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Labt sein Haupt im Sonnenschein", "tokens": ["Labt", "sein", "Haupt", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An der Freiheit goldnem Morgen,", "tokens": ["An", "der", "Frei\u00b7heit", "gold\u00b7nem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann er nicht mehr mit befrei'n,", "tokens": ["Kann", "er", "nicht", "mehr", "mit", "be\u00b7frei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denken kann er doch und sorgen.", "tokens": ["Den\u00b7ken", "kann", "er", "doch", "und", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es pfl\u00fccken oft im Traum", "tokens": ["Und", "es", "pfl\u00fc\u00b7cken", "oft", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochbejahrte Greise wieder", "tokens": ["Hoch\u00b7be\u00b7jahr\u00b7te", "Grei\u00b7se", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Jugend gr\u00fcnem Baum", "tokens": ["Von", "der", "Ju\u00b7gend", "gr\u00fc\u00b7nem", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnungsbilder, Wunderlieder;", "tokens": ["Ah\u00b7nungs\u00b7bil\u00b7der", ",", "Wun\u00b7der\u00b7lie\u00b7der", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sie da geh\u00f6rt, geschaut,", "tokens": ["Was", "sie", "da", "ge\u00b7h\u00f6rt", ",", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "J\u00fcngre wird es unterweisen;", "tokens": ["J\u00fcng\u00b7re", "wird", "es", "un\u00b7ter\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So auch neiget sich ergraut", "tokens": ["So", "auch", "nei\u00b7get", "sich", "er\u00b7graut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jetzt zum Traum das Haupt des Greisen.", "tokens": ["Jetzt", "zum", "Traum", "das", "Haupt", "des", "Grei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Gesicht f\u00fchrt ihn empor,", "tokens": ["Ein", "Ge\u00b7sicht", "f\u00fchrt", "ihn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo mit seinem gr\u00fcnen R\u00fccken", "tokens": ["Wo", "mit", "sei\u00b7nem", "gr\u00fc\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Berge der Kamor", "tokens": ["In", "die", "Ber\u00b7ge", "der", "Ka\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ins Thal zugleich darf blicken.", "tokens": ["Und", "ins", "Thal", "zu\u00b7gleich", "darf", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Alpsteins Riesenkluft", "tokens": ["In", "des", "A\u00b7lpsteins", "Rie\u00b7sen\u00b7kluft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaut er, kann das Rheinthal gr\u00fc\u00dfen.", "tokens": ["Schaut", "er", ",", "kann", "das", "Rheint\u00b7hal", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Thur- und Heg\u00e4u winkt im Duft,", "tokens": ["Thur", "und", "He\u00b7g\u00e4u", "winkt", "im", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Appenzell zu seinen F\u00fc\u00dfen.", "tokens": ["Ap\u00b7pen\u00b7zell", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ihm d\u00fcnket menschenleer", "tokens": ["Und", "ihm", "d\u00fcn\u00b7ket", "men\u00b7schen\u00b7leer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Heimat Thalgel\u00e4nde,", "tokens": ["Sei\u00b7ner", "Hei\u00b7mat", "Thal\u00b7ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine H\u00fctten hin und her", "tokens": ["Kei\u00b7ne", "H\u00fct\u00b7ten", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind gebaut durch kluge H\u00e4nde.", "tokens": ["Sind", "ge\u00b7baut", "durch", "klu\u00b7ge", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Bewohner harrt es stumm,", "tokens": ["Der", "Be\u00b7woh\u00b7ner", "harrt", "es", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sitter nur und Urn\u00e4sch", "tokens": ["Sit\u00b7ter", "nur", "und", "Ur\u00b7n\u00e4sch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schauernd sieht der Greis sich um:", "tokens": ["Schau\u00b7ernd", "sieht", "der", "Greis", "sich", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer wird kommen, hier zu hausen?", "tokens": ["Wer", "wird", "kom\u00b7men", ",", "hier", "zu", "hau\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Luft und Erde jetzt erschallt", "tokens": ["Luft", "und", "Er\u00b7de", "jetzt", "er\u00b7schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als von Fl\u00fcgelschlag und Tritten,", "tokens": ["Als", "von", "Fl\u00fc\u00b7gel\u00b7schlag", "und", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es wimmelt aus dem Wald,", "tokens": ["Und", "es", "wim\u00b7melt", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt mit Fittichen und Schritten;", "tokens": ["Kommt", "mit", "Fit\u00b7ti\u00b7chen", "und", "Schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thiere sind's in bunter Schar,", "tokens": ["Thie\u00b7re", "sin\u00b7d's", "in", "bun\u00b7ter", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Wollen Herrn des Landes werden,", "tokens": ["Wol\u00b7len", "Herrn", "des", "Lan\u00b7des", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein schwarzer, stolzer Aar", "tokens": ["Und", "ein", "schwar\u00b7zer", ",", "stol\u00b7zer", "Aar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schl\u00e4gt den Fittich vor den Heerden.", "tokens": ["Schl\u00e4gt", "den", "Fit\u00b7tich", "vor", "den", "Heer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dr\u00fcben kommen sie vom ", "tokens": ["Dr\u00fc\u00b7ben", "kom\u00b7men", "sie", "vom"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Falken, Schw\u00e4ne, Greifen, Drachen;", "tokens": ["Fal\u00b7ken", ",", "Schw\u00e4\u00b7ne", ",", "Grei\u00b7fen", ",", "Dra\u00b7chen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcllend, wiehernd, Stier und Ro\u00df,", "tokens": ["Br\u00fcl\u00b7lend", ",", "wie\u00b7hernd", ",", "Stier", "und", "Ro\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00f6lfe mit dem blut'gen Rachen;", "tokens": ["W\u00f6l\u00b7fe", "mit", "dem", "blut'\u00b7gen", "Ra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eber w\u00fchlen mit dem Zahn,", "tokens": ["E\u00b7ber", "w\u00fch\u00b7len", "mit", "dem", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem R\u00fcssel Elephanten,", "tokens": ["Mit", "dem", "R\u00fcs\u00b7sel", "E\u00b7le\u00b7phan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "St\u00fcrzen auf den gr\u00fcnen Plan", "tokens": ["St\u00fcr\u00b7zen", "auf", "den", "gr\u00fc\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder von des Berges Kanten.", "tokens": ["Nie\u00b7der", "von", "des", "Ber\u00b7ges", "Kan\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bange schaut der Greis zu Grund:", "tokens": ["Ban\u00b7ge", "schaut", "der", "Greis", "zu", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft das Land sich die gefallen?", "tokens": ["L\u00e4\u00dft", "das", "Land", "sich", "die", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ART", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alsobald im Alpenschlund", "tokens": ["Al\u00b7so\u00b7bald", "im", "Al\u00b7pen\u00b7schlund"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Murrt es, da\u00df die Felsen hallen.", "tokens": ["Murrt", "es", ",", "da\u00df", "die", "Fel\u00b7sen", "hal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Staunend blickt er um sich her:", "tokens": ["Stau\u00b7nend", "blickt", "er", "um", "sich", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn hervor aus sieben Th\u00e4lern", "tokens": ["Denn", "her\u00b7vor", "aus", "sie\u00b7ben", "Th\u00e4\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "St\u00fcrzt der Alpen Herr, der B\u00e4r,", "tokens": ["St\u00fcrzt", "der", "Al\u00b7pen", "Herr", ",", "der", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4\u00dft das Hausrecht sich nicht schm\u00e4lern.", "tokens": ["L\u00e4\u00dft", "das", "Haus\u00b7recht", "sich", "nicht", "schm\u00e4\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Droben ist er schon am Wald,", "tokens": ["Dro\u00b7ben", "ist", "er", "schon", "am", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4hrt den Thieren in die H\u00fcften,", "tokens": ["F\u00e4hrt", "den", "Thie\u00b7ren", "in", "die", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4umt sich, steht und streitet bald", "tokens": ["B\u00e4