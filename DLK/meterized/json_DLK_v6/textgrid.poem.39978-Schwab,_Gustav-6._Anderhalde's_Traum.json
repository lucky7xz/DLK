ken_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Haus er schleunig bitten,", "tokens": ["In", "das", "Haus", "er", "schleu\u00b7nig", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht: \u00bbIhr Br\u00fcder, haltet fest,", "tokens": ["Spricht", ":", "\u00bb", "Ihr", "Br\u00fc\u00b7der", ",", "hal\u00b7tet", "fest", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn auf's Neue wird gestritten.", "tokens": ["Denn", "auf's", "Neu\u00b7e", "wird", "ge\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor dem Auge steht mir hell,", "tokens": ["Vor", "dem", "Au\u00b7ge", "steht", "mir", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer sich f\u00fcr den Abt wird r\u00fcsten:", "tokens": ["Wer", "sich", "f\u00fcr", "den", "Abt", "wird", "r\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Oestreichs Adler, Appenzell,", "tokens": ["O\u00b7e\u00b7streichs", "Ad\u00b7ler", ",", "Ap\u00b7pen\u00b7zell", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Will in deinem Horste nisten.", "tokens": ["Will", "in", "dei\u00b7nem", "Hors\u00b7te", "nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ritter bringt er, k\u00fchn und wild,", "tokens": ["Rit\u00b7ter", "bringt", "er", ",", "k\u00fchn", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Thier' auf Helm und Wappen,", "tokens": ["Wie", "die", "Thier'", "auf", "Helm", "und", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle sah mein Traum im Bild", "tokens": ["Al\u00b7le", "sah", "mein", "Traum", "im", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolze Herren, freche Knappen,", "tokens": ["Stol\u00b7ze", "Her\u00b7ren", ",", "fre\u00b7che", "Knap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wolfurt, Schwaneck, Greifenstein,", "tokens": ["Wol\u00b7furt", ",", "Schwa\u00b7neck", ",", "Grei\u00b7fen\u00b7stein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trautburg mit dem Haupt des Stieres;", "tokens": ["Traut\u00b7burg", "mit", "dem", "Haupt", "des", "Stie\u00b7res", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, es wird kein Ende sein", "tokens": ["Ach", ",", "es", "wird", "kein", "En\u00b7de", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PIAT", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses grimmigen Gethieres!", "tokens": ["Die\u00b7ses", "grim\u00b7mi\u00b7gen", "Ge\u00b7thie\u00b7res", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber dich, o V\u00f6lklein, auch", "tokens": ["A\u00b7ber", "dich", ",", "o", "V\u00f6l\u00b7klein", ",", "auch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich streitbar abgebildet,", "tokens": ["Sah", "ich", "streit\u00b7bar", "ab\u00b7ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie nach grauer V\u00e4ter Brauch", "tokens": ["Wie", "nach", "grau\u00b7er", "V\u00e4\u00b7ter", "Brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Gauen sich beschildet;", "tokens": ["Dei\u00b7ne", "Gau\u00b7en", "sich", "be\u00b7schil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner W\u00e4lder altes Wild", "tokens": ["Dei\u00b7ner", "W\u00e4l\u00b7der", "al\u00b7tes", "Wild"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrest du zu deinem Zeichen:", "tokens": ["F\u00fch\u00b7rest", "du", "zu", "dei\u00b7nem", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwarzer B\u00e4r im goldnen Schild,", "tokens": ["Schwar\u00b7zer", "B\u00e4r", "im", "gold\u00b7nen", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keinem Thiere wirst du weichen!", "tokens": ["Kei\u00b7nem", "Thie\u00b7re", "wirst", "du", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nur getrost hinauf zum Sto\u00df,", "tokens": ["Nur", "ge\u00b7trost", "hin\u00b7auf", "zum", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dorthin durft' ich tr\u00e4umend blicken,", "tokens": ["Dor\u00b7thin", "durft'", "ich", "tr\u00e4u\u00b7mend", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stier und Drachen, Greif und Ro\u00df,", "tokens": ["Stier", "und", "Dra\u00b7chen", ",", "Greif", "und", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dorther wird's der Adler schicken.", "tokens": ["Dor\u00b7ther", "wird's", "der", "Ad\u00b7ler", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, dein Leben gilt es, B\u00e4r!", "tokens": ["Ja", ",", "dein", "Le\u00b7ben", "gilt", "es", ",", "B\u00e4r", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df ihn f\u00fchlen deine Klauen,", "tokens": ["La\u00df", "ihn", "f\u00fch\u00b7len", "dei\u00b7ne", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einer nur, du oder er,", "tokens": ["Ei\u00b7ner", "nur", ",", "du", "o\u00b7der", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Wohn' hinfort in diesen Gauen!\u00ab", "tokens": ["Wohn'", "hin\u00b7fort", "in", "die\u00b7sen", "Gau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}