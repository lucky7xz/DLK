{"textgrid.poem.26110": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtigall ruht jetzt nicht die ganze Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine lange Wolkenwand vor dem Mond steht,", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "Wol\u00b7ken\u00b7wand", "vor", "dem", "Mond", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie eine T\u00fcr, die nicht zugeht;", "tokens": ["Wie", "ei\u00b7ne", "T\u00fcr", ",", "die", "nicht", "zu\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drunter ein Lichtschein \u00fcber die Schwelle weht.", "tokens": ["Drun\u00b7ter", "ein", "Licht\u00b7schein", "\u00fc\u00b7ber", "die", "Schwel\u00b7le", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall ruht jetzt nicht die ganze Nacht.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ruht", "jetzt", "nicht", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie hat sich stundenlang um den Schlaf gebracht,", "tokens": ["Sie", "hat", "sich", "stun\u00b7den\u00b7lang", "um", "den", "Schlaf", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als tut nur ein Lied dem m\u00fcden Blut gut.", "tokens": ["Als", "tut", "nur", "ein", "Lied", "dem", "m\u00fc\u00b7den", "Blut", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf die Nachtigall horchend ich oft aufstand,", "tokens": ["Auf", "die", "Nach\u00b7ti\u00b7gall", "hor\u00b7chend", "ich", "oft", "auf\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Als sp\u00fcrt' ich ein Zwiegespr\u00e4ch hinter der Wand.", "tokens": ["Als", "sp\u00fcrt'", "ich", "ein", "Zwie\u00b7ge\u00b7spr\u00e4ch", "hin\u00b7ter", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Aber nur den Mond bei der Wolke ich fand.", "tokens": ["A\u00b7ber", "nur", "den", "Mond", "bei", "der", "Wol\u00b7ke", "ich", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}