{"textgrid.poem.48252": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Land Gosen", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft h\u00f6r' ich: \u00bbUnsre gute Stadt", "tokens": ["Oft", "h\u00f6r'", "ich", ":", "\u00bb", "Uns\u00b7re", "gu\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Augenscheinlich eine Verhei\u00dfung hat,", "tokens": ["Au\u00b7gen\u00b7schein\u00b7lich", "ei\u00b7ne", "Ver\u00b7hei\u00b7\u00dfung", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Himmel, der uns so hegt und pflegt,", "tokens": ["Der", "Him\u00b7mel", ",", "der", "uns", "so", "hegt", "und", "pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat uns alles wie vor die T\u00fcre gelegt.", "tokens": ["Hat", "uns", "al\u00b7les", "wie", "vor", "die", "T\u00fc\u00b7re", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "KOKOM", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ja, ja, wir haben es leicht und bequem:", "tokens": ["Ja", ",", "ja", ",", "wir", "ha\u00b7ben", "es", "leicht", "und", "be\u00b7quem", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Brieselang Eichen, in Glindow Lehm,", "tokens": ["Im", "Brie\u00b7se\u00b7lang", "Ei\u00b7chen", ",", "in", "Glin\u00b7dow", "Lehm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In Rauen Kohlen, in Linum Torf,", "tokens": ["In", "Rau\u00b7en", "Koh\u00b7len", ",", "in", "Li\u00b7num", "Torf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kalkgeschiebe bei R\u00fcdersdorf,", "tokens": ["Kalk\u00b7ge\u00b7schie\u00b7be", "bei", "R\u00fc\u00b7ders\u00b7dorf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Im Grunewald Schwarzwild, Hirsch und Reh,", "tokens": ["Im", "Gru\u00b7ne\u00b7wald", "Schwarz\u00b7wild", ",", "Hirsch", "und", "Reh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Spargel en masse bei Halensee,", "tokens": ["Spar\u00b7gel", "en", "mas\u00b7se", "bei", "Ha\u00b7len\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "APPR", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Dill und Morcheln und Teltower R\u00fcben,", "tokens": ["Dill", "und", "Mor\u00b7cheln", "und", "Tel\u00b7to\u00b7wer", "R\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Oderkrebse h\u00fcben und dr\u00fcben,", "tokens": ["O\u00b7der\u00b7kreb\u00b7se", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "ADV", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Auf dem Hohen Barnim Fetthammel-Herden", "tokens": ["Auf", "dem", "Ho\u00b7hen", "Bar\u00b7nim", "Fett\u00b7ham\u00b7mel\u00b7Her\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "(werden mit n\u00e4chstem Southdowns werden),", "tokens": ["(", "wer\u00b7den", "mit", "n\u00e4chs\u00b7tem", "South\u00b7downs", "wer\u00b7den", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "APPR", "ADJA", "NN", "VAINF", "$(", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "K\u00f6nigshorster Butter, in Sperenberg Salz,", "tokens": ["K\u00f6\u00b7nigs\u00b7hors\u00b7ter", "But\u00b7ter", ",", "in", "Spe\u00b7ren\u00b7berg", "Salz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Im Warthebruch Gerste, Graupen und Malz,", "tokens": ["Im", "Wart\u00b7heb\u00b7ruch", "Gers\u00b7te", ",", "Grau\u00b7pen", "und", "Malz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "In Kienbaum Honig, im Havelland Milch,", "tokens": ["In", "Kien\u00b7baum", "Ho\u00b7nig", ",", "im", "Ha\u00b7vel\u00b7land", "Milch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "In Luckenwalde Tuch und Drillch,", "tokens": ["In", "Lu\u00b7cken\u00b7wal\u00b7de", "Tuch", "und", "Drillch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bei den Werderschen Kirschen und Aprikosen", "tokens": ["Bei", "den", "Wer\u00b7der\u00b7schen", "Kir\u00b7schen", "und", "Ap\u00b7ri\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Und bei Potsdam ganze Felder von Rosen.", "tokens": ["Und", "bei", "Pots\u00b7dam", "gan\u00b7ze", "Fel\u00b7der", "von", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJA", "NN", "APPR", "NE", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Nichts entlehnt und nichts geborgt,", "tokens": ["Nichts", "ent\u00b7lehnt", "und", "nichts", "ge\u00b7borgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "KON", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "F\u00fcr Gro\u00dfes und Kleines ringsum gesorgt,", "tokens": ["F\u00fcr", "Gro\u00b7\u00dfes", "und", "Klei\u00b7nes", "ring\u00b7sum", "ge\u00b7sorgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und gesorgt vor allem auch (und nicht schlecht)", "tokens": ["Und", "ge\u00b7sorgt", "vor", "al\u00b7lem", "auch", "(", "und", "nicht", "schlecht", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PIS", "ADV", "$(", "KON", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Schon f\u00fcr unser kommendes Geschlecht, \u2013", "tokens": ["Schon", "f\u00fcr", "un\u00b7ser", "kom\u00b7men\u00b7des", "Ge\u00b7schlecht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Und fast unm\u00f6glichen Spreewaldsammen.\u00ab", "tokens": ["Und", "fast", "un\u00b7m\u00f6g\u00b7li\u00b7chen", "Spree\u00b7wald\u00b7sam\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}