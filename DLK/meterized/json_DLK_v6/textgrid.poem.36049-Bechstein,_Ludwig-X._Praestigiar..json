{"textgrid.poem.36049": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "X. Praestigiar.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhoch Faustus! Hoch! Er lebe!\u00ab so schallt's im hellen Saal", "tokens": ["\u00bb", "hoch", "Faus\u00b7tus", "!", "Hoch", "!", "Er", "le\u00b7be", "!", "\u00ab", "so", "schallt's", "im", "hel\u00b7len", "Saal"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "NE", "$.", "ADJD", "$.", "PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Vom Munde froher Zecher, Pokal klingt an Pokal;", "tokens": ["Vom", "Mun\u00b7de", "fro\u00b7her", "Ze\u00b7cher", ",", "Po\u00b7kal", "klingt", "an", "Po\u00b7kal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gewandte Diener fliegen den Elfen gleich, umher;", "tokens": ["Ge\u00b7wand\u00b7te", "Die\u00b7ner", "flie\u00b7gen", "den", "El\u00b7fen", "gleich", ",", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "S\u00fcss rauscht von holden Kl\u00e4ngen ein unsichtbares Meer.", "tokens": ["S\u00fcss", "rauscht", "von", "hol\u00b7den", "Kl\u00e4n\u00b7gen", "ein", "un\u00b7sicht\u00b7ba\u00b7res", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von bunten Lichtern schimmern die Spiegelw\u00e4nde all',", "tokens": ["Von", "bun\u00b7ten", "Lich\u00b7tern", "schim\u00b7mern", "die", "Spie\u00b7gel\u00b7w\u00e4n\u00b7de", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Zauberbilder l\u00e4cheln und gaukeln im Krystall.", "tokens": ["Und", "Zau\u00b7ber\u00b7bil\u00b7der", "l\u00e4\u00b7cheln", "und", "gau\u00b7keln", "im", "Krys\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+++", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dort hauchen Blumen Balsam, dort schwebt des Weihrauchs Duft,", "tokens": ["Dort", "hau\u00b7chen", "Blu\u00b7men", "Bal\u00b7sam", ",", "dort", "schwebt", "des", "Weih\u00b7rauchs", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zum Garten wird der Festsaal, wie zum Arom die Luft.", "tokens": ["Zum", "Gar\u00b7ten", "wird", "der", "Fest\u00b7saal", ",", "wie", "zum", "A\u00b7rom", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Dort springen klare Bronnen, wie steigt ihr Silberstrahl!", "tokens": ["Dort", "sprin\u00b7gen", "kla\u00b7re", "Bron\u00b7nen", ",", "wie", "steigt", "ihr", "Sil\u00b7ber\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort bieten sich die Fr\u00fcchte Hesperiens zur Wahl.", "tokens": ["Dort", "bie\u00b7ten", "sich", "die", "Fr\u00fcch\u00b7te", "Hes\u00b7pe\u00b7ri\u00b7ens", "zur", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleich fl\u00fcss'gen Edelsteinen, im feuergoldnen Glanz,", "tokens": ["Gleich", "fl\u00fcss'\u00b7gen", "E\u00b7del\u00b7stei\u00b7nen", ",", "im", "feu\u00b7er\u00b7gold\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Blickt dort von edlen Weinen krystallklar mancher Kranz.", "tokens": ["Blickt", "dort", "von", "ed\u00b7len", "Wei\u00b7nen", "krys\u00b7tall\u00b7klar", "man\u00b7cher", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und dort bei hohen G\u00e4sten sitzt Faustus, reichgeschm\u00fcckt;", "tokens": ["Und", "dort", "bei", "ho\u00b7hen", "G\u00e4s\u00b7ten", "sitzt", "Faus\u00b7tus", ",", "reich\u00b7ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ihn hat mit Wundergaben, so scheint's, das Gl\u00fcck begl\u00fcckt.", "tokens": ["Ihn", "hat", "mit", "Wun\u00b7der\u00b7ga\u00b7ben", ",", "so", "scheint's", ",", "das", "Gl\u00fcck", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "ADV", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wie die Sonne freudig ausstrahlt ihr ew'ges Licht,", "tokens": ["Und", "wie", "die", "Son\u00b7ne", "freu\u00b7dig", "aus\u00b7strahlt", "ihr", "ew'\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bei ihm der Kranz des Frohsinns in holde Bl\u00fcthen bricht.", "tokens": ["Bei", "ihm", "der", "Kranz", "des", "Froh\u00b7sinns", "in", "hol\u00b7de", "Bl\u00fc\u00b7then", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja, Frohsinn ist ein Gastwirth, den Keiner schelten soll;", "tokens": ["Ja", ",", "Froh\u00b7sinn", "ist", "ein", "Gast\u00b7wirth", ",", "den", "Kei\u00b7ner", "schel\u00b7ten", "soll", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-++++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Vom Zauberwein der Freude sind seine Keller voll.", "tokens": ["Vom", "Zau\u00b7ber\u00b7wein", "der", "Freu\u00b7de", "sind", "sei\u00b7ne", "Kel\u00b7ler", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihm dienen heitre Scherze, sind immer bei der Hand,", "tokens": ["Ihm", "die\u00b7nen", "heit\u00b7re", "Scher\u00b7ze", ",", "sind", "im\u00b7mer", "bei", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Lust ist seine Kelln'rin, holdselig, flink, gewandt.", "tokens": ["Und", "Lust", "ist", "sei\u00b7ne", "Kelln'\u00b7rin", ",", "hold\u00b7se\u00b7lig", ",", "flink", ",", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Sein Schild ist aufgehangen hochprangend am Portal,", "tokens": ["Sein", "Schild", "ist", "auf\u00b7ge\u00b7han\u00b7gen", "hoch\u00b7pran\u00b7gend", "am", "Por\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Darauf ein passend Sinnbild: ein sch\u00e4umender Pokal.", "tokens": ["Da\u00b7rauf", "ein", "pas\u00b7send", "Sinn\u00b7bild", ":", "ein", "sch\u00e4u\u00b7men\u00b7der", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und drunter steht mit Goldschrift, die weit ins Auge gleisst,", "tokens": ["Und", "drun\u00b7ter", "steht", "mit", "Gold\u00b7schrift", ",", "die", "weit", "ins", "Au\u00b7ge", "gleisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das: ", "tokens": ["Das", ":"], "token_info": ["word", "punct"], "pos": ["PDS", "$."], "meter": "-", "measure": "single.down"}}, "stanza.7": {"line.1": {"text": "Zu frohen Liedesstimmen, zu hellem Becherklang,", "tokens": ["Zu", "fro\u00b7hen", "Lie\u00b7des\u00b7stim\u00b7men", ",", "zu", "hel\u00b7lem", "Be\u00b7cher\u00b7klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ert\u00f6nt von Waldess\u00e4ngern melodischzarter Sang.", "tokens": ["Er\u00b7t\u00f6nt", "von", "Wal\u00b7des\u00b7s\u00e4n\u00b7gern", "me\u00b7lo\u00b7dischzar\u00b7ter", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus gr\u00fcnem Laubwerk fl\u00f6tet ihr Lied die Nachtigall,", "tokens": ["Aus", "gr\u00fc\u00b7nem", "Laub\u00b7werk", "fl\u00f6\u00b7tet", "ihr", "Lied", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hell t\u00f6nt aus d\u00fcstern B\u00fcschen des Amselschlages Schall.", "tokens": ["Hell", "t\u00f6nt", "aus", "d\u00fcs\u00b7tern", "B\u00fc\u00b7schen", "des", "Am\u00b7sel\u00b7schla\u00b7ges", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Und plaudernd wiegt in Ringen sich mancher Papagei;", "tokens": ["Und", "plau\u00b7dernd", "wiegt", "in", "Rin\u00b7gen", "sich", "man\u00b7cher", "Pa\u00b7pa\u00b7gei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zahm schreiten Goldfasanen und Kraniche herbei.", "tokens": ["Zahm", "schrei\u00b7ten", "Gold\u00b7fa\u00b7sa\u00b7nen", "und", "Kra\u00b7ni\u00b7che", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+--++--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wie fr\u00f6hlich sind die G\u00e4ste; sie scherzen laut und viel,", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "sind", "die", "G\u00e4s\u00b7te", ";", "sie", "scher\u00b7zen", "laut", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und treiben mit den Thieren manch neckisch Gaukelspiel.", "tokens": ["Und", "trei\u00b7ben", "mit", "den", "Thie\u00b7ren", "manch", "ne\u00b7ckisch", "Gau\u00b7kel\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da schallt ein grimmes Heulen, gemischt aus Wath und Qual,", "tokens": ["Da", "schallt", "ein", "grim\u00b7mes", "Heu\u00b7len", ",", "ge\u00b7mischt", "aus", "Wath", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es zittern fast die S\u00e4ulen im lustdurchrauschten Saal;", "tokens": ["Es", "zit\u00b7tern", "fast", "die", "S\u00e4u\u00b7len", "im", "lust\u00b7durc\u00b7hrauschten", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und stille wird's. \u2013 Was thut sich so furchtbar drohend kund?", "tokens": ["Und", "stil\u00b7le", "wird'", "s.", "\u2013", "Was", "thut", "sich", "so", "furcht\u00b7bar", "dro\u00b7hend", "kund", "?"], "token_info": ["word", "word", "word", "abbreviation", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "VVIMP", "$(", "PWS", "VVFIN", "PRF", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aufspringt die Th\u00fcr, es schreitet herein \u2013 ein schwarzer Hund.", "tokens": ["Auf\u00b7springt", "die", "Th\u00fcr", ",", "es", "schrei\u00b7tet", "her\u00b7ein", "\u2013", "ein", "schwar\u00b7zer", "Hund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Gross, riesengross, und kohlschwarz und scheusslich nackt, das Paar", "tokens": ["Gross", ",", "rie\u00b7sen\u00b7gross", ",", "und", "kohl\u00b7schwarz", "und", "scheuss\u00b7lich", "nackt", ",", "das", "Paar"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "KON", "ADJD", "KON", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Augen gl\u00fcht wie Kohlen, das ist ", "tokens": ["Der", "Au\u00b7gen", "gl\u00fcht", "wie", "Koh\u00b7len", ",", "das", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,", "PDS", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Z\u00e4hne zeigt er murrend, und wild der Augen Gluth;", "tokens": ["Die", "Z\u00e4h\u00b7ne", "zeigt", "er", "mur\u00b7rend", ",", "und", "wild", "der", "Au\u00b7gen", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und alles schweigt \u2013 sein Anblick l\u00e4hmt Freude, Lust und Muth.", "tokens": ["Und", "al\u00b7les", "schweigt", "\u2013", "sein", "An\u00b7blick", "l\u00e4hmt", "Freu\u00b7de", ",", "Lust", "und", "Muth", "."], "token_info": ["