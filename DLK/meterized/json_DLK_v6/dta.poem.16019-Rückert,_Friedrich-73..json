{"dta.poem.16019": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df heilige der Zweck die Mittel, wird bestritten,", "tokens": ["Da\u00df", "hei\u00b7li\u00b7ge", "der", "Zweck", "die", "Mit\u00b7tel", ",", "wird", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ART", "NN", "ART", "NN", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir aber m\u00fc\u00dfen nur Scheinheiligkeit verbitten.", "tokens": ["Wir", "a\u00b7ber", "m\u00fc\u00b7\u00dfen", "nur", "Schein\u00b7hei\u00b7lig\u00b7keit", "ver\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der gute Zweck macht gut die Mittel, recht verstanden,", "tokens": ["Der", "gu\u00b7te", "Zweck", "macht", "gut", "die", "Mit\u00b7tel", ",", "recht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil wir nie guten Zweck durch schlechte Mittel fanden.", "tokens": ["Weil", "wir", "nie", "gu\u00b7ten", "Zweck", "durch", "schlech\u00b7te", "Mit\u00b7tel", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VV