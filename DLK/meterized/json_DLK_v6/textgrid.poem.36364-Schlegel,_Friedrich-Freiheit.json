{"textgrid.poem.36364": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit, so die Fl\u00fcgel", "tokens": ["Frei\u00b7heit", ",", "so", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwingt zur Felsenkluft,", "tokens": ["Schwingt", "zur", "Fel\u00b7sen\u00b7kluft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn um gr\u00fcne H\u00fcgel", "tokens": ["Wenn", "um", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weht des Fr\u00fchlings Luft;", "tokens": ["Weht", "des", "Fr\u00fch\u00b7lings", "Luft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sprich aus dem Gesange,", "tokens": ["Sprich", "aus", "dem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rausch' in deutschem Klange,", "tokens": ["Rausch'", "in", "deut\u00b7schem", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Atme Waldes Duft!", "tokens": ["At\u00b7me", "Wal\u00b7des", "Duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was mit Lust und Beben", "tokens": ["Was", "mit", "Lust", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In die Seele bricht,", "tokens": ["In", "die", "See\u00b7le", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dies geheime Leben,", "tokens": ["Dies", "ge\u00b7hei\u00b7me", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist es Freiheit nicht?", "tokens": ["Ist", "es", "Frei\u00b7heit", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Diese Wunderf\u00fclle,", "tokens": ["Die\u00b7se", "Wun\u00b7der\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die in Liebesh\u00fclle", "tokens": ["Die", "in", "Lie\u00b7bes\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "An die Sinne spricht?", "tokens": ["An", "die", "Sin\u00b7ne", "spricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Frei sich regt und froher", "tokens": ["Frei", "sich", "regt", "und", "fro\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ahndung in der Brust,", "tokens": ["Ahn\u00b7dung", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und des Waldes hoher", "tokens": ["Und", "des", "Wal\u00b7des", "ho\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geist wird uns bewu\u00dft.", "tokens": ["Geist", "wird", "uns", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Linde Bl\u00fctenwellen", "tokens": ["Lin\u00b7de", "Bl\u00fc\u00b7ten\u00b7wel\u00b7len"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schlagen an und schwellen", "tokens": ["Schla\u00b7gen", "an", "und", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00f6her stets die Lust.", "tokens": ["H\u00f6\u00b7her", "stets", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00f6her noch entz\u00fcndet", "tokens": ["H\u00f6\u00b7her", "noch", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Flammt der Geist empor,", "tokens": ["Flammt", "der", "Geist", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wessen Herz verb\u00fcndet,", "tokens": ["Wes\u00b7sen", "Herz", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich den Freund erkor.", "tokens": ["Sich", "den", "Freund", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fcr die Freiheit sterben", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sah man, Ruhm erwerben", "tokens": ["Sah", "man", ",", "Ruhm", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Oft der Freunde Chor.", "tokens": ["Oft", "der", "Freun\u00b7de", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Br\u00fcderlich verbunden", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr der Ehre Wort,", "tokens": ["F\u00fcr", "der", "Eh\u00b7re", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rei\u00dft in Todes Wunden", "tokens": ["Rei\u00dft", "in", "To\u00b7des", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sturm die Edlen fort.", "tokens": ["Sturm", "die", "Ed\u00b7len", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Auf in Ruhmes Flammen", "tokens": ["Auf", "in", "Ruh\u00b7mes", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schl\u00e4gt ihr Herz zusammen", "tokens": ["Schl\u00e4gt", "ihr", "Herz", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zu der Sonne dort.", "tokens": ["Zu", "der", "Son\u00b7ne", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ach dem Vaterlande", "tokens": ["Ach", "dem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wird der Geist nie fern,", "tokens": ["Wird", "der", "Geist", "nie", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ehrt in treuem Bande", "tokens": ["Ehrt", "in", "treu\u00b7em", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Es als seinen Herrn.", "tokens": ["Es", "als", "sei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00fchnen Stolzes schlagen", "tokens": ["K\u00fch\u00b7nen", "Stol\u00b7zes", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Freie Herzen, wagen", "tokens": ["Frei\u00b7e", "Her\u00b7zen", ",", "wa\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Daf\u00fcr alles gern.", "tokens": ["Da\u00b7f\u00fcr", "al\u00b7les", "gern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wo nach altem Rechte", "tokens": ["Wo", "nach", "al\u00b7tem", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fromme Sitte gilt,", "tokens": ["From\u00b7me", "Sit\u00b7te", "gilt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da sind edle M\u00e4chte", "tokens": ["Da", "sind", "ed\u00b7le", "M\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Noch der Freiheit Schild.", "tokens": ["Noch", "der", "Frei\u00b7heit", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jeder stark alleine,", "tokens": ["Je\u00b7der", "stark", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "St\u00e4rker im Vereine,", "tokens": ["St\u00e4r\u00b7ker", "im", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ist des Ganzen Bild.", "tokens": ["Ist", "des", "Gan\u00b7zen", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Doch die h\u00f6chste Liebe", "tokens": ["Doch", "die", "h\u00f6chs\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nimmt wohl andern Lauf;", "tokens": ["Nimmt", "wohl", "an\u00b7dern", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ihr eines bliebe,", "tokens": ["Da\u00df", "ihr", "ei\u00b7nes", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gibt sie alles auf.", "tokens": ["Gibt", "sie", "al\u00b7les", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Irdisch hier in Tr\u00e4nen", "tokens": ["Ir\u00b7disch", "hier", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Steigt ihr sanftes Sehnen", "tokens": ["Steigt", "ihr", "sanf\u00b7tes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dann zum Licht hinauf.", "tokens": ["Dann", "zum", "Licht", "hin\u00b7auf", "."], "token_info": ["word",