{"dta.poem.11132": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schein-heilige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man k\u00fc\u00dfte Julien bey einer compagnie,", "tokens": ["Man", "k\u00fc\u00df\u00b7te", "Ju\u00b7li\u00b7en", "bey", "ei\u00b7ner", "com\u00b7pag\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da seufftzte sie gar tief. Warum es nun geschehen,", "tokens": ["Da", "seufftz\u00b7te", "sie", "gar", "tief", ".", "Wa\u00b7rum", "es", "nun", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befragte sie ein freund: Ich seufftze, sagte sie,", "tokens": ["Be\u00b7frag\u00b7te", "sie", "ein", "freund", ":", "Ich", "seufft\u00b7ze", ",", "sag\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht, da\u00df man mich gek\u00fc\u00dft, nur weil man es gesehen.", "tokens": ["Nicht", ",", "da\u00df", "man", "mich", "ge\u00b7k\u00fc\u00dft", ",", "nur", "weil", "man", "es", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PIS", "PRF", "VVPP", "$,", "ADV", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}