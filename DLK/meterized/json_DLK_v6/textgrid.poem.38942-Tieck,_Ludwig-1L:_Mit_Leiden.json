{"textgrid.poem.38942": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Leiden", "tokens": ["Mit", "Lei\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und Freuden", "tokens": ["Und", "Freu\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gleich lieblich zu spielen,", "tokens": ["Gleich", "lieb\u00b7lich", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Schmerzen", "tokens": ["Und", "Schmer\u00b7zen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Im Scherzen", "tokens": ["Im", "Scher\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "So leise zu f\u00fchlen,", "tokens": ["So", "lei\u00b7se", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ist Wen'gen beschieden;", "tokens": ["Ist", "Wen'\u00b7gen", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sie w\u00e4hlen zum Frieden", "tokens": ["Sie", "w\u00e4h\u00b7len", "zum", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das eine von beiden,", "tokens": ["Das", "ei\u00b7ne", "von", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Sind nicht zu beneiden:", "tokens": ["Sind", "nicht", "zu", "be\u00b7nei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Ach gar zu bescheiden", "tokens": ["Ach", "gar", "zu", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ADV", "PTKZU", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.12": {"text": "Sind doch ihre Freuden", "tokens": ["Sind", "doch", "ih\u00b7re", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Und kaum von Leiden", "tokens": ["Und", "kaum", "von", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Zu unterscheiden. \u2013", "tokens": ["Zu", "un\u00b7ter\u00b7schei\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}