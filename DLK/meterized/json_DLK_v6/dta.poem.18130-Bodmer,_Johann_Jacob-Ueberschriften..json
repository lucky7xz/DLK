{"dta.poem.18130": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was lieb ich doch so sehr die Heiden und die W\u00fcsten?", "tokens": ["Was", "lieb", "ich", "doch", "so", "sehr", "die", "Hei\u00b7den", "und", "die", "W\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was la\u00df ich mich nach den Waldg\u00f6ttinnen gel\u00fcsten?", "tokens": ["Was", "la\u00df", "ich", "mich", "nach", "den", "Wald\u00b7g\u00f6t\u00b7tin\u00b7nen", "ge\u00b7l\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Lieb die \u00fcbertrift doch aller W\u00e4lder Zier,", "tokens": ["Mein", "Lieb", "die", "\u00fc\u00b7bert\u00b7rift", "doch", "al\u00b7ler", "W\u00e4l\u00b7der", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Diana weichet auch an Sch\u00f6nheit selber ihr.", "tokens": ["Di\u00b7a\u00b7na", "wei\u00b7chet", "auch", "an", "Sch\u00f6n\u00b7heit", "sel\u00b7ber", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was la\u00df ich mir so sehr die Bl\u00fcmelein gefallen?", "tokens": ["Was", "la\u00df", "ich", "mir", "so", "sehr", "die", "Bl\u00fc\u00b7me\u00b7lein", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Lieb hat doch allein die Blum der Blumen allen,", "tokens": ["Mein", "Lieb", "hat", "doch", "al\u00b7lein", "die", "Blum", "der", "Blu\u00b7men", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NE", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00dfgleichen nie zuvor ist kommen an den Tag,", "tokens": ["De\u00df\u00b7glei\u00b7chen", "nie", "zu\u00b7vor", "ist", "kom\u00b7men", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VAFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O wie gl\u00fcckselig ist der, so sie brechen mag?", "tokens": ["O", "wie", "gl\u00fcck\u00b7se\u00b7lig", "ist", "der", ",", "so", "sie", "bre\u00b7chen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "$,", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}