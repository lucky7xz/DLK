{"textgrid.poem.58339": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df fahren, was du hast", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df fahren, was du hast", "tokens": ["La\u00df", "fah\u00b7ren", ",", "was", "du", "hast"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Liebe fest gefa\u00dft,", "tokens": ["Mit", "Lie\u00b7be", "fest", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es h\u00e4lt auch dich gebunden!", "tokens": ["Es", "h\u00e4lt", "auch", "dich", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer herzlich, frei und rein", "tokens": ["Wer", "herz\u00b7lich", ",", "frei", "und", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gott zugekehrt kann sein,", "tokens": ["Gott", "zu\u00b7ge\u00b7kehrt", "kann", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Hat bald den Schatz gefunden.", "tokens": ["Hat", "bald", "den", "Schatz", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}