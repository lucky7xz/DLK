{"textgrid.poem.56464": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blaue Schnee liegt auf dem ebenen Land,", "tokens": ["Der", "blau\u00b7e", "Schnee", "liegt", "auf", "dem", "e\u00b7be\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Winter dehnt. Und die Wegweiser zeigen", "tokens": ["Das", "Win\u00b7ter", "dehnt", ".", "Und", "die", "Weg\u00b7wei\u00b7ser", "zei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einander mit der ausgestreckten Hand", "tokens": ["Ein\u00b7an\u00b7der", "mit", "der", "aus\u00b7ge\u00b7streck\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Horizonte violettes Schweigen.", "tokens": ["Der", "Ho\u00b7ri\u00b7zon\u00b7te", "vi\u00b7o\u00b7let\u00b7tes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier treffen sich auf ihrem Weg ins Leere", "tokens": ["Hier", "tref\u00b7fen", "sich", "auf", "ih\u00b7rem", "Weg", "ins", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vier Stra\u00dfen an. Die niedren B\u00e4ume stehen", "tokens": ["Vier", "Stra\u00b7\u00dfen", "an", ".", "Die", "nied\u00b7ren", "B\u00e4u\u00b7me", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Bettler kahl. Das Rot der Vogelbeere", "tokens": ["Wie", "Bett\u00b7ler", "kahl", ".", "Das", "Rot", "der", "Vo\u00b7gel\u00b7bee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gl\u00e4nzt wie ihr Auge tr\u00fcbe. Die Chausseen", "tokens": ["Gl\u00e4nzt", "wie", "ihr", "Au\u00b7ge", "tr\u00fc\u00b7be", ".", "Die", "Chaus\u00b7seen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Verweilen kurz und sprechen aus den \u00c4sten.", "tokens": ["Ver\u00b7wei\u00b7len", "kurz", "und", "spre\u00b7chen", "aus", "den", "\u00c4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann ziehn sie weiter in die Einsamkeit", "tokens": ["Dann", "ziehn", "sie", "wei\u00b7ter", "in", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gen Nord und S\u00fcden und nach Ost und Westen,", "tokens": ["Gen", "Nord", "und", "S\u00fc\u00b7den", "und", "nach", "Ost", "und", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "KON", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo bleicht der niedere Tag der Winterzeit.", "tokens": ["Wo", "bleicht", "der", "nie\u00b7de\u00b7re", "Tag", "der", "Win\u00b7ter\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein hoher Korb mit rissigem Geflecht", "tokens": ["Ein", "ho\u00b7her", "Korb", "mit", "ris\u00b7si\u00b7gem", "Ge\u00b7flecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blieb von der Ernte noch im Ackerfeld.", "tokens": ["Blieb", "von", "der", "Ern\u00b7te", "noch", "im", "A\u00b7cker\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00dfb\u00e4rtig, ein Soldat, der nach Gefecht", "tokens": ["Wei\u00df\u00b7b\u00e4r\u00b7tig", ",", "ein", "Sol\u00b7dat", ",", "der", "nach", "Ge\u00b7fecht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hei\u00dfem Tag der Toten Wache h\u00e4lt.", "tokens": ["Und", "hei\u00b7\u00dfem", "Tag", "der", "To\u00b7ten", "Wa\u00b7che", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Schnee wird bleicher, und der Tag vergeht.", "tokens": ["Der", "Schnee", "wird", "blei\u00b7cher", ",", "und", "der", "Tag", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonne Atem dampft am Firmament,", "tokens": ["Der", "Son\u00b7ne", "A\u00b7tem", "dampft", "am", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Davon das Eis, das in den Lachen steht", "tokens": ["Da\u00b7von", "das", "Eis", ",", "das", "in", "den", "La\u00b7chen", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinab die Stra\u00dfe rot wie Feuer brennt.", "tokens": ["Hin\u00b7ab", "die", "Stra\u00b7\u00dfe", "rot", "wie", "Feu\u00b7er", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}