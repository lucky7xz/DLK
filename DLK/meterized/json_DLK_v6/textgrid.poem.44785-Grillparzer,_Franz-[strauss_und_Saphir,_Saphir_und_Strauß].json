{"textgrid.poem.44785": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[strauss und Saphir, Saphir und Strau\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strauss und Saphir, Saphir und Strau\u00df,", "tokens": ["Strauss", "und", "Sa\u00b7phir", ",", "Sa\u00b7phir", "und", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Aus diesem Wahlspruch kommt ihr nie heraus.", "tokens": ["Aus", "die\u00b7sem", "Wahl\u00b7spruch", "kommt", "ihr", "nie", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sind Saphire hart, wie uns die Kenner sagen,", "tokens": ["Und", "sind", "Sa\u00b7phi\u00b7re", "hart", ",", "wie", "uns", "die", "Ken\u00b7ner", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verdaut wohl mehr als das ein echter Strau\u00dfenmagen.", "tokens": ["Ver\u00b7daut", "wohl", "mehr", "als", "das", "ein", "ech\u00b7ter", "Strau\u00b7\u00dfen\u00b7ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "KOKOM", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}