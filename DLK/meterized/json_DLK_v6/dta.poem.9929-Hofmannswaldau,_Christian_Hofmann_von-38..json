{"dta.poem.9929": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine reise geht nie weit/ ob ich iederzeit schon gehe/", "tokens": ["Mei\u00b7ne", "rei\u00b7se", "geht", "nie", "weit", "/", "ob", "ich", "ie\u00b7der\u00b7zeit", "schon", "ge\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+--++--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Kein mahl sonsten/ als wenn ich bin verderbet/ stille stehe/", "tokens": ["Kein", "mahl", "sons\u00b7ten", "/", "als", "wenn", "ich", "bin", "ver\u00b7der\u00b7bet", "/", "stil\u00b7le", "ste\u00b7he", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADV", "ADV", "$(", "KOKOM", "KOUS", "PPER", "VAFIN", "VVFIN", "$(", "ADJA", "VVFIN", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Meine richtschnur ist die zeit; doch hengt es an einem faden/", "tokens": ["Mei\u00b7ne", "richt\u00b7schnur", "ist", "die", "zeit", ";", "doch", "hengt", "es", "an", "ei\u00b7nem", "fa\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+-+---+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ob ich lange leben kan/ oder mir der tod soll schaden.", "tokens": ["Ob", "ich", "lan\u00b7ge", "le\u00b7ben", "kan", "/", "o\u00b7der", "mir", "der", "tod", "soll", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "KON", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}