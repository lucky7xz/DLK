{"dta.poem.14034": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abendst\u00e4ndchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, den s\u00fc\u00dfen Klang verf\u00fchret", "tokens": ["Ach", ",", "den", "s\u00fc\u00b7\u00dfen", "Klang", "ver\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit der buhlerische Wind,", "tokens": ["Weit", "der", "buh\u00b7le\u00b7ri\u00b7sche", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch Schlo\u00df und Wand ihn sp\u00fcret", "tokens": ["Und", "durch", "Schlo\u00df", "und", "Wand", "ihn", "sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PPER", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Tr\u00e4umend jedes sch\u00f6ne Kind.", "tokens": ["Tr\u00e4u\u00b7mend", "je\u00b7des", "sch\u00f6\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}