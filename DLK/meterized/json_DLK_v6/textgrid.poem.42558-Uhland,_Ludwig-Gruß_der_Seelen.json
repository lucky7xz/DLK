{"textgrid.poem.42558": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df der Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00f6sen sich die ird'schen Bande?", "tokens": ["L\u00f6\u00b7sen", "sich", "die", "ird'\u00b7schen", "Ban\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird auch mir die Schwinge frei?", "tokens": ["Wird", "auch", "mir", "die", "Schwin\u00b7ge", "frei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich in dem Heimatlande,", "tokens": ["Da\u00df", "ich", "in", "dem", "Hei\u00b7mat\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundin, dir vereinigt sei?", "tokens": ["Freun\u00b7din", ",", "dir", "ver\u00b7ei\u00b7nigt", "sei", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja! dein seliges Entschweben", "tokens": ["Ja", "!", "dein", "se\u00b7li\u00b7ges", "Ent\u00b7schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zog mir l\u00e4ngst den Blick empor;", "tokens": ["Zog", "mir", "l\u00e4ngst", "den", "Blick", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt im Lichte, jetzt im Leben", "tokens": ["Jetzt", "im", "Lich\u00b7te", ",", "jetzt", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Find ich, die ich nie verlor. \u2013", "tokens": ["Find", "ich", ",", "die", "ich", "nie", "ver\u00b7lor", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwas vernehm ich? lockst du nieder", "tokens": ["\u00bb", "was", "ver\u00b7nehm", "ich", "?", "lockst", "du", "nie\u00b7der"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder steigst du auf zu mir?", "tokens": ["O\u00b7der", "steigst", "du", "auf", "zu", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lacht mir Erdenfr\u00fchling wieder", "tokens": ["Lacht", "mir", "Er\u00b7den\u00b7fr\u00fch\u00b7ling", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder bl\u00fcht ein sch\u00f6nrer hier?", "tokens": ["O\u00b7der", "bl\u00fcht", "ein", "sch\u00f6n\u00b7rer", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja! in dieser lichten H\u00f6he", "tokens": ["Ja", "!", "in", "die\u00b7ser", "lich\u00b7ten", "H\u00f6\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hast du ", "tokens": ["Hast", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Komm! ich f\u00fchle deine N\u00e4he,", "tokens": ["Komm", "!", "ich", "f\u00fch\u00b7le", "dei\u00b7ne", "N\u00e4\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den Himmel mir beseelt.\u00ab", "tokens": ["Die", "den", "Him\u00b7mel", "mir", "be\u00b7seelt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}