{"textgrid.poem.38571": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Sander", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sander, du scheidest? Jezt da immer b\u00e4nger,", "tokens": ["San\u00b7der", ",", "du", "schei\u00b7dest", "?", "Jezt", "da", "im\u00b7mer", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Immer schw\u00fcler und schw\u00fcler mir der Tag wird,", "tokens": ["Im\u00b7mer", "schw\u00fc\u00b7ler", "und", "schw\u00fc\u00b7ler", "mir", "der", "Tag", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Immer steiler, dornichter, klippenvoller", "tokens": ["Im\u00b7mer", "stei\u00b7ler", ",", "dor\u00b7nich\u00b7ter", ",", "klip\u00b7pen\u00b7vol\u00b7ler"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sich durch des Lebens", "tokens": ["Sich", "durch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "N\u00e4chtliche W\u00fcsten meine Pfade winden,", "tokens": ["N\u00e4cht\u00b7li\u00b7che", "W\u00fcs\u00b7ten", "mei\u00b7ne", "Pfa\u00b7de", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Jeder Schimmer der Hofnung sich verdunkelt,", "tokens": ["Je\u00b7der", "Schim\u00b7mer", "der", "Hof\u00b7nung", "sich", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mir kein Quell mehr Labungen str\u00f6mt, kein k\u00fchler", "tokens": ["Mir", "kein", "Quell", "mehr", "La\u00b7bun\u00b7gen", "str\u00f6mt", ",", "kein", "k\u00fch\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PIAT", "NN", "PIAT", "NN", "VVFIN", "$,", "PIAT", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schatten mehr wehet;", "tokens": ["Schat\u00b7ten", "mehr", "we\u00b7het", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Keines der Thale mehr, wo einst mit Liedern", "tokens": ["Kei\u00b7nes", "der", "Tha\u00b7le", "mehr", ",", "wo", "einst", "mit", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ADV", "$,", "PWAV", "ADV", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wir den rosigen Wonnemond begr\u00fc\u00dften,", "tokens": ["Wir", "den", "ro\u00b7si\u00b7gen", "Won\u00b7ne\u00b7mond", "be\u00b7gr\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In die stille D\u00e4mmerung seiner B\u00e4ume", "tokens": ["In", "die", "stil\u00b7le", "D\u00e4m\u00b7me\u00b7rung", "sei\u00b7ner", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "G\u00fctig mich aufnimmt;", "tokens": ["G\u00fc\u00b7tig", "mich", "auf\u00b7nimmt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Keine der Rosenlauben mich umduftet,", "tokens": ["Kei\u00b7ne", "der", "Ro\u00b7sen\u00b7lau\u00b7ben", "mich", "um\u00b7duf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo dem Liede der Nachtigall wir horchten,", "tokens": ["Wo", "dem", "Lie\u00b7de", "der", "Nach\u00b7ti\u00b7gall", "wir", "horch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn im Schimmer wallender Westgew\u00f6lke", "tokens": ["Wenn", "im", "Schim\u00b7mer", "wal\u00b7len\u00b7der", "West\u00b7ge\u00b7w\u00f6l\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hesper erwachte!", "tokens": ["Hes\u00b7per", "er\u00b7wach\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Sanftes Entz\u00fccken, Ruh' und Seelenstille", "tokens": ["Sanf\u00b7tes", "Ent\u00b7z\u00fc\u00b7cken", ",", "Ruh'", "und", "See\u00b7len\u00b7stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wehte, von des umb\u00fcschten Seees Ufern,", "tokens": ["Weh\u00b7te", ",", "von", "des", "um\u00b7b\u00fcschten", "See\u00b7es", "U\u00b7fern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dann des Abends thauender Purpurfittig", "tokens": ["Dann", "des", "A\u00b7bends", "thau\u00b7en\u00b7der", "Pur\u00b7pur\u00b7fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zu uns her\u00fcber!", "tokens": ["Zu", "uns", "her\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Hauche des Fr\u00fchlings bebten durch die Erlen,", "tokens": ["Hau\u00b7che", "des", "Fr\u00fch\u00b7lings", "beb\u00b7ten", "durch", "die", "Er\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Beugten lispelnd der jungen Blumenwiese", "tokens": ["Beug\u00b7ten", "lis\u00b7pelnd", "der", "jun\u00b7gen", "Blu\u00b7men\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zarte Halme, wiegten sich auf des Seees", "tokens": ["Zar\u00b7te", "Hal\u00b7me", ",", "wieg\u00b7ten", "sich", "auf", "des", "See\u00b7es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Silbernen Wellen!", "tokens": ["Sil\u00b7ber\u00b7nen", "Wel\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Ach! so erbebten unsre Seelen, Bester!", "tokens": ["Ach", "!", "so", "er\u00b7beb\u00b7ten", "uns\u00b7re", "See\u00b7len", ",", "Bes\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So durchwandelt' uns leiser Ahndung Schauer,", "tokens": ["So", "durch\u00b7wan\u00b7delt'", "uns", "lei\u00b7ser", "Ahn\u00b7dung", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann dein Flammengenius, o Begeistrung!", "tokens": ["Wann", "dein", "Flam\u00b7men\u00b7ge\u00b7nius", ",", "o", "Be\u00b7geis\u00b7trung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nun uns umschwebte.", "tokens": ["Nun", "uns", "um\u00b7schweb\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wenn wir, geschlungen Arm in Arm, der Bl\u00fcthen,", "tokens": ["Wenn", "wir", ",", "ge\u00b7schlun\u00b7gen", "Arm", "in", "Arm", ",", "der", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und des wehenden Grases und der Saaten,", "tokens": ["Und", "des", "we\u00b7hen\u00b7den", "Gra\u00b7ses", "und", "der", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die den gr\u00fcnen H\u00fcgel hinunter wallten,", "tokens": ["Die", "den", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", "hin\u00b7un\u00b7ter", "wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPO", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herzlich uns freuten!", "tokens": ["Herz\u00b7lich", "uns", "freu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn uns der Thauduft und des Baches Rauschen,", "tokens": ["Wenn", "uns", "der", "Thau\u00b7duft", "und", "des", "Ba\u00b7ches", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und des steigenden Mondes stilles Antliz,", "tokens": ["Und", "des", "stei\u00b7gen\u00b7den", "Mon\u00b7des", "stil\u00b7les", "Ant\u00b7liz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und der Sterne Reigen in Sommern\u00e4chten", "tokens": ["Und", "der", "Ster\u00b7ne", "Rei\u00b7gen", "in", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Himmlisch entz\u00fcckte!", "tokens": ["Himm\u00b7lisch", "ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Wenn wir im Weidenthale dich, o Elbe!", "tokens": ["Wenn", "wir", "im", "Wei\u00b7dent\u00b7ha\u00b7le", "dich", ",", "o", "El\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit gefl\u00fcgelter Eil vor\u00fcbergleiten", "tokens": ["Mit", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "Eil", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sahn, und ahndend seufzten: Ach! so wird alles,", "tokens": ["Sahn", ",", "und", "ahn\u00b7dend", "seufz\u00b7ten", ":", "Ach", "!", "so", "wird", "al\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "VVFIN", "$.", "ITJ", "$.", "ADV", "VAFIN", "PIS", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Alles dahingehn!", "tokens": ["Al\u00b7les", "da\u00b7hin\u00b7gehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wehe! dahingerauscht mit Wetterschnelle", "tokens": ["We\u00b7he", "!", "da\u00b7hin\u00b7ge\u00b7rauscht", "mit", "Wet\u00b7ter\u00b7schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sind die Stunden der Freundschaft und der Liebe!", "tokens": ["Sind", "die", "Stun\u00b7den", "der", "Freund\u00b7schaft", "und", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Keine Klage, Sander, ach! keine Thr\u00e4ne", "tokens": ["Kei\u00b7ne", "Kla\u00b7ge", ",", "San\u00b7der", ",", "ach", "!", "kei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "NE", "$,", "ITJ", "$.", "PIAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bringt sie uns wieder!", "tokens": ["Bringt", "sie", "uns", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Scheidet der Winter nicht des Haines Bl\u00e4tter", "tokens": ["Schei\u00b7det", "der", "Win\u00b7ter", "nicht", "des", "Hai\u00b7nes", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von dem Zweige der sie gebar auf ewig?", "tokens": ["Von", "dem", "Zwei\u00b7ge", "der", "sie", "ge\u00b7bar", "auf", "e\u00b7wig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "ADJD", "APPR", "ADJD", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kehrt zur Mutterquelle des Stromes Woge", "tokens": ["Kehrt", "zur", "Mut\u00b7ter\u00b7quel\u00b7le", "des", "Stro\u00b7mes", "Wo\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Jemals wohl wieder?", "tokens": ["Je\u00b7mals", "wohl", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Edler! wie war mir's wohl an deinem Busen!", "tokens": ["Ed\u00b7ler", "!", "wie", "war", "mir's", "wohl", "an", "dei\u00b7nem", "Bu\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie beseligend str\u00f6mte deine Rede", "tokens": ["Wie", "be\u00b7se\u00b7li\u00b7gend", "str\u00f6m\u00b7te", "dei\u00b7ne", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ruhe, Tr\u00f6stung, Ahndungen, Himmelsfreuden", "tokens": ["Ru\u00b7he", ",", "Tr\u00f6s\u00b7tung", ",", "Ahn\u00b7dun\u00b7gen", ",", "Him\u00b7mels\u00b7freu\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mir in die Seele!", "tokens": ["Mir", "in", "die", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanz