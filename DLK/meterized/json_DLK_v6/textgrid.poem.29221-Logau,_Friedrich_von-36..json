{"textgrid.poem.29221": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du must f\u00fcrher wol stechen,", "tokens": ["Du", "must", "f\u00fcr\u00b7her", "wol", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll Anwalt f\u00fcr dich sprechen;", "tokens": ["Soll", "An\u00b7walt", "f\u00fcr", "dich", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesetze wird er bringen,", "tokens": ["Ge\u00b7set\u00b7ze", "wird", "er", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach dem die M\u00fcntzen klingen;", "tokens": ["Nach", "dem", "die", "M\u00fcnt\u00b7zen", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am besten ist gerathen", "tokens": ["Am", "bes\u00b7ten", "ist", "ge\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit denen f\u00fcr Ducaten.", "tokens": ["Mit", "de\u00b7nen", "f\u00fcr", "Du\u00b7ca\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}