{"textgrid.poem.47231": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt', ich st\u00fcnd' auf einem Felseilande", "tokens": ["Mir", "tr\u00e4umt'", ",", "ich", "st\u00fcnd'", "auf", "ei\u00b7nem", "Fel\u00b7sei\u00b7lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein mit mir, und wie aus Nebelflore", "tokens": ["Al\u00b7lein", "mit", "mir", ",", "und", "wie", "aus", "Ne\u00b7bel\u00b7flo\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sp\u00e4ht' ich mit Augen, horcht' ich mit dem Ohre", "tokens": ["Sp\u00e4ht'", "ich", "mit", "Au\u00b7gen", ",", "horcht'", "ich", "mit", "dem", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hin\u00fcber fern nach einem festen Lande;", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "fern", "nach", "ei\u00b7nem", "fes\u00b7ten", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und s\u00e4he, wie sie dr\u00fcben an dem Strande", "tokens": ["Und", "s\u00e4\u00b7he", ",", "wie", "sie", "dr\u00fc\u00b7ben", "an", "dem", "Stran\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einz\u00e4unten G\u00e4rten, bauten H\u00e4userthore,", "tokens": ["Ein\u00b7z\u00e4un\u00b7ten", "G\u00e4r\u00b7ten", ",", "bau\u00b7ten", "H\u00e4u\u00b7ser\u00b7tho\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Brautreih'n gingen und im Leichenchore", "tokens": ["Im", "Brau\u00b7treih'n", "gin\u00b7gen", "und", "im", "Lei\u00b7chen\u00b7cho\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lebten, webten, all in ihrem Tande.", "tokens": ["Und", "leb\u00b7ten", ",", "web\u00b7ten", ",", "all", "in", "ih\u00b7rem", "Tan\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr dumpfes Summen drang zu mir her\u00fcber,", "tokens": ["Ihr", "dum\u00b7pfes", "Sum\u00b7men", "drang", "zu", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sahn sie mich nicht stehn in meiner Ferne,", "tokens": ["Doch", "sahn", "sie", "mich", "nicht", "stehn", "in", "mei\u00b7ner", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie hatten keine Zeit zum Sehen \u00fcber.", "tokens": ["Sie", "hat\u00b7ten", "kei\u00b7ne", "Zeit", "zum", "Se\u00b7hen", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch zugerufen h\u00e4tt' ich ihnen gerne,", "tokens": ["Auch", "zu\u00b7ge\u00b7ru\u00b7fen", "h\u00e4tt'", "ich", "ih\u00b7nen", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch drang mein Laut zu ihnen nicht hin\u00fcber,", "tokens": ["Doch", "drang", "mein", "Laut", "zu", "ih\u00b7nen", "nicht", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "APPR", "APPR", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und einsam blickt' ich auf zu meinem Sterne.", "tokens": ["Und", "ein\u00b7sam", "blickt'", "ich", "auf", "zu", "mei\u00b7nem", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}