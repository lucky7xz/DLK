{"textgrid.poem.63376": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Als sie die ihr geschenkte Kristallflasche", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brechen sich im Glas die Strahlen,", "tokens": ["Bre\u00b7chen", "sich", "im", "Glas", "die", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht das Glas sich in den Strahlen?", "tokens": ["Bricht", "das", "Glas", "sich", "in", "den", "Strah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nzt dein Auge in der Sonne,", "tokens": ["Gl\u00e4nzt", "dein", "Au\u00b7ge", "in", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt die Sonn' in deinem Auge?", "tokens": ["Gl\u00e4nzt", "die", "Sonn'", "in", "dei\u00b7nem", "Au\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebt dein Herz mich? Herzt mich deine", "tokens": ["Liebt", "dein", "Herz", "mich", "?", "Herzt", "mich", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$.", "VVFIN", "PPER", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe? Seliges Verd\u00e4mmern:", "tokens": ["Lie\u00b7be", "?", "Se\u00b7li\u00b7ges", "Ver\u00b7d\u00e4m\u00b7mern", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn wir sterben unser Leben", "tokens": ["Denn", "wir", "ster\u00b7ben", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wir leben unsren Tod.", "tokens": ["Und", "wir", "le\u00b7ben", "un\u00b7sren", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}