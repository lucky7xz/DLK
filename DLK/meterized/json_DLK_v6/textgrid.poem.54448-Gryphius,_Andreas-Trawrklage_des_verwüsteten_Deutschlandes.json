{"textgrid.poem.54448": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Trawrklage des verw\u00fcsteten Deutschlandes", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind doch numehr gantz/ ja mehr al\u00df gantz vertorben.", "tokens": ["Wir", "sind", "doch", "nu\u00b7mehr", "gantz", "/", "ja", "mehr", "al\u00df", "gantz", "ver\u00b7tor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$(", "ADV", "PIAT", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frechen V\u00f6lcker schar/ die rasende Posaun/", "tokens": ["Der", "fre\u00b7chen", "V\u00f6l\u00b7cker", "schar", "/", "die", "ra\u00b7sen\u00b7de", "Po\u00b7saun", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df vom Blutt feiste Schwerd/ die donnernde Carthaun/", "tokens": ["Da\u00df", "vom", "Blutt", "feis\u00b7te", "Schwerd", "/", "die", "don\u00b7nern\u00b7de", "Car\u00b7thaun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJD", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Hat alles di\u00df hinweg/ was mancher fawr erworben/", "tokens": ["Hat", "al\u00b7les", "di\u00df", "hin\u00b7weg", "/", "was", "man\u00b7cher", "fawr", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDS", "PTKVZ", "$(", "PWS", "PIS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die alte Redligkeit vnnd Tugend ist gestorben;", "tokens": ["Die", "al\u00b7te", "Red\u00b7lig\u00b7keit", "vnnd", "Tu\u00b7gend", "ist", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Kirchen sind vorheert/ die Starcken vmbgehawn/", "tokens": ["Die", "Kir\u00b7chen", "sind", "vor\u00b7heert", "/", "die", "Star\u00b7cken", "vmb\u00b7ge\u00b7hawn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Jungfrawn sind gesch\u00e4nd; vnd wo wir hin nur schawn/", "tokens": ["Die", "Jung\u00b7frawn", "sind", "ge\u00b7sch\u00e4nd", ";", "vnd", "wo", "wir", "hin", "nur", "schawn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "KON", "PWAV", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist Fewr/ Pest/ Mord vnd Todt/ hier zwischen Schantz v\u00f1 Korb\u1ebd", "tokens": ["Ist", "Fewr", "/", "Pest", "/", "Mord", "vnd", "Todt", "/", "hier", "zwi\u00b7schen", "Schantz", "v\u00f1", "Kor\u00b7b\u1ebd"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$(", "NE", "$(", "NN", "KON", "NN", "$(", "ADV", "APPR", "NE", "NE", "NE"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Dort zwischen Mawr v\u00f1 Stad/ rint allzeit frisches Blutt", "tokens": ["Dort", "zwi\u00b7schen", "Mawr", "v\u00f1", "Stad", "/", "rint", "all\u00b7zeit", "fri\u00b7sches", "Blutt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE", "NE", "$(", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dreymal sind schon sechs Jahr als vnser Str\u00f6me Flutt", "tokens": ["Drey\u00b7mal", "sind", "schon", "sechs", "Jahr", "als", "vn\u00b7ser", "Str\u00f6\u00b7me", "Flutt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "CARD", "NN", "KOKOM", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von so viel Leichen schwer/ sich langsam fortgedrungen.", "tokens": ["Von", "so", "viel", "Lei\u00b7chen", "schwer", "/", "sich", "lang\u00b7sam", "fort\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADJD", "$(", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich schweige noch von dehm/ was st\u00e4rcker als der Todt/", "tokens": ["Ich", "schwei\u00b7ge", "noch", "von", "dehm", "/", "was", "st\u00e4r\u00b7cker", "als", "der", "Todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$(", "PWS", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(du Stra\u00dfburg weist es wol) der grimmen Hungersnoth/", "tokens": ["(", "du", "Stra\u00df\u00b7burg", "weist", "es", "wol", ")", "der", "grim\u00b7men", "Hun\u00b7gers\u00b7noth", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd da\u00df der Seelen-Schatz gar vielen abgezwungen.", "tokens": ["Vnd", "da\u00df", "der", "See\u00b7len\u00b7Schatz", "gar", "vie\u00b7len", "ab\u00b7ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}