{"textgrid.poem.31653": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "25. O Caesar, O Nulla", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorgestecktes Tugend Ziel! wo nicht dich / den Tod erworben!", "tokens": ["Vor\u00b7ge\u00b7steck\u00b7tes", "Tu\u00b7gend", "Ziel", "!", "wo", "nicht", "dich", "/", "den", "Tod", "er\u00b7wor\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "PWAV", "PTKNEG", "PPER", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lorbeer- Zweige zieren mich lebend / oder doch gestorben", "tokens": ["Lor\u00b7beer", "Zwei\u00b7ge", "zie\u00b7ren", "mich", "le\u00b7bend", "/", "o\u00b7der", "doch", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["TRUNC", "NN", "VVFIN", "PPER", "ADJD", "$(", "KON", "ADV", "VVPP"], "meter": "+-+-+--+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "lieber / als ohn' Ehr' / ohn Leben will ich seyn zu aller Zeit.", "tokens": ["lie\u00b7ber", "/", "als", "ohn'", "Ehr'", "/", "ohn", "Le\u00b7ben", "will", "ich", "seyn", "zu", "al\u00b7ler", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "APPR", "NN", "$(", "APPR", "NN", "VMFIN", "PPER", "VAINF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Adlers-Lust ist in der Sonne / in der Tugend meine Freud.", "tokens": ["Ad\u00b7ler\u00b7sLust", "ist", "in", "der", "Son\u00b7ne", "/", "in", "der", "Tu\u00b7gend",