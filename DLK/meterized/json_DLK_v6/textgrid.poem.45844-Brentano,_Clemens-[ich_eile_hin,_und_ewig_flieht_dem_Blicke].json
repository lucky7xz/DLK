{"textgrid.poem.45844": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich eile hin, und ewig flieht dem Blicke]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich eile hin, und ewig flieht dem Blicke", "tokens": ["Ich", "ei\u00b7le", "hin", ",", "und", "e\u00b7wig", "flieht", "dem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Spiegel fort in wilder Flut,", "tokens": ["Des", "Le\u00b7bens", "Spie\u00b7gel", "fort", "in", "wil\u00b7der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sehnsucht in die Ferne nimmer ruht,", "tokens": ["Die", "Sehn\u00b7sucht", "in", "die", "Fer\u00b7ne", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weinend schaut Erinnerung zur\u00fccke", "tokens": ["Und", "wei\u00b7nend", "schaut", "E\u00b7rin\u00b7ne\u00b7rung", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da blickt aus einer Blume neu Geschicke.", "tokens": ["Da", "blickt", "aus", "ei\u00b7ner", "Blu\u00b7me", "neu", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zwei blaue Kelche voll von Liebesglut", "tokens": ["Zwei", "blau\u00b7e", "Kel\u00b7che", "voll", "von", "Lie\u00b7bes\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erwecken in dem Fl\u00fcchtling neuen Mut;", "tokens": ["Er\u00b7we\u00b7cken", "in", "dem", "Fl\u00fccht\u00b7ling", "neu\u00b7en", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df er das Leben wieder jung erblicke.", "tokens": ["Da\u00df", "er", "das", "Le\u00b7ben", "wie\u00b7der", "jung", "er\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es hat der Sinn die Aussicht wiederfunden,", "tokens": ["Es", "hat", "der", "Sinn", "die", "Aus\u00b7sicht", "wie\u00b7der\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Er sieht im klaren Strome abgespiegelt,", "tokens": ["Er", "sieht", "im", "kla\u00b7ren", "Stro\u00b7me", "ab\u00b7ge\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Wechsel-Lebens zwiefach-lieblich Bild,", "tokens": ["Des", "Wech\u00b7sel\u00b7Le\u00b7bens", "zwie\u00b7fach\u00b7lieb\u00b7lich", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Fl\u00e4che ruht und schwillt in tiefen Stunden,", "tokens": ["Die", "Fl\u00e4\u00b7che", "ruht", "und", "schwillt", "in", "tie\u00b7fen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn Leidenschaft die Trunkenheit entz\u00fcgelt,", "tokens": ["Wenn", "Lei\u00b7den\u00b7schaft", "die", "Trun\u00b7ken\u00b7heit", "ent\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Liebe sich dem Strome nackt enth\u00fcllt.", "tokens": ["Und", "Lie\u00b7be", "sich", "dem", "Stro\u00b7me", "nackt", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}