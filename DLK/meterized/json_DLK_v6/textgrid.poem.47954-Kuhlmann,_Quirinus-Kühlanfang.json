{"textgrid.poem.47954": {"metadata": {"author": {"name": "Kuhlmann, Quirinus", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fchlanfang", "genre": "verse", "period": "N.A.", "pub_year": 1670, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdr\u00fckkte B\u00fcrgerschafft des virichten Cosmopel!", "tokens": ["Ver\u00b7dr\u00fckk\u00b7te", "B\u00fcr\u00b7ger\u00b7schafft", "des", "vi\u00b7rich\u00b7ten", "Cos\u00b7mo\u00b7pel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr ausgestossene vom neu Fridericopel!", "tokens": ["Ihr", "aus\u00b7ge\u00b7stos\u00b7se\u00b7ne", "vom", "neu", "Fri\u00b7de\u00b7ri\u00b7co\u00b7pel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Di stund ist endlich da zubauen ", "tokens": ["Di", "stund", "ist", "end\u00b7lich", "da", "zu\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich komm in deiner st\u00e4rk! Du hast mich hergesand,", "tokens": ["Ich", "komm", "in", "dei\u00b7ner", "st\u00e4rk", "!", "Du", "hast", "mich", "her\u00b7ge\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das deine krafft erschall in mir durch alle land!", "tokens": ["Das", "dei\u00b7ne", "krafft", "er\u00b7schall", "in", "mir", "durch", "al\u00b7le", "land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kam und s\u00e4umte nicht! Drum f\u00fchre meine Weg!", "tokens": ["Ich", "kam", "und", "s\u00e4um\u00b7te", "nicht", "!", "Drum", "f\u00fch\u00b7re", "mei\u00b7ne", "Weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$.", "PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun leite neu, mein Gott, durch deine Wundersteg!", "tokens": ["Nun", "lei\u00b7te", "neu", ",", "mein", "Gott", ",", "durch", "dei\u00b7ne", "Wun\u00b7ders\u00b7teg", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Vater, sei mit mir! Nichts sei, das mich anreg!", "tokens": ["Mein", "Va\u00b7ter", ",", "sei", "mit", "mir", "!", "Nichts", "sei", ",", "das", "mich", "an\u00b7reg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "APPR", "PPER", "$.", "PIS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was an dem ", "tokens": ["Was", "an", "dem"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dis stellt mir ", "tokens": ["Dis", "stellt", "mir"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was noch nicht ", "tokens": ["Was", "noch", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Als es in ", "tokens": ["Als", "es", "in"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Was noch an thron und ", "tokens": ["Was", "noch", "an", "thron", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NE", "KON"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dis hat, Gottlob, ", "tokens": ["Dis", "hat", ",", "Gott\u00b7lob", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "NN", "$,"], "meter": "+---", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Auf, ", "tokens": ["Auf", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Si flihen, wi wir flohn! Es triffet si das wesen!", "tokens": ["Si", "fli\u00b7hen", ",", "wi", "wir", "flohn", "!", "Es", "trif\u00b7fet", "si", "das", "we\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit ihren St\u00e4dten, L\u00e4ndern, Printzen!", "tokens": ["Mit", "ih\u00b7ren", "St\u00e4d\u00b7ten", ",", "L\u00e4n\u00b7dern", ",", "Print\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verflucht war di Figur: Verflucht ist ihre Frucht:", "tokens": ["Ver\u00b7flucht", "war", "di", "Fi\u00b7gur", ":", "Ver\u00b7flucht", "ist", "ih\u00b7re", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NN", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Si wird verfluchterer noch idem augenblikk.", "tokens": ["Si", "wird", "ver\u00b7fluch\u00b7te\u00b7rer", "noch", "i\u00b7dem", "au\u00b7gen\u00b7blikk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wi schrekklich haben sich bezeichet di ", "tokens": ["Wi", "schrek\u00b7klich", "ha\u00b7ben", "sich", "be\u00b7zei\u00b7chet", "di"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PRF", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weh dem Jahrhundert, weh, das drauf dem Feind gefolgt!", "tokens": ["Weh", "dem", "Jahr\u00b7hun\u00b7dert", ",", "weh", ",", "das", "drauf", "dem", "Feind", "ge\u00b7folgt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKVZ", "$,", "PRELS", "PAV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}