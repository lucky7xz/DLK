{"dta.poem.13181": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia merckte wohl, was mein Gem\u00fcth empfand;", "tokens": ["Tha\u00b7lia", "merck\u00b7te", "wohl", ",", "was", "mein", "Ge\u00b7m\u00fcth", "emp\u00b7fand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dahero nahm sie mich mit Eifer bey der Hand", "tokens": ["Da\u00b7he\u00b7ro", "nahm", "sie", "mich", "mit", "Ei\u00b7fer", "bey", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zeigte selber fast, was ich nun wircklich dachte;", "tokens": ["Und", "zeig\u00b7te", "sel\u00b7ber", "fast", ",", "was", "ich", "nun", "wir\u00b7ck\u00b7lich", "dach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da sie durch ihr Gespr\u00e4ch mich stets abwendig machte.", "tokens": ["Da", "sie", "durch", "ihr", "Ge\u00b7spr\u00e4ch", "mich", "stets", "ab\u00b7wen\u00b7dig", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Mithin wand\u2019 ich den Blick, wie sie befahl, zum Thron,", "tokens": ["Mi\u00b7thin", "wand'", "ich", "den", "Blick", ",", "wie", "sie", "be\u00b7fahl", ",", "zum", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "530Und kame seit der Zeit auch nimmermehr davon.", "tokens": ["ka\u00b7me", "seit", "der", "Zeit", "auch", "nim\u00b7mer\u00b7mehr", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "PAV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "\u201esieh die Gro\u00dfm\u00e4chtigsten \u201e, sprach sie, den Thron besteigen;", "tokens": ["\u201e", "sieh", "die", "Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ten", "\u201e", ",", "sprach", "sie", ",", "den", "Thron", "be\u00b7stei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "\u201eden \u00fcbrigen den Plaz auf beyden Seiten zeigen.", "tokens": ["\u201e", "den", "\u00fcb\u00b7ri\u00b7gen", "den", "Plaz", "auf", "bey\u00b7den", "Sei\u00b7ten", "zei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf einem K\u00f6nigs-Thron drey Majest\u00e4ten sehn,", "tokens": ["Auf", "ei\u00b7nem", "K\u00f6\u00b7nigs\u00b7Thron", "drey", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo Tugenden, dacht\u2019 ich, anstatt der R\u00e4the stehn:", "tokens": ["Wo", "Tu\u00b7gen\u00b7den", ",", "dacht'", "ich", ",", "an\u00b7statt", "der", "R\u00e4\u00b7the", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "535Wo sieht man einen Thron, den solche Sch\u00e4ze zieren,", "tokens": ["sieht", "man", "ei\u00b7nen", "Thron", ",", "den", "sol\u00b7che", "Sch\u00e4\u00b7ze", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Als wo ", "tokens": ["Als", "wo"], "token_info": ["word", "word"], "pos": ["KOUS", "PWAV"], "meter": "-+", "measure": "iambic.single"}}}}}