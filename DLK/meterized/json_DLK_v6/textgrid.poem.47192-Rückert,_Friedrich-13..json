{"textgrid.poem.47192": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr zwei ungl\u00fccksel'ge Folianten,", "tokens": ["Ihr", "zwei", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ge", "Fo\u00b7li\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kehrt zur\u00fcck zu eueren Verwandten", "tokens": ["Kehrt", "zu\u00b7r\u00fcck", "zu", "eu\u00b7e\u00b7ren", "Ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In der dunklen Kammer, den Verbannten!", "tokens": ["In", "der", "dunk\u00b7len", "Kam\u00b7mer", ",", "den", "Ver\u00b7bann\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr von allen aus dem staub'gen Neste", "tokens": ["Ihr", "von", "al\u00b7len", "aus", "dem", "staub'\u00b7gen", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wart hervorgeholt als stumme G\u00e4ste,", "tokens": ["Wart", "her\u00b7vor\u00b7ge\u00b7holt", "als", "stum\u00b7me", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Beizuwohnen dem Familienfeste.", "tokens": ["Bei\u00b7zu\u00b7woh\u00b7nen", "dem", "Fa\u00b7mi\u00b7li\u00b7en\u00b7fes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn am Tisch die gr\u00f6\u00dfern Kleinen sa\u00dfen,", "tokens": ["Wenn", "am", "Tisch", "die", "gr\u00f6\u00b7\u00dfern", "Klei\u00b7nen", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deren Glieder lang genug schon ma\u00dfen,", "tokens": ["De\u00b7ren", "Glie\u00b7der", "lang", "ge\u00b7nug", "schon", "ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fertig zu handhaben, was sie a\u00dfen;", "tokens": ["Fer\u00b7tig", "zu", "hand\u00b7ha\u00b7ben", ",", "was", "sie", "a\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Waren zwei zu klein noch f\u00fcr die St\u00fchle", "tokens": ["Wa\u00b7ren", "zwei", "zu", "klein", "noch", "f\u00fcr", "die", "St\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "CARD", "PTKA", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und bedurften Unters\u00e4tz' und Pf\u00fchle,", "tokens": ["Und", "be\u00b7durf\u00b7ten", "Un\u00b7ter\u00b7s\u00e4tz'", "und", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df in Gang k\u00e4m' ihres Mundwerks M\u00fchle.", "tokens": ["Da\u00df", "in", "Gang", "k\u00e4m'", "ih\u00b7res", "Mund\u00b7werks", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und ihr beiden mit dem breiten Witze", "tokens": ["Und", "ihr", "bei\u00b7den", "mit", "dem", "brei\u00b7ten", "Wit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PIAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dientet ihnen zu erh\u00f6hn die Sitze:", "tokens": ["Dien\u00b7tet", "ih\u00b7nen", "zu", "er\u00b7h\u00f6hn", "die", "Sit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das war euerer Bestimmung Spitze.", "tokens": ["Das", "war", "eu\u00b7e\u00b7rer", "Be\u00b7stim\u00b7mung", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und ihr habt mit weislicher Anstaltung,", "tokens": ["Und", "ihr", "habt", "mit", "weis\u00b7li\u00b7cher", "An\u00b7stal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ohne eurer Weisheitssch\u00e4tz' Entfaltung", "tokens": ["Oh\u00b7ne", "eu\u00b7rer", "Weis\u00b7heits\u00b7sch\u00e4tz'", "Ent\u00b7fal\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Beigetragen so zur Unterhaltung.", "tokens": ["Bei\u00b7ge\u00b7tra\u00b7gen", "so", "zur", "Un\u00b7ter\u00b7hal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Euch bezeug' ich's, da\u00df ihr euch verhieltet", "tokens": ["Euch", "be\u00b7zeug'", "ich's", ",", "da\u00df", "ihr", "euch", "ver\u00b7hiel\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Treu und brav im Dienst, den ihr erhieltet,", "tokens": ["Treu", "und", "brav", "im", "Dienst", ",", "den", "ihr", "er\u00b7hiel\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Still und fest euch ihnen unterhieltet.", "tokens": ["Still", "und", "fest", "euch", "ih\u00b7nen", "un\u00b7ter\u00b7hiel\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nicht durch eure Schuld sind sie gefallen,", "tokens": ["Nicht", "durch", "eu\u00b7re", "Schuld", "sind", "sie", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sondern weil es dem hat wohlgefallen,", "tokens": ["Son\u00b7dern", "weil", "es", "dem", "hat", "wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der gewollt, uns sollte Weh befallen.", "tokens": ["Der", "ge\u00b7wollt", ",", "uns", "soll\u00b7te", "Weh", "be\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMPP", "$,", "PPER", "VMFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Doch um euer Amt ist's hier geschehen,", "tokens": ["Doch", "um", "eu\u00b7er", "Amt", "ist's", "hier", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht vor Augen kann ich mehr euch sehen", "tokens": ["Nicht", "vor", "Au\u00b7gen", "kann", "ich", "mehr", "euch", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den St\u00fchlen, die uns leer nun stehen.", "tokens": ["Auf", "den", "St\u00fch\u00b7len", ",", "die", "uns", "leer", "nun", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Geht hinweg mit zugeklappter Klammer,", "tokens": ["Geht", "hin\u00b7weg", "mit", "zu\u00b7ge\u00b7klapp\u00b7ter", "Klam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euern Br\u00fcdern in der dunklen Kammer", "tokens": ["Eu\u00b7ern", "Br\u00fc\u00b7dern", "in", "der", "dunk\u00b7len", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Anzusagen unsers Hauses Jammer!", "tokens": ["An\u00b7zu\u00b7sa\u00b7gen", "un\u00b7sers", "Hau\u00b7ses", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}