{"textgrid.poem.25972": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Leute sehen mich lichterloh", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bisher war der Tag eine Leier von Stunden,", "tokens": ["Bis\u00b7her", "war", "der", "Tag", "ei\u00b7ne", "Lei\u00b7er", "von", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hast mir des Augenblicks Wollust erfunden.", "tokens": ["Hast", "mir", "des", "Au\u00b7gen\u00b7blicks", "Wol\u00b7lust", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Von dir ward ich erst zur Welt gebracht,", "tokens": ["Von", "dir", "ward", "ich", "erst", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hast mich mit Liebe liebend erdacht.", "tokens": ["Hast", "mich", "mit", "Lie\u00b7be", "lie\u00b7bend", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Die Leute sehen mich lichterloh,", "tokens": ["Die", "Leu\u00b7te", "se\u00b7hen", "mich", "lich\u00b7ter\u00b7loh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich brenne so gr\u00fcndlich und brenne so froh.", "tokens": ["Ich", "bren\u00b7ne", "so", "gr\u00fcnd\u00b7lich", "und", "bren\u00b7ne", "so", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}