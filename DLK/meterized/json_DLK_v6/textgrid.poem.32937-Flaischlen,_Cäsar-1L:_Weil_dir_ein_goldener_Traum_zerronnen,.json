{"textgrid.poem.32937": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil dir ein goldener Traum zerronnen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil dir ein goldener Traum zerronnen,", "tokens": ["Weil", "dir", "ein", "gol\u00b7de\u00b7ner", "Traum", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "was hast du drum f\u00fcr herbe Qual?!", "tokens": ["was", "hast", "du", "drum", "f\u00fcr", "her\u00b7be", "Qual", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es ist doch nicht das erste Mal,", "tokens": ["es", "ist", "doch", "nicht", "das", "ers\u00b7te", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df dich entt\u00e4uscht, was du begonnen!", "tokens": ["da\u00df", "dich", "ent\u00b7t\u00e4uscht", ",", "was", "du", "be\u00b7gon\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Kopf hoch! auf! wozu verzagen", "tokens": ["Den", "Kopf", "hoch", "!", "auf", "!", "wo\u00b7zu", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "PTKVZ", "$.", "PWAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "kleingl\u00e4ubig gleich und hoffnungslos?!", "tokens": ["klein\u00b7gl\u00e4u\u00b7big", "gleich", "und", "hoff\u00b7nungs\u00b7los", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dein Mut schien doch so riesengro\u00df,", "tokens": ["dein", "Mut", "schien", "doch", "so", "rie\u00b7sen\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Letzte selber k\u00fchn zu wagen!", "tokens": ["das", "Letz\u00b7te", "sel\u00b7ber", "k\u00fchn", "zu", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf drum und weiter! ohne Bangen!", "tokens": ["Auf", "drum", "und", "wei\u00b7ter", "!", "oh\u00b7ne", "Ban\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PAV", "KON", "ADV", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und wenn's dir noch soviel entlaubt!", "tokens": ["und", "wenn's", "dir", "noch", "so\u00b7viel", "ent\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer will und an sein K\u00f6nnen glaubt,", "tokens": ["Wer", "will", "und", "an", "sein", "K\u00f6n\u00b7nen", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wird immer an sein Ziel gelangen!", "tokens": ["wird", "im\u00b7mer", "an", "sein", "Ziel", "ge\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}