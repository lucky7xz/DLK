{"dta.poem.5391": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann nun der Sonnen diese Kraft", "tokens": ["Wann", "nun", "der", "Son\u00b7nen", "die\u00b7se", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem, der alle Dinge schaft,", "tokens": ["Von", "dem", ",", "der", "al\u00b7le", "Din\u00b7ge", "schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu seiner Ehr und unsrer Lust, gegeben;", "tokens": ["Zu", "sei\u00b7ner", "Ehr", "und", "uns\u00b7rer", "Lust", ",", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sollten wir auch billig uns bestreben,", "tokens": ["So", "soll\u00b7ten", "wir", "auch", "bil\u00b7lig", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn wir so sch\u00f6ne Wercke sehn,", "tokens": ["Wenn", "wir", "so", "sch\u00f6\u00b7ne", "Wer\u00b7cke", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Ernst sie nicht nur zu betrachten;", "tokens": ["Mit", "Ernst", "sie", "nicht", "nur", "zu", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch auf die weise Art zu achten,", "tokens": ["Auch", "auf", "die", "wei\u00b7se", "Art", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf welche Weise sie geschehn,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und, als so viele Wunder-Proben", "tokens": ["Und", ",", "als", "so", "vie\u00b7le", "Wun\u00b7der\u00b7Pro\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von dessen Weisheit, Lieb\u2019 und Macht,", "tokens": ["Von", "des\u00b7sen", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der C\u00f6rper, Geister, Aug\u2019 und Licht hervor gebracht,", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "Geis\u00b7ter", ",", "Aug'", "und", "Licht", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie anzusehen, ihn zu loben,", "tokens": ["Sie", "an\u00b7zu\u00b7se\u00b7hen", ",", "ihn", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und in bewundernder Verehrung zu erh\u00f6h\u2019n!", "tokens": ["Und", "in", "be\u00b7wun\u00b7dern\u00b7der", "Ver\u00b7eh\u00b7rung", "zu", "er\u00b7h\u00f6h'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}