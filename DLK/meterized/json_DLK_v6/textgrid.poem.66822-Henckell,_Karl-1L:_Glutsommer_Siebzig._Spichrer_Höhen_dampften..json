{"textgrid.poem.66822": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Glutsommer Siebzig. Spichrer H\u00f6hen dampften.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glutsommer Siebzig. Spichrer H\u00f6hen dampften.", "tokens": ["Glut\u00b7som\u00b7mer", "Sieb\u00b7zig", ".", "Spich\u00b7rer", "H\u00f6\u00b7hen", "dampf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Kanonen heulten. Schwerschwadronen stampften.", "tokens": ["Ka\u00b7no\u00b7nen", "heul\u00b7ten", ".", "Schwersc\u00b7hwad\u00b7ro\u00b7nen", "stampf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Leiber zuckten in den Ackergrund,", "tokens": ["Die", "Lei\u00b7ber", "zuck\u00b7ten", "in", "den", "A\u00b7cker\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entsetzen atmete der Erde Mund.", "tokens": ["Ent\u00b7set\u00b7zen", "at\u00b7me\u00b7te", "der", "Er\u00b7de", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Blut flo\u00df, als sei schon Rotwein-Kelterzeit.", "tokens": ["Blut", "flo\u00df", ",", "als", "sei", "schon", "Rot\u00b7wein\u00b7Kel\u00b7ter\u00b7zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOKOM", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dumpf Trommelwirbel! Die Trompete schreit.", "tokens": ["Dumpf", "Trom\u00b7mel\u00b7wir\u00b7bel", "!", "Die", "Trom\u00b7pe\u00b7te", "schreit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und Zug auf Zug, im Opferrausche, st\u00fcrmt.", "tokens": ["Und", "Zug", "auf", "Zug", ",", "im", "Op\u00b7fer\u00b7rau\u00b7sche", ",", "st\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hurra und Vorw\u00e4rts! Leichen aufget\u00fcrmt!", "tokens": ["Hur\u00b7ra", "und", "Vor\u00b7w\u00e4rts", "!", "Lei\u00b7chen", "auf\u00b7ge\u00b7t\u00fcrmt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$.", "NN", "VVPP", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.9": {"text": "Zehntausend Menschen weniger oder mehr.", "tokens": ["Zehn\u00b7tau\u00b7send", "Men\u00b7schen", "we\u00b7ni\u00b7ger", "o\u00b7der", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Hurra! du preu\u00dfisches, du tapfres Heer! \u2013", "tokens": ["Hur\u00b7ra", "!", "du", "preu\u00b7\u00dfi\u00b7sches", ",", "du", "tapf\u00b7res", "Heer", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Genommen! Sieg! Der Abend k\u00fchlt hernieder", "tokens": ["Ge\u00b7nom\u00b7men", "!", "Sieg", "!", "Der", "A\u00b7bend", "k\u00fchlt", "her\u00b7nie\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und k\u00fc\u00dft mitleidig die erstarrten Glieder.", "tokens": ["Und", "k\u00fc\u00dft", "mit\u00b7lei\u00b7dig", "die", "er\u00b7starr\u00b7ten", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Halbtote lechzen in die laue Luft,", "tokens": ["Halb\u00b7to\u00b7te", "lech\u00b7zen", "in", "die", "lau\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "In ihre Nase wittert Leichenduft.", "tokens": ["In", "ih\u00b7re", "Na\u00b7se", "wit\u00b7tert", "Lei\u00b7chen\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die roten Kreuze bahren auf, verbinden", "tokens": ["Die", "ro\u00b7ten", "Kreu\u00b7ze", "bah\u00b7ren", "auf", ",", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und h\u00f6ren Sterbeseufzer sich entwinden.", "tokens": ["Und", "h\u00f6\u00b7ren", "Ster\u00b7be\u00b7seuf\u00b7zer", "sich", "ent\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "\u00bbmein Weib, mein armes, o mein armes \u2013 ah!\u00ab", "tokens": ["\u00bb", "mein", "Weib", ",", "mein", "ar\u00b7mes", ",", "o", "mein", "ar\u00b7mes", "\u2013", "ah", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "FM", "PPOSAT", "ADJA", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Rumpf schl\u00e4gt hin. Hurra, Germania!", "tokens": ["Der", "Rumpf", "schl\u00e4gt", "hin", ".", "Hur\u00b7ra", ",", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "NN", "$,", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die Bibel offen. Feldpropst im Talar.", "tokens": ["Die", "Bi\u00b7bel", "of\u00b7fen", ".", "Feld\u00b7propst", "im", "Ta\u00b7lar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die schwachen Bataillone rund rangiert.", "tokens": ["Die", "schwa\u00b7chen", "Ba\u00b7tail\u00b7lo\u00b7ne", "rund", "ran\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00bbhelm in die Hand!\u00ab der Hauptmann kommandiert.", "tokens": ["\u00bb", "helm", "in", "die", "Hand", "!", "\u00ab", "der", "Haupt\u00b7mann", "kom\u00b7man\u00b7diert", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "APPR", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der Feldpropst r\u00e4uspert sich: \u00bbO, du da droben,", "tokens": ["Der", "Feld\u00b7propst", "r\u00e4us\u00b7pert", "sich", ":", "\u00bb", "O", ",", "du", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "$(", "ITJ", "$,", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "La\u00df deinen unerforschten Ratschlu\u00df loben!", "tokens": ["La\u00df", "dei\u00b7nen", "un\u00b7er\u00b7forschten", "Rat\u00b7schlu\u00df", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Der heiligen Sache hast du Sieg gew\u00e4hrt", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Sa\u00b7che", "hast", "du", "Sieg", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Und deinen Willen wunderbar erkl\u00e4rt.", "tokens": ["Und", "dei\u00b7nen", "Wil\u00b7len", "wun\u00b7der\u00b7bar", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wir danken dir, du h\u00f6chster Herr der Welt,", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "du", "h\u00f6chs\u00b7ter", "Herr", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Da\u00df du des Erbfeinds H\u00f6llenplan zerschellt.", "tokens": ["Da\u00df", "du", "des", "Erb\u00b7feinds", "H\u00f6l\u00b7len\u00b7plan", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Sei f\u00fcrder mit uns! Segne du den K\u00f6nig", "tokens": ["Sei", "f\u00fcr\u00b7der", "mit", "uns", "!", "Seg\u00b7ne", "du", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Samt Bundesf\u00fcrsten!\u00ab Weiter dann eint\u00f6nig:", "tokens": ["Samt", "Bun\u00b7des\u00b7f\u00fcrs\u00b7ten", "!", "\u00ab", "Wei\u00b7ter", "dann", "ein\u00b7t\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "$(", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "\u00bbla\u00df deine Gnade aufgehn \u00fcber allen,", "tokens": ["\u00bb", "la\u00df", "dei\u00b7ne", "Gna\u00b7de", "auf\u00b7gehn", "\u00fc\u00b7ber", "al\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "VVIZU", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Insonderheit f\u00fcr die, so heut gefallen!", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "f\u00fcr", "die", ",", "so", "heut", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "F\u00fcr dich sind sie geboren in den Tod.", "tokens": ["F\u00fcr", "dich", "sind", "sie", "ge\u00b7bo\u00b7ren", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Gott, sei uns gn\u00e4dig! Hilf aus aller Not!\u00ab \u2013 \u2013", "tokens": ["Gott", ",", "sei", "uns", "gn\u00e4\u00b7dig", "!", "Hilf", "aus", "al\u00b7ler", "Not", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "$.", "NE", "APPR", "PIAT", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Die Mannschaft singt: \u00bbHerr Gott, dich loben wir!\u00ab", "tokens": ["Die", "Mann\u00b7schaft", "singt", ":", "\u00bb", "Herr", "Gott", ",", "dich", "lo\u00b7ben", "wir", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u2013 \u00bbHelm auf!\u00ab \u2013 Die Leute r\u00fccken ins Quartier.", "tokens": ["\u2013", "\u00bb", "Helm", "auf", "!", "\u00ab", "\u2013", "Die", "Leu\u00b7te", "r\u00fc\u00b7cken", "ins", "Quar\u00b7tier", "."], "token_info": ["punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "PTKVZ", "$.", "$(", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Jenseits im Tale ward zur selben Zeit", "tokens": ["Jen\u00b7seits", "im", "Ta\u00b7le", "ward", "zur", "sel\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Gott Napoleons der Dienst geweiht.", "tokens": ["Dem", "Gott", "Na\u00b7po\u00b7le\u00b7ons", "der", "Dienst", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Matt, knielahm stehn sie mit gesunknem Blick", "tokens": ["Matt", ",", "knie\u00b7lahm", "stehn", "sie", "mit", "ge\u00b7sun\u00b7knem", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und denken an ihr trauriges Geschick.", "tokens": ["Und", "den\u00b7ken", "an", "ihr", "trau\u00b7ri\u00b7ges", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im stillen ballt und krampft sich manche Faust,", "tokens": ["Im", "stil\u00b7len", "ballt", "und", "krampft", "sich", "man\u00b7che", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Indes der d\u00fcstere Choral erbraust.", "tokens": ["In\u00b7des", "der", "d\u00fcs\u00b7te\u00b7re", "Cho\u00b7ral", "er\u00b7braust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fleuch du dem kaiserlichen Aar voraus", "tokens": ["Fleuch", "du", "dem", "kai\u00b7ser\u00b7li\u00b7chen", "Aar", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sto\u00df das Geiertier in Nacht und Graus!", "tokens": ["Und", "sto\u00df", "das", "Gei\u00b7er\u00b7tier", "in", "Nacht", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gott segne, segne unser Herrscherhaus!", "tokens": ["Gott", "seg\u00b7ne", ",", "seg\u00b7ne", "un\u00b7ser", "Herr\u00b7scher\u00b7haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich wei\u00df, du wolltest uns gewi\u00df erst pr\u00fcfen.", "tokens": ["Ich", "wei\u00df", ",", "du", "woll\u00b7test", "uns", "ge\u00b7wi\u00df", "erst", "pr\u00fc\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nun leih uns Sieg! Wir schrein aus Herzenstiefen.\u00ab", "tokens": ["Nun", "leih", "uns", "Sieg", "!", "Wir", "schrein", "aus", "Her\u00b7zen\u00b7stie\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$.",