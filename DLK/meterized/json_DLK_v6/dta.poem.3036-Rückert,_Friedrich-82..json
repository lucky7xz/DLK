{"dta.poem.3036": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unendlich ist zugleich und endlich jedes Ding;", "tokens": ["Un\u00b7end\u00b7lich", "ist", "zu\u00b7gleich", "und", "end\u00b7lich", "je\u00b7des", "Ding", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort achtest du es gro\u00df, hier sch\u00e4tzest du's gering.", "tokens": ["Dort", "ach\u00b7test", "du", "es", "gro\u00df", ",", "hier", "sch\u00e4t\u00b7zest", "du's", "ge\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das was du liebest, lern' als ewig fest zu halten,", "tokens": ["Das", "was", "du", "lie\u00b7best", ",", "lern'", "als", "e\u00b7wig", "fest", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "KOKOM", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewurzelt im Gem\u00fct, um niemals zu veralten.", "tokens": ["Ge\u00b7wur\u00b7zelt", "im", "Ge\u00b7m\u00fct", ",", "um", "nie\u00b7mals", "zu", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch was Unliebes dir macht Aerger und Verdru\u00df,", "tokens": ["Doch", "was", "Un\u00b7lie\u00b7bes", "dir", "macht", "A\u00b7er\u00b7ger", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Das wirf entschlossen in der ird'schen Dinge Flu\u00df.", "tokens": ["Das", "wirf", "ent\u00b7schlos\u00b7sen", "in", "der", "ird'\u00b7schen", "Din\u00b7ge", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich tr\u00f6st' es, da\u00df im Flu\u00df es wird vor\u00fcbertreiben,", "tokens": ["Dich", "tr\u00f6st'", "es", ",", "da\u00df", "im", "Flu\u00df", "es", "wird", "vor\u00b7\u00fc\u00b7bert\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Meer der Ewigkeit wird deine Liebe bleiben.", "tokens": ["Im", "Meer", "der", "E\u00b7wig\u00b7keit", "wird", "dei\u00b7ne", "Lie\u00b7be", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}