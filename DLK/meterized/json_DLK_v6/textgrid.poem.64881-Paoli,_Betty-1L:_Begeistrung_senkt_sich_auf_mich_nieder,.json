{"textgrid.poem.64881": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begeistrung senkt sich auf mich nieder,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeistrung senkt sich auf mich nieder,", "tokens": ["Be\u00b7geis\u00b7trung", "senkt", "sich", "auf", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Auge gl\u00e4nzt, mein Busen schwillt,", "tokens": ["Mein", "Au\u00b7ge", "gl\u00e4nzt", ",", "mein", "Bu\u00b7sen", "schwillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedenk ich dein und deiner Lieder,", "tokens": ["Ge\u00b7denk", "ich", "dein", "und", "dei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hohes, ernstes Dichterbild!", "tokens": ["Du", "ho\u00b7hes", ",", "erns\u00b7tes", "Dich\u00b7ter\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Strahl des Gesangs! mit dunklem Beben", "tokens": ["Strahl", "des", "Ge\u00b7sangs", "!", "mit", "dunk\u00b7lem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Erfa\u00dft mich tiefer Sehnsucht Weh,", "tokens": ["Er\u00b7fa\u00dft", "mich", "tie\u00b7fer", "Sehn\u00b7sucht", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir geleitet hinzuschweben", "tokens": ["Von", "dir", "ge\u00b7lei\u00b7tet", "hin\u00b7zu\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf der Gedanken weiter See!", "tokens": ["Auf", "der", "Ge\u00b7dan\u00b7ken", "wei\u00b7ter", "See", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und eh mich noch die Wellen tragen,", "tokens": ["Und", "eh", "mich", "noch", "die", "Wel\u00b7len", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht es wohl fest in meinem Sinn,", "tokens": ["Steht", "es", "wohl", "fest", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich allzuferne nicht zu wagen,", "tokens": ["Mich", "all\u00b7zu\u00b7fer\u00b7ne", "nicht", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu schiffen nur am Strande hin;", "tokens": ["Zu", "schif\u00b7fen", "nur", "am", "Stran\u00b7de", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn Hilfe, Trost und Zuflucht fehlen", "tokens": ["Denn", "Hil\u00b7fe", ",", "Trost", "und", "Zu\u00b7flucht", "feh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jener Bahn, die w\u00e4hlt mein Drang,", "tokens": ["Auf", "je\u00b7ner", "Bahn", ",", "die", "w\u00e4hlt", "mein", "Drang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und viele Schiffer sind zu z\u00e4hlen,", "tokens": ["Und", "vie\u00b7le", "Schif\u00b7fer", "sind", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dieser Ocean verschlang.", "tokens": ["Die", "die\u00b7ser", "O\u00b7cean", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Doch seh ich deinen Strahl, den klaren,", "tokens": ["Doch", "seh", "ich", "dei\u00b7nen", "Strahl", ",", "den", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Liedesstern, so licht und hehr: \u2013", "tokens": ["Du", "Lie\u00b7des\u00b7stern", ",", "so", "licht", "und", "hehr", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verge\u00df ich tr\u00e4umrisch der Gefahren", "tokens": ["Ver\u00b7ge\u00df", "ich", "tr\u00e4um\u00b7risch", "der", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steure fort in's offne Meer.", "tokens": ["Und", "steu\u00b7re", "fort", "in's", "off\u00b7ne", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und steure fort, bis fern die K\u00fcste", "tokens": ["Und", "steu\u00b7re", "fort", ",", "bis", "fern", "die", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor meinem trunknen Blick versinkt", "tokens": ["Vor", "mei\u00b7nem", "trunk\u00b7nen", "Blick", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der weiten Weltenw\u00fcste", "tokens": ["Und", "in", "der", "wei\u00b7ten", "Wel\u00b7ten\u00b7w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir nur dein Licht entgegenblinkt.", "tokens": ["Mir", "nur", "dein", "Licht", "ent\u00b7ge\u00b7gen\u00b7blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es zog mich dieses Licht vom Strande,", "tokens": ["Es", "zog", "mich", "die\u00b7ses", "Licht", "vom", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Leben raubt' es eine Braut,", "tokens": ["Dem", "Le\u00b7ben", "raubt'", "es", "ei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wird's den Weg zu h\u00f6hrem Lande", "tokens": ["Doch", "wird's", "den", "Weg", "zu", "h\u00f6h\u00b7rem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herzen zeigen, das ihm traut.", "tokens": ["Dem", "Her\u00b7zen", "zei\u00b7gen", ",", "das", "ihm", "traut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}