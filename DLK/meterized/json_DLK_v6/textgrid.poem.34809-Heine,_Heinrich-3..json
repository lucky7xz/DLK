{"textgrid.poem.34809": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ganges rauscht, der gro\u00dfe Ganges schwillt,", "tokens": ["Der", "Gan\u00b7ges", "rauscht", ",", "der", "gro\u00b7\u00dfe", "Gan\u00b7ges", "schwillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Himalaja strahlt im Abendscheine,", "tokens": ["Der", "Hi\u00b7ma\u00b7la\u00b7ja", "strahlt", "im", "A\u00b7bend\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der Nacht der Banianenhaine", "tokens": ["Und", "aus", "der", "Nacht", "der", "Ba\u00b7ni\u00b7a\u00b7nen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Elefantenherde st\u00fcrzt und br\u00fcllt \u2013", "tokens": ["Die", "E\u00b7lef\u00b7an\u00b7ten\u00b7her\u00b7de", "st\u00fcrzt", "und", "br\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Bild! Ein Bild! Mein Pferd f\u00fcr'n gutes Bild!", "tokens": ["Ein", "Bild", "!", "Ein", "Bild", "!", "Mein", "Pferd", "f\u00fcr'n", "gu\u00b7tes", "Bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Womit ich dich vergleiche, Sch\u00f6ne, Feine,", "tokens": ["Wo\u00b7mit", "ich", "dich", "ver\u00b7glei\u00b7che", ",", "Sch\u00f6\u00b7ne", ",", "Fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich Unvergleichliche, dich Gute, Reine,", "tokens": ["Dich", "Un\u00b7ver\u00b7gleich\u00b7li\u00b7che", ",", "dich", "Gu\u00b7te", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir das Herz mit heitrer Lust erf\u00fcllt!", "tokens": ["Die", "mir", "das", "Herz", "mit", "hei\u00b7trer", "Lust", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vergebens siehst du mich nach Bildern schweifen,", "tokens": ["Ver\u00b7ge\u00b7bens", "siehst", "du", "mich", "nach", "Bil\u00b7dern", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und siehst mich mit Gef\u00fchl und Reimen ringen \u2013", "tokens": ["Und", "siehst", "mich", "mit", "Ge\u00b7f\u00fchl", "und", "Rei\u00b7men", "rin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, ach! du l\u00e4chelst gar ob meiner Qual!", "tokens": ["Und", ",", "ach", "!", "du", "l\u00e4\u00b7chelst", "gar", "ob", "mei\u00b7ner", "Qual", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VVFIN", "ADV", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch l\u00e4chle nur! Denn wenn du l\u00e4chelst, greifen", "tokens": ["Doch", "l\u00e4ch\u00b7le", "nur", "!", "Denn", "wenn", "du", "l\u00e4\u00b7chelst", ",", "grei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "KON", "KOUS", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gandarven nach der Zither, und sie singen", "tokens": ["Gan\u00b7dar\u00b7ven", "nach", "der", "Zi\u00b7ther", ",", "und", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort oben in dem goldnen Sonnensaal.", "tokens": ["Dort", "o\u00b7ben", "in", "dem", "gold\u00b7nen", "Son\u00b7nen\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}