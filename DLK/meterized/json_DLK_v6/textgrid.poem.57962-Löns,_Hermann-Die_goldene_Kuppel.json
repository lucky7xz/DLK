{"textgrid.poem.57962": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die goldene Kuppel", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist doch etwas Sch\u00f6nes", "tokens": ["Es", "ist", "doch", "et\u00b7was", "Sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um unsre Rathauskup-", "tokens": ["Um", "uns\u00b7re", "Rat\u00b7haus\u00b7kup"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "pel, denn die ist von Golde,", "tokens": ["pel", ",", "denn", "die", "ist", "von", "Gol\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PDS", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir bilden uns viel ein drub-", "tokens": ["Wir", "bil\u00b7den", "uns", "viel", "ein", "drub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "TRUNC"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "e; kost's auch viel Moneten,", "tokens": ["e", ";", "kost's", "auch", "viel", "Mo\u00b7ne\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist uns v\u00f6llig schnup-", "tokens": ["Das", "ist", "uns", "v\u00f6l\u00b7lig", "schnup"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "pe, denn wir geben's gerne", "tokens": ["pe", ",", "denn", "wir", "ge\u00b7ben's", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "--+-+--", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fcr unsre Rathauskup-", "tokens": ["F\u00fcr", "uns\u00b7re", "Rat\u00b7haus\u00b7kup"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "pel, das ist unsre Freude,", "tokens": ["pel", ",", "das", "ist", "uns\u00b7re", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Pa\u00dft auf, auf einmal, schwupp,", "tokens": ["Pa\u00dft", "auf", ",", "auf", "ein\u00b7mal", ",", "schwupp", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPR", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergold't man auch die T\u00fcrme", "tokens": ["Ver\u00b7gold't", "man", "auch", "die", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Rathaus, wie die Kup-", "tokens": ["Am", "Rat\u00b7haus", ",", "wie", "die", "Kup"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "pel, oder soll'n wir,", "tokens": ["pel", ",", "o\u00b7der", "soll'n", "wir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VMFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was kommt denn an darub,", "tokens": ["Was", "kommt", "denn", "an", "da\u00b7rub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Magistrat vergolden,", "tokens": ["Den", "Ma\u00b7gist\u00b7rat", "ver\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Genau so wie die Kupp?", "tokens": ["Ge\u00b7nau", "so", "wie", "die", "Kupp", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}