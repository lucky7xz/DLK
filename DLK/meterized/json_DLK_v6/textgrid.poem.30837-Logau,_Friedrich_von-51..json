{"textgrid.poem.30837": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann wie in Indien die Leute blo\u00df sich tr\u00fcgen,", "tokens": ["Wann", "wie", "in", "In\u00b7di\u00b7en", "die", "Leu\u00b7te", "blo\u00df", "sich", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "APPR", "NE", "ART", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So k\u00fcnte Schm\u00fcnck und Schmuck nicht so betrieglich l\u00fcgen.", "tokens": ["So", "k\u00fcn\u00b7te", "Schm\u00fcnck", "und", "Schmuck", "nicht", "so", "be\u00b7trieg\u00b7lich", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}