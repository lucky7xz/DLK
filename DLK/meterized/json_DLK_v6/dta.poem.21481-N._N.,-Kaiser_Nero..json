{"dta.poem.21481": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Nero.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Sahst du das prachtvoll d\u00fcstere Nerobild,", "tokens": ["Sahst", "du", "das", "pracht\u00b7voll", "d\u00fcs\u00b7te\u00b7re", "Ne\u00b7ro\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Meister Kaulbach\u2019s fl\u00fcchtige Hand entwarf?", "tokens": ["Das", "Meis\u00b7ter", "Kaul\u00b7bach's", "fl\u00fcch\u00b7ti\u00b7ge", "Hand", "ent\u00b7warf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Zauberreiz bleibt unausl\u00f6schlich", "tokens": ["Sein", "Zau\u00b7berr\u00b7eiz", "bleibt", "un\u00b7aus\u00b7l\u00f6schlich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Winkend mir tief in das Herz gegraben.", "tokens": ["Win\u00b7kend", "mir", "tief", "in", "das", "Herz", "ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Hoch oben steht machtstrahlend der Caesar da", "tokens": ["Hoch", "o\u00b7ben", "steht", "machts\u00b7trah\u00b7lend", "der", "Cae\u00b7sar", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im l\u00e4ssig weichumh\u00fcllenden Prunkgewand,", "tokens": ["Im", "l\u00e4s\u00b7sig", "wei\u00b7ch\u00b7um\u00b7h\u00fcl\u00b7len\u00b7den", "Prunk\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Indessen h\u00e4lt die ausgeklung\u2019ne", "tokens": ["In\u00b7des\u00b7sen", "h\u00e4lt", "die", "aus\u00b7ge\u00b7klung'\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leier ein knieender, sch\u00f6ner Knabe.", "tokens": ["Lei\u00b7er", "ein", "kni\u00b7e\u00b7en\u00b7der", ",", "sch\u00f6\u00b7ner", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Hoch als Apollon ragt er, im Lorbeer stolz;", "tokens": ["Hoch", "als", "A\u00b7pol\u00b7lon", "ragt", "er", ",", "im", "Lor\u00b7beer", "stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "VVFIN", "PPER", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von links dr\u00e4ngt an vollbusiger Weiber Schaar,", "tokens": ["Von", "links", "dr\u00e4ngt", "an", "voll\u00b7bu\u00b7si\u00b7ger", "Wei\u00b7ber", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "M\u00e4nadisch sch\u00f6n, mit liebestrunk\u2019nen", "tokens": ["M\u00e4\u00b7na\u00b7disch", "sch\u00f6n", ",", "mit", "lie\u00b7bes\u00b7trunk'\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Augen, in \u00fcppiger Leibesnacktheit.", "tokens": ["Au\u00b7gen", ",", "in", "\u00fcp\u00b7pi\u00b7ger", "Lei\u00b7be\u00b7snackt\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Links aber nah\u2019n mit grinsenden Sclavenblick", "tokens": ["Links", "a\u00b7ber", "nah'n", "mit", "grin\u00b7sen\u00b7den", "Scla\u00b7ven\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich M\u00e4nner, feig und seelenverderbt, ob nun", "tokens": ["Sich", "M\u00e4n\u00b7ner", ",", "feig", "und", "see\u00b7len\u00b7ver\u00b7derbt", ",", "ob", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "NN", "$,", "ADJD", "KON", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die weite Toga, ob der Panzer", "tokens": ["Die", "wei\u00b7te", "To\u00b7ga", ",", "ob", "der", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fccke die immer noch stolzen Glieder.", "tokens": ["Schm\u00fc\u00b7cke", "die", "im\u00b7mer", "noch", "stol\u00b7zen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "So schl\u00e4ngeln gl\u00fcckwunschbringend sie sich zum Herrn,", "tokens": ["So", "schl\u00e4n\u00b7geln", "gl\u00fcck\u00b7wunschbrin\u00b7gend", "sie", "sich", "zum", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Der eben aussang \u2014 Aber betrachte jetzt", "tokens": ["Der", "e\u00b7ben", "aus\u00b7sang", "A\u00b7ber", "be\u00b7trach\u00b7te", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$(", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Kaiser selbst: Was sieht sein Auge?", "tokens": ["Den", "Kai\u00b7ser", "selbst", ":", "Was", "sieht", "sein", "Au\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche Trag\u00f6die sich zu F\u00fc\u00dfen?", "tokens": ["Wel\u00b7che", "Tra\u00b7g\u00f6\u00b7die", "sich", "zu", "F\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PRF", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Ein Christenh\u00e4uflein! ", "tokens": ["Ein", "Chris\u00b7ten\u00b7h\u00e4uf\u00b7lein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den nackten S\u00e4ugling hier und die Mutter dort!", "tokens": ["Den", "nack\u00b7ten", "S\u00e4ug\u00b7ling", "hier", "und", "die", "Mut\u00b7ter", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "J\u00fcnglinge, trotzig sch\u00f6n in Demuth,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", ",", "trot\u00b7zig", "sch\u00f6n", "in", "De\u00b7muth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hoffend wie ", "tokens": ["Hof\u00b7fend", "wie"], "token_info": ["word", "word"], "pos": ["ADJD", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "O schn\u00f6der Zeitgeist, welcher gefangen h\u00e4lt", "tokens": ["O", "schn\u00f6\u00b7der", "Zeit\u00b7geist", ",", "wel\u00b7cher", "ge\u00b7fan\u00b7gen", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dumpfem Bann ach alle Gem\u00fcther \u2014 ha,", "tokens": ["In", "dum\u00b7pfem", "Bann", "ach", "al\u00b7le", "Ge\u00b7m\u00fc\u00b7ther", "ha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PIAT", "NN", "$(", "ITJ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Kaiser Nero m\u00f6cht\u2019 ich heute", "tokens": ["Wie", "Kai\u00b7ser", "Ne\u00b7ro", "m\u00f6cht'", "ich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sitzen und richten vom goldenen Thronstuhl.", "tokens": ["Sit\u00b7zen", "und", "rich\u00b7ten", "vom", "gol\u00b7de\u00b7nen", "Thron\u00b7stuhl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie alle rief\u2019 ich, riefe bei Namen sie,", "tokens": ["Sie", "al\u00b7le", "rie\u00b7f'", "ich", ",", "rie\u00b7fe", "bei", "Na\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die frecher Selbstsucht fr\u00f6hnen, und die sogar", "tokens": ["Die", "fre\u00b7cher", "Selbst\u00b7sucht", "fr\u00f6h\u00b7nen", ",", "und", "die", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "ART", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der blinden Armuth d\u00fcnnen Mantel", "tokens": ["Der", "blin\u00b7den", "Ar\u00b7muth", "d\u00fcn\u00b7nen", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4chtens zu rauben sich nicht entbl\u00f6den.", "tokens": ["N\u00e4ch\u00b7tens", "zu", "rau\u00b7ben", "sich", "nicht", "ent\u00b7bl\u00f6\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Viel and\u2019re, mehr noch! Donnernd, ein Nero-Zeus,", "tokens": ["Viel", "an\u00b7d'\u00b7re", ",", "mehr", "noch", "!", "Don\u00b7nernd", ",", "ein", "Nero\u00b7Zeus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "ADV", "$.", "VVPP", "$,", "ART", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "W\u00fcrf\u2019 ich des Urtheils z\u00fcrnenden Racheblitz \u2014", "tokens": ["W\u00fcr\u00b7f'", "ich", "des", "Ur\u00b7theils", "z\u00fcr\u00b7nen\u00b7den", "Ra\u00b7che\u00b7blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und als Apollon-Nero s\u00e4ng\u2019 ich", "tokens": ["Und", "als", "A\u00b7pol\u00b7lon\u00b7Ne\u00b7ro", "s\u00e4ng'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einen gewaltigen Schicksalshymnus.", "tokens": ["Ei\u00b7nen", "ge\u00b7wal\u00b7ti\u00b7gen", "Schick\u00b7sals\u00b7hym\u00b7nus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Ha, ", "tokens": ["Ha", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Sanft, blumenfromm bl\u00fcht immer ein deutsches Herz:", "tokens": ["Sanft", ",", "blu\u00b7men\u00b7fromm", "bl\u00fcht", "im\u00b7mer", "ein", "deut\u00b7sches", "Herz", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Wort und Bild nur l\u00e4\u00dft es k\u00fchn die", "tokens": ["In", "Wort", "und", "Bild", "nur", "l\u00e4\u00dft", "es", "k\u00fchn", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "PPER", "ADJD", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rachegedanken des Zorns verbrausen.", "tokens": ["Ra\u00b7che\u00b7ge\u00b7dan\u00b7ken", "des", "Zorns", "ver\u00b7brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}