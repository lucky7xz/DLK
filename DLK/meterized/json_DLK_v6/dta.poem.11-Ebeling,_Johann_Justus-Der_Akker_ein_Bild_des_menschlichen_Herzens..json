{"dta.poem.11": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Akker  \n ein Bild des menschlichen Herzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch die kleine Welt, sieht stets\ndie grosse an,", "tokens": ["Der", "Mensch", "die", "klei\u00b7ne", "Welt", ",", "sieht", "stets", "die", "gros\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin das kleinste St\u00fck zum Lehr-", "tokens": ["Da\u00b7rin", "das", "kleins\u00b7te", "St\u00fck", "zum", "Lehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man es recht beschaut: Man", "tokens": ["Wenn", "man", "es", "recht", "be\u00b7schaut", ":", "Man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "$.", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seine Herrlichkeit an jeden Din-", "tokens": ["Und", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "an", "je\u00b7den", "Din"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Kreaturen sind, wie Spiegel drin sich zeigt,", "tokens": ["Die", "Kre\u00b7a\u00b7tu\u00b7ren", "sind", ",", "wie", "Spie\u00b7gel", "drin", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie gros des H\u00f6chsten Macht, wie sehr er uns", "tokens": ["Wie", "gros", "des", "H\u00f6chs\u00b7ten", "Macht", ",", "wie", "sehr", "er", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "$,", "PWAV", "ADV", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie weise dessen Huld, den wir als Sch\u00f6pfer eh-", "tokens": ["Wie", "wei\u00b7se", "des\u00b7sen", "Huld", ",", "den", "wir", "als", "Sch\u00f6p\u00b7fer", "eh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PDS", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan uns der Erden-Bau, des Himmels Stern-", "tokens": ["Kan", "uns", "der", "Er\u00b7den\u00b7Bau", ",", "des", "Him\u00b7mels", "Stern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Zeigt uns also von sich manch sichtbar Ebenbild,", "tokens": ["Zeigt", "uns", "al\u00b7so", "von", "sich", "manch", "sicht\u00b7bar", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PRF", "PIAT", "ADJD", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wir k\u00f6nnen aber auch von uns viel Bilder sehen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "a\u00b7ber", "auch", "von", "uns", "viel", "Bil\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn wir das Reich der Macht, zu unsrer Lust", "tokens": ["Wenn", "wir", "das", "Reich", "der", "Macht", ",", "zu", "uns\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Dinge Aehnlichkeit, die frommer Wiz ein-", "tokens": ["Der", "Din\u00b7ge", "A\u00b7ehn\u00b7lich\u00b7keit", ",", "die", "from\u00b7mer", "Wiz", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Der jeden Vorwurf n\u00fczt, und draus Erbauung", "tokens": ["Der", "je\u00b7den", "Vor\u00b7wurf", "n\u00fczt", ",", "und", "draus", "Er\u00b7bau\u00b7ung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KON", "PAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Macht uns die Welt zum Buch darin wir viele", "tokens": ["Macht", "uns", "die", "Welt", "zum", "Buch", "da\u00b7rin", "wir", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "PAV", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Zur Seelen Besserung erkennen, sehn und h\u00f6ren.", "tokens": ["Zur", "See\u00b7len", "Bes\u00b7se\u00b7rung", "er\u00b7ken\u00b7nen", ",", "sehn", "und", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Lehrer der selbst ", "tokens": ["Der", "Leh\u00b7rer", "der", "selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Der uns die Warheit lehrt, zur Besserung an-", "tokens": ["Der", "uns", "die", "War\u00b7heit", "lehrt", ",", "zur", "Bes\u00b7se\u00b7rung", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "F\u00fchrt uns oft in die Welt darinnen zu erblikken,", "tokens": ["F\u00fchrt", "uns", "oft", "in", "die", "Welt", "da\u00b7rin\u00b7nen", "zu", "er\u00b7blik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Dinge die das Herz der Sterblichen abdr\u00fckken.", "tokens": ["Die", "Din\u00b7ge", "die", "das", "Herz", "der", "Sterb\u00b7li\u00b7chen", "ab\u00b7dr\u00fck\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Er f\u00fchrt uns in das Feld, und weiset unsern Sinn,", "tokens": ["Er", "f\u00fchrt", "uns", "in", "das", "Feld", ",", "und", "wei\u00b7set", "un\u00b7sern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein achtsames Gem\u00fct, zu einem Akker hin,", "tokens": ["Ein", "acht\u00b7sa\u00b7mes", "Ge\u00b7m\u00fct", ",", "zu", "ei\u00b7nem", "Ak\u00b7ker", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Hier bald zum steinigten, da zu dem guten Lande,", "tokens": ["Hier", "bald", "zum", "stei\u00b7nig\u00b7ten", ",", "da", "zu", "dem", "gu\u00b7ten", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Da wir ein Bildnis sehn, von unsers Herzens", "tokens": ["Da", "wir", "ein", "Bild\u00b7nis", "sehn", ",", "von", "un\u00b7sers", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sieht man den Akker an, eh er wird umge-", "tokens": ["Sieht", "man", "den", "Ak\u00b7ker", "an", ",", "eh", "er", "wird", "um\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VAFIN", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.26": {"text": "So ist desselben Fl\u00e4ch, rauh, ungebahnt und hart,", "tokens": ["So", "ist", "des\u00b7sel\u00b7ben", "Fl\u00e4ch", ",", "rauh", ",", "un\u00b7ge\u00b7bahnt", "und", "hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Pflug, der ihn durchw\u00fchlt, der mu\u00df ihn erst", "tokens": ["Der", "Pflug", ",", "der", "ihn", "durch\u00b7w\u00fchlt", ",", "der", "mu\u00df", "ihn", "erst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Da\u00df er das Saamenkorn, kan in dem Schoos auf-", "tokens": ["Da\u00df", "er", "das", "Saa\u00b7men\u00b7korn", ",", "kan", "in", "dem", "Schoos", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "VMFIN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und bleibt er unged\u00fcngt, von fetter Fruchtbarkeit,", "tokens": ["Und", "bleibt", "er", "un\u00b7ge\u00b7d\u00fcngt", ",", "von", "fet\u00b7ter", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wird das Saamenkorn vergeblich ausgestreut,", "tokens": ["So", "wird", "das", "Saa\u00b7men\u00b7korn", "ver\u00b7geb\u00b7lich", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wird es nicht tief verstekt; so kan man leicht er-", "tokens": ["Wird", "es", "nicht", "tief", "vers\u00b7tekt", ";", "so", "kan", "man", "leicht", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$.", "ADV", "VMFIN", "PIS", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Es wird von V\u00f6gel-Raub uns wieder weggefressen.", "tokens": ["Es", "wird", "von", "V\u00f6\u00b7gel\u00b7Raub", "uns", "wie\u00b7der", "weg\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und taugt der Boden nichts, ist er wie Fels und", "tokens": ["Und", "taugt", "der", "Bo\u00b7den", "nichts", ",", "ist", "er", "wie", "Fels", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "$,", "VAFIN", "PPER", "KOKOM", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "So dringt der Wurzelkeim nicht tief genug hinein;", "tokens": ["So", "dringt", "der", "Wur\u00b7zel\u00b7keim", "nicht", "tief", "ge\u00b7nug", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bekommt er keine Kraft, empf\u00e4ngt er keinen Re-", "tokens": ["Be\u00b7kommt", "er", "kei\u00b7ne", "Kraft", ",", "emp\u00b7f\u00e4ngt", "er", "kei\u00b7nen", "Re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "VVFIN", "PPER", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So w\u00e4chset auch nicht auf der eingesaamte Segen;", "tokens": ["So", "w\u00e4ch\u00b7set", "auch", "nicht", "auf", "der", "ein\u00b7ge\u00b7saam\u00b7te", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hingegen von sich selbst ohn da\u00df wir uns bem\u00fchn,", "tokens": ["Hin\u00b7ge\u00b7gen", "von", "sich", "selbst", "ohn", "da\u00df", "wir", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "APPR", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Sehn wir aus seinem Schoos, das b\u00f6se Unkraut", "tokens": ["Sehn", "wir", "aus", "sei\u00b7nem", "Schoos", ",", "das", "b\u00f6\u00b7se", "Un\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Die Dorn und Diesteln sind, ohn Pflanzen, oh-", "tokens": ["Die", "Dorn", "und", "Dies\u00b7teln", "sind", ",", "ohn", "Pflan\u00b7zen", ",", "oh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "$,", "KOUI", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Auf einem Akkerst\u00fck in Ueberflus zu haben,", "tokens": ["Auf", "ei\u00b7nem", "Ak\u00b7kers\u00b7t\u00fck", "in", "Ue\u00b7ber\u00b7flus", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dies ist o! Mensch ein Bild von unsers Herzens", "tokens": ["Dies", "ist", "o", "!", "Mensch", "ein", "Bild", "von", "un\u00b7sers", "Her\u00b7zens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "FM", "$.", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Was ist es von Natur? ein unfruchtbahres Land,", "tokens": ["Was", "ist", "es", "von", "Na\u00b7tur", "?", "ein", "un\u00b7frucht\u00b7bah\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das Dorn und Diesteln bringt, und lauter La-", "tokens": ["Das", "Dorn", "und", "Dies\u00b7teln", "bringt", ",", "und", "lau\u00b7ter", "La"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Wozu das Herz von selbst aus eignen Trieb genei-", "tokens": ["Wo\u00b7zu", "das", "Herz", "von", "selbst", "aus", "eig\u00b7nen", "Trieb", "ge\u00b7nei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wer seinen Akker baut, und hofft ein froh Gedein,", "tokens": ["Wer", "sei\u00b7nen", "Ak\u00b7ker", "baut", ",", "und", "hofft", "ein