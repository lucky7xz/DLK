{"dta.poem.23362": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Langweilig und schlecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist die Willk\u00fcr und Gewalt", "tokens": ["Wie", "ist", "die", "Will\u00b7k\u00fcr", "und", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Doch in der Welt gemein!", "tokens": ["Doch", "in", "der", "Welt", "ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt ist schon so klug und alt", "tokens": ["Die", "Welt", "ist", "schon", "so", "klug", "und", "alt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df noch dienstbar sein!", "tokens": ["Und", "mu\u00df", "noch", "dienst\u00b7bar", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann bricht der Freiheit goldner Strahl", "tokens": ["Wann", "bricht", "der", "Frei\u00b7heit", "gold\u00b7ner", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In unsre Nacht hinein?", "tokens": ["In", "uns\u00b7re", "Nacht", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann endet unser Joch einmal,", "tokens": ["Wann", "en\u00b7det", "un\u00b7ser", "Joch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann unsre Noth und Pein?", "tokens": ["Wann", "uns\u00b7re", "Noth", "und", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O weh! o weh!", "tokens": ["O", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wann unsre Noth und Pein?", "tokens": ["Wann", "uns\u00b7re", "Noth", "und", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geduld ist unsre Fr\u00f6hlichkeit,", "tokens": ["Ge\u00b7duld", "ist", "uns\u00b7re", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gehorsam unser Gl\u00fcck,", "tokens": ["Ge\u00b7hor\u00b7sam", "un\u00b7ser", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und niemals kommt Zufriedenheit", "tokens": ["Und", "nie\u00b7mals", "kommt", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unsre Welt zur\u00fcck.", "tokens": ["In", "uns\u00b7re", "Welt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl anders wird es jeden Tag,", "tokens": ["Wohl", "an\u00b7ders", "wird", "es", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch besser wird es nie.", "tokens": ["Doch", "bes\u00b7ser", "wird", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ist dumm wie's liebe Vieh,", "tokens": ["Ist", "dumm", "wie's", "lie\u00b7be", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O weh! o weh!", "tokens": ["O", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ist dumm wie's liebe Vieh.", "tokens": ["Ist", "dumm", "wie's", "lie\u00b7be", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}