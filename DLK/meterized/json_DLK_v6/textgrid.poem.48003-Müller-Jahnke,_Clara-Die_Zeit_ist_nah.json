{"textgrid.poem.48003": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeit ist nah", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gloria singend geht die Winternacht", "tokens": ["Ein", "Glo\u00b7ria", "sin\u00b7gend", "geht", "die", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "durch Schneegefilde; keines Sternbilds Pracht", "tokens": ["durch", "Schnee\u00b7ge\u00b7fil\u00b7de", ";", "kei\u00b7nes", "Stern\u00b7bilds", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "schaut aus den schwarzverh\u00fcllten Himmeln nieder, \u2013", "tokens": ["schaut", "aus", "den", "schwarz\u00b7ver\u00b7h\u00fcll\u00b7ten", "Him\u00b7meln", "nie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "durch eisbereifte Fenster aber bricht", "tokens": ["durch", "eis\u00b7be\u00b7reif\u00b7te", "Fens\u00b7ter", "a\u00b7ber", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ins Stra\u00dfendunkel eine Flut von Licht", "tokens": ["ins", "Stra\u00b7\u00dfen\u00b7dun\u00b7kel", "ei\u00b7ne", "Flut", "von", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und eine Woge kindhaft s\u00fc\u00dfer Lieder.", "tokens": ["und", "ei\u00b7ne", "Wo\u00b7ge", "kind\u00b7haft", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Bethlems T\u00e4lern nicht, \u2013 nicht weltenfern", "tokens": ["In", "Beth\u00b7lems", "T\u00e4\u00b7lern", "nicht", ",", "\u2013", "nicht", "wel\u00b7ten\u00b7fern"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "NE", "NN", "PTKNEG", "$,", "$(", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und himmelhoch gl\u00e4nzt heut der Weihnacht Stern,", "tokens": ["und", "him\u00b7mel\u00b7hoch", "gl\u00e4nzt", "heut", "der", "Weih\u00b7nacht", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nach dessen Strahl die Brust sich sehnend weitet:", "tokens": ["nach", "des\u00b7sen", "Strahl", "die", "Brust", "sich", "seh\u00b7nend", "wei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Zeit ist nah, wo licht und h\u00fcllenlos,", "tokens": ["die", "Zeit", "ist", "nah", ",", "wo", "licht", "und", "h\u00fcl\u00b7len\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "wo neugeboren aus der Menschheit Scho\u00df", "tokens": ["wo", "neu\u00b7ge\u00b7bo\u00b7ren", "aus", "der", "Menschheit", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "die Liebe durch des Elends N\u00e4chte schreitet.", "tokens": ["die", "Lie\u00b7be", "durch", "des", "E\u00b7lends", "N\u00e4ch\u00b7te", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist nah, wo jede Klage schweigt,", "tokens": ["Die", "Zeit", "ist", "nah", ",", "wo", "je\u00b7de", "Kla\u00b7ge", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wo jedem Flehn ein menschlich Herz sich neigt,", "tokens": ["wo", "je\u00b7dem", "Flehn", "ein", "menschlich", "Herz", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "ADJD", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Bruder hei\u00dft den Irrenden und Armen, \u2013", "tokens": ["Das", "Bru\u00b7der", "hei\u00dft", "den", "Ir\u00b7ren\u00b7den", "und", "Ar\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wo sich der Keim aus brauner Scholle dr\u00e4ngt", "tokens": ["wo", "sich", "der", "Keim", "aus", "brau\u00b7ner", "Schol\u00b7le", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und Licht und W\u00e4rme als sein Recht empf\u00e4ngt", "tokens": ["und", "Licht", "und", "W\u00e4r\u00b7me", "als", "sein", "Recht", "emp\u00b7f\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und nicht als Bettelgabe \u2013 aus Erbarmen!", "tokens": ["und", "nicht", "als", "Bet\u00b7tel\u00b7ga\u00b7be", "\u2013", "aus", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOUS", "NN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Zeit ist nah: schon bl\u00fcht ein bleiches Rot", "tokens": ["Die", "Zeit", "ist", "nah", ":", "schon", "bl\u00fcht", "ein", "blei\u00b7ches", "Rot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im Osten auf, \u2013 schon zuckt in hei\u00dfer Not", "tokens": ["im", "Os\u00b7ten", "auf", ",", "\u2013", "schon", "zuckt", "in", "hei\u00b7\u00dfer", "Not"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "$(", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein letztes Wehe durch der Menschheit Glieder;", "tokens": ["ein", "letz\u00b7tes", "We\u00b7he", "durch", "der", "Menschheit", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sie ruft und ringt \u2013 der D\u00e4mmerung Schleier f\u00e4llt:", "tokens": ["sie", "ruft", "und", "ringt", "\u2013", "der", "D\u00e4m\u00b7me\u00b7rung", "Schlei\u00b7er", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "erl\u00f6sungsfreudig steigt zur dunklen Welt", "tokens": ["er\u00b7l\u00f6\u00b7sungs\u00b7freu\u00b7dig", "steigt", "zur", "dunk\u00b7len", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "das Himmelskind, die goldne Liebe, nieder.", "tokens": ["das", "Him\u00b7mel\u00b7skind", ",", "die", "gold\u00b7ne", "Lie\u00b7be", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}