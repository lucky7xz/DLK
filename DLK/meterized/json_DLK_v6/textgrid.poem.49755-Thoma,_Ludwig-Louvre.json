{"textgrid.poem.49755": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Louvre", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft ehrfurchtsvoll uns wandeln durch die S\u00e4le", "tokens": ["La\u00dft", "ehr\u00b7furchts\u00b7voll", "uns", "wan\u00b7deln", "durch", "die", "S\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tiefer als in Kirchen uns verneigen!", "tokens": ["Und", "tie\u00b7fer", "als", "in", "Kir\u00b7chen", "uns", "ver\u00b7nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn feierlicher klingt als Festchor\u00e4le", "tokens": ["Denn", "fei\u00b7er\u00b7li\u00b7cher", "klingt", "als", "Fest\u00b7cho\u00b7r\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Marmorbilder traumverlornes Schweigen.", "tokens": ["Der", "Mar\u00b7mor\u00b7bil\u00b7der", "traum\u00b7ver\u00b7lor\u00b7nes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was sie uns sagen, kann in unsern Herzen", "tokens": ["Was", "sie", "uns", "sa\u00b7gen", ",", "kann", "in", "un\u00b7sern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PRF", "VVINF", "$,", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach! so viel l\u00e4nger als Gebete w\u00e4hren!", "tokens": ["Ach", "!", "so", "viel", "l\u00e4n\u00b7ger", "als", "Ge\u00b7be\u00b7te", "w\u00e4h\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie leuchten heller als die tausend Kerzen", "tokens": ["Sie", "leuch\u00b7ten", "hel\u00b7ler", "als", "die", "tau\u00b7send", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf \u00fcberbunt geschm\u00fcckten Hochalt\u00e4ren.", "tokens": ["Auf", "\u00fc\u00b7ber\u00b7bunt", "ge\u00b7schm\u00fcck\u00b7ten", "Hoch\u00b7al\u00b7t\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr mildes L\u00e4cheln in den Marmorz\u00fcgen,", "tokens": ["Ihr", "mil\u00b7des", "L\u00e4\u00b7cheln", "in", "den", "Mar\u00b7mor\u00b7z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es ist geblieben in den tausend Jahren,", "tokens": ["Es", "ist", "ge\u00b7blie\u00b7ben", "in", "den", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als wenn sie Mitleid mit den Menschen tr\u00fcgen,", "tokens": ["Als", "wenn", "sie", "Mit\u00b7leid", "mit", "den", "Men\u00b7schen", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die immer klein und immer elend waren.", "tokens": ["Die", "im\u00b7mer", "klein", "und", "im\u00b7mer", "e\u00b7lend", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie sind sie sch\u00f6n! La\u00dft sie uns schweigend gr\u00fc\u00dfen", "tokens": ["Wie", "sind", "sie", "sch\u00f6n", "!", "La\u00dft", "sie", "uns", "schwei\u00b7gend", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "VVIMP", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und mit erhobnen Herzen vor sie treten!", "tokens": ["Und", "mit", "er\u00b7hob\u00b7nen", "Her\u00b7zen", "vor", "sie", "tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Als sie die Welt beherrschten, galt kein B\u00fc\u00dfen,", "tokens": ["Als", "sie", "die", "Welt", "be\u00b7herrschten", ",", "galt", "kein", "B\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Kein Psalmenpl\u00e4rren und auf Knien beten.", "tokens": ["Kein", "Psal\u00b7men\u00b7pl\u00e4r\u00b7ren", "und", "auf", "Kni\u00b7en", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie tragen in den H\u00e4nden keine Waffen", "tokens": ["Sie", "tra\u00b7gen", "in", "den", "H\u00e4n\u00b7den", "kei\u00b7ne", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie die in Stein gehaunen Menschenw\u00fcrger,", "tokens": ["Wie", "die", "in", "Stein", "ge\u00b7hau\u00b7nen", "Men\u00b7schen\u00b7w\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die in Berlin wir Tag f\u00fcr Tag begaffen,", "tokens": ["Die", "in", "Ber\u00b7lin", "wir", "Tag", "f\u00fcr", "Tag", "be\u00b7ga\u00b7ffen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "PPER", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Die zweiunddrei\u00dfig dicken Brandenb\u00fcrger.", "tokens": ["Die", "zwei\u00b7und\u00b7drei\u00b7\u00dfig", "di\u00b7cken", "Bran\u00b7den\u00b7b\u00fcr\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}