,"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bringt bald ein B\u00fcchelchen, bald Karten,", "tokens": ["Bringt", "bald", "ein", "B\u00fc\u00b7chel\u00b7chen", ",", "bald", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Bald streut sie alles in den Wind,", "tokens": ["Bald", "streut", "sie", "al\u00b7les", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und eilt hinunter in den Garten.", "tokens": ["Und", "eilt", "hin\u00b7un\u00b7ter", "in", "den", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Ich hinterher, ereile sie", "tokens": ["Ich", "hin\u00b7ter\u00b7her", ",", "er\u00b7ei\u00b7le", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer sichern stillen Grotte.", "tokens": ["In", "ei\u00b7ner", "si\u00b7chern", "stil\u00b7len", "Grot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Freund Amor treibt, sie weis nicht wie?", "tokens": ["Freund", "A\u00b7mor", "treibt", ",", "sie", "weis", "nicht", "wie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "PPER", "PTKVZ", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie tief ins Dunkel. Dank dem Gotte!", "tokens": ["Sie", "tief", "ins", "Dun\u00b7kel", ".", "Dank", "dem", "Got\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie bebt, von meinem Arm umstrikt.", "tokens": ["Sie", "bebt", ",", "von", "mei\u00b7nem", "Arm", "um\u00b7strikt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Kus erstikt ihr leztes Lallen.", "tokens": ["Mein", "Kus", "ers\u00b7tikt", "ihr", "lez\u00b7tes", "Lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie sinkt. Ich halte sie entz\u00fckt,", "tokens": ["Sie", "sinkt", ".", "Ich", "hal\u00b7te", "sie", "ent\u00b7z\u00fckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u2014 halt! \u2014 und lasse sie nicht fallen.", "tokens": ["Und", "halt", "!", "und", "las\u00b7se", "sie", "nicht", "fal\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "$.", "$(", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}