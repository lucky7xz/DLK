{"dta.poem.19873": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Liebenden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Andrer werb' um Ehr' und Gold!               ", "tokens": ["Ein", "A\u00b7ndrer", "werb'", "um", "Ehr'", "und", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich werb\u2019 um Wollust bei Selinden.", "tokens": ["Ich", "werb'", "um", "Wol\u00b7lust", "bei", "Se\u00b7lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich kan nur s\u00fcsser Minnesold", "tokens": ["Mich", "kan", "nur", "s\u00fcs\u00b7ser", "Min\u00b7ne\u00b7sold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An algetreue Dienste binden.", "tokens": ["An", "al\u00b7ge\u00b7treu\u00b7e", "Diens\u00b7te", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fck l\u00e4st manchen Ehrenman", "tokens": ["Das", "Gl\u00fck", "l\u00e4st", "man\u00b7chen", "Eh\u00b7ren\u00b7man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinem Dienst\u2019 umsonst verderben.", "tokens": ["In", "sei\u00b7nem", "Dienst'", "um\u00b7sonst", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein bei trauter Minne kan", "tokens": ["Al\u00b7lein", "bei", "trau\u00b7ter", "Min\u00b7ne", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hirt auch sichern Sold erwerben.", "tokens": ["Der", "Hirt", "auch", "si\u00b7chern", "Sold", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin kein grosser reicher Herr,", "tokens": ["Ich", "bin", "kein", "gros\u00b7ser", "rei\u00b7cher", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie ist keine hohe Dame.", "tokens": ["Und", "sie", "ist", "kei\u00b7ne", "ho\u00b7he", "Da\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dagegen klingt viel reizender", "tokens": ["Da\u00b7ge\u00b7gen", "klingt", "viel", "rei\u00b7zen\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein kurzer sch\u00e4ferlicher Name.", "tokens": ["Ein", "kur\u00b7zer", "sch\u00e4\u00b7fer\u00b7li\u00b7cher", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dagegen herzen wir uns frei,", "tokens": ["Da\u00b7ge\u00b7gen", "her\u00b7zen", "wir", "uns", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind sicher vor Verr\u00e4tert\u00fccken,", "tokens": ["Sind", "si\u00b7cher", "vor", "Ver\u00b7r\u00e4\u00b7ter\u00b7t\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch schielet keine Sp\u00f6tterei,", "tokens": ["Auch", "schie\u00b7let", "kei\u00b7ne", "Sp\u00f6t\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann wir uns Knie und H\u00e4nde dr\u00fccken.", "tokens": ["Wann", "wir", "uns", "Knie", "und", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Prunk der hochstaffirten Kunst,", "tokens": ["Der", "Prunk", "der", "hoch\u00b7staf\u00b7fir\u00b7ten", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst die Natur im Feierkleide,", "tokens": ["Selbst", "die", "Na\u00b7tur", "im", "Fei\u00b7er\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erbulen selten meine Gunst;", "tokens": ["Er\u00b7bu\u00b7len", "sel\u00b7ten", "mei\u00b7ne", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie besch\u00e4mt an Reizen beide.", "tokens": ["Denn", "sie", "be\u00b7sch\u00e4mt", "an", "Rei\u00b7zen", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "NN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das tausendstimmige Konzert", "tokens": ["Das", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7ge", "Kon\u00b7zert"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Lerchen und der Nachtigallen", "tokens": ["Der", "Ler\u00b7chen", "und", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist mir kaum halb so lieb und wehrt,", "tokens": ["Ist", "mir", "kaum", "halb", "so", "lieb", "und", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann ihre Solotriller schallen.", "tokens": ["Wann", "ih\u00b7re", "So\u00b7lo\u00b7tril\u00b7ler", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Denken ist sie Pallas ganz,", "tokens": ["Im", "Den\u00b7ken", "ist", "sie", "Pal\u00b7las", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Juno ganz am edlen Gange,", "tokens": ["Und", "Ju\u00b7no", "ganz", "am", "ed\u00b7len", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Terpsikore beim Freudentanz\u2019,", "tokens": ["Ter\u00b7psi\u00b7ko\u00b7re", "beim", "Freu\u00b7den\u00b7tanz'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Euterpe neidet sie im Sange;", "tokens": ["Eu\u00b7ter\u00b7pe", "nei\u00b7det", "sie", "im", "San\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ihr weicht Aglaja, wann sie lacht,", "tokens": ["Ihr", "weicht", "Ag\u00b7la\u00b7ja", ",", "wann", "sie", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Melpomene bei sanfter Klage,", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "bei", "sanf\u00b7ter", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die Wollust ist sie in der Nacht,", "tokens": ["Die", "Wol\u00b7lust", "ist", "sie", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die holde Sitsamkeit bei Tage.", "tokens": ["Die", "hol\u00b7de", "Sit\u00b7sam\u00b7keit", "bei", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des Morgens, welch ein Malerbild!", "tokens": ["Des", "Mor\u00b7gens", ",", "welch", "ein", "Ma\u00b7ler\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wallt sie hervor in leichtem Kleide,", "tokens": ["Wallt", "sie", "her\u00b7vor", "in", "leich\u00b7tem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch ungeschn\u00fcrt, und halb verh\u00fclt", "tokens": ["Noch", "un\u00b7ge\u00b7schn\u00fcrt", ",", "und", "halb", "ver\u00b7h\u00fclt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur in ein M\u00e4ntelchen von Seide.", "tokens": ["Nur", "in", "ein", "M\u00e4n\u00b7tel\u00b7chen", "von", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entringelt auf die Schulter sinkt", "tokens": ["Ent\u00b7rin\u00b7gelt", "auf", "die", "Schul\u00b7ter", "sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00e4lfte goldner Locken nieder.", "tokens": ["Die", "H\u00e4lf\u00b7te", "gold\u00b7ner", "Lo\u00b7cken", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie dann ihr rasches Auge blinkt,", "tokens": ["Wie", "dann", "ihr", "ra\u00b7sches", "Au\u00b7ge", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So blinkt das Licht aus Quellen wieder.", "tokens": ["So", "blinkt", "das", "Licht", "aus", "Quel\u00b7len", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Natur und Einfalt helfen ihr,", "tokens": ["Na\u00b7tur", "und", "Ein\u00b7falt", "hel\u00b7fen", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An ihrem kleinen Morgentischgen.", "tokens": ["An", "ih\u00b7rem", "klei\u00b7nen", "Mor\u00b7gen\u00b7tischgen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Busens und des Hauptes Zier", "tokens": ["Des", "Bu\u00b7sens", "und", "des", "Haup\u00b7tes", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Ros\u2019 und Myrt\u2019 in einem B\u00fcschgen.", "tokens": ["Sind", "Ros'", "und", "Myrt'", "in", "ei\u00b7nem", "B\u00fcschgen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu ihren Wangen wurde nie", "tokens": ["Zu", "ih\u00b7ren", "Wan\u00b7gen", "wur\u00b7de", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Pinsel in Karmin getauchet;", "tokens": ["Ein", "Pin\u00b7sel", "in", "Kar\u00b7min", "ge\u00b7tau\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und doch, wie Rosen, bl\u00fchen sie,", "tokens": ["Und", "doch", ",", "wie", "Ro\u00b7sen", ",", "bl\u00fc\u00b7hen", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Fr\u00fchlingsodem aufgehauchet.", "tokens": ["Von", "Fr\u00fch\u00b7lin\u00b7gso\u00b7dem", "auf\u00b7ge\u00b7hau\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wann sie an ihrem Tischgen sizt,", "tokens": ["Wann", "sie", "an", "ih\u00b7rem", "Tischgen", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So werd\u2019 ich scherzend hingewinket:", "tokens": ["So", "werd'", "ich", "scher\u00b7zend", "hin\u00b7ge\u00b7win\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ekom, schm\u00fccke selbst dein M\u00e4dchen izt,", "tokens": ["\u201e", "kom", ",", "schm\u00fc\u00b7cke", "selbst", "dein", "M\u00e4d\u00b7chen", "izt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie deiner Laun\u2019 am besten d\u00fcnket!\u201e", "tokens": ["Wie", "dei\u00b7ner", "Laun'", "am", "bes\u00b7ten", "d\u00fcn\u00b7ket", "!", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKA", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich befl\u00fcgelt ihr Gebot,", "tokens": ["Und", "mich", "be\u00b7fl\u00fc\u00b7gelt", "ihr", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie unvermutet zu umfangen.", "tokens": ["Sie", "un\u00b7ver\u00b7mu\u00b7tet", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann schminkt mit hohem Morgenrot", "tokens": ["Dann", "schminkt", "mit", "ho\u00b7hem", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Kus die jugendlichen Wangen.", "tokens": ["Mein", "Kus", "die", "ju\u00b7gend\u00b7li\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr Haar im Nacken reizet mich", "tokens": ["Ihr", "Haar", "im", "Na\u00b7cken", "rei\u00b7zet", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu hundert kleinen Thorenspielen.", "tokens": ["Zu", "hun\u00b7dert", "klei\u00b7nen", "Tho\u00b7ren\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast nimmer m\u00fcde l\u00e4st es sich", "tokens": ["Fast", "nim\u00b7mer", "m\u00fc\u00b7de", "l\u00e4st", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesen seidnen Locken w\u00fclen.", "tokens": ["In", "die\u00b7sen", "seid\u00b7nen", "Lo\u00b7cken", "w\u00fc\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie \u00e4ugelt nach dem Spiegel hin,", "tokens": ["Sie", "\u00e4u\u00b7gelt", "nach", "dem", "Spie\u00b7gel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lauschet meinen Neckereien.", "tokens": ["Und", "lau\u00b7schet", "mei\u00b7nen", "Ne\u00b7cke\u00b7rei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schilt, da\u00df ich ein T\u00e4ndler bin,", "tokens": ["Sie", "schilt", ",", "da\u00df", "ich", "ein", "T\u00e4nd\u00b7ler", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und freut sich doch der T\u00e4ndeleien.", "tokens": ["Und", "freut", "sich", "doch", "der", "T\u00e4n\u00b7de\u00b7lei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drauf leg\u2019 ich ihr die Schn\u00fcrbrust an.", "tokens": ["Drauf", "leg'", "ich", "ihr", "die", "Schn\u00fcr\u00b7brust", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Wonne beben mir die H\u00e4nde.", "tokens": ["Vor", "Won\u00b7ne", "be\u00b7ben", "mir", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Band zerreist, so oft es kan,", "tokens": ["Das", "Band", "zer\u00b7reist", ",", "so", "oft", "es", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit die Arbeit sp\u00e4ter ende.", "tokens": ["Da\u00b7mit", "die", "Ar\u00b7beit", "sp\u00e4\u00b7ter", "en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie flink bin ich nicht stets bereit,", "tokens": ["Wie", "flink", "bin", "ich", "nicht", "stets", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So liebe Dienste zu verrichten!", "tokens": ["So", "lie\u00b7be", "Diens\u00b7te", "zu", "ver\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch flinker noch, zur Abendzeit,", "tokens": ["Doch", "flin\u00b7ker", "noch", ",", "zur", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Werk des Morgens zu zernichten.", "tokens": ["Das", "Werk", "des", "Mor\u00b7gens", "zu", "zer\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun schlinget meine k\u00fchne Hand \u2014", "tokens": ["Nun", "schlin\u00b7get", "mei\u00b7ne", "k\u00fch\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Liebe, Liebe, welche Gnade! \u2014", "tokens": ["O", "Lie\u00b7be", ",", "Lie\u00b7be", ",", "wel\u00b7che", "Gna\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "PWAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sanf