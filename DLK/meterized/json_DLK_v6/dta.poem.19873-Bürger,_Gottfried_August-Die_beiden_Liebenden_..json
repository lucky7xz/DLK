n_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schl\u00e4gt mit den Silberf\u00fcschen Triller,", "tokens": ["Schl\u00e4gt", "mit", "den", "Sil\u00b7ber\u00b7f\u00fc\u00b7schen", "Tril\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tanzet hin an das Klavier,", "tokens": ["Und", "tan\u00b7zet", "hin", "an", "das", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und singt ein Lied, nach Wei\u00df, von Miller.", "tokens": ["Und", "singt", "ein", "Lied", ",", "nach", "Wei\u00df", ",", "von", "Mil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "NE", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit welcher Wollustf\u00fclle schwelt", "tokens": ["Mit", "wel\u00b7cher", "Wol\u00b7lust\u00b7f\u00fcl\u00b7le", "schwelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz der Zauber ihrer Kehle!", "tokens": ["Mein", "Herz", "der", "Zau\u00b7ber", "ih\u00b7rer", "Keh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hinweg, aus aller Gotteswelt,", "tokens": ["Hin\u00b7weg", ",", "aus", "al\u00b7ler", "Got\u00b7tes\u00b7welt", ","], "t