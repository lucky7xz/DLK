{"textgrid.poem.43742": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Birnenschmaus", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So komm, du lieber Sonnenschein,", "tokens": ["So", "komm", ",", "du", "lie\u00b7ber", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df unsre Birnen gut gedeih'n!", "tokens": ["La\u00df", "uns\u00b7re", "Bir\u00b7nen", "gut", "ge\u00b7deih'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn sie gelb geworden sind,", "tokens": ["Und", "wenn", "sie", "gelb", "ge\u00b7wor\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann komm und wehe, lieber Wind!", "tokens": ["Dann", "komm", "und", "we\u00b7he", ",", "lie\u00b7ber", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, Wind, und sch\u00fcttle jeden Ast", "tokens": ["Komm", ",", "Wind", ",", "und", "sch\u00fctt\u00b7le", "je\u00b7den", "Ast"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lad' uns alle samt zu Gast!", "tokens": ["Und", "lad'", "uns", "al\u00b7le", "samt", "zu", "Gast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann eilen wir zum Haus hinaus", "tokens": ["Dann", "ei\u00b7len", "wir", "zum", "Haus", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und halten einen Birnenschmaus.", "tokens": ["Und", "hal\u00b7ten", "ei\u00b7nen", "Bir\u00b7nen\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}