{"textgrid.poem.36641": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein S\u00e4nger, der in weiter Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein S\u00e4nger, der in weiter Ferne", "tokens": ["Ein", "S\u00e4n\u00b7ger", ",", "der", "in", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom deutschen Vaterlande lebt,", "tokens": ["Vom", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dessen Geist und Herz so gerne", "tokens": ["In", "des\u00b7sen", "Geist", "und", "Herz", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Heimat Bild her\u00fcberschwebt,", "tokens": ["Der", "Hei\u00b7mat", "Bild", "her\u00b7\u00fc\u00b7bersc\u00b7hwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Singt unter Fr\u00fchlingslaub und Bl\u00fcte", "tokens": ["Singt", "un\u00b7ter", "Fr\u00fch\u00b7lings\u00b7laub", "und", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum ersten Mal voll stiller Ruh", "tokens": ["Zum", "ers\u00b7ten", "Mal", "voll", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im tiefbes\u00e4nftigten Gem\u00fcthe", "tokens": ["Im", "tief\u00b7be\u00b7s\u00e4nf\u00b7tig\u00b7ten", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Lied euch in den Norden zu.", "tokens": ["Sein", "Lied", "euch", "in", "den", "Nor\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Euch Allen r\u00fchrt sie sanft den Busen,", "tokens": ["Euch", "Al\u00b7len", "r\u00fchrt", "sie", "sanft", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehnsucht nach dem sch\u00f6nen Land,", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "dem", "sch\u00f6\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo einst der heil'ge Chor der Musen", "tokens": ["Wo", "einst", "der", "heil'\u00b7ge", "Chor", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vorzeit Lorbeerkr\u00e4nze band,", "tokens": ["Der", "Vor\u00b7zeit", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7ze", "band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unsterbliche, gepries'ne Siege", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", ",", "ge\u00b7pries'\u00b7ne", "Sie\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Weltgebieter einst gekr\u00f6nt,", "tokens": ["Die", "Welt\u00b7ge\u00b7bie\u00b7ter", "einst", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Sanzio seine gro\u00dfe Wiege", "tokens": ["Und", "San\u00b7zio", "sei\u00b7ne", "gro\u00b7\u00dfe", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit allem Himmelsglanz versch\u00f6nt.", "tokens": ["Mit", "al\u00b7lem", "Him\u00b7mels\u00b7glanz", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum hofft der S\u00e4nger, auch willkommen", "tokens": ["Drum", "hofft", "der", "S\u00e4n\u00b7ger", ",", "auch", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Herzensgru\u00df zu sein:", "tokens": ["Mit", "sei\u00b7nem", "Her\u00b7zens\u00b7gru\u00df", "zu", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ob ihm schon das Gl\u00fcck genommen", "tokens": ["Denn", "ob", "ihm", "schon", "das", "Gl\u00fcck", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was wild und zart, was gro\u00df und klein", "tokens": ["Was", "wild", "und", "zart", ",", "was", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das hei\u00dfe Herz ihm einst erfreute,", "tokens": ["Das", "hei\u00b7\u00dfe", "Herz", "ihm", "einst", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Heimat wie der Liebe Lust;", "tokens": ["Der", "Hei\u00b7mat", "wie", "der", "Lie\u00b7be", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach Wonnen, die er nie bereute,", "tokens": ["Ach", "Won\u00b7nen", ",", "die", "er", "nie", "be\u00b7reu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sehnsucht jeder Menschenbrust;", "tokens": ["Die", "Sehn\u00b7sucht", "je\u00b7der", "Men\u00b7schen\u00b7brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ist der Trennung bittre Klage,", "tokens": ["Doch", "ist", "der", "Tren\u00b7nung", "bitt\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ach des Lebewohls gestillt,", "tokens": ["Das", "Ach", "des", "Le\u00b7be\u00b7wohls", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Gram verlorner Tage,", "tokens": ["Und", "al\u00b7len", "Gram", "ver\u00b7lor\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das tr\u00fcbe Nachtst\u00fcck, \u00fcberschwillt", "tokens": ["Das", "tr\u00fc\u00b7be", "Nacht\u00b7st\u00fcck", ",", "\u00fc\u00b7bersc\u00b7hwillt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die reine Flut des neuen Lebens,", "tokens": ["Die", "rei\u00b7ne", "Flut", "des", "neu\u00b7en", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo die Vergangenheit versank,", "tokens": ["Wo", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ich des wunden Seelenstrebens", "tokens": ["Wo", "ich", "des", "wun\u00b7den", "See\u00b7len\u00b7stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergessenheit in F\u00fclle trank.", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", "in", "F\u00fcl\u00b7le", "trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein feuchtes Auge voll Vertrauen,", "tokens": ["Kein", "feuch\u00b7tes", "Au\u00b7ge", "voll", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Liebesweh, voll sel'gem Wahn,", "tokens": ["Voll", "Lie\u00b7bes\u00b7weh", ",", "voll", "sel'\u00b7gem", "Wahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wohl auf immergr\u00fcnen Auen", "tokens": ["Doch", "wohl", "auf", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt mich manch s\u00fc\u00dfes Veilchen an;", "tokens": ["Blickt", "mich", "manch", "s\u00fc\u00b7\u00dfes", "Veil\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach keiner Lippe holdes Schmachten,", "tokens": ["Ach", "kei\u00b7ner", "Lip\u00b7pe", "hol\u00b7des", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Seufzer, kein beredter Schwall,", "tokens": ["Kein", "Seuf\u00b7zer", ",", "kein", "be\u00b7red\u00b7ter", "Schwall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch Haine, die schon Flaccus lachten,", "tokens": ["Doch", "Hai\u00b7ne", ",", "die", "schon", "Flac\u00b7cus", "lach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll vom Gesang der Nachtigall!", "tokens": ["Voll", "vom", "Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl jauchzt die Seele voll Entz\u00fccken,", "tokens": ["Wohl", "jauchzt", "die", "See\u00b7le", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn von M\u00e4cenas Wunderhaus,", "tokens": ["Wenn", "von", "M\u00e4\u00b7ce\u00b7nas", "Wun\u00b7der\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gleich einem Schleier anzublicken,", "tokens": ["Gleich", "ei\u00b7nem", "Schlei\u00b7er", "an\u00b7zu\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus alter B\u00f6gen Nacht heraus,", "tokens": ["Aus", "al\u00b7ter", "B\u00f6\u00b7gen", "Nacht", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Tiburs Fels, wie aus den L\u00fcften,", "tokens": ["Von", "Ti\u00b7burs", "Fels", ",", "wie", "aus", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die silberne Kaskade sch\u00e4umt,", "tokens": ["Die", "sil\u00b7ber\u00b7ne", "Kas\u00b7ka\u00b7de", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Im Wasserklang, in Blumend\u00fcften", "tokens": ["Im", "Was\u00b7ser\u00b7klang", ",", "in", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die gro\u00dfe, sch\u00f6ne Vorwelt tr\u00e4umt!", "tokens": ["Die", "gro\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", "Vor\u00b7welt", "tr\u00e4umt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn sie an deinem klaren Spiegel,", "tokens": ["Wenn", "sie", "an", "dei\u00b7nem", "kla\u00b7ren", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dianensee, dem Winde lauscht,", "tokens": ["Di\u00b7a\u00b7nen\u00b7see", ",", "dem", "Win\u00b7de", "lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der in dem Laub mit sanftem Fl\u00fcgel", "tokens": ["Der", "in", "dem", "Laub", "mit", "sanf\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich einem Geist der Fabel rauscht;", "tokens": ["Gleich", "ei\u00b7nem", "Geist", "der", "Fa\u00b7bel", "rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Lust, die nur die G\u00f6tter kennen,", "tokens": ["O", "Lust", ",", "die", "nur", "die", "G\u00f6t\u00b7ter", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn oft so unaussprechlich hold", "tokens": ["Wenn", "oft", "so", "un\u00b7aus\u00b7sprech\u00b7lich", "hold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die lichten gr\u00fcnen Haine brennen,", "tokens": ["Die", "lich\u00b7ten", "gr\u00fc\u00b7nen", "Hai\u00b7ne", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Psyche schwelgt im Abendgold;", "tokens": ["Und", "Psy\u00b7che", "schwelgt", "im", "A\u00b7bend\u00b7gold", ";"], "token_info": ["word", "word", "word", "