{"textgrid.poem.41581": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage im Advent", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf keinen andern wart' ich mehr,", "tokens": ["Auf", "kei\u00b7nen", "an\u00b7dern", "wart'", "ich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer soll noch Liebres kommen mir?", "tokens": ["Wer", "soll", "noch", "Lieb\u00b7res", "kom\u00b7men", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer soll so mild und doch so hehr", "tokens": ["Wer", "soll", "so", "mild", "und", "doch", "so", "hehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir treten an des Herzens T\u00fcr?", "tokens": ["Mir", "tre\u00b7ten", "an", "des", "Her\u00b7zens", "T\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer durch des Fiebers Qual und Brennen", "tokens": ["Wer", "durch", "des", "Fie\u00b7bers", "Qual", "und", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So liebreich meinen Namen nennen,", "tokens": ["So", "lieb\u00b7reich", "mei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Balsamtr\u00e4ufeln f\u00fcr und f\u00fcr?", "tokens": ["Ein", "Bal\u00b7sam\u00b7tr\u00e4u\u00b7feln", "f\u00fcr", "und", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du wu\u00dftest es von Ewigkeit,", "tokens": ["Du", "wu\u00df\u00b7test", "es", "von", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df der Gedanken \u00dcberma\u00df,", "tokens": ["Da\u00df", "der", "Ge\u00b7dan\u00b7ken", "\u00dc\u00b7berm\u00b7a\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dem Sinn entzogne Herrlichkeit,", "tokens": ["Dem", "Sinn", "ent\u00b7zog\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zersprengen m\u00fc\u00dft' mein Hirn wie Glas;", "tokens": ["Zer\u00b7spren\u00b7gen", "m\u00fc\u00dft'", "mein", "Hirn", "wie", "Glas", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kamst du niedrig unsersgleichen,", "tokens": ["So", "kamst", "du", "nied\u00b7rig", "un\u00b7sers\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie zu der Armut Fromme schleichen,", "tokens": ["Wie", "zu", "der", "Ar\u00b7mut", "From\u00b7me", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich setzend wo der Bettler sa\u00df.", "tokens": ["Sich", "set\u00b7zend", "wo", "der", "Bett\u00b7ler", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn fast zum Schwindeln mich gebracht", "tokens": ["Wenn", "fast", "zum", "Schwin\u00b7deln", "mich", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wirbelnden Betrachtung Kreis,", "tokens": ["Der", "wir\u00b7beln\u00b7den", "Be\u00b7trach\u00b7tung", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dann trittst du aus der D\u00fcnste Nacht", "tokens": ["Dann", "trittst", "du", "aus", "der", "D\u00fcns\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Stimme fl\u00fcstert leis:", "tokens": ["Und", "dei\u00b7ne", "Stim\u00b7me", "fl\u00fcs\u00b7tert", "leis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier bin ich, bin ich, woll' mich fassen,", "tokens": ["Hier", "bin", "ich", ",", "bin", "ich", ",", "woll'", "mich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann magst du alles andre lassen;", "tokens": ["Dann", "magst", "du", "al\u00b7les", "and\u00b7re", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf meinem Kreuze hegt der Preis.", "tokens": ["Auf", "mei\u00b7nem", "Kreu\u00b7ze", "hegt", "der", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Stimme, immer mir bekannt,", "tokens": ["O", "Stim\u00b7me", ",", "im\u00b7mer", "mir", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Wort, das stets verst\u00e4ndlich mir,", "tokens": ["O", "Wort", ",", "das", "stets", "ver\u00b7st\u00e4nd\u00b7lich", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du legst mir auf der Liebe Band", "tokens": ["Du", "legst", "mir", "auf", "der", "Lie\u00b7be", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Schritte folgen dir!", "tokens": ["Und", "mei\u00b7ne", "Schrit\u00b7te", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Liebe glaub' ich, liebewund", "tokens": ["In", "Lie\u00b7be", "glaub'", "ich", ",", "lie\u00b7be\u00b7wund"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schieb' ich des Herzens T\u00fcr auf, und", "tokens": ["Schieb'", "ich", "des", "Her\u00b7zens", "T\u00fcr", "auf", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschlossen ist des Gr\u00fcbelns T\u00fcr,", "tokens": ["Ge\u00b7schlos\u00b7sen", "ist", "des", "Gr\u00fc\u00b7belns", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gehemmt die Jagd, durch scharfen Stein", "tokens": ["Ge\u00b7hemmt", "die", "Jagd", ",", "durch", "schar\u00b7fen", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dornen hetzend meinen Fu\u00df;", "tokens": ["Und", "Dor\u00b7nen", "het\u00b7zend", "mei\u00b7nen", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ruh' in deinem k\u00fchlen Hain", "tokens": ["Ich", "ruh'", "in", "dei\u00b7nem", "k\u00fch\u00b7len", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lausche deinem sanften Gru\u00df.", "tokens": ["Und", "lau\u00b7sche", "dei\u00b7nem", "sanf\u00b7ten", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Blinden sehn, die Kalten gl\u00fchen", "tokens": ["Die", "Blin\u00b7den", "sehn", ",", "die", "Kal\u00b7ten", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aus des Irren Haupte ziehen", "tokens": ["Und", "aus", "des", "Ir\u00b7ren", "Haup\u00b7te", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der dumpfen Schatten Menge mu\u00df.", "tokens": ["Der", "dum\u00b7pfen", "Schat\u00b7ten", "Men\u00b7ge", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich folge dir zu Berges H\u00f6hn,", "tokens": ["Ich", "fol\u00b7ge", "dir", "zu", "Ber\u00b7ges", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Leben von den Lippen flie\u00dft,", "tokens": ["Wo", "Le\u00b7ben", "von", "den", "Lip\u00b7pen", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Tr\u00e4nen darf ich sehn,", "tokens": ["Und", "dei\u00b7ne", "Tr\u00e4\u00b7nen", "darf", "ich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O tausendmal mit Heil gegr\u00fc\u00dft,", "tokens": ["O", "tau\u00b7send\u00b7mal", "mit", "Heil", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df in Gethsemane erzittern,", "tokens": ["Mu\u00df", "in", "Geth\u00b7se\u00b7ma\u00b7ne", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Da\u00df Schrecken Gottes Leib ersch\u00fcttern,", "tokens": ["Da\u00df", "Schre\u00b7cken", "Got\u00b7tes", "Leib", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Blutschwei\u00dfe Gottes Stirn vergie\u00dft.", "tokens": ["Blut\u00b7schwei\u00b7\u00dfe", "Got\u00b7tes", "Stirn", "ver\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Er hat gehorsam bis zum Tod,", "tokens": ["Er", "hat", "ge\u00b7hor\u00b7sam", "bis", "zum", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja zu des Todes eitlem Graus,", "tokens": ["Ja", "zu", "des", "To\u00b7des", "eit\u00b7lem", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gekostet jede Menschennot", "tokens": ["Ge\u00b7kos\u00b7tet", "je\u00b7de", "Men\u00b7schen\u00b7not"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trank den vollen Becher aus.", "tokens": ["Und", "trank", "den", "vol\u00b7len", "Be\u00b7cher", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So richte dich aus Dorn und H\u00f6hle,", "tokens": ["So", "rich\u00b7te", "dich", "aus", "Dorn", "und", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du meine angstgeknickte Seele,", "tokens": ["Du", "mei\u00b7ne", "angst\u00b7ge\u00b7knick\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch du nur tr\u00e4gst ein irdisch Haus.", "tokens": ["Auch", "du", "nur", "tr\u00e4gst", "ein", "ir\u00b7disch", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df wanken denn die Tr\u00fcmmer grau", "tokens": ["La\u00df", "wan\u00b7ken", "denn", "die", "Tr\u00fcm\u00b7mer", "grau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mische deine Tr\u00e4nen nur", "tokens": ["Und", "mi\u00b7sche", "dei\u00b7ne", "Tr\u00e4\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deines Heilands blut'gem Tau,", "tokens": ["Mit", "dei\u00b7nes", "Hei\u00b7lands", "blut'\u00b7gem", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gequ\u00e4lter Sklave der Natur!", "tokens": ["Ge\u00b7qu\u00e4l\u00b7ter", "Skla\u00b7ve", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, dessen Schwei\u00df den Grund ger\u00f6tet,", "tokens": ["O", ",", "des\u00b7sen", "Schwei\u00df", "den", "Grund", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er wei\u00df es, wie ein Seufzer betet,", "tokens": ["Er", "wei\u00df", "es", ",", "wie", "ein", "Seuf\u00b7zer", "be\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Jesu, meine Hoffnungsau!", "tokens": ["Mein", "Je\u00b7su", ",", "mei\u00b7ne", "Hoff\u00b7nun\u00b7gsau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}