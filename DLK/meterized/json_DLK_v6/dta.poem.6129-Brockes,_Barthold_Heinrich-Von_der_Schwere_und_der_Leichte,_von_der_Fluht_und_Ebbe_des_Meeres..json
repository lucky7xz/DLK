{"dta.poem.6129": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Daselbst nun, wo der Durchschnitt lang,", "tokens": ["Da\u00b7selbst", "nun", ",", "wo", "der", "Durch\u00b7schnitt", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo der Lauff am mindsten schnelle gehet,", "tokens": ["Und", "wo", "der", "Lauff", "am", "minds\u00b7ten", "schnel\u00b7le", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4sst unsers Wirbels Lauff, der sich best\u00e4ndig drehet,", "tokens": ["L\u00e4sst", "un\u00b7sers", "Wir\u00b7bels", "Lauff", ",", "der", "sich", "be\u00b7st\u00e4n\u00b7dig", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Wasser wieder seinen Gang.", "tokens": ["Dem", "Was\u00b7ser", "wie\u00b7der", "sei\u00b7nen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sencket sich das Meer in seine Lager-Statt:", "tokens": ["Es", "sen\u00b7cket", "sich", "das", "Meer", "in", "sei\u00b7ne", "La\u00b7ger\u00b7Statt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Fl\u00fcsse sind so dann entbunden.", "tokens": ["Die", "Fl\u00fcs\u00b7se", "sind", "so", "dann", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Wasser, da es so verschiednen Eindruck hat,", "tokens": ["Das", "Was\u00b7ser", ",", "da", "es", "so", "ver\u00b7schied\u00b7nen", "Ein\u00b7druck", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erhebt sich jeden Tag zweymal sechs Stunden,", "tokens": ["Er\u00b7hebt", "sich", "je\u00b7den", "Tag", "zwey\u00b7mal", "sechs", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und sinckt zweymal sechs Stunden wieder,", "tokens": ["Und", "sinckt", "zwey\u00b7mal", "sechs", "Stun\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald frey und bald gezwungen, nieder.", "tokens": ["Bald", "frey", "und", "bald", "ge\u00b7zwun\u00b7gen", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}