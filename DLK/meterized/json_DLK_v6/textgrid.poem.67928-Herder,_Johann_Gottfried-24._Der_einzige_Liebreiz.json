{"textgrid.poem.67928": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24. Der einzige Liebreiz", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit nicht, o M\u00e4dchen, nicht", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "nicht", ",", "o", "M\u00e4d\u00b7chen", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKNEG", "$,", "FM", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit uns begl\u00fcckt!", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "uns", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonn', ein Engelsangesicht,", "tokens": ["Die", "Sonn'", ",", "ein", "En\u00b7gel\u00b7san\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht blind, wer in sie blickt.", "tokens": ["Macht", "blind", ",", "wer", "in", "sie", "blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Putz uns nicht, o M\u00e4dchen, nicht", "tokens": ["Dein", "Putz", "uns", "nicht", ",", "o", "M\u00e4d\u00b7chen", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Putz uns selig macht;", "tokens": ["Dein", "Putz", "uns", "se\u00b7lig", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfau gar bunte Farben bricht,", "tokens": ["Der", "Pfau", "gar", "bun\u00b7te", "Far\u00b7ben", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dummer leerer Pracht.", "tokens": ["In", "dum\u00b7mer", "lee\u00b7rer", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Witzes Pfeil, ein spitzer Pfeil", "tokens": ["Des", "Wit\u00b7zes", "Pfeil", ",", "ein", "spit\u00b7zer", "Pfeil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trift selten tief das Herz,", "tokens": ["Trift", "sel\u00b7ten", "tief", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er fliegt vorbei in schneller Eil", "tokens": ["Er", "fliegt", "vor\u00b7bei", "in", "schnel\u00b7ler", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4sset \u00f6fters Schmerz.", "tokens": ["Und", "l\u00e4s\u00b7set", "\u00f6f\u00b7ters", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur Eine Macht, der nichts entgeht,", "tokens": ["Nur", "Ei\u00b7ne", "Macht", ",", "der", "nichts", "ent\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Eine kenn' ich nur:", "tokens": ["Und", "Ei\u00b7ne", "kenn'", "ich", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O M\u00e4dchen, wenn sie bei dir steht! \u2013", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "wenn", "sie", "bei", "dir", "steht", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hei\u00dft: Natur! Natur!", "tokens": ["Sie", "hei\u00dft", ":", "Na\u00b7tur", "!", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}