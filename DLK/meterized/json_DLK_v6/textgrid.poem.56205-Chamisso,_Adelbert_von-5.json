{"textgrid.poem.56205": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kraft der Erde, Licht der Sonne,", "tokens": ["Kraft", "der", "Er\u00b7de", ",", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00e4umt der edle Wein;", "tokens": ["Sch\u00e4umt", "der", "ed\u00b7le", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00dft, ihr Br\u00fcder, ernst und heilig", "tokens": ["La\u00dft", ",", "ihr", "Br\u00fc\u00b7der", ",", "ernst", "und", "hei\u00b7lig"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Stimmung sein.", "tokens": ["Uns\u00b7re", "Stim\u00b7mung", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Heute nicht dem Rausch der Freude,", "tokens": ["Heu\u00b7te", "nicht", "dem", "Rausch", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht der eiteln Lust,", "tokens": ["Nicht", "der", "ei\u00b7teln", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nein dem Gotte soll er gelten", "tokens": ["Nein", "dem", "Got\u00b7te", "soll", "er", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief in unsrer Brust.", "tokens": ["Tief", "in", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gleich dem Weine warm und kr\u00e4ftig,", "tokens": ["Gleich", "dem", "Wei\u00b7ne", "warm", "und", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter, rein und klar,", "tokens": ["Lau\u00b7ter", ",", "rein", "und", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bringen wir das volle Leben", "tokens": ["Brin\u00b7gen", "wir", "das", "vol\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm zum Opfer dar.", "tokens": ["Ihm", "zum", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schmach der Feigheit! Krieg der L\u00fcge!", "tokens": ["Schmach", "der", "Feig\u00b7heit", "!", "Krieg", "der", "L\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allem Schlechten Krieg!", "tokens": ["Al\u00b7lem", "Schlech\u00b7ten", "Krieg", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Herrlich f\u00fcr die Freiheit sterben,", "tokens": ["Herr\u00b7lich", "f\u00fcr", "die", "Frei\u00b7heit", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrlicher der Sieg!", "tokens": ["Herr\u00b7li\u00b7cher", "der", "Sieg", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Wir f\u00fcr Menschenrecht und W\u00fcrde", "tokens": ["Wir", "f\u00fcr", "Men\u00b7schen\u00b7recht", "und", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpen allzumal,", "tokens": ["K\u00e4m\u00b7pen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weihen den gefallnen Helden", "tokens": ["Wei\u00b7hen", "den", "ge\u00b7fall\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funkelnd den Pokal.", "tokens": ["Fun\u00b7kelnd", "den", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}