{"textgrid.poem.25935": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir gingen sacht dem Abend nach,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gingen sacht dem Abend nach,", "tokens": ["Wir", "gin\u00b7gen", "sacht", "dem", "A\u00b7bend", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel war ein goldnes Dach,", "tokens": ["Der", "Him\u00b7mel", "war", "ein", "gold\u00b7nes", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tann voll dunkler Kammern stand,", "tokens": ["Der", "Tann", "voll", "dunk\u00b7ler", "Kam\u00b7mern", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie ein Bett das \u00c4hrenland.", "tokens": ["Und", "wie", "ein", "Bett", "das", "\u00c4h\u00b7ren\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Lockruf tat ans Ohr mir gehen,", "tokens": ["Ein", "Lock\u00b7ruf", "tat", "ans", "Ohr", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "And\u00e4chtig blieb der Fu\u00df uns stehen.", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "blieb", "der", "Fu\u00df", "uns", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Wachtel Herz noch sp\u00e4t anschlug,", "tokens": ["Der", "Wach\u00b7tel", "Herz", "noch", "sp\u00e4t", "an\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie hatte nicht vom Tag genug,", "tokens": ["Sie", "hat\u00b7te", "nicht", "vom", "Tag", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie fand noch keine Abendzeit,", "tokens": ["Sie", "fand", "noch", "kei\u00b7ne", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Rief noch ihr Gl\u00fcck aus meilenweit.", "tokens": ["Rief", "noch", "ihr", "Gl\u00fcck", "aus", "mei\u00b7len\u00b7weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}