{"textgrid.poem.26628": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr nur, schattige Gassen und hell beschienene Pl\u00e4tze", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr nur, schattige Gassen und hell beschienene Pl\u00e4tze", "tokens": ["Ihr", "nur", ",", "schat\u00b7ti\u00b7ge", "Gas\u00b7sen", "und", "hell", "be\u00b7schie\u00b7ne\u00b7ne", "Pl\u00e4t\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Tief im Inn'ren der Stadt \u2013 ihr seid allein mir noch Wien!", "tokens": ["Tief", "im", "Inn'\u00b7ren", "der", "Stadt", "\u2013", "ihr", "seid", "al\u00b7lein", "mir", "noch", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$(", "PPER", "VAFIN", "ADV", "PPER", "ADV", "NE", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "O wie hebt sich die Brust, nun ich euch wieder betrete,", "tokens": ["O", "wie", "hebt", "sich", "die", "Brust", ",", "nun", "ich", "euch", "wie\u00b7der", "be\u00b7tre\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und bei jeglichem Schritt Liebes, Bekanntes mich gr\u00fc\u00dft!", "tokens": ["Und", "bei", "jeg\u00b7li\u00b7chem", "Schritt", "Lie\u00b7bes", ",", "Be\u00b7kann\u00b7tes", "mich", "gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "NN", "$,", "NN", "PPER", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ja, hier pulst noch das Leben! An alten Pal\u00e4sten und H\u00e4usern \u2013", "tokens": ["Ja", ",", "hier", "pulst", "noch", "das", "Le\u00b7ben", "!", "An", "al\u00b7ten", "Pa\u00b7l\u00e4s\u00b7ten", "und", "H\u00e4u\u00b7sern", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$.", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "An Sankt Stephan vorbei fluthet und wogt es wie einst.", "tokens": ["An", "Sankt", "Ste\u00b7phan", "vor\u00b7bei", "flut\u00b7het", "und", "wogt", "es", "wie", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "KOKOM", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Treibend im bunten Gew\u00fchl versch\u00e4rfen sich alle Contraste,", "tokens": ["Trei\u00b7bend", "im", "bun\u00b7ten", "Ge\u00b7w\u00fchl", "ver\u00b7sch\u00e4r\u00b7fen", "sich", "al\u00b7le", "Cont\u00b7ras\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "+--+--+-+--+-+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und der Einzelne wird ", "tokens": ["Und", "der", "Ein\u00b7zel\u00b7ne", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Typen treten hervor, es waltet die Seele des Volkes,", "tokens": ["Ty\u00b7pen", "tre\u00b7ten", "her\u00b7vor", ",", "es", "wal\u00b7tet", "die", "See\u00b7le", "des", "Vol\u00b7kes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Die im Wechsel der Zeit dennoch unsterblich sich weist;", "tokens": ["Die", "im", "Wech\u00b7sel", "der", "Zeit", "den\u00b7noch", "uns\u00b7terb\u00b7lich", "sich", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Waltet im Drang nach Genu\u00df, in gern verweilender Schaulust,", "tokens": ["Wal\u00b7tet", "im", "Drang", "nach", "Ge\u00b7nu\u00df", ",", "in", "gern", "ver\u00b7wei\u00b7len\u00b7der", "Schau\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Welche die L\u00e4den umdr\u00e4ngt, w\u00e4hrend die Stunde entflieht.", "tokens": ["Wel\u00b7che", "die", "L\u00e4\u00b7den", "um\u00b7dr\u00e4ngt", ",", "w\u00e4h\u00b7rend", "die", "Stun\u00b7de", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Lieblich entfaltet die Wienerin noch den gepriesenen Zauber,", "tokens": ["Lieb\u00b7lich", "ent\u00b7fal\u00b7tet", "die", "Wie\u00b7ne\u00b7rin", "noch", "den", "ge\u00b7prie\u00b7se\u00b7nen", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Ob im schleppenden Kleid, ob im gesch\u00fcrzten sie geht;", "tokens": ["Ob", "im", "schlep\u00b7pen\u00b7den", "Kleid", ",", "ob", "im", "ge\u00b7sch\u00fcrz\u00b7ten", "sie", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,", "KOUS", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Mit begehrendem Blick verfolgt sie das m\u00e4nnliche Auge,", "tokens": ["Mit", "be\u00b7geh\u00b7ren\u00b7dem", "Blick", "ver\u00b7folgt", "sie", "das", "m\u00e4nn\u00b7li\u00b7che", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Und der gefl\u00fcgelte Gott flattert wie fr\u00fcher umher. \u2013", "tokens": ["Und", "der", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Gott", "flat\u00b7tert", "wie", "fr\u00fc\u00b7her", "um\u00b7her", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Freilich vollzieht sich auch hier stets rascher ein Wandel der Dinge,", "tokens": ["Frei\u00b7lich", "voll\u00b7zieht", "sich", "auch", "hier", "stets", "ra\u00b7scher", "ein", "Wan\u00b7del", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.18": {"text": "Fast mit jeglichem Jahr schwindet ein Reiz aus dem Bild;", "tokens": ["Fast", "mit", "jeg\u00b7li\u00b7chem", "Jahr", "schwin\u00b7det", "ein", "Reiz", "aus", "dem", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Aber noch immer behauptet sich Altes inmitten des Neuen,", "tokens": ["A\u00b7ber", "noch", "im\u00b7mer", "be\u00b7haup\u00b7tet", "sich", "Al\u00b7tes", "in\u00b7mit\u00b7ten", "des", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und Vergangenheit tr\u00e4umt still in die Zukunft hinein.", "tokens": ["Und", "Ver\u00b7gan\u00b7gen\u00b7heit", "tr\u00e4umt", "still", "in", "die", "Zu\u00b7kunft", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}