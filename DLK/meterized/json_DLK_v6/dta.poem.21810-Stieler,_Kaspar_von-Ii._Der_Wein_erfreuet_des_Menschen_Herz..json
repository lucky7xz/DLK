{"dta.poem.21810": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Der Wein erfreuet des Menschen  \n Herz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Auff! bringet Wein.", "tokens": ["Auff", "!", "brin\u00b7get", "Wein", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein Schmerze wil ertr\u00e4nket sein.", "tokens": ["Mein", "Schmer\u00b7ze", "wil", "er\u00b7tr\u00e4n\u00b7ket", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der edle Safft der Reben", "tokens": ["Der", "ed\u00b7le", "Safft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mu\u00df mich des grimmen Leids entheben.", "tokens": ["mu\u00df", "mich", "des", "grim\u00b7men", "Leids", "ent\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jachus Safft", "tokens": ["Ja\u00b7chus", "Safft"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "hat manchen Kummer weggerafft:", "tokens": ["hat", "man\u00b7chen", "Kum\u00b7mer", "weg\u00b7ge\u00b7rafft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "er wird auch mein Verdriessen", "tokens": ["er", "wird", "auch", "mein", "Ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "durch seiner Trauben Blut vers\u00fcssen.", "tokens": ["durch", "sei\u00b7ner", "Trau\u00b7ben", "Blut", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sp\u00fchlt Gl\u00e4ser au\u00df/", "tokens": ["Sp\u00fchlt", "Gl\u00e4\u00b7ser", "au\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ich soll versuchen/ ob ein Schmau\u00df", "tokens": ["ich", "soll", "ver\u00b7su\u00b7chen", "/", "ob", "ein", "Schmau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "m\u00f6g\u2019 ins Vergessen senken/", "tokens": ["m\u00f6g'", "ins", "Ver\u00b7ges\u00b7sen", "sen\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "was mich so ungemenscht wil kr\u00e4nken.", "tokens": ["was", "mich", "so", "un\u00b7ge\u00b7menscht", "wil", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du harte T\u00fchr/", "tokens": ["Du", "har\u00b7te", "T\u00fchr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "verfluchet seystu f\u00fcr und f\u00fcr!", "tokens": ["ver\u00b7flu\u00b7chet", "seys\u00b7tu", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es m\u00fcssen deine Pfosten", "tokens": ["es", "m\u00fcs\u00b7sen", "dei\u00b7ne", "Pfos\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zu ihrem eignem Unheil rosten.", "tokens": ["zu", "ih\u00b7rem", "eig\u00b7nem", "Un\u00b7heil", "ros\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Diespiter", "tokens": ["Dies\u00b7pi\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "es m\u00fcssen deine Schwellen", "tokens": ["es", "m\u00fcs\u00b7sen", "dei\u00b7ne", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "durch seinen Blizz in st\u00fckken schellen.", "tokens": ["durch", "sei\u00b7nen", "Blizz", "in", "st\u00fck\u00b7ken", "schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So manche Nacht", "tokens": ["So", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "hab\u2019 ich umsonst bey dir gewacht/", "tokens": ["hab'", "ich", "um\u00b7sonst", "bey", "dir", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und andern groben Hachen", "tokens": ["und", "an\u00b7dern", "gro\u00b7ben", "Ha\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "l\u00e4\u00dftu nu knarrend auff-dich machen.", "tokens": ["l\u00e4\u00df\u00b7tu", "nu", "knar\u00b7rend", "auf\u00b7fdich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Rosilis", "tokens": ["Die", "Ro\u00b7si\u00b7lis"], "token_info": ["word", "word"], "pos": ["ART", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ist mir bey Tage zwar gewi\u00df:", "tokens": ["ist", "mir", "bey", "Ta\u00b7ge", "zwar", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADV", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch stehn zu allen Zeiten", "tokens": ["doch", "stehn", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die sch\u00e4len W\u00e4chter uns zur Seiten.", "tokens": ["die", "sch\u00e4\u00b7len", "W\u00e4ch\u00b7ter", "uns", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So bald die Nacht", "tokens": ["So", "bald", "die", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "dem Tag\u2019 ein Ende macht/", "tokens": ["dem", "Tag'", "ein", "En\u00b7de", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mu\u00df ich denn Abschied nehmen/", "tokens": ["mu\u00df", "ich", "denn", "Ab\u00b7schied", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "denn f\u00e4ngt sich an mein Weh und Gr\u00e4men.", "tokens": ["denn", "f\u00e4ngt", "sich", "an", "mein", "Weh", "und", "Gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Teufel hat", "tokens": ["Der", "Teu\u00b7fel", "hat"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "erdacht den schlimmen Raht/", "tokens": ["er\u00b7dacht", "den", "schlim\u00b7men", "Raht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df man mit blinden Schl\u00f6ssern", "tokens": ["da\u00df", "man", "mit", "blin\u00b7den", "Schl\u00f6s\u00b7sern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die T\u00fchr verwahrt/ mein Leid zu gr\u00f6ssern.", "tokens": ["die", "T\u00fchr", "ver\u00b7wahrt", "/", "mein", "Leid", "zu", "gr\u00f6s\u00b7sern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der b\u00f6se Hund", "tokens": ["Der", "b\u00f6\u00b7se", "Hund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ist wachsam iede Stund\u2019", "tokens": ["ist", "wach\u00b7sam", "ie\u00b7de", "Stund'"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "er lauschet an der Schwellen", "tokens": ["er", "lau\u00b7schet", "an", "der", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mit murren/ rimpfen und mit bellen.", "tokens": ["mit", "mur\u00b7ren", "/", "rimp\u00b7fen", "und", "mit", "bel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$(", "VVINF", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So geh\u2019 ich blind", "tokens": ["So", "geh'", "ich", "blind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "in blinder Nacht/ ich armes Kind!", "tokens": ["in", "blin\u00b7der", "Nacht", "/", "ich", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und werde niemals eingelassen.", "tokens": ["und", "wer\u00b7de", "nie\u00b7mals", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Drum her! ihr Freund\u2019", "tokens": ["Drum", "her", "!", "ihr", "Freund'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ich mu\u00df die Grillen heunt", "tokens": ["ich", "mu\u00df", "die", "Gril\u00b7len", "heunt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "im Wein zu tode schlagen.", "tokens": ["im", "Wein", "zu", "to\u00b7de", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Teufel m\u00f6chte so sich plagen!", "tokens": ["Der", "Teu\u00b7fel", "m\u00f6ch\u00b7te", "so", "sich", "pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So bringt nu Wein!", "tokens": ["So", "bringt", "nu", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "mein Schmerze wil ertr\u00e4nket sein.", "tokens": ["mein", "Schmer\u00b7ze", "wil", "er\u00b7tr\u00e4n\u00b7ket", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der edle Safft der Reben", "tokens": ["Der", "ed\u00b7le", "Safft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}