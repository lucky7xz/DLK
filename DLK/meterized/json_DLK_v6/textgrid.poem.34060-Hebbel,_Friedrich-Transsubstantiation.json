{"textgrid.poem.34060": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Transsubstantiation", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen den Z\u00e4hnen noch wehrt sich der Apfel gegen den Menschen,", "tokens": ["Zwi\u00b7schen", "den", "Z\u00e4h\u00b7nen", "noch", "wehrt", "sich", "der", "Ap\u00b7fel", "ge\u00b7gen", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber, wehrt sich der Mensch weniger gegen die Welt?", "tokens": ["A\u00b7ber", ",", "wehrt", "sich", "der", "Mensch", "we\u00b7ni\u00b7ger", "ge\u00b7gen", "die", "Welt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}