{"textgrid.poem.34924": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Plateniden", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Iliaden, Odysseen", "tokens": ["I\u00b7lia\u00b7den", ",", "O\u00b7dys\u00b7seen"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "K\u00fcndigst du uns prahlend an,", "tokens": ["K\u00fcn\u00b7digst", "du", "uns", "prah\u00b7lend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir sollen in dir sehen", "tokens": ["Und", "wir", "sol\u00b7len", "in", "dir", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutscher Zukunft gr\u00f6\u00dften Mann.", "tokens": ["Deut\u00b7scher", "Zu\u00b7kunft", "gr\u00f6\u00df\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine gro\u00dfe Tat in Worten,", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "Tat", "in", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du einst zu tun gedenkst! \u2013", "tokens": ["Die", "du", "einst", "zu", "tun", "ge\u00b7denkst", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oh, ich kenne solche Sorten", "tokens": ["Oh", ",", "ich", "ken\u00b7ne", "sol\u00b7che", "Sor\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist'ger Schuldenmacher l\u00e4ngst.", "tokens": ["Geist'\u00b7ger", "Schul\u00b7den\u00b7ma\u00b7cher", "l\u00e4ngst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier ist Rhodus, komm und zeige", "tokens": ["Hier", "ist", "Rho\u00b7dus", ",", "komm", "und", "zei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Kunst, hier wird getanzt!", "tokens": ["Dei\u00b7ne", "Kunst", ",", "hier", "wird", "ge\u00b7tanzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder trolle dich und schweige,", "tokens": ["O\u00b7der", "trol\u00b7le", "dich", "und", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du heut nicht tanzen kannst.", "tokens": ["Wenn", "du", "heut", "nicht", "tan\u00b7zen", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahre Prinzen aus Genieland", "tokens": ["Wah\u00b7re", "Prin\u00b7zen", "aus", "Ge\u00b7nie\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zahlen bar, was sie verzehrt,", "tokens": ["Zah\u00b7len", "bar", ",", "was", "sie", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schiller, Goethe, Lessing, Wieland", "tokens": ["Schil\u00b7ler", ",", "Goe\u00b7the", ",", "Les\u00b7sing", ",", "Wie\u00b7land"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben nie Kredit begehrt.", "tokens": ["Ha\u00b7ben", "nie", "Kre\u00b7dit", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wollten keine Ovationen", "tokens": ["Woll\u00b7ten", "kei\u00b7ne", "O\u00b7va\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von dem Publiko auf Pump,", "tokens": ["Von", "dem", "Pub\u00b7li\u00b7ko", "auf", "Pump", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Vorschu\u00dflorbeerkronen,", "tokens": ["Kei\u00b7ne", "Vor\u00b7schu\u00df\u00b7lor\u00b7beer\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchmten sich nicht keck und plump.", "tokens": ["R\u00fchm\u00b7ten", "sich", "nicht", "keck", "und", "plump", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tot ist l\u00e4ngst der alte Junker,", "tokens": ["Tot", "ist", "l\u00e4ngst", "der", "al\u00b7te", "Jun\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sein Same lebt noch heut \u2013", "tokens": ["Doch", "sein", "Sa\u00b7me", "lebt", "noch", "heut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oh, ich kenne das Geflunker", "tokens": ["Oh", ",", "ich", "ken\u00b7ne", "das", "Ge\u00b7flun\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcnftiger Unsterblichkeit.", "tokens": ["K\u00fcnf\u00b7ti\u00b7ger", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Das sind Platens echte Kinder,", "tokens": ["Das", "sind", "Pla\u00b7tens", "ech\u00b7te", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Echtes Platenidenblut \u2013", "tokens": ["Ech\u00b7tes", "Pla\u00b7te\u00b7ni\u00b7den\u00b7blut", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine teuern Hallerm\u00fcnder,", "tokens": ["Mei\u00b7ne", "teu\u00b7ern", "Hal\u00b7ler\u00b7m\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oh, ich kenn euch gar zu gut!", "tokens": ["Oh", ",", "ich", "kenn", "euch", "gar", "zu", "gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}