{"textgrid.poem.60213": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Held! der in tapfrer Hand verdeckte Keile tr\u00e4gt,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held! der in tapfrer Hand verdeckte Keile tr\u00e4gt,", "tokens": ["Held", "!", "der", "in", "tapf\u00b7rer", "Hand", "ver\u00b7deck\u00b7te", "Kei\u00b7le", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "APPR", "ADJA", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bi\u00df schlangenf\u00f6rmig hin und wieder", "tokens": ["Bi\u00df", "schlan\u00b7gen\u00b7f\u00f6r\u00b7mig", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Blitz die Luft durchf\u00e4hrt, und schnell zur Erde nieder", "tokens": ["Sein", "Blitz", "die", "Luft", "durch\u00b7f\u00e4hrt", ",", "und", "schnell", "zur", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$,", "KON", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des stolzen Frankreichs Fahnen schl\u00e4gt.", "tokens": ["Des", "stol\u00b7zen", "Fran\u00b7kreichs", "Fah\u00b7nen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Nur einen Augenblick verweil und merk auf mich;", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "ver\u00b7weil", "und", "merk", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, unter frischen Lorbeerzweigen", "tokens": ["Und", ",", "un\u00b7ter", "fri\u00b7schen", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu mir herunter neige sich", "tokens": ["Zu", "mir", "her\u00b7un\u00b7ter", "nei\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APZR", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein offnes Ohr, und h\u00f6re Klagen!", "tokens": ["Dein", "off\u00b7nes", "Ohr", ",", "und", "h\u00f6\u00b7re", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf einem Schutte sing ich Dir,", "tokens": ["Auf", "ei\u00b7nem", "Schut\u00b7te", "sing", "ich", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unaufger\u00e4umt, unabgetragen;", "tokens": ["Un\u00b7auf\u00b7ge\u00b7r\u00e4umt", ",", "un\u00b7ab\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Tempel Gottes war er einst.", "tokens": ["Ein", "Tem\u00b7pel", "Got\u00b7tes", "war", "er", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O Du, der Du nach jedem Siege", "tokens": ["O", "Du", ",", "der", "Du", "nach", "je\u00b7dem", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Thr\u00e4ne der Erbarmung weinst,", "tokens": ["Die", "Thr\u00e4\u00b7ne", "der", "Er\u00b7bar\u00b7mung", "weinst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und menschlich f\u00fchlst im wilden Kriege;", "tokens": ["Und", "menschlich", "f\u00fchlst", "im", "wil\u00b7den", "Krie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Empfinde dieser kleinen Stadt", "tokens": ["Emp\u00b7fin\u00b7de", "die\u00b7ser", "klei\u00b7nen", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu schwer gewordnen Gram, und h\u00f6re", "tokens": ["Zu", "schwer", "ge\u00b7word\u00b7nen", "Gram", ",", "und", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mich, wegen unsers Gottes Ehre,", "tokens": ["Mich", ",", "we\u00b7gen", "un\u00b7sers", "Got\u00b7tes", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der pr\u00e4chtig hier gewohnet hat,", "tokens": ["Der", "pr\u00e4ch\u00b7tig", "hier", "ge\u00b7woh\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Als von den Umfang hoher B\u00fchnen", "tokens": ["Als", "von", "den", "Um\u00b7fang", "ho\u00b7her", "B\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sein Lobgesang erscholl, und festlich am Altar", "tokens": ["Sein", "Lob\u00b7ge\u00b7sang", "er\u00b7scholl", ",", "und", "fest\u00b7lich", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der ganz mit Gold bezogen war,", "tokens": ["Der", "ganz", "mit", "Gold", "be\u00b7zo\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Priester standen, dem zu dienen", "tokens": ["Die", "Pries\u00b7ter", "stan\u00b7den", ",", "dem", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Deine grosse Seele liebt,", "tokens": ["Der", "Dei\u00b7ne", "gros\u00b7se", "See\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und in die Flucht vor Dir, des Feindes Haufen giebt!", "tokens": ["Und", "in", "die", "Flucht", "vor", "Dir", ",", "des", "Fein\u00b7des", "Hau\u00b7fen", "giebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In seinem Namen darf ich k\u00fchn Dein Herz beschw\u00f6ren", "tokens": ["In", "sei\u00b7nem", "Na\u00b7men", "darf", "ich", "k\u00fchn", "Dein", "Herz", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis es auf diesen Aschenh\u00fcgel blickt,", "tokens": ["Bis", "es", "auf", "die\u00b7sen", "A\u00b7schen\u00b7h\u00fc\u00b7gel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcbrig blieb, als Glut das Heiligthum verzehren", "tokens": ["Der", "\u00fcb\u00b7rig", "blieb", ",", "als", "Glut", "das", "Hei\u00b7lig\u00b7thum", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KOUS", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In einer Stunde kam", "tokens": ["In", "ei\u00b7ner", "Stun\u00b7de", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der arme B\u00fcrgerstand, den itzt der Krieg erdr\u00fcckt;", "tokens": ["Der", "ar\u00b7me", "B\u00fcr\u00b7ger\u00b7stand", ",", "den", "itzt", "der", "Krieg", "er\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nichts blieb ihm \u00fcbrig, als nur schlecht bedecktes Leben,", "tokens": ["Nichts", "blieb", "ihm", "\u00fcb\u00b7rig", ",", "als", "nur", "schlecht", "be\u00b7deck\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht m\u00f6glich ist ihm aus dem Staub", "tokens": ["Nicht", "m\u00f6g\u00b7lich", "ist", "ihm", "aus", "dem", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gotteshaus empor zu heben;", "tokens": ["Das", "Got\u00b7tes\u00b7haus", "em\u00b7por", "zu", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sein Brod, von schwerem Flei\u00df ihm vor den Mund gegeben,", "tokens": ["Sein", "Brod", ",", "von", "schwe\u00b7rem", "Flei\u00df", "ihm", "vor", "den", "Mund", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ward oft des schnell zur\u00fcck gekommnen Feindes Raub,", "tokens": ["Ward", "oft", "des", "schnell", "zu\u00b7r\u00fcck", "ge\u00b7komm\u00b7nen", "Fein\u00b7des", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "PTKVZ", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der auch den allerletzten Bissen", "tokens": ["Der", "auch", "den", "al\u00b7ler\u00b7letz\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Drohung, und mit Fluch ihm aus der Hand gerissen,", "tokens": ["Mit", "Dro\u00b7hung", ",", "und", "mit", "Fluch", "ihm", "aus", "der", "Hand", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und taub bey seinen Klagen blieb,", "tokens": ["Und", "taub", "bey", "sei\u00b7nen", "Kla\u00b7gen", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ganz ohne menschliches Gef\u00fchle,", "tokens": ["Ganz", "oh\u00b7ne", "menschli\u00b7ches", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Nur Spott mit seinen Thr\u00e4nen trieb,", "tokens": ["Nur", "Spott", "mit", "sei\u00b7nen", "Thr\u00e4\u00b7nen", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und drohend nannte den Bellisle,", "tokens": ["Und", "dro\u00b7hend", "nann\u00b7te", "den", "Bel\u00b7lis\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den vormals mit getreuer Hand", "tokens": ["Den", "vor\u00b7mals", "mit", "ge\u00b7treu\u00b7er", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Stadt gefangen nahm, und zu Georgens F\u00fcssen", "tokens": ["Die", "Stadt", "ge\u00b7fan\u00b7gen", "nahm", ",", "und", "zu", "Ge\u00b7or\u00b7gens", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KON", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als Krieges-Opfer ihn gesandt:", "tokens": ["Als", "Krie\u00b7ge\u00b7sOp\u00b7fer", "ihn", "ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dis wollt er nun zu r\u00e4chen wissen,", "tokens": ["Dis", "wollt", "er", "nun", "zu", "r\u00e4\u00b7chen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Drum hat er seinen Blick in jene Zeit gewandt,", "tokens": ["Drum", "hat", "er", "sei\u00b7nen", "Blick", "in", "je\u00b7ne", "Zeit", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und grimmiger geh\u00e4uft der armen B\u00fcrger Plagen,", "tokens": ["Und", "grim\u00b7mi\u00b7ger", "ge\u00b7h\u00e4uft", "der", "ar\u00b7men", "B\u00fcr\u00b7ger", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die, ganz bet\u00e4ubt von Gram, des Feindes Trotz und Spott,", "tokens": ["Die", ",", "ganz", "be\u00b7t\u00e4ubt", "von", "Gram", ",", "des", "Fein\u00b7des", "Trotz", "und", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVPP", "APPR", "NE", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem h\u00f6chsten Richter, ihrem Gott,", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Rich\u00b7ter", ",", "ih\u00b7rem", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In einem Bretterhause klagen.", "tokens": ["In", "ei\u00b7nem", "Bret\u00b7ter\u00b7hau\u00b7se", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O ", "tok