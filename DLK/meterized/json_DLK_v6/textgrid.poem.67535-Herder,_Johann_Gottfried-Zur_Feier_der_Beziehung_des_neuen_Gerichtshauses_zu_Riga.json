{"textgrid.poem.67535": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zur Feier der Beziehung des neuen Gerichtshauses zu Riga", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland! dem Ohr des Patrioten", "tokens": ["O", "Va\u00b7ter\u00b7land", "!", "dem", "Ohr", "des", "Pat\u00b7ri\u00b7o\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Silberton, ein Ton des Ruhms dem Geist,", "tokens": ["Ein", "Sil\u00b7ber\u00b7ton", ",", "ein", "Ton", "des", "Ruhms", "dem", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der, hundert andrer werth, sich von lebend'gen Todten", "tokens": ["Der", ",", "hun\u00b7dert", "an\u00b7drer", "werth", ",", "sich", "von", "le\u00b7ben\u00b7d'\u00b7gen", "Tod\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "CARD", "ADJA", "ADJD", "$,", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+----+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Hinauf ins Heil'ge der Verdienste rei\u00dft;", "tokens": ["Hin\u00b7auf", "ins", "Heil'\u00b7ge", "der", "Ver\u00b7diens\u00b7te", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo Die wie Erdeng\u00f6tter gl\u00e4nzen,", "tokens": ["Wo", "Die", "wie", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Dir, o Vaterland, sich weihn", "tokens": ["Die", "Dir", ",", "o", "Va\u00b7ter\u00b7land", ",", "sich", "weihn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "FM", "NN", "$,", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sich mit Lorbeerkr\u00e4nzen", "tokens": ["Und", "sich", "mit", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus Deinen H\u00e4nden freun!", "tokens": ["Aus", "Dei\u00b7nen", "H\u00e4n\u00b7den", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fcr Dich, o Theure, geht der Held zum Streite", "tokens": ["F\u00fcr", "Dich", ",", "o", "Theu\u00b7re", ",", "geht", "der", "Held", "zum", "Strei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "FM", "$,", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie zum Triumph und lockt sein goldnes Haar;", "tokens": ["Wie", "zum", "Tri\u00b7umph", "und", "lockt", "sein", "gold\u00b7nes", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der J\u00fcngling gl\u00fcht ins Feld und giebt aus seiner Seite", "tokens": ["Der", "J\u00fcng\u00b7ling", "gl\u00fcht", "ins", "Feld", "und", "giebt", "aus", "sei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein bestes Herzensblut Dir jauchzend dar;", "tokens": ["Sein", "bes\u00b7tes", "Her\u00b7zens\u00b7blut", "Dir", "jauch\u00b7zend", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sterbend labst Du seine Ohren;", "tokens": ["Und", "ster\u00b7bend", "labst", "Du", "sei\u00b7ne", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schon brechend, lallt der Zunge Band:", "tokens": ["Schon", "bre\u00b7chend", ",", "lallt", "der", "Zun\u00b7ge", "Band", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbdir, das Du mich geboren,", "tokens": ["\u00bb", "dir", ",", "das", "Du", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir sterb' ich, Vaterland!\u00ab", "tokens": ["Dir", "sterb'", "ich", ",", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, Vaterland! Du Mutter, der der Weise", "tokens": ["Ja", ",", "Va\u00b7ter\u00b7land", "!", "Du", "Mut\u00b7ter", ",", "der", "der", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erstgeburt des Geists zum Opfer bringt;", "tokens": ["Die", "Erst\u00b7ge\u00b7burt", "des", "Geists", "zum", "Op\u00b7fer", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Du, sein Sonnenflug, wenn er zum hohen Preise", "tokens": ["O", "Du", ",", "sein", "Son\u00b7nen\u00b7flug", ",", "wenn", "er", "zum", "ho\u00b7hen", "Prei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tugend Dich mit edlen Thaten singt;", "tokens": ["Der", "Tu\u00b7gend", "Dich", "mit", "ed\u00b7len", "Tha\u00b7ten", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dich, die mit goldnen Liebesseilen", "tokens": ["Dich", ",", "die", "mit", "gold\u00b7nen", "Lie\u00b7bes\u00b7sei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der S\u00f6hne Herzen an sich zieht", "tokens": ["Der", "S\u00f6h\u00b7ne", "Her\u00b7zen", "an", "sich", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, wie mit Sonnenpfeilen,", "tokens": ["Und", ",", "wie", "mit", "Son\u00b7nen\u00b7pfei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Ruhme sie durchgl\u00fcht!", "tokens": ["Mit", "Ruh\u00b7me", "sie", "durch\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir, G\u00f6ttin, Dir, der Freiheit s\u00fc\u00dfer Namen,", "tokens": ["Dir", ",", "G\u00f6t\u00b7tin", ",", "Dir", ",", "der", "Frei\u00b7heit", "s\u00fc\u00b7\u00dfer", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr den der Held den Tod als Freund umfa\u00dft,", "tokens": ["F\u00fcr", "den", "der", "Held", "den", "Tod", "als", "Freund", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir s\u00e4't der Menschenfreund, der B\u00fcrger stillen Samen,", "tokens": ["Dir", "s\u00e4't", "der", "Men\u00b7schen\u00b7freund", ",", "der", "B\u00fcr\u00b7ger", "stil\u00b7len", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kann er nichts \u2013 so tr\u00e4gt er Deine Last.", "tokens": ["Und", "kann", "er", "nichts", "\u2013", "so", "tr\u00e4gt", "er", "Dei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir weihn die G\u00f6tter auf dem Throne,", "tokens": ["Dir", "weihn", "die", "G\u00f6t\u00b7ter", "auf", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Handel und die K\u00fcnste weihn", "tokens": ["Der", "Han\u00b7del", "und", "die", "K\u00fcns\u00b7te", "weihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Deiner Siegeskrone", "tokens": ["Zu", "Dei\u00b7ner", "Sie\u00b7ges\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den besten Edelstein.", "tokens": ["Den", "bes\u00b7ten", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein sind die Schulen! Deine zarten S\u00f6hne", "tokens": ["Dein", "sind", "die", "Schu\u00b7len", "!", "Dei\u00b7ne", "zar\u00b7ten", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erziehn sie Dir; damit Dein Hoffnungsreis", "tokens": ["Er\u00b7ziehn", "sie", "Dir", ";", "da\u00b7mit", "Dein", "Hoff\u00b7nungs\u00b7reis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Bl\u00fcthe Dich, mit Frucht, mit Schatten kr\u00f6ne,", "tokens": ["Mit", "Bl\u00fc\u00b7the", "Dich", ",", "mit", "Frucht", ",", "mit", "Schat\u00b7ten", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So trinkt sein junger Keim erst theuren Schwei\u00df.", "tokens": ["So", "trinkt", "sein", "jun\u00b7ger", "Keim", "erst", "theu\u00b7ren", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir bl\u00fchn die Tempel; denn sie weihen", "tokens": ["Dir", "bl\u00fchn", "die", "Tem\u00b7pel", ";", "denn", "sie", "wei\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den B\u00fcrger Dir zum Christen ein;", "tokens": ["Den", "B\u00fcr\u00b7ger", "Dir", "zum", "Chris\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Weihrauch, den sie streuen,", "tokens": ["Der", "Weih\u00b7rauch", ",", "den", "sie", "streu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Vaterland, ist Dein!", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "ist", "Dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein ist dies Haus in ", "tokens": ["Dein", "ist", "dies", "Haus", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PDS", "NN", "APPR"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Das sie Dir selbst voll Majest\u00e4t geweiht!", "tokens": ["Das", "sie", "Dir", "selbst", "voll", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "ADV", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lang' sich Ru\u00dflands Adlerh\u00e4upter gatten,", "tokens": ["So", "lang'", "sich", "Ru\u00df\u00b7lands", "Ad\u00b7ler\u00b7h\u00e4up\u00b7ter", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So lange bl\u00fcht hier Recht und Billigkeit;", "tokens": ["So", "lan\u00b7ge", "bl\u00fcht", "hier", "Recht", "und", "Bil\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Unschuld flieht zu diesen Schranken,", "tokens": ["Die", "Un\u00b7schuld", "flieht", "zu", "die\u00b7sen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fccke wird sie l\u00e4chelnd gehn;", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "wird", "sie", "l\u00e4\u00b7chelnd", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Thr\u00e4nen werden danken,", "tokens": ["Und", "Thr\u00e4\u00b7nen", "wer\u00b7den", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So lang' hier Pfeiler stehn.", "tokens": ["So", "lang'", "hier", "Pfei\u00b7ler", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich seh', Astr\u00e4a steigt vom Himmel nieder,", "tokens": ["Ich", "seh'", ",", "A\u00b7str\u00e4a", "steigt", "vom", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit Palmen, Oel und Lorbeer neu bekr\u00e4nzt,", "tokens": ["Mit", "Pal\u00b7men", ",", "O\u00b7el", "und", "Lor\u00b7beer", "neu", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor ihr der Wahrheit Schild, nach ihr der Engel Lieder.", "tokens": ["Vor", "ihr", "der", "Wahr\u00b7heit", "Schild", ",", "nach", "ihr", "der", "En\u00b7gel", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "NN", "$,", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie flammt ihr Schild! Seht, wie ihr Antlitz gl\u00e4nzt!", "tokens": ["Wie", "flammt", "ihr", "Schild", "!", "Seht", ",", "wie", "ihr", "Ant\u00b7litz", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sinken ihrer Wage Schalen;", "tokens": ["Es", "sin\u00b7ken", "ih\u00b7rer", "Wa\u00b7ge", "Scha\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie streiten; keine \u00fcberwiegt:", "tokens": ["Sie", "strei\u00b7ten", ";", "kei\u00b7ne", "\u00fc\u00b7berw\u00b7iegt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier wirft die Weisheit Strahlen,", "tokens": ["Hier", "wirft", "die", "Weis\u00b7heit", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie dort die Treue siegt.", "tokens": ["Wie", "dort", "die", "Treu\u00b7e", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich seh', ich seh'! Sie schreibt mit ew'gen Z\u00fcgen", "tokens": ["Ich", "seh'", ",", "ich", "seh'", "!", "Sie", "schreibt", "mit", "ew'\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier das Gesetz und dort des Handels Gl\u00fcck", "tokens": ["Hier", "das", "Ge\u00b7setz", "und", "dort", "des", "Han\u00b7dels", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An beide Pfeiler, schreibt hier Ruhe, dort Vergn\u00fcgen,", "tokens": ["An", "bei\u00b7de", "Pfei\u00b7ler", ",", "schreibt", "hier", "Ru\u00b7he", ",", "dort", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier Ru\u00dflands Huld und dort der Sonne Blick.", "tokens": ["Hier", "Ru\u00df\u00b7lands", "Huld", "und", "dort", "der", "Son\u00b7ne", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O kommt, die unsre Freude loben,", "tokens": ["O", "kommt", ",", "die", "uns\u00b7re", "Freu\u00b7de", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schreibt mit patriot'scher Hand,", "tokens": ["Und", "schreibt", "mit", "pa\u00b7tri\u00b7ot'\u00b7scher", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Krone, mitten oben:", "tokens": ["Als", "Kro\u00b7ne", ",", "mit\u00b7ten", "o\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}