{"dta.poem.9725": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie spielet die hertzen dame aus.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man la\u00df die dame gehn; sie acht der knechte nicht.", "tokens": ["Man", "la\u00df", "die", "da\u00b7me", "gehn", ";", "sie", "acht", "der", "knech\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "VVINF", "$.", "PPER", "CARD", "ART", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es mu\u00df ein k\u00f6nig seyn/ der solch ein hertze sticht.", "tokens": ["Es", "mu\u00df", "ein", "k\u00f6\u00b7nig", "seyn", "/", "der", "solch", "ein", "hert\u00b7ze", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "VAINF", "$(", "ART", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bin ich nun dein knecht/ welch th\u00f6richtes beginnen/", "tokens": ["Und", "bin", "ich", "nun", "dein", "knecht", "/", "welch", "th\u00f6\u00b7rich\u00b7tes", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "PWAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ich mich untersteh dein hertze zu gewinnen?", "tokens": ["Da\u00df", "ich", "mich", "un\u00b7ter\u00b7steh", "dein", "hert\u00b7ze", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}