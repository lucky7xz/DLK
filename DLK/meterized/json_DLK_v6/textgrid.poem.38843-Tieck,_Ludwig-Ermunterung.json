{"textgrid.poem.38843": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keinen hat es noch gereut", "tokens": ["Kei\u00b7nen", "hat", "es", "noch", "ge\u00b7reut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das Ro\u00df bestiegen,", "tokens": ["Der", "das", "Ro\u00df", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Um in frischer Jugendzeit", "tokens": ["Um", "in", "fri\u00b7scher", "Ju\u00b7gend\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Welt zu fliegen.", "tokens": ["Durch", "die", "Welt", "zu", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Berge und Auen,", "tokens": ["Ber\u00b7ge", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Einsamer Wald,", "tokens": ["Ein\u00b7sa\u00b7mer", "Wald", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "M\u00e4dchen und Frauen", "tokens": ["M\u00e4d\u00b7chen", "und", "Frau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Pr\u00e4chtig im Kleide,", "tokens": ["Pr\u00e4ch\u00b7tig", "im", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Golden Geschmeide,", "tokens": ["Gol\u00b7den", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Alles erfreut ihn mit sch\u00f6ner Gestalt.", "tokens": ["Al\u00b7les", "er\u00b7freut", "ihn", "mit", "sch\u00f6\u00b7ner", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Wunderlich fliehen", "tokens": ["Wun\u00b7der\u00b7lich", "flie\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gestalten dahin,", "tokens": ["Ge\u00b7stal\u00b7ten", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schw\u00e4rmerisch gl\u00fchen", "tokens": ["Schw\u00e4r\u00b7me\u00b7risch", "gl\u00fc\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "W\u00fcnsche im jugendlich trunkenen Sinn.", "tokens": ["W\u00fcn\u00b7sche", "im", "ju\u00b7gend\u00b7lich", "trun\u00b7ke\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ruhm streut ihm Rosen", "tokens": ["Ruhm", "streut", "ihm", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schnell in die Bahn,", "tokens": ["Schnell", "in", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Lieben und Kosen,", "tokens": ["Lie\u00b7ben", "und", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lorbeer und Rosen", "tokens": ["Lor\u00b7beer", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "F\u00fchren ihn h\u00f6her und h\u00f6her hinan.", "tokens": ["F\u00fch\u00b7ren", "ihn", "h\u00f6\u00b7her", "und", "h\u00f6\u00b7her", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Rund um ihn Freuden,", "tokens": ["Rund", "um", "ihn", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Feinde beneiden,", "tokens": ["Fein\u00b7de", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Erliegend, den Held, \u2013", "tokens": ["Er\u00b7lie\u00b7gend", ",", "den", "Held", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "$,", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dann w\u00e4hlt er bescheiden", "tokens": ["Dann", "w\u00e4hlt", "er", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das Fr\u00e4ulein das ihm nur vor allen gef\u00e4llt.", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "das", "ihm", "nur", "vor", "al\u00b7len", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Und Berge und Felder", "tokens": ["Und", "Ber\u00b7ge", "und", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und einsame W\u00e4lder", "tokens": ["Und", "ein\u00b7sa\u00b7me", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mi\u00dft er zur\u00fcck.", "tokens": ["Mi\u00dft", "er", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Eltern in Thr\u00e4nen,", "tokens": ["Die", "El\u00b7tern", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ach alle ihr Sehnen, \u2013", "tokens": ["Ach", "al\u00b7le", "ihr", "Seh\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "PIAT", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie alle vereinigt das lieblichste Gl\u00fcck.", "tokens": ["Sie", "al\u00b7le", "ver\u00b7ei\u00b7nigt", "das", "lieb\u00b7lichs\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Sind Jahre verschwunden,", "tokens": ["Sind", "Jah\u00b7re", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erz\u00e4hlt er dem Sohn", "tokens": ["Er\u00b7z\u00e4hlt", "er", "dem", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In traulichen Stunden,", "tokens": ["In", "trau\u00b7li\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und zeigt seine Wunden,", "tokens": ["Und", "zeigt", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Tapferkeit Lohn.", "tokens": ["Der", "Tap\u00b7fer\u00b7keit", "Lohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "So bleibt das Alter selbst noch jung,", "tokens": ["So", "bleibt", "das", "Al\u00b7ter", "selbst", "noch", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Lichtstrahl in der Dammerung.", "tokens": ["Ein", "Licht\u00b7strahl", "in", "der", "Dam\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}