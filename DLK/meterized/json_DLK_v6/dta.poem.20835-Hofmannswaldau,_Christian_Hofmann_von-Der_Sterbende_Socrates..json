{"dta.poem.20835": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Eitelkeit und Lust nicht hat Gewalt gethan/", "tokens": ["Dem", "Ei\u00b7tel\u00b7keit", "und", "Lust", "nicht", "hat", "Ge\u00b7walt", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Silber/ Geld und Stein hat keinen Zug gegeben/", "tokens": ["Dem", "Sil\u00b7ber", "/", "Geld", "und", "Stein", "hat", "kei\u00b7nen", "Zug", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den die verliebte Brunst nicht \u00fcbermeistern kan/", "tokens": ["Den", "die", "ver\u00b7lieb\u00b7te", "Brunst", "nicht", "\u00fc\u00b7ber\u00b7meis\u00b7tern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nicht geschworen hat zu Bachus nasser Fahn/", "tokens": ["Der", "nicht", "ge\u00b7schwo\u00b7ren", "hat", "zu", "Ba\u00b7chus", "nas\u00b7ser", "Fahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "VAFIN", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den sch\u00e4tzet man vor todt auch mitten in dem Leben.", "tokens": ["Den", "sch\u00e4t\u00b7zet", "man", "vor", "todt", "auch", "mit\u00b7ten", "in", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}