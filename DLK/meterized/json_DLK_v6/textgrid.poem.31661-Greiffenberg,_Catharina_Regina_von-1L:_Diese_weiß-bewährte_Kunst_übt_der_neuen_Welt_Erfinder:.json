{"textgrid.poem.31661": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diese wei\u00df-bew\u00e4hrte Kunst \u00fcbt der neuen Welt Erfinder:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese wei\u00df-bew\u00e4hrte Kunst \u00fcbt der neuen Welt Erfinder:", "tokens": ["Die\u00b7se", "wei\u00df\u00b7be\u00b7w\u00e4hr\u00b7te", "Kunst", "\u00fcbt", "der", "neu\u00b7en", "Welt", "Er\u00b7fin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Gl\u00fcck Erwartung / machet ihm alles leidend Vngl\u00fcck linder:", "tokens": ["Gl\u00fcck", "Er\u00b7war\u00b7tung", "/", "ma\u00b7chet", "ihm", "al\u00b7les", "lei\u00b7dend", "Vn\u00b7gl\u00fcck", "lin\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "VVFIN", "PPER", "PIS", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "er erduldet Schmach und Dr\u00e4uen: bi\u00df er neues Land erblickt /", "tokens": ["er", "er\u00b7dul\u00b7det", "Schmach", "und", "Dr\u00e4u\u00b7en", ":", "bi\u00df", "er", "neu\u00b7es", "Land", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "KOUS", "PPER", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "wird dann / von verzeihungs bitten / Sieg' und Ehr zugleich begl\u00fcckt.", "tokens": ["wird", "dann", "/", "von", "ver\u00b7zei\u00b7hungs", "bit\u00b7ten", "/", "Sieg'", "und", "Ehr", "zu\u00b7gleich", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "APPR", "ADV", "VVINF", "$(", "VVIMP", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "wer auf Erden wird verspott / mag an Glaubens-Mastbaum steigen:", "tokens": ["wer", "auf", "Er\u00b7den", "wird", "ver\u00b7spott", "/", "mag", "an", "Glau\u00b7bens\u00b7Mast\u00b7baum", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$(", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "der wird das verhoffte Land / nahend / reich und gro\u00df / ihm zeigen.", "tokens": ["der", "wird", "das", "ver\u00b7hoff\u00b7te", "Land", "/", "na\u00b7hend", "/", "reich", "und", "gro\u00df", "/", "ihm", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "ADJA", "NN", "$(", "VVPP", "$(", "ADJD", "KON", "ADJD", "$(", "PPER", "VVINF", "$."], "meter": "-+--+-++-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}