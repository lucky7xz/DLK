{"textgrid.poem.35623": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich sing ich wilde Zerkl\u00fcftung,", "tokens": ["Dich", "sing", "ich", "wil\u00b7de", "Zer\u00b7kl\u00fcf\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Nachtsturm", "tokens": ["Im", "Nachts\u00b7turm"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Aufget\u00fcrmtes Gebirge;", "tokens": ["Auf\u00b7ge\u00b7t\u00fcrm\u00b7tes", "Ge\u00b7bir\u00b7ge", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihr grauen T\u00fcrme", "tokens": ["Ihr", "grau\u00b7en", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00dcberflie\u00dfend von h\u00f6llischen Fratzen,", "tokens": ["\u00dc\u00b7berf\u00b7lie\u00b7\u00dfend", "von", "h\u00f6l\u00b7li\u00b7schen", "Frat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Feurigem Getier,", "tokens": ["Feu\u00b7ri\u00b7gem", "Ge\u00b7tier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Rauhen Farnen, Fichten,", "tokens": ["Rau\u00b7hen", "Far\u00b7nen", ",", "Fich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Kristallnen Blumen.", "tokens": ["Kris\u00b7tall\u00b7nen", "Blu\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Unendliche Qual,", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Qual", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Da\u00df du Gott erjagtest", "tokens": ["Da\u00df", "du", "Gott", "er\u00b7jag\u00b7test"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Sanfter Geist,", "tokens": ["Sanf\u00b7ter", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Aufseufzend im Wassersturz,", "tokens": ["Auf\u00b7seuf\u00b7zend", "im", "Was\u00b7ser\u00b7sturz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "In wogenden F\u00f6hren.", "tokens": ["In", "wo\u00b7gen\u00b7den", "F\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Golden lodern die Feuer", "tokens": ["Gol\u00b7den", "lo\u00b7dern", "die", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der V\u00f6lker rings.", "tokens": ["Der", "V\u00f6l\u00b7ker", "rings", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00dcber schw\u00e4rzliche Klippen", "tokens": ["\u00dc\u00b7ber", "schw\u00e4rz\u00b7li\u00b7che", "Klip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "St\u00fcrzt todestrunken", "tokens": ["St\u00fcrzt", "to\u00b7des\u00b7trun\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die ergl\u00fchende Windsbraut,", "tokens": ["Die", "er\u00b7gl\u00fc\u00b7hen\u00b7de", "Winds\u00b7braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die blaue Woge", "tokens": ["Die", "blau\u00b7e", "Wo\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Des Gletschers", "tokens": ["Des", "Glet\u00b7schers"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Und es dr\u00f6hnt", "tokens": ["Und", "es", "dr\u00f6hnt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Gewaltig die Glocke im Tal:", "tokens": ["Ge\u00b7wal\u00b7tig", "die", "Glo\u00b7cke", "im", "Tal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Flammen, Fl\u00fcche", "tokens": ["Flam\u00b7men", ",", "Fl\u00fc\u00b7che"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und die dunklen", "tokens": ["Und", "die", "dunk\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Spiele der Wollust,", "tokens": ["Spie\u00b7le", "der", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "St\u00fcrmt den Himmel", "tokens": ["St\u00fcrmt", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Ein versteinertes Haupt.", "tokens": ["Ein", "ver\u00b7stei\u00b7ner\u00b7tes", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}