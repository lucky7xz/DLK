{"textgrid.poem.53940": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der englischen Chorknaben", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehre sei Gott in der H\u00f6-h\u00f6-he!", "tokens": ["Eh\u00b7re", "sei", "Gott", "in", "der", "H\u00f6\u00b7h\u00f6\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--++-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wer hat die Wanzen und Fl\u00f6-h\u00f6-he?", "tokens": ["Wer", "hat", "die", "Wan\u00b7zen", "und", "Fl\u00f6\u00b7h\u00f6\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Armen,", "tokens": ["Die", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "die Armen \u2013", "tokens": ["die", "Ar\u00b7men", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Oh, habet Erbarmen!", "tokens": ["Oh", ",", "ha\u00b7bet", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Reichen", "tokens": ["Die", "Rei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "die Reichen", "tokens": ["die", "Rei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "die brauchen das nicht;", "tokens": ["die", "brau\u00b7chen", "das", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "sie liegen auf weichen,", "tokens": ["sie", "lie\u00b7gen", "auf", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "weichen Kissen im Licht", "tokens": ["wei\u00b7chen", "Kis\u00b7sen", "im", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "oder bei ihren Damen \u2013", "tokens": ["o\u00b7der", "bei", "ih\u00b7ren", "Da\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Ehre sei Gott in der ersten Etage!", "tokens": ["Eh\u00b7re", "sei", "Gott", "in", "der", "ers\u00b7ten", "E\u00b7ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Courage! Courage!", "tokens": ["Cou\u00b7ra\u00b7ge", "!", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Macht eure Fabrik auch mal Plei-hei-te,", "tokens": ["Macht", "eu\u00b7re", "Fab\u00b7rik", "auch", "mal", "Plei\u00b7hei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADV", "NN", "$,"], "meter": "++-+-+-++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "die Kirche, die steht euch zur Sei-hei-te", "tokens": ["die", "Kir\u00b7che", ",", "die", "steht", "euch", "zur", "Sei\u00b7hei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und gibt euch stets das Geleite:", "tokens": ["und", "gibt", "euch", "stets", "das", "Ge\u00b7lei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "sie beugt dem Proleten den R\u00fccken krumm", "tokens": ["sie", "beugt", "dem", "Pro\u00b7le\u00b7ten", "den", "R\u00fc\u00b7cken", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "und h\u00e4lt ihn sein ganzes Leben lang dumm,", "tokens": ["und", "h\u00e4lt", "ihn", "sein", "gan\u00b7zes", "Le\u00b7ben", "lang", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und segnet den Staat und seine Soldaten,", "tokens": ["und", "seg\u00b7net", "den", "Staat", "und", "sei\u00b7ne", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "die Unternehmer und Potentaten", "tokens": ["die", "Un\u00b7ter\u00b7neh\u00b7mer", "und", "Po\u00b7ten\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und segnet \u00fcberhaupt jede Schweinerei", "tokens": ["und", "seg\u00b7net", "\u00fc\u00b7ber\u00b7haupt", "je\u00b7de", "Schwei\u00b7ne\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "und ist allemal dabei.", "tokens": ["und", "ist", "al\u00b7le\u00b7mal", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Jeder lebe in seinem Rahmen:", "tokens": ["Je\u00b7der", "le\u00b7be", "in", "sei\u00b7nem", "Rah\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "unten die Arbeitsamen", "tokens": ["un\u00b7ten", "die", "Ar\u00b7beit\u00b7sa\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "und oben die mit den B\u00f6rseneinnahmen \u2013", "tokens": ["und", "o\u00b7ben", "die", "mit", "den", "B\u00f6r\u00b7sen\u00b7ein\u00b7nah\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Ehre den Gott der herrschenden Klassen!", "tokens": ["Eh\u00b7re", "den", "Gott", "der", "herr\u00b7schen\u00b7den", "Klas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir z\u00e4hmen die Massen!", "tokens": ["Wir", "z\u00e4h\u00b7men", "die", "Mas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir lassen sie beten,", "tokens": ["Wir", "las\u00b7sen", "sie", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "wenn sie getreten;", "tokens": ["wenn", "sie", "ge\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "wir lassen sie singen,", "tokens": ["wir", "las\u00b7sen", "sie", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "wenn sie vor Hunger zerspringen;", "tokens": ["wenn", "sie", "vor", "Hun\u00b7ger", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "wir lassen sie knien:", "tokens": ["wir", "las\u00b7sen", "sie", "kni\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wir wollen den Proletarier erziehn", "tokens": ["Wir", "wol\u00b7len", "den", "Pro\u00b7le\u00b7ta\u00b7rier", "er\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "zu einem geduldigen", "tokens": ["zu", "ei\u00b7nem", "ge\u00b7dul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.10": {"text": "unschuldigen", "tokens": ["un\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+---", "measure": "dactylic.init"}, "line.11": {"text": "Arbeitstier \u2013 I-A! I-A!", "tokens": ["Ar\u00b7beits\u00b7tier", "\u2013", "I\u00b7A", "!", "I\u00b7A", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NE", "$.", "NE", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.12": {"text": "Hallelujah!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Oh, tut doch nimmer im Beten erlahmen!", "tokens": ["Oh", ",", "tut", "doch", "nim\u00b7mer", "im", "Be\u00b7ten", "er\u00b7lah\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "und h\u00f6ret auf der Kirche Reklamen \u2013", "tokens": ["und", "h\u00f6\u00b7ret", "auf", "der", "Kir\u00b7che", "Re\u00b7kla\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "jedes Ding, das ihr schiebt, schiebt ihr in IHREM Namen", "tokens": ["je\u00b7des", "Ding", ",", "das", "ihr", "schiebt", ",", "schiebt", "ihr", "in", "Ih\u00b7REM", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "CARD", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}