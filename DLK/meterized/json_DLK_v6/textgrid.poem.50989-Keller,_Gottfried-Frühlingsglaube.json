{"textgrid.poem.50989": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsglaube", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wandert eine sch\u00f6ne Sage", "tokens": ["Es", "wan\u00b7dert", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Veilchenduft auf Erden um,", "tokens": ["Wie", "Veil\u00b7chen\u00b7duft", "auf", "Er\u00b7den", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sehnend eine Liebesklage", "tokens": ["Wie", "seh\u00b7nend", "ei\u00b7ne", "Lie\u00b7bes\u00b7kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht sie bei Tag und Nacht herum.", "tokens": ["Geht", "sie", "bei", "Tag", "und", "Nacht", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist das Lied vom V\u00f6lkerfrieden", "tokens": ["Das", "ist", "das", "Lied", "vom", "V\u00f6l\u00b7ker\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von der Menschheit letztem Gl\u00fcck,", "tokens": ["Und", "von", "der", "Menschheit", "letz\u00b7tem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Von goldner Zeit, die einst hienieden,", "tokens": ["Von", "gold\u00b7ner", "Zeit", ",", "die", "einst", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Traum als Wahrheit, kehrt zur\u00fcck;", "tokens": ["Der", "Traum", "als", "Wahr\u00b7heit", ",", "kehrt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo einig alle V\u00f6lker beten", "tokens": ["Wo", "ei\u00b7nig", "al\u00b7le", "V\u00f6l\u00b7ker", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von jenem Tag, wo den Propheten", "tokens": ["Von", "je\u00b7nem", "Tag", ",", "wo", "den", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr leuchtend Recht gesprochen wird.", "tokens": ["Ihr", "leuch\u00b7tend", "Recht", "ge\u00b7spro\u00b7chen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann wird's nur ", "tokens": ["Dann", "wird's", "nur"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Des Eigen-Neides Widerstreben,", "tokens": ["Des", "Ei\u00b7gen\u00b7Nei\u00b7des", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der es f\u00fcr Traum und Wahnsinn h\u00e4lt.", "tokens": ["Der", "es", "f\u00fcr", "Traum", "und", "Wahn\u00b7sinn", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer jene Hoffnung gab verloren", "tokens": ["Wer", "je\u00b7ne", "Hoff\u00b7nung", "gab", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und b\u00f6slich sie verloren gab,", "tokens": ["Und", "b\u00f6s\u00b7lich", "sie", "ver\u00b7lo\u00b7ren", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der w\u00e4re besser ungeboren;", "tokens": ["Der", "w\u00e4\u00b7re", "bes\u00b7ser", "un\u00b7ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn lebend wohnt er schon im Grab.", "tokens": ["Denn", "le\u00b7bend", "wohnt", "er", "schon", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}