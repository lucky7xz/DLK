{"textgrid.poem.61889": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsfeier in Athen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch denk' ich des Tags, da du sonnengebr\u00e4unt", "tokens": ["Noch", "denk'", "ich", "des", "Tags", ",", "da", "du", "son\u00b7nen\u00b7ge\u00b7br\u00e4unt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Heimkehrtest von Zantes Gestaden, o Freund,", "tokens": ["Heim\u00b7kehr\u00b7test", "von", "Zan\u00b7tes", "Ge\u00b7sta\u00b7den", ",", "o", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Um das Fest zu begehn", "tokens": ["Um", "das", "Fest", "zu", "be\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In dem sch\u00f6nen, dem veilchenbekr\u00e4nzten Athen.", "tokens": ["In", "dem", "sch\u00f6\u00b7nen", ",", "dem", "veil\u00b7chen\u00b7be\u00b7kr\u00e4nz\u00b7ten", "A\u00b7then", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mit wehenden Locken und freudigem Gru\u00df", "tokens": ["Mit", "we\u00b7hen\u00b7den", "Lo\u00b7cken", "und", "freu\u00b7di\u00b7gem", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hinschrittest du leicht, als beschwingte den Fu\u00df", "tokens": ["Hin\u00b7schrit\u00b7test", "du", "leicht", ",", "als", "be\u00b7schwing\u00b7te", "den", "Fu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dir ein ahnend Gef\u00fchl,", "tokens": ["Dir", "ein", "ah\u00b7nend", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und ich folgte dir nach in des Volkes Gew\u00fchl.", "tokens": ["Und", "ich", "folg\u00b7te", "dir", "nach", "in", "des", "Vol\u00b7kes", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Schon stand der Hymettus in purpurner Glut,", "tokens": ["Schon", "stand", "der", "Hy\u00b7met\u00b7tus", "in", "pur\u00b7pur\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie ein K\u00f6nig im Schmuck, und die t\u00f6nende Flut", "tokens": ["Wie", "ein", "K\u00f6\u00b7nig", "im", "Schmuck", ",", "und", "die", "t\u00f6\u00b7nen\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Go\u00df klar wie Rubin", "tokens": ["Go\u00df", "klar", "wie", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "KOKOM", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Durch die Blumen des Tals der Ilissus dahin.", "tokens": ["Durch", "die", "Blu\u00b7men", "des", "Tals", "der", "I\u00b7lis\u00b7sus", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NE", "PAV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und die J\u00fcnglinge pr\u00fcften die Kraft des Gespanns", "tokens": ["Und", "die", "J\u00fcng\u00b7lin\u00b7ge", "pr\u00fcf\u00b7ten", "die", "Kraft", "des", "Ge\u00b7spanns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wettjagend im Feld, und es schwebte der Tanz", "tokens": ["Wett\u00b7ja\u00b7gend", "im", "Feld", ",", "und", "es", "schweb\u00b7te", "der", "Tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Blondlockiger Fraun", "tokens": ["Blon\u00b7dlo\u00b7cki\u00b7ger", "Fraun"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "----+", "measure": "unknown.measure.single"}, "line.4": {"text": "Um die S\u00e4ulen des Zeus, die im Strom sich beschaun.", "tokens": ["Um", "die", "S\u00e4u\u00b7len", "des", "Zeus", ",", "die", "im", "Strom", "sich", "be\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NE", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Doch, die Schl\u00e4fe mit bakchischem Eppich umlaubt,", "tokens": ["Doch", ",", "die", "Schl\u00e4\u00b7fe", "mit", "bak\u00b7chi\u00b7schem", "Ep\u00b7pich", "um\u00b7laubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sa\u00df schweigsam die Sch\u00f6nste, das sinnende Haupt", "tokens": ["Sa\u00df", "schweig\u00b7sam", "die", "Sch\u00f6ns\u00b7te", ",", "das", "sin\u00b7nen\u00b7de", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Auf die Zither gelehnt,", "tokens": ["Auf", "die", "Zi\u00b7ther", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mit dem d\u00e4mmernden Blick, der nach Liebe sich sehnt.", "tokens": ["Mit", "dem", "d\u00e4m\u00b7mern\u00b7den", "Blick", ",", "der", "nach", "Lie\u00b7be", "sich", "sehnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Und es traf dich ihr Aug', und du gr\u00fc\u00dftest sie k\u00fchn,", "tokens": ["Und", "es", "traf", "dich", "ihr", "Aug'", ",", "und", "du", "gr\u00fc\u00df\u00b7test", "sie", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und ich sah sie erbleichen und hastig ergl\u00fchn;", "tokens": ["Und", "ich", "sah", "sie", "er\u00b7blei\u00b7chen", "und", "has\u00b7tig", "er\u00b7gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "In befl\u00fcgelter Eil'", "tokens": ["In", "be\u00b7fl\u00fc\u00b7gel\u00b7ter", "Eil'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hatt' euch Eros ber\u00fchrt mit dem feurigen Pfeil.", "tokens": ["Hatt'", "euch", "E\u00b7ros", "be\u00b7r\u00fchrt", "mit", "dem", "feu\u00b7ri\u00b7gen", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und er lehrt' euch, was z\u00e4rtliche Trunkenheit spricht,", "tokens": ["Und", "er", "lehrt'", "euch", ",", "was", "z\u00e4rt\u00b7li\u00b7che", "Trun\u00b7ken\u00b7heit", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und die Fremdheit der Zungen verwehrt' es euch nicht;", "tokens": ["Und", "die", "Fremd\u00b7heit", "der", "Zun\u00b7gen", "ver\u00b7wehrt'", "es", "euch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ihr vernahmet im Wort,", "tokens": ["Ihr", "ver\u00b7nah\u00b7met", "im", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Im gestammelten, nur der Empfindung Akkord.", "tokens": ["Im", "ge\u00b7stam\u00b7mel\u00b7ten", ",", "nur", "der", "Emp\u00b7fin\u00b7dung", "Ak\u00b7kord", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADV", "ART", "NN", "NE", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Und der Tag war vergl\u00fcht, und ihr wu\u00dftet es kaum,", "tokens": ["Und", "der", "Tag", "war", "ver\u00b7gl\u00fcht", ",", "und", "ihr", "wu\u00df\u00b7tet", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und, die Sterne zu H\u00e4upten, in seligem Traum", "tokens": ["Und", ",", "die", "Ster\u00b7ne", "zu", "H\u00e4up\u00b7ten", ",", "in", "se\u00b7li\u00b7gem", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NN", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Hinwalltet ihr sacht", "tokens": ["Hin\u00b7wall\u00b7tet", "ihr", "sacht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Durchs ambrosische Dunkel der attischen Nacht.", "tokens": ["Durchs", "am\u00b7bro\u00b7si\u00b7sche", "Dun\u00b7kel", "der", "at\u00b7ti\u00b7schen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}