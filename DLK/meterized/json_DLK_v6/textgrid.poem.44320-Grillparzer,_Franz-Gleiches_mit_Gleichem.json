{"textgrid.poem.44320": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gleiches mit Gleichem", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Florenz lebte eine Sch\u00f6ne,", "tokens": ["In", "Flo\u00b7renz", "leb\u00b7te", "ei\u00b7ne", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ber\u00fchmt durch manchen schweren Sieg,", "tokens": ["be\u00b7r\u00fchmt", "durch", "man\u00b7chen", "schwe\u00b7ren", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so reizend, da\u00df selbst Anadiomene,", "tokens": ["so", "rei\u00b7zend", ",", "da\u00df", "selbst", "A\u00b7na\u00b7di\u00b7o\u00b7me\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "ADV", "NE", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "als sie der Flut entschwamm, ihr wich,", "tokens": ["als", "sie", "der", "Flut", "ent\u00b7schwamm", ",", "ihr", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch launisch, wie das Meer, aus dem sie stieg.", "tokens": ["Doch", "lau\u00b7nisch", ",", "wie", "das", "Meer", ",", "aus", "dem", "sie", "stieg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer sie nur sah, der ward entbrannt,", "tokens": ["Wer", "sie", "nur", "sah", ",", "der", "ward", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "aus jedem Munde h\u00f6rt sie Seufzer schallen,", "tokens": ["aus", "je\u00b7dem", "Mun\u00b7de", "h\u00f6rt", "sie", "Seuf\u00b7zer", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "allein umsonst, denn, ach, von allen,", "tokens": ["al\u00b7lein", "um\u00b7sonst", ",", "denn", ",", "ach", ",", "von", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "$,", "ITJ", "$,", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "die sie auf jedem Schritt umgaben, fand", "tokens": ["die", "sie", "auf", "je\u00b7dem", "Schritt", "um\u00b7ga\u00b7ben", ",", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PRELS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "nicht einer Mittel, Julien zu gefallen.", "tokens": ["nicht", "ei\u00b7ner", "Mit\u00b7tel", ",", "Ju\u00b7li\u00b7en", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Das \u00dcbel greifet immer weiter,", "tokens": ["Das", "\u00dc\u00b7bel", "grei\u00b7fet", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "der eine ", "tokens": ["der", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "ja, und verd\u00fcrbe Wasser nicht die Kleider,", "tokens": ["ja", ",", "und", "ver\u00b7d\u00fcr\u00b7be", "Was\u00b7ser", "nicht", "die", "Klei\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "ADJA", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "ein andrer l\u00e4ge l\u00e4ngst im n\u00e4chsten Flu\u00df,", "tokens": ["ein", "an\u00b7drer", "l\u00e4\u00b7ge", "l\u00e4ngst", "im", "n\u00e4chs\u00b7ten", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "ein dritter plagt in einer langen Ode", "tokens": ["ein", "drit\u00b7ter", "plagt", "in", "ei\u00b7ner", "lan\u00b7gen", "O\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "sich und den Leser grausamlich zu Tode.", "tokens": ["sich", "und", "den", "Le\u00b7ser", "grau\u00b7sam\u00b7lich", "zu", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}