{"textgrid.poem.48702": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf des lobw\u00fcrdigsten K\u00f6nigs der Schweden Gustaf Adolfs des Gro\u00dfen seinen Todesfall", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschwinder als der Plitz, mit mehr als Donnerschrecken", "tokens": ["Ge\u00b7schwin\u00b7der", "als", "der", "Plitz", ",", "mit", "mehr", "als", "Don\u00b7ner\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "APPR", "PIAT", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "brach ich, ein wahrer Mars, aus Norden stark herf\u00fcr.", "tokens": ["brach", "ich", ",", "ein", "wah\u00b7rer", "Mars", ",", "aus", "Nor\u00b7den", "stark", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit mir kam Furcht und Tod, man hie\u00df mich da und hier", "tokens": ["Mit", "mir", "kam", "Furcht", "und", "Tod", ",", "man", "hie\u00df", "mich", "da", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$,", "PIS", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gerechtigkeit und Krieg. Ich schlug an allen Ecken", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Krieg", ".", "Ich", "schlug", "an", "al\u00b7len", "E\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wie Glas die Kraft entzwei, die wider mich sich strecken", "tokens": ["wie", "Glas", "die", "Kraft", "ent\u00b7zwei", ",", "die", "wi\u00b7der", "mich", "sich", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und trutzig durfte sein. Gesteh' es, Deutschland, mir,", "tokens": ["und", "trut\u00b7zig", "durf\u00b7te", "sein", ".", "Ge\u00b7steh'", "es", ",", "Deutschland", ",", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VAINF", "$.", "NN", "PPER", "$,", "NE", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df ich zu Dienste steif mich unterst\u00fctzte dir,", "tokens": ["da\u00df", "ich", "zu", "Diens\u00b7te", "steif", "mich", "un\u00b7ter\u00b7st\u00fctz\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da nirgends m\u00fcglich war dergleichen zu erwecken,", "tokens": ["da", "nir\u00b7gends", "m\u00fcg\u00b7lich", "war", "derg\u00b7lei\u00b7chen", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "der erste bei der Schlacht, der Kl\u00fcgste vor Gerichte.", "tokens": ["der", "ers\u00b7te", "bei", "der", "Schlacht", ",", "der", "Kl\u00fcgs\u00b7te", "vor", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glanz der Kaiser stirbt vor meiner Tugend Lichte,", "tokens": ["Der", "Glanz", "der", "Kai\u00b7ser", "stirbt", "vor", "mei\u00b7ner", "Tu\u00b7gend", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die ganze Welt erstarrt vor meiner Ehren Schein.", "tokens": ["die", "gan\u00b7ze", "Welt", "er\u00b7starrt", "vor", "mei\u00b7ner", "Eh\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn hat man vor der Zeit so einen Held gesp\u00fcret?", "tokens": ["Wenn", "hat", "man", "vor", "der", "Zeit", "so", "ei\u00b7nen", "Held", "ge\u00b7sp\u00fc\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe lebend stets, ja tot auch triumfiret.", "tokens": ["Ich", "ha\u00b7be", "le\u00b7bend", "stets", ",", "ja", "tot", "auch", "tri\u00b7um\u00b7fi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Feld, da ich gesiegt, das sol mein Grabmal sein.", "tokens": ["Das", "Feld", ",", "da", "ich", "ge\u00b7siegt", ",", "das", "sol", "mein", "Grab\u00b7mal", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}