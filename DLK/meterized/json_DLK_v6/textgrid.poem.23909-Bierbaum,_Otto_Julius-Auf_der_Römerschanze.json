{"textgrid.poem.23909": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Auf der R\u00f6merschanze", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Roma hat uns das Bett gemacht,", "tokens": ["Frau", "Ro\u00b7ma", "hat", "uns", "das", "Bett", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die goldene Sonne hat dr\u00fcber gewacht,", "tokens": ["Die", "gol\u00b7de\u00b7ne", "Son\u00b7ne", "hat", "dr\u00fc\u00b7ber", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ei jo!", "tokens": ["Ei", "jo", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vom blauesten Himmel wars \u00fcberdacht.", "tokens": ["Vom", "blau\u00b7es\u00b7ten", "Him\u00b7mel", "wars", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Das hohe Gras war flaumfederweich,", "tokens": ["Das", "ho\u00b7he", "Gras", "war", "flaum\u00b7fe\u00b7der\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein C\u00e4sar machte den Boden uns gleich,", "tokens": ["Ein", "C\u00e4\u00b7sar", "mach\u00b7te", "den", "Bo\u00b7den", "uns", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ei jo!", "tokens": ["Ei", "jo", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wer hat ein Bett so rar und reich?", "tokens": ["Wer", "hat", "ein", "Bett", "so", "rar", "und", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}