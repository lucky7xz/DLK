{"textgrid.poem.39599": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stolpholperl war stark in der Dichtkunst,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolpholperl war stark in der Dichtkunst,", "tokens": ["Stol\u00b7phol\u00b7perl", "war", "stark", "in", "der", "Dicht\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und schmiedete Verse gar viel;", "tokens": ["Und", "schmie\u00b7de\u00b7te", "Ver\u00b7se", "gar", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch gewann er der Kritiker Richtgunst", "tokens": ["Auch", "ge\u00b7wann", "er", "der", "Kri\u00b7ti\u00b7ker", "Richt\u00b7gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mit seinem packspaltigen Kiel.", "tokens": ["Mit", "sei\u00b7nem", "pack\u00b7spal\u00b7ti\u00b7gen", "Kiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Er sang von der Liebenden Scherzzorn", "tokens": ["Er", "sang", "von", "der", "Lie\u00b7ben\u00b7den", "Scherz\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+--++", "measure": "prosodiakos"}, "line.2": {"text": "Und den Listen des schalkhaften Gotts.", "tokens": ["Und", "den", "Lis\u00b7ten", "des", "schalk\u00b7haf\u00b7ten", "Gotts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das erregt mir den innersten Herzborn:", "tokens": ["Das", "er\u00b7regt", "mir", "den", "in\u00b7ners\u00b7ten", "Herz\u00b7born", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Potz Tausend und tausendmal Potz!", "tokens": ["Potz", "Tau\u00b7send", "und", "tau\u00b7send\u00b7mal", "Potz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "KON", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Mir wurde der Scherzzorn zum Zornscherz;", "tokens": ["Mir", "wur\u00b7de", "der", "Scherz\u00b7zorn", "zum", "Zorn\u00b7scherz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Stolpholperl hat's \u00fcbel gelenkt.", "tokens": ["Stol\u00b7phol\u00b7perl", "hat's", "\u00fc\u00b7bel", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In den Z\u00e4hnen tobt hinten und vorn Schmerz", "tokens": ["In", "den", "Z\u00e4h\u00b7nen", "tobt", "hin\u00b7ten", "und", "vorn", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "KON", "ADV", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Kinnbacken sind mir verrenkt.", "tokens": ["Und", "die", "Kinn\u00b7ba\u00b7cken", "sind", "mir", "ver\u00b7renkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}