{"dta.poem.1798": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.71", "fr:0.28"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kanst du nur die Heller sparen/", "tokens": ["Kanst", "du", "nur", "die", "Hel\u00b7ler", "spa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df den gutten Nahmen fahren/", "tokens": ["La\u00df", "den", "gut\u00b7ten", "Nah\u00b7men", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer fragt itzo viel nach Flecken", "tokens": ["Wer", "fragt", "it\u00b7zo", "viel", "nach", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die man kan mit Gelde decken?", "tokens": ["Die", "man", "kan", "mit", "Gel\u00b7de", "de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}