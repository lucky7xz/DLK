{"textgrid.poem.33473": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Ausflug", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, ich halte diese festen", "tokens": ["Du", ",", "ich", "hal\u00b7te", "die\u00b7se", "fes\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stuben und die d\u00fcrren Stra\u00dfen", "tokens": ["Stu\u00b7ben", "und", "die", "d\u00fcr\u00b7ren", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die rote H\u00e4usersonne,", "tokens": ["Und", "die", "ro\u00b7te", "H\u00e4u\u00b7ser\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verruchte Unlust aller", "tokens": ["Die", "ver\u00b7ruch\u00b7te", "Un\u00b7lust", "al\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4ngst schon abgeblickten B\u00fccher", "tokens": ["L\u00e4ngst", "schon", "ab\u00b7ge\u00b7blick\u00b7ten", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht mehr aus.", "tokens": ["Nicht", "mehr", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Komm, wir m\u00fcssen von der Stadt", "tokens": ["Komm", ",", "wir", "m\u00fcs\u00b7sen", "von", "der", "Stadt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit hinweg.", "tokens": ["Weit", "hin\u00b7weg", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wollen uns in eine sanfte", "tokens": ["Wol\u00b7len", "uns", "in", "ei\u00b7ne", "sanf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiese legen.", "tokens": ["Wie\u00b7se", "le\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Werden drohend und so hilflos", "tokens": ["Wer\u00b7den", "dro\u00b7hend", "und", "so", "hil\u00b7flos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "KON", "ADV", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gegen den unsinnig gro\u00dfen,", "tokens": ["Ge\u00b7gen", "den", "un\u00b7sin\u00b7nig", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "T\u00f6dlich blauen, blanken Himmel", "tokens": ["T\u00f6d\u00b7lich", "blau\u00b7en", ",", "blan\u00b7ken", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die entfleischten, dumpfen Augen,", "tokens": ["Die", "ent\u00b7fleischten", ",", "dum\u00b7pfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.9": {"text": "Die verwunschnen,", "tokens": ["Die", "ver\u00b7wun\u00b7schnen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Und verheulte H\u00e4nde heben.", "tokens": ["Und", "ver\u00b7heul\u00b7te", "H\u00e4n\u00b7de", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}