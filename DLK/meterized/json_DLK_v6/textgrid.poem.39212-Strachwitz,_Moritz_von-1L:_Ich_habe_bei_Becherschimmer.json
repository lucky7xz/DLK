{"textgrid.poem.39212": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe bei Becherschimmer", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe bei Becherschimmer", "tokens": ["Ich", "ha\u00b7be", "bei", "Be\u00b7cher\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gestern allein gewacht", "tokens": ["Ge\u00b7stern", "al\u00b7lein", "ge\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und habe wohl, wie immer,", "tokens": ["Und", "ha\u00b7be", "wohl", ",", "wie", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Schlachten und St\u00fcrme gedacht.", "tokens": ["An", "Schlach\u00b7ten", "und", "St\u00fcr\u00b7me", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der Wein, der kraftgew\u00fcrzte,", "tokens": ["Der", "Wein", ",", "der", "kraft\u00b7ge\u00b7w\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War hell wie Heldenblut,", "tokens": ["War", "hell", "wie", "Hel\u00b7den\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, je mehr ich hinunterst\u00fcrzte,", "tokens": ["Doch", ",", "je", "mehr", "ich", "hin\u00b7un\u00b7ter\u00b7st\u00fcrz\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je tr\u00fcber ward mein Mut.", "tokens": ["Je", "tr\u00fc\u00b7ber", "ward", "mein", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich mocht' es nicht mehr tragen,", "tokens": ["Ich", "mocht'", "es", "nicht", "mehr", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich ging in die Nacht hinein;", "tokens": ["Ich", "ging", "in", "die", "Nacht", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lichtwellen sah ich schlagen", "tokens": ["Licht\u00b7wel\u00b7len", "sah", "ich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aus Fenster und Fensterlein.", "tokens": ["Aus", "Fens\u00b7ter", "und", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da sah wie ein Bettlerkind ich", "tokens": ["Da", "sah", "wie", "ein", "Bett\u00b7ler\u00b7kind", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In jeden erhellten Raum;", "tokens": ["In", "je\u00b7den", "er\u00b7hell\u00b7ten", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wo steht ", "tokens": ["Wo", "steht"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Und als ich kam nach Hause,", "tokens": ["Und", "als", "ich", "kam", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ist das in aller Welt?", "tokens": ["Was", "ist", "das", "in", "al\u00b7ler", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "APPR", "PIAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da war in meiner Klause", "tokens": ["Da", "war", "in", "mei\u00b7ner", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein jedes Fenster erhellt.", "tokens": ["Ein", "je\u00b7des", "Fens\u00b7ter", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und als ich trat ins Zimmer,", "tokens": ["Und", "als", "ich", "trat", "ins", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da war's nicht mehr ein Traum,", "tokens": ["Da", "wa\u00b7r's", "nicht", "mehr", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Da stand im vollsten Schimmer", "tokens": ["Da", "stand", "im", "volls\u00b7ten", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6nste Weihnachtsbaum.", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Weih\u00b7nachts\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und an dem Strahl der Kerzen,", "tokens": ["Und", "an", "dem", "Strahl", "der", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da f\u00fchlt' ich, wie zerschmolz", "tokens": ["Da", "f\u00fchlt'", "ich", ",", "wie", "zer\u00b7schmolz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im sturmbegierigen Herzen", "tokens": ["Im", "sturm\u00b7be\u00b7gie\u00b7ri\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der wilde, sehnende Stolz.", "tokens": ["Der", "wil\u00b7de", ",", "seh\u00b7nen\u00b7de", "Stolz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Es war so mild zu schauen,", "tokens": ["Es", "war", "so", "mild", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie jedes Lichtlein glomm,", "tokens": ["Wie", "je\u00b7des", "Licht\u00b7lein", "glomm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In die Augen t\u00e4t' mir tauen", "tokens": ["In", "die", "Au\u00b7gen", "t\u00e4t'", "mir", "tau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein F\u00fchlen kindesfromm.", "tokens": ["Ein", "F\u00fch\u00b7len", "kin\u00b7des\u00b7fromm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mir war's, als d\u00fcrft' ich tr\u00e4umen,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "d\u00fcrft'", "ich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich sei nicht mehr verwaist,", "tokens": ["Ich", "sei", "nicht", "mehr", "ver\u00b7waist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und es webte in den R\u00e4umen", "tokens": ["Und", "es", "web\u00b7te", "in", "den", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Mutter s\u00fc\u00dfer Geist.", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "s\u00fc\u00b7\u00dfer", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch die den Baum mir stellten", "tokens": ["Doch", "die", "den", "Baum", "mir", "stell\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meine \u00f6de Nacht,", "tokens": ["In", "mei\u00b7ne", "\u00f6\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag's ihnen Gott vergelten,", "tokens": ["Mag's", "ih\u00b7nen", "Gott", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie selig sie mich gemacht!", "tokens": ["Wie", "se\u00b7lig", "sie", "mich", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}