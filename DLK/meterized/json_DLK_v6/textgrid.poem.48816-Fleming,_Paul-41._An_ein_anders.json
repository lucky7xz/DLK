{"textgrid.poem.48816": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "41. An ein anders", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch du wilst mir ein Pfand auf ihre Treue sein,", "tokens": ["Auch", "du", "wilst", "mir", "ein", "Pfand", "auf", "ih\u00b7re", "Treu\u00b7e", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "an der ich niemals doch bei mir gezweifelt habe?", "tokens": ["an", "der", "ich", "nie\u00b7mals", "doch", "bei", "mir", "ge\u00b7zwei\u00b7felt", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was denkt sie doch, mein Lieb, da\u00df sie dich fertigt abe?", "tokens": ["Was", "denkt", "sie", "doch", ",", "mein", "Lieb", ",", "da\u00df", "sie", "dich", "fer\u00b7tigt", "a\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hab' ich doch, wie auch sie von mir mein ganzes Mein,", "tokens": ["Hab'", "ich", "doch", ",", "wie", "auch", "sie", "von", "mir", "mein", "gan\u00b7zes", "Mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWAV", "ADV", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "vorhin ihr ganzes Sie. Doch sprich nur zu mir ein", "tokens": ["vor\u00b7hin", "ihr", "gan\u00b7zes", "Sie", ".", "Doch", "sprich", "nur", "zu", "mir", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "PPER", "$.", "KON", "ADV", "ADV", "APPR", "PPER", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und ziere mich mit dir. Du mehr als edle Gabe,", "tokens": ["und", "zie\u00b7re", "mich", "mit", "dir", ".", "Du", "mehr", "als", "ed\u00b7le", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "PIAT", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "solst auch mit dieser Hand, der deinen, gehn zu Grabe,", "tokens": ["solst", "auch", "mit", "die\u00b7ser", "Hand", ",", "der", "dei\u00b7nen", ",", "gehn", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wenn mich bedecken soll mein blasser Leichenstein.", "tokens": ["wenn", "mich", "be\u00b7de\u00b7cken", "soll", "mein", "blas\u00b7ser", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was send' ich ihr f\u00fcr dich? Gold ist f\u00fcr sie zu schlecht,", "tokens": ["Was", "send'", "ich", "ihr", "f\u00fcr", "dich", "?", "Gold", "ist", "f\u00fcr", "sie", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$.", "NN", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "kein edler Stein, wie wert er ist, bezalt dich recht.", "tokens": ["kein", "ed\u00b7ler", "Stein", ",", "wie", "wert", "er", "ist", ",", "be\u00b7zalt", "dich", "recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist ihr auch nicht lieb, was lieb ist aller Enden.", "tokens": ["So", "ist", "ihr", "auch", "nicht", "lieb", ",", "was", "lieb", "ist", "al\u00b7ler", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich seh' in und um mich, nichts ist um und in mir.", "tokens": ["Ich", "seh'", "in", "und", "um", "mich", ",", "nichts", "ist", "um", "und", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "PPER", "$,", "PIS", "VAFIN", "APPR", "KON", "APPR", "PPER", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und h\u00e4tte sies nicht schon, so wolt' ich st\u00fcndlich", "tokens": ["Und", "h\u00e4t\u00b7te", "sies", "nicht", "schon", ",", "so", "wolt'", "ich", "st\u00fcnd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "PTKNEG", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ihr mein eignes Herze selbst zu eigen \u00fcbersenden.", "tokens": ["ihr", "mein", "eig\u00b7nes", "Her\u00b7ze", "selbst", "zu", "ei\u00b7gen", "\u00fc\u00b7ber\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "VVFIN", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}