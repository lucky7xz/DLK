{"textgrid.poem.64556": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "180. Bey Gelegenheit folgenden Sinngedichts des Hrn. Gleim", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf diesem lieblichen Gefilde,", "tokens": ["Auf", "die\u00b7sem", "lieb\u00b7li\u00b7chen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mahl' ihn, als wie er schreibt, mahl' ihn, als wie er spricht,", "tokens": ["Mahl'", "ihn", ",", "als", "wie", "er", "schreibt", ",", "mahl'", "ihn", ",", "als", "wie", "er", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Ueberwinder mahl' ihn nicht.", "tokens": ["Als", "Ue\u00b7ber\u00b7win\u00b7der", "mahl'", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}