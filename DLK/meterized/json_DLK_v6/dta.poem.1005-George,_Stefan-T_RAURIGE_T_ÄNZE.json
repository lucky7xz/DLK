{"dta.poem.1005": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Keins wie dein feines ohr               ", "tokens": ["Keins", "wie", "dein", "fei\u00b7nes", "ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Merkt was tief innen singt               ", "tokens": ["Merkt", "was", "tief", "in\u00b7nen", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PWS", "ADJD", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was noch so sch&#252;chtern schwingt               ", "tokens": ["Was", "noch", "so", "sch", "&#252;", "ch\u00b7tern", "schwingt"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "$(", "VVINF", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Was halb sich schon verlor             ", "tokens": ["Was", "halb", "sich", "schon", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Keins wie dein festes wort               ", "tokens": ["Keins", "wie", "dein", "fes\u00b7tes", "wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sucht so bestimmt den trost               ", "tokens": ["Sucht", "so", "be\u00b7stimmt", "den", "trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In dem was wir erlost               ", "tokens": ["In", "dem", "was", "wir", "er\u00b7lost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des wahren friedens hort             ", "tokens": ["Des", "wah\u00b7ren", "frie\u00b7dens", "hort"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Keins wie dein fromm gem&#252;t               ", "tokens": ["Keins", "wie", "dein", "fromm", "gem", "&#252;", "t"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "ADJD", "ADJA", "$(", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bespricht so leicht den gram               ", "tokens": ["Be\u00b7spricht", "so", "leicht", "den", "gram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der eines abends nahm               ", "tokens": ["Der", "ei\u00b7nes", "a\u00b7bends", "nahm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was uns im tag gegl&#252;ht.             ", "tokens": ["Was", "uns", "im", "tag", "ge\u00b7gl", "&#252;", "ht", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "NE", "$(", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}