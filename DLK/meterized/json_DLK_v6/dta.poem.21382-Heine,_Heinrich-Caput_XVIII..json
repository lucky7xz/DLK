{"dta.poem.21382": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verfluchter Quast! der die ganze Nacht", "tokens": ["Ver\u00b7fluch\u00b7ter", "Quast", "!", "der", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die liebe Ruhe mir raubte!", "tokens": ["Die", "lie\u00b7be", "Ru\u00b7he", "mir", "raub\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er hing mir, wie des Damokles Schwert,", "tokens": ["Er", "hing", "mir", ",", "wie", "des", "Da\u00b7mok\u00b7les", "Schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So drohend \u00fcber dem Haupte!", "tokens": ["So", "dro\u00b7hend", "\u00fc\u00b7ber", "dem", "Haup\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Schien manchmal ein Schlangenkopf zu seyn,", "tokens": ["Schien", "manch\u00b7mal", "ein", "Schlan\u00b7gen\u00b7kopf", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ich h\u00f6rte ihn heimlich zischen:", "tokens": ["Und", "ich", "h\u00f6r\u00b7te", "ihn", "heim\u00b7lich", "zi\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du bist und bleibst in der Festung jetzt,", "tokens": ["Du", "bist", "und", "bleibst", "in", "der", "Fes\u00b7tung", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du kannst nicht mehr entwischen!", "tokens": ["Du", "kannst", "nicht", "mehr", "ent\u00b7wi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O, da\u00df ich w\u00e4re \u2013 seufzte ich \u2013", "tokens": ["O", ",", "da\u00df", "ich", "w\u00e4\u00b7re", "\u2013", "seufz\u00b7te", "ich", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VAFIN", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich zu Hause w\u00e4re,", "tokens": ["Da\u00df", "ich", "zu", "Hau\u00b7se", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey meiner lieben Frau in Paris,", "tokens": ["Bey", "mei\u00b7ner", "lie\u00b7ben", "Frau", "in", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im Faubourg-Poissoni\u00e8re!", "tokens": ["Im", "Fau\u00b7bour\u00b7gPois\u00b7so\u00b7ni\u00e8\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchlte, wie \u00fcber die Stirne mir", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "wie", "\u00fc\u00b7ber", "die", "Stir\u00b7ne", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch manchmal etwas gestrichen,", "tokens": ["Auch", "manch\u00b7mal", "et\u00b7was", "ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gleich einer kalten Censorhand,", "tokens": ["Gleich", "ei\u00b7ner", "kal\u00b7ten", "Cen\u00b7sor\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Gedanken wichen \u2013", "tokens": ["Und", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "wi\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}