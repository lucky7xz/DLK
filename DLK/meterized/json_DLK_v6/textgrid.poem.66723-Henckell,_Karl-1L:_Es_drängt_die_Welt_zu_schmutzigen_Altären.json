{"textgrid.poem.66723": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es dr\u00e4ngt die Welt zu schmutzigen Alt\u00e4ren", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dr\u00e4ngt die Welt zu schmutzigen Alt\u00e4ren", "tokens": ["Es", "dr\u00e4ngt", "die", "Welt", "zu", "schmut\u00b7zi\u00b7gen", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und geht vorbei, wo rein das Opfer flammt ...", "tokens": ["Und", "geht", "vor\u00b7bei", ",", "wo", "rein", "das", "Op\u00b7fer", "flammt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Priester m\u00fcssen sich vom P\u00f6bel n\u00e4hren.", "tokens": ["Die", "Pries\u00b7ter", "m\u00fcs\u00b7sen", "sich", "vom", "P\u00f6\u00b7bel", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Versteh und l\u00e4chle! Sonst zu Qual verdammt,", "tokens": ["Ver\u00b7steh", "und", "l\u00e4ch\u00b7le", "!", "Sonst", "zu", "Qual", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hoffst du umsonst, da\u00df sie das Heilige sp\u00fcre,", "tokens": ["Hoffst", "du", "um\u00b7sonst", ",", "da\u00df", "sie", "das", "Hei\u00b7li\u00b7ge", "sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das aus den unentweihten H\u00f6hen stammt.", "tokens": ["Das", "aus", "den", "un\u00b7ent\u00b7weih\u00b7ten", "H\u00f6\u00b7hen", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df angelehnt der H\u00fctte schmale T\u00fcre,", "tokens": ["La\u00df", "an\u00b7ge\u00b7lehnt", "der", "H\u00fct\u00b7te", "schma\u00b7le", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo deiner Liebe frommes Feuer brennt,", "tokens": ["Wo", "dei\u00b7ner", "Lie\u00b7be", "from\u00b7mes", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch warte nicht, da\u00df es der Haufe sch\u00fcre!", "tokens": ["Doch", "war\u00b7te", "nicht", ",", "da\u00df", "es", "der", "Hau\u00b7fe", "sch\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Pilgers denke, dessen Blick erkennt", "tokens": ["Des", "Pil\u00b7gers", "den\u00b7ke", ",", "des\u00b7sen", "Blick", "er\u00b7kennt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von fern am Rauch das Sanktum deiner Gabe \u2013", "tokens": ["Von", "fern", "am", "Rauch", "das", "Sank\u00b7tum", "dei\u00b7ner", "Ga\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn f\u00fchrt sein Weg zum gleichen Element.", "tokens": ["Ihn", "f\u00fchrt", "sein", "Weg", "zum", "glei\u00b7chen", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er \u00f6ffnet leise mit dem Wanderstabe", "tokens": ["Er", "\u00f6ff\u00b7net", "lei\u00b7se", "mit", "dem", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Pf\u00f6rtchen, klopft dir auf die Schulter sacht", "tokens": ["Das", "Pf\u00f6rt\u00b7chen", ",", "klopft", "dir", "auf", "die", "Schul\u00b7ter", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und spricht: \u00bbIch bringe alles, was ich habe.", "tokens": ["Und", "spricht", ":", "\u00bb", "Ich", "brin\u00b7ge", "al\u00b7les", ",", "was", "ich", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hier ist mein Herz, dem Herzen dargebracht.\u00ab", "tokens": ["Hier", "ist", "mein", "Herz", ",", "dem", "Her\u00b7zen", "dar\u00b7ge\u00b7bracht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}