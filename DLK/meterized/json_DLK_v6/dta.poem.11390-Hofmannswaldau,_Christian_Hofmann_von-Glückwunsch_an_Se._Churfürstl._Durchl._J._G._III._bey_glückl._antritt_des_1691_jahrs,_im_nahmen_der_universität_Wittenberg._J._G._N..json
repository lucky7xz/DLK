{"dta.poem.11390": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch an Se. Churf\u00fcrstl. Durchl.  \n  J. G. III.  bey gl\u00fcckl. antritt des 1691 jahrs,  \n im nahmen der universit\u00e4t  \n Wittenberg.  \n J. G. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Theurer trost bedr\u00e4ngter zeiten!", "tokens": ["Theu\u00b7rer", "trost", "be\u00b7dr\u00e4ng\u00b7ter", "zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hector der verfolgten welt!", "tokens": ["Hec\u00b7tor", "der", "ver\u00b7folg\u00b7ten", "welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem gantz Teutschland sich zur seiten,", "tokens": ["Dem", "gantz", "Teutschland", "sich", "zur", "sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PRF", "APPRART", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auch wohl gar zun f\u00fcssen, stellt:", "tokens": ["Auch", "wohl", "gar", "zun", "f\u00fcs\u00b7sen", ",", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem die vorwelt zinsen reicht,", "tokens": ["Dem", "die", "vor\u00b7welt", "zin\u00b7sen", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja vor welchem ost und westen offt die stoltzen seegel", "tokens": ["Ja", "vor", "wel\u00b7chem", "ost", "und", "wes\u00b7ten", "offt", "die", "stolt\u00b7zen", "see\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "PWAT", "NN", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Nimm mit gn\u00e4digem gem\u00fcthe,", "tokens": ["Nimm", "mit", "gn\u00e4\u00b7di\u00b7gem", "ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die demuth bringen kan!", "tokens": ["Was", "die", "de\u00b7muth", "brin\u00b7gen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn es sieht des himmels-g\u00fcte", "tokens": ["Denn", "es", "sieht", "des", "him\u00b7mels\u00b7g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch ein sp\u00e4tes opfer an.", "tokens": ["Auch", "ein", "sp\u00e4\u00b7tes", "op\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fehlt uns gleich dein sonnenlicht;", "tokens": ["Fehlt", "uns", "gleich", "dein", "son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So vergessen deine strahlen doch der hyacinthen", "tokens": ["So", "ver\u00b7ges\u00b7sen", "dei\u00b7ne", "strah\u00b7len", "doch", "der", "hya\u00b7cin\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Unsres Pindus hohe spitzen", "tokens": ["Uns\u00b7res", "Pin\u00b7dus", "ho\u00b7he", "spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stunden hier und da verletzt;", "tokens": ["Stun\u00b7den", "hier", "und", "da", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nun hast du g\u00fcldne st\u00fctzen", "tokens": ["Doch", "nun", "hast", "du", "g\u00fcld\u00b7ne", "st\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter diesen ri\u00df gesetzt:", "tokens": ["Un\u00b7ter", "die\u00b7sen", "ri\u00df", "ge\u00b7setzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Elb-strohm h\u00e4ltst du rein,", "tokens": ["Dei\u00b7nen", "Elb\u00b7strohm", "h\u00e4ltst", "du", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und l\u00e4ufft sonst das wasser tr\u00fcbe, mu\u00df es hier cry-", "tokens": ["Und", "l\u00e4ufft", "sonst", "das", "was\u00b7ser", "tr\u00fc\u00b7be", ",", "mu\u00df", "es", "hier", "cry"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "$,", "VMFIN", "PPER", "ADV", "TRUNC"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "La\u00df die faleonetten knallen!", "tokens": ["La\u00df", "die", "fa\u00b7le\u00b7o\u00b7net\u00b7ten", "knal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Unser sitz bleibt unbewegt:", "tokens": ["Un\u00b7ser", "sitz", "bleibt", "un\u00b7be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die feuer-kugeln fallen!", "tokens": ["La\u00df", "die", "feu\u00b7er\u00b7ku\u00b7geln", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts ist, das den lorbeer schl\u00e4gt;", "tokens": ["Nichts", "ist", ",", "das", "den", "lor\u00b7beer", "schl\u00e4gt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn wir in den schatten gehn,", "tokens": ["Wenn", "wir", "in", "den", "schat\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df dein haupt bey sturm und wetter unter freyem", "tokens": ["Mu\u00df", "dein", "haupt", "bey", "sturm", "und", "wet\u00b7ter", "un\u00b7ter", "frey\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ADJD", "KON", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}