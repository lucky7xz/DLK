{"textgrid.poem.35247": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, milder Gott, begnade mich,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, milder Gott, begnade mich,", "tokens": ["Ach", ",", "mil\u00b7der", "Gott", ",", "be\u00b7gna\u00b7de", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Indem ich will erkennen dich,", "tokens": ["In\u00b7dem", "ich", "will", "er\u00b7ken\u00b7nen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Wege wallen.", "tokens": ["Und", "dei\u00b7ne", "We\u00b7ge", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erneu' mein Herz und nimm mich mir;", "tokens": ["Er\u00b7neu'", "mein", "Herz", "und", "nimm", "mich", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe mich gelobet hier", "tokens": ["Ich", "ha\u00b7be", "mich", "ge\u00b7lo\u00b7bet", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein dir zu gefallen.", "tokens": ["Al\u00b7lein", "dir", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Will' sei mein Will' f\u00fcr und f\u00fcr,", "tokens": ["Dein", "Will'", "sei", "mein", "Will'", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So da\u00df ich mich in dir verlier'.", "tokens": ["So", "da\u00df", "ich", "mich", "in", "dir", "ver\u00b7lier'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das gute Werk, das ich vollbring',", "tokens": ["Das", "gu\u00b7te", "Werk", ",", "das", "ich", "voll\u00b7bring'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ein gef\u00fcgter Kettenring,", "tokens": ["Ist", "ein", "ge\u00b7f\u00fcg\u00b7ter", "Ket\u00b7ten\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gottes Gnad' umschlossen.", "tokens": ["Von", "Got\u00b7tes", "Gnad'", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich thue nun, so viel ich woll',", "tokens": ["Ich", "thue", "nun", ",", "so", "viel", "ich", "woll'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So thu' ich doch nicht, was ich soll,", "tokens": ["So", "thu'", "ich", "doch", "nicht", ",", "was", "ich", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schwachheit ist verdrossen.", "tokens": ["Die", "Schwach\u00b7heit", "ist", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch nimmt Gott auch den Willen an,", "tokens": ["Doch", "nimmt", "Gott", "auch", "den", "Wil\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn man nur leistet, was man kann.", "tokens": ["Wenn", "man", "nur", "leis\u00b7tet", ",", "was", "man", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich meide S\u00fcnd' und Missethat", "tokens": ["Ich", "mei\u00b7de", "S\u00fcnd'", "und", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und thue Gut's, durch deine Gnad',", "tokens": ["Und", "thue", "Gut's", ",", "durch", "dei\u00b7ne", "Gnad'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel mir Huld erschienen.", "tokens": ["So", "viel", "mir", "Huld", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deines Willens Heiligkeit", "tokens": ["In", "dei\u00b7nes", "Wil\u00b7lens", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bin ich zu jeder Zeit bereit,", "tokens": ["Bin", "ich", "zu", "je\u00b7der", "Zeit", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem N\u00e4chsten stets zu dienen,", "tokens": ["Dem", "N\u00e4chs\u00b7ten", "stets", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und traure, da\u00df ich nicht kann sein", "tokens": ["Und", "trau\u00b7re", ",", "da\u00df", "ich", "nicht", "kann", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In dieser Schwachheit engelrein.", "tokens": ["In", "die\u00b7ser", "Schwach\u00b7heit", "en\u00b7gel\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie gerne wollt' ich hinter mir,", "tokens": ["Wie", "ger\u00b7ne", "wollt'", "ich", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was irdisch ist, vergessen hier", "tokens": ["Was", "ir\u00b7disch", "ist", ",", "ver\u00b7ges\u00b7sen", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Gott allein anhangen!", "tokens": ["Und", "Gott", "al\u00b7lein", "an\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie gerne wollt' ich Gottes Ehr',", "tokens": ["Wie", "ger\u00b7ne", "wollt'", "ich", "Got\u00b7tes", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was gem\u00e4\u00df ist seiner Lehr',", "tokens": ["Und", "was", "ge\u00b7m\u00e4\u00df", "ist", "sei\u00b7ner", "Lehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn' allen Ruhm erlangen,", "tokens": ["Ohn'", "al\u00b7len", "Ruhm", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf da\u00df die h\u00f6chste Heiligkeit", "tokens": ["Auf", "da\u00df", "die", "h\u00f6chs\u00b7te", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erleuchte mich zu aller Zeit.", "tokens": ["Er\u00b7leuch\u00b7te", "mich", "zu", "al\u00b7ler", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die h\u00f6chste Stufe, die man kann", "tokens": ["Die", "h\u00f6chs\u00b7te", "Stu\u00b7fe", ",", "die", "man", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Leben treten an,", "tokens": ["In", "die\u00b7sem", "Le\u00b7ben", "tre\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist, Gott vereinbar werden.", "tokens": ["Ist", ",", "Gott", "ver\u00b7ein\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann wei\u00df man nichts mehr, als von Gott,", "tokens": ["Dann", "wei\u00df", "man", "nichts", "mehr", ",", "als", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "ADV", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und achtet man f\u00fcr eiteln Spott", "tokens": ["Und", "ach\u00b7tet", "man", "f\u00fcr", "ei\u00b7teln", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nichtigkeit der Erden.", "tokens": ["Die", "Nich\u00b7tig\u00b7keit", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist der Frommen h\u00f6chster Ruhm,", "tokens": ["Das", "ist", "der", "From\u00b7men", "h\u00f6chs\u00b7ter", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vollkommen sein im Christenthum.", "tokens": ["Voll\u00b7kom\u00b7men", "sein", "im", "Chris\u00b7ten\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}