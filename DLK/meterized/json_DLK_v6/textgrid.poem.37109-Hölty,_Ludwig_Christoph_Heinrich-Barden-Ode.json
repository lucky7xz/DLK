{"textgrid.poem.37109": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Barden-Ode", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter! welche drohende Gefahren", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "wel\u00b7che", "dro\u00b7hen\u00b7de", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begleiten uns von Kindheit an,", "tokens": ["Be\u00b7glei\u00b7ten", "uns", "von", "Kind\u00b7heit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lauren stets, in unz\u00e4hlbaren Schaaren,", "tokens": ["Und", "lau\u00b7ren", "stets", ",", "in", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf unsrer Lebensbahn! \u2013", "tokens": ["Auf", "uns\u00b7rer", "Le\u00b7bens\u00b7bahn", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst, als ich lange schon genug gespielet,", "tokens": ["J\u00fcngst", ",", "als", "ich", "lan\u00b7ge", "schon", "ge\u00b7nug", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sa\u00df ich bey stiller Abendluft,", "tokens": ["Sa\u00df", "ich", "bey", "stil\u00b7ler", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mein Gelock bes\u00e4uselt und bek\u00fchlet,", "tokens": ["Die", "mein", "Ge\u00b7lock", "be\u00b7s\u00e4u\u00b7selt", "und", "be\u00b7k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meiner Felsenkluft.", "tokens": ["In", "mei\u00b7ner", "Fel\u00b7sen\u00b7kluft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mondstral glitschte durch die Felsenritze,", "tokens": ["Der", "Mond\u00b7stral", "glitschte", "durch", "die", "Fel\u00b7sen\u00b7rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beschimmernd meinen Polstermoos,", "tokens": ["Be\u00b7schim\u00b7mernd", "mei\u00b7nen", "Pols\u00b7ter\u00b7moos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Irmgard sa\u00df, mit abgel\u00f6\u00dfter M\u00fctze,", "tokens": ["Und", "Irm\u00b7gard", "sa\u00df", ",", "mit", "ab\u00b7ge\u00b7l\u00f6\u00df\u00b7ter", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir damals auf dem Schoo\u00df.", "tokens": ["Mir", "da\u00b7mals", "auf", "dem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich k\u00e4mmete dem g\u00f6ttergleichen Lamme,", "tokens": ["Ich", "k\u00e4m\u00b7me\u00b7te", "dem", "g\u00f6t\u00b7ter\u00b7glei\u00b7chen", "Lam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das alle Sch\u00f6nen \u00fcberstralt,", "tokens": ["Das", "al\u00b7le", "Sch\u00f6\u00b7nen", "\u00fc\u00b7bers\u00b7tralt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich Rabenhaar mit einem engen Kamme,", "tokens": ["Ich", "Ra\u00b7ben\u00b7haar", "mit", "ei\u00b7nem", "en\u00b7gen", "Kam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ich erst j\u00fcngst bezahlt.", "tokens": ["Den", "ich", "erst", "j\u00fcngst", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie Sterne, taumelte das Ungeziefer,", "tokens": ["Wie", "Ster\u00b7ne", ",", "tau\u00b7mel\u00b7te", "das", "Un\u00b7ge\u00b7zie\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihr zu G\u00f6tterchen ges\u00e4ugt!", "tokens": ["Von", "ihr", "zu", "G\u00f6t\u00b7ter\u00b7chen", "ge\u00b7s\u00e4ugt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rechte k\u00e4mmt', inde\u00df die Linke tiefer", "tokens": ["Die", "Rech\u00b7te", "k\u00e4mmt'", ",", "in\u00b7de\u00df", "die", "Lin\u00b7ke", "tie\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Busen schalkhaft kreucht. \u2013", "tokens": ["Zum", "Bu\u00b7sen", "schalk\u00b7haft", "kreucht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch was geschah? Am wolkumth\u00fcrmten Himmel", "tokens": ["Doch", "was", "ge\u00b7schah", "?", "Am", "wol\u00b7kumt\u00b7h\u00fcrm\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstand urpl\u00f6tzlich ein Orkan,", "tokens": ["Er\u00b7stand", "ur\u00b7pl\u00f6tz\u00b7lich", "ein", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und l\u00e4rmte mit so schrecklichem Get\u00fcmmel,", "tokens": ["Und", "l\u00e4rm\u00b7te", "mit", "so", "schreck\u00b7li\u00b7chem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als er noch nie gethan!", "tokens": ["Als", "er", "noch", "nie", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und ich erschrack! Mein Kamm entfiel zur Erden!", "tokens": ["Und", "ich", "er\u00b7schrack", "!", "Mein", "Kamm", "ent\u00b7fiel", "zur", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sprach zu meiner Lieblinginn:", "tokens": ["Ich", "sprach", "zu", "mei\u00b7ner", "Lieb\u00b7lin\u00b7ginn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! Irmgard! Irmgard! Was will hieraus werden?", "tokens": ["O", "!", "Irm\u00b7gard", "!", "Irm\u00b7gard", "!", "Was", "will", "hier\u00b7aus", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$.", "PWS", "VMFIN", "PAV", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sprach: Wo will das hin? \u2013", "tokens": ["Sie", "sprach", ":", "Wo", "will", "das", "hin", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VMFIN", "PDS", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da kroch ich hin zum Eingang meiner H\u00f6le,", "tokens": ["Da", "kroch", "ich", "hin", "zum", "Ein\u00b7gang", "mei\u00b7ner", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und steckte meinen Kopf heraus,", "tokens": ["Und", "steck\u00b7te", "mei\u00b7nen", "Kopf", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcbersah (noch schaudert mir die Seele)", "tokens": ["Und", "\u00fc\u00b7ber\u00b7sah", "(", "noch", "schau\u00b7dert", "mir", "die", "See\u00b7le", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Scene voller Graus!", "tokens": ["Die", "Sce\u00b7ne", "vol\u00b7ler", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Indem ich nun so sitz', und jammernd weine,", "tokens": ["In\u00b7dem", "ich", "nun", "so", "sitz'", ",", "und", "jam\u00b7mernd", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da st\u00fcrzte von der Felsenh\u00f6h'", "tokens": ["Da", "st\u00fcrz\u00b7te", "von", "der", "Fel\u00b7sen\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Eichbaum, der mir Schultern, Kopf und Beine", "tokens": ["Ein", "Eich\u00b7baum", ",", "der", "mir", "Schul\u00b7tern", ",", "Kopf", "und", "Bei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beynah zerschmetterte.", "tokens": ["Bey\u00b7nah", "zer\u00b7schmet\u00b7ter\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch Braga stand mir unsichtbar zur Seite,", "tokens": ["Doch", "Bra\u00b7ga", "stand", "mir", "un\u00b7sicht\u00b7bar", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stie\u00df hinweg den alten Stamm,", "tokens": ["Und", "stie\u00df", "hin\u00b7weg", "den", "al\u00b7ten", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mich ins Schattenreich zu bringen dr\u00e4ute,", "tokens": ["Der", "mich", "ins", "Schat\u00b7ten\u00b7reich", "zu", "brin\u00b7gen", "dr\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so gewaltig kam!", "tokens": ["Und", "so", "ge\u00b7wal\u00b7tig", "kam", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Allein verfluchst seyst du, du alte Eiche!", "tokens": ["Al\u00b7lein", "ver\u00b7fluchst", "seyst", "du", ",", "du", "al\u00b7te", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verflucht die Hand, die dich gesetzt!", "tokens": ["Ver\u00b7flucht", "die", "Hand", ",", "die", "dich", "ge\u00b7setzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein goldner Apfel schm\u00fccke deine Zweige,", "tokens": ["Kein", "gold\u00b7ner", "Ap\u00b7fel", "schm\u00fc\u00b7cke", "dei\u00b7ne", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von nun an bis zuletzt!", "tokens": ["Von", "nun", "an", "bis", "zu\u00b7letzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "ADV", "$."], "meter": "-+---+", "measure": "dactylic.init"}}}}}