{"textgrid.poem.66088": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchlingsreiter", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bin ich j\u00e4h erwacht.", "tokens": ["Bin", "ich", "j\u00e4h", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hufschlag hallte, ein Horn erklang,", "tokens": ["Huf\u00b7schlag", "hall\u00b7te", ",", "ein", "Horn", "er\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Da\u00df ich erschreckt ans Fenster sprang.", "tokens": ["Da\u00df", "ich", "er\u00b7schreckt", "ans", "Fens\u00b7ter", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mond schien hell,", "tokens": ["Der", "Mond", "schien", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und da kam es zur Stell:", "tokens": ["Und", "da", "kam", "es", "zur", "Stell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein Schatten voraus, dann ein milchwei\u00df Ro\u00df,", "tokens": ["Ein", "Schat\u00b7ten", "vo\u00b7raus", ",", "dann", "ein", "milch\u00b7wei\u00df", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dar\u00fcber des Mondes Silber flo\u00df,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "des", "Mon\u00b7des", "Sil\u00b7ber", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und ein Reiter ganz jung, einen blauen Kranz", "tokens": ["Und", "ein", "Rei\u00b7ter", "ganz", "jung", ",", "ei\u00b7nen", "blau\u00b7en", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Im Gelock. Hell blitzte des Hornes Glanz", "tokens": ["Im", "Ge\u00b7lock", ".", "Hell", "blitz\u00b7te", "des", "Hor\u00b7nes", "Glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "In der Faust, und er stie\u00df in das Horn hinein,", "tokens": ["In", "der", "Faust", ",", "und", "er", "stie\u00df", "in", "das", "Horn", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Als sollte und m\u00fc\u00dfte geblasen sein.", "tokens": ["Als", "soll\u00b7te", "und", "m\u00fc\u00df\u00b7te", "ge\u00b7bla\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "O war das ein Klang", "tokens": ["O", "war", "das", "ein", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PDS", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "In dem Horngesang!", "tokens": ["In", "dem", "Horn\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Eine s\u00fc\u00dfe Kraft, eine bl\u00fchende Kraft,", "tokens": ["Ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Kraft", ",", "ei\u00b7ne", "bl\u00fc\u00b7hen\u00b7de", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Eine zitternde, quellende Leidenschaft,", "tokens": ["Ei\u00b7ne", "zit\u00b7tern\u00b7de", ",", "quel\u00b7len\u00b7de", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Ein Herz und ein Jubel, ein seliger Schrei!", "tokens": ["Ein", "Herz", "und", "ein", "Ju\u00b7bel", ",", "ein", "se\u00b7li\u00b7ger", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.18": {"text": "Ein Klingen, ein Leuchten \u2013 da war es vorbei.", "tokens": ["Ein", "Klin\u00b7gen", ",", "ein", "Leuch\u00b7ten", "\u2013", "da", "war", "es", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.19": {"text": "Hatte mich ein Traum bet\u00f6rt?", "tokens": ["Hat\u00b7te", "mich", "ein", "Traum", "be\u00b7t\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Nicht einer hatte den Reiter geh\u00f6rt,", "tokens": ["Nicht", "ei\u00b7ner", "hat\u00b7te", "den", "Rei\u00b7ter", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sie lachten mich alle am Morgen aus:", "tokens": ["Sie", "lach\u00b7ten", "mich", "al\u00b7le", "am", "Mor\u00b7gen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Da kommt der Tr\u00e4umer, der Dichter heraus.", "tokens": ["Da", "kommt", "der", "Tr\u00e4u\u00b7mer", ",", "der", "Dich\u00b7ter", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Aber mein T\u00f6chterchen kam mit Hurra:", "tokens": ["A\u00b7ber", "mein", "T\u00f6ch\u00b7ter\u00b7chen", "kam", "mit", "Hur\u00b7ra", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Seht mal, die ersten Veilchen sind da!", "tokens": ["Seht", "mal", ",", "die", "ers\u00b7ten", "Veil\u00b7chen", "sind", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Und ich glaube, auch Krokus und Narzissen", "tokens": ["Und", "ich", "glau\u00b7be", ",", "auch", "Kro\u00b7kus", "und", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "NE", "KON", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Kommen schon. \u2013 Was wollt ich noch wissen?", "tokens": ["Kom\u00b7men", "schon", ".", "\u2013", "Was", "wollt", "ich", "noch", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.27": {"text": "Ich l\u00e4chelte nur und sagte: Ja, ja,", "tokens": ["Ich", "l\u00e4\u00b7chel\u00b7te", "nur", "und", "sag\u00b7te", ":", "Ja", ",", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$.", "PTKANT", "$,", "PTKANT", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Ich wei\u00df, die Veilchen sind wieder da.", "tokens": ["Ich", "wei\u00df", ",", "die", "Veil\u00b7chen", "sind", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}