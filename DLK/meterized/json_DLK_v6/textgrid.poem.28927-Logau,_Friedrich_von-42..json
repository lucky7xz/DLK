{"textgrid.poem.28927": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nheit, die man h\u00e4lt so werth,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "die", "man", "h\u00e4lt", "so", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nheit, die man so begehrt,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "die", "man", "so", "be\u00b7gehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist gar sparsam eingericht,", "tokens": ["Ist", "gar", "spar\u00b7sam", "ein\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meistens unters Angesicht;", "tokens": ["Meis\u00b7tens", "un\u00b7ters", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Menschen giengen blo\u00df,", "tokens": ["Wann", "die", "Men\u00b7schen", "gien\u00b7gen", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r sie vielmals nicht so gro\u00df;", "tokens": ["W\u00e4r", "sie", "viel\u00b7mals", "nicht", "so", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmuck und Kleider helffen ein,", "tokens": ["Schmuck", "und", "Klei\u00b7der", "helf\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Machen Anmut, geben Schein.", "tokens": ["Ma\u00b7chen", "An\u00b7mut", ",", "ge\u00b7ben", "Schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}