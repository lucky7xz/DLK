{"textgrid.poem.60093": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Grieche war dem Sch\u00f6nen hold", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Grieche war dem Sch\u00f6nen hold", "tokens": ["Der", "Grie\u00b7che", "war", "dem", "Sch\u00f6\u00b7nen", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey gr\u00fcn umwundnen Schaalen;", "tokens": ["Bey", "gr\u00fcn", "um\u00b7wund\u00b7nen", "Schaa\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Deutsche, wie des Bechers Gold,", "tokens": ["Der", "Deut\u00b7sche", ",", "wie", "des", "Be\u00b7chers", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So treu bey seinen Mahlen:", "tokens": ["So", "treu", "bey", "sei\u00b7nen", "Mah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Sch\u00f6nen hold ist unser Sinn,", "tokens": ["Dem", "Sch\u00f6\u00b7nen", "hold", "ist", "un\u00b7ser", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz ist \u00e4cht und bieder:", "tokens": ["Das", "Herz", "ist", "\u00e4cht", "und", "bie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm dieses Glas, o Vater! hin,", "tokens": ["Nimm", "die\u00b7ses", "Glas", ",", "o", "Va\u00b7ter", "!", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "FM", "NN", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir kr\u00e4nzen oft es wieder!", "tokens": ["Wir", "kr\u00e4n\u00b7zen", "oft", "es", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}