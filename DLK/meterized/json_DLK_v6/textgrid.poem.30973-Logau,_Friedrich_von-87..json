{"textgrid.poem.30973": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einander stets zu wider,", "tokens": ["Ein\u00b7an\u00b7der", "stets", "zu", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist die Art der Br\u00fcder.", "tokens": ["Das", "ist", "die", "Art", "der", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie solten seyn wie einer;", "tokens": ["Sie", "sol\u00b7ten", "seyn", "wie", "ei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "KOKOM", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das w\u00e4r viel n\u00fctz- und feiner.", "tokens": ["Das", "w\u00e4r", "viel", "n\u00fctz", "und", "fei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}