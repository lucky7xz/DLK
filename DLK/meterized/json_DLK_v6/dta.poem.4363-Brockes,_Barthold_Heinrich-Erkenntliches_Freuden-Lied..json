{"dta.poem.4363": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erkenntliches Freuden-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe, mit tausend Vergn\u00fcgen und Freuden,", "tokens": ["Ich", "se\u00b7he", ",", "mit", "tau\u00b7send", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den herrlichen Himmel, die liebliche Welt,", "tokens": ["Den", "herr\u00b7li\u00b7chen", "Him\u00b7mel", ",", "die", "lieb\u00b7li\u00b7che", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die, unsere Seele durchs Auge zu weiden,", "tokens": ["Die", ",", "un\u00b7se\u00b7re", "See\u00b7le", "durchs", "Au\u00b7ge", "zu", "wei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein m\u00e4chtiger Sch\u00f6pfer vor Augen gestellt,", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Sch\u00f6p\u00b7fer", "vor", "Au\u00b7gen", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie weislich erschaffen, regieret, erh\u00e4lt.", "tokens": ["Sie", "weis\u00b7lich", "er\u00b7schaf\u00b7fen", ",", "re\u00b7gie\u00b7ret", ",", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich seh\u2019 und erg\u00f6tze mich, dank\u2019 und erh\u00f6he", "tokens": ["Ich", "seh'", "und", "er\u00b7g\u00f6t\u00b7ze", "mich", ",", "dank'", "und", "er\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Weisheit, die g\u00fctige Liebe, die Macht,", "tokens": ["Die", "Weis\u00b7heit", ",", "die", "g\u00fc\u00b7ti\u00b7ge", "Lie\u00b7be", ",", "die", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wodurch ich die himmlisch- und irdische Pracht,", "tokens": ["Wo\u00b7durch", "ich", "die", "himm\u00b7lisch", "und", "ir\u00b7di\u00b7sche", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "In solcher erg\u00f6tzenden Lieblichkeit, sehe.", "tokens": ["In", "sol\u00b7cher", "er\u00b7g\u00f6t\u00b7zen\u00b7den", "Lieb\u00b7lich\u00b7keit", ",", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ach, la\u00df Dir, o ewiger Sch\u00f6pfer! mein Lallen,", "tokens": ["Ach", ",", "la\u00df", "Dir", ",", "o", "e\u00b7wi\u00b7ger", "Sch\u00f6p\u00b7fer", "!", "mein", "Lal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "$,", "FM", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Von thr\u00e4nenden Freuden begleitet, gefallen!", "tokens": ["Von", "thr\u00e4\u00b7nen\u00b7den", "Freu\u00b7den", "be\u00b7glei\u00b7tet", ",", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}