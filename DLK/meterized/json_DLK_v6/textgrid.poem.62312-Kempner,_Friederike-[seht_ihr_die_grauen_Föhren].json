{"textgrid.poem.62312": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[seht ihr die grauen F\u00f6hren]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr die grauen F\u00f6hren", "tokens": ["Seht", "ihr", "die", "grau\u00b7en", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am blauen runden Teich,", "tokens": ["Am", "blau\u00b7en", "run\u00b7den", "Teich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dazwischen die kleine Insel,", "tokens": ["Da\u00b7zwi\u00b7schen", "die", "klei\u00b7ne", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Fast einem Berge gleich.", "tokens": ["Fast", "ei\u00b7nem", "Ber\u00b7ge", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie ist nur halb, die Insel,", "tokens": ["Sie", "ist", "nur", "halb", ",", "die", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Von au\u00dfen kann man heran,", "tokens": ["Von", "au\u00b7\u00dfen", "kann", "man", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch jenseits von dem Berge", "tokens": ["Doch", "jen\u00b7seits", "von", "dem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man nicht mehr weiter kann.", "tokens": ["Man", "nicht", "mehr", "wei\u00b7ter", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "ADV", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man gleicht auf jener H\u00f6he", "tokens": ["Man", "gleicht", "auf", "je\u00b7ner", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fast einem Heiligenbild,", "tokens": ["Fast", "ei\u00b7nem", "Hei\u00b7li\u00b7gen\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bis man gleich einem Gletscher", "tokens": ["Bis", "man", "gleich", "ei\u00b7nem", "Glet\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herunter st\u00fcrzet wild.", "tokens": ["Her\u00b7un\u00b7ter", "st\u00fcr\u00b7zet", "wild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}