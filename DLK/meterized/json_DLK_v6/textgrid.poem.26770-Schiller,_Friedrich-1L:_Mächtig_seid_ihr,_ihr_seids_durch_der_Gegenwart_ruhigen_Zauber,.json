{"textgrid.poem.26770": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4chtig seid ihr, ihr seids durch der Gegenwart ruhigen Zauber,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4chtig seid ihr, ihr seids durch der Gegenwart ruhigen Zauber,", "tokens": ["M\u00e4ch\u00b7tig", "seid", "ihr", ",", "ihr", "seids", "durch", "der", "Ge\u00b7gen\u00b7wart", "ru\u00b7hi\u00b7gen", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Was die stille nicht wirkt, wirket die rauschende nie.", "tokens": ["Was", "die", "stil\u00b7le", "nicht", "wirkt", ",", "wir\u00b7ket", "die", "rau\u00b7schen\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "PTKNEG", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kraft erwart ich vom Mann, des Gesetzes W\u00fcrde behaupt er,", "tokens": ["Kraft", "er\u00b7wart", "ich", "vom", "Mann", ",", "des", "Ge\u00b7set\u00b7zes", "W\u00fcr\u00b7de", "be\u00b7haupt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber durch Anmut allein herrschet und herrsche das Weib.", "tokens": ["A\u00b7ber", "durch", "An\u00b7mut", "al\u00b7lein", "herr\u00b7schet", "und", "herr\u00b7sche", "das", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Manche zwar haben geherrscht durch des Geistes Macht und der Taten,", "tokens": ["Man\u00b7che", "zwar", "ha\u00b7ben", "ge\u00b7herrscht", "durch", "des", "Geis\u00b7tes", "Macht", "und", "der", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber dann haben sie dich, h\u00f6chste der Kronen, entbehrt.", "tokens": ["A\u00b7ber", "dann", "ha\u00b7ben", "sie", "dich", ",", "h\u00f6chs\u00b7te", "der", "Kro\u00b7nen", ",", "ent\u00b7behrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wahre K\u00f6nigin ist nur des Weibes weibliche Sch\u00f6nheit,", "tokens": ["Wah\u00b7re", "K\u00f6\u00b7ni\u00b7gin", "ist", "nur", "des", "Wei\u00b7bes", "weib\u00b7li\u00b7che", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Wo sie sich zeige, sie herrscht, herrschet blo\u00df, weil sie sich zeigt.", "tokens": ["Wo", "sie", "sich", "zei\u00b7ge", ",", "sie", "herrscht", ",", "herr\u00b7schet", "blo\u00df", ",", "weil", "sie", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}