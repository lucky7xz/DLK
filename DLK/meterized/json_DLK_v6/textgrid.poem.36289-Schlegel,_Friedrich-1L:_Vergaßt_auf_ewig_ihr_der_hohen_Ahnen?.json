{"textgrid.poem.36289": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verga\u00dft auf ewig ihr der hohen Ahnen?", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verga\u00dft auf ewig ihr der hohen Ahnen?", "tokens": ["Ver\u00b7ga\u00dft", "auf", "e\u00b7wig", "ihr", "der", "ho\u00b7hen", "Ah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr uneins all', an Stumpfheit alle gleich,", "tokens": ["Ihr", "un\u00b7eins", "all'", ",", "an", "Stumpf\u00b7heit", "al\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PIS", "$,", "APPR", "NN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelehrte, Laien, Herrn und Untertanen!", "tokens": ["Ge\u00b7lehr\u00b7te", ",", "Lai\u00b7en", ",", "Herrn", "und", "Un\u00b7ter\u00b7ta\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach schmolz der V\u00e4ter Tugendkraft so weich,", "tokens": ["Ach", "schmolz", "der", "V\u00e4\u00b7ter", "Tu\u00b7gend\u00b7kraft", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die einst wie Rom so Schwert als Griffel f\u00fchrten,", "tokens": ["Die", "einst", "wie", "Rom", "so", "Schwert", "als", "Grif\u00b7fel", "f\u00fchr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "NE", "ADV", "NE", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald welterobernd, bald von Kunstsinn bleich,", "tokens": ["Bald", "wel\u00b7te\u00b7ro\u00b7bernd", ",", "bald", "von", "Kunst\u00b7sinn", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Rittertum durch Caesars W\u00fcrde zierten,", "tokens": ["Das", "Rit\u00b7ter\u00b7tum", "durch", "Cae\u00b7sars", "W\u00fcr\u00b7de", "zier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der neuen Dichtkunst vollsten Strom ergossen,", "tokens": ["Der", "neu\u00b7en", "Dicht\u00b7kunst", "volls\u00b7ten", "Strom", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Europa, eh' die Kirche brach, regierten?", "tokens": ["Eu\u00b7ro\u00b7pa", ",", "eh'", "die", "Kir\u00b7che", "brach", ",", "re\u00b7gier\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Deutschland war der heil'ge Krieg entsprossen,", "tokens": ["In", "Deutschland", "war", "der", "heil'\u00b7ge", "Krieg", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Deutschland sich im Frieden ganz zerst\u00f6rte,", "tokens": ["Als", "Deutschland", "sich", "im", "Frie\u00b7den", "ganz", "zer\u00b7st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da war das letzte deutsche Blut geflossen.", "tokens": ["Da", "war", "das", "letz\u00b7te", "deut\u00b7sche", "Blut", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch da gab's Stimmen, einen kaum der h\u00f6rte.", "tokens": ["Noch", "da", "gab's", "Stim\u00b7men", ",", "ei\u00b7nen", "kaum", "der", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "$,", "ART", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von F\u00fcrsten Recht, bei B\u00fcrgern edle Sitte,", "tokens": ["Von", "F\u00fcrs\u00b7ten", "Recht", ",", "bei", "B\u00fcr\u00b7gern", "ed\u00b7le", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War wen'ger Ziel, seit sich das Reich verkehrte.", "tokens": ["War", "wen'\u00b7ger", "Ziel", ",", "seit", "sich", "das", "Reich", "ver\u00b7kehr\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was m\u00f6gen einzle, fehlt die gro\u00dfe Mitte?", "tokens": ["Was", "m\u00f6\u00b7gen", "einz\u00b7le", ",", "fehlt", "die", "gro\u00b7\u00dfe", "Mit\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Taten hat uns Gottes Will' umschr\u00e4nkt,", "tokens": ["In", "Ta\u00b7ten", "hat", "uns", "Got\u00b7tes", "Will'", "um\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kraft der Kunst gew\u00e4hrt er sonder Bitte.", "tokens": ["Die", "Kraft", "der", "Kunst", "ge\u00b7w\u00e4hrt", "er", "son\u00b7der", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schon fr\u00fch hat uns Gelehrsamkeit getr\u00e4nkt", "tokens": ["Schon", "fr\u00fch", "hat", "uns", "Ge\u00b7lehr\u00b7sam\u00b7keit", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit alter V\u00f6lker Mark. Zur Geistessonne", "tokens": ["Mit", "al\u00b7ter", "V\u00f6l\u00b7ker", "Mark", ".", "Zur", "Geis\u00b7tes\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird Kraft und Kunst durch stillen Bund gelenkt.", "tokens": ["Wird", "Kraft", "und", "Kunst", "durch", "stil\u00b7len", "Bund", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Aus s\u00fc\u00dfer Poesie quillt ew'ge Wonne,", "tokens": ["Aus", "s\u00fc\u00b7\u00dfer", "Poe\u00b7sie", "quillt", "ew'\u00b7ge", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch Religion entz\u00fcnd't sich lichte G\u00fcte,", "tokens": ["Durch", "Re\u00b7li\u00b7gi\u00b7on", "ent\u00b7z\u00fcnd't", "sich", "lich\u00b7te", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Denker ist Natur der Lebensbronne.", "tokens": ["Dem", "Den\u00b7ker", "ist", "Na\u00b7tur", "der", "Le\u00b7bens\u00b7bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Was Hellas schlau ersann, was Indien bl\u00fchte,", "tokens": ["Was", "Hel\u00b7las", "schlau", "er\u00b7sann", ",", "was", "In\u00b7di\u00b7en", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "German'scher M\u00e4nner Lied wird's neu entfalten,", "tokens": ["Ger\u00b7man'\u00b7scher", "M\u00e4n\u00b7ner", "Lied", "wird's", "neu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie zornig blinder P\u00f6bel gegenw\u00fcte.", "tokens": ["Wie", "zor\u00b7nig", "blin\u00b7der", "P\u00f6\u00b7bel", "ge\u00b7gen\u00b7w\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich sagte zweimal Uns. Die Worte galten", "tokens": ["Ich", "sag\u00b7te", "zwei\u00b7mal", "Uns", ".", "Die", "Wor\u00b7te", "gal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Heldenk\u00fcnstlern, die sich selber nennen;", "tokens": ["Den", "Hel\u00b7den\u00b7k\u00fcnst\u00b7lern", ",", "die", "sich", "sel\u00b7ber", "nen\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn nimmer kann solch Feur wie dies erkalten.", "tokens": ["Denn", "nim\u00b7mer", "kann", "solch", "Feur", "wie", "dies", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIAT", "NN", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Nachwelt wird sie glorreich anerkennen.", "tokens": ["Die", "Nach\u00b7welt", "wird", "sie", "glor\u00b7reich", "an\u00b7er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer will, sei mit im Uns. Die sind versto\u00dfen,", "tokens": ["Wer", "will", ",", "sei", "mit", "im", "Uns", ".", "Die", "sind", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VAFIN", "APPR", "APPRART", "NN", "$.", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die nach dem Nichts, von Gott verlassen, rennen,", "tokens": ["Die", "nach", "dem", "Nichts", ",", "von", "Gott", "ver\u00b7las\u00b7sen", ",", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "An Religion und Dichtkunst sich erbo\u00dfen,", "tokens": ["An", "Re\u00b7li\u00b7gi\u00b7on", "und", "Dicht\u00b7kunst", "sich", "er\u00b7bo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von der Natur Mysterien nichts nicht wissen,", "tokens": ["Von", "der", "Na\u00b7tur", "Mys\u00b7te\u00b7ri\u00b7en", "nichts", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Zu sich in Kot das Heil'ge niedersto\u00dfen.", "tokens": ["Zu", "sich", "in", "Kot", "das", "Heil'\u00b7ge", "nie\u00b7der\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NE", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Solch S\u00fcndenvolk, die leicht schier von Gewissen,", "tokens": ["Solch", "S\u00fcn\u00b7den\u00b7volk", ",", "die", "leicht", "schier", "von", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen schlaff, von Sinnen stumpf, nicht merken,", "tokens": ["Im", "Her\u00b7zen", "schlaff", ",", "von", "Sin\u00b7nen", "stumpf", ",", "nicht", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich der Nacht ein Weltall neu entrissen,", "tokens": ["Da\u00df", "sich", "der", "Nacht", "ein", "Wel\u00b7tall", "neu", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mag ewig Gott im Totenschlaf best\u00e4rken,", "tokens": ["Mag", "e\u00b7wig", "Gott", "im", "To\u00b7ten\u00b7schlaf", "be\u00b7st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis kraft des j\u00fcngsten Tags zuletzt sie wachen,", "tokens": ["Bis", "kraft", "des", "j\u00fcng\u00b7sten", "Tags", "zu\u00b7letzt", "sie", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' sie zergehn samt ihren nicht'gen Werken.", "tokens": ["Eh'", "sie", "zer\u00b7gehn", "samt", "ih\u00b7ren", "nicht'\u00b7gen", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Wer Feuer, Wasser, Luft, die ersten Sachen", "tokens": ["Wer", "Feu\u00b7er", ",", "Was\u00b7ser", ",", "Luft", ",", "die", "ers\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus tiefer Seele liebt, kann's nie mehr lassen,", "tokens": ["Aus", "tie\u00b7fer", "See\u00b7le", "liebt", ",", "kann's", "nie", "mehr", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schw\u00f6mm' auch allein auf weitem Meer sein Nachen.", "tokens": ["Schw\u00f6mm'", "auch", "al\u00b7lein", "auf", "wei\u00b7tem", "Meer", "sein", "Na\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {