{"textgrid.poem.63112": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rausche, Laub, am braunen Hang,", "tokens": ["Rau\u00b7sche", ",", "Laub", ",", "am", "brau\u00b7nen", "Hang", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rausche deine bunten Bl\u00e4tter", "tokens": ["Rau\u00b7sche", "dei\u00b7ne", "bun\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir hernieder in den Gang.", "tokens": ["Mir", "her\u00b7nie\u00b7der", "in", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst fiel eines wie ein Tropfen", "tokens": ["Erst", "fiel", "ei\u00b7nes", "wie", "ein", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferner Wetter.", "tokens": ["Fer\u00b7ner", "Wet\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nun sinds viele, die wie Schmetter-", "tokens": ["Nun", "sinds", "vie\u00b7le", ",", "die", "wie", "Schmet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PRELS", "KOKOM", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "linge tot den Boden klopfen.", "tokens": ["lin\u00b7ge", "tot", "den", "Bo\u00b7den", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und vom Baum sah ich ein Blatt sich falten.", "tokens": ["Und", "vom", "Baum", "sah", "ich", "ein", "Blatt", "sich", "fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ist es eine Bl\u00fcte? Farbentrunken", "tokens": ["Ist", "es", "ei\u00b7ne", "Bl\u00fc\u00b7te", "?", "Far\u00b7ben\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist sie schon auf mich herabgesunken,", "tokens": ["Ist", "sie", "schon", "auf", "mich", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die H\u00e4nde", "tokens": ["Und", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Halten", "tokens": ["Hal\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Eines Jahres Sonnenbr\u00e4nde.", "tokens": ["Ei\u00b7nes", "Jah\u00b7res", "Son\u00b7nen\u00b7br\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rot und gl\u00fchend zuckte es im Teller", "tokens": ["Rot", "und", "gl\u00fc\u00b7hend", "zuck\u00b7te", "es", "im", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meiner Hand, auf der die Blicke brannten,", "tokens": ["Mei\u00b7ner", "Hand", ",", "auf", "der", "die", "Bli\u00b7cke", "brann\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hrend meine wehen Sohlen schneller", "tokens": ["W\u00e4h\u00b7rend", "mei\u00b7ne", "we\u00b7hen", "Soh\u00b7len", "schnel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch das tote Laub am Boden rannten.", "tokens": ["Durch", "das", "to\u00b7te", "Laub", "am", "Bo\u00b7den", "rann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}