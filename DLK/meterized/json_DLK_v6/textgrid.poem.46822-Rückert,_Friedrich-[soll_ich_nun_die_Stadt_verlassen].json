{"textgrid.poem.46822": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[soll ich nun die Stadt verlassen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich nun die Stadt verlassen,", "tokens": ["Soll", "ich", "nun", "die", "Stadt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirds nicht schwer mir fallen,", "tokens": ["Wirds", "nicht", "schwer", "mir", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meinen Abschied kurz zu fassen", "tokens": ["Mei\u00b7nen", "Ab\u00b7schied", "kurz", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Lebenden Allen.", "tokens": ["Von", "den", "Le\u00b7ben\u00b7den", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Aber schwer wird mir das Scheiden", "tokens": ["A\u00b7ber", "schwer", "wird", "mir", "das", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von zwei lieben Todten;", "tokens": ["Von", "zwei", "lie\u00b7ben", "Tod\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Noch von fernher sei den beiden", "tokens": ["Noch", "von", "fern\u00b7her", "sei", "den", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "VAFIN", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebewohl geboten.", "tokens": ["Le\u00b7be\u00b7wohl", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wird ein Lebewohl geboten", "tokens": ["Wird", "ein", "Le\u00b7be\u00b7wohl", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch den Todten eben?", "tokens": ["Auch", "den", "Tod\u00b7ten", "e\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wohl ein Lebewohl den Todten,", "tokens": ["Wohl", "ein", "Le\u00b7be\u00b7wohl", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die allein mir leben.", "tokens": ["Die", "al\u00b7lein", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und o Stadt, wo sie mir leben,", "tokens": ["Und", "o", "Stadt", ",", "wo", "sie", "mir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir sei auch geboten", "tokens": ["Dir", "sei", "auch", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lebewohl; dir \u00fcbergeben", "tokens": ["Le\u00b7be\u00b7wohl", ";", "dir", "\u00fc\u00b7ber\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich meine Todten.", "tokens": ["Hab'", "ich", "mei\u00b7ne", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}