{"textgrid.poem.62026": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[fr\u00fchlingsl\u00fcfte wehen leise]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchlingsl\u00fcfte wehen leise,", "tokens": ["Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "we\u00b7hen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traurig ist das Herz,", "tokens": ["Trau\u00b7rig", "ist", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In der unbewu\u00dften Weise,", "tokens": ["In", "der", "un\u00b7be\u00b7wu\u00df\u00b7ten", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch verwandt dem Schmerz.", "tokens": ["Doch", "ver\u00b7wandt", "dem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bunte Schmetterlinge fliegen", "tokens": ["Bun\u00b7te", "Schmet\u00b7ter\u00b7lin\u00b7ge", "flie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den Bl\u00fcten auf,", "tokens": ["Zu", "den", "Bl\u00fc\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "N\u00e4chst der Bl\u00fcte kriecht das W\u00fcrmlein,", "tokens": ["N\u00e4chst", "der", "Bl\u00fc\u00b7te", "kriecht", "das", "W\u00fcrm\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauert schon darauf! \u2013", "tokens": ["Lau\u00b7ert", "schon", "da\u00b7rauf", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PAV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ist auch sch\u00f6n die Au\u00dfenseite, \u2013", "tokens": ["Ist", "auch", "sch\u00f6n", "die", "Au\u00b7\u00dfen\u00b7sei\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Inn'res ist nicht \u2013 s\u00fc\u00df:", "tokens": ["Inn'\u00b7res", "ist", "nicht", "\u2013", "s\u00fc\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "$(", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In der Welten L\u00e4ng' und Breite", "tokens": ["In", "der", "Wel\u00b7ten", "L\u00e4ng'", "und", "Brei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbitter\u00ab \u2013 man es hie\u00df!", "tokens": ["\u00bb", "bit\u00b7ter", "\u00ab", "\u2013", "man", "es", "hie\u00df", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$(", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}