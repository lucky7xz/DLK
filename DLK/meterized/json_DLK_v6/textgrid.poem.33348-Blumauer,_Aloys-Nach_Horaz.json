{"textgrid.poem.33348": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Nach Horaz", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell \u00fcber's Sterngewimmel", "tokens": ["Hell", "\u00fc\u00b7ber's", "Stern\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergo\u00df sich Lunens Schein,", "tokens": ["Er\u00b7go\u00df", "sich", "Lu\u00b7nens", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00fcllte Erd' und Himmel", "tokens": ["Und", "h\u00fcll\u00b7te", "Erd'", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In stille Feyer ein;", "tokens": ["In", "stil\u00b7le", "Fe\u00b7yer", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als du von Wonneweben", "tokens": ["Als", "du", "von", "Won\u00b7ne\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchschauert, mich umfingst,", "tokens": ["Durch\u00b7schau\u00b7ert", ",", "mich", "um\u00b7fingst", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fest an mir, wie Reben", "tokens": ["Und", "fest", "an", "mir", ",", "wie", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Am Ulmenstabe, hingst.", "tokens": ["Am", "Ul\u00b7men\u00b7sta\u00b7be", ",", "hingst", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da schwur im Angesichte", "tokens": ["Da", "schwur", "im", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der heiligen Natur", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Mund mir armen Wichte", "tokens": ["Dein", "Mund", "mir", "ar\u00b7men", "Wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den bald verge\u00df'nen Schwur:", "tokens": ["Den", "bald", "ver\u00b7ge\u00df'\u00b7nen", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir treu zu bleiben immer,", "tokens": ["Mir", "treu", "zu", "blei\u00b7ben", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mein, einzig mein zu sein,", "tokens": ["Mein", ",", "ein\u00b7zig", "mein", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "ADJD", "PPOSAT", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang der Sterne Schimmer", "tokens": ["So", "lang", "der", "Ster\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich birgt vor Lunens Schein.", "tokens": ["Sich", "birgt", "vor", "Lu\u00b7nens", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wi\u00df', an deiner Th\u00fcre", "tokens": ["Doch", "wi\u00df'", ",", "an", "dei\u00b7ner", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Belauschte dich mein Ohr:", "tokens": ["Be\u00b7lauschte", "dich", "mein", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Weit heiligere Schw\u00fcre", "tokens": ["Weit", "hei\u00b7li\u00b7ge\u00b7re", "Schw\u00fc\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwurst du Kleanthen vor,", "tokens": ["Schwurst", "du", "Kle\u00b7an\u00b7then", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und gabst in deinem Bette", "tokens": ["Und", "gabst", "in", "dei\u00b7nem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm eine Nacht, die mir,", "tokens": ["Ihm", "ei\u00b7ne", "Nacht", ",", "die", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir zugeh\u00f6ret h\u00e4tte;", "tokens": ["Mir", "zu\u00b7ge\u00b7h\u00f6\u00b7ret", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O merke, merk' es dir!", "tokens": ["O", "mer\u00b7ke", ",", "merk'", "es", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Bald sollst du's bitter f\u00fchlen;", "tokens": ["Bald", "sollst", "du's", "bit\u00b7ter", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es soll dein Flattersinn", "tokens": ["Es", "soll", "dein", "Flat\u00b7ter\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht l\u00e4nger mit mir spielen,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "mit", "mir", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So war ein Mann ich bin!", "tokens": ["So", "war", "ein", "Mann", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und dringt einmal die Galle", "tokens": ["Und", "dringt", "ein\u00b7mal", "die", "Gal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir recht durch Mark und Bein", "tokens": ["Mir", "recht", "durch", "Mark", "und", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So soll dein Zauber alle", "tokens": ["So", "soll", "dein", "Zau\u00b7ber", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An mir verloren sein.", "tokens": ["An", "mir", "ver\u00b7lo\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du aber hoch im Gl\u00fccke", "tokens": ["Du", "a\u00b7ber", "hoch", "im", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stolzirender Rival,", "tokens": ["Stol\u00b7zi\u00b7ren\u00b7der", "Ri\u00b7val", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der mir durch List und T\u00fccke", "tokens": ["Der", "mir", "durch", "List", "und", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ne\u00e4rens Liebe stahl:", "tokens": ["Ne\u00e4\u00b7rens", "Lie\u00b7be", "stahl", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Sei tapfer, wie ein Ritter,", "tokens": ["Sei", "tap\u00b7fer", ",", "wie", "ein", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und reizend, wie Adon,", "tokens": ["Und", "rei\u00b7zend", ",", "wie", "A\u00b7don", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' Ehr' und Gl\u00fcckesg\u00fcter,", "tokens": ["Hab'", "Ehr'", "und", "Gl\u00fc\u00b7ckes\u00b7g\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sey eines F\u00fcrsten Sohn!", "tokens": ["Sey", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Was wett' ich, stolzer Ritter,", "tokens": ["Was", "wett'", "ich", ",", "stol\u00b7zer", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir bleibt Ne\u00e4re nicht? \u2013", "tokens": ["Dir", "bleibt", "Ne\u00e4\u00b7re", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und raubt sie dir ein Dritter,", "tokens": ["Und", "raubt", "sie", "dir", "ein", "Drit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lach' ich dir in's Gesicht.", "tokens": ["Lach'", "ich", "dir", "in's", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}}}}