{"textgrid.poem.41654": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich, sch\u00f6ne Liebe, sch\u00f6ne S\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, sch\u00f6ne Liebe, sch\u00f6ne S\u00fc\u00dfe,", "tokens": ["Dich", ",", "sch\u00f6\u00b7ne", "Lie\u00b7be", ",", "sch\u00f6\u00b7ne", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich Engel, der zum Licht mich weiht,", "tokens": ["Dich", "En\u00b7gel", ",", "der", "zum", "Licht", "mich", "weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unsterbliches Idol, dich gr\u00fc\u00dfe", "tokens": ["U\u00b7nsterb\u00b7li\u00b7ches", "I\u00b7dol", ",", "dich", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Ich gl\u00fchend in Unsterblichkeit.", "tokens": ["Ich", "gl\u00fc\u00b7hend", "in", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Du flutest durch mein ganzes Leben", "tokens": ["Du", "flu\u00b7test", "durch", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich einem Seewind, herb und rein,", "tokens": ["Gleich", "ei\u00b7nem", "See\u00b7wind", ",", "herb", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meiner Seele bangem Streben", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "ban\u00b7gem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00f6\u00dft du Begehr nach Ewgem ein.", "tokens": ["Fl\u00f6\u00dft", "du", "Be\u00b7gehr", "nach", "Ew\u00b7gem", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Stets frischer Wohlgeruch, der bl\u00fchend", "tokens": ["Stets", "fri\u00b7scher", "Wohl\u00b7ge\u00b7ruch", ",", "der", "bl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lieb Gemach in D\u00fcfte taucht,", "tokens": ["Ein", "lieb", "Ge\u00b7mach", "in", "D\u00fcf\u00b7te", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verge\u00dfner Weihrauch, der ergl\u00fchend", "tokens": ["Ver\u00b7ge\u00df\u00b7ner", "Weih\u00b7rauch", ",", "der", "er\u00b7gl\u00fc\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geheim in tiefer Nacht verhaucht!", "tokens": ["Ge\u00b7heim", "in", "tie\u00b7fer", "Nacht", "ver\u00b7haucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie soll ich nennen dich in Wahrheit,", "tokens": ["Wie", "soll", "ich", "nen\u00b7nen", "dich", "in", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Demantenreine Liebesglut,", "tokens": ["De\u00b7man\u00b7ten\u00b7rei\u00b7ne", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in der Seele ewger Klarheit,", "tokens": ["Die", "in", "der", "See\u00b7le", "ew\u00b7ger", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ambrakorn, verborgen ruht?", "tokens": ["Ein", "A\u00b7mbra\u00b7korn", ",", "ver\u00b7bor\u00b7gen", "ruht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dich, sch\u00f6ne Gute, sch\u00f6ne S\u00fc\u00dfe,", "tokens": ["Dich", ",", "sch\u00f6\u00b7ne", "Gu\u00b7te", ",", "sch\u00f6\u00b7ne", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kraft und Freude mir verleiht,", "tokens": ["Die", "Kraft", "und", "Freu\u00b7de", "mir", "ver\u00b7leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unsterbliches Idol, dich gr\u00fc\u00dfe", "tokens": ["U\u00b7nsterb\u00b7li\u00b7ches", "I\u00b7dol", ",", "dich", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Ich gl\u00fchend in Unsterblichkeit.", "tokens": ["Ich", "gl\u00fc\u00b7hend", "in", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}