{"dta.poem.5226": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.71", "sv:0.28"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Was fehlt doch dem allm\u00e4chtgen G\u00f6tterkinde,", "tokens": ["Was", "fehlt", "doch", "dem", "all\u00b7m\u00e4cht\u00b7gen", "G\u00f6t\u00b7ter\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das alle Welt zu drohen scheint,", "tokens": ["Das", "al\u00b7le", "Welt", "zu", "dro\u00b7hen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Cytherens Sohn? Was that er denn f\u00fcr S\u00fcnde?", "tokens": ["Cy\u00b7the\u00b7rens", "Sohn", "?", "Was", "that", "er", "denn", "f\u00fcr", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Er ward gestraft, und weint?", "tokens": ["Er", "ward", "ge\u00b7straft", ",", "und", "weint", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Thr\u00e4ne redet von der rechten Backen", "tokens": ["Die", "Thr\u00e4\u00b7ne", "re\u00b7det", "von", "der", "rech\u00b7ten", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In stummer Sprache, Schmerz herab!", "tokens": ["In", "stum\u00b7mer", "Spra\u00b7che", ",", "Schmerz", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Pfeilen leer h\u00e4ngt ihm der K\u00f6cher auf dem Nacken;", "tokens": ["Von", "Pfei\u00b7len", "leer", "h\u00e4ngt", "ihm", "der", "K\u00f6\u00b7cher", "auf", "dem", "Na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die grosse Venus gab", "tokens": ["Die", "gros\u00b7se", "Ve\u00b7nus", "gab"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihm einen Kelch voll Mutterzorn zu schmecken:", "tokens": ["Ihm", "ei\u00b7nen", "Kelch", "voll", "Mut\u00b7ter\u00b7zorn", "zu", "schme\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie nahm ihm Pfeile. Was empfand", "tokens": ["Sie", "nahm", "ihm", "Pfei\u00b7le", ".", "Was", "emp\u00b7fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Stolz? Wie schaamvoll steht er sich das Auge decken,", "tokens": ["Sein", "Stolz", "?", "Wie", "schaam\u00b7voll", "steht", "er", "sich", "das", "Au\u00b7ge", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit seiner linken Hand?", "tokens": ["Mit", "sei\u00b7ner", "lin\u00b7ken", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit Bitterkeit schilt sie auf ihn hernieder,", "tokens": ["Mit", "Bit\u00b7ter\u00b7keit", "schilt", "sie", "auf", "ihn", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt seine nasse Klagen nicht.", "tokens": ["F\u00fchlt", "sei\u00b7ne", "nas\u00b7se", "Kla\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! keinen Pfeil giebt sie dem armen Knaben wieder?", "tokens": ["Ach", "!", "kei\u00b7nen", "Pfeil", "giebt", "sie", "dem", "ar\u00b7men", "Kna\u00b7ben", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein, Spott ins Angesicht!", "tokens": ["Nein", ",", "Spott", "ins", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er nennt ihr seines Bogens gr\u00f6\u00dfte Thaten,", "tokens": ["Er", "nennt", "ihr", "sei\u00b7nes", "Bo\u00b7gens", "gr\u00f6\u00df\u00b7te", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Helden, die er zwang,", "tokens": ["Und", "al\u00b7le", "Hel\u00b7den", ",", "die", "er", "zwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie ihr opferten, und ihre H\u00fclfe baten,", "tokens": ["Da\u00df", "sie", "ihr", "op\u00b7fer\u00b7ten", ",", "und", "ih\u00b7re", "H\u00fcl\u00b7fe", "ba\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wann sie sein Pfeil durchdrang!", "tokens": ["Wann", "sie", "sein", "Pfeil", "durch\u00b7drang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, spricht sie, schweig, du nutzenloser Knabe,", "tokens": ["Ja", ",", "spricht", "sie", ",", "schweig", ",", "du", "nut\u00b7zen\u00b7lo\u00b7ser", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hilft mir Hercul, was Achill?", "tokens": ["Was", "hilft", "mir", "Her\u00b7cul", ",", "was", "A\u00b7chill", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NE", "$,", "PRELS", "NE", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wann ich den Helden nicht zu meinen F\u00fcssen habe,", "tokens": ["Wann", "ich", "den", "Hel\u00b7den", "nicht", "zu", "mei\u00b7nen", "F\u00fcs\u00b7sen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nicht empfinden will?", "tokens": ["Der", "nicht", "emp\u00b7fin\u00b7den", "will", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur Heldenlorbeer will er sich erfechten,", "tokens": ["Nur", "Hel\u00b7den\u00b7lor\u00b7beer", "will", "er", "sich", "er\u00b7fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Mars folgt er und dem Apoll,", "tokens": ["Dem", "Mars", "folgt", "er", "und", "dem", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit vereinter Hand ihm eine Crone flechten,", "tokens": ["Die", "mit", "ver\u00b7ein\u00b7ter", "Hand", "ihm", "ei\u00b7ne", "Cro\u00b7ne", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ewig strahlen soll!", "tokens": ["Die", "e\u00b7wig", "strah\u00b7len", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Soll meiner Macht ein Sohn der Erde lachen?", "tokens": ["Soll", "mei\u00b7ner", "Macht", "ein", "Sohn", "der", "Er\u00b7de", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hie\u00df den Donnergott zum Schwan,", "tokens": ["Ich", "hie\u00df", "den", "Don\u00b7ner\u00b7gott", "zum", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Stier, ich hie\u00df ihn sich zur goldnen Wolke machen,", "tokens": ["Zum", "Stier", ",", "ich", "hie\u00df", "ihn", "sich", "zur", "gold\u00b7nen", "Wol\u00b7ke", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir v\u00f6llig unterthan.", "tokens": ["Mir", "v\u00f6l\u00b7lig", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Geh\u2019 Bube, fern von meinen Augen eile!", "tokens": ["Geh'", "Bu\u00b7be", ",", "fern", "von", "mei\u00b7nen", "Au\u00b7gen", "ei\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Friedrich mehr, als Jupiter?", "tokens": ["Ist", "Fried\u00b7rich", "mehr", ",", "als", "Ju\u00b7pi\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ja \u2014 schluchzte Amor, gieb mir \u2014 Mutter \u2014", "tokens": ["Ja", "schluchz\u00b7te", "A\u00b7mor", ",", "gieb", "mir", "Mut\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "NE", "$,", "VVIMP", "PPER", "$(", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "meine Pfeile!", "tokens": ["mei\u00b7ne", "Pfei\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mehr als ein Gott ist er!", "tokens": ["Mehr", "als", "ein", "Gott", "ist", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "++-+--", "measure": "unknown.measure.tri"}}}}}