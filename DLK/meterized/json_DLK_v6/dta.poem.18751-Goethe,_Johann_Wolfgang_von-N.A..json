{"dta.poem.18751": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wer auf die Welt kommt baut ein neues Haus,", "tokens": ["Wer", "auf", "die", "Welt", "kommt", "baut", "ein", "neu\u00b7es", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er geht und l\u00e4sst es einem zweyten,", "tokens": ["Er", "geht", "und", "l\u00e4sst", "es", "ei\u00b7nem", "zwey\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der wird sich\u2019s anders zubereiten", "tokens": ["Der", "wird", "sich's", "an\u00b7ders", "zu\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemand baut es aus.", "tokens": ["Und", "nie\u00b7mand", "baut", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}