{"textgrid.poem.32787": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ritter Richard", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ritter Richard sah einmal", "tokens": ["Der", "Rit\u00b7ter", "Ric\u00b7hard", "sah", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fr\u00e4ulein Adelgund,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "A\u00b7del\u00b7gund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und herzlich that er seine Qual", "tokens": ["Und", "herz\u00b7lich", "that", "er", "sei\u00b7ne", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr unter Thr\u00e4nen kund;", "tokens": ["Ihr", "un\u00b7ter", "Thr\u00e4\u00b7nen", "kund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wurde bald erh\u00f6rt. Es sprach", "tokens": ["Und", "wur\u00b7de", "bald", "er\u00b7h\u00f6rt", ".", "Es", "sprach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lieb' aus ihrem Blick,", "tokens": ["Die", "Lieb'", "aus", "ih\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sahen sich an jedem Tag,", "tokens": ["Sie", "sa\u00b7hen", "sich", "an", "je\u00b7dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und t\u00e4glich wuchs ihr Gl\u00fcck.", "tokens": ["Und", "t\u00e4g\u00b7lich", "wuchs", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch schneller schwand es, als der Strahl", "tokens": ["Doch", "schnel\u00b7ler", "schwand", "es", ",", "als", "der", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom falben Abendlicht;", "tokens": ["Vom", "fal\u00b7ben", "A\u00b7bend\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hienieden dau'rt kein Gl\u00fcck, zumal", "tokens": ["Hien\u00b7ie\u00b7den", "dau'rt", "kein", "Gl\u00fcck", ",", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gl\u00fcck der Liebe nicht.", "tokens": ["Das", "Gl\u00fcck", "der", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er soll in Krieg, er wappnet sich,", "tokens": ["Er", "soll", "in", "Krieg", ",", "er", "wapp\u00b7net", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt ihr den Scheideku\u00df;", "tokens": ["Giebt", "ihr", "den", "Schei\u00b7de\u00b7ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie umarmt ihn inniglich", "tokens": ["Und", "sie", "um\u00b7armt", "ihn", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Thr\u00e4nengu\u00df.", "tokens": ["Mit", "ei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7gu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch trocknen, wie ein jeder wei\u00df,", "tokens": ["Doch", "trock\u00b7nen", ",", "wie", "ein", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4dchen Thr\u00e4nen bald;", "tokens": ["Der", "M\u00e4d\u00b7chen", "Thr\u00e4\u00b7nen", "bald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Busen, eben noch so hei\u00df,", "tokens": ["Ihr", "Bu\u00b7sen", ",", "e\u00b7ben", "noch", "so", "hei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward augenblicklich kalt.", "tokens": ["Ward", "au\u00b7gen\u00b7blick\u00b7lich", "kalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein sch\u00f6ngeputzter Edelmann,", "tokens": ["Ein", "sch\u00f6n\u00b7ge\u00b7putz\u00b7ter", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Robert jung und fein,", "tokens": ["Herr", "Ro\u00b7bert", "jung", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah sie mit Liebesblicken an,", "tokens": ["Sah", "sie", "mit", "Lie\u00b7bes\u00b7bli\u00b7cken", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nahm sie j\u00e4hlings ein.", "tokens": ["Und", "nahm", "sie", "j\u00e4h\u00b7lings", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zwar hatt' er, wie ihr Richard, nicht", "tokens": ["Zwar", "hatt'", "er", ",", "wie", "ihr", "Ric\u00b7hard", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "NE", "$,", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herz ohn' allen Trug,", "tokens": ["Ein", "Herz", "ohn'", "al\u00b7len", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch lieblich war sein Angesicht,", "tokens": ["Doch", "lieb\u00b7lich", "war", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist M\u00e4dchen g'nug.", "tokens": ["Und", "das", "ist", "M\u00e4d\u00b7chen", "g'\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Bald, schrieb ihr Richard, bin ich dein,", "tokens": ["Bald", ",", "schrieb", "ihr", "Ric\u00b7hard", ",", "bin", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "NE", "$,", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich komm', o Teure, schon;", "tokens": ["Ich", "komm'", ",", "o", "Teu\u00b7re", ",", "schon", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "FM", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie aber las, und lachte sein,", "tokens": ["Sie", "a\u00b7ber", "las", ",", "und", "lach\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KON", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach ihm bittern Hohn;", "tokens": ["Und", "sprach", "ihm", "bit\u00b7tern", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und flog zu ihrem Robert hin,", "tokens": ["Und", "flog", "zu", "ih\u00b7rem", "Ro\u00b7bert", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach: Bin ich dir wert,", "tokens": ["Und", "sprach", ":", "Bin", "ich", "dir", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00df die Trauung uns vollziehn,", "tokens": ["So", "la\u00df", "die", "Trau\u00b7ung", "uns", "voll\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' uns ein andrer st\u00f6rt.", "tokens": ["Eh'", "uns", "ein", "an\u00b7drer", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Kein Augenblick ward da gespart,", "tokens": ["Kein", "Au\u00b7gen\u00b7blick", "ward", "da", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man fuhr hinaus aufs Land,", "tokens": ["Man", "fuhr", "hin\u00b7aus", "aufs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gleich der zweite Morgen ward", "tokens": ["Und", "gleich", "der", "zwei\u00b7te", "Mor\u00b7gen", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Trauungstag ernannt.", "tokens": ["Zum", "Trau\u00b7ungs\u00b7tag", "er\u00b7nannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Indessen k\u00f6mmt, mit Ruhm bekr\u00e4nzt,", "tokens": ["In\u00b7des\u00b7sen", "k\u00f6mmt", ",", "mit", "Ruhm", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter Richard an;", "tokens": ["Der", "Rit\u00b7ter", "Ric\u00b7hard", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Busen pocht, sein Auge gl\u00e4nzt,", "tokens": ["Sein", "Bu\u00b7sen", "pocht", ",", "sein", "Au\u00b7ge", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fr\u00e4ulein zu empfahn.", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "zu", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ach, was er da vernimmt! Die ist", "tokens": ["Ach", ",", "was", "er", "da", "ver\u00b7nimmt", "!", "Die", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWS", "PPER", "ADV", "VVFIN", "$.", "PDS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ritter Roberts Braut,", "tokens": ["Des", "Rit\u00b7ter", "Ro\u00b7berts", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, eh' der zweite Tag verflie\u00dft,", "tokens": ["Und", ",", "eh'", "der", "zwei\u00b7te", "Tag", "ver\u00b7flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sie ihm angetraut.", "tokens": ["Wird", "sie", "ihm", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er flucht und betet, springt aufs Ro\u00df,", "tokens": ["Er", "flucht", "und", "be\u00b7tet", ",", "springt", "aufs", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rennt im wilden Trab", "tokens": ["Und", "rennt", "im", "wil\u00b7den", "Trab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor Fr\u00e4ulein Adelgundens Schlo\u00df,", "tokens": ["Vor", "Fr\u00e4u\u00b7lein", "A\u00b7del\u00b7gun\u00b7dens", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hastig springt er ab;", "tokens": ["Und", "has\u00b7tig", "springt", "er", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und will im ersten Augenblick", "tokens": ["Und", "will", "im", "ers\u00b7ten", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die falsche Dirne sehn:", "tokens": ["Die", "fal\u00b7sche", "Dir\u00b7ne", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00f6hnisch weist man ihn zur\u00fcck,", "tokens": ["Doch", "h\u00f6h\u00b7nisch", "weist", "man", "ihn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft ihn staunend stehn.", "tokens": ["Und", "l\u00e4\u00dft", "ihn", "stau\u00b7nend", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Gott! ruft er rasend, welch ein Lohn!", "tokens": ["Gott", "!", "ruft", "er", "ra\u00b7send", ",", "welch", "ein", "Lohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "VVPP", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stampft, und knirscht, und lacht,", "tokens": ["Und", "stampft", ",", "und", "knirscht", ",", "und", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "