: "Zum Kirchhofthor hinaus,", "tokens": ["Zum", "Kirch\u00b7hof\u00b7thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jetzo noch ist dieser Ort", "tokens": ["Und", "jet\u00b7zo", "noch", "ist", "die\u00b7ser", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ganzen Land ein Graus.", "tokens": ["Dem", "gan\u00b7zen", "Land", "ein", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Um tiefe Mitternacht erscheint", "tokens": ["Um", "tie\u00b7fe", "Mit\u00b7ter\u00b7nacht", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fr\u00e4ulein hier im Flor,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "hier", "im", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ringt die bleichen H\u00e4nd' und weint,", "tokens": ["Und", "ringt", "die", "blei\u00b7chen", "H\u00e4nd'", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Robert steigt empor;", "tokens": ["Und", "Ro\u00b7bert", "steigt", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Und hinter ihm hebt wild und stumm", "tokens": ["Und", "hin\u00b7ter", "ihm", "hebt", "wild", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sich Richard aus dem Grab,", "tokens": ["Sich", "Ric\u00b7hard", "aus", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und beide sinken wiederum", "tokens": ["Und", "bei\u00b7de", "sin\u00b7ken", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Zeterschrei hinab.", "tokens": ["Mit", "Ze\u00b7ter\u00b7schrei", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}