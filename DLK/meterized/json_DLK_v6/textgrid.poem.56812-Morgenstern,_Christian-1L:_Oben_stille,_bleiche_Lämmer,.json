{"textgrid.poem.56812": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oben stille, bleiche L\u00e4mmer,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oben stille, bleiche L\u00e4mmer,", "tokens": ["O\u00b7ben", "stil\u00b7le", ",", "blei\u00b7che", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "drunter sonngoldschwere Z\u00fcge,", "tokens": ["drun\u00b7ter", "sonn\u00b7gold\u00b7schwe\u00b7re", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "trotz erh\u00f6hter Hellnis L\u00fcge", "tokens": ["trotz", "er\u00b7h\u00f6h\u00b7ter", "Hell\u00b7nis", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ohne Wehr dem nahen D\u00e4mmer.", "tokens": ["oh\u00b7ne", "Wehr", "dem", "na\u00b7hen", "D\u00e4m\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer doch den tr\u00fcben Wahn erfunden,", "tokens": ["Wer", "doch", "den", "tr\u00fc\u00b7ben", "Wahn", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df keine Seele gl\u00fccklich sei!", "tokens": ["da\u00df", "kei\u00b7ne", "See\u00b7le", "gl\u00fcck\u00b7lich", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war's, ich ", "tokens": ["Ich", "wa\u00b7r's", ",", "ich"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "von aller kleinen Tr\u00fcbsal frei.", "tokens": ["von", "al\u00b7ler", "klei\u00b7nen", "Tr\u00fcb\u00b7sal", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht wahrlich, da mit heisrem Atem", "tokens": ["Nicht", "wahr\u00b7lich", ",", "da", "mit", "heis\u00b7rem", "A\u00b7tem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Menge mir den Weg verbellt, \u2013", "tokens": ["die", "Men\u00b7ge", "mir", "den", "Weg", "ver\u00b7bellt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch nun Suleika sich und Hatem", "tokens": ["doch", "nun", "Sul\u00b7ei\u00b7ka", "sich", "und", "Ha\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NE", "PRF", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit goldnen Liedern mir gesellt.", "tokens": ["mit", "gold\u00b7nen", "Lie\u00b7dern", "mir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun da Natur mich treu umbreitet", "tokens": ["Nun", "da", "Na\u00b7tur", "mich", "treu", "um\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mit Tannen, hehr wie Hafis' Geist,", "tokens": ["mit", "Tan\u00b7nen", ",", "hehr", "wie", "Ha\u00b7fis'", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und dr\u00fcber mir die Blicke weitet,", "tokens": ["und", "dr\u00fc\u00b7ber", "mir", "die", "Bli\u00b7cke", "wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis, wo der letzte Fels vereist.", "tokens": ["bis", ",", "wo", "der", "letz\u00b7te", "Fels", "ver\u00b7eist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sollt ich da nicht ", "tokens": ["Wie", "sollt", "ich", "da", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "ein weltverleumderischer Tropf!", "tokens": ["ein", "welt\u00b7ver\u00b7leum\u00b7de\u00b7ri\u00b7scher", "Tropf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gern sie auch herunter b\u00f6gen", "tokens": ["So", "gern", "sie", "auch", "her\u00b7un\u00b7ter", "b\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den heitren, hochgemuten Kopf.", "tokens": ["den", "hei\u00b7tren", ",", "hoch\u00b7ge\u00b7mu\u00b7ten", "Kopf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}