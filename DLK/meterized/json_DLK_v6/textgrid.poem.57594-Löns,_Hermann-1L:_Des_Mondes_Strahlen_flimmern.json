{"textgrid.poem.57594": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Mondes Strahlen flimmern", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Mondes Strahlen flimmern", "tokens": ["Des", "Mon\u00b7des", "Strah\u00b7len", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Magisch \u00fcber den Teich,", "tokens": ["Ma\u00b7gisch", "\u00fc\u00b7ber", "den", "Teich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Nixenblumen schimmern", "tokens": ["Die", "Ni\u00b7xen\u00b7blu\u00b7men", "schim\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Romantisch geisterbleich;", "tokens": ["Ro\u00b7man\u00b7tisch", "geis\u00b7ter\u00b7bleich", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Es klingt der Nachtigall Weise", "tokens": ["Es", "klingt", "der", "Nach\u00b7ti\u00b7gall", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Voll tiefer Liebesglut,", "tokens": ["Voll", "tie\u00b7fer", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der wei\u00dfe Schwan zieht leise", "tokens": ["Der", "wei\u00b7\u00dfe", "Schwan", "zieht", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00dcber die schwarze Flut.", "tokens": ["\u00dc\u00b7ber", "die", "schwar\u00b7ze", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "So wie der Mond durchziehet", "tokens": ["So", "wie", "der", "Mond", "durch\u00b7zie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er langsam den dunklen Teich,", "tokens": ["Er", "lang\u00b7sam", "den", "dunk\u00b7len", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das wei\u00dfe Gefieder bl\u00fchet", "tokens": ["Das", "wei\u00b7\u00dfe", "Ge\u00b7fie\u00b7der", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie Weiberbr\u00fcste weich;", "tokens": ["Wie", "Wei\u00b7ber\u00b7br\u00fcs\u00b7te", "weich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Halses gef\u00e4llige Kr\u00fcmmung", "tokens": ["Des", "Hal\u00b7ses", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Kr\u00fcm\u00b7mung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Vollendet den Zauberbann,", "tokens": ["Voll\u00b7en\u00b7det", "den", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nur eins tr\u00fcbt mir die Stimmung,", "tokens": ["Nur", "eins", "tr\u00fcbt", "mir", "die", "Stim\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df er nicht fliegen kann.", "tokens": ["Da\u00df", "er", "nicht", "flie\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah am Ostseestrande", "tokens": ["Ich", "sah", "am", "Ost\u00b7see\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wilden Schw\u00e4ne ziehn,", "tokens": ["Die", "wil\u00b7den", "Schw\u00e4\u00b7ne", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah nach dem Tropenlande", "tokens": ["Sah", "nach", "dem", "Tro\u00b7pen\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Weithinklaft'rer fliehn,", "tokens": ["Die", "Weit\u00b7hin\u00b7klaft'\u00b7rer", "fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah ihre Schwingen sich dehnen", "tokens": ["Ich", "sah", "ih\u00b7re", "Schwin\u00b7gen", "sich", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Im Abendsonnenlicht \u2013", "tokens": ["Im", "A\u00b7bend\u00b7son\u00b7nen\u00b7licht", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dir schnitt man Band und Sehnen,", "tokens": ["Dir", "schnitt", "man", "Band", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Flugfreiheit kennst du nicht.", "tokens": ["Flug\u00b7frei\u00b7heit", "kennst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn dich treibt nach andern", "tokens": ["Und", "wenn", "dich", "treibt", "nach", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gew\u00e4ssern wilder Drang,", "tokens": ["Ge\u00b7w\u00e4s\u00b7sern", "wil\u00b7der", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wird ein trauriges Wandern,", "tokens": ["Das", "wird", "ein", "trau\u00b7ri\u00b7ges", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein trauriger Humpelgang;", "tokens": ["Ein", "trau\u00b7ri\u00b7ger", "Hum\u00b7pel\u00b7gang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das stolze Bild entweichet", "tokens": ["Das", "stol\u00b7ze", "Bild", "ent\u00b7wei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Armselig auf dem Land,", "tokens": ["Arm\u00b7se\u00b7lig", "auf", "dem", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Manch Dichter dir darin gleichet,", "tokens": ["Manch", "Dich\u00b7ter", "dir", "da\u00b7rin", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Von Vorurteil gebannt.", "tokens": ["Von", "Vor\u00b7ur\u00b7teil", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wen einmal gefesselt haben", "tokens": ["Wen", "ein\u00b7mal", "ge\u00b7fes\u00b7selt", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "VAINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "R\u00fccksicht und Strebertum,", "tokens": ["R\u00fcck\u00b7sicht", "und", "Stre\u00b7ber\u00b7tum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Sucht nach Ehrengaben,", "tokens": ["Die", "Sucht", "nach", "Eh\u00b7ren\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach hohlem Tagesruhm,", "tokens": ["Nach", "hoh\u00b7lem", "Ta\u00b7ges\u00b7ruhm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Niemehr wird der gesunden \u2013", "tokens": ["Nie\u00b7mehr", "wird", "der", "ge\u00b7sun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Nur wer die Fessel flieht,", "tokens": ["Nur", "wer", "die", "Fes\u00b7sel", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Flugfrei und ungebunden \u2013", "tokens": ["Flug\u00b7frei", "und", "un\u00b7ge\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$("], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der singt ein gro\u00dfes Lied.", "tokens": ["Der", "singt", "ein", "gro\u00b7\u00dfes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}