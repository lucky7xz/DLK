{"textgrid.poem.44840": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Pfisters Vergleichung von Uhland und R\u00fcckert", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00e4hnlich beide, zeigt er wohlgesinnt", "tokens": ["Wie", "\u00e4hn\u00b7lich", "bei\u00b7de", ",", "zeigt", "er", "wohl\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PIS", "$,", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gleichen Beifalls in die H\u00e4nde klopft er.", "tokens": ["Und", "glei\u00b7chen", "Bei\u00b7falls", "in", "die", "H\u00e4n\u00b7de", "klopft", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind auch \u00e4hnlich, wie zwei Adler sind,", "tokens": ["Sie", "sind", "auch", "\u00e4hn\u00b7lich", ",", "wie", "zwei", "Ad\u00b7ler", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOKOM", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein lebender, ei, und ein ausgestopfter.", "tokens": ["Ein", "le\u00b7ben\u00b7der", ",", "ei", ",", "und", "ein", "aus\u00b7ge\u00b7stopf\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ITJ", "$,", "KON", "ART", "ADJA", "$."], "meter": "----+-+--+-", "measure": "iambic.tri.relaxed"}}}}}