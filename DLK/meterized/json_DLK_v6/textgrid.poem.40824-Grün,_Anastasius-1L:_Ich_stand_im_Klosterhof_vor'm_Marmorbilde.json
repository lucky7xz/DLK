{"textgrid.poem.40824": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand im Klosterhof vor'm Marmorbilde", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand im Klosterhof vor'm Marmorbilde", "tokens": ["Ich", "stand", "im", "Klos\u00b7ter\u00b7hof", "vor'm", "Mar\u00b7mor\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Engels mit dem Flammenschwert und Schilde,", "tokens": ["Des", "En\u00b7gels", "mit", "dem", "Flam\u00b7men\u00b7schwert", "und", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Fu\u00df tritt sieghaft auf den H\u00f6llendrachen", "tokens": ["Sein", "Fu\u00df", "tritt", "sieg\u00b7haft", "auf", "den", "H\u00f6l\u00b7len\u00b7dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit schupp'gem Leib und offnem Feuerrachen.", "tokens": ["Mit", "schupp'\u00b7gem", "Leib", "und", "off\u00b7nem", "Feu\u00b7er\u00b7ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch seht jetzt zwiefach Satanas bezwungen:", "tokens": ["Doch", "seht", "jetzt", "zwie\u00b7fach", "Sa\u00b7ta\u00b7nas", "be\u00b7zwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Rosenstrauch hat bl\u00fchend sie umschlungen", "tokens": ["Ein", "Ro\u00b7sen\u00b7strauch", "hat", "bl\u00fc\u00b7hend", "sie", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4chst und dr\u00fcckt dem Seraph auch behende", "tokens": ["Und", "w\u00e4chst", "und", "dr\u00fcckt", "dem", "Se\u00b7raph", "auch", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anstatt des Schwerts ein R\u00f6slein in die H\u00e4nde.", "tokens": ["An\u00b7statt", "des", "Schwerts", "ein", "R\u00f6s\u00b7lein", "in", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich ging ins Refectorium der Br\u00fcder", "tokens": ["Ich", "ging", "ins", "Re\u00b7fec\u00b7to\u00b7ri\u00b7um", "der", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und setzte mich zum Mahl mit ihnen nieder,", "tokens": ["Und", "setz\u00b7te", "mich", "zum", "Mahl", "mit", "ih\u00b7nen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf schwarzer Tafel aber stand mit Kreide:", "tokens": ["Auf", "schwar\u00b7zer", "Ta\u00b7fel", "a\u00b7ber", "stand", "mit", "Krei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbsilentium!\u00ab der Todesspruch der Freude.", "tokens": ["\u00bb", "si\u00b7len\u00b7ti\u00b7um", "!", "\u00ab", "der", "To\u00b7des\u00b7spruch", "der", "Freu\u00b7de", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch des Verbotes scheint gar wenig fragend", "tokens": ["Doch", "des", "Ver\u00b7bo\u00b7tes", "scheint", "gar", "we\u00b7nig", "fra\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PIS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachtigall, in nahen B\u00fcschen schlagend,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "in", "na\u00b7hen", "B\u00fc\u00b7schen", "schla\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Taubenpaar, vor'm Fenster liebegirrend,", "tokens": ["Das", "Tau\u00b7ben\u00b7paar", ",", "vor'm", "Fens\u00b7ter", "lie\u00b7be\u00b7gir\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der V\u00e4ter frommes Sinnen fast verwirrend!", "tokens": ["Der", "V\u00e4\u00b7ter", "from\u00b7mes", "Sin\u00b7nen", "fast", "ver\u00b7wir\u00b7rend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich wallte durch des Gartens Duftgel\u00e4nde,", "tokens": ["Ich", "wall\u00b7te", "durch", "des", "Gar\u00b7tens", "Duft\u00b7ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schm\u00fcckt' einst eine Sonnenuhr die W\u00e4nde,", "tokens": ["Da", "schm\u00fcckt'", "einst", "ei\u00b7ne", "Son\u00b7ne\u00b7nuhr", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf stand in schwarzer Schrift die tr\u00fcbe Kunde:", "tokens": ["Drauf", "stand", "in", "schwar\u00b7zer", "Schrift", "die", "tr\u00fc\u00b7be", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbo Mensch, du kennest weder Tag noch Stunde!\u00ab", "tokens": ["\u00bb", "o", "Mensch", ",", "du", "ken\u00b7nest", "we\u00b7der", "Tag", "noch", "Stun\u00b7de", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch Reben ranken jetzt um Zahl und Zeiger,", "tokens": ["Doch", "Re\u00b7ben", "ran\u00b7ken", "jetzt", "um", "Zahl", "und", "Zei\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dran eine Traube h\u00e4ngt als Schenkenzeiger,", "tokens": ["Dran", "ei\u00b7ne", "Trau\u00b7be", "h\u00e4ngt", "als", "Schen\u00b7ken\u00b7zei\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die dichten Ranken s\u00e4useln lust'ge Kunde,", "tokens": ["Die", "dich\u00b7ten", "Ran\u00b7ken", "s\u00e4u\u00b7seln", "lust'\u00b7ge", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Mensch, du kennst jetzt wahrlich nicht die Stunde.", "tokens": ["O", "Mensch", ",", "du", "kennst", "jetzt", "wahr\u00b7lich", "nicht", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mich d\u00fcnkt, als ob Natur mir allerwegen", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "ob", "Na\u00b7tur", "mir", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hielt' eine gro\u00dfe, lichte Freud' entgegen,", "tokens": ["Hielt'", "ei\u00b7ne", "gro\u00b7\u00dfe", ",", "lich\u00b7te", "Freud'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie Madonna mit dem heil'gen Kinde,", "tokens": ["Und", "wie", "Ma\u00b7don\u00b7na", "mit", "dem", "heil'\u00b7gen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Schmerz der Welt vers\u00f6hnend, vor mir st\u00fcnde.", "tokens": ["Den", "Schmerz", "der", "Welt", "ver\u00b7s\u00f6h\u00b7nend", ",", "vor", "mir", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es hat ihr Arm geheftet ihren blauen,", "tokens": ["Es", "hat", "ihr", "Arm", "ge\u00b7hef\u00b7tet", "ih\u00b7ren", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewalt'gen Mantel vor der Zukunft Grauen;", "tokens": ["Ge\u00b7walt'\u00b7gen", "Man\u00b7tel", "vor", "der", "Zu\u00b7kunft", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie lie\u00df den gr\u00fcnen Teppich niedergleiten", "tokens": ["Sie", "lie\u00df", "den", "gr\u00fc\u00b7nen", "Tep\u00b7pich", "nie\u00b7derg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf all den Moder der Vergangenheiten.", "tokens": ["Auf", "all", "den", "Mo\u00b7der", "der", "Ver\u00b7gan\u00b7gen\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie aber spricht: Bereitet sind die Wege!", "tokens": ["Sie", "a\u00b7ber", "spricht", ":", "Be\u00b7rei\u00b7tet", "sind", "die", "We\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchzieh', mein Gast, frei meines Reiches Stege,", "tokens": ["Durch\u00b7zieh'", ",", "mein", "Gast", ",", "frei", "mei\u00b7nes", "Rei\u00b7ches", "Ste\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Haupt umstrahlt von Himmels Sternenglanze,", "tokens": ["Das", "Haupt", "um\u00b7strahlt", "von", "Him\u00b7mels", "Ster\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Fu\u00df gek\u00fc\u00dft vom Erden-Bl\u00fcthenkranze!", "tokens": ["Den", "Fu\u00df", "ge\u00b7k\u00fc\u00dft", "vom", "Er\u00b7den\u00b7B\u00b7l\u00fcthen\u00b7kran\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Setz' dich zu Tisch, doch zieh nicht Gramgesichter,", "tokens": ["Setz'", "dich", "zu", "Tisch", ",", "doch", "zieh", "nicht", "Gram\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei meiner Satzung kein tr\u00fcbsel'ger Richter,", "tokens": ["Sei", "mei\u00b7ner", "Sat\u00b7zung", "kein", "tr\u00fcb\u00b7sel'\u00b7ger", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn \u00fcb'rall hinter dir mit gr\u00fcner Ruthe", "tokens": ["Denn", "\u00fc\u00b7b'rall", "hin\u00b7ter", "dir", "mit", "gr\u00fc\u00b7ner", "Ru\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steht Lenz, mein lust'ger Rath, im Schellenhute.", "tokens": ["Steht", "Lenz", ",", "mein", "lust'\u00b7ger", "Rath", ",", "im", "Schel\u00b7len\u00b7hu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}