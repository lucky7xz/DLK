{"textgrid.poem.54492": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der g\u00fcldne Morgenstern/ dringt durch die tr\u00fcbe Nacht!", "tokens": ["Der", "g\u00fcld\u00b7ne", "Mor\u00b7gens\u00b7tern", "/", "dringt", "durch", "die", "tr\u00fc\u00b7be", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nah ist die Gnaden-Sonn! last vns den H\u00f6chsten loben/", "tokens": ["Nah", "ist", "die", "Gna\u00b7den\u00b7Sonn", "!", "last", "vns", "den", "H\u00f6chs\u00b7ten", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der vnser Freyheit nicht hat l\u00e4nger auffgeschoben/", "tokens": ["Der", "vn\u00b7ser", "Frey\u00b7heit", "nicht", "hat", "l\u00e4n\u00b7ger", "auff\u00b7ge\u00b7scho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd vns Horn de\u00df Heils in Davids Haus gemacht.", "tokens": ["Vnd", "vns", "Horn", "de\u00df", "Heils", "in", "Da\u00b7vids", "Haus", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Was der Propheten Chor/ was Priester vorgebracht/", "tokens": ["Was", "der", "Pro\u00b7phe\u00b7ten", "Chor", "/", "was", "Pries\u00b7ter", "vor\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$(", "PWS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird war/ er bricht entzwey der stoltzen Feinde toben/", "tokens": ["Wird", "war", "/", "er", "bricht", "ent\u00b7zwey", "der", "stolt\u00b7zen", "Fein\u00b7de", "to\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAFIN", "$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Itzt leuchtet seine Gunst/ vnd treue Treu von oben.", "tokens": ["Itzt", "leuch\u00b7tet", "sei\u00b7ne", "Gunst", "/", "vnd", "treu\u00b7e", "Treu", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er hat den teuren Eyd/ de\u00df Bundes Pfand bedacht.", "tokens": ["Er", "hat", "den", "teu\u00b7ren", "Eyd", "/", "de\u00df", "Bun\u00b7des", "Pfand", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihm dient/ so lang euch scheint die helle Lebens Kertzen/", "tokens": ["Ihm", "dient", "/", "so", "lang", "euch", "scheint", "die", "hel\u00b7le", "Le\u00b7bens", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADJD", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Furcht vnd Banden frey/ mit heilig reinem Hertzen.", "tokens": ["Von", "Furcht", "vnd", "Ban\u00b7den", "frey", "/", "mit", "hei\u00b7lig", "rei\u00b7nem", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$(", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geht vor ihm her/ vnd+ b\u00e4hnt der rauen Wege Pfad.", "tokens": ["Geht", "vor", "ihm", "her", "/", "vnd", "+", "b\u00e4hnt", "der", "rau\u00b7en", "We\u00b7ge", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$(", "KON", "$(", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Rufft: schaut das Heil ist dar/ der Abgrund h\u00f6chster G\u00fcte", "tokens": ["Rufft", ":", "schaut", "das", "Heil", "ist", "dar", "/", "der", "Ab\u00b7grund", "h\u00f6chs\u00b7ter", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "ART", "NN", "VAFIN", "PTKVZ", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Steckt seine Stralen au\u00df. Geht von der todten H\u00fctte.", "tokens": ["Steckt", "sei\u00b7ne", "Stra\u00b7len", "au\u00df", ".", "Geht", "von", "der", "tod\u00b7ten", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Itzt weil das Licht erwacht/ in Gottes Frieden-Stadt.", "tokens": ["Itzt", "weil", "das", "Licht", "er\u00b7wacht", "/", "in", "Got\u00b7tes", "Frie\u00b7den\u00b7Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$(", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}