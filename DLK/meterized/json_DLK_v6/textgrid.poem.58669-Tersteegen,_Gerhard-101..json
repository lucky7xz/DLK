{"textgrid.poem.58669": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mensch aus Staub und Erd', und du mein Gott und Herre;", "tokens": ["Ich", "bin", "ein", "Mensch", "aus", "Staub", "und", "Erd'", ",", "und", "du", "mein", "Gott", "und", "Her\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,", "KON", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ich blieb' ein armer Mensch vor dir, wenn ich gleich heilig w\u00e4re.", "tokens": ["Ich", "blieb'", "ein", "ar\u00b7mer", "Mensch", "vor", "dir", ",", "wenn", "ich", "gleich", "hei\u00b7lig", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ich bin ein Mensch, doch bin ich dein,", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "doch", "bin", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin ja, Herr, dein Sch\u00e4felein;", "tokens": ["Ich", "bin", "ja", ",", "Herr", ",", "dein", "Sch\u00e4\u00b7fe\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu deiner Weide f\u00fchr mich hin,", "tokens": ["Zu", "dei\u00b7ner", "Wei\u00b7de", "f\u00fchr", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil du mein Gott, ich Sch\u00e4flein bin!", "tokens": ["Weil", "du", "mein", "Gott", ",", "ich", "Sch\u00e4f\u00b7lein", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zwar ich ein Mensch und S\u00fcnder bin, der dir oft widerstehet,", "tokens": ["Zwar", "ich", "ein", "Mensch", "und", "S\u00fcn\u00b7der", "bin", ",", "der", "dir", "oft", "wi\u00b7der\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "KON", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Ein Mensch, ein schwach' und dummes Tier, das stets den Irrweg gehet;", "tokens": ["Ein", "Mensch", ",", "ein", "schwach'", "und", "dum\u00b7mes", "Tier", ",", "das", "stets", "den", "Irr\u00b7weg", "ge\u00b7het", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "KON", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Doch du bist Gott und hast Geduld,", "tokens": ["Doch", "du", "bist", "Gott", "und", "hast", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es tr\u00e4gt mich deine Gnad' und Huld,", "tokens": ["Es", "tr\u00e4gt", "mich", "dei\u00b7ne", "Gnad'", "und", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch meine Untreu deine Treu", "tokens": ["Durch", "mei\u00b7ne", "Un\u00b7treu", "dei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich nicht so leicht l\u00e4\u00dft machen scheu.", "tokens": ["Sich", "nicht", "so", "leicht", "l\u00e4\u00dft", "ma\u00b7chen", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "ADJD", "VVFIN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}