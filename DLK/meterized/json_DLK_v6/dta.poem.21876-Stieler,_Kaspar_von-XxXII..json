{"dta.poem.21876": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der Rauch vertreibt den Schwarm der Bie-", "tokens": ["Der", "Rauch", "ver\u00b7treibt", "den", "Schwarm", "der", "Bie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df man den Honig brechen kan.", "tokens": ["da\u00df", "man", "den", "Ho\u00b7nig", "bre\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mir mein Honig-gl\u00fckk\u2019 erschienen/", "tokens": ["Da\u00df", "mir", "mein", "Ho\u00b7nig\u00b7gl\u00fc\u00b7kk'", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "hat/ Trozz dem Neid! auch Rauch getahn.", "tokens": ["hat", "/", "Trozz", "dem", "Neid", "!", "auch", "Rauch", "ge\u00b7tahn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "NN", "ART", "NN", "$.", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}