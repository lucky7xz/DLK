{"textgrid.poem.28663": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schrifft die ist ein Licht, den Gang uns recht zu weisen;", "tokens": ["Die", "Schrifft", "die", "ist", "ein", "Licht", ",", "den", "Gang", "uns", "recht", "zu", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schrifft die ist die Kost, die Seele wol zu speisen;", "tokens": ["Die", "Schrifft", "die", "ist", "die", "Kost", ",", "die", "See\u00b7le", "wol", "zu", "spei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Schrifft die ist das Kraut, Gebrechligkeit zu d\u00e4mpffen;", "tokens": ["Die", "Schrifft", "die", "ist", "das", "Kraut", ",", "Ge\u00b7brech\u00b7lig\u00b7keit", "zu", "d\u00e4mpf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Schrifft die ist das Schwerdt, die Feinde zu bek\u00e4mpffen;", "tokens": ["Die", "Schrifft", "die", "ist", "das", "Schwerdt", ",", "die", "Fein\u00b7de", "zu", "be\u00b7k\u00e4mpf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Schrifft die ist die Salb auff de\u00df Gem\u00fctes Wunden;", "tokens": ["Die", "Schrifft", "die", "ist", "die", "Salb", "auff", "de\u00df", "Ge\u00b7m\u00fc\u00b7tes", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Schrifft die ist die Zucht, drinn ewig Heil wird funden.", "tokens": ["Die", "Schrifft", "die", "ist", "die", "Zucht", ",", "drinn", "e\u00b7wig", "Heil", "wird", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}