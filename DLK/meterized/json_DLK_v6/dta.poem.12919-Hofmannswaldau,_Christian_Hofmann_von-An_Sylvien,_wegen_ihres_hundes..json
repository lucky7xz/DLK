{"dta.poem.12919": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, wegen ihres hundes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjeviel gel\u00fccklicher ist dieser hund, als ich!", "tokens": ["Wie\u00b7viel", "ge\u00b7l\u00fcck\u00b7li\u00b7cher", "ist", "die\u00b7ser", "hund", ",", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "PDAT", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er k\u00fcsset deinen mund, ich aber niemals dich.", "tokens": ["Er", "k\u00fcs\u00b7set", "dei\u00b7nen", "mund", ",", "ich", "a\u00b7ber", "nie\u00b7mals", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}