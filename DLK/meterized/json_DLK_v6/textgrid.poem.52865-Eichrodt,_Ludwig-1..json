{"textgrid.poem.52865": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer unter Rachepl\u00e4nen", "tokens": ["Wer", "un\u00b7ter", "Ra\u00b7che\u00b7pl\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei Tag im Finstern schleicht,", "tokens": ["Bei", "Tag", "im", "Fins\u00b7tern", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer seines N\u00e4chsten Thr\u00e4nen", "tokens": ["Wer", "sei\u00b7nes", "N\u00e4chs\u00b7ten", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf's Butterbrod sich streicht,", "tokens": ["Auf's", "But\u00b7ter\u00b7brod", "sich", "streicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gleicht der nicht einem Blinden,", "tokens": ["Gleicht", "der", "nicht", "ei\u00b7nem", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der durch sich selbst genarrt", "tokens": ["Der", "durch", "sich", "selbst", "ge\u00b7narrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich wei\u00df zurecht zu finden", "tokens": ["Sich", "wei\u00df", "zu\u00b7recht", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In keiner Gegenwart?", "tokens": ["In", "kei\u00b7ner", "Ge\u00b7gen\u00b7wart", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie lange noch will scheinen", "tokens": ["Wie", "lan\u00b7ge", "noch", "will", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond auf Menschenwitz,", "tokens": ["Der", "Mond", "auf", "Men\u00b7schen\u00b7witz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange noch soll weinen", "tokens": ["Wie", "lan\u00b7ge", "noch", "soll", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlust um Vollbesitz?", "tokens": ["Ver\u00b7lust", "um", "Voll\u00b7be\u00b7sitz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie lange noch, o frage", "tokens": ["Wie", "lan\u00b7ge", "noch", ",", "o", "fra\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "$,", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht Du noch Du und Du,", "tokens": ["Nicht", "Du", "noch", "Du", "und", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "PPER", "KON", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Es trocknet jede Klage", "tokens": ["Es", "trock\u00b7net", "je\u00b7de", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor ihrer Thr\u00e4ne zu.", "tokens": ["Vor", "ih\u00b7rer", "Thr\u00e4\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So will ich mich verkriechen", "tokens": ["So", "will", "ich", "mich", "ver\u00b7krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In's tiefre Selbst hinab,", "tokens": ["In's", "tief\u00b7re", "Selbst", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An Wunderblumen riechen,", "tokens": ["An", "Wun\u00b7der\u00b7blu\u00b7men", "rie\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mir der Abgrund gab,", "tokens": ["Die", "mir", "der", "Ab\u00b7grund", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Abgrund meiner Seele,", "tokens": ["Der", "Ab\u00b7grund", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der k\u00fchle Herzensschacht,", "tokens": ["Der", "k\u00fch\u00b7le", "Her\u00b7zens\u00b7schacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drin ehmals Gabriele", "tokens": ["Drin", "eh\u00b7mals", "Ga\u00b7bri\u00b7e\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entfinsterte die Nacht.", "tokens": ["Ent\u00b7fins\u00b7ter\u00b7te", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Fahr wohl, mein Aberglaube,", "tokens": ["Fahr", "wohl", ",", "mein", "A\u00b7berg\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leb' wohl, mein blauer Traum!", "tokens": ["Leb'", "wohl", ",", "mein", "blau\u00b7er", "Traum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Wehmuthswurm zum Raube", "tokens": ["Dem", "Weh\u00b7muths\u00b7wurm", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward meines Lebens Baum.", "tokens": ["Ward", "mei\u00b7nes", "Le\u00b7bens", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lebe todt f\u00fcr's Leben,", "tokens": ["Ich", "le\u00b7be", "todt", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Rabe meines Hirns,", "tokens": ["Der", "Ra\u00b7be", "mei\u00b7nes", "Hirns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was soll ich weiterweben?", "tokens": ["Was", "soll", "ich", "wei\u00b7ter\u00b7we\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die G\u00f6tter sie verwirrn's.", "tokens": ["Die", "G\u00f6t\u00b7ter", "sie", "ver\u00b7wirrn'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}