{"textgrid.poem.36020": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[zu den sch\u00f6nen 80 Lenzen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu den sch\u00f6nen 80 Lenzen,", "tokens": ["Zu", "den", "sch\u00f6\u00b7nen", "80", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die vor\u00fcber du sahst schweben,", "tokens": ["Die", "vor\u00b7\u00fc\u00b7ber", "du", "sahst", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reich an Segen, Ruhm und Kr\u00e4nzen,", "tokens": ["Reich", "an", "Se\u00b7gen", ",", "Ruhm", "und", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolle Gott noch manchen geben.", "tokens": ["Wol\u00b7le", "Gott", "noch", "man\u00b7chen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}