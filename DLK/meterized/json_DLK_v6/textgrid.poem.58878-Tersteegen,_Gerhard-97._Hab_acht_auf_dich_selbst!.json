{"textgrid.poem.58878": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "97. Hab acht auf dich selbst!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer andre will und soll erbauen,", "tokens": ["Wer", "and\u00b7re", "will", "und", "soll", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df viel in seinen Busen schauen", "tokens": ["Mu\u00df", "viel", "in", "sei\u00b7nen", "Bu\u00b7sen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bringen in sein Herz zuvor", "tokens": ["Und", "brin\u00b7gen", "in", "sein", "Herz", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das, was er andern bringt ins Ohr.", "tokens": ["Das", ",", "was", "er", "an\u00b7dern", "bringt", "ins", "Ohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}