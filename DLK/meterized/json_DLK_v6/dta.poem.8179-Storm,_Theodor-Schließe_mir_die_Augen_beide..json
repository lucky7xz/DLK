{"dta.poem.8179": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schlie\u00dfe mir die Augen beide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Mit den lieben H\u00e4nden zu!", "tokens": ["Mit", "den", "lie\u00b7ben", "H\u00e4n\u00b7den", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht doch Alles, was ich leide,", "tokens": ["Geht", "doch", "Al\u00b7les", ",", "was", "ich", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter deiner Hand zur Ruh'.", "tokens": ["Un\u00b7ter", "dei\u00b7ner", "Hand", "zur", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie leise sich der Schmerz", "tokens": ["Und", "wie", "lei\u00b7se", "sich", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Well' um Welle schlafen leget,", "tokens": ["Well'", "um", "Wel\u00b7le", "schla\u00b7fen", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der letzte Schlag sich reget,", "tokens": ["Wie", "der", "letz\u00b7te", "Schlag", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcllest du mein ganzes Herz.", "tokens": ["F\u00fcl\u00b7lest", "du", "mein", "gan\u00b7zes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}