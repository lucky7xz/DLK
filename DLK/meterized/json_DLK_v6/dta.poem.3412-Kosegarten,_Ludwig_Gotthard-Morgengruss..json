{"dta.poem.3412": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Morgengruss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl&#228;nzend tauchet schon die Sonne", "tokens": ["Gl", "&#228;", "nzend", "tau\u00b7chet", "schon", "die", "Son\u00b7ne"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus kristallner Fluth herauf.", "tokens": ["Aus", "kris\u00b7tall\u00b7ner", "Fluth", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wache zu des Daseyns Freuden,", "tokens": ["Wa\u00b7che", "zu", "des", "Da\u00b7seyns", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Ida, wieder auf!", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wie\u00b7der", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wache auf zum Hochgef\u00fchle,", "tokens": ["Wa\u00b7che", "auf", "zum", "Hoch\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass du bist, und gl\u00fccklich bist,", "tokens": ["Dass", "du", "bist", ",", "und", "gl\u00fcck\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fccklich vor den Myriaden,", "tokens": ["Gl\u00fcck\u00b7lich", "vor", "den", "My\u00b7ria\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die der Strahl des Aufgangs k\u00fcsst!", "tokens": ["Die", "der", "Strahl", "des", "Auf\u00b7gangs", "k\u00fcsst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wache auf zu stiller R\u00fchrung!", "tokens": ["Wa\u00b7che", "auf", "zu", "stil\u00b7ler", "R\u00fch\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldige dem grossen Geist,", "tokens": ["Hul\u00b7di\u00b7ge", "dem", "gros\u00b7sen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Den der Kreaturen Jubel,", "tokens": ["Den", "der", "Kre\u00b7a\u00b7tu\u00b7ren", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den der Puls der Leben preist.", "tokens": ["Den", "der", "Puls", "der", "Le\u00b7ben", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem langgeschlafnen Schlummer,", "tokens": ["Aus", "dem", "lang\u00b7ge\u00b7schlaf\u00b7nen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem stummen \u00f6den Nichts,", "tokens": ["Aus", "dem", "stum\u00b7men", "\u00f6\u00b7den", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat dich heraufgerufen", "tokens": ["Wer", "hat", "dich", "her\u00b7auf\u00b7ge\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Genuss des s\u00fcssen Lichts?", "tokens": ["Zum", "Ge\u00b7nuss", "des", "s\u00fcs\u00b7sen", "Lichts", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Funfzehn Lenze, funfzehn Winter,", "tokens": ["Funf\u00b7zehn", "Len\u00b7ze", ",", "funf\u00b7zehn", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer hat dich geschirmt, gedeckt?", "tokens": ["Wer", "hat", "dich", "ge\u00b7schirmt", ",", "ge\u00b7deckt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer von dir die tausend W\u00fcrger", "tokens": ["Wer", "von", "dir", "die", "tau\u00b7send", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarter Kindheit weggeschreckt?", "tokens": ["Zar\u00b7ter", "Kind\u00b7heit", "weg\u00b7ge\u00b7schreckt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer gab sein Gedeihn und Segen,", "tokens": ["Wer", "gab", "sein", "Ge\u00b7deihn", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dass dein Halm so muthig schoss,", "tokens": ["Dass", "dein", "Halm", "so", "mut\u00b7hig", "schoss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Knosp' in Thau und Regen", "tokens": ["Dei\u00b7ne", "Knosp'", "in", "Thau", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausendbl\u00e4ttrig sich erschloss?", "tokens": ["Tau\u00b7send\u00b7bl\u00e4tt\u00b7rig", "sich", "er\u00b7schloss", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer beschirmt den schlanken Stengel,", "tokens": ["Wer", "be\u00b7schirmt", "den", "schlan\u00b7ken", "Sten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass kein barscher Nord ihn knickt?", "tokens": ["Dass", "kein", "bar\u00b7scher", "Nord", "ihn", "knickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer die kaum entknospte Rose", "tokens": ["Wer", "die", "kaum", "ent\u00b7knosp\u00b7te", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dass kein frecher West sie pfl\u00fcckt?", "tokens": ["Dass", "kein", "fre\u00b7cher", "West", "sie", "pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer gab deinen Augen Helle?", "tokens": ["Wer", "gab", "dei\u00b7nen", "Au\u00b7gen", "Hel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Deiner Stimme Lautenklang?", "tokens": ["Dei\u00b7ner", "Stim\u00b7me", "Lau\u00b7ten\u00b7klang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Tritten Rehesschnelle?", "tokens": ["Dei\u00b7nen", "Trit\u00b7ten", "Re\u00b7hes\u00b7schnel\u00b7le", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anmuth deinem Tanz und Gang?", "tokens": ["An\u00b7muth", "dei\u00b7nem", "Tanz", "und", "Gang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deinen Wangen Farb' und F\u00fclle?", "tokens": ["Dei\u00b7nen", "Wan\u00b7gen", "Fa\u00b7rb'", "und", "F\u00fcl\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deinen Lippen Purpurgluth?", "tokens": ["Dei\u00b7nen", "Lip\u00b7pen", "Pur\u00b7pur\u00b7gluth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Busen Fried' und Stille?", "tokens": ["Dei\u00b7nem", "Bu\u00b7sen", "Fried'", "und", "Stil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Herzen frohen Muth?", "tokens": ["Dei\u00b7nem", "Her\u00b7zen", "fro\u00b7hen", "Muth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wer blies der Marmorb\u00fcste", "tokens": ["Und", "wer", "blies", "der", "Mar\u00b7mor\u00b7b\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Den lebend'gen Odem ein!", "tokens": ["Den", "le\u00b7ben\u00b7d'\u00b7gen", "O\u00b7dem", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eine Seele, hochbegabet,", "tokens": ["Ei\u00b7ne", "See\u00b7le", ",", "hoch\u00b7be\u00b7ga\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Engelfroh und engelrein!", "tokens": ["En\u00b7gel\u00b7froh", "und", "en\u00b7gel\u00b7rein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Die da denket, die da dichtet,", "tokens": ["Die", "da", "den\u00b7ket", ",", "die", "da", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich freut und sich betr\u00fcbt,", "tokens": ["Die", "sich", "freut", "und", "sich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "KON", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hofft und f\u00fcrchtet, w\u00e4hlt und meidet,", "tokens": ["Hofft", "und", "f\u00fcrch\u00b7tet", ",", "w\u00e4hlt", "und", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahndet, w\u00fcnschet, schmachtet, liebt?", "tokens": ["Ahn\u00b7det", ",", "w\u00fcn\u00b7schet", ",", "schmach\u00b7tet", ",", "liebt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die sich weidet nimmerm\u00fcde", "tokens": ["Die", "sich", "wei\u00b7det", "nim\u00b7mer\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Sch\u00f6nheit der Natur,", "tokens": ["An", "der", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Gl\u00fchwurm im Geb\u00fcsche,", "tokens": ["An", "dem", "Gl\u00fch\u00b7wurm", "im", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Veilchen auf der Flur?", "tokens": ["An", "dem", "Veil\u00b7chen", "auf", "der", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und die von dem goldnen W\u00fcrmchen,", "tokens": ["Und", "die", "von", "dem", "gold\u00b7nen", "W\u00fcrm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Das auf schlankem Halm sich wiegt,", "tokens": ["Das", "auf", "schlan\u00b7kem", "Halm", "sich", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Rastlos weiter, rastlos h\u00f6her", "tokens": ["Rast\u00b7los", "wei\u00b7ter", ",", "rast\u00b7los", "h\u00f6\u00b7her"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum grossen Geiste fliegt? \u2014", "tokens": ["Bis", "zum", "gros\u00b7sen", "Geis\u00b7te", "fliegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Lispelt Preise, fliesset Thr\u00e4nen,", "tokens": ["Lis\u00b7pelt", "Prei\u00b7se", ",", "flies\u00b7set", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldiget dem grossen Geist,", "tokens": ["Hul\u00b7di\u00b7get", "dem", "gros\u00b7sen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Den der Jubel aller Wesen,", "tokens": ["Den", "der", "Ju\u00b7bel", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Leben Pulsschlag preist! \u2014 \u2014", "tokens": ["Al\u00b7ler", "Le\u00b7ben", "Puls\u00b7schlag", "preist", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ida, Ida, meine S\u00fcsse,", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "mei\u00b7ne", "S\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Edle, Liebliche,", "tokens": ["Mei\u00b7ne", "Ed\u00b7le", ",", "Lieb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sch\u00f