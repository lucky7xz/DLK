 "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "O, so wirst du sichern Schrittes", "tokens": ["O", ",", "so", "wirst", "du", "si\u00b7chern", "Schrit\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Lebens Irren gehn,", "tokens": ["Durch", "des", "Le\u00b7bens", "Ir\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigend dulden, l\u00e4chelnd sterben,", "tokens": ["Schwei\u00b7gend", "dul\u00b7den", ",", "l\u00e4\u00b7chelnd", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzend wieder auferstehn!", "tokens": ["Jauch\u00b7zend", "wie\u00b7der", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}