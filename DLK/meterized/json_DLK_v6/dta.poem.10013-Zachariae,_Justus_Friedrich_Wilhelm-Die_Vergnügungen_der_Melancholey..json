{"dta.poem.10013": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O was ist der nichtige Pomp, der H\u00f6fe Gepr\u00e4nge?", "tokens": ["O", "was", "ist", "der", "nich\u00b7ti\u00b7ge", "Pomp", ",", "der", "H\u00f6\u00b7fe", "Ge\u00b7pr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Gl\u00fccklicher scheint mir sogar der hohe Verbannte, der", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "scheint", "mir", "so\u00b7gar", "der", "ho\u00b7he", "Ver\u00b7bann\u00b7te", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In Siberiens W\u00fcsten, in alten verfallnen Gem\u00e4chern", "tokens": ["In", "Si\u00b7be\u00b7ri\u00b7ens", "W\u00fcs\u00b7ten", ",", "in", "al\u00b7ten", "ver\u00b7fall\u00b7nen", "Ge\u00b7m\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$,", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Eines hohen Kastells, die langsamen Stunden zur\u00fccklegt.", "tokens": ["Ei\u00b7nes", "ho\u00b7hen", "Kas\u00b7tells", ",", "die", "lang\u00b7sa\u00b7men", "Stun\u00b7den", "zu\u00b7r\u00fcck\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Nichts entdecket sein Blick, als unabsehliche Haiden,", "tokens": ["Nichts", "ent\u00b7de\u00b7cket", "sein", "Blick", ",", "als", "un\u00b7ab\u00b7seh\u00b7li\u00b7che", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wo ein ewiger Winter den Wagen von Eise dahinrollt.", "tokens": ["Wo", "ein", "e\u00b7wi\u00b7ger", "Win\u00b7ter", "den", "Wa\u00b7gen", "von", "Ei\u00b7se", "da\u00b7hin\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "In der N\u00e4h auch zeiget sich ihm stets einerley Aussicht,", "tokens": ["In", "der", "N\u00e4h", "auch", "zei\u00b7get", "sich", "ihm", "stets", "ei\u00b7ner\u00b7ley", "Aus\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Feste schreckliche Mauern, die dicken dunkeln Basteyen,", "tokens": ["Fes\u00b7te", "schreck\u00b7li\u00b7che", "Mau\u00b7ern", ",", "die", "di\u00b7cken", "dun\u00b7keln", "Bas\u00b7te\u00b7yen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und die hohen Spitzen des Dachs; indessen die Glocke", "tokens": ["Und", "die", "ho\u00b7hen", "Spit\u00b7zen", "des", "Dachs", ";", "in\u00b7des\u00b7sen", "die", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Fern vom h\u00f6hesten Thurm unwirthbare W\u00fcsten durch-", "tokens": ["Fern", "vom", "h\u00f6\u00b7hes\u00b7ten", "Thurm", "un\u00b7wirth\u00b7ba\u00b7re", "W\u00fcs\u00b7ten", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und mit dem traurigen Schall auch neuen Kummer er-", "tokens": ["Und", "mit", "dem", "trau\u00b7ri\u00b7gen", "Schall", "auch", "neu\u00b7en", "Kum\u00b7mer", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "TRUNC"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und doch ist er begl\u00fcckter, als jener verw\u00f6hnte Satrape,", "tokens": ["Und", "doch", "ist", "er", "be\u00b7gl\u00fcck\u00b7ter", ",", "als", "je\u00b7ner", "ver\u00b7w\u00f6hn\u00b7te", "Sa\u00b7tra\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Den er hinter sich lie\u00df in Moskaus goldnen Pall\u00e4sten,", "tokens": ["Den", "er", "hin\u00b7ter", "sich", "lie\u00df", "in", "Mos\u00b7kaus", "gold\u00b7nen", "Pal\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PRF", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Da in schwelgrischer Ruh und lachenden Freuden zu leben.", "tokens": ["Da", "in", "schwel\u00b7gri\u00b7scher", "Ruh", "und", "la\u00b7chen\u00b7den", "Freu\u00b7den", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}}}}}