{"textgrid.poem.33669": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingssegen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, aus goldnen Jugendtagen,", "tokens": ["Mein", "Herz", ",", "aus", "gold\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus gl\u00fccklicher Vergangenheit", "tokens": ["Aus", "gl\u00fcck\u00b7li\u00b7cher", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In gr\u00fcnes Laub ist's ausgeschlagen,", "tokens": ["In", "gr\u00fc\u00b7nes", "Laub", "ist's", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da lebt's und atmet und gedeiht.", "tokens": ["Da", "lebt's", "und", "at\u00b7met", "und", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sehnsucht aber, die ich hatte,", "tokens": ["Die", "Sehn\u00b7sucht", "a\u00b7ber", ",", "die", "ich", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mancher wunders\u00fc\u00dfe Traum,", "tokens": ["Und", "man\u00b7cher", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie s\u00e4useln jetzt im Lindenblatte", "tokens": ["Sie", "s\u00e4u\u00b7seln", "jetzt", "im", "Lin\u00b7den\u00b7blat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fl\u00fcstern in dem Tannenbaum.", "tokens": ["Und", "fl\u00fcs\u00b7tern", "in", "dem", "Tan\u00b7nen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lebe, wo die Finken schlagen,", "tokens": ["Ich", "le\u00b7be", ",", "wo", "die", "Fin\u00b7ken", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man kann mich in der Bl\u00fctezeit", "tokens": ["Man", "kann", "mich", "in", "der", "Bl\u00fc\u00b7te\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Haus in einem Zweige tragen,", "tokens": ["Nach", "Haus", "in", "ei\u00b7nem", "Zwei\u00b7ge", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gefangen bin ich und befreit.", "tokens": ["Ge\u00b7fan\u00b7gen", "bin", "ich", "und", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es bringt mir in der Morgenk\u00fchle", "tokens": ["Es", "bringt", "mir", "in", "der", "Mor\u00b7gen\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sonntags reine Himmelslust", "tokens": ["Des", "Sonn\u00b7tags", "rei\u00b7ne", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die l\u00e4ngst entschlummerten Gef\u00fchle", "tokens": ["Die", "l\u00e4ngst", "ent\u00b7schlum\u00b7mer\u00b7ten", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnernd wieder in die Brust.", "tokens": ["E\u00b7rin\u00b7nernd", "wie\u00b7der", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}