{"dta.poem.3840": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine alte blinde Frau/  \n  A. M. K\u00fcchlerin.  1736.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Nun HErr, du, deine Allmachts-Hand", "tokens": ["Nun", "Herr", ",", "du", ",", "dei\u00b7ne", "All\u00b7machts\u00b7Hand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat mich auch endlich ausgespannt,", "tokens": ["Hat", "mich", "auch", "end\u00b7lich", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ich im Finstern seuffzen m\u00fcssen,", "tokens": ["Da", "ich", "im", "Fins\u00b7tern", "seuff\u00b7zen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey herber Z\u00e4hren-Fluth und G\u00fcssen.", "tokens": ["Bey", "her\u00b7ber", "Z\u00e4h\u00b7ren\u00b7Fluth", "und", "G\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du heist mich in dein Salem gehn,", "tokens": ["Du", "heist", "mich", "in", "dein", "Sa\u00b7lem", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott Lob! nun kan ich wieder sehn.", "tokens": ["Gott", "Lob", "!", "nun", "kan", "ich", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Blindheit weicht, o meine Wonne,", "tokens": ["Die", "Blind\u00b7heit", "weicht", ",", "o", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist die rechte Gnaden-Sonne.", "tokens": ["Du", "bist", "die", "rech\u00b7te", "Gna\u00b7den\u00b7Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da, HErr, dein Glantz das dunckle bricht,", "tokens": ["Da", ",", "Herr", ",", "dein", "Glantz", "das", "dunck\u00b7le", "bricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So leuchtet mir ein ewig Licht.", "tokens": ["So", "leuch\u00b7tet", "mir", "ein", "e\u00b7wig", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}