{"textgrid.poem.61018": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist so still, so sanft, so sinnig,", "tokens": ["Du", "bist", "so", "still", ",", "so", "sanft", ",", "so", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schau' ich dir ins Angesicht,", "tokens": ["Und", "schau'", "ich", "dir", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da leuchtet mir verst\u00e4ndnisinnig", "tokens": ["Da", "leuch\u00b7tet", "mir", "ver\u00b7st\u00e4nd\u00b7ni\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dunkeln Augen frommes Licht.", "tokens": ["Der", "dun\u00b7keln", "Au\u00b7gen", "from\u00b7mes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht Worte gibst du dem Gef\u00fchle,", "tokens": ["Nicht", "Wor\u00b7te", "gibst", "du", "dem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du redest nicht, du l\u00e4chelst nur;", "tokens": ["Du", "re\u00b7dest", "nicht", ",", "du", "l\u00e4\u00b7chelst", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So l\u00e4chelt in des Abends K\u00fchle", "tokens": ["So", "l\u00e4\u00b7chelt", "in", "des", "A\u00b7bends", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der lichte Mond auf Wald und Flur.", "tokens": ["Der", "lich\u00b7te", "Mond", "auf", "Wald", "und", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Traumesd\u00e4mmerung allm\u00e4hlich", "tokens": ["In", "Trau\u00b7mes\u00b7d\u00e4m\u00b7me\u00b7rung", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zerrinnt die ganze Seele mir,", "tokens": ["Zer\u00b7rinnt", "die", "gan\u00b7ze", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur das eine f\u00fchl' ich selig,", "tokens": ["Und", "nur", "das", "ei\u00b7ne", "f\u00fchl'", "ich", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich vereinigt bin mit dir.", "tokens": ["Da\u00df", "ich", "ver\u00b7ei\u00b7nigt", "bin", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}