{"textgrid.poem.60189": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, der vom Weine berauscht, die Lust der Erde besungen,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der vom Weine berauscht, die Lust der Erde besungen,", "tokens": ["Du", ",", "der", "vom", "Wei\u00b7ne", "be\u00b7rauscht", ",", "die", "Lust", "der", "Er\u00b7de", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir gab Apollo kein lyrisches Spiel", "tokens": ["Mir", "gab", "A\u00b7pol\u00b7lo", "kein", "ly\u00b7ri\u00b7sches", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bespannt mit Saiten von Gold, doch sind mir Lieder gelungen,", "tokens": ["Be\u00b7spannt", "mit", "Sai\u00b7ten", "von", "Gold", ",", "doch", "sind", "mir", "Lie\u00b7der", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "S\u00fc\u00dfklingend sang ich der Seele Gef\u00fchl.", "tokens": ["S\u00fc\u00df\u00b7klin\u00b7gend", "sang", "ich", "der", "See\u00b7le", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mich h\u00f6rt der eiserne Held, mir horcht der ernste Gesandte", "tokens": ["Mich", "h\u00f6rt", "der", "ei\u00b7ser\u00b7ne", "Held", ",", "mir", "horcht", "der", "erns\u00b7te", "Ge\u00b7sand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Herunter kommend vom Stuhle des Herrn,", "tokens": ["Her\u00b7un\u00b7ter", "kom\u00b7mend", "vom", "Stuh\u00b7le", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch h\u00f6ret meinen Gesang, wer sonst die Muse verkannte,", "tokens": ["Auch", "h\u00f6\u00b7ret", "mei\u00b7nen", "Ge\u00b7sang", ",", "wer", "sonst", "die", "Mu\u00b7se", "ver\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Geizes Priester, vernehmen ihn gern.", "tokens": ["Des", "Gei\u00b7zes", "Pries\u00b7ter", ",", "ver\u00b7neh\u00b7men", "ihn", "gern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mir gab dein liebender Freund, der Felsenspringerin Laute,", "tokens": ["Mir", "gab", "dein", "lie\u00b7ben\u00b7der", "Freund", ",", "der", "Fel\u00b7sen\u00b7sprin\u00b7ge\u00b7rin", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O, ihn nur denken wird s\u00fcsser Gesang", "tokens": ["O", ",", "ihn", "nur", "den\u00b7ken", "wird", "s\u00fcs\u00b7ser", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "ADV", "VVINF", "VAFIN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "In der ganz saphischen Brust; der Liebes G\u00f6tter Vertraute", "tokens": ["In", "der", "ganz", "sa\u00b7phi\u00b7schen", "Brust", ";", "der", "Lie\u00b7bes", "G\u00f6t\u00b7ter", "Ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ward ich und habe die Herzen in Zwang!", "tokens": ["Ward", "ich", "und", "ha\u00b7be", "die", "Her\u00b7zen", "in", "Zwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mich f\u00fchlt der wankende Greis, die abgelebte Matrone,", "tokens": ["Mich", "f\u00fchlt", "der", "wan\u00b7ken\u00b7de", "Greis", ",", "die", "ab\u00b7ge\u00b7leb\u00b7te", "Mat\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich horcht der J\u00fcnglinge klopfendes Herz.", "tokens": ["Mich", "horcht", "der", "J\u00fcng\u00b7lin\u00b7ge", "klop\u00b7fen\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das M\u00e4dchen f\u00fcrchtet den Pfeil! er rauscht im saphischen Thone", "tokens": ["Das", "M\u00e4d\u00b7chen", "f\u00fcrch\u00b7tet", "den", "Pfeil", "!", "er", "rauscht", "im", "sa\u00b7phi\u00b7schen", "Tho\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Laut, wie im Utzischen Liede voll Scherz.", "tokens": ["Laut", ",", "wie", "im", "Ut\u00b7zi\u00b7schen", "Lie\u00b7de", "voll", "Scherz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "APPRART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}