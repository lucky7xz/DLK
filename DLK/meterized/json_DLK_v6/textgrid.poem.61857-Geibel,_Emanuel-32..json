{"textgrid.poem.61857": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Gl\u00fcck, die leichte Dirne,", "tokens": ["Wenn", "das", "Gl\u00fcck", ",", "die", "leich\u00b7te", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Launisch dir den R\u00fccken kehrt,", "tokens": ["Lau\u00b7nisch", "dir", "den", "R\u00fc\u00b7cken", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebe doppelt k\u00fchn die Stirne,", "tokens": ["He\u00b7be", "dop\u00b7pelt", "k\u00fchn", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fcrte doppelt fest das Schwert.", "tokens": ["G\u00fcr\u00b7te", "dop\u00b7pelt", "fest", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rasch verwelkt ein Kranz aus Zweigen,", "tokens": ["Rasch", "ver\u00b7welkt", "ein", "Kranz", "aus", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du spielend dir gewannst;", "tokens": ["Die", "du", "spie\u00b7lend", "dir", "ge\u00b7wannst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Not erst magst du zeigen,", "tokens": ["In", "der", "Not", "erst", "magst", "du", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer du bist, und was du kannst.", "tokens": ["Wer", "du", "bist", ",", "und", "was", "du", "kannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}