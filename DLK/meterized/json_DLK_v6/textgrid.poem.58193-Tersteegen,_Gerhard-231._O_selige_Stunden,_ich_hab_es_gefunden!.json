{"textgrid.poem.58193": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "231. O selige Stunden, ich hab es gefunden!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nicht seufzen mehr, ich habe, was ich will,", "tokens": ["Ich", "kann", "nicht", "seuf\u00b7zen", "mehr", ",", "ich", "ha\u00b7be", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "ADV", "$,", "PPER", "VAFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was ich fern gesucht, find' ich nun in mir nahe.", "tokens": ["Und", "was", "ich", "fern", "ge\u00b7sucht", ",", "find'", "ich", "nun", "in", "mir", "na\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein stetes Beten ist, da\u00df ich gar schweige still,", "tokens": ["Mein", "ste\u00b7tes", "Be\u00b7ten", "ist", ",", "da\u00df", "ich", "gar", "schwei\u00b7ge", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschaue meinen Schatz und ihn vergn\u00fcgt umfahe.", "tokens": ["Be\u00b7schau\u00b7e", "mei\u00b7nen", "Schatz", "und", "ihn", "ver\u00b7gn\u00fcgt", "um\u00b7fa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}