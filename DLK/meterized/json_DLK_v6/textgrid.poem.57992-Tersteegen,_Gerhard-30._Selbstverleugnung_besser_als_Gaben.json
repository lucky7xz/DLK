{"textgrid.poem.57992": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "30. Selbstverleugnung besser als Gaben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Offenbarung, Wundergaben,", "tokens": ["Of\u00b7fen\u00b7ba\u00b7rung", ",", "Wun\u00b7der\u00b7ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trost und S\u00fc\u00dfigkeiten haben,", "tokens": ["Trost", "und", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehre, Welt und Geld verachten,", "tokens": ["Eh\u00b7re", ",", "Welt", "und", "Geld", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieles wissen und betrachten,", "tokens": ["Vie\u00b7les", "wis\u00b7sen", "und", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fasten, lesen, singen, beten", "tokens": ["Fas\u00b7ten", ",", "le\u00b7sen", ",", "sin\u00b7gen", ",", "be\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Engelzungen reden:", "tokens": ["Und", "mit", "En\u00b7gel\u00b7zun\u00b7gen", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles dieses acht ich nicht,", "tokens": ["Al\u00b7les", "die\u00b7ses", "acht", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "CARD", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo man nicht den Willen bricht.", "tokens": ["Wo", "man", "nicht", "den", "Wil\u00b7len", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}