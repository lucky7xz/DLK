{"textgrid.poem.45045": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In Dullers Album", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon fr\u00fch der Heimat Muttergrund enthoben", "tokens": ["Schon", "fr\u00fch", "der", "Hei\u00b7mat", "Mut\u00b7ter\u00b7grund", "ent\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fernehin verpflanzt in fremde Erde,", "tokens": ["Und", "fer\u00b7ne\u00b7hin", "ver\u00b7pflanzt", "in", "frem\u00b7de", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darfst du des Wechsels dich als Gl\u00fcck beloben.", "tokens": ["Darfst", "du", "des", "Wech\u00b7sels", "dich", "als", "Gl\u00fcck", "be\u00b7lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn freire Luft lie\u00df wachsen dich nach oben,", "tokens": ["Denn", "frei\u00b7re", "Luft", "lie\u00df", "wach\u00b7sen", "dich", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df, was innen, fest und t\u00fcchtig werde,", "tokens": ["Und", "da\u00df", ",", "was", "in\u00b7nen", ",", "fest", "und", "t\u00fcch\u00b7tig", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PRELS", "ADV", "$,", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blieb an den Wurzeln hangen vaterl\u00e4ndsche Erde.", "tokens": ["Blieb", "an", "den", "Wur\u00b7zeln", "han\u00b7gen", "va\u00b7ter\u00b7l\u00e4nd\u00b7sche", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}