{"dta.poem.11061": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem 18. Psalm,  \n vom 8. bis 16. Vers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es erbebete die Erde, und es ward ihr Bau bewegt,", "tokens": ["Es", "er\u00b7be\u00b7be\u00b7te", "die", "Er\u00b7de", ",", "und", "es", "ward", "ihr", "Bau", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ja die Grundfest hoher Berge wurden wankend und ge-", "tokens": ["Ja", "die", "Grund\u00b7fest", "ho\u00b7her", "Ber\u00b7ge", "wur\u00b7den", "wan\u00b7kend", "und", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Da der Sch\u00f6pfer zornig war. Dampf ging aus von seiner Nasen,", "tokens": ["Da", "der", "Sch\u00f6p\u00b7fer", "zor\u00b7nig", "war", ".", "Dampf", "ging", "aus", "von", "sei\u00b7ner", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$.", "NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und es ward, aus seinem Mund, ein verzehrend Feur geblasen,", "tokens": ["Und", "es", "ward", ",", "aus", "sei\u00b7nem", "Mund", ",", "ein", "ver\u00b7zeh\u00b7rend", "Feur", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "VVPP", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Da\u00df es davon blitzete, mit entsetzlichem Get\u00fcmmel.", "tokens": ["Da\u00df", "es", "da\u00b7von", "blit\u00b7ze\u00b7te", ",", "mit", "ent\u00b7setz\u00b7li\u00b7chem", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Er erniedrigte den Himmel;", "tokens": ["Er", "er\u00b7nied\u00b7rig\u00b7te", "den", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der strengen Wolken G\u00fcssen,", "tokens": ["Mit", "der", "stren\u00b7gen", "Wol\u00b7ken", "G\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fuhr der Herr herab; es war dunkel unter seinen F\u00fc\u00dfen;", "tokens": ["Fuhr", "der", "Herr", "her\u00b7ab", ";", "es", "war", "dun\u00b7kel", "un\u00b7ter", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Er fuhr auf dem Cherubim, flog daher und schwebete;", "tokens": ["Er", "fuhr", "auf", "dem", "Che\u00b7ru\u00b7bim", ",", "flog", "da\u00b7her", "und", "schwe\u00b7be\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,", "VVFIN", "PAV", "KON", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Jhn erhoben und ihn trugen aller Winde Fittige.", "tokens": ["Jhn", "er\u00b7ho\u00b7ben", "und", "ihn", "tru\u00b7gen", "al\u00b7ler", "Win\u00b7de", "Fit\u00b7ti\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "PPER", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Finstre, schwarz\u2019 und dicke Wolken waren sein Gezelt um ihn,", "tokens": ["Finst\u00b7re", ",", "schwa\u00b7rz'", "und", "di\u00b7cke", "Wol\u00b7ken", "wa\u00b7ren", "sein", "Ge\u00b7zelt", "um", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.12": {"text": "Worin er verborgen war. Von dem Glanz, der von ihm schien,", "tokens": ["Wo\u00b7rin", "er", "ver\u00b7bor\u00b7gen", "war", ".", "Von", "dem", "Glanz", ",", "der", "von", "ihm", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$.", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Trennte sich der Wolken Heer, voller Hagel, Blitz und Schlossen;", "tokens": ["Trenn\u00b7te", "sich", "der", "Wol\u00b7ken", "Heer", ",", "vol\u00b7ler", "Ha\u00b7gel", ",", "Blitz", "und", "Schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "In dem Himmel donnert er, und mit Blitz und Hagel Graus,", "tokens": ["In", "dem", "Him\u00b7mel", "don\u00b7nert", "er", ",", "und", "mit", "Blitz", "und", "Ha\u00b7gel", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Lie\u00df der H\u00f6chste seinen Donner f\u00fcrchterlich und schrecklich aus.", "tokens": ["Lie\u00df", "der", "H\u00f6chs\u00b7te", "sei\u00b7nen", "Don\u00b7ner", "f\u00fcrch\u00b7ter\u00b7lich", "und", "schreck\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Feurige verzehrnde Stralen, wurden von ihm abgeschossen;", "tokens": ["Feu\u00b7ri\u00b7ge", "ver\u00b7zehrn\u00b7de", "Stra\u00b7len", ",", "wur\u00b7den", "von", "ihm", "ab\u00b7ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Er zerstreute sie, er lie\u00df blitzen, womit er sie schreckt,", "tokens": ["Er", "zer\u00b7streu\u00b7te", "sie", ",", "er", "lie\u00df", "blit\u00b7zen", ",", "wo\u00b7mit", "er", "sie", "schreckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da erblickt man Wasser-G\u00fcsse, und der Erden holder Boden", "tokens": ["Da", "er\u00b7blickt", "man", "Was\u00b7ser\u00b7G\u00fcs\u00b7se", ",", "und", "der", "Er\u00b7den", "hol\u00b7der", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "War, o Herr, von deinem Oden,", "tokens": ["War", ",", "o", "Herr", ",", "von", "dei\u00b7nem", "O\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Von dem Schnauben deiner Nasen, und vom Schelten auf-", "tokens": ["Von", "dem", "Schnau\u00b7ben", "dei\u00b7ner", "Na\u00b7sen", ",", "und", "vom", "Schel\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "KON", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}