{"textgrid.poem.60794": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An die Poetaster", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlechten, gest\u00fcmperten Versen gen\u00fcgt ein geringer Gehalt schon,", "tokens": ["Schlech\u00b7ten", ",", "ge\u00b7st\u00fcm\u00b7per\u00b7ten", "Ver\u00b7sen", "ge\u00b7n\u00fcgt", "ein", "ge\u00b7rin\u00b7ger", "Ge\u00b7halt", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "W\u00e4hrend die edlere Form tiefe Gedanken bedarf:", "tokens": ["W\u00e4h\u00b7rend", "die", "ed\u00b7le\u00b7re", "Form", "tie\u00b7fe", "Ge\u00b7dan\u00b7ken", "be\u00b7darf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wollte man euer Geschw\u00e4tz auspr\u00e4gen zur sapphischen Ode,", "tokens": ["Woll\u00b7te", "man", "eu\u00b7er", "Ge\u00b7schw\u00e4tz", "aus\u00b7pr\u00e4\u00b7gen", "zur", "sap\u00b7phi\u00b7schen", "O\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00fcrde die Welt einsehn, da\u00df es ein leeres Geschw\u00e4tz.", "tokens": ["W\u00fcr\u00b7de", "die", "Welt", "ein\u00b7sehn", ",", "da\u00df", "es", "ein", "lee\u00b7res", "Ge\u00b7schw\u00e4tz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}