{"textgrid.poem.39157": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' einen gro\u00dfen Spiegel,", "tokens": ["Ich", "hab'", "ei\u00b7nen", "gro\u00b7\u00dfen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Das ist das gr\u00fcne Meer,", "tokens": ["Das", "ist", "das", "gr\u00fc\u00b7ne", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bla\u00df werfen die Wasserh\u00fcgel", "tokens": ["Bla\u00df", "wer\u00b7fen", "die", "Was\u00b7ser\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein blasses Gesicht mir her.", "tokens": ["Mein", "blas\u00b7ses", "Ge\u00b7sicht", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es dehnt sich und es bricht sich", "tokens": ["Es", "dehnt", "sich", "und", "es", "bricht", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf jeder Woge Bug,", "tokens": ["Auf", "je\u00b7der", "Wo\u00b7ge", "Bug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zieht sich und es flicht sich", "tokens": ["Es", "zieht", "sich", "und", "es", "flicht", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jedes Wirbels Zug.", "tokens": ["In", "je\u00b7des", "Wir\u00b7bels", "Zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wellen, sie wallen und rollen", "tokens": ["Die", "Wel\u00b7len", ",", "sie", "wal\u00b7len", "und", "rol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich \u00fcbereinander hinauf,", "tokens": ["Sich", "\u00fc\u00b7be\u00b7re\u00b7in\u00b7an\u00b7der", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Draus sieht im stummen Grollen", "tokens": ["Draus", "sieht", "im", "stum\u00b7men", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein finsteres Auge herauf.", "tokens": ["Mein", "fins\u00b7te\u00b7res", "Au\u00b7ge", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}