{"textgrid.poem.47435": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[so halt' ich es mit meiner Liebe]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So halt' ich es mit meiner Liebe,", "tokens": ["So", "halt'", "ich", "es", "mit", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich mein eignes Sein vergesse,", "tokens": ["Da\u00df", "ich", "mein", "eig\u00b7nes", "Sein", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich mein Ich, mein ganzes, opfre;", "tokens": ["Da\u00df", "ich", "mein", "Ich", ",", "mein", "gan\u00b7zes", ",", "opf\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PPER", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zehntausend Seelen wenn ich h\u00e4tte,", "tokens": ["Zehn\u00b7tau\u00b7send", "See\u00b7len", "wenn", "ich", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich w\u00fcrde sie vor dir verstreu'n.", "tokens": ["Ich", "w\u00fcr\u00b7de", "sie", "vor", "dir", "ver\u00b7streu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So halt' ich es mit meiner Liebe:", "tokens": ["So", "halt'", "ich", "es", "mit", "mei\u00b7ner", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treu bin ich ohne Wank und Wandel.", "tokens": ["Treu", "bin", "ich", "oh\u00b7ne", "Wank", "und", "Wan\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df jeden h\u00f6chsten Reiz der Erde", "tokens": ["La\u00df", "je\u00b7den", "h\u00f6chs\u00b7ten", "Reiz", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vor\u00fcber zieh'n vor meinem Auge,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zieh'n", "vor", "mei\u00b7nem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht weckt er eine leise Regung;", "tokens": ["Nicht", "weckt", "er", "ei\u00b7ne", "lei\u00b7se", "Re\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sehe dich, nur dich alleine,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "nur", "dich", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und jedes andre Bild verweht.", "tokens": ["Und", "je\u00b7des", "and\u00b7re", "Bild", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin Medschnun, der, lieberasend", "tokens": ["Ich", "bin", "Medsc\u00b7hnun", ",", "der", ",", "lie\u00b7be\u00b7ra\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NE", "$,", "PRELS", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Persien und nicht Arabien,", "tokens": ["Nicht", "Per\u00b7si\u00b7en", "und", "nicht", "A\u00b7ra\u00b7bi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Erde nicht mit ihren Blumen,", "tokens": ["Die", "Er\u00b7de", "nicht", "mit", "ih\u00b7ren", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Himmel nicht mit seinen Sternen,", "tokens": ["Den", "Him\u00b7mel", "nicht", "mit", "sei\u00b7nen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr seine s\u00fc\u00dfe Leila nimmt.", "tokens": ["F\u00fcr", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Lei\u00b7la", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man drohe mit gehobner Klinge,", "tokens": ["Man", "dro\u00b7he", "mit", "ge\u00b7hob\u00b7ner", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man lasse Wund' auf Wunde bluten,", "tokens": ["Man", "las\u00b7se", "Wund'", "auf", "Wun\u00b7de", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man schlage mir das Haupt herunter!", "tokens": ["Man", "schla\u00b7ge", "mir", "das", "Haupt", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich weiche nicht von deiner Schwelle,", "tokens": ["Ich", "wei\u00b7che", "nicht", "von", "dei\u00b7ner", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich liefere dem Schwertbewehrten", "tokens": ["Ich", "lie\u00b7fe\u00b7re", "dem", "Schwert\u00b7be\u00b7wehr\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht aus das einzig edle Kleinod,", "tokens": ["Nicht", "aus", "das", "ein\u00b7zig", "ed\u00b7le", "Klei\u00b7nod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das ich besitze, meine Liebe;", "tokens": ["Das", "ich", "be\u00b7sit\u00b7ze", ",", "mei\u00b7ne", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit eisiger, erstarrter Hand noch", "tokens": ["Mit", "ei\u00b7si\u00b7ger", ",", "er\u00b7starr\u00b7ter", "Hand", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Halt' ich die sch\u00f6ne Perle fest.", "tokens": ["Halt'", "ich", "die", "sch\u00f6\u00b7ne", "Per\u00b7le", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}