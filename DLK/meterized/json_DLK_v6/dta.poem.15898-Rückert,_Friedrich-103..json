{"dta.poem.15898": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bescheiden ist, wer sich bescheidet, wer bescheiden", "tokens": ["Be\u00b7schei\u00b7den", "ist", ",", "wer", "sich", "be\u00b7schei\u00b7det", ",", "wer", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PRF", "VVPP", "$,", "PWS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich l\u00e4\u00dft, und Grenzen ehrt, die ihn von andern scheiden.", "tokens": ["Sich", "l\u00e4\u00dft", ",", "und", "Gren\u00b7zen", "ehrt", ",", "die", "ihn", "von", "an\u00b7dern", "schei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bescheiden seid ihr, wenn ihr annehmt den Bescheid,", "tokens": ["Be\u00b7schei\u00b7den", "seid", "ihr", ",", "wenn", "ihr", "an\u00b7nehmt", "den", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ihr, was ihr vielleicht einst werdet, noch nicht seid.", "tokens": ["Da\u00df", "ihr", ",", "was", "ihr", "viel\u00b7leicht", "einst", "wer\u00b7det", ",", "noch", "nicht", "seid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word",