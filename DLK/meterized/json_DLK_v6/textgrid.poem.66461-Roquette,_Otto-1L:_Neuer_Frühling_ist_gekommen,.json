{"textgrid.poem.66461": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neuer Fr\u00fchling ist gekommen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neuer Fr\u00fchling ist gekommen,", "tokens": ["Neu\u00b7er", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neues Laub und Sonnenschein,", "tokens": ["Neu\u00b7es", "Laub", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Ohr hat ihn vernommen,", "tokens": ["Je\u00b7des", "Ohr", "hat", "ihn", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Auge saugt ihn ein.", "tokens": ["Je\u00b7des", "Au\u00b7ge", "saugt", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das ist ein Bl\u00fchn und Sprie\u00dfen,", "tokens": ["Und", "das", "ist", "ein", "Bl\u00fchn", "und", "Sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Waldesduften, Quellenflie\u00dfen,", "tokens": ["Wal\u00b7des\u00b7duf\u00b7ten", ",", "Quel\u00b7len\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Brust wird wieder weit,", "tokens": ["Und", "die", "Brust", "wird", "wie\u00b7der", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fr\u00fchling, Fr\u00fchling, goldne Zeit!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", ",", "gold\u00b7ne", "Zeit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dem Felsen in die Weite", "tokens": ["Von", "dem", "Fel\u00b7sen", "in", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliege hin, mein Fr\u00fchlingssang,", "tokens": ["Flie\u00b7ge", "hin", ",", "mein", "Fr\u00fch\u00b7lings\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Str\u00f6me und Gebreite,", "tokens": ["Ue\u00b7ber", "Str\u00f6\u00b7me", "und", "Ge\u00b7brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch Gebirg und Bl\u00fcthenhang!", "tokens": ["Durch", "Ge\u00b7birg", "und", "Bl\u00fc\u00b7then\u00b7hang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darf nicht wandern, mu\u00df ja bleiben", "tokens": ["Darf", "nicht", "wan\u00b7dern", ",", "mu\u00df", "ja", "blei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "$,", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob's mich ziehn auch will und treiben,", "tokens": ["Ob's", "mich", "ziehn", "auch", "will", "und", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VMFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch so weit mein Himmel blau't", "tokens": ["Doch", "so", "weit", "mein", "Him\u00b7mel", "blau't"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Singen, singen will ich laut!", "tokens": ["Sin\u00b7gen", ",", "sin\u00b7gen", "will", "ich", "laut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "VMFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie die Welt auch wechselnd gehe,", "tokens": ["Wie", "die", "Welt", "auch", "wech\u00b7selnd", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Schicksal auch mich treibt,", "tokens": ["Wie", "das", "Schick\u00b7sal", "auch", "mich", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komme Gl\u00fcck und komme Wehe,", "tokens": ["Kom\u00b7me", "Gl\u00fcck", "und", "kom\u00b7me", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn nur Eines mir verbleibt:", "tokens": ["Wenn", "nur", "Ei\u00b7nes", "mir", "ver\u00b7bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fester Muth der freien Seele", "tokens": ["Fes\u00b7ter", "Muth", "der", "frei\u00b7en", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die freudge Liederkehle,", "tokens": ["Und", "die", "freud\u00b7ge", "Lie\u00b7der\u00b7keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lebenslust und Lebensdrang,", "tokens": ["Le\u00b7bens\u00b7lust", "und", "Le\u00b7bens\u00b7drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Goldnes Leben im Gesang!", "tokens": ["Gold\u00b7nes", "Le\u00b7ben", "im", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}