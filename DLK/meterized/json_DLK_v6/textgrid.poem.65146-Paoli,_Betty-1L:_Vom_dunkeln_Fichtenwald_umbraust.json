{"textgrid.poem.65146": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom dunkeln Fichtenwald umbraust", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom dunkeln Fichtenwald umbraust", "tokens": ["Vom", "dun\u00b7keln", "Fich\u00b7ten\u00b7wald", "um\u00b7braust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lehnt die Ruine an dem H\u00fcgel;", "tokens": ["Lehnt", "die", "Ru\u00b7i\u00b7ne", "an", "dem", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zahn der Zeit, des Sturmes Fl\u00fcgel", "tokens": ["Der", "Zahn", "der", "Zeit", ",", "des", "Stur\u00b7mes", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie haben t\u00fcchtig hier gehaust.", "tokens": ["Sie", "ha\u00b7ben", "t\u00fcch\u00b7tig", "hier", "ge\u00b7haust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rot gl\u00fcht des Abendhimmels Feuer", "tokens": ["Rot", "gl\u00fcht", "des", "A\u00b7bend\u00b7him\u00b7mels", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch das gespaltne Dach herein,", "tokens": ["Durch", "das", "ge\u00b7spalt\u00b7ne", "Dach", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von dem geborstenen Gem\u00e4uer", "tokens": ["Von", "dem", "ge\u00b7bors\u00b7te\u00b7nen", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00f6st sich zerbr\u00f6ckelnd Stein um Stein.", "tokens": ["L\u00f6st", "sich", "zer\u00b7br\u00f6\u00b7ckelnd", "Stein", "um", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Epheus gr\u00fcn Geflechte schlingt", "tokens": ["Des", "E\u00b7pheus", "gr\u00fcn", "Ge\u00b7flech\u00b7te", "schlingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich um die Pfeiler und Balkone", "tokens": ["Sich", "um", "die", "Pfei\u00b7ler", "und", "Bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Siegeszeichen, das, zum Hohne,", "tokens": ["Ein", "Sie\u00b7ges\u00b7zei\u00b7chen", ",", "das", ",", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Natur, die ewig junge, schwingt!", "tokens": ["Na\u00b7tur", ",", "die", "e\u00b7wig", "jun\u00b7ge", ",", "schwingt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie, die aus unersch\u00f6pfter F\u00fclle", "tokens": ["Sie", ",", "die", "aus", "un\u00b7er\u00b7sch\u00f6pf\u00b7ter", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stets neues kr\u00e4ft'ges Leben treibt,", "tokens": ["Stets", "neu\u00b7es", "kr\u00e4ft'\u00b7ges", "Le\u00b7ben", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Inde\u00df zu Schutt und zu Ger\u00fclle", "tokens": ["In\u00b7de\u00df", "zu", "Schutt", "und", "zu", "Ge\u00b7r\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Werk der Menschenhand zerst\u00e4ubt.", "tokens": ["Das", "Werk", "der", "Men\u00b7schen\u00b7hand", "zer\u00b7st\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und stille sinnend sitz' ich dort", "tokens": ["Und", "stil\u00b7le", "sin\u00b7nend", "sitz'", "ich", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen sommerlichen Abend", "tokens": ["So", "man\u00b7chen", "som\u00b7mer\u00b7li\u00b7chen", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Gl\u00fcck der Einsamkeit mich labend", "tokens": ["Am", "Gl\u00fcck", "der", "Ein\u00b7sam\u00b7keit", "mich", "la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gest\u00f6rt von keinem Menschenwort,", "tokens": ["Ge\u00b7st\u00f6rt", "von", "kei\u00b7nem", "Men\u00b7schen\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verkehrend nur mit den Gedanken", "tokens": ["Ver\u00b7keh\u00b7rend", "nur", "mit", "den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die, wenn der D\u00e4mon in mir spricht,", "tokens": ["Die", ",", "wenn", "der", "D\u00e4\u00b7mon", "in", "mir", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die bewegte Seele schwanken", "tokens": ["Durch", "die", "be\u00b7weg\u00b7te", "See\u00b7le", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt dunkel und jetzt wieder licht! \u2013", "tokens": ["Jetzt", "dun\u00b7kel", "und", "jetzt", "wie\u00b7der", "licht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So sa\u00df ich gestern erst, allein", "tokens": ["So", "sa\u00df", "ich", "ge\u00b7stern", "erst", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie immer, in den \u00f6den Hallen", "tokens": ["Wie", "im\u00b7mer", ",", "in", "den", "\u00f6\u00b7den", "Hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df an mir vor\u00fcberwallen", "tokens": ["Und", "lie\u00df", "an", "mir", "vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Phantast'scher Bilder bunte Reihn.", "tokens": ["Phan\u00b7tast'\u00b7scher", "Bil\u00b7der", "bun\u00b7te", "Reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich f\u00fchlte sie mich \u00fcberkommen,", "tokens": ["Ich", "f\u00fchl\u00b7te", "sie", "mich", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich \u00fcberw\u00e4lt'gen je und je;", "tokens": ["Mich", "\u00fc\u00b7ber\u00b7w\u00e4lt'\u00b7gen", "je", "und", "je", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Herz war schwer und war beklommen", "tokens": ["Mein", "Herz", "war", "schwer", "und", "war", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem r\u00e4tselhaften Weh.", "tokens": ["Von", "ei\u00b7nem", "r\u00e4t\u00b7sel\u00b7haf\u00b7ten", "Weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gedenken mu\u00dft' ich schwermutvoll,", "tokens": ["Ge\u00b7den\u00b7ken", "mu\u00dft'", "ich", "schwer\u00b7mut\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Geistes wachem Tr\u00e4umen,", "tokens": ["In", "mei\u00b7nes", "Geis\u00b7tes", "wa\u00b7chem", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zeiten, wo in diesen R\u00e4umen", "tokens": ["Der", "Zei\u00b7ten", ",", "wo", "in", "die\u00b7sen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der rasche Strom des Lebens schwoll!", "tokens": ["Der", "ra\u00b7sche", "Strom", "des", "Le\u00b7bens", "schwoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der l\u00e4ngst zu Staub zerfallnen Herzen,", "tokens": ["Der", "l\u00e4ngst", "zu", "Staub", "zer\u00b7fall\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die bang und freudig hier gepocht,", "tokens": ["Die", "bang", "und", "freu\u00b7dig", "hier", "ge\u00b7pocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Wonnen bald und bald von Schmerzen", "tokens": ["Von", "Won\u00b7nen", "bald", "und", "bald", "von", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Lust und Jammer unterjocht!", "tokens": ["Von", "Lust", "und", "Jam\u00b7mer", "un\u00b7ter\u00b7jocht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was schmeichelnd und was ungelind", "tokens": ["Was", "schmei\u00b7chelnd", "und", "was", "un\u00b7ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "PWS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wechselnd in ihr Sein verwoben,", "tokens": ["Sich", "wech\u00b7selnd", "in", "ihr", "Sein", "ver\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Lieben, Hassen ist zerstoben,", "tokens": ["Ihr", "Lie\u00b7ben", ",", "Has\u00b7sen", "ist", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dahingegangen in den Wind!", "tokens": ["Da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", "in", "den", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wonach sie hei\u00dfverlangend stritten", "tokens": ["Wo\u00b7nach", "sie", "hei\u00df\u00b7ver\u00b7lan\u00b7gend", "strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis zu dem letzten Kampf und Hauch,", "tokens": ["Bis", "zu", "dem", "letz\u00b7ten", "Kampf", "und", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was sie genossen, was sie litten,", "tokens": ["Was", "sie", "ge\u00b7nos\u00b7sen", ",", "was", "sie", "lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entschwunden ist's, verweht wie Rauch!", "tokens": ["Ent\u00b7schwun\u00b7den", "ist's", ",", "ver\u00b7weht", "wie", "Rauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie Rauch? Da sah ich an der Wand", "tokens": ["Wie", "Rauch", "?", "Da", "sah", "ich", "an", "der", "Wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An des Kamines spitzem Bogen", "tokens": ["An", "des", "Ka\u00b7mi\u00b7nes", "spit\u00b7zem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die dunkle Spur, die hier gezogen", "tokens": ["Die", "dunk\u00b7le", "Spur", ",", "die", "hier", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Herdes halbvergl\u00fchter Brand.", "tokens": ["Des", "Her\u00b7des", "halb\u00b7ver\u00b7gl\u00fch\u00b7ter", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich fuhr empor, von Grau'n durchschauert!", "tokens": ["Ich", "fuhr", "em\u00b7por", ",", "von", "Grau'n", "durch\u00b7schau\u00b7ert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ersch\u00fcttert sah ich Gl\u00fcck und Leid", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "sah", "ich", "Gl\u00fcck", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Menschenseele \u00fcberdauert", "tokens": ["Der", "Men\u00b7schen\u00b7see\u00b7le", "\u00fc\u00b7berd\u00b7au\u00b7ert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Sinnbild der Verg\u00e4nglichkeit!", "tokens": ["Vom", "Sinn\u00b7bild", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr Toten! rief ich, tief und fest", "tokens": ["Ihr", "To\u00b7ten", "!", "rief", "ich", ",", "tief", "und", "fest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun schlummernd in den Grabeshallen,", "tokens": ["Nun", "schlum\u00b7mernd", "in", "den", "Gra\u00b7bes\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seht hier von eurem Erdenwallen", "tokens": ["Seht", "hier", "von", "eu\u00b7rem", "Er\u00b7den\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten, einz'gen Erdenrest!", "tokens": ["Den", "letz\u00b7ten", ",", "einz'\u00b7gen", "Er\u00b7den\u00b7rest", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Rauch, der eure Hallen schw\u00e4rzte,", "tokens": ["Der", "Rauch", ",", "der", "eu\u00b7re", "Hal\u00b7len", "schw\u00e4rz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er zeigt sich noch der Enkel Blick, \u2013", "tokens": ["Er", "zeigt", "sich", "noch", "der", "En\u00b7kel", "Blick", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "p