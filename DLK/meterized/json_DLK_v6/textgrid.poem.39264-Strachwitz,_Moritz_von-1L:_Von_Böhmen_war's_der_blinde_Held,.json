{"textgrid.poem.39264": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von B\u00f6hmen war's der blinde Held,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von B\u00f6hmen war's der blinde Held,", "tokens": ["Von", "B\u00f6h\u00b7men", "wa\u00b7r's", "der", "blin\u00b7de", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der hielt am Bergessaum,", "tokens": ["Der", "hielt", "am", "Ber\u00b7ges\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es brandet die Schlacht wohl an sein Ohr", "tokens": ["Es", "bran\u00b7det", "die", "Schlacht", "wohl", "an", "sein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie blutiger Todestraum.", "tokens": ["Wie", "blu\u00b7ti\u00b7ger", "To\u00b7des\u00b7traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er horcht und lauscht dem Kampfesgetos", "tokens": ["Er", "horcht", "und", "lauscht", "dem", "Kamp\u00b7fes\u00b7ge\u00b7tos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie verklungenem Zauberschall,", "tokens": ["Wie", "ver\u00b7klun\u00b7ge\u00b7nem", "Zau\u00b7ber\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er neigt sich leis von des Sattels Saum", "tokens": ["Er", "neigt", "sich", "leis", "von", "des", "Sat\u00b7tels", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem t\u00f6nenden Widerhall.", "tokens": ["Dem", "t\u00f6\u00b7nen\u00b7den", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbauf, sag' mir an! Du Knappe brav,", "tokens": ["\u00bb", "auf", ",", "sag'", "mir", "an", "!", "Du", "Knap\u00b7pe", "brav", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fliegt Frankreichs Banner im Sturm?", "tokens": ["Fliegt", "Fran\u00b7kreichs", "Ban\u00b7ner", "im", "Sturm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Liegt England nieder im Blute rot,", "tokens": ["Liegt", "En\u00b7gland", "nie\u00b7der", "im", "Blu\u00b7te", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der trotzige Meereswurm?\u00ab", "tokens": ["Der", "trot\u00b7zi\u00b7ge", "Mee\u00b7res\u00b7wurm", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbhoch b\u00e4umt er auf, Herr K\u00f6nig mein,", "tokens": ["\u00bb", "hoch", "b\u00e4umt", "er", "auf", ",", "Herr", "K\u00f6\u00b7nig", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lilie bleicht im Tod,", "tokens": ["Die", "Li\u00b7lie", "bleicht", "im", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr England ficht ein Ritter schwarz,", "tokens": ["F\u00fcr", "En\u00b7gland", "ficht", "ein", "Rit\u00b7ter", "schwarz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bringt uns grimmige Not.\u00ab", "tokens": ["Der", "bringt", "uns", "grim\u00b7mi\u00b7ge", "Not", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Da hebt sich hoch der Heldengreis", "tokens": ["Da", "hebt", "sich", "hoch", "der", "Hel\u00b7den\u00b7greis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der Jugend erloschener Kraft,", "tokens": ["In", "der", "Ju\u00b7gend", "er\u00b7lo\u00b7sche\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Aus der Augen H\u00f6hlung stumm und tot", "tokens": ["Aus", "der", "Au\u00b7gen", "H\u00f6h\u00b7lung", "stumm", "und", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dringt's leuchtend zauberhaft.", "tokens": ["Dringt's", "leuch\u00b7tend", "zau\u00b7ber\u00b7haft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da f\u00e4hrt die Faust an das alte Schwert,", "tokens": ["Da", "f\u00e4hrt", "die", "Faust", "an", "das", "al\u00b7te", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da Helm und Panzer erklingt,", "tokens": ["Da", "Helm", "und", "Pan\u00b7zer", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da haut er wild in die Nebelluft,", "tokens": ["Da", "haut", "er", "wild", "in", "die", "Ne\u00b7bel\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df leuchtender Funke springt.", "tokens": ["Da\u00df", "leuch\u00b7ten\u00b7der", "Fun\u00b7ke", "springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbsoll Frankreich strecken, f\u00fcr das ich focht,", "tokens": ["\u00bb", "soll", "Fran\u00b7kreich", "stre\u00b7cken", ",", "f\u00fcr", "das", "ich", "focht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NE", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die blutige Klinge scharf?", "tokens": ["Die", "blu\u00b7ti\u00b7ge", "Klin\u00b7ge", "scharf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Soll singend br\u00fcllen der Inselleu,", "tokens": ["Soll", "sin\u00b7gend", "br\u00fcl\u00b7len", "der", "In\u00b7sel\u00b7leu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der's zornig in St\u00fccke warf?\u00ab", "tokens": ["Der's", "zor\u00b7nig", "in", "St\u00fc\u00b7cke", "warf", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "ADJD", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "\u00bbha! w\u00e4re noch markig der greise Arm,", "tokens": ["\u00bb", "ha", "!", "w\u00e4\u00b7re", "noch", "mar\u00b7kig", "der", "grei\u00b7se", "Arm", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch wuchtig der Schwerterschlag,", "tokens": ["Noch", "wuch\u00b7tig", "der", "Schwer\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollte bestehen den Ritter schwarz,", "tokens": ["Ich", "woll\u00b7te", "be\u00b7ste\u00b7hen", "den", "Rit\u00b7ter", "schwarz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es w\u00e4re sein letzter Tag.\u00ab", "tokens": ["Es", "w\u00e4\u00b7re", "sein", "letz\u00b7ter", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbha! K\u00f6nnt' ich noch schauen der Sonne Gold,", "tokens": ["\u00bb", "ha", "!", "K\u00f6nnt'", "ich", "noch", "schau\u00b7en", "der", "Son\u00b7ne", "Gold", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Noch schauen des Schwertes Strahl,", "tokens": ["Noch", "schau\u00b7en", "des", "Schwer\u00b7tes", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollte zerrei\u00dfen des L\u00f6wen Herz", "tokens": ["Ich", "woll\u00b7te", "zer\u00b7rei\u00b7\u00dfen", "des", "L\u00f6\u00b7wen", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In zuckender Todesqual.\u00ab", "tokens": ["In", "zu\u00b7cken\u00b7der", "To\u00b7des\u00b7qual", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbwill fechten den letzten Ritterstreit,", "tokens": ["\u00bb", "will", "fech\u00b7ten", "den", "letz\u00b7ten", "Rit\u00b7ter\u00b7streit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will f\u00fchren den letzten Hieb,", "tokens": ["Will", "f\u00fch\u00b7ren", "den", "letz\u00b7ten", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O! nimm mich auf in die Arme dein,", "tokens": ["O", "!", "nimm", "mich", "auf", "in", "die", "Ar\u00b7me", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "APPR", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Du heilige Jungfrau lieb!\u00ab", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Jung\u00b7frau", "lieb", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "In die Mitte nehmen die Ritter ihn,", "tokens": ["In", "die", "Mit\u00b7te", "neh\u00b7men", "die", "Rit\u00b7ter", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie koppeln die Rosse all',", "tokens": ["Sie", "kop\u00b7peln", "die", "Ros\u00b7se", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sprengen hinunter zum Schlachtgewog'", "tokens": ["Sie", "spren\u00b7gen", "hin\u00b7un\u00b7ter", "zum", "Schlacht\u00b7ge\u00b7wo\u00b7g'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom blutigen H\u00fcgelwall.", "tokens": ["Vom", "blu\u00b7ti\u00b7gen", "H\u00fc\u00b7gel\u00b7wall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Da f\u00fchrte der K\u00f6nig den letzten Sto\u00df,", "tokens": ["Da", "f\u00fchr\u00b7te", "der", "K\u00f6\u00b7nig", "den", "letz\u00b7ten", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis alle das Schwert verschlang,", "tokens": ["Bis", "al\u00b7le", "das", "Schwert", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da f\u00fchrte der K\u00f6nig den letzten Streich,", "tokens": ["Da", "f\u00fchr\u00b7te", "der", "K\u00f6\u00b7nig", "den", "letz\u00b7ten", "Streich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bis das mutige Herz ihm sprang.", "tokens": ["Bis", "das", "mu\u00b7ti\u00b7ge", "Herz", "ihm", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}