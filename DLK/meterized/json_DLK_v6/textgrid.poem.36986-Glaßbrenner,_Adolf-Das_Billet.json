{"textgrid.poem.36986": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Das Billet", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfremdling, nie der Liebe noch", "tokens": ["\u00bb", "fremd\u00b7ling", ",", "nie", "der", "Lie\u00b7be", "noch"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADV", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gedacht' ich,", "tokens": ["ge\u00b7dacht'", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Eh' gesehen ich in Deiner", "tokens": ["Eh'", "ge\u00b7se\u00b7hen", "ich", "in", "Dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPER", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pracht Dich!", "tokens": ["Pracht", "Dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Fr\u00fchinge erlebte Zehn und", "tokens": ["Fr\u00fc\u00b7hin\u00b7ge", "er\u00b7leb\u00b7te", "Zehn", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "CARD", "KON"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Acht ich;", "tokens": ["Acht", "ich", ";"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Schaukelnd in der Jugend bunten", "tokens": ["Schau\u00b7kelnd", "in", "der", "Ju\u00b7gend", "bun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jacht mich,", "tokens": ["Jacht", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Sah ich meine Tage flie\u00dfen;", "tokens": ["Sah", "ich", "mei\u00b7ne", "Ta\u00b7ge", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "macht' ich", "tokens": ["macht'", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Sch\u00f6ne Reime, spielte, sang und", "tokens": ["Sch\u00f6\u00b7ne", "Rei\u00b7me", ",", "spiel\u00b7te", ",", "sang", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "lacht' ich,", "tokens": ["lacht'", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und es st\u00f6rte keines Kummers", "tokens": ["Und", "es", "st\u00f6r\u00b7te", "kei\u00b7nes", "Kum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Fracht mich.", "tokens": ["Fracht", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Da nahmst Du, den in der fremden", "tokens": ["Da", "nahmst", "Du", ",", "den", "in", "der", "frem\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Tracht ich", "tokens": ["Tracht", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "An dem Fenster sah, in Deine", "tokens": ["An", "dem", "Fens\u00b7ter", "sah", ",", "in", "Dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Acht mich,", "tokens": ["Acht", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Fesseltest durch Deiner Reize", "tokens": ["Fes\u00b7sel\u00b7test", "durch", "Dei\u00b7ner", "Rei\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Macht mich,", "tokens": ["Macht", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Schlepptest in der Lebenssorgen", "tokens": ["Schlepp\u00b7test", "in", "der", "Le\u00b7bens\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wacht mich,", "tokens": ["Wacht", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Wo nun seufzend liege Tag und", "tokens": ["Wo", "nun", "seuf\u00b7zend", "lie\u00b7ge", "Tag", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ADJA", "NN", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nacht ich,", "tokens": ["Nacht", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Rufend, w\u00e4hrend Thr\u00e4nen schleichen", "tokens": ["Ru\u00b7fend", ",", "w\u00e4h\u00b7rend", "Thr\u00e4\u00b7nen", "schlei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "sacht sich", "tokens": ["sacht", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Aus dem Aug', durch Tausend O und", "tokens": ["Aus", "dem", "Aug'", ",", "durch", "Tau\u00b7send", "O", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "NE", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ach Dich,", "tokens": ["Ach", "Dich", ","], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Dich, den J\u00fcngling, der so weit", "tokens": ["Dich", ",", "den", "J\u00fcng\u00b7ling", ",", "der", "so", "weit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "gebracht mich!", "tokens": ["ge\u00b7bracht", "mich", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Lange k\u00e4mpfte in der Zweifel", "tokens": ["Lan\u00b7ge", "k\u00e4mpf\u00b7te", "in", "der", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schlacht ich:", "tokens": ["Schlacht", "ich", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Ob nicht tr\u00e4f' ein niedriger", "tokens": ["Ob", "nicht", "tr\u00e4f'", "ein", "nied\u00b7ri\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "VVFIN", "ART", "ADJA"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.34": {"text": "Verdacht mich", "tokens": ["Ver\u00b7dacht", "mich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Dieses Briefes wegen, doch jetzt", "tokens": ["Die\u00b7ses", "Brie\u00b7fes", "we\u00b7gen", ",", "doch", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "APPR", "$,", "ADV", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "acht' ich", "tokens": ["acht'", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Mich als Sieg'rin rufend: offen", "tokens": ["Mich", "als", "Sieg'\u00b7rin", "ru\u00b7fend", ":", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "KOUS", "NN", "VVPP", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "tracht' ich", "tokens": ["tracht'", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Darnach, da\u00df in Deines Herzens", "tokens": ["Dar\u00b7nach", ",", "da\u00df", "in", "Dei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Schacht sich", "tokens": ["Schacht", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Gleiche Gluth entz\u00fcnde, wie", "tokens": ["Glei\u00b7che", "Gluth", "ent\u00b7z\u00fcn\u00b7de", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "entfacht sich", "tokens": ["ent\u00b7facht", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "In dem meinen! Und nun, Theurer,", "tokens": ["In", "dem", "mei\u00b7nen", "!", "Und", "nun", ",", "Theu\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$.", "KON", "ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "schmacht' ich,", "tokens": ["schmacht'", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Bis Du kurz die Antwort schreibst:", "tokens": ["Bis", "Du", "kurz", "die", "Ant\u00b7wort", "schreibst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "\u203aes macht sich!\u2039", "tokens": ["\u203a", "es", "macht", "sich", "!", "\u2039"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Nachdem als Herr den Inhalt des Billetes", "tokens": ["Nach\u00b7dem", "als", "Herr", "den", "In\u00b7halt", "des", "Bil\u00b7le\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pflichtschuldigst ich der Dienerin erkl\u00e4rt,", "tokens": ["Pflicht\u00b7schul\u00b7digst", "ich", "der", "Die\u00b7ne\u00b7rin", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief ich mit wahrhaft k\u00f6niglicher W\u00fcrde:", "tokens": ["Rief", "ich", "mit", "wahr\u00b7haft", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "W\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdie Bitte dieser Dame sei gew\u00e4hrt!\u00ab", "tokens": ["\u00bb", "die", "Bit\u00b7te", "die\u00b7ser", "Da\u00b7me", "sei", "ge\u00b7w\u00e4hrt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PDAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}