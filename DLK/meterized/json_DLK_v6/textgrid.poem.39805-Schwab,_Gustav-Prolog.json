{"textgrid.poem.39805": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu feiern einen Schmaus dem Ohre", "tokens": ["Zu", "fei\u00b7ern", "ei\u00b7nen", "Schmaus", "dem", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchzogen wir bekr\u00e4nzte Thore,", "tokens": ["Durch\u00b7zo\u00b7gen", "wir", "be\u00b7kr\u00e4nz\u00b7te", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich K\u00e4mpfern auf olymp'scher Bahn;", "tokens": ["Gleich", "K\u00e4mp\u00b7fern", "auf", "o\u00b7lym\u00b7p'\u00b7scher", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Musen, wie in alten Tagen,", "tokens": ["Den", "Mu\u00b7sen", ",", "wie", "in", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist hier ein Lager aufgeschlagen,", "tokens": ["Ist", "hier", "ein", "La\u00b7ger", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Wettsang str\u00f6mt das Volk heran.", "tokens": ["Zum", "Wett\u00b7sang", "str\u00f6mt", "das", "Volk", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie hei\u00dft die Stadt, zu der wir wallen,", "tokens": ["Wie", "hei\u00dft", "die", "Stadt", ",", "zu", "der", "wir", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren Th\u00fcrmen, ihren Hallen?", "tokens": ["Mit", "ih\u00b7ren", "Th\u00fcr\u00b7men", ",", "ih\u00b7ren", "Hal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O h\u00e4tt' ich einen Pindarsmund!", "tokens": ["O", "h\u00e4tt'", "ich", "ei\u00b7nen", "Pin\u00b7dars\u00b7mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fl\u00f6sse mir am Tag der Lieder", "tokens": ["Wie", "fl\u00f6s\u00b7se", "mir", "am", "Tag", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Hymne von der Lippe nieder,", "tokens": ["Die", "Hym\u00b7ne", "von", "der", "Lip\u00b7pe", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und th\u00e4t' ihr Lob den Horchern kund!", "tokens": ["Und", "th\u00e4t'", "ihr", "Lob", "den", "Hor\u00b7chern", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erst eilt des Neckars leise Welle", "tokens": ["Erst", "eilt", "des", "Ne\u00b7ckars", "lei\u00b7se", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vorbei an einer kleinen Zelle,", "tokens": ["Vor\u00b7bei", "an", "ei\u00b7ner", "klei\u00b7nen", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drin ruht ein Heiligengebein:", "tokens": ["Drin", "ruht", "ein", "Hei\u00b7li\u00b7gen\u00b7ge\u00b7bein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schon ist es ein Platz der Ehren,", "tokens": ["Doch", "schon", "ist", "es", "ein", "Platz", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und mit des Reiches Glanze kehren", "tokens": ["Und", "mit", "des", "Rei\u00b7ches", "Glan\u00b7ze", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schon deutsche K\u00f6nige hier ein.", "tokens": ["Schon", "deut\u00b7sche", "K\u00f6\u00b7ni\u00b7ge", "hier", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und bald, wie Staufens gro\u00dfen S\u00f6hnen", "tokens": ["Und", "bald", ",", "wie", "Stau\u00b7fens", "gro\u00b7\u00dfen", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verliehen wird ihr Haupt zu kr\u00f6nen,", "tokens": ["Ver\u00b7lie\u00b7hen", "wird", "ihr", "Haupt", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun die Schwaben Meister sind,", "tokens": ["Und", "nun", "die", "Schwa\u00b7ben", "Meis\u00b7ter", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da dehnet sich die enge Klause,", "tokens": ["Da", "deh\u00b7net", "sich", "die", "en\u00b7ge", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da wurdest du im K\u00f6nigshause,", "tokens": ["Da", "wur\u00b7dest", "du", "im", "K\u00f6\u00b7nigs\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Stadt! ein sorgenfreies Kind.", "tokens": ["O", "Stadt", "!", "ein", "sor\u00b7gen\u00b7frei\u00b7es", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Rothbart baut an deinem Thurme,", "tokens": ["Der", "Roth\u00b7bart", "baut", "an", "dei\u00b7nem", "Thur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Philipp nimmt in Kampf und Sturme", "tokens": ["Der", "Phi\u00b7lipp", "nimmt", "in", "Kampf", "und", "Stur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch deiner jungen Mauern wahr.", "tokens": ["Doch", "dei\u00b7ner", "jun\u00b7gen", "Mau\u00b7ern", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des gr\u00f6\u00dften Friedrichs Adler schm\u00fccket", "tokens": ["Des", "gr\u00f6\u00df\u00b7ten", "Fried\u00b7richs", "Ad\u00b7ler", "schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein graues Thor, und unverr\u00fccket", "tokens": ["Dein", "grau\u00b7es", "Thor", ",", "und", "un\u00b7ver\u00b7r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bewacht es noch sein L\u00f6wenpaar.", "tokens": ["Be\u00b7wacht", "es", "noch", "sein", "L\u00f6\u00b7wen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Zeit des schw\u00e4bischen Gesanges!", "tokens": ["O", "Zeit", "des", "schw\u00e4\u00b7bi\u00b7schen", "Ge\u00b7san\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da rauschte br\u00fcderlichen Klanges", "tokens": ["Da", "rauschte", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Klan\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Lied zum Neckarwellenschlag;", "tokens": ["Das", "Lied", "zum", "Ne\u00b7ckar\u00b7wel\u00b7len\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Meister war auch dir gegeben,", "tokens": ["Ein", "Meis\u00b7ter", "war", "auch", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du Stadt der Bl\u00fcten und der Reben,", "tokens": ["Du", "Stadt", "der", "Bl\u00fc\u00b7ten", "und", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sang von deinem Maientag.", "tokens": ["Der", "sang", "von", "dei\u00b7nem", "Mai\u00b7en\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch dieses Haus ward jetzt gegr\u00fcndet,", "tokens": ["Auch", "die\u00b7ses", "Haus", "ward", "jetzt", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem sich unser Lied entz\u00fcndet;", "tokens": ["In", "dem", "sich", "un\u00b7ser", "Lied", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Staufe Heinrich baut' es aus;", "tokens": ["Der", "Stau\u00b7fe", "Hein\u00b7rich", "baut'", "es", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch fiel er vom geraubten Throne,", "tokens": ["Doch", "fiel", "er", "vom", "ge\u00b7raub\u00b7ten", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da nahm sein Weib vom Haupt die Krone,", "tokens": ["Da", "nahm", "sein", "Weib", "vom", "Haupt", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und trat mit ihr ", "tokens": ["Und", "trat", "mit", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "\u00bbihr M\u00f6nche, gebt dies Gold den Armen,", "tokens": ["\u00bb", "ihr", "M\u00f6n\u00b7che", ",", "gebt", "dies", "Gold", "den", "Ar\u00b7men", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr M\u00f6nche, flehet um Erbarmen,", "tokens": ["Ihr", "M\u00f6n\u00b7che", ",", "fle\u00b7het", "um", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fleht f\u00fcr die Seele meines Herrn!\u00ab \u2013", "tokens": ["Fleht", "f\u00fcr", "die", "See\u00b7le", "mei\u00b7nes", "Herrn", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wert ist dies Weib, da\u00df man sein denket,", "tokens": ["Wert", "ist", "dies", "Weib", ",", "da\u00df", "man", "sein", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "NN", "$,", "KOUS", "PIS", "VAINF", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Das auch der Krone Gold verschenket,", "tokens": ["Das", "auch", "der", "Kro\u00b7ne", "Gold", "ver\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als unterging der Ehre Stern.", "tokens": ["Als", "un\u00b7ter\u00b7ging", "der", "Eh\u00b7re", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Bald bleichten, Stadt, auch deine Sterne,", "tokens": ["Bald", "bleich\u00b7ten", ",", "Stadt", ",", "auch", "dei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als nun ein Henker in der Ferne", "tokens": ["Als", "nun", "ein", "Hen\u00b7ker", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Beil f\u00fcr Staufens Enkel schliff;", "tokens": ["Das", "Beil", "f\u00fcr", "Stau\u00b7fens", "En\u00b7kel", "schliff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sang der Meister deiner Schule:", "tokens": ["Da", "sang", "der", "Meis\u00b7ter", "dei\u00b7ner", "Schu\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er warnte Gott auf seinem Stuhle,", "tokens": ["Er", "warn\u00b7te", "Gott", "auf", "sei\u00b7nem", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Habsburg nach der Krone griff.", "tokens": ["Als", "Habs\u00b7burg", "nach", "der", "Kro\u00b7ne", "griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN