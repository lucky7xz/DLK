{"textgrid.poem.25213": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es tr\u00e4umt aus d\u00fcsterm Felsenschacht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4umt aus d\u00fcsterm Felsenschacht", "tokens": ["Es", "tr\u00e4umt", "aus", "d\u00fcs\u00b7term", "Fel\u00b7sen\u00b7schacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein totenstiller See", "tokens": ["Ein", "to\u00b7tens\u00b7til\u00b7ler", "See"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur grenzenlosen Sternenpracht/", "tokens": ["Zur", "gren\u00b7zen\u00b7lo\u00b7sen", "Ster\u00b7nen\u00b7pracht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Seligkeit und Weh!", "tokens": ["O", "Se\u00b7lig\u00b7keit", "und", "Weh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00dft taumeln mich, ihr Himmelsh\u00f6hn,", "tokens": ["La\u00dft", "tau\u00b7meln", "mich", ",", "ihr", "Him\u00b7mels\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versinken ganz in Schau!", "tokens": ["Ver\u00b7sin\u00b7ken", "ganz", "in", "Schau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Funkelstern, so br\u00e4utlich sch\u00f6n", "tokens": ["Mein", "Fun\u00b7kels\u00b7tern", ",", "so", "br\u00e4ut\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Perle Tau!", "tokens": ["Wie", "ei\u00b7ne", "Per\u00b7le", "Tau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bleibst du, Engel, weltenfern,", "tokens": ["Und", "bleibst", "du", ",", "En\u00b7gel", ",", "wel\u00b7ten\u00b7fern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$,", "PWAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Streu deinen Silberschein/", "tokens": ["Streu", "dei\u00b7nen", "Sil\u00b7ber\u00b7schein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Seelengleichnis/ keuscher Stern,", "tokens": ["Dein", "See\u00b7len\u00b7gleich\u00b7nis", "/", "keu\u00b7scher", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meine Tiefen ein.", "tokens": ["In", "mei\u00b7ne", "Tie\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In meine Tiefen lockt ein Grund/", "tokens": ["In", "mei\u00b7ne", "Tie\u00b7fen", "lockt", "ein", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O find ihn, Sternenbraut/", "tokens": ["O", "find", "ihn", ",", "Ster\u00b7nen\u00b7braut", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Erd und Himmel Mund an Mund", "tokens": ["Wo", "Erd", "und", "Him\u00b7mel", "Mund", "an", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur ewgen Ruh sich traut.", "tokens": ["Zur", "ew\u00b7gen", "Ruh", "sich", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}