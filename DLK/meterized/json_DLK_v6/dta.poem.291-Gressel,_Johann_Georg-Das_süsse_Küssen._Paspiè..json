{"dta.poem.291": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das s\u00fcsse K\u00fcssen.  \n  Paspi\u00e8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es schmecken die Lippen der Damen so s\u00fcsse/", "tokens": ["Es", "schme\u00b7cken", "die", "Lip\u00b7pen", "der", "Da\u00b7men", "so", "s\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und reichen im K\u00fcssen ein himmlisches Brodt/", "tokens": ["Und", "rei\u00b7chen", "im", "K\u00fcs\u00b7sen", "ein", "himm\u00b7li\u00b7sches", "Brodt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Man weidet die Seelen durch wechselnde K\u00fcsse/", "tokens": ["Man", "wei\u00b7det", "die", "See\u00b7len", "durch", "wech\u00b7seln\u00b7de", "K\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und solche entziehen befordert den Tod.", "tokens": ["Und", "sol\u00b7che", "ent\u00b7zie\u00b7hen", "be\u00b7for\u00b7dert", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es tr\u00e4uffet ein ", "tokens": ["Es", "tr\u00e4uf\u00b7fet", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und tausend entz\u00fcckende L\u00fcste darbey:", "tokens": ["Und", "tau\u00b7send", "ent\u00b7z\u00fc\u00b7cken\u00b7de", "L\u00fcs\u00b7te", "dar\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wem pfleget dis Manna nicht stets zugefallen", "tokens": ["Wem", "pfle\u00b7get", "dis", "Man\u00b7na", "nicht", "stets", "zu\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDS", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie eckel sonst immer im andern er sey?", "tokens": ["Wie", "ec\u00b7kel", "sonst", "im\u00b7mer", "im", "an\u00b7dern", "er", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "APPRART", "ADJA", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Es m\u00fcssen den Br\u00fcsten die Lippen doch weichen/", "tokens": ["Es", "m\u00fcs\u00b7sen", "den", "Br\u00fcs\u00b7ten", "die", "Lip\u00b7pen", "doch", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Weil mehre Vergn\u00fcgung bey ihnen sich findt/", "tokens": ["Weil", "meh\u00b7re", "Ver\u00b7gn\u00fc\u00b7gung", "bey", "ih\u00b7nen", "sich", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "PRF", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da kan man die ", "tokens": ["Da", "kan", "man", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die uns in dem ", "tokens": ["Die", "uns", "in", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es hitzen und k\u00fchlen die ", "tokens": ["Es", "hit\u00b7zen", "und", "k\u00fch\u00b7len", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und dieses erquickende Schnee-Geb\u00fcrg macht", "tokens": ["Und", "die\u00b7ses", "er\u00b7qui\u00b7cken\u00b7de", "Schnee\u00b7Ge\u00b7b\u00fcrg", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Da\u00df man in die Flam\u0303en nur w\u00fcnschet zu fallen/", "tokens": ["Da\u00df", "man", "in", "die", "Flam\u0303en", "nur", "w\u00fcn\u00b7schet", "zu", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Weil einen erw\u00fcnschete K\u00fchlung anlacht.", "tokens": ["Weil", "ei\u00b7nen", "er\u00b7w\u00fcn\u00b7sche\u00b7te", "K\u00fch\u00b7lung", "an\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr Lippen und Br\u00fcste ihr schmecket zwar s\u00fcsse/", "tokens": ["Ihr", "Lip\u00b7pen", "und", "Br\u00fcs\u00b7te", "ihr", "schme\u00b7cket", "zwar", "s\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PPER", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und zinset den Kostenden v\u00f6llige Lust/", "tokens": ["Und", "zin\u00b7set", "den", "Kos\u00b7ten\u00b7den", "v\u00f6l\u00b7li\u00b7ge", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch machen des Schoosses entz\u00fcckende K\u00fcsse", "tokens": ["Doch", "ma\u00b7chen", "des", "Schoos\u00b7ses", "ent\u00b7z\u00fc\u00b7cken\u00b7de", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Uns gr\u00f6sser Vergn\u00fcgen und Anmuth bewust.", "tokens": ["Uns", "gr\u00f6s\u00b7ser", "Ver\u00b7gn\u00fc\u00b7gen", "und", "An\u00b7muth", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da bricht man die Fr\u00fcchte vom Baume des Lebens/", "tokens": ["Da", "bricht", "man", "die", "Fr\u00fcch\u00b7te", "vom", "Bau\u00b7me", "des", "Le\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und trincket den fliessenden Honig darbey.", "tokens": ["Und", "trin\u00b7cket", "den", "flies\u00b7sen\u00b7den", "Ho\u00b7nig", "dar\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Man suchet das Manna da niemahls vergebens/", "tokens": ["Man", "su\u00b7chet", "das", "Man\u00b7na", "da", "nie\u00b7mahls", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df es nicht zu finden im Uberflu\u00df sey.", "tokens": ["Da\u00df", "es", "nicht", "zu", "fin\u00b7den", "im", "U\u00b7ber\u00b7flu\u00df", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKZU", "VVINF", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}