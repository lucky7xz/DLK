{"dta.poem.2316": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Br\u00fcderschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft mich lachen, la\u00dft mich scherzen,", "tokens": ["La\u00dft", "mich", "la\u00b7chen", ",", "la\u00dft", "mich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn was hilft mir Gram und Sorge.", "tokens": ["Denn", "was", "hilft", "mir", "Gram", "und", "Sor\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg, verdammter Schwarm der Grillen!", "tokens": ["Weg", ",", "ver\u00b7damm\u00b7ter", "Schwarm", "der", "Gril\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weg, und schw\u00e4rme schnell und sicher", "tokens": ["Weg", ",", "und", "schw\u00e4r\u00b7me", "schnell", "und", "si\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach den Feinden meiner Freude.", "tokens": ["Nach", "den", "Fein\u00b7den", "mei\u00b7ner", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm! du Stifter des Vergn\u00fcgens,", "tokens": ["Komm", "!", "du", "Stif\u00b7ter", "des", "Ver\u00b7gn\u00fc\u00b7gens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Komm! du Freund von Lust und Br\u00fcdern,", "tokens": ["Komm", "!", "du", "Freund", "von", "Lust", "und", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Komm! es soll der Saft im Glase,", "tokens": ["Komm", "!", "es", "soll", "der", "Saft", "im", "Gla\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den du aus den Trauben dr\u00fckkest,", "tokens": ["Den", "du", "aus", "den", "Trau\u00b7ben", "dr\u00fck\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Grillen, Gram und Ha\u00df ertr\u00e4nken.", "tokens": ["Gril\u00b7len", ",", "Gram", "und", "Ha\u00df", "er\u00b7tr\u00e4n\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Trinke mit, vergn\u00fcgter Vater,", "tokens": ["Trin\u00b7ke", "mit", ",", "ver\u00b7gn\u00fcg\u00b7ter", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bringe mir don vollen R\u00f6mer,", "tokens": ["Brin\u00b7ge", "mir", "don", "vol\u00b7len", "R\u00f6\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich will ihn dem Nachbar bringen,", "tokens": ["Ich", "will", "ihn", "dem", "Nach\u00b7bar", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Und der Nachbar soll ihn nehmen", "tokens": ["Und", "der", "Nach\u00b7bar", "soll", "ihn", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ihn andern Nachbarn bringen,", "tokens": ["Und", "ihn", "an\u00b7dern", "Nach\u00b7barn", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Bis ihn dir der letzte Nachbar", "tokens": ["Bis", "ihn", "dir", "der", "letz\u00b7te", "Nach\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Voll und perlend wieder bringet.", "tokens": ["Voll", "und", "per\u00b7lend", "wie\u00b7der", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dann sollt du von forne trinken,", "tokens": ["Dann", "sollt", "du", "von", "for\u00b7ne", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Und dann werd ich wieder dursten,", "tokens": ["Und", "dann", "werd", "ich", "wie\u00b7der", "durs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und ihn freudig wieder leeren,", "tokens": ["Und", "ihn", "freu\u00b7dig", "wie\u00b7der", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und, durch deinen Ku\u00df berechtigt,", "tokens": ["Und", ",", "durch", "dei\u00b7nen", "Ku\u00df", "be\u00b7rech\u00b7tigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Werd\u2019 ich mich zum Bruder trinken.", "tokens": ["Werd'", "ich", "mich", "zum", "Bru\u00b7der", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Bruder Weingott! ia, ich merke,", "tokens": ["Bru\u00b7der", "Wein\u00b7gott", "!", "i\u00b7a", ",", "ich", "mer\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NE", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Ja, ich bin bereits dein Bruder.", "tokens": ["Ja", ",", "ich", "bin", "be\u00b7reits", "dein", "Bru\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "K\u00fcsse mich, ich will dich k\u00fcssen,", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "ich", "will", "dich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Da\u00df sich Treu und Bruderliebe", "tokens": ["Da\u00df", "sich", "Treu", "und", "Bru\u00b7der\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Durch den Bruderku\u00df verst\u00e4rke.", "tokens": ["Durch", "den", "Bru\u00b7der\u00b7ku\u00df", "ver\u00b7st\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Bruder, gnug! sie ist gest\u00e4rket,", "tokens": ["Bru\u00b7der", ",", "gnug", "!", "sie", "ist", "ge\u00b7st\u00e4r\u00b7ket", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "La\u00df mich nun die Br\u00fcder k\u00fcssen.", "tokens": ["La\u00df", "mich", "nun", "die", "Br\u00fc\u00b7der", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Br\u00fcder! habt ihr keine Schwestern?", "tokens": ["Br\u00fc\u00b7der", "!", "habt", "ihr", "kei\u00b7ne", "Schwes\u00b7tern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Nun sind sie mir auch verschwistert.", "tokens": ["Nun", "sind", "sie", "mir", "auch", "ver\u00b7schwis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Holt sie mir, ich will sie k\u00fcssen.", "tokens": ["Holt", "sie", "mir", ",", "ich", "will", "sie", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}