{"textgrid.poem.58746": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wasche dein Herz von der Bosheit, auf da\u00df dir geholfen werde! Jer. 4, 14", "tokens": ["Wa\u00b7sche", "dein", "Herz", "von", "der", "Bos\u00b7heit", ",", "auf", "da\u00df", "dir", "ge\u00b7hol\u00b7fen", "wer\u00b7de", "!", "Jer", ".", "4", ",", "14"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "APPR", "KOUS", "PPER", "VVPP", "VAFIN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie seh' ich mich so unrein, ach!", "tokens": ["Wie", "seh'", "ich", "mich", "so", "un\u00b7rein", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein gro\u00dfes Elend ich anschaue,", "tokens": ["Mein", "gro\u00b7\u00dfes", "E\u00b7lend", "ich", "an\u00b7schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich so arm, da\u00df ich so schwach;", "tokens": ["Da\u00df", "ich", "so", "arm", ",", "da\u00df", "ich", "so", "schwach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber, Herr, in den ich traue,", "tokens": ["Du", "a\u00b7ber", ",", "Herr", ",", "in", "den", "ich", "trau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du kannst mich reinigen. Dein kostbar Blut ist mir", "tokens": ["Du", "kannst", "mich", "rei\u00b7ni\u00b7gen", ".", "Dein", "kost\u00b7bar", "Blut", "ist", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$.", "PPOSAT", "ADJD", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein heil'ges Waschgef\u00e4\u00df, ein offnes Bad der Gnaden,", "tokens": ["Ein", "heil'\u00b7ges", "Waschge\u00b7f\u00e4\u00df", ",", "ein", "off\u00b7nes", "Bad", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NE", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Drein senk ich alle S\u00fcnd' und Schaden,", "tokens": ["Drein", "senk", "ich", "al\u00b7le", "S\u00fcnd'", "und", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu werden angenehm vor dir.", "tokens": ["Zu", "wer\u00b7den", "an\u00b7ge\u00b7nehm", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, h\u00e4tt' ich gleich so viele S\u00fcnd'", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "gleich", "so", "vie\u00b7le", "S\u00fcnd'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Sand der Berge, nicht zu z\u00e4hlen,", "tokens": ["Als", "Sand", "der", "Ber\u00b7ge", ",", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Jesu Blut ich Reinheit find'", "tokens": ["In", "Je\u00b7su", "Blut", "ich", "Rein\u00b7heit", "find'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen Leiden, die mich qu\u00e4len.", "tokens": ["In", "al\u00b7len", "Lei\u00b7den", ",", "die", "mich", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies g\u00f6ttlich Waschgef\u00e4\u00df mir Ruhe gibt und Mut,", "tokens": ["Dies", "g\u00f6tt\u00b7lich", "Waschge\u00b7f\u00e4\u00df", "mir", "Ru\u00b7he", "gibt", "und", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Was sonst beschwert, wird leicht, die Last sich gar verlieret,", "tokens": ["Was", "sonst", "be\u00b7schwert", ",", "wird", "leicht", ",", "die", "Last", "sich", "gar", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VAFIN", "ADJD", "$,", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Herz die rechte Weite sp\u00fcret", "tokens": ["Mein", "Herz", "die", "rech\u00b7te", "Wei\u00b7te", "sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In diesem reinen, h\u00f6chsten Gut.", "tokens": ["In", "die\u00b7sem", "rei\u00b7nen", ",", "h\u00f6chs\u00b7ten", "Gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ei, warum s\u00e4umt man denn, sich nur fein oft und frei", "tokens": ["Ei", ",", "wa\u00b7rum", "s\u00e4umt", "man", "denn", ",", "sich", "nur", "fein", "oft", "und", "frei"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PIS", "ADV", "$,", "PRF", "ADV", "ADJD", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem hochedeln Teich zu baden?", "tokens": ["In", "dem", "ho\u00b7che\u00b7deln", "Teich", "zu", "ba\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "O trefflich sch\u00f6ne Arzenei,", "tokens": ["O", "treff\u00b7lich", "sch\u00f6\u00b7ne", "Ar\u00b7ze\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du machst mein Herz vergn\u00fcgt und heilest allen Schaden.", "tokens": ["Du", "machst", "mein", "Herz", "ver\u00b7gn\u00fcgt", "und", "hei\u00b7lest", "al\u00b7len", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}