{"textgrid.poem.34293": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleines Ding, um uns zu qu\u00e4len,", "tokens": ["Klei\u00b7nes", "Ding", ",", "um", "uns", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in diese Brust gelegt!", "tokens": ["Hier", "in", "die\u00b7se", "Brust", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach wers vors\u00e4h, was er tr\u00e4gt,", "tokens": ["Ach", "wers", "vor\u00b7s\u00e4h", ",", "was", "er", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "W\u00fcrde w\u00fcnschen, th\u00e4tst ihm fehlen!", "tokens": ["W\u00fcr\u00b7de", "w\u00fcn\u00b7schen", ",", "th\u00e4tst", "ihm", "feh\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Schl\u00e4ge, wie so selten", "tokens": ["Dei\u00b7ne", "Schl\u00e4\u00b7ge", ",", "wie", "so", "sel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mischt sich Lust in sie hinein!", "tokens": ["Mischt", "sich", "Lust", "in", "sie", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Augenblicks vergelten", "tokens": ["Und", "wie", "Au\u00b7gen\u00b7blicks", "ver\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ihm jede Lust mit Pein!", "tokens": ["Sie", "ihm", "je\u00b7de", "Lust", "mit", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! und weder Lust noch Qualen", "tokens": ["Ach", "!", "und", "we\u00b7der", "Lust", "noch", "Qua\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "KON", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind ihm schrecklicher als das:", "tokens": ["Sind", "ihm", "schreck\u00b7li\u00b7cher", "als", "das", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kalt und f\u00fchllos! O ihr Strahlen,", "tokens": ["Kalt", "und", "f\u00fchl\u00b7los", "!", "O", "ihr", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$.", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmelzt es lieber mir zu Glas!", "tokens": ["Schmelzt", "es", "lie\u00b7ber", "mir", "zu", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieben, hassen, f\u00fcrchten, zittern,", "tokens": ["Lie\u00b7ben", ",", "has\u00b7sen", ",", "f\u00fcrch\u00b7ten", ",", "zit\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffen, zagen bis ins Mark,", "tokens": ["Hof\u00b7fen", ",", "za\u00b7gen", "bis", "ins", "Mark", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann das Leben zwar verbittern;", "tokens": ["Kann", "das", "Le\u00b7ben", "zwar", "ver\u00b7bit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ohne sie w\u00e4rs Quark!", "tokens": ["A\u00b7ber", "oh\u00b7ne", "sie", "w\u00e4rs", "Quark", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}