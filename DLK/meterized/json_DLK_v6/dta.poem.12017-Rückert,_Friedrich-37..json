{"dta.poem.12017": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht, was im Verstand sich ewig widerspricht,", "tokens": ["Wer", "nicht", ",", "was", "im", "Ver\u00b7stand", "sich", "e\u00b7wig", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "$,", "PRELS", "APPRART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zugleich kann denken, denkt den Ew'gen ewig nicht.", "tokens": ["Zu\u00b7gleich", "kann", "den\u00b7ken", ",", "denkt", "den", "Ew'\u00b7gen", "e\u00b7wig", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,", "VVFIN", "ART", "NN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum magst du, statt dir selbst zum Schrecken oder Spott", "tokens": ["Drum", "magst", "du", ",", "statt", "dir", "selbst", "zum", "Schre\u00b7cken", "o\u00b7der", "Spott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$,", "KOUI", "PPER", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus All und Eins und Nichts zu schaffen einen Gott,", "tokens": ["Aus", "All", "und", "Eins", "und", "Nichts", "zu", "schaf\u00b7fen", "ei\u00b7nen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihn lieber denken dir mit Mund und Angesicht,", "tokens": ["Ihn", "lie\u00b7ber", "den\u00b7ken", "dir", "mit", "Mund", "und", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er bl\u00e4st Odem ein und Sch\u00f6pfungsworte spricht.", "tokens": ["Wie", "er", "bl\u00e4st", "O\u00b7dem", "ein", "und", "Sch\u00f6p\u00b7fungs\u00b7wor\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NE", "PTKVZ", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann aber mu\u00dft du ihm auch geben einen Ort,", "tokens": ["Dann", "a\u00b7ber", "mu\u00dft", "du", "ihm", "auch", "ge\u00b7ben", "ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Unendlichkeit des Raumes r\u00e4umen fort.", "tokens": ["Und", "die", "Un\u00b7end\u00b7lich\u00b7keit", "des", "Rau\u00b7mes", "r\u00e4u\u00b7men", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Erde mu\u00dft du fest in ihre Mitte bannen,", "tokens": ["Die", "Er\u00b7de", "mu\u00dft", "du", "fest", "in", "ih\u00b7re", "Mit\u00b7te", "ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umher das Firmament, das goldbeschlagne, spannen;", "tokens": ["Um\u00b7her", "das", "Fir\u00b7ma\u00b7ment", ",", "das", "gold\u00b7be\u00b7schlag\u00b7ne", ",", "span\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df dir die Sonn' am Tag bescheine deinen Raum,", "tokens": ["Da\u00df", "dir", "die", "Sonn'", "am", "Tag", "be\u00b7schei\u00b7ne", "dei\u00b7nen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Mond und Stern bei Nacht bescheine deinen Traum.", "tokens": ["Und", "Mond", "und", "Stern", "bei", "Nacht", "be\u00b7schei\u00b7ne", "dei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn so dein Sinn zur\u00fcck sich wiegt in sel'ge Kindheit,", "tokens": ["Wenn", "so", "dein", "Sinn", "zu\u00b7r\u00fcck", "sich", "wiegt", "in", "sel'\u00b7ge", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKVZ", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol m\u00f6gen Schauende beneiden deine Blindheit.", "tokens": ["Wol", "m\u00f6\u00b7gen", "Schau\u00b7en\u00b7de", "be\u00b7nei\u00b7den", "dei\u00b7ne", "Blind\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}}}}}