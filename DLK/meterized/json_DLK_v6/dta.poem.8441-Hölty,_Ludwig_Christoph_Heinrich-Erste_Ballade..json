{"dta.poem.8441": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erste Ballade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Glich unter allen Sch\u00f6nen,", "tokens": ["Glich", "un\u00b7ter", "al\u00b7len", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier unterm Mond, das ist gewiss,", "tokens": ["Hier", "un\u00b7term", "Mond", ",", "das", "ist", "ge\u00b7wiss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Mutterkind Ismenen.", "tokens": ["Kein", "Mut\u00b7ter\u00b7kind", "Is\u00b7me\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie war nur eben achzehn Jahr,", "tokens": ["Sie", "war", "nur", "e\u00b7ben", "ach\u00b7zehn", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein M\u00e4dchen zum Entz\u00fccken,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "zum", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit runder Brust und blondem Haar,", "tokens": ["Mit", "run\u00b7der", "Brust", "und", "blon\u00b7dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Adel in den Blicken.", "tokens": ["Und", "A\u00b7del", "in", "den", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Wuchs, voll Reiz und Majest\u00e4t,", "tokens": ["Ihr", "Wuchs", ",", "voll", "Reiz", "und", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War gleich der schlanken Maie;", "tokens": ["War", "gleich", "der", "schlan\u00b7ken", "Mai\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wange junger Rosen R\u00f6th',", "tokens": ["Die", "Wan\u00b7ge", "jun\u00b7ger", "Ro\u00b7sen", "R\u00f6\u00b7th'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ihr Auge Himmelbl\u00e4ue.", "tokens": ["Ihr", "Au\u00b7ge", "Him\u00b7mel\u00b7bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mund, ein bl\u00fchend Paradies,", "tokens": ["Der", "Mund", ",", "ein", "bl\u00fc\u00b7hend", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War sonder alle M\u00e4ngel;", "tokens": ["War", "son\u00b7der", "al\u00b7le", "M\u00e4n\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wann sie sang, so klangs so s\u00fcss,", "tokens": ["Und", "wann", "sie", "sang", ",", "so", "klangs", "so", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als s\u00e4ng' ein heilger Engel.", "tokens": ["Als", "s\u00e4ng'", "ein", "heil\u00b7ger", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die holde Sch\u00f6ne, denkt einmal,", "tokens": ["Die", "hol\u00b7de", "Sch\u00f6\u00b7ne", ",", "denkt", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That aber arge Thaten,", "tokens": ["That", "a\u00b7ber", "ar\u00b7ge", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und muss vielleicht im Pful der Qual", "tokens": ["Und", "muss", "viel\u00b7leicht", "im", "Pful", "der", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Izt kochen oder braten:", "tokens": ["Izt", "ko\u00b7chen", "o\u00b7der", "bra\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie hexte Froschleich, Russ und Haar", "tokens": ["Sie", "hex\u00b7te", "Frosc\u00b7hleich", ",", "Russ", "und", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Butterfass des K\u00fcsters,", "tokens": ["Ins", "But\u00b7ter\u00b7fass", "des", "K\u00fcs\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und zauberte voll Finnen gar", "tokens": ["Und", "zau\u00b7ber\u00b7te", "voll", "Fin\u00b7nen", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Schweine des Magisters.", "tokens": ["Die", "Schwei\u00b7ne", "des", "Ma\u00b7gis\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie kn\u00fcpfte manchem Ehepaar", "tokens": ["Sie", "kn\u00fcpf\u00b7te", "man\u00b7chem", "E\u00b7he\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Nestel als ein Meister,", "tokens": ["Den", "Nes\u00b7tel", "als", "ein", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und rief, wanns ihr gef\u00e4llig war,", "tokens": ["Und", "rief", ",", "wanns", "ihr", "ge\u00b7f\u00e4l\u00b7lig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Rudel H\u00f6llengeister;", "tokens": ["Ein", "Ru\u00b7del", "H\u00f6l\u00b7len\u00b7geis\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ritt, troz den besten Postkurier,", "tokens": ["Ritt", ",", "troz", "den", "bes\u00b7ten", "Post\u00b7ku\u00b7rier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ihrem Besenstiele,", "tokens": ["Auf", "ih\u00b7rem", "Be\u00b7sen\u00b7stie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00fcbergab den Winden ihr", "tokens": ["Und", "\u00fc\u00b7berg\u00b7ab", "den", "Win\u00b7den", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geringelt Haar zum Spiele.", "tokens": ["Ge\u00b7rin\u00b7gelt", "Haar", "zum", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie tanzte stets am ersten Mai,", "tokens": ["Sie", "tanz\u00b7te", "stets", "am", "ers\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blumen in den Locken,", "tokens": ["Mit", "Blu\u00b7men", "in", "den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den weissen Busen schleierfrei,", "tokens": ["Den", "weis\u00b7sen", "Bu\u00b7sen", "schlei\u00b7er\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Reigen auf dem Brocken.", "tokens": ["Im", "Rei\u00b7gen", "auf", "dem", "Bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann pflag der alte Satanas", "tokens": ["Dann", "pflag", "der", "al\u00b7te", "Sa\u00b7ta\u00b7nas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den s\u00fcssen Herrn zu spielen,", "tokens": ["Den", "s\u00fcs\u00b7sen", "Herrn", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wann sie stand, und wann sie sass,", "tokens": ["Und", "wann", "sie", "stand", ",", "und", "wann", "sie", "sass", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach ihrer Brust zu schielen.", "tokens": ["Nach", "ih\u00b7rer", "Brust", "zu", "schie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Begierig k\u00fcsst' er ihre Hand,", "tokens": ["Be\u00b7gie\u00b7rig", "k\u00fcsst'", "er", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wollt' ers H\u00e4ndchen fressen,", "tokens": ["Als", "wollt'", "ers", "H\u00e4nd\u00b7chen", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und konnt' am schwarzen Feuerstrand", "tokens": ["Und", "konnt'", "am", "schwar\u00b7zen", "Feu\u00b7er\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6ne nicht vergessen,", "tokens": ["Die", "Sch\u00f6\u00b7ne", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sandt' ihr so manches Billet doux", "tokens": ["Sandt'", "ihr", "so", "man\u00b7ches", "Bil\u00b7let", "doux"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch seinen Hoflakaien,", "tokens": ["Durch", "sei\u00b7nen", "Ho\u00b7fla\u00b7kai\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schloss kaum die Augenwimper zu,", "tokens": ["Schloss", "kaum", "die", "Au\u00b7gen\u00b7wim\u00b7per", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und tr\u00e4umte schon vom Freien.", "tokens": ["Und", "tr\u00e4um\u00b7te", "schon", "vom", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Allein Ismene lachte nur", "tokens": ["Al\u00b7lein", "Is\u00b7me\u00b7ne", "lach\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des gr\u00e4mlichen Pedanten,", "tokens": ["Des", "gr\u00e4m\u00b7li\u00b7chen", "Pe\u00b7dan\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und suchte sich, bald auf der Flur,", "tokens": ["Und", "such\u00b7te", "sich", ",", "bald", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald in der Stadt, Amanten.", "tokens": ["Bald", "in", "der", "Stadt", ",", "A\u00b7man\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sah einmal am Wiesenbach,", "tokens": ["Sie", "sah", "ein\u00b7mal", "am", "Wie\u00b7sen\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo manches Bl\u00fcmchen keimte,", "tokens": ["Wo", "man\u00b7ches", "Bl\u00fcm\u00b7chen", "ke\u00b7im\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Leandern, der im Schatten lag,", "tokens": ["Le\u00b7an\u00b7dern", ",", "der", "im", "Schat\u00b7ten", "lag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und s\u00fcsse Tr\u00e4ume tr\u00e4umte.", "tokens": ["Und", "s\u00fcs\u00b7se", "Tr\u00e4u\u00b7me", "tr\u00e4um\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er tr\u00e4umte von der Adelheid,", "tokens": ["Er", "tr\u00e4um\u00b7te", "von", "der", "A\u00b7del\u00b7heid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der er sich versprochen,", "tokens": ["Mit", "der", "er", "sich", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daneben von der Seligkeit", "tokens": ["Da\u00b7ne\u00b7ben", "von", "der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ersten Flitterwochen.", "tokens": ["Der", "ers\u00b7ten", "Flit\u00b7ter\u00b7wo\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es sollte schon die Priesterhand", "tokens": ["Es", "soll\u00b7te", "schon", "die", 