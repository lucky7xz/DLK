{"dta.poem.3261": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Kurtze  \n  Abhandlung  \n von den Dichtungen \u00fcberhaupt.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und fliegt in eine Welt des Epicurus hin,", "tokens": ["Und", "fliegt", "in", "ei\u00b7ne", "Welt", "des", "E\u00b7pi\u00b7cu\u00b7rus", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und macht sich ein Gesch\u00f6pf, von dem man nie gelesen,", "tokens": ["Und", "macht", "sich", "ein", "Ge\u00b7sch\u00f6pf", ",", "von", "dem", "man", "nie", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das k\u00fcnftig nicht seyn wird, noch jemahls ist gewesen.", "tokens": ["Das", "k\u00fcnf\u00b7tig", "nicht", "seyn", "wird", ",", "noch", "je\u00b7mahls", "ist", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PTKNEG", "VAINF", "VAFIN", "$,", "ADV", "ADV", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}