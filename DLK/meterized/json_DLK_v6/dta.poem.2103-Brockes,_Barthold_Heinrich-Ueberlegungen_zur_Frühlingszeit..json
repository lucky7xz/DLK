{"dta.poem.2103": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueberlegungen zur Fr\u00fchlingszeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und zielt auf einen weisen Zweck) wodurch die Gottheit", "tokens": ["Und", "zielt", "auf", "ei\u00b7nen", "wei\u00b7sen", "Zweck", ")", "wo\u00b7durch", "die", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "mittelbar", "tokens": ["mit\u00b7tel\u00b7bar"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In dem, was er erschaffen, wirkt. Ist dieses dir nun", "tokens": ["In", "dem", ",", "was", "er", "er\u00b7schaf\u00b7fen", ",", "wirkt", ".", "Ist", "die\u00b7ses", "dir", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,", "VVFIN", "$.", "VAFIN", "PDAT", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "noch nicht klar;", "tokens": ["noch", "nicht", "klar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So werden wir, nebst der Vernunft, erkennen und be-", "tokens": ["So", "wer\u00b7den", "wir", ",", "nebst", "der", "Ver\u00b7nunft", ",", "er\u00b7ken\u00b7nen", "und", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "ART", "NN", "$,", "VVINF", "KON", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "kennen m\u00fcssen:", "tokens": ["ken\u00b7nen", "m\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df wir, ein mehrers vom Naturgeist, nicht fassen", "tokens": ["Da\u00df", "wir", ",", "ein", "meh\u00b7rers", "vom", "Na\u00b7tur\u00b7geist", ",", "nicht", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "ADV", "APPRART", "NN", "$,", "PTKNEG", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "k\u00f6nnen und nicht wissen,", "tokens": ["k\u00f6n\u00b7nen", "und", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "So denn ja wohl nicht zu bewundern, da sie von sich selbst", "tokens": ["So", "denn", "ja", "wohl", "nicht", "zu", "be\u00b7wun\u00b7dern", ",", "da", "sie", "von", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "APPR", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "in der That", "tokens": ["in", "der", "That"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Nichts anders, als ein schwebend Meynen und dunkele", "tokens": ["Nichts", "an\u00b7ders", ",", "als", "ein", "schwe\u00b7bend", "Mey\u00b7nen", "und", "dun\u00b7ke\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "$,", "KOUS", "ART", "ADJD", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Begriffe, hat.", "tokens": ["Be\u00b7grif\u00b7fe", ",", "hat", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "In diesem Dunklen brennt jedoch ein herrlich unausl\u00f6sch-", "tokens": ["In", "die\u00b7sem", "Dunk\u00b7len", "brennt", "je\u00b7doch", "ein", "herr\u00b7lich", "un\u00b7aus\u00b7l\u00f6sch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "lich Licht,", "tokens": ["lich", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Das, (da in allen Kreaturen, in den hervorgebrachten", "tokens": ["Das", ",", "(", "da", "in", "al\u00b7len", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "in", "den", "her\u00b7vor\u00b7ge\u00b7brach\u00b7ten"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "$(", "ADV", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.16": {"text": "Werken,", "tokens": ["Wer\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Ein\u2019 Ordnung \u00fcberall zu sehn, und eine Weisheit zu be-", "tokens": ["Ein'", "Ord\u00b7nung", "\u00fc\u00b7be\u00b7rall", "zu", "sehn", ",", "und", "ei\u00b7ne", "Weis\u00b7heit", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "KON", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "merken,", "tokens": ["mer\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Nichts von sich selbst entstehen kann, auch nichts von un-", "tokens": ["Nichts", "von", "sich", "selbst", "ent\u00b7ste\u00b7hen", "kann", ",", "auch", "nichts", "von", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PRF", "ADV", "VVINF", "VMFIN", "$,", "ADV", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "gef\u00e4hr geschicht,)", "tokens": ["ge\u00b7f\u00e4hr", "ge\u00b7schicht", ",", ")"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Den wahren Gott so deutlich zeigt, da\u00df unsrer Sonnen", "tokens": ["Den", "wah\u00b7ren", "Gott", "so", "deut\u00b7lich", "zeigt", ",", "da\u00df", "uns\u00b7rer", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Glanz und Schein", "tokens": ["Glanz", "und", "Schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Den k\u00f6rperlichen Blick und Augen nicht hell-, nicht sicht-", "tokens": ["Den", "k\u00f6r\u00b7per\u00b7li\u00b7chen", "Blick", "und", "Au\u00b7gen", "nicht", "hell", ",", "nicht", "sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKNEG", "TRUNC", "$,", "PTKNEG", "TRUNC"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "barer kann seyn,", "tokens": ["ba\u00b7rer", "kann", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VAINF", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Als unserm Geist sein g\u00f6ttlich Wesen. Ob aber, da itzt", "tokens": ["Als", "un\u00b7serm", "Geist", "sein", "g\u00f6tt\u00b7lich", "We\u00b7sen", ".", "Ob", "a\u00b7ber", ",", "da", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "$.", "KOUS", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "alles gr\u00fcnet,", "tokens": ["al\u00b7les", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Die Gottheit sich noch mittler Kr\u00e4fte, und in wie fern,", "tokens": ["Die", "Got\u00b7theit", "sich", "noch", "mitt\u00b7ler", "Kr\u00e4f\u00b7te", ",", "und", "in", "wie", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADJA", "NN", "$,", "KON", "APPR", "PWAV", "ADJD", "$,"], "meter": "-++--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "dazu bedienet?", "tokens": ["da\u00b7zu", "be\u00b7die\u00b7net", "?"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Ob die Natur ein eigenes f\u00fcr sich bestehend Wesen sey?", "tokens": ["Ob", "die", "Na\u00b7tur", "ein", "ei\u00b7ge\u00b7nes", "f\u00fcr", "sich", "be\u00b7ste\u00b7hend", "We\u00b7sen", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "APPR", "PRF", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.30": {"text": "Was die\u00df ihr Wesen eigentlich, wie fern sich ihre Kr\u00e4ft\u2019", "tokens": ["Was", "die\u00df", "ihr", "We\u00b7sen", "ei\u00b7gent\u00b7lich", ",", "wie", "fern", "sich", "ih\u00b7re", "Kr\u00e4ft'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "PPOSAT", "NN", "ADV", "$,", "PWAV", "ADJD", "PRF", "PPOSAT", "NN"], "meter": "-+-+-++--+-+-+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "erstrecken?", "tokens": ["er\u00b7stre\u00b7cken", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Von diesen l\u00e4\u00dft sich nach dem Stande des Menschen-", "tokens": ["Von", "die\u00b7sen", "l\u00e4\u00dft", "sich", "nach", "dem", "Stan\u00b7de", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "geistes nichts entdecken.", "tokens": ["geis\u00b7tes", "nichts", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}