{"textgrid.poem.43971": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach vieler M\u00fch und Zeit und Schmachten,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach vieler M\u00fch und Zeit und Schmachten,", "tokens": ["Nach", "vie\u00b7ler", "M\u00fch", "und", "Zeit", "und", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die warlich nicht gering zu achten,", "tokens": ["Die", "war\u00b7lich", "nicht", "ge\u00b7ring", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kam einmahl auch ein Augenblick", "tokens": ["Kam", "ein\u00b7mahl", "auch", "ein", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alt- und edlen Zeit zur\u00fcck.", "tokens": ["Der", "al\u00b7t", "und", "ed\u00b7len", "Zeit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich k\u00fcste Leonorens Wangen,", "tokens": ["Ich", "k\u00fcs\u00b7te", "Le\u00b7o\u00b7no\u00b7rens", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch eh ich noch die Hand,", "tokens": ["Doch", "eh", "ich", "noch", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie g\u00e4nzlich zu empfangen,", "tokens": ["Sie", "g\u00e4nz\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gewand,", "tokens": ["Ge\u00b7wand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "So fiel der neunte Seigerschlag", "tokens": ["So", "fiel", "der", "neun\u00b7te", "Sei\u00b7ger\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sowohl in Herz als Ohren.", "tokens": ["So\u00b7wohl", "in", "Herz", "als", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ach, sprach sie, Kind, ich bin verloren;", "tokens": ["Ach", ",", "sprach", "sie", ",", "Kind", ",", "ich", "bin", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn da\u00df ich jezt nicht bleiben mag,", "tokens": ["Denn", "da\u00df", "ich", "jezt", "nicht", "blei\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das thut Befehl und Zeit, die V\u00f6gel abzuf\u00fcttern.", "tokens": ["Das", "thut", "Be\u00b7fehl", "und", "Zeit", ",", "die", "V\u00f6\u00b7gel", "ab\u00b7zu\u00b7f\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie k\u00fcste mich noch halb und lief mit Zittern;", "tokens": ["Sie", "k\u00fcs\u00b7te", "mich", "noch", "halb", "und", "lief", "mit", "Zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich aber schrie:", "tokens": ["Ich", "a\u00b7ber", "schrie", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ach Himmel, h\u00e4ltstu uns doch h\u00e4rter als das Vieh!", "tokens": ["Ach", "Him\u00b7mel", ",", "h\u00e4lts\u00b7tu", "uns", "doch", "h\u00e4r\u00b7ter", "als", "das", "Vieh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}