{"textgrid.poem.26143": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das Dunkel geht nicht aus den Dingen heraus", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein fr\u00fcher Abend schleicht im Haus herum,", "tokens": ["Ein", "fr\u00fc\u00b7her", "A\u00b7bend", "schleicht", "im", "Haus", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er l\u00f6scht die Farben deiner Wangen aus", "tokens": ["Er", "l\u00f6scht", "die", "Far\u00b7ben", "dei\u00b7ner", "Wan\u00b7gen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4ngt dir seine Bl\u00e4sse um.", "tokens": ["Und", "h\u00e4ngt", "dir", "sei\u00b7ne", "Bl\u00e4s\u00b7se", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Maib\u00e4ume stehen im Regen geb\u00fcckt,", "tokens": ["Mai\u00b7b\u00e4u\u00b7me", "ste\u00b7hen", "im", "Re\u00b7gen", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Die Berge dampfend voll Wolken wehen,", "tokens": ["Die", "Ber\u00b7ge", "damp\u00b7fend", "voll", "Wol\u00b7ken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deine Brust ist dumpf wie der Abend bedr\u00fcckt.", "tokens": ["Dei\u00b7ne", "Brust", "ist", "dumpf", "wie", "der", "A\u00b7bend", "be\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Dunkel geht nicht aus den Dingen heraus,", "tokens": ["Das", "Dun\u00b7kel", "geht", "nicht", "aus", "den", "Din\u00b7gen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dein Gesicht allein leuchtet wei\u00df hinaus", "tokens": ["Dein", "Ge\u00b7sicht", "al\u00b7lein", "leuch\u00b7tet", "wei\u00df", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "VVFIN", "APZR"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und sieht starr wie die Maske des Kummers aus.", "tokens": ["Und", "sieht", "starr", "wie", "die", "Mas\u00b7ke", "des", "Kum\u00b7mers", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}