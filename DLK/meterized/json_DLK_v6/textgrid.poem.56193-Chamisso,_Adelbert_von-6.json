{"textgrid.poem.56193": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab ihn im Schlafe zu sehen gemeint,", "tokens": ["Ich", "hab", "ihn", "im", "Schla\u00b7fe", "zu", "se\u00b7hen", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch str\u00e4ubt vor Entsetzen mein Haar sich empor,", "tokens": ["Noch", "str\u00e4ubt", "vor", "Ent\u00b7set\u00b7zen", "mein", "Haar", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O h\u00e4tt ich doch schlaflos die Nacht durchweint,", "tokens": ["O", "h\u00e4tt", "ich", "doch", "schlaf\u00b7los", "die", "Nacht", "durch\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie manche der N\u00e4chte zuvor.", "tokens": ["Wie", "man\u00b7che", "der", "N\u00e4ch\u00b7te", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich sah ihn verst\u00f6rt, zerrissen und bleich,", "tokens": ["Ich", "sah", "ihn", "ver\u00b7st\u00f6rt", ",", "zer\u00b7ris\u00b7sen", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie er in den Sand zu schreiben schien,", "tokens": ["Wie", "er", "in", "den", "Sand", "zu", "schrei\u00b7ben", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er schrieb unsre Namen, ich kannt es gleich,", "tokens": ["Er", "schrieb", "uns\u00b7re", "Na\u00b7men", ",", "ich", "kannt", "es", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da hab ich wohl laut geschrien.", "tokens": ["Da", "hab", "ich", "wohl", "laut", "ge\u00b7schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er fuhr zusammen vom Schrei erschreckt,", "tokens": ["Er", "fuhr", "zu\u00b7sam\u00b7men", "vom", "Schrei", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und blickte mich an, verstummt wie das Grab,", "tokens": ["Und", "blick\u00b7te", "mich", "an", ",", "ver\u00b7stummt", "wie", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hielt ihm die Arme entgegen gestreckt,", "tokens": ["Ich", "hielt", "ihm", "die", "Ar\u00b7me", "ent\u00b7ge\u00b7gen", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und er \u2013 er wandte sich ab.", "tokens": ["Und", "er", "\u2013", "er", "wand\u00b7te", "sich", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}