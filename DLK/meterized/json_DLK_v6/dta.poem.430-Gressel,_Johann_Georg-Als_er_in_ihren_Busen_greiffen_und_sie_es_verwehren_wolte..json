{"dta.poem.430": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er in ihren Busen greiffen/ und sie es  \n verwehren wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Rosabelle la\u00df die Finger nur in deinen Busen gehn/", "tokens": ["Ro\u00b7sa\u00b7bel\u00b7le", "la\u00df", "die", "Fin\u00b7ger", "nur", "in", "dei\u00b7nen", "Bu\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Lange k\u00f6nnen sie die Hitze nicht von deinem Feur ausstehn.", "tokens": ["Lan\u00b7ge", "k\u00f6n\u00b7nen", "sie", "die", "Hit\u00b7ze", "nicht", "von", "dei\u00b7nem", "Feur", "aus\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aussen sind sie zwar nur Schnee/ weisse Liljen und ", "tokens": ["Aus\u00b7sen", "sind", "sie", "zwar", "nur", "Schnee", "/", "weis\u00b7se", "Lil\u00b7jen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ADV", "NN", "$(", "ADJA", "NN", "KON"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aber einen k\u00fchnen Griff mu\u00df man mit dem Brande b\u00fcssen.", "tokens": ["A\u00b7ber", "ei\u00b7nen", "k\u00fch\u00b7nen", "Griff", "mu\u00df", "man", "mit", "dem", "Bran\u00b7de", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Aus dem Eyse quillen Flammen/ aus der Milch entsteht ein", "tokens": ["Aus", "dem", "Ey\u00b7se", "quil\u00b7len", "Flam\u00b7men", "/", "aus", "der", "Milch", "ent\u00b7steht", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$(", "APPR", "ART", "NN", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Brand/", "tokens": ["Brand", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und man sieht die heissen Funcken an vor s\u00fcsses Zuckerkand/", "tokens": ["Und", "man", "sieht", "die", "heis\u00b7sen", "Fun\u00b7cken", "an", "vor", "s\u00fcs\u00b7ses", "Zu\u00b7cker\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Venus pflegt das meiste Wild bey der Brust zu \u00fcbereilen/", "tokens": ["Ve\u00b7nus", "pflegt", "das", "meis\u00b7te", "Wild", "bey", "der", "Brust", "zu", "\u00fc\u00b7be\u00b7rei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Amorn ists der rechte Ort loszudr\u00fccken mit den Pfeilen.", "tokens": ["A\u00b7morn", "ists", "der", "rech\u00b7te", "Ort", "los\u00b7zu\u00b7dr\u00fc\u00b7cken", "mit", "den", "Pfei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}}}}}