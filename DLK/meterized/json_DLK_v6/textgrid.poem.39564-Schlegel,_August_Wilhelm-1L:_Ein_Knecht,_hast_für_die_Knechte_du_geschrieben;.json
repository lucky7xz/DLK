{"textgrid.poem.39564": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Knecht, hast f\u00fcr die Knechte du geschrieben;", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Knecht, hast f\u00fcr die Knechte du geschrieben;", "tokens": ["Ein", "Knecht", ",", "hast", "f\u00fcr", "die", "Knech\u00b7te", "du", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Samojede f\u00fcr die Samojeden.", "tokens": ["Ein", "Sa\u00b7mo\u00b7je\u00b7de", "f\u00fcr", "die", "Sa\u00b7mo\u00b7je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du m\u00f6chtest gern Vernunft und Freiheit reden,", "tokens": ["Du", "m\u00f6ch\u00b7test", "gern", "Ver\u00b7nunft", "und", "Frei\u00b7heit", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch ist dein eigner Geist leibeigen blieben.", "tokens": ["Doch", "ist", "dein", "eig\u00b7ner", "Geist", "leib\u00b7ei\u00b7gen", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus L\u00e4ndern fort, in St\u00e4dten umgetrieben.", "tokens": ["Aus", "L\u00e4n\u00b7dern", "fort", ",", "in", "St\u00e4d\u00b7ten", "um\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Kneipen, Clubbs, Merkuren, deine schn\u00f6den", "tokens": ["In", "Knei\u00b7pen", ",", "Clubbs", ",", "Mer\u00b7ku\u00b7ren", ",", "dei\u00b7ne", "schn\u00f6\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "$,", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unw\u00fcrd'gen Merkelw\u00fcrdigkeiten \u00fcben?", "tokens": ["Un\u00b7w\u00fcrd'\u00b7gen", "Mer\u00b7kel\u00b7w\u00fcr\u00b7dig\u00b7kei\u00b7ten", "\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Dir ist es Freiheit, frei und frank zu klatschen;", "tokens": ["Dir", "ist", "es", "Frei\u00b7heit", ",", "frei", "und", "frank", "zu", "klat\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir Charit\u00e9, ", "tokens": ["Dir", "Cha\u00b7rit\u00e9", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Genie, in Hennings Genius dich zu betten.", "tokens": ["Ge\u00b7nie", ",", "in", "Hen\u00b7nings", "Ge\u00b7nius", "dich", "zu", "bet\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NE", "NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Kamst du nur darum von den fernen Letten,", "tokens": ["Kamst", "du", "nur", "da\u00b7rum", "von", "den", "fer\u00b7nen", "Let\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Dreck der Menschheit \u00fcberall zu patschen:", "tokens": ["Im", "Dreck", "der", "Menschheit", "\u00fc\u00b7be\u00b7rall", "zu", "pat\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Journale, f\u00fcrchtet Merkeln!", "tokens": ["Jour\u00b7na\u00b7le", ",", "f\u00fcrch\u00b7tet", "Mer\u00b7keln", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Merklich zeigt er verkleinernde Natur;", "tokens": ["Merk\u00b7lich", "zeigt", "er", "ver\u00b7klei\u00b7nern\u00b7de", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schon ward Merkur durch ihn zum Merkelnur.", "tokens": ["Schon", "ward", "Mer\u00b7kur", "durch", "ihn", "zum", "Mer\u00b7kel\u00b7nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}