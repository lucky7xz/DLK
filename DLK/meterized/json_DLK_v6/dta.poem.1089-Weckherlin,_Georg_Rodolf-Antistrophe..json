{"dta.poem.1089": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also wolle der G\u00f6tter gnad", "tokens": ["Al\u00b7so", "wol\u00b7le", "der", "G\u00f6t\u00b7ter", "gnad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dir stehts newe g\u00fcter beraiten/", "tokens": ["Dir", "stehts", "ne\u00b7we", "g\u00fc\u00b7ter", "be\u00b7rai\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vnd deinen gang fr\u00f6lich belaiten", "tokens": ["Vnd", "dei\u00b7nen", "gang", "fr\u00f6\u00b7lich", "be\u00b7lai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ohn strauchlung durch der Tugent pfad", "tokens": ["Ohn", "strauch\u00b7lung", "durch", "der", "Tu\u00b7gent", "pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Also soll dein Nam/ lob vnd ehr", "tokens": ["Al\u00b7so", "soll", "dein", "Nam", "/", "lob", "vnd", "ehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$(", "NN", "KON", "NN"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Vontag zu tag lieblicher bl\u00fchen;", "tokens": ["Von\u00b7tag", "zu", "tag", "lieb\u00b7li\u00b7cher", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Also soll das gl\u00fcck mehr vnd mehr/", "tokens": ["Al\u00b7so", "soll", "das", "gl\u00fcck", "mehr", "vnd", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "KON", "ADV", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Dem neyd zu trutz/ dich herf\u00fcr ziehen:", "tokens": ["Dem", "neyd", "zu", "trutz", "/", "dich", "her\u00b7f\u00fcr", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd also ab de\u00df Vngl\u00fcckslist/", "tokens": ["Vnd", "al\u00b7so", "ab", "de\u00df", "Vn\u00b7gl\u00fccks\u00b7list", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Damit die Hohe oft geschlagen/", "tokens": ["Da\u00b7mit", "die", "Ho\u00b7he", "oft", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hab sich dein hau\u00df zu keiner frist", "tokens": ["Hab", "sich", "dein", "hau\u00df", "zu", "kei\u00b7ner", "frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Weder zu f\u00f6rchten/ noch zuklagen.", "tokens": ["We\u00b7der", "zu", "f\u00f6rch\u00b7ten", "/", "noch", "zu\u00b7kla\u00b7gen", "."], 