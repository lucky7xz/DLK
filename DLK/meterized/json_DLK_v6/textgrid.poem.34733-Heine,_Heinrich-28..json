{"textgrid.poem.34733": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcsse, die man stiehlt im Dunkeln", "tokens": ["K\u00fcs\u00b7se", ",", "die", "man", "stiehlt", "im", "Dun\u00b7keln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Dunkeln wiedergibt,", "tokens": ["Und", "im", "Dun\u00b7keln", "wie\u00b7der\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche K\u00fcsse, wie besel'gen", "tokens": ["Sol\u00b7che", "K\u00fcs\u00b7se", ",", "wie", "be\u00b7sel'\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie die Seele, wenn sie liebt!", "tokens": ["Sie", "die", "See\u00b7le", ",", "wenn", "sie", "liebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ahnend und erinnrungs\u00fcchtig", "tokens": ["Ah\u00b7nend", "und", "e\u00b7rinn\u00b7run\u00b7gs\u00fcch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+----", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denkt die Seele sich dabei", "tokens": ["Denkt", "die", "See\u00b7le", "sich", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches von vergangnen Tagen,", "tokens": ["Man\u00b7ches", "von", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von Zukunft mancherlei.", "tokens": ["Und", "von", "Zu\u00b7kunft", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch das gar zu viele Denken", "tokens": ["Doch", "das", "gar", "zu", "vie\u00b7le", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist bedenklich, wenn man k\u00fc\u00dft; \u2013", "tokens": ["Ist", "be\u00b7denk\u00b7lich", ",", "wenn", "man", "k\u00fc\u00dft", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weine lieber, liebe Seele,", "tokens": ["Wei\u00b7ne", "lie\u00b7ber", ",", "lie\u00b7be", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil das Weinen leichter ist.", "tokens": ["Weil", "das", "Wei\u00b7nen", "leich\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}