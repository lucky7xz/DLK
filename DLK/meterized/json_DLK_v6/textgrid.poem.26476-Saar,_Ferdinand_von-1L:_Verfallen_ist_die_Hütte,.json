{"textgrid.poem.26476": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verfallen ist die H\u00fctte,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verfallen ist die H\u00fctte,", "tokens": ["Ver\u00b7fal\u00b7len", "ist", "die", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bleich lugt hervor die Noth;", "tokens": ["Bleich", "lugt", "her\u00b7vor", "die", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch im umz\u00e4unten G\u00e4rtchen,", "tokens": ["Doch", "im", "um\u00b7z\u00e4un\u00b7ten", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da bl\u00fch'n die Rosen roth.", "tokens": ["Da", "bl\u00fch'n", "die", "Ro\u00b7sen", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da stehen wei\u00dfe Lilien", "tokens": ["Da", "ste\u00b7hen", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei w\u00fcrz'gem Nelkenflor;", "tokens": ["Bei", "w\u00fcrz'\u00b7gem", "Nel\u00b7ken\u00b7flor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bricht's in allen Farben", "tokens": ["Da", "bricht's", "in", "al\u00b7len", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus frischem Gr\u00fcn hervor.", "tokens": ["Aus", "fri\u00b7schem", "Gr\u00fcn", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seltsame Hand, beim Himmel,", "tokens": ["Selt\u00b7sa\u00b7me", "Hand", ",", "beim", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die diesen Boden pflegt!", "tokens": ["Die", "die\u00b7sen", "Bo\u00b7den", "pflegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er tr\u00fcge wohl auch Fr\u00fcchte,", "tokens": ["Er", "tr\u00fc\u00b7ge", "wohl", "auch", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jetzt nur Blumen tr\u00e4gt.", "tokens": ["Der", "jetzt", "nur", "Blu\u00b7men", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch meine Seele segnet", "tokens": ["Doch", "mei\u00b7ne", "See\u00b7le", "seg\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sch\u00f6nheit, die da sprie\u00dft,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "da", "sprie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die mit ihren Thr\u00e4nen", "tokens": ["Und", "die", "mit", "ih\u00b7ren", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Armuth froh begie\u00dft.", "tokens": ["Die", "Ar\u00b7muth", "froh", "be\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}