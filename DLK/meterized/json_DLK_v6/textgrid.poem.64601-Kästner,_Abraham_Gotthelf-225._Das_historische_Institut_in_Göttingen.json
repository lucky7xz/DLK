{"textgrid.poem.64601": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "225. Das historische Institut in G\u00f6ttingen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tadle nicht das Institut:", "tokens": ["Ich", "tad\u00b7le", "nicht", "das", "Ins\u00b7ti\u00b7tut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist zum Unterricht f\u00fcr junge Leute gut;", "tokens": ["Es", "ist", "zum", "Un\u00b7ter\u00b7richt", "f\u00fcr", "jun\u00b7ge", "Leu\u00b7te", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es macht zur Autorschaft den jungen Leuten Muth.", "tokens": ["Es", "macht", "zur", "Au\u00b7tor\u00b7schaft", "den", "jun\u00b7gen", "Leu\u00b7ten", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch zeigt es sich in academ'scher Pracht,", "tokens": ["Doch", "zeigt", "es", "sich", "in", "a\u00b7ca\u00b7dem'\u00b7scher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So f\u00e4llt mir ", "tokens": ["So", "f\u00e4llt", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}}}}