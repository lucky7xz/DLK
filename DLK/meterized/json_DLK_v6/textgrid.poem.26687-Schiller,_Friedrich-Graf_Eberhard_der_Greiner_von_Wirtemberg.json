{"textgrid.poem.26687": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Graf Eberhard der Greiner von Wirtemberg", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr \u2013 ihr dort au\u00dfen in der Welt,", "tokens": ["Ihr", "\u2013", "ihr", "dort", "au\u00b7\u00dfen", "in", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nasen eingespannt!", "tokens": ["Die", "Na\u00b7sen", "ein\u00b7ge\u00b7spannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch manchen Mann, auch manchen Held,", "tokens": ["Auch", "man\u00b7chen", "Mann", ",", "auch", "man\u00b7chen", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Frieden gut, und stark im Feld,", "tokens": ["Im", "Frie\u00b7den", "gut", ",", "und", "stark", "im", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebar das Schwabenland.", "tokens": ["Ge\u00b7bar", "das", "Schwa\u00b7ben\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Prahlt nur mit Karl und Eduard,", "tokens": ["Prahlt", "nur", "mit", "Karl", "und", "E\u00b7duard", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "KON", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit Friedrich, Ludewig.", "tokens": ["Mit", "Fried\u00b7rich", ",", "Lu\u00b7de\u00b7wig", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Karl, Friedrich, Ludwig, Eduard", "tokens": ["Karl", ",", "Fried\u00b7rich", ",", "Lud\u00b7wig", ",", "E\u00b7duard"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist uns der Graf, der Eberhard,", "tokens": ["Ist", "uns", "der", "Graf", ",", "der", "E\u00b7ber\u00b7hard", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wettersturm im Krieg.", "tokens": ["Ein", "Wet\u00b7ter\u00b7sturm", "im", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und auch sein Bub, der Ulerich,", "tokens": ["Und", "auch", "sein", "Bub", ",", "der", "U\u00b7le\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War gern, wo's eisern klang;", "tokens": ["War", "gern", ",", "wo's", "ei\u00b7sern", "klang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Grafen Bub, der Ulerich,", "tokens": ["Des", "Gra\u00b7fen", "Bub", ",", "der", "U\u00b7le\u00b7rich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Fu\u00dfbreit r\u00fcckw\u00e4rts zog er sich,", "tokens": ["Kein", "Fu\u00df\u00b7breit", "r\u00fcck\u00b7w\u00e4rts", "zog", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenns drauf und drunter sprang.", "tokens": ["Wenns", "drauf", "und", "drun\u00b7ter", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "KON", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Reutlinger, auf unsern Glanz", "tokens": ["Die", "Reut\u00b7lin\u00b7ger", ",", "auf", "un\u00b7sern", "Glanz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erbittert, kochten Gift,", "tokens": ["Er\u00b7bit\u00b7tert", ",", "koch\u00b7ten", "Gift", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und buhlten um den Siegeskranz", "tokens": ["Und", "buhl\u00b7ten", "um", "den", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wagten manchen Schwertertanz", "tokens": ["Und", "wag\u00b7ten", "man\u00b7chen", "Schwer\u00b7ter\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und g\u00fcrteten die H\u00fcft \u2013", "tokens": ["Und", "g\u00fcr\u00b7te\u00b7ten", "die", "H\u00fcft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er griff sie an \u2013 und siegte nicht,", "tokens": ["Er", "griff", "sie", "an", "\u2013", "und", "sieg\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kam gepantscht nach Haus,", "tokens": ["Und", "kam", "ge\u00b7pant\u00b7scht", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Vater schnitt ein falsch Gesicht,", "tokens": ["Der", "Va\u00b7ter", "schnitt", "ein", "falsch", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der junge Kriegsmann floh das Licht,", "tokens": ["Der", "jun\u00b7ge", "Kriegs\u00b7mann", "floh", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Tr\u00e4nen drangen raus.", "tokens": ["Und", "Tr\u00e4\u00b7nen", "dran\u00b7gen", "raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das wurmt ihm \u2013 Ha! ihr Schurken, wart'!", "tokens": ["Das", "wurmt", "ihm", "\u2013", "Ha", "!", "ihr", "Schur\u00b7ken", ",", "wart'", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "ITJ", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trugs in seinem Kopf.", "tokens": ["Und", "trugs", "in", "sei\u00b7nem", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auswetzen, bei des Vaters Bart!", "tokens": ["Aus\u00b7wet\u00b7zen", ",", "bei", "des", "Va\u00b7ters", "Bart", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auswetzen wollt er diese Schart", "tokens": ["Aus\u00b7wet\u00b7zen", "wollt", "er", "die\u00b7se", "Schart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit manchem St\u00e4dtlerschopf.", "tokens": ["Mit", "man\u00b7chem", "St\u00e4dt\u00b7ler\u00b7schopf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Fehd entbrannte bald darauf,", "tokens": ["Und", "Fehd", "ent\u00b7brann\u00b7te", "bald", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zogen Ro\u00df und Mann", "tokens": ["Und", "zo\u00b7gen", "Ro\u00df", "und", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei D\u00f6ffingen mit hellem Hauf,", "tokens": ["Bei", "D\u00f6f\u00b7fin\u00b7gen", "mit", "hel\u00b7lem", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und heller gings dem Junker auf,", "tokens": ["Und", "hel\u00b7ler", "gings", "dem", "Jun\u00b7ker", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hurra! hei\u00df gings an.", "tokens": ["Und", "hur\u00b7ra", "!", "hei\u00df", "gings", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und unsers Heeres Losungswort", "tokens": ["Und", "un\u00b7sers", "Hee\u00b7res", "Lo\u00b7sungs\u00b7wort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War die verlorne Schlacht;", "tokens": ["War", "die", "ver\u00b7lor\u00b7ne", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ri\u00df uns wie die Windsbraut fort", "tokens": ["Das", "ri\u00df", "uns", "wie", "die", "Winds\u00b7braut", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schmi\u00df uns tief in Blut und Mord", "tokens": ["Und", "schmi\u00df", "uns", "tief", "in", "Blut", "und", "Mord"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in die Lanzennacht.", "tokens": ["Und", "in", "die", "Lan\u00b7zen\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der junge Graf voll L\u00f6wengrimm", "tokens": ["Der", "jun\u00b7ge", "Graf", "voll", "L\u00f6\u00b7wen\u00b7grimm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwung seinen Heldenstab,", "tokens": ["Schwung", "sei\u00b7nen", "Hel\u00b7den\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wild vor ihm ging das Ungest\u00fcm,", "tokens": ["Wild", "vor", "ihm", "ging", "das", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geheul und Winseln hinter ihm,", "tokens": ["Ge\u00b7heul", "und", "Win\u00b7seln", "hin\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und um ihn her das Grab.", "tokens": ["Und", "um", "ihn", "her", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch weh! ach weh! ein S\u00e4belhieb", "tokens": ["Doch", "weh", "!", "ach", "weh", "!", "ein", "S\u00e4\u00b7bel\u00b7hieb"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "XY", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sunk schwer auf sein Genick,", "tokens": ["Sunk", "schwer", "auf", "sein", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell um ihn her der Helden Trieb,", "tokens": ["Schnell", "um", "ihn", "her", "der", "Hel\u00b7den", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst! umsonst! erstarret blieb", "tokens": ["Um\u00b7sonst", "!", "um\u00b7sonst", "!", "er\u00b7star\u00b7ret", "blieb"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "ADV", "$.", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sterbend brach sein Blick.", "tokens": ["Und", "ster\u00b7bend", "brach", "sein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Best\u00fcrzung hemmt des Sieges Bahn,", "tokens": ["Be\u00b7st\u00fcr\u00b7zung", "hemmt", "des", "Sie\u00b7ges", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut weinte Feind und Freund \u2013", "tokens": ["Laut", "wein\u00b7te", "Feind", "und", "Freund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch f\u00fchrt der Graf die Reuter an:", "tokens": ["Hoch", "f\u00fchrt", "der", "Graf", "die", "Reu\u00b7ter", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Marsch, Kinder! In den Feind!", "tokens": ["Marsch", ",", "Kin\u00b7der", "!", "In", "den", "Feind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und Lanzen sausen feuriger,", "tokens": ["Und", "Lan\u00b7zen", "sau\u00b7sen", "feu\u00b7ri\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rache spornt sie all,", "tokens": ["Die", "Ra\u00b7che", "spornt", "sie", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rasch \u00fcber Leichen gings daher,", "tokens": ["Rasch", "\u00fc\u00b7ber", "Lei\u00b7chen", "gings", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00e4dtler laufen kreuz und quer", "tokens": ["Die", "St\u00e4dt\u00b7ler", "lau\u00b7fen", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "