{"textgrid.poem.24969": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Wanderer", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rastloser, der, vom alten Fluche wankend,", "tokens": ["Rast\u00b7lo\u00b7ser", ",", "der", ",", "vom", "al\u00b7ten", "Flu\u00b7che", "wan\u00b7kend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Wettersturme des Vergangnen schwankend,", "tokens": ["Im", "Wet\u00b7ter\u00b7stur\u00b7me", "des", "Ver\u00b7gang\u00b7nen", "schwan\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit irren Tritten durch das Weltall schweift,", "tokens": ["Mit", "ir\u00b7ren", "Trit\u00b7ten", "durch", "das", "Wel\u00b7tall", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem immer neu der Winterfrost der Jahre", "tokens": ["Dem", "im\u00b7mer", "neu", "der", "Win\u00b7ter\u00b7frost", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und der Jahrhunderte die greisen Haare,", "tokens": ["Und", "der", "Jahr\u00b7hun\u00b7der\u00b7te", "die", "grei\u00b7sen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wie Schnee der Alpen Haupt, bereift!", "tokens": ["Wie", "Schnee", "der", "Al\u00b7pen", "Haupt", ",", "be\u00b7reift", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber sahst du gehn die Menschenalter,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "sahst", "du", "gehn", "die", "Men\u00b7schen\u00b7al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neu zur Gruft erstehen gleich dem Falter,", "tokens": ["Und", "neu", "zur", "Gruft", "er\u00b7ste\u00b7hen", "gleich", "dem", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ew'gen Tod auf seinen Schwingen tr\u00e4gt,", "tokens": ["Der", "ew'\u00b7gen", "Tod", "auf", "sei\u00b7nen", "Schwin\u00b7gen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sahst die V\u00f6lker zu den toten Reichen,", "tokens": ["Und", "sahst", "die", "V\u00f6l\u00b7ker", "zu", "den", "to\u00b7ten", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie blasse Kinder zu der M\u00fctter Leichen,", "tokens": ["Wie", "blas\u00b7se", "Kin\u00b7der", "zu", "der", "M\u00fct\u00b7ter", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Staub zu dem Staub, ins Grab gelegt.", "tokens": ["Staub", "zu", "dem", "Staub", ",", "ins", "Grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Um Freude bettelnd klopfst du an die Pforte", "tokens": ["Um", "Freu\u00b7de", "bet\u00b7telnd", "klopfst", "du", "an", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jeder Zeit; doch jede ruft die Worte", "tokens": ["Von", "je\u00b7der", "Zeit", ";", "doch", "je\u00b7de", "ruft", "die", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "ADV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entgegen dir: Nimm unsre Schmerzen mit!", "tokens": ["Ent\u00b7ge\u00b7gen", "dir", ":", "Nimm", "uns\u00b7re", "Schmer\u00b7zen", "mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Lachen, um den Jammer zu bet\u00e4uben,", "tokens": ["Ein", "La\u00b7chen", ",", "um", "den", "Jam\u00b7mer", "zu", "be\u00b7t\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "D\u00fcnkt dich die Lust; wie welkes Laub umst\u00e4uben", "tokens": ["D\u00fcnkt", "dich", "die", "Lust", ";", "wie", "wel\u00b7kes", "Laub", "um\u00b7st\u00e4u\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "PWAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Menschheit Seufzer deinen Tritt.", "tokens": ["Der", "Menschheit", "Seuf\u00b7zer", "dei\u00b7nen", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt, da nach Aufgang deine Blicke schweifen,", "tokens": ["Jetzt", ",", "da", "nach", "Auf\u00b7gang", "dei\u00b7ne", "Bli\u00b7cke", "schwei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Z\u00e4hlst du am Himmelsrand die blassen Streifen,", "tokens": ["Z\u00e4hlst", "du", "am", "Him\u00b7mels\u00b7rand", "die", "blas\u00b7sen", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob einer noch zum ew'gen Morgen fehlt;", "tokens": ["Ob", "ei\u00b7ner", "noch", "zum", "ew'\u00b7gen", "Mor\u00b7gen", "fehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die D\u00e4mmrung naht, und auf die vierte Stunde", "tokens": ["Die", "D\u00e4mm\u00b7rung", "naht", ",", "und", "auf", "die", "vier\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weist jene Sternenuhr, die als Sekunde", "tokens": ["Weist", "je\u00b7ne", "Ster\u00b7ne\u00b7nuhr", ",", "die", "als", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "$,", "PRELS", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das \u00e4lteste Jahrtausend z\u00e4hlt.", "tokens": ["Das", "\u00e4l\u00b7tes\u00b7te", "Jahr\u00b7tau\u00b7send", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein frost'ger Hauch dringt durch des Ostens Spalten,", "tokens": ["Ein", "frost'\u00b7ger", "Hauch", "dringt", "durch", "des", "Os\u00b7tens", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Heerrauch wallt herab in grauen Falten;", "tokens": ["Und", "Heer\u00b7rauch", "wallt", "her\u00b7ab", "in", "grau\u00b7en", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Morgen tagt, doch tagt in Finsternis;", "tokens": ["Der", "Mor\u00b7gen", "tagt", ",", "doch", "tagt", "in", "Fins\u00b7ter\u00b7nis", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Angstvoll nur flattern einzle Himmelslichter;", "tokens": ["Angst\u00b7voll", "nur", "flat\u00b7tern", "einz\u00b7le", "Him\u00b7mels\u00b7lich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Erdsto\u00df schreitet n\u00e4her, der Vernichter;", "tokens": ["Der", "Erd\u00b7sto\u00df", "schrei\u00b7tet", "n\u00e4\u00b7her", ",", "der", "Ver\u00b7nich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Pol zu Pole klafft ein Ri\u00df.", "tokens": ["Von", "Pol", "zu", "Po\u00b7le", "klafft", "ein", "Ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Donner dr\u00f6hnt von fallenden Lawinen,", "tokens": ["Ein", "Don\u00b7ner", "dr\u00f6hnt", "von", "fal\u00b7len\u00b7den", "La\u00b7wi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Welt an Welt, Ruine an Ruinen,", "tokens": ["Und", "Welt", "an", "Welt", ",", "Ru\u00b7i\u00b7ne", "an", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "St\u00fcrzt zitternd durch die aschenbleiche Luft;", "tokens": ["St\u00fcrzt", "zit\u00b7ternd", "durch", "die", "asc\u00b7hen\u00b7blei\u00b7che", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Monde und die Wandelsterne rollen,", "tokens": ["Die", "Mon\u00b7de", "und", "die", "Wan\u00b7dels\u00b7ter\u00b7ne", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie auf den Sarg der Sterblichen die Schollen,", "tokens": ["Wie", "auf", "den", "Sarg", "der", "Sterb\u00b7li\u00b7chen", "die", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu ihren Sonnen in die Gruft.", "tokens": ["Zu", "ih\u00b7ren", "Son\u00b7nen", "in", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es knicken, losgerissen aus den Fugen,", "tokens": ["Es", "kni\u00b7cken", ",", "los\u00b7ge\u00b7ris\u00b7sen", "aus", "den", "Fu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00e4ulen, die den Bau der Sch\u00f6pfung trugen,", "tokens": ["Die", "S\u00e4u\u00b7len", ",", "die", "den", "Bau", "der", "Sch\u00f6p\u00b7fung", "tru\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie n\u00e4cht'ge Schatten in dem Strahl des Lichts,", "tokens": ["Wie", "n\u00e4cht'\u00b7ge", "Schat\u00b7ten", "in", "dem", "Strahl", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und durch die Nebel, wie sie niedertriefen,", "tokens": ["Und", "durch", "die", "Ne\u00b7bel", ",", "wie", "sie", "nie\u00b7der\u00b7trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "G\u00e4hnt in den ausgeleerten Himmelstiefen", "tokens": ["G\u00e4hnt", "in", "den", "aus\u00b7ge\u00b7leer\u00b7ten", "Him\u00b7mel\u00b7stie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das \u00f6de, grenzenlose Nichts.", "tokens": ["Das", "\u00f6\u00b7de", ",", "gren\u00b7zen\u00b7lo\u00b7se", "Nichts", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch du, o Seufzer auf des Ew'gen Lippe,", "tokens": ["Doch", "du", ",", "o", "Seuf\u00b7zer", "auf", "des", "Ew'\u00b7gen", "Lip\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Wandrer, sp\u00e4hst noch von der Tr\u00fcmmerklippe", "tokens": ["O", "Wand\u00b7rer", ",", "sp\u00e4hst", "noch", "von", "der", "Tr\u00fcm\u00b7mer\u00b7klip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des toten Weltalls nach dem k\u00fcnft'gen Einst;", "tokens": ["Des", "to\u00b7ten", "Welt\u00b7alls", "nach", "dem", "k\u00fcnft'\u00b7gen", "Einst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verronnen sind die Str\u00f6me und die Meere;", "tokens": ["Ver\u00b7ron\u00b7nen", "sind", "die", "Str\u00f6\u00b7me", "und", "die", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch aber ist sie nicht versiegt, die Z\u00e4hre,", "tokens": ["Noch", "a\u00b7ber", "ist", "sie", "nicht", "ver\u00b7siegt", ",", "die", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die brennendhei\u00dfe, die du weinst!", "tokens": ["Die", "bren\u00b7nend\u00b7hei\u00b7\u00dfe", ",", "die", "du", "weinst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "P