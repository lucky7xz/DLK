{"textgrid.poem.43313": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehst du, wie viel aus Psychens Blicken,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du, wie viel aus Psychens Blicken,", "tokens": ["Siehst", "du", ",", "wie", "viel", "aus", "Psy\u00b7chens", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus den versch\u00e4mten Wangen spricht?", "tokens": ["Aus", "den", "ver\u00b7sch\u00e4m\u00b7ten", "Wan\u00b7gen", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O liebe Doris! welch Entz\u00fccken! \u2013", "tokens": ["O", "lie\u00b7be", "Do\u00b7ris", "!", "welch", "Ent\u00b7z\u00fc\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NE", "$.", "PWAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sterblicher begreift es nicht!", "tokens": ["Ein", "Sterb\u00b7li\u00b7cher", "be\u00b7greift", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwillst du die K\u00f6niginn der Sch\u00f6nen,", "tokens": ["\u00bb", "willst", "du", "die", "K\u00f6\u00b7ni\u00b7ginn", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblich, wie Cythere seyn,", "tokens": ["U\u00b7nsterb\u00b7lich", ",", "wie", "Cy\u00b7the\u00b7re", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "VAINF", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gebeut! ein dienstbar Chor von Sch\u00f6nen", "tokens": ["Ge\u00b7beut", "!", "ein", "dienst\u00b7bar", "Chor", "von", "Sch\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll dir im Tempel Weihrauch streun,", "tokens": ["Soll", "dir", "im", "Tem\u00b7pel", "Weih\u00b7rauch", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Soll dir in Lydischweichen T\u00f6nen", "tokens": ["Soll", "dir", "in", "Ly\u00b7disc\u00b7hwei\u00b7chen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Manch ehrerbietig Loblied weihn.", "tokens": ["Manch", "ehr\u00b7er\u00b7bie\u00b7tig", "Lob\u00b7lied", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich soll die Harmonie der Sph\u00e4ren,", "tokens": ["Dich", "soll", "die", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich des Parnasses Hymnen ehren;", "tokens": ["Dich", "des", "Par\u00b7nas\u00b7ses", "Hym\u00b7nen", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Dein Trank soll edler G\u00f6tterwein,", "tokens": ["Dein", "Trank", "soll", "ed\u00b7ler", "G\u00f6t\u00b7ter\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ambrosia soll deine Speise seyn.\u00ab", "tokens": ["A\u00b7mbro\u00b7sia", "soll", "dei\u00b7ne", "Spei\u00b7se", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}