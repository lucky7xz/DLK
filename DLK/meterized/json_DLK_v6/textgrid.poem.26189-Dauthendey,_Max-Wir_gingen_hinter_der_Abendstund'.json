{"textgrid.poem.26189": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wir gingen hinter der Abendstund'", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gingen hinter der Abendstund',", "tokens": ["Wir", "gin\u00b7gen", "hin\u00b7ter", "der", "A\u00b7bend\u00b7stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie in einen Abgrund f\u00fchren finster die W\u00e4lder,", "tokens": ["Und", "wie", "in", "ei\u00b7nen", "Ab\u00b7grund", "f\u00fch\u00b7ren", "fins\u00b7ter", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Halme und \u00c4hren wankten am Rand der Felder,", "tokens": ["Hal\u00b7me", "und", "\u00c4h\u00b7ren", "wank\u00b7ten", "am", "Rand", "der", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und ein paar Wolken zogen zerpfl\u00fcckt hinein ins Land.", "tokens": ["Und", "ein", "paar", "Wol\u00b7ken", "zo\u00b7gen", "zer\u00b7pfl\u00fcckt", "hin\u00b7ein", "ins", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein gro\u00dfer Stern \u00fcber den Halmen wie eine Blume aufgegangen stand,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Stern", "\u00fc\u00b7ber", "den", "Hal\u00b7men", "wie", "ei\u00b7ne", "Blu\u00b7me", "auf\u00b7ge\u00b7gan\u00b7gen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+---+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Stark duftend die Holunderbl\u00fct' im d\u00e4mmerigen Busch verschwand,", "tokens": ["Stark", "duf\u00b7tend", "die", "Ho\u00b7lun\u00b7der\u00b7bl\u00fct'", "im", "d\u00e4m\u00b7me\u00b7ri\u00b7gen", "Busch", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Und Dunkelheit, wie eine Stumme, sich unter jeden Baum einfand.", "tokens": ["Und", "Dun\u00b7kel\u00b7heit", ",", "wie", "ei\u00b7ne", "Stum\u00b7me", ",", "sich", "un\u00b7ter", "je\u00b7den", "Baum", "ein\u00b7fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "ART", "NN", "$,", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "In dieser Stund' blieben die Wege der Erde kaum,", "tokens": ["In", "die\u00b7ser", "Stund'", "blie\u00b7ben", "die", "We\u00b7ge", "der", "Er\u00b7de", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-++--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und wie im Abgrund sucht die Hand nach einer Hand.", "tokens": ["Und", "wie", "im", "Ab\u00b7grund", "sucht", "die", "Hand", "nach", "ei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}