{"textgrid.poem.37598": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Am Eingang", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig rauschen heut des Waldes Kronen,", "tokens": ["Se\u00b7lig", "rau\u00b7schen", "heut", "des", "Wal\u00b7des", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Wind durchw\u00fchlt,", "tokens": ["Von", "dem", "Wind", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unten mag verschwiegne Ruhe lohnen,", "tokens": ["Un\u00b7ten", "mag", "ver\u00b7schwieg\u00b7ne", "Ru\u00b7he", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selig wer die Ferne mitgef\u00fchlt!", "tokens": ["Se\u00b7lig", "wer", "die", "Fer\u00b7ne", "mit\u00b7ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, wie ziehn sie heut an mir vor\u00fcber,", "tokens": ["Ach", ",", "wie", "ziehn", "sie", "heut", "an", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Fr\u00fchlingswind,", "tokens": ["In", "dem", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die dem Herzen immer n\u00e4her, lieber,", "tokens": ["Die", "dem", "Her\u00b7zen", "im\u00b7mer", "n\u00e4\u00b7her", ",", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil sie nie zu fern der Liebe sind.", "tokens": ["Weil", "sie", "nie", "zu", "fern", "der", "Lie\u00b7be", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr verstorbnen freundlichen Genossen,", "tokens": ["Ihr", "ver\u00b7storb\u00b7nen", "freund\u00b7li\u00b7chen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Habt ihr auf mich acht!", "tokens": ["Habt", "ihr", "auf", "mich", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "CARD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Immer hab' ich euch so ganz genossen,", "tokens": ["Im\u00b7mer", "hab'", "ich", "euch", "so", "ganz", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Niemals \u00fcber euch hinausgedacht.", "tokens": ["Nie\u00b7mals", "\u00fc\u00b7ber", "euch", "hin\u00b7aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Habt ihr mich, wie ich euch innig habe,", "tokens": ["Habt", "ihr", "mich", ",", "wie", "ich", "euch", "in\u00b7nig", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Freudig wird das Fest,", "tokens": ["Freu\u00b7dig", "wird", "das", "Fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch mein Herz wird euch zum ew'gen Grabe,", "tokens": ["Doch", "mein", "Herz", "wird", "euch", "zum", "ew'\u00b7gen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ihr treulos mich im Gl\u00fcck verge\u00dft.", "tokens": ["Wenn", "ihr", "treu\u00b7los", "mich", "im", "Gl\u00fcck", "ver\u00b7ge\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}