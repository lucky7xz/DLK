{"textgrid.poem.39831": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Ges\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft im Gewitter, Tr\u00fcbes mir zu sch\u00f6nen,", "tokens": ["Oft", "im", "Ge\u00b7wit\u00b7ter", ",", "Tr\u00fc\u00b7bes", "mir", "zu", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhuben sich die G\u00f6ttinnen des Sanges,", "tokens": ["Er\u00b7hu\u00b7ben", "sich", "die", "G\u00f6t\u00b7tin\u00b7nen", "des", "San\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Donner hallte f\u00fcrchterlichen Klanges:", "tokens": ["Der", "Don\u00b7ner", "hall\u00b7te", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Klan\u00b7ges", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es war der ", "tokens": ["Es", "war", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der Regen tropfte ernst herab durch banges", "tokens": ["Der", "Re\u00b7gen", "tropf\u00b7te", "ernst", "her\u00b7ab", "durch", "ban\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gew\u00f6lk', ein Bild sehns\u00fcchtig weichen Dranges:", "tokens": ["Ge\u00b7w\u00f6l\u00b7k'", ",", "ein", "Bild", "sehn\u00b7s\u00fcch\u00b7tig", "wei\u00b7chen", "Dran\u00b7ges", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Da sah ich zart gew\u00f6lbt, in lichter Bl\u00e4ue,", "tokens": ["Da", "sah", "ich", "zart", "ge\u00b7w\u00f6lbt", ",", "in", "lich\u00b7ter", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Regen eine Mischung und von Sonne,", "tokens": ["Von", "Re\u00b7gen", "ei\u00b7ne", "Misc\u00b7hung", "und", "von", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Farbenschmelz den Regenbogen wallen.", "tokens": ["Im", "Far\u00b7ben\u00b7schmelz", "den", "Re\u00b7gen\u00b7bo\u00b7gen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob auch ein ferner Donner rollend dr\u00e4ue,", "tokens": ["Ob", "auch", "ein", "fer\u00b7ner", "Don\u00b7ner", "rol\u00b7lend", "dr\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Arm umfasset Berg und Thal in Wonne.", "tokens": ["Sein", "Arm", "um\u00b7fas\u00b7set", "Berg", "und", "Thal", "in", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So l\u00e4chelt tr\u00f6stlich das ", "tokens": ["So", "l\u00e4\u00b7chelt", "tr\u00f6st\u00b7lich", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}