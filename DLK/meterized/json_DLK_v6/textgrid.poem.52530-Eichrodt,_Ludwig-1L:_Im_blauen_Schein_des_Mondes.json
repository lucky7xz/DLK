{"textgrid.poem.52530": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im blauen Schein des Mondes", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im blauen Schein des Mondes", "tokens": ["Im", "blau\u00b7en", "Schein", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seh ich die Wellen ziehn,", "tokens": ["Seh", "ich", "die", "Wel\u00b7len", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Rauschen h\u00f6r' ich die Wellen", "tokens": ["Rau\u00b7schen", "h\u00f6r'", "ich", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch Blumenlande hin.", "tokens": ["Durch", "Blu\u00b7men\u00b7lan\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6re die Fischlein pl\u00e4tschern,", "tokens": ["H\u00f6\u00b7re", "die", "Fisc\u00b7hlein", "pl\u00e4t\u00b7schern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Murmeln die Winde im Wald,", "tokens": ["Mur\u00b7meln", "die", "Win\u00b7de", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Hellklagende Vogelstimme", "tokens": ["Hell\u00b7kla\u00b7gen\u00b7de", "Vo\u00b7gel\u00b7stim\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am H\u00fcgel wiederhallt.", "tokens": ["Am", "H\u00fc\u00b7gel", "wie\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da lieg ich auf dem Rasen", "tokens": ["Da", "lieg", "ich", "auf", "dem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In lispelnder Linde Hut,", "tokens": ["In", "lis\u00b7peln\u00b7der", "Lin\u00b7de", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ist so ruheselig,", "tokens": ["Mir", "ist", "so", "ru\u00b7he\u00b7se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wunderswohl zu Muth.", "tokens": ["So", "wun\u00b7ders\u00b7wohl", "zu", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort lausch ich der singenden Quelle", "tokens": ["Dort", "lausch", "ich", "der", "sin\u00b7gen\u00b7den", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schaue den Nachthimmel an,", "tokens": ["Und", "schau\u00b7e", "den", "Nacht\u00b7him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mit den Augen folg ich", "tokens": ["Und", "mit", "den", "Au\u00b7gen", "folg", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sanften Wolkenbahn.", "tokens": ["Der", "sanf\u00b7ten", "Wol\u00b7ken\u00b7bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es wandern die wei\u00dfen Wolken", "tokens": ["Es", "wan\u00b7dern", "die", "wei\u00b7\u00dfen", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vorbei am schweigsamen Mond;", "tokens": ["Vor\u00b7bei", "am", "schweig\u00b7sa\u00b7men", "Mond", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Dort such ich Menschengesichter,", "tokens": ["Dort", "such", "ich", "Men\u00b7schen\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und finde sie wie gewohnt.", "tokens": ["Und", "fin\u00b7de", "sie", "wie", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sieh dort! zwei Sternchen flimmern", "tokens": ["Sieh", "dort", "!", "zwei", "Stern\u00b7chen", "flim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "CARD", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus lieblichem Wolkenflor,", "tokens": ["Aus", "lieb\u00b7li\u00b7chem", "Wol\u00b7ken\u00b7flor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir kommen die hellen Sterne", "tokens": ["Mir", "kom\u00b7men", "die", "hel\u00b7len", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als wie zwei Aeuglein vor.", "tokens": ["Als", "wie", "zwei", "A\u00b7e\u00b7u\u00b7glein", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wei\u00df nicht, wie das mich fasset!", "tokens": ["Wei\u00df", "nicht", ",", "wie", "das", "mich", "fas\u00b7set", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wahrlich, es ist kein Wahn \u2013", "tokens": ["Wahr\u00b7lich", ",", "es", "ist", "kein", "Wahn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Z\u00fcge der Geliebten", "tokens": ["Die", "Z\u00fc\u00b7ge", "der", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie l\u00e4cheln hold mich an.", "tokens": ["Sie", "l\u00e4\u00b7cheln", "hold", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}