{"textgrid.poem.32297": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "An den Horaz", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horaz, wenn ich mein M\u00e4dchen k\u00fcsse,", "tokens": ["Ho\u00b7raz", ",", "wenn", "ich", "mein", "M\u00e4d\u00b7chen", "k\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entflammt von unserm Gott, dem Wein,", "tokens": ["Ent\u00b7flammt", "von", "un\u00b7serm", "Gott", ",", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann seh ich, ohne kritsche Schl\u00fcsse,", "tokens": ["Dann", "seh", "ich", ",", "oh\u00b7ne", "krit\u00b7sche", "Schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich tiefer als zehn Bentleys ein.", "tokens": ["Dich", "tie\u00b7fer", "als", "zehn", "Bent\u00b7leys", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann f\u00fchl' ich sie, die s\u00fc\u00dfen K\u00fcsse,", "tokens": ["Dann", "f\u00fchl'", "ich", "sie", ",", "die", "s\u00fc\u00b7\u00dfen", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ein barbarscher Bi\u00df verletzt,", "tokens": ["Die", "ein", "bar\u00b7bar\u00b7scher", "Bi\u00df", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie, welche Venus, nebst dem Bisse,", "tokens": ["Sie", ",", "wel\u00b7che", "Ve\u00b7nus", ",", "nebst", "dem", "Bis\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihres Nektars F\u00fcnfteil netzt.", "tokens": ["Mit", "ih\u00b7res", "Nek\u00b7tars", "F\u00fcnf\u00b7teil", "netzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann f\u00fchl' ich, mehr als ich kann sagen,", "tokens": ["Dann", "f\u00fchl'", "ich", ",", "mehr", "als", "ich", "kann", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttin, durch die Laura k\u00fc\u00dft,", "tokens": ["Die", "G\u00f6t\u00b7tin", ",", "durch", "die", "Lau\u00b7ra", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie sich Amathunts entschlagen,", "tokens": ["Wie", "sie", "sich", "A\u00b7mat\u00b7hunts", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ganz in mich gest\u00fcrzet ist.", "tokens": ["Und", "ganz", "in", "mich", "ge\u00b7st\u00fcr\u00b7zet", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie herrscht im Herzen, sie gebietet;", "tokens": ["Sie", "herrscht", "im", "Her\u00b7zen", ",", "sie", "ge\u00b7bie\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Laura l\u00f6scht die Phyllis aus.", "tokens": ["Und", "Lau\u00b7ra", "l\u00f6scht", "die", "Phyl\u00b7lis", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie herrscht im Herzen? nein, sie w\u00fctet;", "tokens": ["Sie", "herrscht", "im", "Her\u00b7zen", "?", "nein", ",", "sie", "w\u00fc\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Laura h\u00e4lt mich ab vom Schmaus.", "tokens": ["Denn", "Lau\u00b7ra", "h\u00e4lt", "mich", "ab", "vom", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}