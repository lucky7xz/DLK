{"dta.poem.9476": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n ODE/  \n  Darinnen die ersten beyden Verse Tro-  \n ch\u00e4isch/ der 3. und 4. Jambisch/ der 5. wider Tro-  \n ch\u00e4isch/ der 6. und 7. aber Dacty-  \n lisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! mein Licht/", "tokens": ["Ach", "!", "mein", "Licht", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "La\u00df mich nicht", "tokens": ["La\u00df", "mich", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Verwarten hier/", "tokens": ["Ver\u00b7war\u00b7ten", "hier", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich mu\u00df von Dier.", "tokens": ["Ich", "mu\u00df", "von", "Dier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}