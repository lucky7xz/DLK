{"textgrid.poem.40621": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Ferdinand, dem's fast gelang durch Milde zu vers\u00f6hnen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Ferdinand, dem's fast gelang durch Milde zu vers\u00f6hnen", "tokens": ["O", "Fer\u00b7di\u00b7nand", ",", "dem's", "fast", "ge\u00b7lang", "durch", "Mil\u00b7de", "zu", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mit deines Namens d\u00fcstrem Klang, vererbt den Habsburgs\u00f6hnen", "tokens": ["Mit", "dei\u00b7nes", "Na\u00b7mens", "d\u00fcst\u00b7rem", "Klang", ",", "ver\u00b7erbt", "den", "Habs\u00b7burg\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie einer grausen Ahnenthat noch unges\u00fchnte Kunde,", "tokens": ["Wie", "ei\u00b7ner", "grau\u00b7sen", "Ah\u00b7nen\u00b7that", "noch", "un\u00b7ge\u00b7s\u00fchn\u00b7te", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Wie des zerri\u00dfnen Vaterlands fortblutend offne Wunde.", "tokens": ["Wie", "des", "zer\u00b7ri\u00df\u00b7nen", "Va\u00b7ter\u00b7lands", "fort\u00b7blu\u00b7tend", "off\u00b7ne", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "So \u00fcbergro\u00df ist deine Huld, so fremd dem heut'gen Tage,", "tokens": ["So", "\u00fc\u00b7ber\u00b7gro\u00df", "ist", "dei\u00b7ne", "Huld", ",", "so", "fremd", "dem", "heut'\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Als kl\u00e4ng' aus alter M\u00e4rchenzeit die r\u00fchrend zarte Sage;", "tokens": ["Als", "kl\u00e4ng'", "aus", "al\u00b7ter", "M\u00e4r\u00b7chen\u00b7zeit", "die", "r\u00fch\u00b7rend", "zar\u00b7te", "Sa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Im Zauberstrahl der Dichterwelt begegn' ich deinem Bilde,", "tokens": ["Im", "Zau\u00b7ber\u00b7strahl", "der", "Dich\u00b7ter\u00b7welt", "be\u00b7gegn'", "ich", "dei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "In einem Land, in einer Zeit, die wie dein Herz so milde.", "tokens": ["In", "ei\u00b7nem", "Land", ",", "in", "ei\u00b7ner", "Zeit", ",", "die", "wie", "dein", "Herz", "so", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "KOKOM", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Dort thronst du im Provencerthal, genannt Ren\u00e9 der Gute,", "tokens": ["Dort", "thronst", "du", "im", "Pro\u00b7ven\u00b7cer\u00b7thal", ",", "ge\u00b7nannt", "Ren\u00e9", "der", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "VVPP", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem lieblich wie ein Rebenkranz sein Reich zu F\u00fc\u00dfen ruhte,", "tokens": ["Dem", "lieb\u00b7lich", "wie", "ein", "Re\u00b7ben\u00b7kranz", "sein", "Reich", "zu", "F\u00fc\u00b7\u00dfen", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Da schmiegt sich auch so rebengleich dein Volk zu deinem Throne", "tokens": ["Da", "schmiegt", "sich", "auch", "so", "re\u00b7ben\u00b7gleich", "dein", "Volk", "zu", "dei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und gie\u00dft sein goldnes Traubenblut zum Golde deiner Krone.", "tokens": ["Und", "gie\u00dft", "sein", "gold\u00b7nes", "Trau\u00b7ben\u00b7blut", "zum", "Gol\u00b7de", "dei\u00b7ner", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Gesetze bl\u00fchn als Blumenschrift und klingen als Ges\u00e4nge,", "tokens": ["Ge\u00b7set\u00b7ze", "bl\u00fchn", "als", "Blu\u00b7men\u00b7schrift", "und", "klin\u00b7gen", "als", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "NN", "KON", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Von Milch und Honig flie\u00dft die Trift, von Wein und Oel die H\u00e4nge,", "tokens": ["Von", "Milch", "und", "Ho\u00b7nig", "flie\u00dft", "die", "Trift", ",", "von", "Wein", "und", "O\u00b7el", "die", "H\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Das Meer sp\u00fclt Perlen an den Strand, der Berg treibt Silberbl\u00fcthe,", "tokens": ["Das", "Meer", "sp\u00fclt", "Per\u00b7len", "an", "den", "Strand", ",", "der", "Berg", "treibt", "Sil\u00b7ber\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Als ob dein Herz nur rings im Land fortkl\u00e4nge, spro\u00dfte, gl\u00fchte!", "tokens": ["Als", "ob", "dein", "Herz", "nur", "rings", "im", "Land", "fort\u00b7kl\u00e4n\u00b7ge", ",", "spro\u00df\u00b7te", ",", "gl\u00fch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Dein Zepter ist ein gr\u00fcner Zweig, dran wei\u00dfe Lilien wallen,", "tokens": ["Dein", "Zep\u00b7ter", "ist", "ein", "gr\u00fc\u00b7ner", "Zweig", ",", "dran", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Dein K\u00f6nigsmantel bl\u00fcthenwei\u00df wie Schnee, der frisch gefallen;", "tokens": ["Dein", "K\u00f6\u00b7nigs\u00b7man\u00b7tel", "bl\u00fc\u00b7then\u00b7wei\u00df", "wie", "Schnee", ",", "der", "frisch", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der R\u00f6mer warb im wei\u00dfen Kleid um Stimmensieg beim W\u00e4hlen,", "tokens": ["Der", "R\u00f6\u00b7mer", "warb", "im", "wei\u00b7\u00dfen", "Kleid", "um", "Stim\u00b7men\u00b7sieg", "beim", "W\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Du Kandidat auf goldnem Thron wirbst um die Huld der Seelen.", "tokens": ["Du", "Kan\u00b7di\u00b7dat", "auf", "gold\u00b7nem", "Thron", "wirbst", "um", "die", "Huld", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Wohl sinnen Andre auf dem Thron, die V\u00f6lker zu erdr\u00fccken,", "tokens": ["Wohl", "sin\u00b7nen", "And\u00b7re", "auf", "dem", "Thron", ",", "die", "V\u00f6l\u00b7ker", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dein Haupt doch sinnt erfindungsreich, die Herzen zu begl\u00fccken;", "tokens": ["Dein", "Haupt", "doch", "sinnt", "er\u00b7fin\u00b7dungs\u00b7reich", ",", "die", "Her\u00b7zen", "zu", "be\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So wird die Mu\u00dfe dir zum Ruhm, Festspiele deine Kriege,", "tokens": ["So", "wird", "die", "Mu\u00b7\u00dfe", "dir", "zum", "Ruhm", ",", "Fest\u00b7spie\u00b7le", "dei\u00b7ne", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und deine G\u00fcte Heldenthum, Wohlthaten deine Siege.", "tokens": ["Und", "dei\u00b7ne", "G\u00fc\u00b7te", "Hel\u00b7den\u00b7thum", ",", "Wohlt\u00b7ha\u00b7ten", "dei\u00b7ne", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Weil alle Wirklichkeit zu arm f\u00fcr deinen Drang zu helfen,", "tokens": ["Weil", "al\u00b7le", "Wirk\u00b7lich\u00b7keit", "zu", "arm", "f\u00fcr", "dei\u00b7nen", "Drang", "zu", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Verliehen deinem K\u00f6nigsarm Heilkr\u00e4fte milde Elfen;", "tokens": ["Ver\u00b7lie\u00b7hen", "dei\u00b7nem", "K\u00f6\u00b7nigs\u00b7arm", "Heil\u00b7kr\u00e4f\u00b7te", "mil\u00b7de", "El\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ein offner Kelch ward deine Hand, drein g\u00fct'ge Feen gie\u00dfen", "tokens": ["Ein", "off\u00b7ner", "Kelch", "ward", "dei\u00b7ne", "Hand", ",", "drein", "g\u00fct'\u00b7ge", "Feen", "gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Die Wellen Golds, die dann vom Rand verschwendrisch \u00fcberflie\u00dfen.", "tokens": ["Die", "Wel\u00b7len", "Golds", ",", "die", "dann", "vom", "Rand", "ver\u00b7schwend\u00b7risch", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Es taumelt der Geschichte Strom berauscht durch deine Grenze", "tokens": ["Es", "tau\u00b7melt", "der", "Ge\u00b7schich\u00b7te", "Strom", "be\u00b7rauscht", "durch", "dei\u00b7ne", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und lallt nur deine Lieder nach und tr\u00e4gt nur deine Kr\u00e4nze;", "tokens": ["Und", "lallt", "nur", "dei\u00b7ne", "Lie\u00b7der", "nach", "und", "tr\u00e4gt", "nur", "dei\u00b7ne", "Kr\u00e4n\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wallfahrer schickt dir Nord und S\u00fcd, die Leidenden und Kranken,", "tokens": ["Wall\u00b7fah\u00b7rer", "schickt", "dir", "Nord", "und", "S\u00fcd", ",", "die", "Lei\u00b7den\u00b7den", "und", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NE", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Bis dir, vom Geben, Segnen m\u00fcd', ersch\u00f6pft die Arme sanken.", "tokens": ["Bis", "dir", ",", "vom", "Ge\u00b7ben", ",", "Seg\u00b7nen", "m\u00fcd'", ",", "er\u00b7sch\u00f6pft", "die", "Ar\u00b7me", "san\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "$,", "NN", "VMFIN", "$,", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Einst schreiben sie auf deinen Stein und schreiben sch\u00f6n eint\u00f6nig", "tokens": ["Einst", "schrei\u00b7ben", "sie", "auf", "dei\u00b7nen", "Stein", "und", "schrei\u00b7ben", "sch\u00f6n", "ein\u00b7t\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die Grabschrift auch den Herzen ein: \u00bbHier ruht der gute K\u00f6nig.\u00ab \u2013", "tokens": ["Die", "Grab\u00b7schrift", "auch", "den", "Her\u00b7zen", "ein", ":", "\u00bb", "Hier", "ruht", "der", "gu\u00b7te", "K\u00f6\u00b7nig", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So hat dich fromm ein Dichterherz entr\u00fcckt in Vorweltr\u00e4ume,", "tokens": ["So", "hat", "dich", "fromm", "ein", "Dich\u00b7ter\u00b7herz", "ent\u00b7r\u00fcckt", "in", "Vor\u00b7welt\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Da\u00df es dich Besten deines Stammes den Gl\u00fccklichsten auch tr\u00e4ume.", "tokens": ["Da\u00df", "es", "dich", "Bes\u00b7ten", "dei\u00b7nes", "Stam\u00b7mes", "den", "Gl\u00fcck\u00b7lichs\u00b7ten", "auch", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--++--+-", "measure": "iambic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "Umsonst! umsonst! Ein Wehschrei t\u00f6nt empor aus deinem Volke,", "tokens": ["Um\u00b7sonst", "!", "um\u00b7sonst", "!", "Ein", "Weh\u00b7schrei", "t\u00f6nt", "em\u00b7por", "aus", "dei\u00b7nem", "Vol\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "ART", "NN", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Rasch auf den Grund der Gegenwart senkt dich die goldne Wolke;", "tokens": ["Rasch", "auf", "den", "Grund", "der", "Ge\u00b7gen\u00b7wart", "senkt", "dich", "die", "gold\u00b7ne", "Wol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Da spritzt ein Tropfen Bluts auf dich vom fernen Weichselstrande,", "tokens": ["Da", "spritzt", "ein", "Trop\u00b7fen", "Bluts", "auf", "dich", "vom", "fer\u00b7nen", "Weich\u00b7sel\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Der zu gemeinem K\u00f6nigsroth dir f\u00e4rbt die Schneegewande.", "tokens": ["Der", "zu", "ge\u00b7mei\u00b7nem", "K\u00f6\u00b7nigs\u00b7roth", "dir", "f\u00e4rbt", "die", "Schnee\u00b7ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Und deinen Thron nicht mehr umstehn lichtfrohe Feen und Elfen,", "tokens": ["Und", "dei\u00b7nen", "Thron", "nicht", "mehr", "um\u00b7stehn", "licht\u00b7fro\u00b7he", "Feen", "und", "El\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ein leidend Volk nur blieb zu flehn: an dir ist's, Herr, zu helfen!", "tokens": ["Ein", "lei\u00b7dend", "Volk", "nur", "blieb", "zu", "flehn", ":", "an", "dir", "ist's", ",", "Herr", ",", "zu", "hel\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$.", "APPR", "PPER", "VAFIN", "$,", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Zufriedner ist's als andre nicht, geduld'ger nur und treuer,", "tokens": ["Zu\u00b7fried\u00b7ner", "ist's", "als", "and\u00b7re", "nicht", ",", "ge\u00b7duld'\u00b7ger", "nur", "und", "treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "PIS", "PTKNEG", "$,", "ADJA", "ADV", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Doch in den Herzen knirscht sein Zorn und tobt sein strafend Feuer.", "tokens": ["Doch", "in", "den", "Her\u00b7zen", "knirscht", "sein", "Zorn", "und", "tobt", "sein", "stra\u00b7fend", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "Leg' auf sein Haupt die K\u00f6nigshand, heilkr\u00e4ftig noch zur Stunde,", "tokens": ["Leg'", "auf", "sein", "Haupt", "die", "K\u00f6\u00b7nigs\u00b7hand", ",", "heil\u00b7kr\u00e4f\u00b7tig", "noch", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Senk' an sein Herz dein lauschend Ohr, da pocht dir solche Kunde:", "tokens": ["Senk'", "an", "sein", "Herz", "dein", "lau\u00b7schend", "Ohr", ",", "da", "pocht", "dir", "sol\u00b7che", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbich knirsch' im Zorn ob deines Reichs unr\u00fchmlichem Verfallen,", "tokens": ["\u00bb", "ich", "knir\u00b7sch'", "im", "Zorn", "ob", "dei\u00b7nes", "Reichs", "un\u00b7r\u00fchm\u00b7li\u00b7chem", "Ver\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "KOUS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das ragen k\u00f6nnte hoch und stark, der Stolz und Preis von allen!", "tokens": ["Das", "ra\u00b7gen", "k\u00f6nn\u00b7te", "hoch", "und", "stark", ",", "der", "Stolz", "und", "Preis", "von", "al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "KON", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.13": {"line.1": {"text": "Ich knirsche, weil der V\u00e4ter Blut, die Wetter der Geschichte", "tokens": ["Ich", "knir\u00b7sche", ",", "weil", "der", "V\u00e4\u00b7ter", "Blut", ",", "die", "Wet\u00b7ter", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ich jetzt an deinen R\u00e4then seh' verloren und zunichte;", "tokens": ["Ich", "jetzt", "an", "dei\u00b7nen", "R\u00e4\u00b7then", "seh'", "ver\u00b7lo\u00b7ren", "und", "zu\u00b7nich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "F\u00fcr Gr\u00f6\u00dfres wahrlich galt der Kampf als f\u00fcr die eine Sippe,", "tokens": ["F\u00fcr", "Gr\u00f6\u00df\u00b7res", "wahr\u00b7lich", "galt", "der", "Kampf", "als", "f\u00fcr", "die", "ei\u00b7ne", "Sip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "ART", "NN", "KOKOM", "APPR", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Als f\u00fcr ein alternd Kaiserhaupt und f\u00fcr Cimburga's Lippe.", "tokens": ["Als", "f\u00fcr", "ein", "al\u00b7ternd", "Kai\u00b7ser\u00b7haupt", "und", "f\u00fcr", "Cim\u00b7bur\u00b7ga's", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJD", "NN", "KON", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.14": {"line.1": {"text": "Hut ab, und sei's ein K\u00f6nigshut! vor diesem Volk, dem edeln,", "tokens": ["Hut", "ab", ",", "und", "sei's", "ein", "K\u00f6\u00b7nigs\u00b7hut", "!", "vor", "die\u00b7sem", "Volk", ",", "dem", "e\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "NE", "ART", "NN", "$.", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Das nie das Lieben, doch verlernt das Schmeicheln hat und Wedeln,", "tokens": ["Das", "nie", "das", "Lie\u00b7ben", ",", "doch", "ver\u00b7lernt", "das", "Schmei\u00b7cheln", "hat", "und", "We\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "$,", "ADV", "VVFIN", "ART", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und das sein kostbar Blut nur sch\u00e4tzt nach wahrem Preis und Werthe,", "tokens": ["Und", "das", "sein", "kost\u00b7bar", "Blut", "nur", "sch\u00e4tzt", "nach", "wah\u00b7rem", "Preis", "und", "Wert\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "ADJD", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Wenn's vom verge\u00dfnen Zahler jetzt voraus den Sold begehrte.", "tokens": ["Wenn's", "vom", "ver\u00b7ge\u00df\u00b7nen", "Zah\u00b7ler", "jetzt", "vo\u00b7raus",