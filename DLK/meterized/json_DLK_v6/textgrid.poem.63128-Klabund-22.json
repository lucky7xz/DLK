{"textgrid.poem.63128": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "22", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der blassen D\u00e4mmerung", "tokens": ["Aus", "der", "blas\u00b7sen", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Fuhren deine Silberblicke", "tokens": ["Fuh\u00b7ren", "dei\u00b7ne", "Sil\u00b7berb\u00b7li\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zwei Speere. Im Genicke", "tokens": ["Wie", "zwei", "Spee\u00b7re", ".", "Im", "Ge\u00b7ni\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "CARD", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlt ich ihren Eisensprung.", "tokens": ["F\u00fchlt", "ich", "ih\u00b7ren", "Ei\u00b7sen\u00b7sprung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es warf mich auf das Flie\u00df.", "tokens": ["Und", "es", "warf", "mich", "auf", "das", "Flie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Sterbender die H\u00e4nde", "tokens": ["Wie", "ein", "Ster\u00b7ben\u00b7der", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hob ich in die roten Br\u00e4nde", "tokens": ["Hob", "ich", "in", "die", "ro\u00b7ten", "Br\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Seele, welche mich verstie\u00df.", "tokens": ["Dei\u00b7ner", "See\u00b7le", ",", "wel\u00b7che", "mich", "ver\u00b7stie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}