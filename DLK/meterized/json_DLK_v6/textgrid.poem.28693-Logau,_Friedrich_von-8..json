{"textgrid.poem.28693": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Novula hat gar nicht gerne, was auch andre Weiber haben,", "tokens": ["No\u00b7vu\u00b7la", "hat", "gar", "nicht", "ger\u00b7ne", ",", "was", "auch", "and\u00b7re", "Wei\u00b7ber", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "ADV", "$,", "PRELS", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df sie dann braucht noch die Nase samm den andren finstren Gaben,", "tokens": ["Da\u00df", "sie", "dann", "braucht", "noch", "die", "Na\u00b7se", "samm", "den", "an\u00b7dren", "finst\u00b7ren", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dran ein ieder, auch ein Bauer, sich gemeinlich pflegt zu laben.", "tokens": ["Dran", "ein", "ie\u00b7der", ",", "auch", "ein", "Bau\u00b7er", ",", "sich", "ge\u00b7mein\u00b7lich", "pflegt", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PIAT", "$,", "ADV", "ART", "NN", "$,", "PRF", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}