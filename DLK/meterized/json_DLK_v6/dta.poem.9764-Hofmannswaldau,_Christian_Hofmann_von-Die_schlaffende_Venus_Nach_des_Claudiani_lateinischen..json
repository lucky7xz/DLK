{"dta.poem.9764": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schlaffende Venus  \n Nach des Claudiani lateinischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sonne hatte kaum den mittag hei\u00df gemacht/", "tokens": ["Die", "son\u00b7ne", "hat\u00b7te", "kaum", "den", "mit\u00b7tag", "hei\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Venus gantz ermatt ihr eine h\u00f6l erwehlet/", "tokens": ["Als", "Ve\u00b7nus", "gantz", "er\u00b7matt", "ihr", "ei\u00b7ne", "h\u00f6l", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo weder schlaff noch ruh/ noch k\u00fchler schatten fehlet/", "tokens": ["Wo", "we\u00b7der", "schlaff", "noch", "ruh", "/", "noch", "k\u00fch\u00b7ler", "schat\u00b7ten", "feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "VVFIN", "ADV", "NN", "$(", "ADV", "ADJD", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo ein reben-blat gab dunckel-gr\u00fcne nacht/", "tokens": ["Und", "wo", "ein", "re\u00b7ben\u00b7blat", "gab", "dun\u00b7ckel\u00b7gr\u00fc\u00b7ne", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In die ein linder west mit sanftem rauschen spielte/", "tokens": ["In", "die", "ein", "lin\u00b7der", "west", "mit", "sanf\u00b7tem", "rau\u00b7schen", "spiel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und so der g\u00f6ttin hertz und m\u00fcde seele k\u00fchlte.", "tokens": ["Und", "so", "der", "g\u00f6t\u00b7tin", "hertz", "und", "m\u00fc\u00b7de", "see\u00b7le", "k\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "KON", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie warf die sternen-pracht/ die glieder in das gra\u00df/", "tokens": ["Sie", "warf", "die", "ster\u00b7nen\u00b7pracht", "/", "die", "glie\u00b7der", "in", "das", "gra\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der blumen h\u00f6chster wunsch war so gedr\u00fcckt zu werden/", "tokens": ["Der", "blu\u00b7men", "h\u00f6chs\u00b7ter", "wunsch", "war", "so", "ge\u00b7dr\u00fcckt", "zu", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "ADV", "VVPP", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die nelcke schien ein feur/ die ros\u2019 ein stern der erden/", "tokens": ["Die", "nel\u00b7cke", "schien", "ein", "feur", "/", "die", "ros'", "ein", "stern", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die veilg ein blau saphir/ die lilg ein spiegelgla\u00df/", "tokens": ["Die", "veilg", "ein", "blau", "sa\u00b7phir", "/", "die", "lilg", "ein", "spie\u00b7gel\u00b7gla\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "PPER", "$(", "ART", "NN", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Venus goldnes haupt entschlieff nur auff narcissen.", "tokens": ["Und", "Ve\u00b7nus", "gold\u00b7nes", "haupt", "ent\u00b7schlieff", "nur", "auff", "nar\u00b7cis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVFIN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jesminen legten sich zu pf\u00fcl und unterk\u00fcssen/", "tokens": ["Jes\u00b7mi\u00b7nen", "leg\u00b7ten", "sich", "zu", "pf\u00fcl", "und", "un\u00b7ter\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NE", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So lag die lust der welt ohn alle kleider blo\u00df/", "tokens": ["So", "lag", "die", "lust", "der", "welt", "ohn", "al\u00b7le", "klei\u00b7der", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PIAT", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem die volle brust die trauben nachbar nennte/", "tokens": ["In\u00b7dem", "die", "vol\u00b7le", "brust", "die", "trau\u00b7ben", "nach\u00b7bar", "nenn\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und der belebte schnee von zwey rubinen brennte.", "tokens": ["Und", "der", "be\u00b7leb\u00b7te", "schnee", "von", "zwey", "ru\u00b7bi\u00b7nen", "brenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hold/ freude/ lieb und gunst ruht\u2019 in der sch\u00f6nen schoos/", "tokens": ["Hold", "/", "freu\u00b7de", "/", "lieb", "und", "gunst", "ruht'", "in", "der", "sch\u00f6\u00b7nen", "schoos", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "ADJD", "KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der s\u00fc\u00df geschwollne mund war etwas aufgeschlossen/", "tokens": ["Der", "s\u00fc\u00df", "ge\u00b7schwoll\u00b7ne", "mund", "war", "et\u00b7was", "auf\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Aus dem die zucker-b\u00e4ch und nectar-quellen flossen.", "tokens": ["Aus", "dem", "die", "zu\u00b7cke\u00b7rb\u00e4ch", "und", "nec\u00b7ta\u00b7rquel\u00b7len", "flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den schlaf erg\u00f6tzte noch ein angenehmer bach/", "tokens": ["Den", "schlaf", "er\u00b7g\u00f6tz\u00b7te", "noch", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "bach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der sein bemostes haupt mit reinem silber tr\u00e4nckte/", "tokens": ["Der", "sein", "be\u00b7mos\u00b7tes", "haupt", "mit", "rei\u00b7nem", "sil\u00b7ber", "tr\u00e4nck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und nichts als liebligkeit auf gr\u00fcne wiesen lenckte/", "tokens": ["Und", "nichts", "als", "lieb\u00b7lig\u00b7keit", "auf", "gr\u00fc\u00b7ne", "wie\u00b7sen", "lenck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der etwas zitternd flo\u00df/ und k\u00fcsse nach und nach", "tokens": ["Der", "et\u00b7was", "zit\u00b7ternd", "flo\u00df", "/", "und", "k\u00fcs\u00b7se", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dem lieben ufer gab/ das lorber-b\u00e4ume zierten/", "tokens": ["Dem", "lie\u00b7ben", "u\u00b7fer", "gab", "/", "das", "lor\u00b7ber\u00b7b\u00e4u\u00b7me", "zier\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Um die die Gratien holdreiche t\u00e4ntze f\u00fchrten/", "tokens": ["Um", "die", "die", "Gra\u00b7ti\u00b7en", "hold\u00b7rei\u00b7che", "t\u00e4nt\u00b7ze", "f\u00fchr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bi\u00df sie auch m\u00fcd und matt die augen schlossen zu.", "tokens": ["Bi\u00df", "sie", "auch", "m\u00fcd", "und", "matt", "die", "au\u00b7gen", "schlos\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die liebes-engel gehn indessen wie sie wollen/", "tokens": ["Die", "lie\u00b7bes\u00b7en\u00b7gel", "gehn", "in\u00b7des\u00b7sen", "wie", "sie", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und dencken da\u00df sie samt der mutter ruhen sollen;", "tokens": ["Und", "den\u00b7cken", "da\u00df", "sie", "samt", "der", "mut\u00b7ter", "ru\u00b7hen", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der bogen hengt am baum/ der pfeil hat seine ruh.", "tokens": ["Der", "bo\u00b7gen", "hengt", "am", "baum", "/", "der", "pfeil", "hat", "sei\u00b7ne", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der leere k\u00f6cher seuftzt allein nach liebes-flammen/", "tokens": ["Der", "lee\u00b7re", "k\u00f6\u00b7cher", "seuftzt", "al\u00b7lein", "nach", "lie\u00b7bes\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die fl\u00fcgel-knaben gehn/ und spielen eins zusammen/", "tokens": ["Die", "fl\u00fc\u00b7gel\u00b7kna\u00b7ben", "gehn", "/", "und", "spie\u00b7len", "eins", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "KON", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die laufen ins gestr\u00e4uch/ und nehmen nester aus/", "tokens": ["Die", "lau\u00b7fen", "ins", "ge\u00b7str\u00e4uch", "/", "und", "neh\u00b7men", "nes\u00b7ter", "aus", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$(", "KON", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die andern schwingen sich auf hohe jelmen-b\u00e4ume/", "tokens": ["Die", "an\u00b7dern", "schwin\u00b7gen", "sich", "auf", "ho\u00b7he", "jel\u00b7men\u00b7b\u00e4u\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Theils lesen \u00e4pfel auf/ theils h\u00fcten/ da\u00df die tr\u00e4ume", "tokens": ["Theils", "le\u00b7sen", "\u00e4p\u00b7fel", "auf", "/", "theils", "h\u00fc\u00b7ten", "/", "da\u00df", "die", "tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$(", "ADV", "VVINF", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der g\u00f6ttin niemand st\u00f6hr/ der bindet einen straus", "tokens": ["Der", "g\u00f6t\u00b7tin", "nie\u00b7mand", "st\u00f6hr", "/", "der", "bin\u00b7det", "ei\u00b7nen", "straus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VVFIN", "$(", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Von frischem kr\u00e4ntzelwerck/ und jener jagt die Faunen/", "tokens": ["Von", "fri\u00b7schem", "kr\u00e4nt\u00b7zel\u00b7werck", "/", "und", "je\u00b7ner", "jagt", "die", "Fau\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wirft feuer in ihr hertz/ da\u00df sie vor lieb erstaunen.", "tokens": ["Wirft", "feu\u00b7er", "in", "ihr", "hertz", "/", "da\u00df", "sie", "vor", "lieb", "er\u00b7stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Drauf bricht ein laut geschrey der Venus holden traum/", "tokens": ["Drauf", "bricht", "ein", "laut", "ge\u00b7schrey", "der", "Ve\u00b7nus", "hol\u00b7den", "traum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und von der n\u00e4chsten stadt kommt neue post geflogen/", "tokens": ["Und", "von", "der", "n\u00e4chs\u00b7ten", "stadt", "kommt", "neu\u00b7e", "post", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Chloris h\u00e4tte schon den brautrock angezogen.", "tokens": ["Die", "Chlo\u00b7ris", "h\u00e4t\u00b7te", "schon", "den", "braut\u00b7rock", "an\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mirmillo sey der schatz; wald/ acker/ thal und baum", "tokens": ["Mir\u00b7mil\u00b7lo", "sey", "der", "schatz", ";", "wald", "/", "ac\u00b7ker", "/", "thal", "und", "baum"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "ADV", "$(", "XY", "$("