{"textgrid.poem.40548": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tritt ruhmbekr\u00f6nten Gr\u00f6\u00dfen nicht zu nah!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt ruhmbekr\u00f6nten Gr\u00f6\u00dfen nicht zu nah!", "tokens": ["Tritt", "ruhm\u00b7be\u00b7kr\u00f6n\u00b7ten", "Gr\u00f6\u00b7\u00dfen", "nicht", "zu", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind den Alpen gleich, die vor uns stehn,", "tokens": ["Sie", "sind", "den", "Al\u00b7pen", "gleich", ",", "die", "vor", "uns", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am sch\u00f6nsten, gr\u00f6\u00dften, wenn von fern gesehn,", "tokens": ["Am", "sch\u00f6ns\u00b7ten", ",", "gr\u00f6\u00df\u00b7ten", ",", "wenn", "von", "fern", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "$,", "KOUS", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im blauen Duft, in ihrem fernen Ruhme!", "tokens": ["Im", "blau\u00b7en", "Duft", ",", "in", "ih\u00b7rem", "fer\u00b7nen", "Ruh\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Formen Sch\u00f6nheit, die dich fern entz\u00fcckt,", "tokens": ["Der", "For\u00b7men", "Sch\u00f6n\u00b7heit", ",", "die", "dich", "fern", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "L\u00f6st sich in rauhe Massen, wirr zerst\u00fcckt,", "tokens": ["L\u00f6st", "sich", "in", "rau\u00b7he", "Mas\u00b7sen", ",", "wirr", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn forschend du genaht dem Heiligthume;", "tokens": ["Wenn", "for\u00b7schend", "du", "ge\u00b7naht", "dem", "Hei\u00b7lig\u00b7thu\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Duftschmelz wird Gestein, das wund dich ritzt,", "tokens": ["Der", "Duft\u00b7schmelz", "wird", "Ge\u00b7stein", ",", "das", "wund", "dich", "ritzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wird Ged\u00f6rn, das Rock und Ferse schlitzt.", "tokens": ["Und", "wird", "Ge\u00b7d\u00f6rn", ",", "das", "Rock", "und", "Fer\u00b7se", "schlitzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Auge des Geweihten nur ersp\u00e4ht", "tokens": ["Das", "Au\u00b7ge", "des", "Ge\u00b7weih\u00b7ten", "nur", "er\u00b7sp\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In dunkler Kluft die sch\u00f6ne Alpenblume;", "tokens": ["In", "dunk\u00b7ler", "Kluft", "die", "sch\u00f6\u00b7ne", "Al\u00b7pen\u00b7blu\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nur wer der Geister Liebling, den umweht,", "tokens": ["Nur", "wer", "der", "Geis\u00b7ter", "Lieb\u00b7ling", ",", "den", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "NN", "$,", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Entschleiernd sich, des Berggeists Majest\u00e4t.", "tokens": ["Ent\u00b7schlei\u00b7ernd", "sich", ",", "des", "Berg\u00b7geists", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}