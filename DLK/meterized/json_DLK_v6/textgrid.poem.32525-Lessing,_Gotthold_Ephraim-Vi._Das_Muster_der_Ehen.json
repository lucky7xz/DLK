{"textgrid.poem.32525": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Vi. Das Muster der Ehen", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rares Beispiel will ich singen,", "tokens": ["Ein", "ra\u00b7res", "Bei\u00b7spiel", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wobei die Welt erstaunen wird.", "tokens": ["Wo\u00b7bei", "die", "Welt", "er\u00b7stau\u00b7nen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df alle Ehen Zwietracht bringen,", "tokens": ["Da\u00df", "al\u00b7le", "E\u00b7hen", "Zwiet\u00b7racht", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Glaubt jeder, aber jeder irrt.", "tokens": ["Glaubt", "je\u00b7der", ",", "a\u00b7ber", "je\u00b7der", "irrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sah das Muster aller Ehen,", "tokens": ["Ich", "sah", "das", "Mus\u00b7ter", "al\u00b7ler", "E\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Still, wie die stillste Sommernacht.", "tokens": ["Still", ",", "wie", "die", "stills\u00b7te", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! da\u00df sie keiner m\u00f6ge sehen,", "tokens": ["O", "!", "da\u00df", "sie", "kei\u00b7ner", "m\u00f6\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mich zum frechen L\u00fcgner macht!", "tokens": ["Der", "mich", "zum", "fre\u00b7chen", "L\u00fcg\u00b7ner", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und gleichwohl war die Frau kein Engel,", "tokens": ["Und", "gleich\u00b7wohl", "war", "die", "Frau", "kein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der Gemahl kein Heiliger;", "tokens": ["Und", "der", "Ge\u00b7mahl", "kein", "Hei\u00b7li\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hatte jedes seine M\u00e4ngel.", "tokens": ["Es", "hat\u00b7te", "je\u00b7des", "sei\u00b7ne", "M\u00e4n\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn niemand ist von allen leer.", "tokens": ["Denn", "nie\u00b7mand", "ist", "von", "al\u00b7len", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sollte mich ein Sp\u00f6tter fragen,", "tokens": ["Doch", "soll\u00b7te", "mich", "ein", "Sp\u00f6t\u00b7ter", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie diese Wunder m\u00f6glich sind?", "tokens": ["Wie", "die\u00b7se", "Wun\u00b7der", "m\u00f6g\u00b7lich", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der lasse sich zur Antwort sagen:", "tokens": ["Der", "las\u00b7se", "sich", "zur", "Ant\u00b7wort", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mann war taub, die Frau war blind.", "tokens": ["Der", "Mann", "war", "taub", ",", "die", "Frau", "war", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}