{"textgrid.poem.25769": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du Garten, wo, als Knaben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du Garten, wo, als Knaben,", "tokens": ["O", "du", "Gar\u00b7ten", ",", "wo", ",", "als", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PWAV", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu hoch kein Apfel hing,", "tokens": ["Mir", "zu", "hoch", "kein", "Ap\u00b7fel", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du verschwiegner M\u00fchlengraben,", "tokens": ["Du", "ver\u00b7schwieg\u00b7ner", "M\u00fch\u00b7len\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo den ersten Schmerl ich fing,", "tokens": ["Wo", "den", "ers\u00b7ten", "Schmerl", "ich", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du Busch, durch dessen Aeste", "tokens": ["Und", "du", "Busch", ",", "durch", "des\u00b7sen", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mir ein H\u00e4nfling einst entflog,", "tokens": ["Mir", "ein", "H\u00e4nf\u00b7ling", "einst", "ent\u00b7flog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und f\u00fcnf Junge mir im Neste", "tokens": ["Und", "f\u00fcnf", "Jun\u00b7ge", "mir", "im", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "PPER", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ueberlie\u00df, die ich erzog:", "tokens": ["Ue\u00b7ber\u00b7lie\u00df", ",", "die", "ich", "er\u00b7zog", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eurer werd' ich dann noch denken,", "tokens": ["Eu\u00b7rer", "werd'", "ich", "dann", "noch", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn der Opernt\u00e4nzer Kunst", "tokens": ["Wenn", "der", "O\u00b7pern\u00b7t\u00e4n\u00b7zer", "Kunst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit der Scaramuze Schw\u00e4nken", "tokens": ["Mit", "der", "Sca\u00b7ra\u00b7mu\u00b7ze", "Schw\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00e4ngst bei mir verflog wie Dunst.", "tokens": ["L\u00e4ngst", "bei", "mir", "ver\u00b7flog", "wie", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eurer werd' ich nicht vergessen,", "tokens": ["Eu\u00b7rer", "werd'", "ich", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wenn ich gleich des Witzlings Spa\u00df,", "tokens": ["Wenn", "ich", "gleich", "des", "Witz\u00b7lings", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und der Prunksucht Abendessen", "tokens": ["Und", "der", "Prunk\u00b7sucht", "A\u00b7ben\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schon am Morgen drauf verga\u00df.", "tokens": ["Schon", "am", "Mor\u00b7gen", "drauf", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Nachbarin im Fl\u00fcgelkleide,", "tokens": ["Nach\u00b7ba\u00b7rin", "im", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Meines Herzens erste Braut,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "ers\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Meiner Kindheit liebste Freude!", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", "liebs\u00b7te", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "H\u00f6rst du jetzt noch meinen Laut?", "tokens": ["H\u00f6rst", "du", "jetzt", "noch", "mei\u00b7nen", "Laut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Denkst du noch im Himmel meiner?", "tokens": ["Denkst", "du", "noch", "im", "Him\u00b7mel", "mei\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Oder weist du nichts von mir?", "tokens": ["O\u00b7der", "weist", "du", "nichts", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Lieb' ist dort ja selbst nicht reiner,", "tokens": ["Lieb'", "ist", "dort", "ja", "selbst", "nicht", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Als die meinige zu dir.", "tokens": ["Als", "die", "mei\u00b7ni\u00b7ge", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "J\u00e4hrlich wird der H\u00fcgel kleiner,", "tokens": ["J\u00e4hr\u00b7lich", "wird", "der", "H\u00fc\u00b7gel", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Den dein Sarg so lange trug,", "tokens": ["Den", "dein", "Sarg", "so", "lan\u00b7ge", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Aber das Ged\u00e4chtni\u00df deiner,", "tokens": ["A\u00b7ber", "das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Mindert nicht der Jahre Flug.", "tokens": ["Min\u00b7dert", "nicht", "der", "Jah\u00b7re", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Lieben werd' ich, bis mein Endchen", "tokens": ["Lie\u00b7ben", "werd'", "ich", ",", "bis", "mein", "End\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Lebenslicht ist abgebrannt,", "tokens": ["Le\u00b7bens\u00b7licht", "ist", "ab\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Dich wie sonst, als noch dein H\u00e4ndchen", "tokens": ["Dich", "wie", "sonst", ",", "als", "noch", "dein", "H\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Unschuldsvoll mir Kr\u00e4nze band.", "tokens": ["Un\u00b7schulds\u00b7voll", "mir", "Kr\u00e4n\u00b7ze", "band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Ach! aus keinem Festpokale", "tokens": ["Ach", "!", "aus", "kei\u00b7nem", "Fest\u00b7po\u00b7ka\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Sog ich solchen Rausch noch ein,", "tokens": ["Sog", "ich", "sol\u00b7chen", "Rausch", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Als aus dir, geliebte Saale!", "tokens": ["Als", "aus", "dir", ",", "ge\u00b7lieb\u00b7te", "Saa\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Auf dem Felsen ", "tokens": ["Auf", "dem", "Fel\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Tragt, wenn's seyn kann, aus dem Thale", "tokens": ["Tragt", ",", "wenn's", "seyn", "kann", ",", "aus", "dem", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "VAINF", "VMFIN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.38": {"text": "Sterbend mich auf jene H\u00f6hn,", "tokens": ["Ster\u00b7bend", "mich", "auf", "je\u00b7ne", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df ich da zum letztenmale", "tokens": ["Da\u00df", "ich", "da", "zum", "letz\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Seh' die Sonne niedergehn.", "tokens": ["Seh'", "die", "Son\u00b7ne", "nie\u00b7der\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Von der Newa bis zum Rheine,", "tokens": ["Von", "der", "Ne\u00b7wa", "bis", "zum", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.42": {"text": "Von der Weichsel bis zur Aar,", "tokens": ["Von", "der", "Weich\u00b7sel", "bis", "zur", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Sah ich jede Flur, doch keine,", "tokens": ["Sah", "ich", "je\u00b7de", "Flur", ",", "doch", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Die mir lieb wie jene war.", "tokens": ["Die", "mir", "lieb", "wie", "je\u00b7ne", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KOKOM", "PDS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Wie ich nun allein so walle", "tokens": ["Wie", "ich", "nun", "al\u00b7lein", "so", "wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "In des Lebens Abendroth!", "tokens": ["In", "des", "Le\u00b7bens", "A\u00b7ben\u00b7droth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Saale! Wie zerstreut sind alle", "tokens": ["Saa\u00b7le", "!", "Wie", "zer\u00b7streut", "sind", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVPP", "VAFIN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Unsre Freunde, oder todt!", "tokens": ["Uns\u00b7re", "Freun\u00b7de", ",", "o\u00b7der", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Meinen Schlaf hat nur zwei Lenze,", "tokens": ["Mei\u00b7nen", "Schlaf", "hat", "nur", "zwei", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Aber tausend Veilchenkr\u00e4nze", "tokens": ["A\u00b7ber", "tau\u00b7send", "Veil\u00b7chen\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Mir die Freud' an dir gepfl\u00fcckt.", "tokens": ["Mir", "die", "Freud'", "an", "dir", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "O du trauter Vollmond! gl\u00e4nze", "tokens": ["O", "du", "trau\u00b7ter", "Voll\u00b7mond", "!", "gl\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "So noch jetzt auf meinen Pfad!", "tokens": ["So", "noch", "jetzt", "auf", "mei\u00b7nen", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.54": {"text": "Meine Tage waren T\u00e4nze,", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "wa\u00b7ren", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Meine N\u00e4cht' ein k\u00fchlend Bad.", "tokens": ["Mei\u00b7ne", "N\u00e4cht'", "ein", "k\u00fch\u00b7lend", "Bad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Dort versucht' auf kleiner Leyer", "tokens": ["Dort", "ver\u00b7sucht'", "auf", "klei\u00b7ner", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.57": {"text": "Meine Hand den ersten Griff.", "tokens": ["Mei\u00b7ne", "Hand", "den", "ers\u00b7ten", "Griff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Doch, so segelt ohne Steuer", "tokens": ["Doch", ",", "so", "se\u00b7gelt", "oh\u00b7ne", "Steu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Ein sich selbst gela\u00dfnes Schiff.", "tokens": ["Ein", "sich", "selbst", "ge\u00b7la\u00df\u00b7nes", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Dennoch h\u00fcpfte damals freier", "tokens": ["Den\u00b7noch", "h\u00fcpf\u00b7te", "da\u00b7mals", "frei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "In den Adern mir das Blut,", "tokens": ["In", "den", "A\u00b7dern", "mir", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Als wie jetzt, wo unterm Schleier", "tokens": ["Als", "wie", "jetzt", ",", "wo", "un\u00b7term", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ADV", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Nicht wie sonst mein Name ruht.", "tokens": ["Nicht", "wie", "sonst", "mein", "Na\u00b7me", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Was gibt mehr als edle Liebe?", "tokens": ["Was", "gibt", "mehr", "als", "ed\u00b7le", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.65": {"text": "Und was diese geben kann,", "tokens": ["Und", "was", "die\u00b7se", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.66": {"text": "Gab sie alles mir. Ich bliebe", "tokens": ["Gab", "sie", "al\u00b7les", "mir", ".", "Ich", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "PPER", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Noch ein hochbegl\u00fcckter Mann,", "tokens": ["Noch", "ein", "hoch\u00b7be\u00b7gl\u00fcck\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.68": {"text": "H\u00e4tte sie die H\u00e4lfte dessen,", "tokens": ["H\u00e4t\u00b7te", "sie", "die", "H\u00e4lf\u00b7te", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "(zu partheiisch zwar vielleicht!)", "tokens": ["(", "zu", "part\u00b7hei\u00b7isch", "zwar", "viel\u00b7leicht", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKA", "ADJD", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.70": {"text": "Was sie mir hat zugemessen,", "tokens": ["Was", "sie", "mir", "hat", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.71": {"text": "Nur zu meinem Theil' gereicht.", "tokens": ["Nur", "zu", "mei\u00b7nem", "Theil'", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "T\u00fcckisch stachen zwar die Schlangen", "tokens": ["T\u00fc\u00b7ckisch", "sta\u00b7chen", "zwar", "die", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Um des Neides Haupt, auch mich;", "tokens": ["Um", "des", "Nei\u00b7des", "Haupt", ",", "auch", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.74": {"text": "Doch Vergangnes sey vergangen!", "tokens": ["Doch", "Ver\u00b7gang\u00b7nes", "sey", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Dann so schmerzt nicht mehr ihr Stich.", "tokens": ["Dann", "so", "schmerzt", "nicht", "mehr", "ihr", "Stich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.76": {"text": "Uebrig bleiben nur die Z\u00e4hren,", "tokens": ["Ueb\u00b7rig", "blei\u00b7ben", "nur", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Die der Tod mir ausgepre\u00dft,", "tokens": ["Die", "der", "Tod", "mir", "aus\u00b7ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.78": {"text": "Aber diese selbst gew\u00e4hren", "tokens": ["A\u00b7ber", "die\u00b7se", "selbst", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "Mir ein stilles Herzensfest.", "tokens": ["Mir", "ein", "stil\u00b7les", "Her\u00b7zens\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.80": {"text": "Nimm denn, Gl\u00fcck! mir jede Gabe!", "tokens": ["Nimm", "denn", ",", "Gl\u00fcck", "!", "mir", "je\u00b7de", "Ga\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "NN", "$.", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.81": {"text": "Dennoch bleibt mir noch genung,", "tokens": ["Den\u00b7noch", "bleibt", "mir", "noch", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.82": {"text": "Wenn ich dich nur immer habe,", "tokens": ["Wenn", "ich", "dich", "nur", "im\u00b7mer", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "S\u00fc\u00dfe R\u00fcckerinnerung!", "tokens": ["S\u00fc\u00b7\u00dfe", "R\u00fc\u00b7cke\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.84": {"text": "Bleibst nur du bis an das Ende", "tokens": ["Bleibst", "nur", "du", "bis", "an", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Meines Lebens mir getreu:", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "mir", "ge\u00b7treu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.86": {"text": "O so sterb' ich reich, und f\u00e4nde", "tokens": ["O", "so", "sterb'", "ich", "reich", ",", "und", "f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "Mich der Tod auf einer Streu.", "tokens": ["Mich", "der", "Tod", "auf", "ei\u00b7ner", "Streu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}