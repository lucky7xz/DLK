{"textgrid.poem.25403": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dicht an den Glanz der Pl\u00e4tze fressen sich und w\u00fchlen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht an den Glanz der Pl\u00e4tze fressen sich und w\u00fchlen", "tokens": ["Dicht", "an", "den", "Glanz", "der", "Pl\u00e4t\u00b7ze", "fres\u00b7sen", "sich", "und", "w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Winkelgassen, w\u00fcst in sich verbissen,", "tokens": ["Die", "Win\u00b7kel\u00b7gas\u00b7sen", ",", "w\u00fcst", "in", "sich", "ver\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Narben klaffend in das nackte Fleisch", "tokens": ["Wie", "Nar\u00b7ben", "klaf\u00b7fend", "in", "das", "nack\u00b7te", "Fleisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der H\u00e4user eingerissen", "tokens": ["der", "H\u00e4u\u00b7ser", "ein\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und angef\u00fcllt mit Kehricht,", "tokens": ["Und", "an\u00b7ge\u00b7f\u00fcllt", "mit", "Keh\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "den die schmutzigen Gossen \u00fcbersp\u00fclen.", "tokens": ["den", "die", "schmut\u00b7zi\u00b7gen", "Gos\u00b7sen", "\u00fc\u00b7bers\u00b7p\u00fc\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die vollgestopften L\u00e4den dr\u00e4ngen sich ins Freie.", "tokens": ["Die", "voll\u00b7ge\u00b7stopf\u00b7ten", "L\u00e4\u00b7den", "dr\u00e4n\u00b7gen", "sich", "ins", "Frei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf langen Tischen staut sich Plunder wirr zusammen:", "tokens": ["Auf", "lan\u00b7gen", "Ti\u00b7schen", "staut", "sich", "Plun\u00b7der", "wirr", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kattun und Kleider,", "tokens": ["Kat\u00b7tun", "und", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fische, Fr\u00fcchte, Fleisch, in ekler Reihe", "tokens": ["Fi\u00b7sche", ",", "Fr\u00fcch\u00b7te", ",", "Fleisch", ",", "in", "ek\u00b7ler", "Rei\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Verstapelt und bespritzt", "tokens": ["Ver\u00b7sta\u00b7pelt", "und", "be\u00b7spritzt"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit gelben Naphtaflammen.", "tokens": ["mit", "gel\u00b7ben", "Naph\u00b7ta\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gestank von faulem Fleisch und Fischen klebt an W\u00e4nden.", "tokens": ["Ge\u00b7stank", "von", "fau\u00b7lem", "Fleisch", "und", "Fi\u00b7schen", "klebt", "an", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00fc\u00dflicher Brodem tr\u00e4nkt die Luft, die leise nachtet.", "tokens": ["S\u00fc\u00df\u00b7li\u00b7cher", "Bro\u00b7dem", "tr\u00e4nkt", "die", "Luft", ",", "die", "lei\u00b7se", "nach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ein altes Weib", "tokens": ["Ein", "al\u00b7tes", "Weib"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "scharrt Abfall ein mit gierigen H\u00e4nden,", "tokens": ["scharrt", "Ab\u00b7fall", "ein", "mit", "gie\u00b7ri\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein blinder Bettler", "tokens": ["Ein", "blin\u00b7der", "Bett\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "pl\u00e4rrt ein Lied, das keiner achtet.", "tokens": ["pl\u00e4rrt", "ein", "Lied", ",", "das", "kei\u00b7ner", "ach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Man sitzt vor T\u00fcren, dr\u00fcckt sich um die Karren.", "tokens": ["Man", "sitzt", "vor", "T\u00fc\u00b7ren", ",", "dr\u00fcckt", "sich", "um", "die", "Kar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerlumpte Kinder kreischen \u00fcber d\u00fcrftigem Spiele.", "tokens": ["Zer\u00b7lump\u00b7te", "Kin\u00b7der", "krei\u00b7schen", "\u00fc\u00b7ber", "d\u00fcrf\u00b7ti\u00b7gem", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Grammophon qu\u00e4kt auf,", "tokens": ["Ein", "Gram\u00b7mo\u00b7phon", "qu\u00e4kt", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "zerbrochne Weiberstimmen knarren,", "tokens": ["zer\u00b7broch\u00b7ne", "Wei\u00b7bers\u00b7tim\u00b7men", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und fern erdr\u00f6hnt die Stadt", "tokens": ["Und", "fern", "er\u00b7dr\u00f6hnt", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "im Donner der Automobile.", "tokens": ["im", "Don\u00b7ner", "der", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}