{"dta.poem.18426": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Marquis de Louvois.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Gallischen Tiber dem war ich ein Sejan/", "tokens": ["Dem", "Gal\u00b7li\u00b7schen", "Ti\u00b7ber", "dem", "war", "ich", "ein", "Se\u00b7jan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PDS", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was der zu Rom verricht/ das hab ich auch gethan/", "tokens": ["Was", "der", "zu", "Rom", "ver\u00b7richt", "/", "das", "hab", "ich", "auch", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "NE", "VVPP", "$(", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df mir endlich auch kein hencker m\u00f6chte fehlen/", "tokens": ["Und", "da\u00df", "mir", "end\u00b7lich", "auch", "kein", "hen\u00b7cker", "m\u00f6ch\u00b7te", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So hat mich starckes gifft zu tode m\u00fcssen qu\u00e4len.", "tokens": ["So", "hat", "mich", "star\u00b7ckes", "gifft", "zu", "to\u00b7de", "m\u00fcs\u00b7sen", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}