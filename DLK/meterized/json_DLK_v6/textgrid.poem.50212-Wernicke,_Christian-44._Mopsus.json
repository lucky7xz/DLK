{"textgrid.poem.50212": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "44. Mopsus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Den eine Feder halb umringt,", "tokens": ["Den", "ei\u00b7ne", "Fe\u00b7der", "halb", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mit dem rechten Fl\u00fcgel hinckt;", "tokens": ["Die", "mit", "dem", "rech\u00b7ten", "Fl\u00fc\u00b7gel", "hinckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und eine Weste mit viel Spitzen,", "tokens": ["Und", "ei\u00b7ne", "Wes\u00b7te", "mit", "viel", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die als ein Aussenwerck der Brust,", "tokens": ["Die", "als", "ein", "Aus\u00b7sen\u00b7werck", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn sch\u00fctzen vor dem Frost und Hust;", "tokens": ["Ihn", "sch\u00fct\u00b7zen", "vor", "dem", "Frost", "und", "Hust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Schmall, doch von neun und neuntzig Elen,", "tokens": ["Schmall", ",", "doch", "von", "neun", "und", "ne\u00b7unt\u00b7zig", "E\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "In das sich keine Farben stehlen,", "tokens": ["In", "das", "sich", "kei\u00b7ne", "Far\u00b7ben", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als die sich sind im Blut verwant;", "tokens": ["Als", "die", "sich", "sind", "im", "Blut", "ver\u00b7want", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Die Breite mit der L\u00e4nge schimpffet,", "tokens": ["Die", "Brei\u00b7te", "mit", "der", "L\u00e4n\u00b7ge", "schimpf\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Von dem das Band zur Wehr sich stellt,", "tokens": ["Von", "dem", "das", "Band", "zur", "Wehr", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und die Perruck' in Schrancken h\u00e4lt;", "tokens": ["Und", "die", "Per\u00b7ruck'", "in", "Schran\u00b7cken", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Das in der Scheide sich verzehrt,", "tokens": ["Das", "in", "der", "Schei\u00b7de", "sich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch, wie manch Schienbein es empfunden,", "tokens": ["Doch", ",", "wie", "manch", "Schien\u00b7bein", "es", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Auch in der Scheide kan verwunden;", "tokens": ["Auch", "in", "der", "Schei\u00b7de", "kan", "ver\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Das seine Lands-Arth woll versteht,", "tokens": ["Das", "sei\u00b7ne", "Lands\u00b7Arth", "woll", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.23": {"text": "Nicht geht als bey gemessnen Schritten,", "tokens": ["Nicht", "geht", "als", "bey", "ge\u00b7mess\u00b7nen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und sich vor'm Unflat weiss zu h\u00fctten;", "tokens": ["Und", "sich", "vor'm", "Un\u00b7flat", "weiss", "zu", "h\u00fct\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Und bunte Str\u00fcmpffe noch dazu;", "tokens": ["Und", "bun\u00b7te", "Str\u00fcmpf\u00b7fe", "noch", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Bald ", "tokens": ["Bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Eins fehlt ihm nur; Fragst du ihn was?", "tokens": ["Eins", "fehlt", "ihm", "nur", ";", "Fragst", "du", "ihn", "was", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "PPER", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So kratzt der Schlucker seinen Scheitel,", "tokens": ["So", "kratzt", "der", "Schlu\u00b7cker", "sei\u00b7nen", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}