{"textgrid.poem.48570": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf Herrn Martin Sch\u00f6rkels und Jungfrau Margarethen Putschers Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Nacht, gew\u00fcndschte Schatten,", "tokens": ["Sch\u00f6\u00b7ne", "Nacht", ",", "ge\u00b7w\u00fcnd\u00b7schte", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kommt doch, kommet doch von Statten,", "tokens": ["kommt", "doch", ",", "kom\u00b7met", "doch", "von", "Stat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "eilt doch, eilet doch anher!", "tokens": ["eilt", "doch", ",", "ei\u00b7let", "doch", "an\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, ihr eilet, ja, ihr kommet!", "tokens": ["Ja", ",", "ihr", "ei\u00b7let", ",", "ja", ",", "ihr", "kom\u00b7met", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun ist hier, was beiden frommet,", "tokens": ["Nun", "ist", "hier", ",", "was", "bei\u00b7den", "from\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nun ist hin, was war Beschwer.", "tokens": ["nun", "ist", "hin", ",", "was", "war", "Be\u00b7schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gebt uns, was k\u00f6mmt aus Idumen,", "tokens": ["Gebt", "uns", ",", "was", "k\u00f6mmt", "aus", "I\u00b7du\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gebt uns junge Safranblumen,", "tokens": ["gebt", "uns", "jun\u00b7ge", "Saf\u00b7ran\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmelsschl\u00fcsseln, Ro\u00dfmarin,", "tokens": ["Him\u00b7mels\u00b7schl\u00fcs\u00b7seln", ",", "Ro\u00df\u00b7ma\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df wir sie den lieben Zweien,", "tokens": ["da\u00df", "wir", "sie", "den", "lie\u00b7ben", "Zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "den geliebten zweien Treuen", "tokens": ["den", "ge\u00b7lieb\u00b7ten", "zwei\u00b7en", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "streuen auf ihr Lager hin!", "tokens": ["streu\u00b7en", "auf", "ihr", "La\u00b7ger", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses, dieses sind die Stunden,", "tokens": ["Die\u00b7ses", ",", "die\u00b7ses", "sind", "die", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "da ihr Alles habt empfunden,", "tokens": ["da", "ihr", "Al\u00b7les", "habt", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "trautes Paar, was ihr begehrt.", "tokens": ["trau\u00b7tes", "Paar", ",", "was", "ihr", "be\u00b7gehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was in sechsmal vierzehn Tagen", "tokens": ["Was", "in", "sechs\u00b7mal", "vier\u00b7zehn", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "euch gewesen s\u00fc\u00dfe Plagen,", "tokens": ["euch", "ge\u00b7we\u00b7sen", "s\u00fc\u00b7\u00dfe", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAPP", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hat euch eine Nacht gew\u00e4hrt.", "tokens": ["hat", "euch", "ei\u00b7ne", "Nacht", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nemlich itzund war zu freien,", "tokens": ["Nem\u00b7lich", "it\u00b7zund", "war", "zu", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da man Alles sich verneuen", "tokens": ["da", "man", "Al\u00b7les", "sich", "ver\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIS", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und wie Hochzeit machen sicht,", "tokens": ["und", "wie", "Hoch\u00b7zeit", "ma\u00b7chen", "sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da nun in erw\u00e4rmter Erden", "tokens": ["da", "nun", "in", "er\u00b7w\u00e4rm\u00b7ter", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "alle Sachen rege werden,", "tokens": ["al\u00b7le", "Sa\u00b7chen", "re\u00b7ge", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie bei Bulern auch geschicht.", "tokens": ["wie", "bei", "Bu\u00b7lern", "auch", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die verlebte Welt wird j\u00fcnger", "tokens": ["Die", "ver\u00b7leb\u00b7te", "Welt", "wird", "j\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und streicht mit verliebtem Finger", "tokens": ["und", "streicht", "mit", "ver\u00b7lieb\u00b7tem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ihre Runzeln von der Haut.", "tokens": ["ih\u00b7re", "Run\u00b7zeln", "von", "der", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht, seht, wie sie aus den Feldern,", "tokens": ["Seht", ",", "seht", ",", "wie", "sie", "aus", "den", "Fel\u00b7dern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "aus den Auen, aus den W\u00e4ldern", "tokens": ["aus", "den", "Au\u00b7en", ",", "aus", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mit verbulten Augen schaut!", "tokens": ["mit", "ver\u00b7bul\u00b7ten", "Au\u00b7gen", "schaut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie schaut nach dem lieben Freier,", "tokens": ["Sie", "schaut", "nach", "dem", "lie\u00b7ben", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der uns bringt ein neues Heuer,", "tokens": ["der", "uns", "bringt", "ein", "neu\u00b7es", "Heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der sich ihr schon anvertraut", "tokens": ["der", "sich", "ihr", "schon", "an\u00b7ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und in ihre Glieder dringet.", "tokens": ["und", "in", "ih\u00b7re", "Glie\u00b7der", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Br\u00e4utgam wird verj\u00fcnget", "tokens": ["Un\u00b7ser", "Br\u00e4ut\u00b7gam", "wird", "ver\u00b7j\u00fcn\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in der Scho\u00df der sch\u00f6nen Braut.", "tokens": ["in", "der", "Scho\u00df", "der", "sch\u00f6\u00b7nen", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gleiches Paar, doch nicht an Jahren!", "tokens": ["Glei\u00b7ches", "Paar", ",", "doch", "nicht", "an", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr la\u00dft uns an euch erfahren,", "tokens": ["Ihr", "la\u00dft", "uns", "an", "euch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "da\u00df auch Ungleich gleiche sei.", "tokens": ["da\u00df", "auch", "Un\u00b7gleich", "glei\u00b7che", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch wer fraget nach den Jahren?", "tokens": ["Doch", "wer", "fra\u00b7get", "nach", "den", "Jah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sich soll, das mu\u00df sich paaren.", "tokens": ["Was", "sich", "soll", ",", "das", "mu\u00df", "sich", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VMFIN", "$,", "PDS", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieb' ist hier, wie allzeit, frei.", "tokens": ["Lieb'", "ist", "hier", ",", "wie", "all\u00b7zeit", ",", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "PWAV", "ADV", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn sich ein Paar Liebe k\u00fcssen,", "tokens": ["Wenn", "sich", "ein", "Paar", "Lie\u00b7be", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit halbgemachten Bissen", "tokens": ["und", "mit", "halb\u00b7ge\u00b7mach\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mund mit Munde lieblich ringt,", "tokens": ["Mund", "mit", "Mun\u00b7de", "lieb\u00b7lich", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df die k\u00fcssenden Korallen", "tokens": ["da\u00df", "die", "k\u00fcs\u00b7sen\u00b7den", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "etwas lassen widerschallen,", "tokens": ["et\u00b7was", "las\u00b7sen", "wi\u00b7der\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "das den Sternen gleiche klingt:", "tokens": ["das", "den", "Ster\u00b7nen", "glei\u00b7che", "klingt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "da verlaufen sich die Seelen", "tokens": ["da", "ver\u00b7lau\u00b7fen", "sich", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die unerforschten H\u00f6len", "tokens": ["in", "die", "un\u00b7er\u00b7forschten", "H\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "und verwirren sich in sich.", "tokens": ["und", "ver\u00b7wir\u00b7ren", "sich", "in", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den zimmets\u00fc\u00dfen Kehlen,", "tokens": ["In", "den", "zim\u00b7met\u00b7s\u00fc\u00b7\u00dfen", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da geschiehet das Verm\u00e4hlen,", "tokens": ["da", "ge\u00b7schie\u00b7het", "das", "Ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "das uns wundert ewiglich.", "tokens": ["das", "uns", "wun\u00b7dert", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zwei vermengte L\u00fcfte machen", "tokens": ["Zwei", "ver\u00b7meng\u00b7te", "L\u00fcf\u00b7te", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "einen Geist, der gro\u00dfe Sachen,", "tokens": ["ei\u00b7nen", "Geist", ",", "der", "gro\u00b7\u00dfe", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "doch in kleinem Halle sagt,", "tokens": ["doch", "in", "klei\u00b7nem", "Hal\u00b7le", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sachen, die nur ihr besinnet", "tokens": ["Sa\u00b7chen", ",", "die", "nur", "ihr", "be\u00b7sin\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und doch Keinem sagen k\u00f6nnet,", "tokens": ["und", "doch", "Kei\u00b7nem", "sa\u00b7gen", "k\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der euch um dieselben fragt.", "tokens": ["der", "euch", "um", "die\u00b7sel\u00b7ben", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "In demselben lieben Leben", "tokens": ["In", "dem\u00b7sel\u00b7ben", "lie\u00b7ben", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werdet ihr nicht wissen eben,", "tokens": ["wer\u00b7det", "ihr", "nicht", "wis\u00b7sen", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bei euch stets, stets von euch weit,", "tokens": ["bei", "euch", "stets", ",", "stets", "von", "euch", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ob ihr schlafend oder wachend,", "tokens": ["ob", "ihr", "schla\u00b7fend", "o\u00b7der", "wa\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ob ihr weinend oder lachend", "tokens": ["ob", "ihr", "wei\u00b7nend", "o\u00b7der", "la\u00b7chend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "oder aus euch selbsten seid.", "tokens": ["o\u00b7der", "aus", "euch", "selbs\u00b7ten", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die gestirnten Himmelsscheiben", "tokens": ["Die", "ge\u00b7stirn\u00b7ten", "Him\u00b7mels\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wollen gleich als stehen bleiben", "tokens": ["wol\u00b7len", "gleich", "als", "ste\u00b7hen", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "KOKOM", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcber euch und eurer Zier.", "tokens": ["\u00fc\u00b7ber", "euch", "und", "eu\u00b7rer", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausent', tausent' kleiner W\u00e4chter", "tokens": ["Tau\u00b7sent'", ",", "tau\u00b7sent'", "klei\u00b7ner", "W\u00e4ch\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "treiben ein sehr laut Gel\u00e4chter", "tokens": ["trei\u00b7ben", "ein", "sehr", "laut", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euch zu Ehren f\u00fcr und f\u00fcr.", "tokens": ["euch", "zu", "Eh\u00b7ren", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Geht, Verliebte, teilt die Flammen!", "tokens": ["Geht", ",", "Ver\u00b7lieb\u00b7te", ",", "teilt", "die", "Flam\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der euch itzund giebt zusammen,", "tokens": ["Der", "euch", "it\u00b7zund", "giebt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "f\u00fcrder' eurer Liebe Lauf!", "tokens": ["f\u00fcr\u00b7der'", "eu\u00b7rer", "Lie\u00b7be", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des ersuchten Himmels Segen", "tokens": ["Des", "er\u00b7such\u00b7ten", "Him\u00b7mels", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wird sich mit euch niederlegen,", "tokens": ["wird", "sich", "mit", "euch", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "schlafen, wachen und stehn auf.", "tokens": ["schla\u00b7fen", ",", "wa\u00b7chen", "und", "stehn", "auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wenn der weitgepreiste Garten", "tokens": ["Wenn", "der", "weit\u00b7ge\u00b7preis\u00b7te", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "keiner Blumen mehr wird warten,", "tokens": ["kei\u00b7ner", "Blu\u00b7men", "mehr", "wird", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn das Pomeranzenhaus", "tokens": ["wenn", "das", "Po\u00b7me\u00b7ran\u00b7zen\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "grau von Frost und Schnee wird stehen,", "tokens": ["grau", "von", "Frost", "und", "Schnee", "wird", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn soll eine Blum' aufgehen", "tokens": ["denn", "soll", "ei\u00b7ne", "Blum'", "auf\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und mit Freuden bl\u00fchen aus.", "tokens": ["und", "mit", "Freu\u00b7den", "bl\u00fc\u00b7hen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}