{"textgrid.poem.66518": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es waren drei K\u00f6nige im Morgenland,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren drei K\u00f6nige im Morgenland,", "tokens": ["Es", "wa\u00b7ren", "drei", "K\u00f6\u00b7ni\u00b7ge", "im", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Wort war ihnen ergangen,", "tokens": ["Das", "Wort", "war", "ih\u00b7nen", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wo der Stern am Himmel stand,", "tokens": ["Und", "wo", "der", "Stern", "am", "Him\u00b7mel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sollten sie Heil empfangen.", "tokens": ["Da", "soll\u00b7ten", "sie", "Heil", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es waren Hirten auf freier Wacht,", "tokens": ["Es", "wa\u00b7ren", "Hir\u00b7ten", "auf", "frei\u00b7er", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hatten den Stern gesehen,", "tokens": ["Die", "hat\u00b7ten", "den", "Stern", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er leuchtete hell durch die ganze Nacht,", "tokens": ["Er", "leuch\u00b7te\u00b7te", "hell", "durch", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die andern mu\u00dften vergehen.", "tokens": ["Die", "an\u00b7dern", "mu\u00df\u00b7ten", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Hirten, die K\u00f6nige, zogen ihm nach,", "tokens": ["Die", "Hir\u00b7ten", ",", "die", "K\u00f6\u00b7ni\u00b7ge", ",", "zo\u00b7gen", "ihm", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So weit das Land sich breitet,", "tokens": ["So", "weit", "das", "Land", "sich", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df sie standen am niedren Dach,", "tokens": ["Bis", "da\u00df", "sie", "stan\u00b7den", "am", "nied\u00b7ren", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dahin der Stern sie geleitet. \u2013", "tokens": ["Da\u00b7hin", "der", "Stern", "sie", "ge\u00b7lei\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "In die Welt hinaus", "tokens": ["In", "die", "Welt", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist Mancher gegangen,", "tokens": ["Ist", "Man\u00b7cher", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kehrte sp\u00e4t nach Haus", "tokens": ["Kehr\u00b7te", "sp\u00e4t", "nach", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mit gebr\u00e4unten Wangen.", "tokens": ["Mit", "ge\u00b7br\u00e4un\u00b7ten", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Steht ihm Leben und Muth", "tokens": ["Steht", "ihm", "Le\u00b7ben", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Antlitz geschrieben,", "tokens": ["Im", "Ant\u00b7litz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach, w\u00e4r er auch gut", "tokens": ["Ach", ",", "w\u00e4r", "er", "auch", "gut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Im Herzen geblieben! \u2013", "tokens": ["Im", "Her\u00b7zen", "ge\u00b7blie\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Reite, reite, R\u00f6\u00dflein!", "tokens": ["Rei\u00b7te", ",", "rei\u00b7te", ",", "R\u00f6\u00df\u00b7lein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zu Basel steht ein Schl\u00f6\u00dflein,", "tokens": ["Zu", "Ba\u00b7sel", "steht", "ein", "Schl\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu K\u00f6ln da steht ein Glockenhaus,", "tokens": ["Zu", "K\u00f6ln", "da", "steht", "ein", "Glo\u00b7cken\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daneben schau'n drei Jungfern aus.", "tokens": ["Da\u00b7ne\u00b7ben", "schau'n", "drei", "Jung\u00b7fern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die eine die spinnt Seide,", "tokens": ["Die", "ei\u00b7ne", "die", "spinnt", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die andre spinnt das klare Gold,", "tokens": ["Die", "and\u00b7re", "spinnt", "das", "kla\u00b7re", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dritte, ja, die dritte,", "tokens": ["Die", "drit\u00b7te", ",", "ja", ",", "die", "drit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PTKANT", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist unsrem B\u00fcblein hold! \u2013", "tokens": ["Ist", "uns\u00b7rem", "B\u00fcb\u00b7lein", "hold", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer die Fremde gesehn,", "tokens": ["Wer", "die", "Frem\u00b7de", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hat viel wohl erfahren!", "tokens": ["Hat", "viel", "wohl", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wird er die noch verstehn,", "tokens": ["Wird", "er", "die", "noch", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die so weit nicht waren?", "tokens": ["Die", "so", "weit", "nicht", "wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Daheim ist's nur still,", "tokens": ["Da\u00b7heim", "ist's", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und eng sind die Kreise.", "tokens": ["Und", "eng", "sind", "die", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach, hielt' ihn sein Will'", "tokens": ["Ach", ",", "hielt'", "ihn", "sein", "Will'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In der heimischen Weise! \u2013", "tokens": ["In", "der", "hei\u00b7mi\u00b7schen", "Wei\u00b7se", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Die heilgen drei K\u00f6nige zogen nach Haus,", "tokens": ["Die", "heil\u00b7gen", "drei", "K\u00f6\u00b7ni\u00b7ge", "zo\u00b7gen", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Hirten zu ihren Heerden,", "tokens": ["Die", "Hir\u00b7ten", "zu", "ih\u00b7ren", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Heil ging von dem Kindlein aus,", "tokens": ["Das", "Heil", "ging", "von", "dem", "Kin\u00b7dlein", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl \u00fcber den Kreis der Erden.", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "Kreis", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Und als die heilgen drei K\u00f6nige todt,", "tokens": ["Und", "als", "die", "heil\u00b7gen", "drei", "K\u00f6\u00b7ni\u00b7ge", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da wurden sie begraben,", "tokens": ["Da", "wur\u00b7den", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df der Kaiser viel Sch\u00e4tze bot,", "tokens": ["Bis", "da\u00df", "der", "Kai\u00b7ser", "viel", "Sch\u00e4t\u00b7ze", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Rhein wollt' er sie haben.", "tokens": ["Am", "Rhein", "wollt'", "er", "sie", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VMFIN", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die heilgen drei K\u00f6nige kamen zum Rhein,", "tokens": ["Die", "heil\u00b7gen", "drei", "K\u00f6\u00b7ni\u00b7ge", "ka\u00b7men", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In K\u00f6ln der Stadt zu liegen,", "tokens": ["In", "K\u00f6ln", "der", "Stadt", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da schlafen sie still in ihrem Schrein,", "tokens": ["Da", "schla\u00b7fen", "sie", "still", "in", "ih\u00b7rem", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie's Kindlein in der Wiegen.", "tokens": ["Wie's", "Kin\u00b7dlein", "in", "der", "Wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}