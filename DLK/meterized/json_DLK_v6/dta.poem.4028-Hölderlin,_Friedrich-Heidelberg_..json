{"dta.poem.4028": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Heidelberg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Lange lieb' ich Dich schon, m\u00f6chte Dich, mir zur Lust,", "tokens": ["Lan\u00b7ge", "lieb'", "ich", "Dich", "schon", ",", "m\u00f6ch\u00b7te", "Dich", ",", "mir", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "VMFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Mutter nennen und Dir schenken ein kunstlos Lied,", "tokens": ["Mut\u00b7ter", "nen\u00b7nen", "und", "Dir", "schen\u00b7ken", "ein", "kunst\u00b7los", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du, der Vaterlandsst\u00e4dte", "tokens": ["Du", ",", "der", "Va\u00b7ter\u00b7lands\u00b7st\u00e4d\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "L\u00e4ndlich sch\u00f6nste, so viel ich sah.", "tokens": ["L\u00e4nd\u00b7lich", "sch\u00f6ns\u00b7te", ",", "so", "viel", "ich", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wie der Vogel des Walds \u00fcber die Gipfel fliegt,", "tokens": ["Wie", "der", "Vo\u00b7gel", "des", "Walds", "\u00fc\u00b7ber", "die", "Gip\u00b7fel", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schwingt sich \u00fcber den Strom, wo er vorbei Dir", "tokens": ["Schwingt", "sich", "\u00fc\u00b7ber", "den", "Strom", ",", "wo", "er", "vor\u00b7bei", "Dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "PPER"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "gl\u00e4nzt,", "tokens": ["gl\u00e4nzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Leicht und kr\u00e4ftig die Br\u00fccke,", "tokens": ["Leicht", "und", "kr\u00e4f\u00b7tig", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die von Wagen und Menschen t\u00f6nt.", "tokens": ["Die", "von", "Wa\u00b7gen", "und", "Men\u00b7schen", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie von G\u00f6ttern gesandt, fesselt' ein Zauber einst,", "tokens": ["Wie", "von", "G\u00f6t\u00b7tern", "ge\u00b7sandt", ",", "fes\u00b7selt'", "ein", "Zau\u00b7ber", "einst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf die Br\u00fccke mich an, da ich vor\u00fcber gieng,", "tokens": ["Auf", "die", "Br\u00fc\u00b7cke", "mich", "an", ",", "da", "ich", "vor\u00b7\u00fc\u00b7ber", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und herein in die Berge", "tokens": ["Und", "her\u00b7ein", "in", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mir die reizende Ferne schien.", "tokens": ["Mir", "die", "rei\u00b7zen\u00b7de", "Fer\u00b7ne", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Und der J\u00fcngling, der Strom, fort in die Ebne zog,", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "der", "Strom", ",", "fort", "in", "die", "Eb\u00b7ne", "zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Traurig froh, wie das Herz, wenn es, sich selbst", "tokens": ["Trau\u00b7rig", "froh", ",", "wie", "das", "Herz", ",", "wenn", "es", ",", "sich", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "PPER", "$,", "PRF", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "zu sch\u00f6n,", "tokens": ["zu", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Liebend unterzugehen,", "tokens": ["Lie\u00b7bend", "un\u00b7ter\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In die Fluten der Zeit sich wirft.", "tokens": ["In", "die", "Flu\u00b7ten", "der", "Zeit", "sich", "wirft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Quellen hattest Du ihm, hattest dem Fl\u00fcchtigen", "tokens": ["Quel\u00b7len", "hat\u00b7test", "Du", "ihm", ",", "hat\u00b7test", "dem", "Fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "K\u00fchle Schatten geschenkt, und die Gestade sahn", "tokens": ["K\u00fch\u00b7le", "Schat\u00b7ten", "ge\u00b7schenkt", ",", "und", "die", "Ge\u00b7sta\u00b7de", "sahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "All' ihm nach, und es bebte", "tokens": ["All'", "ihm", "nach", ",", "und", "es", "beb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aus den Wellen ihr lieblich Bild.", "tokens": ["Aus", "den", "Wel\u00b7len", "ihr", "lieb\u00b7lich", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Aber schwer in das Thal hieng die gigantische", "tokens": ["A\u00b7ber", "schwer", "in", "das", "Thal", "hieng", "die", "gi\u00b7gan\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schicksalskundige Burg, nieder bis auf den Grund", "tokens": ["Schick\u00b7sals\u00b7kun\u00b7di\u00b7ge", "Burg", ",", "nie\u00b7der", "bis", "auf", "den", "Grund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PTKVZ", "KON", "APPR", "ART", "NN"], "meter": "+-+--++-++-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Von den Wettern gerissen;", "tokens": ["Von", "den", "Wet\u00b7tern", "ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Doch die ewige Sonne go\u00df", "tokens": ["Doch", "die", "e\u00b7wi\u00b7ge", "Son\u00b7ne", "go\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Ihr verj\u00fcngendes Licht \u00fcber das alternde", "tokens": ["Ihr", "ver\u00b7j\u00fcn\u00b7gen\u00b7des", "Licht", "\u00fc\u00b7ber", "das", "al\u00b7tern\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Riesenbild, und umher gr\u00fcnte lebendiger", "tokens": ["Rie\u00b7sen\u00b7bild", ",", "und", "um\u00b7her", "gr\u00fcn\u00b7te", "le\u00b7ben\u00b7di\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PTKVZ", "VVFIN", "ADJA"], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.3": {"text": "Epheu; freundliche W\u00e4lder", "tokens": ["E\u00b7pheu", ";", "freund\u00b7li\u00b7che", "W\u00e4l\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Rauschten \u00fcber die Burg herab.", "tokens": ["Rauschten", "\u00fc\u00b7ber", "die", "Burg", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Str\u00e4uche bl\u00fchten herab, bis wo im heitern Thal,", "tokens": ["Str\u00e4u\u00b7che", "bl\u00fch\u00b7ten", "her\u00b7ab", ",", "bis", "wo", "im", "hei\u00b7tern", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "KON", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "An den H\u00fcgel gelehnt, oder dem Ufer hold,", "tokens": ["An", "den", "H\u00fc\u00b7gel", "ge\u00b7lehnt", ",", "o\u00b7der", "dem", "U\u00b7fer", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deine fr\u00f6hlichen Gassen", "tokens": ["Dei\u00b7ne", "fr\u00f6h\u00b7li\u00b7chen", "Gas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter duftenden G\u00e4rten ruhn.", "tokens": ["Un\u00b7ter", "duf\u00b7ten\u00b7den", "G\u00e4r\u00b7ten", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}