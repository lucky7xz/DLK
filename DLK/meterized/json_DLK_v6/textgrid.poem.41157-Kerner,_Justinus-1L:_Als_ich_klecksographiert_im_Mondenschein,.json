{"textgrid.poem.41157": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich klecksographiert im Mondenschein,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich klecksographiert im Mondenschein,", "tokens": ["Als", "ich", "kleck\u00b7so\u00b7gra\u00b7phiert", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kam dies Gespenst herauf als wie von Stein,", "tokens": ["Kam", "dies", "Ge\u00b7spenst", "her\u00b7auf", "als", "wie", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "ADV", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch hat's ge\u00f6ffnet seinen Mund. Mit Klagen", "tokens": ["Doch", "hat's", "ge\u00b7\u00f6ff\u00b7net", "sei\u00b7nen", "Mund", ".", "Mit", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "PPOSAT", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat's reuig seine Schuld mir vorgetragen.", "tokens": ["Hat's", "reu\u00b7ig", "sei\u00b7ne", "Schuld", "mir", "vor\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gottes Erbarmen ende seine Pein!", "tokens": ["Got\u00b7tes", "Er\u00b7bar\u00b7men", "en\u00b7de", "sei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Doch zu entschlagen mich weitl\u00e4uf'gen Fragen,", "tokens": ["Doch", "zu", "ent\u00b7schla\u00b7gen", "mich", "weit\u00b7l\u00e4uf'\u00b7gen", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hab' seine ", "tokens": ["Hab'", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "In Wahrheit ich in Verse hier gebracht,", "tokens": ["In", "Wahr\u00b7heit", "ich", "in", "Ver\u00b7se", "hier", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die lest euch vor in stiller Mondennacht.", "tokens": ["Die", "lest", "euch", "vor", "in", "stil\u00b7ler", "Mon\u00b7den\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sooft der Mond im vollen Licht", "tokens": ["Sooft", "der", "Mond", "im", "vol\u00b7len", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um Mitternacht durch Wolken bricht,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "durch", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ruft ein Greis im Irrenhaus", "tokens": ["So", "ruft", "ein", "Greis", "im", "Ir\u00b7ren\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Fenstergitter hohl heraus:", "tokens": ["Durchs", "Fens\u00b7ter\u00b7git\u00b7ter", "hohl", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbim Rhein, im Rhein, im tiefen Rhein,", "tokens": ["\u00bb", "im", "Rhein", ",", "im", "Rhein", ",", "im", "tie\u00b7fen", "Rhein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NE", "$,", "APPRART", "NE", "$,", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da lag ein schwarzer, blut'ger Stein.", "tokens": ["Da", "lag", "ein", "schwar\u00b7zer", ",", "blut'\u00b7ger", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O wenn im Rhein der Stein noch w\u00e4r',", "tokens": ["O", "wenn", "im", "Rhein", "der", "Stein", "noch", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "APPRART", "NE", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oder im tiefen schwarzen Meer!", "tokens": ["O\u00b7der", "im", "tie\u00b7fen", "schwar\u00b7zen", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er dr\u00fccket Kopf und Herz mir ein,", "tokens": ["Er", "dr\u00fc\u00b7cket", "Kopf", "und", "Herz", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O Stein! Stein! wandle mich zu Stein!\u00ab", "tokens": ["O", "Stein", "!", "Stein", "!", "wand\u00b7le", "mich", "zu", "Stein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Fragt man: \u00bbWas ist's mit diesem Stein?\u00ab", "tokens": ["Fragt", "man", ":", "\u00bb", "Was", "ist's", "mit", "die\u00b7sem", "Stein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "$.", "$(", "PWS", "VAFIN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hei\u00dft er den Frager stille sein.", "tokens": ["Hei\u00dft", "er", "den", "Fra\u00b7ger", "stil\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So rief er jahrelang, nie m\u00fcd;", "tokens": ["So", "rief", "er", "jah\u00b7re\u00b7lang", ",", "nie", "m\u00fcd", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch als er einst blieb unbewacht,", "tokens": ["Doch", "als", "er", "einst", "blieb", "un\u00b7be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er sich den bleichen Hals durchschnitt", "tokens": ["Er", "sich", "den", "blei\u00b7chen", "Hals", "durch\u00b7schnitt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In einer hellen Mondennacht.", "tokens": ["In", "ei\u00b7ner", "hel\u00b7len", "Mon\u00b7den\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Bei Bingen in dem tiefen Rhein", "tokens": ["Bei", "Bin\u00b7gen", "in", "dem", "tie\u00b7fen", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "H\u00f6rt man seitdem im Mondenschein", "tokens": ["H\u00f6rt", "man", "seit\u00b7dem", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PAV", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Dieselbe Stimme in der Luft.", "tokens": ["Die\u00b7sel\u00b7be", "Stim\u00b7me", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In T\u00f6nen der Verzweiflung ruft", "tokens": ["In", "T\u00f6\u00b7nen", "der", "Ver\u00b7zwei\u00b7flung", "ruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Stimme: \u00bbStein! o w\u00e4rst du noch", "tokens": ["Die", "Stim\u00b7me", ":", "\u00bb", "Stein", "!", "o", "w\u00e4rst", "du", "noch"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "NN", "$.", "FM", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Tief, tief im schwarzen Binger Loch!", "tokens": ["Tief", ",", "tief", "im", "schwar\u00b7zen", "Bin\u00b7ger", "Loch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Verruchter Stein! Mit dir, mit dir", "tokens": ["Ver\u00b7ruch\u00b7ter", "Stein", "!", "Mit", "dir", ",", "mit", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Schlug ich einst tot den Kaufherrn hier!", "tokens": ["Schlug", "ich", "einst", "tot", "den", "Kauf\u00b7herrn", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dich drauf an seinen Hals ich hing", "tokens": ["Dich", "drauf", "an", "sei\u00b7nen", "Hals", "ich", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und warf ihn in des Strudels Ring,", "tokens": ["Und", "warf", "ihn", "in", "des", "Stru\u00b7dels", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Da\u00df er im blut'gen Gischt verschwand.", "tokens": ["Da\u00df", "er", "im", "blut'\u00b7gen", "Gischt", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Drauf wollt' ich rennen ins fernste Land,", "tokens": ["Drauf", "wollt'", "ich", "ren\u00b7nen", "ins", "ferns\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Da stob aus dem Strudel ein Wirbelwind,", "tokens": ["Da", "stob", "aus", "dem", "Stru\u00b7del", "ein", "Wir\u00b7bel\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Der hob mich \u00fcber den Strudel geschwind,", "tokens": ["Der", "hob", "mich", "\u00fc\u00b7ber", "den", "Stru\u00b7del", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Drehend mich ob ihm in Wirbeln, ach!", "tokens": ["Dre\u00b7hend", "mich", "ob", "ihm", "in", "Wir\u00b7beln", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "PPER", "KOUS", "PPER", "APPR", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Schrecklicher noch! Aus der nassen Gruft", "tokens": ["Schreck\u00b7li\u00b7cher", "noch", "!", "Aus", "der", "nas\u00b7sen", "Gruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Wirbelt des Toten Gespenst mir nach,", "tokens": ["Wir\u00b7belt", "des", "To\u00b7ten", "Ge\u00b7spenst", "mir", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Hielt in der blut'gen Hand den Stein,", "tokens": ["Hielt", "in", "der", "blut'\u00b7gen", "Hand", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dr\u00fcckt, mit mir wirbelnd in der Luft,", "tokens": ["Dr\u00fcckt", ",", "mit", "mir", "wir\u00b7belnd", "in", "der", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ihn mir ins zitternde Herz hinein.", "tokens": ["Ihn", "mir", "ins", "zit\u00b7tern\u00b7de", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Wie war mir der Stein im Leben so schwer,", "tokens": ["Wie", "war", "mir", "der", "Stein", "im", "Le\u00b7ben", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Wie ist er's mir im Tode noch mehr,", "tokens": ["Wie", "ist", "er's", "mir", "im", "To\u00b7de", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PPER", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Ihr alle, die ihr noch wachet am Rhein,", "tokens": ["Ihr", "al\u00b7le", ",", "die", "ihr", "noch", "wa\u00b7chet", "am", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Bittet zu Gott um die Seele mein!\u00ab", "tokens": ["Bit\u00b7tet", "zu", "Gott", "um", "die", "See\u00b7le", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Und wenn es so bei Bingen ruft,", "tokens": ["Und", "wenn", "es", "so", "bei", "Bin\u00b7gen", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man vom Rufer keine Spur,", "tokens": ["Sieht", "man", "vom", "Ru\u00b7fer", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schifft nur der Mond still durch die Luft", "tokens": ["Schifft", "nur", "der", "Mond", "still", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kreist ein schwarzer Vogel nur", "tokens": ["Und", "kreist", "ein", "schwar\u00b7zer", "Vo\u00b7gel", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um des Erschlagnen nasse Gruft.", "tokens": ["Um", "des", "Er\u00b7schlag\u00b7nen", "nas\u00b7se", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}