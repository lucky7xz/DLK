{"textgrid.poem.64225": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel Stimmen gehen durch die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Stimmen gehen durch die Welt,", "tokens": ["Viel", "Stim\u00b7men", "ge\u00b7hen", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verworren sind die Zeiten:", "tokens": ["Ver\u00b7wor\u00b7ren", "sind", "die", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ruf vor allen mir gef\u00e4llt,", "tokens": ["Ein", "Ruf", "vor", "al\u00b7len", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der soll mein Leben leiten:", "tokens": ["Der", "soll", "mein", "Le\u00b7ben", "lei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heil dem, der seinem Volk sich gibt,", "tokens": ["Heil", "dem", ",", "der", "sei\u00b7nem", "Volk", "sich", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Deutschen Heil, der Deutschland liebt.", "tokens": ["Dem", "Deut\u00b7schen", "Heil", ",", "der", "Deutschland", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Gar viele trachten Tag und Nacht,", "tokens": ["Gar", "vie\u00b7le", "trach\u00b7ten", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Eifersucht gesch\u00e4ftig:", "tokens": ["In", "Ei\u00b7fer\u00b7sucht", "ge\u00b7sch\u00e4f\u00b7tig", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat keiner was zustand gebracht,", "tokens": ["Hat", "kei\u00b7ner", "was", "zu\u00b7stand", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PWS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was gro\u00df und dauerkr\u00e4ftig:", "tokens": ["Was", "gro\u00df", "und", "dau\u00b7er\u00b7kr\u00e4f\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur Eitelkeit lieh ihnen Kraft", "tokens": ["Nur", "Ei\u00b7tel\u00b7keit", "lieh", "ih\u00b7nen", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und eitel ist, was sie geschafft.", "tokens": ["Und", "ei\u00b7tel", "ist", ",", "was", "sie", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wann einer von den St\u00fcrmen weht,", "tokens": ["Wann", "ei\u00b7ner", "von", "den", "St\u00fcr\u00b7men", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie ein Volk durchwittern, \u2013", "tokens": ["Wie", "sie", "ein", "Volk", "durch\u00b7wit\u00b7tern", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was, glaubt ihr, da\u00df alsdann besteht", "tokens": ["Was", ",", "glaubt", "ihr", ",", "da\u00df", "als\u00b7dann", "be\u00b7steht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "KOUS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von diesen bunten Flittern?", "tokens": ["Von", "die\u00b7sen", "bun\u00b7ten", "Flit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbfort\u00ab \u2013 wird es donnern \u2013 \u00bbTand und Spiel,", "tokens": ["\u00bb", "fort", "\u00ab", "\u2013", "wird", "es", "don\u00b7nern", "\u2013", "\u00bb", "Tand", "und", "Spiel", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$(", "$(", "VAFIN", "PPER", "VVINF", "$(", "$(", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt gilt es Tat und ernstes Ziel.\u00ab", "tokens": ["Jetzt", "gilt", "es", "Tat", "und", "erns\u00b7tes", "Ziel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann Heil dem Mann, der sprechen kann:", "tokens": ["Dann", "Heil", "dem", "Mann", ",", "der", "spre\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmein Volk, lang harr' ich deiner!", "tokens": ["\u00bb", "mein", "Volk", ",", "lang", "harr'", "ich", "dei\u00b7ner", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib jedem seinen Posten an,", "tokens": ["Gib", "je\u00b7dem", "sei\u00b7nen", "Pos\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich schnelle, wo ist meiner?", "tokens": ["Sprich", "schnel\u00b7le", ",", "wo", "ist", "mei\u00b7ner", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sei's das Wort und sei's das Schwert: \u2013", "tokens": ["Und", "sei's", "das", "Wort", "und", "sei's", "das", "Schwert", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "ART", "NN", "KON", "NE", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will es f\u00fchren, deiner wert!\u00ab", "tokens": ["Ich", "will", "es", "f\u00fch\u00b7ren", ",", "dei\u00b7ner", "wert", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPOSAT", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}