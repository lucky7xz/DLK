{"dta.poem.16293": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ehrenwerth sich f\u00fchlt, will auch geehrt sich sehn;", "tokens": ["Wer", "eh\u00b7ren\u00b7werth", "sich", "f\u00fchlt", ",", "will", "auch", "ge\u00b7ehrt", "sich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "VVFIN", "$,", "VMFIN", "ADV", "VVPP", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jedem sieht er gern auch sich sein Recht geschehn.", "tokens": ["Wie", "je\u00b7dem", "sieht", "er", "gern", "auch", "sich", "sein", "Recht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ADV", "ADV", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Selbst unbedenklich nimmt er \u00e4u\u00dfres Ehrenzeichen", "tokens": ["Selbst", "un\u00b7be\u00b7denk\u00b7lich", "nimmt", "er", "\u00e4u\u00df\u00b7res", "Eh\u00b7ren\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen an, die ihm nicht andres k\u00f6nnen reichen.", "tokens": ["Von", "de\u00b7nen", "an", ",", "die", "ihm", "nicht", "and\u00b7res", "k\u00f6n\u00b7nen", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PTKVZ", "$,", "PRELS", "PPER", "PTKNEG", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch ehrerbiet'ger Gru\u00df, anst\u00e4ndige Verbeugung,", "tokens": ["Auch", "ehr\u00b7er\u00b7biet'\u00b7ger", "Gru\u00df", ",", "an\u00b7st\u00e4n\u00b7di\u00b7ge", "Ver\u00b7beu\u00b7gung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist dem Geehreten willkommene Bezeugung,", "tokens": ["Ist", "dem", "Ge\u00b7eh\u00b7re\u00b7ten", "will\u00b7kom\u00b7me\u00b7ne", "Be\u00b7zeu\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht seines Werthes, den er f\u00fchlt, des Werthes deren,", "tokens": ["Nicht", "sei\u00b7nes", "Wert\u00b7hes", ",", "den", "er", "f\u00fchlt", ",", "des", "Wert\u00b7hes", "de\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die so bezeugen da\u00df sie Ehrenwerthes ehren.", "tokens": ["Die", "so", "be\u00b7zeu\u00b7gen", "da\u00df", "sie", "Eh\u00b7ren\u00b7wert\u00b7hes", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}