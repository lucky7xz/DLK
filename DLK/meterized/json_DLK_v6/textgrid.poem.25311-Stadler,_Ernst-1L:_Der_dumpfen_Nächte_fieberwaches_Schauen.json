{"textgrid.poem.25311": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der dumpfen N\u00e4chte fieberwaches Schauen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der dumpfen N\u00e4chte fieberwaches Schauen", "tokens": ["Der", "dum\u00b7pfen", "N\u00e4ch\u00b7te", "fie\u00b7ber\u00b7wa\u00b7ches", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wob sie dem Teppich ein mit hei\u00dfen H\u00e4nden", "tokens": ["wob", "sie", "dem", "Tep\u00b7pich", "ein", "mit", "hei\u00b7\u00dfen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und sang und spann bis sp\u00e4t ins Abendgrauen.", "tokens": ["und", "sang", "und", "spann", "bis", "sp\u00e4t", "ins", "A\u00b7bend\u00b7grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun hing er hingespannt von steilen W\u00e4nden", "tokens": ["Nun", "hing", "er", "hin\u00b7ge\u00b7spannt", "von", "stei\u00b7len", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit breiten Borden silbergr\u00fcnen S\u00e4umen", "tokens": ["mit", "brei\u00b7ten", "Bor\u00b7den", "sil\u00b7ber\u00b7gr\u00fc\u00b7nen", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und Sternen wei\u00df und wirr gleich Opferbr\u00e4nden", "tokens": ["und", "Ster\u00b7nen", "wei\u00df", "und", "wirr", "gleich", "Op\u00b7fer\u00b7br\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KON", "ADJD", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "goldadrig funkelnd \u00fcber schwarzen R\u00e4umen.", "tokens": ["gol\u00b7dad\u00b7rig", "fun\u00b7kelnd", "\u00fc\u00b7ber", "schwar\u00b7zen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und N\u00e4chte fielen. Und mit hei\u00dfen Wangen", "tokens": ["Und", "N\u00e4ch\u00b7te", "fie\u00b7len", ".", "Und", "mit", "hei\u00b7\u00dfen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stand sie und sah mit Augen wie aus Tr\u00e4umen", "tokens": ["stand", "sie", "und", "sah", "mit", "Au\u00b7gen", "wie", "aus", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "wie sich in stummem Tanz die F\u00e4den schlangen", "tokens": ["wie", "sich", "in", "stum\u00b7mem", "Tanz", "die", "F\u00e4\u00b7den", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "seltsam verwirkt zu flie\u00dfenden Geweben", "tokens": ["selt\u00b7sam", "ver\u00b7wirkt", "zu", "flie\u00b7\u00dfen\u00b7den", "Ge\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "PTKZU", "VVINF", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und j\u00e4h und rot vom Fackellicht umfangen.", "tokens": ["und", "j\u00e4h", "und", "rot", "vom", "Fa\u00b7ckel\u00b7licht", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wie aus Brunnen sprang entzaubert Leben.", "tokens": ["Und", "wie", "aus", "Brun\u00b7nen", "sprang", "ent\u00b7zau\u00b7bert", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "VVFIN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schauernd sah sie aus verrankten Schlingen", "tokens": ["Und", "schau\u00b7ernd", "sah", "sie", "aus", "ver\u00b7rank\u00b7ten", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "im Zwielicht geisternd hohe Schatten schweben", "tokens": ["im", "Zwie\u00b7licht", "geis\u00b7ternd", "ho\u00b7he", "Schat\u00b7ten", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "und Spiegelschein von fremden gro\u00dfen Dingen.", "tokens": ["und", "Spie\u00b7gel\u00b7schein", "von", "frem\u00b7den", "gro\u00b7\u00dfen", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und als im Grund der goldne Flaum vergl\u00fchte", "tokens": ["Und", "als", "im", "Grund", "der", "gold\u00b7ne", "Flaum", "ver\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und Schmelz und Farben welk und bla\u00df zergingen", "tokens": ["und", "Schmelz", "und", "Far\u00b7ben", "welk", "und", "bla\u00df", "zer\u00b7gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "sank sie und losch wie eine M\u00e4rzenbl\u00fcte.", "tokens": ["sank", "sie", "und", "losch", "wie", "ei\u00b7ne", "M\u00e4r\u00b7zen\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}