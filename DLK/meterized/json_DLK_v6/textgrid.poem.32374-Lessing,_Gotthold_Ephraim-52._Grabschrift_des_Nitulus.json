{"textgrid.poem.32374": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "52. Grabschrift des Nitulus", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier modert Nitulus, jungfr\u00e4ulichen Gesichts,", "tokens": ["Hier", "mo\u00b7dert", "Ni\u00b7tu\u00b7lus", ",", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der durch den Tod gewann: er wurde Staub aus Nichts.", "tokens": ["Der", "durch", "den", "Tod", "ge\u00b7wann", ":", "er", "wur\u00b7de", "Staub", "aus", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}