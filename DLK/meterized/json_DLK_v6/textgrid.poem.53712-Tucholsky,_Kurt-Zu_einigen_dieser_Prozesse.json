{"textgrid.poem.53712": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zu einigen dieser Prozesse", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Universit\u00e4ten forsch gesoffen,", "tokens": ["Auf", "U\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4\u00b7ten", "forsch", "ge\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in Kaiser-Fackelz\u00fcgen mitgeloffen,", "tokens": ["in", "Kai\u00b7ser\u00b7Fa\u00b7ckel\u00b7z\u00fc\u00b7gen", "mit\u00b7ge\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so wuchs das auf zum Referendar.", "tokens": ["so", "wuchs", "das", "auf", "zum", "Re\u00b7fe\u00b7ren\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus aufs Land, wo brave Bauern wohnen.", "tokens": ["Hin\u00b7aus", "aufs", "Land", ",", "wo", "bra\u00b7ve", "Bau\u00b7ern", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und auf den ersten Amtsgerichtsstationen", "tokens": ["Und", "auf", "den", "ers\u00b7ten", "Amts\u00b7ge\u00b7richts\u00b7sta\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "kr\u00fcmmt sich, was nie ein Recke war.", "tokens": ["kr\u00fcmmt", "sich", ",", "was", "nie", "ein", "Re\u00b7cke", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie k\u00f6nnen alle Paragraphen nennen", "tokens": ["Sie", "k\u00f6n\u00b7nen", "al\u00b7le", "Pa\u00b7ra\u00b7gra\u00b7phen", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und lernen Menschen nur aus Akten kennen.", "tokens": ["und", "ler\u00b7nen", "Men\u00b7schen", "nur", "aus", "Ak\u00b7ten", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Examenspaukerei. Das Stammtisch-Schnitzel.", "tokens": ["E\u00b7xa\u00b7men\u00b7spau\u00b7ke\u00b7rei", ".", "Das", "Stamm\u00b7tischSchnit\u00b7zel", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Staatsanwalt erzieht zum Herrschaftskitzel.", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "er\u00b7zieht", "zum", "Herr\u00b7schafts\u00b7kit\u00b7zel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbwas heult die Frau? Ich brauch ein Protokoll!", "tokens": ["\u00bb", "was", "heult", "die", "Frau", "?", "Ich", "brauch", "ein", "Pro\u00b7to\u00b7koll", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich schreibe flei\u00dfig meine Akten voll:", "tokens": ["Ich", "schrei\u00b7be", "flei\u00b7\u00dfig", "mei\u00b7ne", "Ak\u00b7ten", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Im Namen des K\u00f6nigs \u2013!\u00ab", "tokens": ["Im", "Na\u00b7men", "des", "K\u00f6\u00b7nigs", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf seinem Armes\u00fcnderst\u00fchlchen droben", "tokens": ["Auf", "sei\u00b7nem", "Ar\u00b7me\u00b7s\u00fcn\u00b7der\u00b7st\u00fchl\u00b7chen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sitzt das in seidenen, faltenreichen Roben \u2013", "tokens": ["sitzt", "das", "in", "sei\u00b7de\u00b7nen", ",", "fal\u00b7ten\u00b7rei\u00b7chen", "Ro\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPOSAT", "$,", "ADJA", "NN", "$("], "meter": "---+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "darunter grauer Spie\u00dferrock.", "tokens": ["da\u00b7run\u00b7ter", "grau\u00b7er", "Spie\u00b7\u00dfer\u00b7rock", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herzen schlagen rechts. In den Prozessen,", "tokens": ["Die", "Her\u00b7zen", "schla\u00b7gen", "rechts", ".", "In", "den", "Pro\u00b7zes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "in denen sich ein Freiheitsmann vergessen,", "tokens": ["in", "de\u00b7nen", "sich", "ein", "Frei\u00b7heits\u00b7mann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "zuckt durch den Saal der B\u00fcttelstock.", "tokens": ["zuckt", "durch", "den", "Saal", "der", "B\u00fct\u00b7tel\u00b7stock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Staatsanwalt amtiert im selben Hause;", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "am\u00b7tiert", "im", "sel\u00b7ben", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "man spricht mit ihm, so in der Fr\u00fchst\u00fcckspause.", "tokens": ["man", "spricht", "mit", "ihm", ",", "so", "in", "der", "Fr\u00fch\u00b7st\u00fccks\u00b7pau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Rechtsanwalt scheint eine Art Komplice.", "tokens": ["Der", "Rechts\u00b7an\u00b7walt", "scheint", "ei\u00b7ne", "Art", "Kom\u00b7pli\u00b7ce", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Staatsanwalt monokelt voll Malice.", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "mo\u00b7no\u00b7kelt", "voll", "Ma\u00b7li\u00b7ce", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die Richter kennen ihn, und er kennt sie:", "tokens": ["Die", "Rich\u00b7ter", "ken\u00b7nen", "ihn", ",", "und", "er", "kennt", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und was er nicht besorgt, besorgen die.", "tokens": ["Und", "was", "er", "nicht", "be\u00b7sorgt", ",", "be\u00b7sor\u00b7gen", "die", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "ADJD", "$,", "VVFIN", "ART", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Im Namen des Volkes \u2013!", "tokens": ["Im", "Na\u00b7men", "des", "Vol\u00b7kes", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "So hat die Urteilsformel sich gewandelt.", "tokens": ["So", "hat", "die", "Ur\u00b7teils\u00b7for\u00b7mel", "sich", "ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wird im alten Ungeist fortverhandelt,", "tokens": ["Doch", "wird", "im", "al\u00b7ten", "Un\u00b7geist", "fort\u00b7ver\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ganz wie in jener Kaiserzeit.", "tokens": ["ganz", "wie", "in", "je\u00b7ner", "Kai\u00b7ser\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vorvernehmung und Geschworenensiebung", "tokens": ["Und", "Vor\u00b7ver\u00b7neh\u00b7mung", "und", "Ge\u00b7schwo\u00b7re\u00b7nen\u00b7sie\u00b7bung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.5": {"text": "und Fragestellung und die Strafverschiebung \u2013", "tokens": ["und", "Fra\u00b7ge\u00b7stel\u00b7lung", "und", "die", "Straf\u00b7ver\u00b7schie\u00b7bung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo steckt da die Parteilichkeit?", "tokens": ["Wo", "steckt", "da", "die", "Par\u00b7tei\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo, deutsche Richter? Tief in euern Herzen!", "tokens": ["Wo", ",", "deut\u00b7sche", "Rich\u00b7ter", "?", "Tief", "in", "eu\u00b7ern", "Her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$.", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir kennen euch und eurer Opfer Schmerzen!", "tokens": ["Wir", "ken\u00b7nen", "euch", "und", "eu\u00b7rer", "Op\u00b7fer", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wir glauben euch nicht mehr und eurer Waage \u2013", "tokens": ["Wir", "glau\u00b7ben", "euch", "nicht", "mehr", "und", "eu\u00b7rer", "Waa\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das Ding h\u00e4ngt schief! Das sehn wir alle Tage.", "tokens": ["Das", "Ding", "h\u00e4ngt", "schief", "!", "Das", "sehn", "wir", "al\u00b7le", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Binde der Justitia \u2013 welch ein Bruch!", "tokens": ["Die", "Bin\u00b7de", "der", "Jus\u00b7ti\u00b7tia", "\u2013", "welch", "ein", "Bruch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "$(", "PWAT", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Steht auf!", "tokens": ["Steht", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Und dies sei euer Urteilsspruch:", "tokens": ["Und", "dies", "sei", "eu\u00b7er", "Ur\u00b7teils\u00b7spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sehn wir euch an, packt uns ein tiefes Graun \u2013", "tokens": ["Sehn", "wir", "euch", "an", ",", "packt", "uns", "ein", "tie\u00b7fes", "Graun", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wir haben zu euch Richtern kein Vertraun!", "tokens": ["Wir", "ha\u00b7ben", "zu", "euch", "Rich\u00b7tern", "kein", "Ver\u00b7traun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Im Namen des Volkes \u2013!", "tokens": ["Im", "Na\u00b7men", "des", "Vol\u00b7kes", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}