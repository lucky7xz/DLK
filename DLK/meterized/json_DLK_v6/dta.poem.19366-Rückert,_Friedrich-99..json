{"dta.poem.19366": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du Gerechtigkeit nicht in des Menschen Brust", "tokens": ["Wenn", "du", "Ge\u00b7rech\u00b7tig\u00b7keit", "nicht", "in", "des", "Men\u00b7schen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewurzelt anerkennst, wie Unrecht du dir thust!", "tokens": ["Ge\u00b7wur\u00b7zelt", "an\u00b7er\u00b7kennst", ",", "wie", "Un\u00b7recht", "du", "dir", "thust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist von st\u00e4rkeren umgeben als du bist,", "tokens": ["Du", "bist", "von", "st\u00e4r\u00b7ke\u00b7ren", "um\u00b7ge\u00b7ben", "als", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die schaden k\u00f6nnten dir, wenn wollten, jede Frist.", "tokens": ["Die", "scha\u00b7den", "k\u00f6nn\u00b7ten", "dir", ",", "wenn", "woll\u00b7ten", ",", "je\u00b7de", "Frist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "$,", "KOUS", "VMFIN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nichts gibt dir Sicherheit, als aus dir selbst zu wissen,", "tokens": ["Nichts", "gibt", "dir", "Si\u00b7cher\u00b7heit", ",", "als", "aus", "dir", "selbst", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$,", "KOUS", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Unrecht dir zu thun sie hindert ihr Gewissen.", "tokens": ["Da\u00df", "Un\u00b7recht", "dir", "zu", "thun", "sie", "hin\u00b7dert", "ihr", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PTKZU", "VVINF", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}