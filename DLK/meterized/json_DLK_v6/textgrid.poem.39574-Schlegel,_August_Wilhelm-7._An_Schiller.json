{"textgrid.poem.39574": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "7. An Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unwi\u00dfend darfst du Friedrich Schlegel schelten?", "tokens": ["Un\u00b7wi\u00b7\u00dfend", "darfst", "du", "Fried\u00b7rich", "Schle\u00b7gel", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie? meinst du selber f\u00fcr gelehrt zu gelten?", "tokens": ["Wie", "?", "meinst", "du", "sel\u00b7ber", "f\u00fcr", "ge\u00b7lehrt", "zu", "gel\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "APPR", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du warst verblendet, da\u00df es Gott erbarm'!", "tokens": ["Du", "warst", "ver\u00b7blen\u00b7det", ",", "da\u00df", "es", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Bettler Irus schilt den Kr\u00f6sus arm.", "tokens": ["Der", "Bett\u00b7ler", "I\u00b7rus", "schilt", "den", "Kr\u00f6\u00b7sus", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}