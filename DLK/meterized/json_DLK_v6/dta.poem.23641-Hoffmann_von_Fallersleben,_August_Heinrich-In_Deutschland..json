{"dta.poem.23641": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "In Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ist Freude, noch ist Leben", "tokens": ["Noch", "ist", "Freu\u00b7de", ",", "noch", "ist", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberall im deutschen Land.", "tokens": ["Ue\u00b7be\u00b7rall", "im", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutsche Fraun und M\u00e4nner geben", "tokens": ["Deut\u00b7sche", "Fraun", "und", "M\u00e4n\u00b7ner", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich einander noch die Hand.", "tokens": ["Sich", "ein\u00b7an\u00b7der", "noch", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der sch\u00f6ne Glaube lebt noch", "tokens": ["Und", "der", "sch\u00f6\u00b7ne", "Glau\u00b7be", "lebt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An die deutsche Ehrlichkeit.", "tokens": ["An", "die", "deut\u00b7sche", "Ehr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Geist der Treue schwebt noch", "tokens": ["Und", "der", "Geist", "der", "Treu\u00b7e", "schwebt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ueber uns und unsrer Zeit.", "tokens": ["Ue\u00b7ber", "uns", "und", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es wird noch Fr\u00fchling wieder", "tokens": ["Und", "es", "wird", "noch", "Fr\u00fch\u00b7ling", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch f\u00fcr uns in Wald und Feld,", "tokens": ["Auch", "f\u00fcr", "uns", "in", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es singt noch frohe Lieder", "tokens": ["Und", "es", "singt", "noch", "fro\u00b7he", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberall die deutsche Welt.", "tokens": ["Ue\u00b7be\u00b7rall", "die", "deut\u00b7sche", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahrheit findet noch und Dichtung", "tokens": ["Wahr\u00b7heit", "fin\u00b7det", "noch", "und", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Herzen, ihren Mund,", "tokens": ["Ih\u00b7re", "Her\u00b7zen", ",", "ih\u00b7ren", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es thut nach mancher Richtung", "tokens": ["Und", "es", "thut", "nach", "man\u00b7cher", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich das Sch\u00f6n' und Bessre kund.", "tokens": ["Sich", "das", "Sch\u00f6n'", "und", "Bess\u00b7re", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tadelt nicht die Zeit die neue", "tokens": ["Ta\u00b7delt", "nicht", "die", "Zeit", "die", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschet nicht das Heute fern!", "tokens": ["W\u00fcn\u00b7schet", "nicht", "das", "Heu\u00b7te", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeit ist da\u00df sich jeder freue,", "tokens": ["Zeit", "ist", "da\u00df", "sich", "je\u00b7der", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOUS", "PRF", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder lobe Gott den Herrn.", "tokens": ["Je\u00b7der", "lo\u00b7be", "Gott", "den", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprecht ihr Weisen, sprecht ihr Thoren!", "tokens": ["Sprecht", "ihr", "Wei\u00b7sen", ",", "sprecht", "ihr", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer w\u00e4re nicht ein Kind? \u2014", "tokens": ["Und", "wer", "w\u00e4\u00b7re", "nicht", "ein", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VAFIN", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich bin zu fr\u00fch geboren!", "tokens": ["Ach", "!", "ich", "bin", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine neue Welt beginnt.", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Welt", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}