{"textgrid.poem.49572": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Magister ", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Magister ", "tokens": ["Ma\u00b7gis\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "\u00bbich bin kein Scharletan;", "tokens": ["\u00bb", "ich", "bin", "kein", "Schar\u00b7le\u00b7tan", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Wenigsten so ungelehrt,", "tokens": ["Am", "We\u00b7nigs\u00b7ten", "so", "un\u00b7ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich nicht schreiben kann.", "tokens": ["Da\u00df", "ich", "nicht", "schrei\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Buch vom Astrolabium,", "tokens": ["Das", "Buch", "vom", "Ast\u00b7ro\u00b7la\u00b7bi\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das mir der Neider raubt,", "tokens": ["Das", "mir", "der", "Nei\u00b7der", "raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist und verbleibt mein Eigenthum:", "tokens": ["Ist", "und", "ver\u00b7bleibt", "mein", "Ei\u00b7gen\u00b7thum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kam aus meinem Haupt'.\u00ab", "tokens": ["Und", "kam", "aus", "mei\u00b7nem", "Haupt'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich glaub es gern, Magister ", "tokens": ["Ich", "glaub", "es", "gern", ",", "Ma\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es kam aus euerm Haupt'!", "tokens": ["Es", "kam", "aus", "eu\u00b7erm", "Haupt'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt aber ein Konsilium", "tokens": ["H\u00f6rt", "a\u00b7ber", "ein", "Kon\u00b7si\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Minervens! Schenkts dem Dokter ", "tokens": ["Mi\u00b7ner\u00b7vens", "!", "Schenkts", "dem", "Dok\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sammt allem Stil und Stof:", "tokens": ["Sammt", "al\u00b7lem", "Stil", "und", "Stof", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So hei\u00dft er bey dem Publikum", "tokens": ["So", "hei\u00dft", "er", "bey", "dem", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein schlechter Philosoph!", "tokens": ["Ein", "schlech\u00b7ter", "Phi\u00b7lo\u00b7soph", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}