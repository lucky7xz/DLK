{"textgrid.poem.29230": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heist Marcipan Soldaten Brot? So essens nur die Grossen;", "tokens": ["Heist", "Mar\u00b7ci\u00b7pan", "Sol\u00b7da\u00b7ten", "Brot", "?", "So", "es\u00b7sens", "nur", "die", "Gros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NN", "$.", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der arme Knecht der mag sich nur am Pompernickel stossen.", "tokens": ["Der", "ar\u00b7me", "Knecht", "der", "mag", "sich", "nur", "am", "Pom\u00b7per\u00b7ni\u00b7ckel", "stos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}