{"textgrid.poem.65314": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Garten des Herzens", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meines Herzens Mitte bl\u00fcht ein G\u00e4rtchen,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Mit\u00b7te", "bl\u00fcht", "ein", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlossen ist es durch ein enges Pf\u00f6rtchen,", "tokens": ["Ver\u00b7schlos\u00b7sen", "ist", "es", "durch", "ein", "en\u00b7ges", "Pf\u00f6rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu dem den Schl\u00fcssel f\u00fchrt mein liebes M\u00e4dchen.", "tokens": ["Zu", "dem", "den", "Schl\u00fcs\u00b7sel", "f\u00fchrt", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist April. Komm, wolle dich nicht sch\u00e4men,", "tokens": ["Es", "ist", "Ap\u00b7ril", ".", "Komm", ",", "wol\u00b7le", "dich", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "VVFIN", "$,", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und pfl\u00fccke dir heraus die liebsten Blumen!", "tokens": ["Und", "pfl\u00fc\u00b7cke", "dir", "he\u00b7raus", "die", "liebs\u00b7ten", "Blu\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie dr\u00e4ngen sich entgegen deinen H\u00e4nden.", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "ent\u00b7ge\u00b7gen", "dei\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Je mehr du pfl\u00fcckst, je mehr sie wieder sprossen,", "tokens": ["Je", "mehr", "du", "pfl\u00fcckst", ",", "je", "mehr", "sie", "wie\u00b7der", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch willst du unber\u00fchrt sie bl\u00fchen lassen,", "tokens": ["Doch", "willst", "du", "un\u00b7be\u00b7r\u00fchrt", "sie", "bl\u00fc\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So werden sie vor ihrer Zeit vertrocknen.", "tokens": ["So", "wer\u00b7den", "sie", "vor", "ih\u00b7rer", "Zeit", "ver\u00b7trock\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}