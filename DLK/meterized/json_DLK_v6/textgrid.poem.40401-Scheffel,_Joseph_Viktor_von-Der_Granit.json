{"textgrid.poem.40401": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Granit", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In unterirdischer Kammer", "tokens": ["In", "un\u00b7ter\u00b7ir\u00b7di\u00b7scher", "Kam\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sprach grollend der alte Granit:", "tokens": ["Sprach", "grol\u00b7lend", "der", "al\u00b7te", "Gra\u00b7nit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbda droben den w\u00e4\u00dfrigen Jammer", "tokens": ["\u00bb", "da", "dro\u00b7ben", "den", "w\u00e4\u00df\u00b7ri\u00b7gen", "Jam\u00b7mer"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den mach' ich jetzt l\u00e4nger nicht mit.", "tokens": ["Den", "mach'", "ich", "jetzt", "l\u00e4n\u00b7ger", "nicht", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Langweilig w\u00e4lzt das Gew\u00e4sser", "tokens": ["Lang\u00b7wei\u00b7lig", "w\u00e4lzt", "das", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seine salzige Flut \u00fcbers Land,", "tokens": ["Sei\u00b7ne", "sal\u00b7zi\u00b7ge", "Flut", "\u00fc\u00b7bers", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Statt stolzer und sch\u00f6ner und besser", "tokens": ["Statt", "stol\u00b7zer", "und", "sch\u00f6\u00b7ner", "und", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "KON", "ADJA", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wird alles voll Schlamm und voll Sand.", "tokens": ["Wird", "al\u00b7les", "voll", "Schlamm", "und", "voll", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das g\u00e4b' eine mitleidwerte", "tokens": ["Das", "g\u00e4b'", "ei\u00b7ne", "mit\u00b7leid\u00b7wer\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geologische Leimsiederei,", "tokens": ["Geo\u00b7lo\u00b7gi\u00b7sche", "Leim\u00b7sie\u00b7de\u00b7rei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wenn die ganze Kruste der Erde", "tokens": ["Wenn", "die", "gan\u00b7ze", "Krus\u00b7te", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nur ein sediment\u00e4res Gebr\u00e4u.", "tokens": ["Nur", "ein", "se\u00b7di\u00b7men\u00b7t\u00e4\u00b7res", "Ge\u00b7br\u00e4u", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Am End' w\u00fcrd' noch Fabel und Dichtung,", "tokens": ["Am", "End'", "w\u00fcrd'", "noch", "Fa\u00b7bel", "und", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Was ein Berg \u2013 was hoch und was tief;", "tokens": ["Was", "ein", "Berg", "\u2013", "was", "hoch", "und", "was", "tief", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$(", "PWS", "ADJD", "KON", "PWS", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Zum Teufel die Fl\u00f6zung und Schichtung,", "tokens": ["Zum", "Teu\u00b7fel", "die", "Fl\u00f6\u00b7zung", "und", "Schich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hurra! ich werd' eruptiv!\u00ab", "tokens": ["Hur\u00b7ra", "!", "ich", "werd'", "e\u00b7rup\u00b7tiv", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach's, und zum Beistand berief er", "tokens": ["Er", "sprach's", ",", "und", "zum", "Bei\u00b7stand", "be\u00b7rief", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die tapfern Porphyre herbei,", "tokens": ["Die", "tap\u00b7fern", "Por\u00b7phy\u00b7re", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die kristallinischen Schiefer", "tokens": ["Die", "kris\u00b7tal\u00b7li\u00b7ni\u00b7schen", "Schie\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ri\u00df h\u00f6hnisch er mitten entzwei.", "tokens": ["Ri\u00df", "h\u00f6h\u00b7nisch", "er", "mit\u00b7ten", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das zischte und lohte und wallte,", "tokens": ["Das", "zischte", "und", "loh\u00b7te", "und", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als nahte das Ende der Welt;", "tokens": ["Als", "nah\u00b7te", "das", "En\u00b7de", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Selbst Grauwack, die z\u00fcchtige Alte,", "tokens": ["Selbst", "Grau\u00b7wack", ",", "die", "z\u00fcch\u00b7ti\u00b7ge", "Al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hat vor Schreck auf den Kopf sich gestellt.", "tokens": ["Hat", "vor", "Schreck", "auf", "den", "Kopf", "sich", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Auch Steinkohl' und Zechstein und Trias", "tokens": ["Auch", "Stein\u00b7kohl'", "und", "Zechs\u00b7tein", "und", "Trias"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Entwichen, im Innern gesprengt,", "tokens": ["Ent\u00b7wi\u00b7chen", ",", "im", "In\u00b7nern", "ge\u00b7sprengt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Laut jammert im Jura der Lias,", "tokens": ["Laut", "jam\u00b7mert", "im", "Ju\u00b7ra", "der", "Lias", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da\u00df die Glut ihn von hinten versengt.", "tokens": ["Da\u00df", "die", "Glut", "ihn", "von", "hin\u00b7ten", "ver\u00b7sengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Auch die Kalke, die Mergel der Kreiden", "tokens": ["Auch", "die", "Kal\u00b7ke", ",", "die", "Mer\u00b7gel", "der", "Krei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sprachen sp\u00e4ter mit wichtigem Ton:", "tokens": ["Spra\u00b7chen", "sp\u00e4\u00b7ter", "mit", "wich\u00b7ti\u00b7gem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u00bbwas erstickte man nicht schon beizeiten", "tokens": ["\u00bb", "was", "er\u00b7stick\u00b7te", "man", "nicht", "schon", "bei\u00b7zei\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PIS", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Den Keim dieser Revolution?\u00ab", "tokens": ["Den", "Keim", "die\u00b7ser", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch vorw\u00e4rts, trotz Schichten und Seen,", "tokens": ["Doch", "vor\u00b7w\u00e4rts", ",", "trotz", "Schich\u00b7ten", "und", "Seen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Drang siegreich der feurige Held,", "tokens": ["Drang", "sieg\u00b7reich", "der", "feu\u00b7ri\u00b7ge", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bis da\u00df er von sonnigen H\u00f6hen", "tokens": ["Bis", "da\u00df", "er", "von", "son\u00b7ni\u00b7gen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu F\u00fc\u00dfen sich schaute die Welt.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "sich", "schau\u00b7te", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da sprach er mit Jodeln und Singen:", "tokens": ["Da", "sprach", "er", "mit", "Jo\u00b7deln", "und", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "\u00bbhurra! das w\u00e4re gegl\u00fcckt!", "tokens": ["\u00bb", "hur\u00b7ra", "!", "das", "w\u00e4\u00b7re", "ge\u00b7gl\u00fcckt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Auch unsereins kann's zu was bringen,", "tokens": ["Auch", "un\u00b7ser\u00b7eins", "kann's", "zu", "was", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn er nur herzhaftiglich dr\u00fcckt!\u00ab", "tokens": ["Wenn", "er", "nur", "herz\u00b7haf\u00b7tig\u00b7lich", "dr\u00fcckt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "---+---+", "measure": "unknown.measure.di"}}}}}