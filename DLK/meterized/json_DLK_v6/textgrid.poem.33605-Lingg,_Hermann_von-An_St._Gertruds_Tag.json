{"textgrid.poem.33605": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "An St. Gertruds Tag", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gertrud, erste G\u00e4rtnerin,", "tokens": ["O", "Ger\u00b7trud", ",", "ers\u00b7te", "G\u00e4rt\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum im wei\u00dfen Kleide?", "tokens": ["Wa\u00b7rum", "im", "wei\u00b7\u00dfen", "Klei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weit besser w\u00e4r' nach deinem Sinn", "tokens": ["Weit", "bes\u00b7ser", "w\u00e4r'", "nach", "dei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kleid von gr\u00fcner Seide.", "tokens": ["Ein", "Kleid", "von", "gr\u00fc\u00b7ner", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein gr\u00fcnes Kleid, das st\u00fcnde dir", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Kleid", ",", "das", "st\u00fcn\u00b7de", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit besser als der Rocken,", "tokens": ["Weit", "bes\u00b7ser", "als", "der", "Ro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Komm aus der Stube, komm und zier", "tokens": ["Komm", "aus", "der", "Stu\u00b7be", ",", "komm", "und", "zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Veilchen deine Locken,", "tokens": ["Mit", "Veil\u00b7chen", "dei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nimm auch den Rechen in die Hand,", "tokens": ["Nimm", "auch", "den", "Re\u00b7chen", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Strohhut und ein Rosenband!", "tokens": ["Den", "Stroh\u00b7hut", "und", "ein", "Ro\u00b7sen\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es m\u00f6chte gern der Br\u00e4utigam,", "tokens": ["Es", "m\u00f6ch\u00b7te", "gern", "der", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling, zu dir kommen.", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "zu", "dir", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat das junge wei\u00dfe Lamm", "tokens": ["Er", "hat", "das", "jun\u00b7ge", "wei\u00b7\u00dfe", "Lamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Winter abgenommen.", "tokens": ["Dem", "Win\u00b7ter", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er m\u00f6cht' es f\u00fchren an den Bach", "tokens": ["Er", "m\u00f6cht'", "es", "f\u00fch\u00b7ren", "an", "den", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00fchren auf die Weide;", "tokens": ["Und", "f\u00fch\u00b7ren", "auf", "die", "Wei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O ruf ihn doch und rufe wach", "tokens": ["O", "ruf", "ihn", "doch", "und", "ru\u00b7fe", "wach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "ADV", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Blumen auf der Heide,", "tokens": ["Die", "Blu\u00b7men", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und schlie\u00df uns auf den Erdengrund", "tokens": ["Und", "schlie\u00df", "uns", "auf", "den", "Er\u00b7den\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit deinem Himmelsschl\u00fcsselbund!", "tokens": ["Mit", "dei\u00b7nem", "Him\u00b7mels\u00b7schl\u00fcs\u00b7sel\u00b7bund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}