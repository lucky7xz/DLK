{"textgrid.poem.51009": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Stiller Augenblick", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliehendes Jahr, in duftigen Schleiern", "tokens": ["Flie\u00b7hen\u00b7des", "Jahr", ",", "in", "duf\u00b7ti\u00b7gen", "Schlei\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Streifend an abendr\u00f6tlichen Weihern,", "tokens": ["Strei\u00b7fend", "an", "a\u00b7bend\u00b7r\u00f6t\u00b7li\u00b7chen", "Wei\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wallest du deine Bahn;", "tokens": ["Wal\u00b7lest", "du", "dei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Siehst mich am k\u00fchlen Waldsee stehen,", "tokens": ["Siehst", "mich", "am", "k\u00fch\u00b7len", "Wald\u00b7see", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo an herbstlichen Uferh\u00f6hen", "tokens": ["Wo", "an", "herbst\u00b7li\u00b7chen", "Uf\u00b7er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zieht entlang ein stummer Schwan.", "tokens": ["Zieht", "ent\u00b7lang", "ein", "stum\u00b7mer", "Schwan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still und einsam schwingt er die Fl\u00fcgel,", "tokens": ["Still", "und", "ein\u00b7sam", "schwingt", "er", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Tauchet in den Wasserspiegel,", "tokens": ["Tau\u00b7chet", "in", "den", "Was\u00b7ser\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt den Hals empor und lauscht;", "tokens": ["Hebt", "den", "Hals", "em\u00b7por", "und", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Taucht zum andern Male nieder,", "tokens": ["Taucht", "zum", "an\u00b7dern", "Ma\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Richtet sich auf und lauschet wieder,", "tokens": ["Rich\u00b7tet", "sich", "auf", "und", "lau\u00b7schet", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie's im fl\u00fcsternden Schilfe rauscht.", "tokens": ["Wie's", "im", "fl\u00fcs\u00b7tern\u00b7den", "Schil\u00b7fe", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und in seinem Tun und Lassen", "tokens": ["Und", "in", "sei\u00b7nem", "Tun", "und", "Las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will's mich wie ein Traum erfassen,", "tokens": ["Will's", "mich", "wie", "ein", "Traum", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob's meine Seele w\u00e4r,", "tokens": ["Als", "ob's", "mei\u00b7ne", "See\u00b7le", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verwundert \u00fcber das Leben,", "tokens": ["Die", "ver\u00b7wun\u00b7dert", "\u00fc\u00b7ber", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u00dcber das Hin- und Widerschweben,", "tokens": ["\u00dc\u00b7ber", "das", "Hin", "und", "Wi\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Lugt' und lauschte hin und her.", "tokens": ["Lugt'", "und", "lauschte", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Atme nur in vollen Z\u00fcgen", "tokens": ["At\u00b7me", "nur", "in", "vol\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses friedliche Gen\u00fcgen", "tokens": ["Die\u00b7ses", "fried\u00b7li\u00b7che", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam auf der stillen Flur!", "tokens": ["Ein\u00b7sam", "auf", "der", "stil\u00b7len", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hast du dich klar empfunden,", "tokens": ["Und", "hast", "du", "dich", "klar", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "M\u00f6gen enden deine Stunden,", "tokens": ["M\u00f6\u00b7gen", "en\u00b7den", "dei\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie zerflie\u00dft die Schwanenspur!", "tokens": ["Wie", "zer\u00b7flie\u00dft", "die", "Schwa\u00b7nens\u00b7pur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}