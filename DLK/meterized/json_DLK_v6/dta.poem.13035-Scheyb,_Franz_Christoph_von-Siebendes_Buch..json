{"dta.poem.13035": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf erblickte man in seinem Angesicht,", "tokens": ["Hier\u00b7auf", "er\u00b7blick\u00b7te", "man", "in", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als h\u00e4tt er etwan schon von allem Unterricht:", "tokens": ["Als", "h\u00e4tt", "er", "et\u00b7wan", "schon", "von", "al\u00b7lem", "Un\u00b7ter\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da\u00df er sich vielleicht noch mehr belehren wollte,", "tokens": ["Doch", "da\u00df", "er", "sich", "viel\u00b7leicht", "noch", "mehr", "be\u00b7leh\u00b7ren", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "240Bevor man seinen Sinn und Rath vernehmen sollte.", "tokens": ["man", "sei\u00b7nen", "Sinn", "und", "Rath", "ver\u00b7neh\u00b7men", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Schnee-wei\u00df krauses Haar, so Kinn und Haupt umfieng;", "tokens": ["Sein", "Schnee\u00b7wei\u00df", "krau\u00b7ses", "Haar", ",", "so", "Kinn", "und", "Haupt", "um\u00b7fi\u00b7eng", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Das Aug, aus welchem Ernst und Geist und Anmuth gieng,", "tokens": ["Das", "Aug", ",", "aus", "wel\u00b7chem", "Ernst", "und", "Geist", "und", "An\u00b7muth", "gieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gab dessen Wiz, Vernunft und klugen Geist zu kennen;", "tokens": ["Gab", "des\u00b7sen", "Wiz", ",", "Ver\u00b7nunft", "und", "klu\u00b7gen", "Geist", "zu", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$,", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum war man auch bereit, ihm gleich Geh\u00f6r zu g\u00f6nnen.", "tokens": ["Drum", "war", "man", "auch", "be\u00b7reit", ",", "ihm", "gleich", "Ge\u00b7h\u00f6r", "zu", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "ADJD", "$,", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}