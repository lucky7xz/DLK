{"textgrid.poem.45836": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[der Gottheit hoher Tempel ist zerst\u00f6ret]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gottheit hoher Tempel ist zerst\u00f6ret", "tokens": ["Der", "Got\u00b7theit", "ho\u00b7her", "Tem\u00b7pel", "ist", "zer\u00b7st\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es r\u00fcnden an der heil'gen Kuppel sich die T\u00f6ne", "tokens": ["Es", "r\u00fcn\u00b7den", "an", "der", "heil'\u00b7gen", "Kup\u00b7pel", "sich", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht mehr in sch\u00f6ne Worte des Gebetes,", "tokens": ["Nicht", "mehr", "in", "sch\u00f6\u00b7ne", "Wor\u00b7te", "des", "Ge\u00b7be\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und teilen sich im Takte an den S\u00e4ulen", "tokens": ["Und", "tei\u00b7len", "sich", "im", "Tak\u00b7te", "an", "den", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den' in den Kronen leichte Melodien", "tokens": ["Den'", "in", "den", "Kro\u00b7nen", "leich\u00b7te", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "In lieblicher Verirrung sch\u00f6ner Locken", "tokens": ["In", "lieb\u00b7li\u00b7cher", "Ver\u00b7ir\u00b7rung", "sch\u00f6\u00b7ner", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf ihre ernsten hohen Stirnen wallen.", "tokens": ["Auf", "ih\u00b7re", "erns\u00b7ten", "ho\u00b7hen", "Stir\u00b7nen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zertr\u00fcmmert ist das herrliche Geb\u00e4ude", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "ist", "das", "herr\u00b7li\u00b7che", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und mit dem Echo ist das Wort gestorben.", "tokens": ["Und", "mit", "dem", "E\u00b7cho", "ist", "das", "Wort", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vom weiten Himmel hallt kein Lied zur\u00fccke,", "tokens": ["Vom", "wei\u00b7ten", "Him\u00b7mel", "hallt", "kein", "Lied", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Denn schrecklich ist die Macht des gro\u00dfen Lebens", "tokens": ["Denn", "schreck\u00b7lich", "ist", "die", "Macht", "des", "gro\u00b7\u00dfen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und unerme\u00dflich ist es hier zu beten.", "tokens": ["Und", "un\u00b7er\u00b7me\u00df\u00b7lich", "ist", "es", "hier", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}