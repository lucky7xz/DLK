{"dta.poem.15889": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ela\u00df \u00fcber dieses Buch uns nun zum Urtheil schreiten!\u201c", "tokens": ["\u201e", "la\u00df", "\u00fc\u00b7ber", "die\u00b7ses", "Buch", "uns", "nun", "zum", "Ur\u00b7theil", "schrei\u00b7ten", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "APPR", "PDAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Urtheil ist nicht leicht, das Buch hat viele Seiten.", "tokens": ["Das", "Ur\u00b7theil", "ist", "nicht", "leicht", ",", "das", "Buch", "hat", "vie\u00b7le", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Fragst du, was du daraus f\u00fcr Kopf und Herz gewannst,", "tokens": ["Fragst", "du", ",", "was", "du", "da\u00b7raus", "f\u00fcr", "Kopf", "und", "Herz", "ge\u00b7wannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ists ein Buch das du genug nicht sch\u00e4tzen kannst.", "tokens": ["So", "ists", "ein", "Buch", "das", "du", "ge\u00b7nug", "nicht", "sch\u00e4t\u00b7zen", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fragst du nach dem Genu\u00df, so ist es zu genie\u00dfen", "tokens": ["Fragst", "du", "nach", "dem", "Ge\u00b7nu\u00df", ",", "so", "ist", "es", "zu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie sch\u00f6ne Rosen, die an garstigen Dornen sprie\u00dfen.", "tokens": ["Wie", "sch\u00f6\u00b7ne", "Ro\u00b7sen", ",", "die", "an", "gars\u00b7ti\u00b7gen", "Dor\u00b7nen", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Du freuest immer fort dich jeder sch\u00f6nen Bl\u00fcte,", "tokens": ["Du", "freu\u00b7est", "im\u00b7mer", "fort", "dich", "je\u00b7der", "sch\u00f6\u00b7nen", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchlest stets dabei den Stachel im Gem\u00fcte.", "tokens": ["Und", "f\u00fch\u00b7lest", "stets", "da\u00b7bei", "den", "Sta\u00b7chel", "im", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und fragst du endlich: Was kommt f\u00fcr der Menschheit Heil", "tokens": ["Und", "fragst", "du", "end\u00b7lich", ":", "Was", "kommt", "f\u00fcr", "der", "Menschheit", "Heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dabei heraus? das ist des Buches schw\u00e4chster Theil.", "tokens": ["Da\u00b7bei", "he\u00b7raus", "?", "das", "ist", "des", "Bu\u00b7ches", "schw\u00e4chs\u00b7ter", "Theil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Rein sch\u00f6n, das ist es nicht, und minder noch rein wahr,", "tokens": ["Rein", "sch\u00f6n", ",", "das", "ist", "es", "nicht", ",", "und", "min\u00b7der", "noch", "rein", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "$,", "KON", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rein gut am wenigsten, ein Zwitter ganz und gar.", "tokens": ["Rein", "gut", "am", "we\u00b7nigs\u00b7ten", ",", "ein", "Zwit\u00b7ter", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "PIS", "$,", "ART", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}