{"textgrid.poem.53698": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Harden", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nicht mehr \u2013 Sie werden das begreifen \u2013", "tokens": ["Ich", "kann", "nicht", "mehr", "\u2013", "Sie", "wer\u00b7den", "das", "be\u00b7grei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$(", "PPER", "VAFIN", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bei jedem Attentat ein Trauerliedchen pfeifen \u2013", "tokens": ["bei", "je\u00b7dem", "At\u00b7ten\u00b7tat", "ein", "Trau\u00b7er\u00b7lied\u00b7chen", "pfei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "es sind zu viel.", "tokens": ["es", "sind", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es gibt da ehemalige Offiziere,", "tokens": ["Es", "gibt", "da", "e\u00b7hem\u00b7a\u00b7li\u00b7ge", "Of\u00b7fi\u00b7zie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "die schie\u00dfen w\u00f6chentlich, wie ich taxiere,", "tokens": ["die", "schie\u00b7\u00dfen", "w\u00f6\u00b7chent\u00b7lich", ",", "wie", "ich", "ta\u00b7xie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auf das Zivil.", "tokens": ["auf", "das", "Zi\u00b7vil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Es bietet kaum der feine Mann", "tokens": ["Es", "bie\u00b7tet", "kaum", "der", "fei\u00b7ne", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "zum Gru\u00dfe den Revolver an.", "tokens": ["zum", "Gru\u00b7\u00dfe", "den", "Re\u00b7vol\u00b7ver", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie kannten die internen Personalien,", "tokens": ["Sie", "kann\u00b7ten", "die", "in\u00b7ter\u00b7nen", "Per\u00b7so\u00b7na\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Geschichte und die Materialien \u2013", "tokens": ["Ge\u00b7schich\u00b7te", "und", "die", "Ma\u00b7te\u00b7ri\u00b7a\u00b7li\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie kennens gut.", "tokens": ["Sie", "ken\u00b7nens", "gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Kein Auslandsweg war Ihnen je beschwerlich.", "tokens": ["Kein", "Aus\u00b7lands\u00b7weg", "war", "Ih\u00b7nen", "je", "be\u00b7schwer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und deshalb waren Sie dem Pack gef\u00e4hrlich.", "tokens": ["Und", "des\u00b7halb", "wa\u00b7ren", "Sie", "dem", "Pack", "ge\u00b7f\u00e4hr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Daher die Wut.", "tokens": ["Da\u00b7her", "die", "Wut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wer schwarz-wei\u00df-rote Farben w\u00e4hlt,", "tokens": ["Wer", "schwa\u00b7rz\u00b7wei\u00df\u00b7ro\u00b7te", "Far\u00b7ben", "w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "zeigt, da\u00df ihm jeder Kunstsinn fehlt.", "tokens": ["zeigt", ",", "da\u00df", "ihm", "je\u00b7der", "Kunst\u00b7sinn", "fehlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Sie schlug ein Untertan, ein geistiger Kr\u00fcppel.", "tokens": ["Sie", "schlug", "ein", "Un\u00b7ter\u00b7tan", ",", "ein", "geis\u00b7ti\u00b7ger", "Kr\u00fcp\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der eine hat den Kopf \u2013 der andre hat den Kn\u00fcppel.", "tokens": ["Der", "ei\u00b7ne", "hat", "den", "Kopf", "\u2013", "der", "and\u00b7re", "hat", "den", "Kn\u00fcp\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "$(", "ART", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur sie hat nichts:", "tokens": ["Nur", "sie", "hat", "nichts", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Republik hat weder den noch jenen.", "tokens": ["Die", "Re\u00b7pub\u00b7lik", "hat", "we\u00b7der", "den", "noch", "je\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "ART", "ADV", "PDAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und freundlich lauscht sie diesen blutigen Szenen", "tokens": ["Und", "freund\u00b7lich", "lauscht", "sie", "die\u00b7sen", "blu\u00b7ti\u00b7gen", "Sze\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "verd\u00f6sten Angesichts.", "tokens": ["ver\u00b7d\u00f6s\u00b7ten", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft hier uns edles Blut vergie\u00dfen:", "tokens": ["La\u00dft", "hier", "uns", "ed\u00b7les", "Blut", "ver\u00b7gie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Himmel gibt es nichts zu schie\u00dfen \u2013!", "tokens": ["Im", "Him\u00b7mel", "gibt", "es", "nichts", "zu", "schie\u00b7\u00dfen", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur ungern nimmt der deutsche Mann", "tokens": ["Nur", "un\u00b7gern", "nimmt", "der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "statt Monarchismus Wahrheit an.", "tokens": ["statt", "Mon\u00b7ar\u00b7chis\u00b7mus", "Wahr\u00b7heit", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}