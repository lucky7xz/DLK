{"textgrid.poem.66220": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Bastillentr\u00fcmmer von der Kerkerth\u00fcre Voltaire's", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank dir, o Freund, aus voller Herzensf\u00fclle", "tokens": ["Dank", "dir", ",", "o", "Freund", ",", "aus", "vol\u00b7ler", "Her\u00b7zens\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr die Reliquie der greulichen Bastille,", "tokens": ["F\u00fcr", "die", "Re\u00b7li\u00b7qui\u00b7e", "der", "greu\u00b7li\u00b7chen", "Bas\u00b7til\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die freier B\u00fcrger starke Hand", "tokens": ["Die", "frei\u00b7er", "B\u00fcr\u00b7ger", "star\u00b7ke", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zermalmend warf in Schutt und Sand.", "tokens": ["Zer\u00b7mal\u00b7mend", "warf", "in", "Schutt", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zertr\u00fcmmert ist die Schauerklause,", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "ist", "die", "Schau\u00b7er\u00b7klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst, o ", "tokens": ["Die", "einst", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADV", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kein Holz, kein Stein, kein Nagel bleibe von dem Hause,", "tokens": ["Kein", "Holz", ",", "kein", "Stein", ",", "kein", "Na\u00b7gel", "blei\u00b7be", "von", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo oft der Unschuld Z\u00e4hre sich ergo\u00df!", "tokens": ["Wo", "oft", "der", "Un\u00b7schuld", "Z\u00e4h\u00b7re", "sich", "er\u00b7go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum, Biedermann, empfange meinen Segen", "tokens": ["Drum", ",", "Bie\u00b7der\u00b7mann", ",", "emp\u00b7fan\u00b7ge", "mei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr diese Tr\u00fcmmer, die du mir geschickt;", "tokens": ["F\u00fcr", "die\u00b7se", "Tr\u00fcm\u00b7mer", ",", "die", "du", "mir", "ge\u00b7schickt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie ist mir theurer als ein goldner Degen,", "tokens": ["Sie", "ist", "mir", "theu\u00b7rer", "als", "ein", "gold\u00b7ner", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Womit einst ein Tyrann die Freien unterdr\u00fcckt.", "tokens": ["Wo\u00b7mit", "einst", "ein", "Ty\u00b7rann", "die", "Frei\u00b7en", "un\u00b7ter\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}