{"textgrid.poem.40614": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du herrlichste aller, o wei\u00dfe Rose,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du herrlichste aller, o wei\u00dfe Rose,", "tokens": ["Du", "herr\u00b7lichs\u00b7te", "al\u00b7ler", ",", "o", "wei\u00b7\u00dfe", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du zarte und reine, du makellose,", "tokens": ["Du", "zar\u00b7te", "und", "rei\u00b7ne", ",", "du", "ma\u00b7kel\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "ADJA", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die thaugeschm\u00fcckt, im Schneegewand,", "tokens": ["Die", "thau\u00b7ge\u00b7schm\u00fcckt", ",", "im", "Schnee\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Morgenstrahl zum Bl\u00fchn erstand,", "tokens": ["Am", "Mor\u00b7gen\u00b7strahl", "zum", "Bl\u00fchn", "er\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bebst, weil ein Hauch dich schon entstellt,", "tokens": ["Du", "bebst", ",", "weil", "ein", "Hauch", "dich", "schon", "ent\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dir im Ber\u00fchren die Krone zerf\u00e4llt;", "tokens": ["Dir", "im", "Be\u00b7r\u00fch\u00b7ren", "die", "Kro\u00b7ne", "zer\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Es bl\u00fcht ja so sch\u00f6n, so hold, so rein", "tokens": ["Es", "bl\u00fcht", "ja", "so", "sch\u00f6n", ",", "so", "hold", ",", "so", "rein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nur eine, die ", "tokens": ["Nur", "ei\u00b7ne", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ART", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In solcher Stunde, die rasch entfloh,", "tokens": ["In", "sol\u00b7cher", "Stun\u00b7de", ",", "die", "rasch", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich d\u00e4ucht, sah ich dich schon irgendwo;", "tokens": ["Mich", "d\u00e4ucht", ",", "sah", "ich", "dich", "schon", "ir\u00b7gend\u00b7wo", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch damals umflo\u00df dein lieblich Haupt,", "tokens": ["Doch", "da\u00b7mals", "um\u00b7flo\u00df", "dein", "lieb\u00b7lich", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von gr\u00fcnen Myrthenreisern umlaubt,", "tokens": ["Von", "gr\u00fc\u00b7nen", "Myr\u00b7then\u00b7rei\u00b7sern", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Schleier von Spitzen aus Brabant;", "tokens": ["Ein", "Schlei\u00b7er", "von", "Spit\u00b7zen", "aus", "Bra\u00b7bant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das blendend wei\u00dfe Atlasgewand", "tokens": ["Das", "blen\u00b7dend", "wei\u00b7\u00dfe", "At\u00b7las\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Umschlang des Leibes magdlichen Bau,", "tokens": ["Um\u00b7schlang", "des", "Lei\u00b7bes", "magd\u00b7li\u00b7chen", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Auch sah ich etwas blinken wie Thau;", "tokens": ["Auch", "sah", "ich", "et\u00b7was", "blin\u00b7ken", "wie", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Du knietest vor einem schmucken Altar,", "tokens": ["Du", "knie\u00b7test", "vor", "ei\u00b7nem", "schmu\u00b7cken", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Den Segen sprach ein Mann im Talar,", "tokens": ["Den", "Se\u00b7gen", "sprach", "ein", "Mann", "im", "Ta\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.11": {"text": "Es flammte von Kerzen und goldenen Ringen", "tokens": ["Es", "flamm\u00b7te", "von", "Ker\u00b7zen", "und", "gol\u00b7de\u00b7nen", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Und \u00fcber dir f\u00e4chelten Seraphschwingen.", "tokens": ["Und", "\u00fc\u00b7ber", "dir", "f\u00e4\u00b7chel\u00b7ten", "Se\u00b7raph\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Die Stunde war's, die so heilig und hehr", "tokens": ["Die", "Stun\u00b7de", "wa\u00b7r's", ",", "die", "so", "hei\u00b7lig", "und", "hehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "ADJD", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Nur einmal kommt und dann nicht mehr,", "tokens": ["Nur", "ein\u00b7mal", "kommt", "und", "dann", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Uns Andern, wie dir, du makellose,", "tokens": ["Uns", "An\u00b7dern", ",", "wie", "dir", ",", "du", "ma\u00b7kel\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PWAV", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Drum herrlichste aller, o wei\u00dfe Rose.", "tokens": ["Drum", "herr\u00b7lichs\u00b7te", "al\u00b7ler", ",", "o", "wei\u00b7\u00dfe", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}