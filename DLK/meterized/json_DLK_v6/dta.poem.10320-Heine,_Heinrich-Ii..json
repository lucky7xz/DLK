{"dta.poem.10320": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Traum, gar seltsam schauerlich,", "tokens": ["Ein", "Traum", ",", "gar", "selt\u00b7sam", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erg\u00f6tzte und erschreckte mich.", "tokens": ["Er\u00b7g\u00f6tz\u00b7te", "und", "er\u00b7schreck\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch schwebt mir vor manch grausig Bild,", "tokens": ["Noch", "schwebt", "mir", "vor", "manch", "grau\u00b7sig", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem Herzen wogt's mir wild.", "tokens": ["Und", "in", "dem", "Her\u00b7zen", "wogt's", "mir", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da war ein Garten, wundersch\u00f6n,", "tokens": ["Da", "war", "ein", "Gar\u00b7ten", ",", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wollt' ich lustig mich ergehn;", "tokens": ["Da", "wollt'", "ich", "lus\u00b7tig", "mich", "er\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel sch\u00f6ne Blumen sahn mich an,", "tokens": ["Viel", "sch\u00f6\u00b7ne", "Blu\u00b7men", "sahn", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hatte meine Freude dran.", "tokens": ["Ich", "hat\u00b7te", "mei\u00b7ne", "Freu\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es zwitscherten die V\u00f6gelein", "tokens": ["Es", "zwit\u00b7scher\u00b7ten", "die", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Viel muntre Liebesmelodei'n;", "tokens": ["Viel", "mun\u00b7tre", "Lie\u00b7bes\u00b7me\u00b7lo\u00b7de\u00b7i'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Sonne war von Gold umstrahlt,", "tokens": ["Die", "Son\u00b7ne", "war", "von", "Gold", "um\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blumen lustig bunt bemalt.", "tokens": ["Die", "Blu\u00b7men", "lus\u00b7tig", "bunt", "be\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel Balsamduft aus Kr\u00e4utern rinnt,", "tokens": ["Viel", "Bal\u00b7sam\u00b7duft", "aus", "Kr\u00e4u\u00b7tern", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die L\u00fcfte wehen lieb und lind;", "tokens": ["Die", "L\u00fcf\u00b7te", "we\u00b7hen", "lieb", "und", "lind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Alles schimmert, Alles lacht,", "tokens": ["Und", "Al\u00b7les", "schim\u00b7mert", ",", "Al\u00b7les", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeigt mir freundlich seine Pracht.", "tokens": ["Und", "zeigt", "mir", "freund\u00b7lich", "sei\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Inmitten in dem Blumenland", "tokens": ["In\u00b7mit\u00b7ten", "in", "dem", "Blu\u00b7men\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein klarer Marmorbrunnen stand;", "tokens": ["Ein", "kla\u00b7rer", "Mar\u00b7mor\u00b7brun\u00b7nen", "stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schaut' ich eine sch\u00f6ne Maid,", "tokens": ["Da", "schaut'", "ich", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die emsig wusch ein wei\u00dfes Kleid.", "tokens": ["Die", "em\u00b7sig", "wusch", "ein", "wei\u00b7\u00dfes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die W\u00e4nglein s\u00fc\u00df, die Aeuglein mild,", "tokens": ["Die", "W\u00e4ng\u00b7lein", "s\u00fc\u00df", ",", "die", "A\u00b7e\u00b7u\u00b7glein", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein blondgelocktes Heil'genbild;", "tokens": ["Ein", "blond\u00b7ge\u00b7lock\u00b7tes", "Heil'\u00b7gen\u00b7bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich schau, die Maid ich fand", "tokens": ["Und", "wie", "ich", "schau", ",", "die", "Maid", "ich", "fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PTKVZ", "$,", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fremd und doch so wohl bekannt.", "tokens": ["So", "fremd", "und", "doch", "so", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die sch\u00f6ne Maid, die sputet sich,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Maid", ",", "die", "spu\u00b7tet", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie summt ein Lied gar wunderlich:", "tokens": ["Sie", "summt", "ein", "Lied", "gar", "wun\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201erinne, rinne, W\u00e4sserlein,", "tokens": ["\u201e", "rin\u00b7ne", ",", "rin\u00b7ne", ",", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ewasche, wasche Hemde rein.\u201c", "tokens": ["\u201e", "wa\u00b7sche", ",", "wa\u00b7sche", "Hem\u00b7de", "rein", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich ging und nahete mich ihr,", "tokens": ["Ich", "ging", "und", "na\u00b7he\u00b7te", "mich", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fl\u00fcsterte: O sage mir,", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7te", ":", "O", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Du wundersch\u00f6ne, s\u00fc\u00dfe Maid!", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", ",", "s\u00fc\u00b7\u00dfe", "Maid", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr wen ist dieses wei\u00dfe Kleid?", "tokens": ["F\u00fcr", "wen", "ist", "die\u00b7ses", "wei\u00b7\u00dfe", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da sprach sie schnell: Sey bald bereit,", "tokens": ["Da", "sprach", "sie", "schnell", ":", "Sey", "bald", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ich wasche dir dein Todtenkleid!", "tokens": ["Ich", "wa\u00b7sche", "dir", "dein", "Tod\u00b7ten\u00b7kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als sie die\u00df gesprochen kaum,", "tokens": ["Und", "als", "sie", "die\u00df", "ge\u00b7spro\u00b7chen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerflo\u00df das ganze Bild, wie Schaum. \u2014", "tokens": ["Zer\u00b7flo\u00df", "das", "gan\u00b7ze", "Bild", ",", "wie", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Schnell fortgezaubert stand ich bald", "tokens": ["Schnell", "fort\u00b7ge\u00b7zau\u00b7bert", "stand", "ich", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem d\u00fcstern, wilden Wald.", "tokens": ["In", "ei\u00b7nem", "d\u00fcs\u00b7tern", ",", "wil\u00b7den", "Wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die B\u00e4ume ragten himmelan;", "tokens": ["Die", "B\u00e4u\u00b7me", "rag\u00b7ten", "him\u00b7me\u00b7lan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich stand erstaunt und sann und sann.", "tokens": ["Ich", "stand", "er\u00b7staunt", "und", "sann", "und", "sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und horch! welch dumpfer Wiederhall!", "tokens": ["Und", "horch", "!", "welch", "dum\u00b7pfer", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ferner Aextenschl\u00e4ge Schall;", "tokens": ["Wie", "fer\u00b7ner", "A\u00b7ex\u00b7ten\u00b7schl\u00e4\u00b7ge", "Schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich eil' durch Busch und Wildni\u00df fort,", "tokens": ["Ich", "eil'", "durch", "Busch", "und", "Wild\u00b7ni\u00df", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und komm' an einen freien Ort.", "tokens": ["Und", "komm'", "an", "ei\u00b7nen", "frei\u00b7en", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Inmitten in dem gr\u00fcnen Raum,", "tokens": ["In\u00b7mit\u00b7ten", "in", "dem", "gr\u00fc\u00b7nen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand ein gro\u00dfer Eichenbaum;", "tokens": ["Da", "stand", "ein", "gro\u00b7\u00dfer", "Ei\u00b7chen\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieh! mein M\u00e4gdlein wundersam", "tokens": ["Und", "sieh", "!", "mein", "M\u00e4gd\u00b7lein", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Haut mit dem Beil den Eichenstamm.", "tokens": ["Haut", "mit", "dem", "Beil", "den", "Ei\u00b7chen\u00b7stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": 