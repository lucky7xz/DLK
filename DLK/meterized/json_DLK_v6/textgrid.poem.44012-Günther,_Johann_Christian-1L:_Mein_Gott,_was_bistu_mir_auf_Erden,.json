{"textgrid.poem.44012": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott, was bistu mir auf Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, was bistu mir auf Erden,", "tokens": ["Mein", "Gott", ",", "was", "bis\u00b7tu", "mir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was wirstu mir im Himmel seyn?", "tokens": ["Was", "wirs\u00b7tu", "mir", "im", "Him\u00b7mel", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Ruhm kan nicht ergr\u00fcndet werden,", "tokens": ["Dein", "Ruhm", "kan", "nicht", "er\u00b7gr\u00fcn\u00b7det", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn die Vernunft ist gar zu klein.", "tokens": ["Denn", "die", "Ver\u00b7nunft", "ist", "gar", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch h\u00f6re mich, o H\u00f6chster, an,", "tokens": ["Doch", "h\u00f6\u00b7re", "mich", ",", "o", "H\u00f6chs\u00b7ter", ",", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich von dir erzehlen kan:", "tokens": ["Was", "ich", "von", "dir", "er\u00b7zeh\u00b7len", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist, mein Fels, drum kan ich stehen,", "tokens": ["Du", "bist", ",", "mein", "Fels", ",", "drum", "kan", "ich", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "PAV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn gleich die Feinde wider mich", "tokens": ["Wenn", "gleich", "die", "Fein\u00b7de", "wi\u00b7der", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrer Macht zu Felde gehen,", "tokens": ["Mit", "ih\u00b7rer", "Macht", "zu", "Fel\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schafen nichts, ich tret auf dich;", "tokens": ["Sie", "scha\u00b7fen", "nichts", ",", "ich", "tret", "auf", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gesezt, da\u00df auch die Erde bricht,", "tokens": ["Ge\u00b7sezt", ",", "da\u00df", "auch", "die", "Er\u00b7de", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So weicht doch dieser Felsen nicht.", "tokens": ["So", "weicht", "doch", "die\u00b7ser", "Fel\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist die Burg, darein ich fliehe,", "tokens": ["Du", "bist", "die", "Burg", ",", "da\u00b7rein", "ich", "flie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald ein Ungel\u00fcck entsteht;", "tokens": ["So", "bald", "ein", "Un\u00b7ge\u00b7l\u00fcck", "ent\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Thor ist ofen sp\u00e4t und fr\u00fche,", "tokens": ["Das", "Thor", "ist", "o\u00b7fen", "sp\u00e4t", "und", "fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch man zum Erretter geht,", "tokens": ["Da\u00b7durch", "man", "zum", "Er\u00b7ret\u00b7ter", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00e4chst der Jammer noch so gro\u00df,", "tokens": ["Und", "w\u00e4chst", "der", "Jam\u00b7mer", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bricht er nicht in dieses Schlo\u00df.", "tokens": ["So", "bricht", "er", "nicht", "in", "die\u00b7ses", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist mein Hort, auf den ich traue,", "tokens": ["Du", "bist", "mein", "Hort", ",", "auf", "den", "ich", "trau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Grund, auf den ich in der Welt", "tokens": ["Der", "Grund", ",", "auf", "den", "ich", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die allerbeste Hofnung baue,", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7te", "Hof\u00b7nung", "bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil der Grundstein mich erh\u00e4lt;", "tokens": ["Die\u00b7weil", "der", "Grund\u00b7stein", "mich", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wer auf schwache Menschen sieht,", "tokens": ["Denn", "wer", "auf", "schwa\u00b7che", "Men\u00b7schen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der hat sich oft umsonst bem\u00fcht.", "tokens": ["Der", "hat", "sich", "oft", "um\u00b7sonst", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist mein Schild und Horn im Leben,", "tokens": ["Du", "bist", "mein", "Schild", "und", "Horn", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Daraus mein Heil geflo\u00dfen k\u00f6mmt,", "tokens": ["Da\u00b7raus", "mein", "Heil", "ge\u00b7flo\u00b7\u00dfen", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn alles hastu mir gegeben,", "tokens": ["Denn", "al\u00b7les", "has\u00b7tu", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles wird durch dich bestimmt;", "tokens": ["Und", "al\u00b7les", "wird", "durch", "dich", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Tropfen Blutes wird in mir,", "tokens": ["Kein", "Trop\u00b7fen", "Blu\u00b7tes", "wird", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es r\u00fchre denn, mein Gott, von dir.", "tokens": ["Es", "r\u00fch\u00b7re", "denn", ",", "mein", "Gott", ",", "von", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du wilst mein Schuz und Zuflucht bleiben", "tokens": ["Du", "wilst", "mein", "Schuz", "und", "Zu\u00b7flucht", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eher, als ich bitten kan,", "tokens": ["Und", "e\u00b7her", ",", "als", "ich", "bit\u00b7ten", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Feinde List zur\u00fccke treiben;", "tokens": ["Der", "Fein\u00b7de", "List", "zu\u00b7r\u00fc\u00b7cke", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast es schon vorhin gethan,", "tokens": ["Du", "hast", "es", "schon", "vor\u00b7hin", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst h\u00e4tte mich der Neider Macht", "tokens": ["Sonst", "h\u00e4t\u00b7te", "mich", "der", "Nei\u00b7der", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4ngst unter ihren Gift gebracht.", "tokens": ["L\u00e4ngst", "un\u00b7ter", "ih\u00b7ren", "Gift", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist der Heiland meiner Seelen,", "tokens": ["Du", "bist", "der", "Hei\u00b7land", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kanst durch deinen Creuzestod", "tokens": ["Du", "kanst", "durch", "dei\u00b7nen", "Creu\u00b7ze\u00b7stod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ganze S\u00fcndenschuld verheelen,", "tokens": ["Die", "gan\u00b7ze", "S\u00fcn\u00b7den\u00b7schuld", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du sch\u00fczst mich vor der H\u00f6llen Noth;", "tokens": ["Du", "sch\u00fcz\u00b7st", "mich", "vor", "der", "H\u00f6l\u00b7len", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo k\u00e4m mir sonst der Himmel her,", "tokens": ["Wo", "k\u00e4m", "mir", "sonst", "der", "Him\u00b7mel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Gott nicht auch mein Jesus w\u00e4r?", "tokens": ["Wenn", "Gott", "nicht", "auch", "mein", "Je\u00b7sus", "w\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "PPOSAT", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie darf ich dich noch weiter nennen?", "tokens": ["Wie", "darf", "ich", "dich", "noch", "wei\u00b7ter", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist mein Alles hier und dort,", "tokens": ["Du", "bist", "mein", "Al\u00b7les", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "PIS", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und soll ich dich noch kl\u00e4rer kennen,", "tokens": ["Und", "soll", "ich", "dich", "noch", "kl\u00e4\u00b7rer", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bringe mich nur an den Ort,", "tokens": ["So", "brin\u00b7ge", "mich", "nur", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da man dich siehet, wie du bist", "tokens": ["Da", "man", "dich", "sie\u00b7het", ",", "wie", "du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie dein Glanz so herrlich ist.", "tokens": ["Und", "wie", "dein", "Glanz", "so", "herr\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Inzwischen gieb, da\u00df ich bedencke,", "tokens": ["I\u00b7nzwi\u00b7schen", "gieb", ",", "da\u00df", "ich", "be\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich vor deine Lieb und Treu", "tokens": ["Was", "ich", "vor", "dei\u00b7ne", "Lieb", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Erden dir vor ein Geschencke", "tokens": ["Auf", "Er\u00b7den", "dir", "vor", "ein", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu liefern wieder schuldig sey;", "tokens": ["Zu", "lie\u00b7fern", "wie\u00b7der", "schul\u00b7dig", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du giebest mir dich selber hin,", "tokens": ["Du", "gie\u00b7best", "mir", "dich", "sel\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punc