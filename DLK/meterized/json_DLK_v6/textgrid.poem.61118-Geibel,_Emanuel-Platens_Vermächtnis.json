{"textgrid.poem.61118": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Platens Verm\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch schweift der kr\u00e4ft'ge Geist auf fernen Bahnen,", "tokens": ["Noch", "schweift", "der", "kr\u00e4ft'\u00b7ge", "Geist", "auf", "fer\u00b7nen", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rasch durch diese Adern pocht das Leben;", "tokens": ["Und", "rasch", "durch", "die\u00b7se", "A\u00b7dern", "pocht", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch Stimmen gibt's, geheime, deren Mahnen", "tokens": ["Doch", "Stim\u00b7men", "gibt's", ",", "ge\u00b7hei\u00b7me", ",", "de\u00b7ren", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "VVFIN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz umsonst sich m\u00fcht zu widerstreben,", "tokens": ["Das", "Herz", "um\u00b7sonst", "sich", "m\u00fcht", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und mir verk\u00fcndet solch ein dunkles Ahnen:", "tokens": ["Und", "mir", "ver\u00b7k\u00fcn\u00b7det", "solch", "ein", "dunk\u00b7les", "Ah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald mu\u00df ich diesen Staub dem Staube geben,", "tokens": ["Bald", "mu\u00df", "ich", "die\u00b7sen", "Staub", "dem", "Stau\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und den sie mir im Leben nicht gestatten,", "tokens": ["Und", "den", "sie", "mir", "im", "Le\u00b7ben", "nicht", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPER", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Lorbeer wird auf meinem Grabe schatten.", "tokens": ["Der", "Lor\u00b7beer", "wird", "auf", "mei\u00b7nem", "Gra\u00b7be", "schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sei's immer. Ich erf\u00fcllte meine Sendung,", "tokens": ["Sei's", "im\u00b7mer", ".", "Ich", "er\u00b7f\u00fcll\u00b7te", "mei\u00b7ne", "Sen\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein rastlos treuer Priester der Kam\u00f6nen;", "tokens": ["Ein", "rast\u00b7los", "treu\u00b7er", "Pries\u00b7ter", "der", "Ka\u00b7m\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich deutete mit jeder leisen Wendung", "tokens": ["Ich", "deu\u00b7te\u00b7te", "mit", "je\u00b7der", "lei\u00b7sen", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fackeltr\u00e4ger nach dem Reich des Sch\u00f6nen.", "tokens": ["Ein", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7ger", "nach", "dem", "Reich", "des", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umwallt vom K\u00f6nigsmantel der Vollendung", "tokens": ["Um\u00b7wallt", "vom", "K\u00f6\u00b7nigs\u00b7man\u00b7tel", "der", "Vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schritt mein Gesang dahin in Feiert\u00f6nen,", "tokens": ["Schritt", "mein", "Ge\u00b7sang", "da\u00b7hin", "in", "Fei\u00b7er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und was vordem den Griechen nur gelungen,", "tokens": ["Und", "was", "vor\u00b7dem", "den", "Grie\u00b7chen", "nur", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In deutscher Rede hab' ich's nachgesungen.", "tokens": ["In", "deut\u00b7scher", "Re\u00b7de", "hab'", "ich's", "nach\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar habt ihr selten meinen Ernst begriffen", "tokens": ["Zwar", "habt", "ihr", "sel\u00b7ten", "mei\u00b7nen", "Ernst", "be\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nie das Ziel bedacht, das ich erkoren;", "tokens": ["Und", "nie", "das", "Ziel", "be\u00b7dacht", ",", "das", "ich", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu meinem Spotte habt ihr grell gepfiffen,", "tokens": ["Zu", "mei\u00b7nem", "Spot\u00b7te", "habt", "ihr", "grell", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn seine Wahrheit kitzelt nicht die Ohren,", "tokens": ["Denn", "sei\u00b7ne", "Wahr\u00b7heit", "kit\u00b7zelt", "nicht", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie der Wogenschlag an Felsenriffen,", "tokens": ["Und", "wie", "der", "Wo\u00b7gen\u00b7schlag", "an", "Fel\u00b7sen\u00b7rif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ging selbst des Liedes Ma\u00df an euch verloren;", "tokens": ["Ging", "selbst", "des", "Lie\u00b7des", "Ma\u00df", "an", "euch", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch wie ihr mich verleugnet und mein Dichten,", "tokens": ["Doch", "wie", "ihr", "mich", "ver\u00b7leug\u00b7net", "und", "mein", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin getrost, die Nachwelt wird mich richten.", "tokens": ["Ich", "bin", "ge\u00b7trost", ",", "die", "Nach\u00b7welt", "wird", "mich", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist auch das Saatkorn noch nicht aufgegangen,", "tokens": ["Ist", "auch", "das", "Saat\u00b7korn", "noch", "nicht", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ich gestreut in unsrer Heimat Boden,", "tokens": ["Das", "ich", "ge\u00b7streut", "in", "uns\u00b7rer", "Hei\u00b7mat", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verzagt ihr auch, von Kleinmut noch befangen,", "tokens": ["Ver\u00b7zagt", "ihr", "auch", ",", "von", "Klein\u00b7mut", "noch", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Unkrauts tr\u00e4ge Wildnis auszuroden:", "tokens": ["Des", "Un\u00b7krauts", "tr\u00e4\u00b7ge", "Wild\u00b7nis", "aus\u00b7zu\u00b7ro\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erscheinen wird der Tag, wo mit Verlangen", "tokens": ["Er\u00b7schei\u00b7nen", "wird", "der", "Tag", ",", "wo", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Aschenkrug ihr suchet des Rhapsoden,", "tokens": ["Den", "A\u00b7schen\u00b7krug", "ihr", "su\u00b7chet", "des", "Rhap\u00b7so\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der ringend nach der Sch\u00f6nheit goldnen Fr\u00fcchten", "tokens": ["Der", "rin\u00b7gend", "nach", "der", "Sch\u00f6n\u00b7heit", "gold\u00b7nen", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor eurem Groll zum S\u00fcden mu\u00dfte fl\u00fcchten.", "tokens": ["Vor", "eu\u00b7rem", "Groll", "zum", "S\u00fc\u00b7den", "mu\u00df\u00b7te", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dann wird der deutsche Wald von Liedern schallen,", "tokens": ["Dann", "wird", "der", "deut\u00b7sche", "Wald", "von", "Lie\u00b7dern", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die pr\u00e4chtig wie auf Adlersfl\u00fcgeln rauschen,", "tokens": ["Die", "pr\u00e4ch\u00b7tig", "wie", "auf", "Ad\u00b7ler\u00b7sfl\u00fc\u00b7geln", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der heitre S\u00fcden wird zum Norden wallen,", "tokens": ["Der", "heit\u00b7re", "S\u00fc\u00b7den", "wird", "zum", "Nor\u00b7den", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Um seines Ernstes Sch\u00e4tze einzutauschen.", "tokens": ["Um", "sei\u00b7nes", "Erns\u00b7tes", "Sch\u00e4t\u00b7ze", "ein\u00b7zu\u00b7tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und heilig wird der S\u00e4nger sein vor allen,", "tokens": ["Und", "hei\u00b7lig", "wird", "der", "S\u00e4n\u00b7ger", "sein", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und fromme H\u00f6rer werden rings ihm lauschen.", "tokens": ["Und", "from\u00b7me", "H\u00f6\u00b7rer", "wer\u00b7den", "rings", "ihm", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was soll ich drum den fr\u00fchen Tod beweinen? -", "tokens": ["Was", "soll", "ich", "drum", "den", "fr\u00fc\u00b7hen", "Tod", "be\u00b7wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "PPER", "PAV", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Dichter lebt, solang die Sterne scheinen.", "tokens": ["Der", "Dich\u00b7ter", "lebt", ",", "so\u00b7lang", "die", "Ster\u00b7ne", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}