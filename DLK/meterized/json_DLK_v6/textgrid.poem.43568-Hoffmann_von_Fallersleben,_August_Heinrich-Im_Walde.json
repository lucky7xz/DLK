{"textgrid.poem.43568": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde wie sch\u00f6n", "tokens": ["Im", "Wal\u00b7de", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Um die Maienzeit,", "tokens": ["Um", "die", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn der Kuckuck schreit", "tokens": ["Wenn", "der", "Ku\u00b7ckuck", "schreit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Im Walde, im Walde!", "tokens": ["Im", "Wal\u00b7de", ",", "im", "Wal\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es spielen des Fr\u00fchlings gaukelnde Weste", "tokens": ["Es", "spie\u00b7len", "des", "Fr\u00fch\u00b7lings", "gau\u00b7keln\u00b7de", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im Dufte der gr\u00fcnbelaubten Aeste;", "tokens": ["Im", "Duf\u00b7te", "der", "gr\u00fcn\u00b7be\u00b7laub\u00b7ten", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Sonnenstrahlen im Laube flittern,", "tokens": ["Die", "Son\u00b7nen\u00b7strah\u00b7len", "im", "Lau\u00b7be", "flit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Bl\u00e4tter im goldigen Glanze zittern;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "im", "gol\u00b7di\u00b7gen", "Glan\u00b7ze", "zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und die V\u00f6gel lustig die Wipfel umfliegen", "tokens": ["Und", "die", "V\u00f6\u00b7gel", "lus\u00b7tig", "die", "Wip\u00b7fel", "um\u00b7flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "ART", "NN", "VVINF"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und singend sich auf den Zweigen wiegen.", "tokens": ["Und", "sin\u00b7gend", "sich", "auf", "den", "Zwei\u00b7gen", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Im Walde wie sch\u00f6n,", "tokens": ["Im", "Wal\u00b7de", "wie", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Wie sch\u00f6n im Walde, im Walde!", "tokens": ["Wie", "sch\u00f6n", "im", "Wal\u00b7de", ",", "im", "Wal\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}