{"textgrid.poem.39437": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Genius des gro\u00dfen Britten", "tokens": ["Dem", "Ge\u00b7nius", "des", "gro\u00b7\u00dfen", "Brit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ich begeistert nachgeschritten,", "tokens": ["War", "ich", "be\u00b7geis\u00b7tert", "nach\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da lockt' ich auf die deutsche Flur", "tokens": ["Da", "lockt'", "ich", "auf", "die", "deut\u00b7sche", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Echo seiner Worte nur.", "tokens": ["Ein", "E\u00b7cho", "sei\u00b7ner", "Wor\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast den Worten Seel' und Leben,", "tokens": ["Du", "hast", "den", "Wor\u00b7ten", "Seel'", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seel' ein sichtbar Bild gegeben:", "tokens": ["Der", "Seel'", "ein", "sicht\u00b7bar", "Bild", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Dichters zarte Julia", "tokens": ["Des", "Dich\u00b7ters", "zar\u00b7te", "Ju\u00b7lia"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht hingezaubert vor uns da.", "tokens": ["Steht", "hin\u00b7ge\u00b7zau\u00b7bert", "vor", "uns", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sittsame W\u00fcrd' und edle Sitte", "tokens": ["Sitt\u00b7sa\u00b7me", "W\u00fcrd'", "und", "ed\u00b7le", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Begleiten dich bei jedem Tritte,", "tokens": ["Be\u00b7glei\u00b7ten", "dich", "bei", "je\u00b7dem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fordern stille Huldigung", "tokens": ["Und", "for\u00b7dern", "stil\u00b7le", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.4": {"text": "Bei feuriger Begeisterung.", "tokens": ["Bei", "feu\u00b7ri\u00b7ger", "Be\u00b7geis\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ist dir lieber? \u2013 Beifallswellen,", "tokens": ["Was", "ist", "dir", "lie\u00b7ber", "?", "\u2013", "Bei\u00b7falls\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die rauschend an die B\u00fchne schwellen?", "tokens": ["Die", "rau\u00b7schend", "an", "die", "B\u00fch\u00b7ne", "schwel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie? oder was die Brust nur hegt,", "tokens": ["Wie", "?", "o\u00b7der", "was", "die", "Brust", "nur", "hegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unverge\u00dflich in sich tr\u00e4gt?", "tokens": ["Und", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "in", "sich", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}