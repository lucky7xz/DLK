{"textgrid.poem.63334": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Theater", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir heben unsre Beine wie an Schn\u00fcren,", "tokens": ["Wir", "he\u00b7ben", "uns\u00b7re", "Bei\u00b7ne", "wie", "an", "Schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unsre Herzen sind Papiermach\u00e9.", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "sind", "Pa\u00b7pier\u00b7mach\u00e9", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Woran wir auch mit unsren Worten r\u00fchren:", "tokens": ["Wo\u00b7ran", "wir", "auch", "mit", "un\u00b7sren", "Wor\u00b7ten", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei's Lust, sei's Weh:", "tokens": ["Sei's", "Lust", ",", "sei's", "Weh", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gott wird uns schon das richtige Wort soufflieren.", "tokens": ["Gott", "wird", "uns", "schon", "das", "rich\u00b7ti\u00b7ge", "Wort", "souf\u00b7flie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Pa\u00df nur auf deinen Stich \u2013", "tokens": ["Pa\u00df", "nur", "auf", "dei\u00b7nen", "Stich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn im Parkett, da sitzt der Teufel,", "tokens": ["Denn", "im", "Par\u00b7kett", ",", "da", "sitzt", "der", "Teu\u00b7fel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und ohne Zweifel,", "tokens": ["Und", "oh\u00b7ne", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Er am\u00fcsiert sich k\u00f6niglich...", "tokens": ["Er", "a\u00b7m\u00fc\u00b7siert", "sich", "k\u00f6\u00b7nig\u00b7lich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}