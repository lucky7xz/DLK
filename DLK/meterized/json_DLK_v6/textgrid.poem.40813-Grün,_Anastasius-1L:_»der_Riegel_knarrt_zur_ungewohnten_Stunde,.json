{"textgrid.poem.40813": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder Riegel knarrt zur ungewohnten Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Riegel knarrt zur ungewohnten Stunde,", "tokens": ["\u00bb", "der", "Rie\u00b7gel", "knarrt", "zur", "un\u00b7ge\u00b7wohn\u00b7ten", "Stun\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mann tritt ein im Kleid von schwarzer Farbe,", "tokens": ["Ein", "Mann", "tritt", "ein", "im", "Kleid", "von", "schwar\u00b7zer", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verschnitten ist sein Haar zur Glatzenrunde,", "tokens": ["Ver\u00b7schnit\u00b7ten", "ist", "sein", "Haar", "zur", "Glat\u00b7zen\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Mund fast lippenlos wie eine Narbe.", "tokens": ["Sein", "Mund", "fast", "lip\u00b7pen\u00b7los", "wie", "ei\u00b7ne", "Nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Kr\u00fcppelast des Edelpalmenbaumes,", "tokens": ["Ein", "Kr\u00fcp\u00b7pe\u00b7last", "des", "E\u00b7del\u00b7pal\u00b7men\u00b7bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mannheit genannt! Nicht tr\u00e4nkt und n\u00e4hrt begeisternd", "tokens": ["Mann\u00b7heit", "ge\u00b7nannt", "!", "Nicht", "tr\u00e4nkt", "und", "n\u00e4hrt", "be\u00b7geis\u00b7ternd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "PTKNEG", "VVFIN", "KON", "VVFIN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sein Wort als s\u00fc\u00dfe Frucht so sch\u00f6nen Baumes,", "tokens": ["Sein", "Wort", "als", "s\u00fc\u00b7\u00dfe", "Frucht", "so", "sch\u00f6\u00b7nen", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als unrein Harz nur trieft's andringlich, kleisternd!", "tokens": ["Als", "un\u00b7rein", "Harz", "nur", "trieft's", "and\u00b7ring\u00b7lich", ",", "kleis\u00b7ternd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er spricht von B\u00fc\u00dfen und Bereu'n, Bekehren,", "tokens": ["Er", "spricht", "von", "B\u00fc\u00b7\u00dfen", "und", "Be\u00b7reu'n", ",", "Be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Demut, die sich h\u00f6h'rer Weisheit schmiege,", "tokens": ["Von", "De\u00b7mut", ",", "die", "sich", "h\u00f6h'\u00b7rer", "Weis\u00b7heit", "schmie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von R\u00fcckkehr zu der Gl\u00e4ub'gen frommen Heeren,", "tokens": ["Von", "R\u00fcck\u00b7kehr", "zu", "der", "Gl\u00e4ub'\u00b7gen", "from\u00b7men", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Todesgraun, das einst auch Starke biege.", "tokens": ["Von", "To\u00b7des\u00b7graun", ",", "das", "einst", "auch", "Star\u00b7ke", "bie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O lieber Mann, wollt ihr ein Vogler werden,", "tokens": ["O", "lie\u00b7ber", "Mann", ",", "wollt", "ihr", "ein", "Vog\u00b7ler", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fc\u00dft ihr aufstreuen bess're Futterbrocken;", "tokens": ["M\u00fc\u00dft", "ihr", "auf\u00b7streu\u00b7en", "bess'\u00b7re", "Fut\u00b7ter\u00b7bro\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVIZU", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wollt ihr als schlauer Werber euch geberden,", "tokens": ["Wollt", "ihr", "als", "schlau\u00b7er", "Wer\u00b7ber", "euch", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df Uniform und Handgeld reicher locken!", "tokens": ["Mu\u00df", "U\u00b7ni\u00b7form", "und", "Hand\u00b7geld", "rei\u00b7cher", "lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es legt ein Mann dem alten satten Leuen,", "tokens": ["Es", "legt", "ein", "Mann", "dem", "al\u00b7ten", "sat\u00b7ten", "Leu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den mehr als er der feuchte Norden z\u00e4hmte,", "tokens": ["Den", "mehr", "als", "er", "der", "feuch\u00b7te", "Nor\u00b7den", "z\u00e4hm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Haupt zum Schlund, drin keine Z\u00e4hne dr\u00e4uen;", "tokens": ["Sein", "Haupt", "zum", "Schlund", ",", "drin", "kei\u00b7ne", "Z\u00e4h\u00b7ne", "dr\u00e4u\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob er des P\u00f6beljubels sich nicht sch\u00e4mte?", "tokens": ["Ob", "er", "des", "P\u00f6\u00b7bel\u00b7ju\u00b7bels", "sich", "nicht", "sch\u00e4m\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein Gaukler ist's, inde\u00df ein Held mir hei\u00dfet", "tokens": ["Ein", "Gauk\u00b7ler", "ist's", ",", "in\u00b7de\u00df", "ein", "Held", "mir", "hei\u00b7\u00dfet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Neger, der im W\u00fcstensand ihn meistert,", "tokens": ["Der", "Ne\u00b7ger", ",", "der", "im", "W\u00fcs\u00b7ten\u00b7sand", "ihn", "meis\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Lamm dem Rachen jenes Leu'n entrei\u00dfet,", "tokens": ["Das", "Lamm", "dem", "Ra\u00b7chen", "je\u00b7nes", "Leu'n", "ent\u00b7rei\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Hunger stachelt, Sonnenbrand begeistert!", "tokens": ["Den", "Hun\u00b7ger", "sta\u00b7chelt", ",", "Son\u00b7nen\u00b7brand", "be\u00b7geis\u00b7tert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nur leichten Gauklerruhm, nicht Heldensiege", "tokens": ["Nur", "leich\u00b7ten", "Gauk\u00b7ler\u00b7ruhm", ",", "nicht", "Hel\u00b7den\u00b7sie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PTKNEG", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wird euer Priestereifer sich erjagen,", "tokens": ["Wird", "eu\u00b7er", "Pries\u00b7te\u00b7rei\u00b7fer", "sich", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nimmt als Genossen er im Glaubenskriege", "tokens": ["Nimmt", "als", "Ge\u00b7nos\u00b7sen", "er", "im", "Glau\u00b7bens\u00b7krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "NN", "PPER", "APPRART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Mein Elend, meine Ketten, Todeszagen.", "tokens": ["Mein", "E\u00b7lend", ",", "mei\u00b7ne", "Ket\u00b7ten", ",", "To\u00b7des\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Sterbender ist gar ein Sanfter, Milder,", "tokens": ["Ein", "Ster\u00b7ben\u00b7der", "ist", "gar", "ein", "Sanf\u00b7ter", ",", "Mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df viel, wird euch sich auch gefallen lassen", "tokens": ["Mu\u00df", "viel", ",", "wird", "euch", "sich", "auch", "ge\u00b7fal\u00b7len", "las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gleichen Sinns Sterbkerze, Heil'genbilder,", "tokens": ["Und", "glei\u00b7chen", "Sinns", "Sterb\u00b7ker\u00b7ze", ",", "Heil'\u00b7gen\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Kuhschwanz auch nach Inderweise fassen.", "tokens": ["Den", "Kuh\u00b7schwanz", "auch", "nach", "In\u00b7der\u00b7wei\u00b7se", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er kann euch nicht von seinem Bette scheuchen:", "tokens": ["Er", "kann", "euch", "nicht", "von", "sei\u00b7nem", "Bet\u00b7te", "scheu\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt' er die H\u00e4nde regen, wollt' er lieber", "tokens": ["K\u00f6nnt'", "er", "die", "H\u00e4n\u00b7de", "re\u00b7gen", ",", "wollt'", "er", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Weib, den Kindern sie zum Abschied reichen;", "tokens": ["Dem", "Weib", ",", "den", "Kin\u00b7dern", "sie", "zum", "Ab\u00b7schied", "rei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht ihr bekehrt, besiegt ihn, nein, das Fieber.", "tokens": ["Nicht", "ihr", "be\u00b7kehrt", ",", "be\u00b7siegt", "ihn", ",", "nein", ",", "das", "Fie\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PTKANT", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Mich wird das heil'ge Brod von wei\u00dfem Waizen", "tokens": ["Mich", "wird", "das", "heil'\u00b7ge", "Brod", "von", "wei\u00b7\u00dfem", "Wai\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach schwarzer Kerkerkrumme nicht anwidern;", "tokens": ["Nach", "schwar\u00b7zer", "Ker\u00b7ker\u00b7krum\u00b7me", "nicht", "an\u00b7wi\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch m\u00f6gt ihr mit dem heil'gen Oel nicht geizen,", "tokens": ["Auch", "m\u00f6gt", "ihr", "mit", "dem", "heil'\u00b7gen", "O\u00b7el", "nicht", "gei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heilbalsam ist's den kettenwunden Gliedern.", "tokens": ["Heil\u00b7bal\u00b7sam", "ist's", "den", "ket\u00b7ten\u00b7wun\u00b7den", "Glie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Mit dem gesunden, geistesfrischen S\u00fcnder", "tokens": ["Mit", "dem", "ge\u00b7sun\u00b7den", ",", "geis\u00b7tes\u00b7fri\u00b7schen", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klimmt auf den Berg, da\u00df weit ins Land er sehe,", "tokens": ["Klimmt", "auf", "den", "Berg", ",", "da\u00df", "weit", "ins", "Land", "er", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort werdet ihm des heil'gen Worts Verk\u00fcnder,", "tokens": ["Dort", "wer\u00b7det", "ihm", "des", "heil'\u00b7gen", "Worts", "Ver\u00b7k\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Gottes Rede scheut nicht Gottes N\u00e4he.", "tokens": ["Denn", "Got\u00b7tes", "Re\u00b7de", "scheut", "nicht", "Got\u00b7tes", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Steht Mann dem Mann und Wort dem Wort entgegen,", "tokens": ["Steht", "Mann", "dem", "Mann", "und", "Wort", "dem", "Wort", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Licht und Waffen gleich f\u00fcr beide Streiter!", "