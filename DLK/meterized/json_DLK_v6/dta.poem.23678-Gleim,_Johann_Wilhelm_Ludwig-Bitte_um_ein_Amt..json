{"dta.poem.23678": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bitte um ein Amt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer darf nicht scherzen?", "tokens": ["Wer", "darf", "nicht", "scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Urteilssprecher.", "tokens": ["Ein", "Ur\u00b7teils\u00b7spre\u00b7cher", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wer darf nicht lachen?", "tokens": ["Wer", "darf", "nicht", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein frommer Pfarrer.", "tokens": ["Ein", "from\u00b7mer", "Pfar\u00b7rer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wer darf nicht froh seyn?", "tokens": ["Wer", "darf", "nicht", "froh", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ein armer Kaiser.", "tokens": ["Ein", "ar\u00b7mer", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Drum mach, o Himmel,", "tokens": ["Drum", "mach", ",", "o", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mich nicht zum Richter,", "tokens": ["Mich", "nicht", "zum", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Auch nicht zum Pfarrer,", "tokens": ["Auch", "nicht", "zum", "Pfar\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Auch nicht zum Kaiser.", "tokens": ["Auch", "nicht", "zum", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Zum Probst im Kloster,", "tokens": ["Zum", "Probst", "im", "Klos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Kannst du mich machen.", "tokens": ["Kannst", "du", "mich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}