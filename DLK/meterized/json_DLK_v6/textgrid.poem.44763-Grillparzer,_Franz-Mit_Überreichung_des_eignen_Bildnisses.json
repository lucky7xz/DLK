{"textgrid.poem.44763": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Mit \u00dcberreichung des eignen Bildnisses", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer viel verschenken will, ob F\u00fcrst und K\u00f6nig,", "tokens": ["Wer", "viel", "ver\u00b7schen\u00b7ken", "will", ",", "ob", "F\u00fcrst", "und", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr als sich selbst gab keiner noch, der war.", "tokens": ["Mehr", "als", "sich", "selbst", "gab", "kei\u00b7ner", "noch", ",", "der", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PRF", "ADV", "VVFIN", "PIS", "ADV", "$,", "PRELS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier nimm mich selbst, und selber bring ichs dar,", "tokens": ["Hier", "nimm", "mich", "selbst", ",", "und", "sel\u00b7ber", "bring", "ichs", "dar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "KON", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Herz entscheide nun, obs viel ist oder wenig.", "tokens": ["Dein", "Herz", "ent\u00b7schei\u00b7de", "nun", ",", "obs", "viel", "ist", "o\u00b7der", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VAFIN", "KON", "PIS", "$."], "meter": "-+-+-+