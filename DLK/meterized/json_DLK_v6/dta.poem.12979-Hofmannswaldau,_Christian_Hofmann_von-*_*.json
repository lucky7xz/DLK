{"dta.poem.12979": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Omensch! du dummes thier, du gauckelspiel des gl\u00fcckes,", "tokens": ["O\u00b7mensch", "!", "du", "dum\u00b7mes", "thier", ",", "du", "gau\u00b7ckel\u00b7spiel", "des", "gl\u00fc\u00b7ckes", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der gesetze knecht, so geitz und stoltz erdacht!", "tokens": ["Und", "der", "ge\u00b7set\u00b7ze", "knecht", ",", "so", "geitz", "und", "stoltz", "er\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "We\u00dfwegen zitterst du doch vor des todes macht,", "tokens": ["We\u00df\u00b7we\u00b7gen", "zit\u00b7terst", "du", "doch", "vor", "des", "to\u00b7des", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00fcrdigest das grab nicht eines steiffen blickes?", "tokens": ["Und", "w\u00fcr\u00b7di\u00b7gest", "das", "grab", "nicht", "ei\u00b7nes", "steif\u00b7fen", "bli\u00b7ckes", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du, dem das leben doch durchaus beschwerlich f\u00e4llt,", "tokens": ["Du", ",", "dem", "das", "le\u00b7ben", "doch", "durc\u00b7haus", "be\u00b7schwer\u00b7lich", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PDS", "VVFIN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Verla\u00df den feigen sinn, der deinen witz verblendet;", "tokens": ["Ver\u00b7la\u00df", "den", "fei\u00b7gen", "sinn", ",", "der", "dei\u00b7nen", "witz", "ver\u00b7blen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wi\u00df\u2019: es ist der streich, der unser leben endet,", "tokens": ["Und", "wi\u00df'", ":", "es", "ist", "der", "streich", ",", "der", "un\u00b7ser", "le\u00b7ben", "en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJD", "$,", "PRELS", "PPOSAT", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hart er immer scheint, der letzt in dieser welt.", "tokens": ["So", "hart", "er", "im\u00b7mer", "scheint", ",", "der", "letzt", "in", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}