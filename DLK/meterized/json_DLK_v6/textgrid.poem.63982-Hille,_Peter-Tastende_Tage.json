{"textgrid.poem.63982": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Tastende Tage", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die \u00c4ste in Flammen, die Wipfel entlaubt", "tokens": ["Die", "\u00c4s\u00b7te", "in", "Flam\u00b7men", ",", "die", "Wip\u00b7fel", "ent\u00b7laubt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Kreuze das friedenumspr\u00fchete Haupt.", "tokens": ["Am", "Kreu\u00b7ze", "das", "frie\u00b7den\u00b7um\u00b7spr\u00fc\u00b7he\u00b7te", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Sehnen und Dehnen, wie M\u00e4dchen es haben,", "tokens": ["Ein", "Seh\u00b7nen", "und", "Deh\u00b7nen", ",", "wie", "M\u00e4d\u00b7chen", "es", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PWAV", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Renettenrot in die L\u00fcfte gegraben.", "tokens": ["Re\u00b7net\u00b7ten\u00b7rot", "in", "die", "L\u00fcf\u00b7te", "ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein streckendes Zittern, ein schwellendes Gl\u00fchen,", "tokens": ["Ein", "stre\u00b7cken\u00b7des", "Zit\u00b7tern", ",", "ein", "schwel\u00b7len\u00b7des", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des scheinenden Baumes Adern erbl\u00fchen.", "tokens": ["Des", "schei\u00b7nen\u00b7den", "Bau\u00b7mes", "A\u00b7dern", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "In gereiztem Scheine Feier-Weh,", "tokens": ["In", "ge\u00b7reiz\u00b7tem", "Schei\u00b7ne", "Fei\u00b7er\u00b7Weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Flammt Ziegelglut auf Erdenschnee.", "tokens": ["Flammt", "Zie\u00b7gel\u00b7glut", "auf", "Er\u00b7den\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die versteinerte Glut, ein Liebesgedicht,", "tokens": ["Die", "ver\u00b7stei\u00b7ner\u00b7te", "Glut", ",", "ein", "Lie\u00b7bes\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "F\u00e4llt rosig warm auf der K\u00e4lte Gesicht.", "tokens": ["F\u00e4llt", "ro\u00b7sig", "warm", "auf", "der", "K\u00e4l\u00b7te", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Einsamkeit der Einsamkeiten,", "tokens": ["Ein\u00b7sam\u00b7keit", "der", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welt und ich: wir beide schreiten.", "tokens": ["Welt", "und", "ich", ":", "wir", "bei\u00b7de", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "$.", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Haltende H\u00e4nde leise schweben", "tokens": ["Hal\u00b7ten\u00b7de", "H\u00e4n\u00b7de", "lei\u00b7se", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu der Sonne goldenem Geben.", "tokens": ["Zu", "der", "Son\u00b7ne", "gol\u00b7de\u00b7nem", "Ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Im schmelzenden Schnee was heimlich geht,", "tokens": ["Im", "schmel\u00b7zen\u00b7den", "Schnee", "was", "heim\u00b7lich", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ob schon der Fr\u00fchling im Felde steht?", "tokens": ["Ob", "schon", "der", "Fr\u00fch\u00b7ling", "im", "Fel\u00b7de", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Apostelh\u00e4upter im Abendscheine:", "tokens": ["A\u00b7pos\u00b7tel\u00b7h\u00e4up\u00b7ter", "im", "A\u00b7bend\u00b7schei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Kartenspieler tr\u00fcbe Gemeinde.", "tokens": ["Der", "Kar\u00b7ten\u00b7spie\u00b7ler", "tr\u00fc\u00b7be", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Die \u00c4ste entflammen, die Wipfel entlaubt", "tokens": ["Die", "\u00c4s\u00b7te", "ent\u00b7flam\u00b7men", ",", "die", "Wip\u00b7fel", "ent\u00b7laubt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Kreuze das friedenumspr\u00fchete Haupt.", "tokens": ["Am", "Kreu\u00b7ze", "das", "frie\u00b7den\u00b7um\u00b7spr\u00fc\u00b7he\u00b7te", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}