{"dta.poem.6008": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mars, Jupiter, Saturn, sind auch \u00fcm sie gedreht;", "tokens": ["Mars", ",", "Ju\u00b7pi\u00b7ter", ",", "Sa\u00b7turn", ",", "sind", "auch", "\u00fcm", "sie", "ge\u00b7dreht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ist der Mittel-Punct. Der andern ihre Reise", "tokens": ["Sie", "ist", "der", "Mit\u00b7tel\u00b7Punct", ".", "Der", "an\u00b7dern", "ih\u00b7re", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ART", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht um der Sonnen Gluht auf eine gleiche Weise.", "tokens": ["Geht", "um", "der", "Son\u00b7nen", "Gluht", "auf", "ei\u00b7ne", "glei\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}