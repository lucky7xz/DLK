{"textgrid.poem.48834": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "59. An seine Schmerzen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr mir, wie ihr dreut, auch t\u00e4tet meinen Tod", "tokens": ["Wenn", "ihr", "mir", ",", "wie", "ihr", "dreut", ",", "auch", "t\u00e4\u00b7tet", "mei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und hieltet einmal mir, was ihr mir stets versprechst,", "tokens": ["und", "hiel\u00b7tet", "ein\u00b7mal", "mir", ",", "was", "ihr", "mir", "stets", "ver\u00b7sprechst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so w\u00e4r' es Alles gut. Je mehr ihr aber brechet,", "tokens": ["so", "w\u00e4r'", "es", "Al\u00b7les", "gut", ".", "Je", "mehr", "ihr", "a\u00b7ber", "bre\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$.", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was ihr mir stets sagt zu, je mehr hats mit mir Not.", "tokens": ["was", "ihr", "mir", "stets", "sagt", "zu", ",", "je", "mehr", "hats", "mit", "mir", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Auweh, was bin ich doch, als mein selbsteigner Spott?", "tokens": ["Au\u00b7weh", ",", "was", "bin", "ich", "doch", ",", "als", "mein", "selbs\u00b7teig\u00b7ner", "Spott", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr st\u00e4rkt euch stets an mir, da\u00df ihr mich stets mehr schw\u00e4chet.", "tokens": ["Ihr", "st\u00e4rkt", "euch", "stets", "an", "mir", ",", "da\u00df", "ihr", "mich", "stets", "mehr", "schw\u00e4\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Waffen geb' ich selbst, darmit ihr auf mich stechet,", "tokens": ["Die", "Waf\u00b7fen", "geb'", "ich", "selbst", ",", "dar\u00b7mit", "ihr", "auf", "mich", "ste\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und da\u00df ich werde bla\u00df, so machet ihr mich rot.", "tokens": ["und", "da\u00df", "ich", "wer\u00b7de", "bla\u00df", ",", "so", "ma\u00b7chet", "ihr", "mich", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O meine Peiniger, wie soll ich euch vers\u00f6nen?", "tokens": ["O", "mei\u00b7ne", "Pei\u00b7ni\u00b7ger", ",", "wie", "soll", "ich", "euch", "ver\u00b7s\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df, ihr habt Befehl von meiner ", "tokens": ["Ich", "wei\u00df", ",", "ihr", "habt", "Be\u00b7fehl", "von", "mei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df ihr mich sperret ein in dieses harte Joch.", "tokens": ["da\u00df", "ihr", "mich", "sper\u00b7ret", "ein", "in", "die\u00b7ses", "har\u00b7te", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ART", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, da\u00df sie w\u00fc\u00dfte nur die kleinste meiner N\u00f6ten,", "tokens": ["Ach", ",", "da\u00df", "sie", "w\u00fc\u00df\u00b7te", "nur", "die", "kleins\u00b7te", "mei\u00b7ner", "N\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so w\u00fcrde sie mich doch auf einmal lassen t\u00f6ten.", "tokens": ["so", "w\u00fcr\u00b7de", "sie", "mich", "doch", "auf", "ein\u00b7mal", "las\u00b7sen", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "APPR", "ADV", "VVINF", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun sterb' ich immerhin und sterbe nimmer doch.", "tokens": ["Nun", "sterb'", "ich", "im\u00b7mer\u00b7hin", "und", "ster\u00b7be", "nim\u00b7mer", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}