{"dta.poem.11485": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das kupffer-bild H. R. T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dafern pedanterie, geschm\u00fcnckter tugend schein,", "tokens": ["Da\u00b7fern", "pe\u00b7dan\u00b7te\u00b7rie", ",", "ge\u00b7schm\u00fcnck\u00b7ter", "tu\u00b7gend", "schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und irrthum, in der welt drey ungeheuer seyn;", "tokens": ["Und", "irr\u00b7thum", ",", "in", "der", "welt", "drey", "un\u00b7ge\u00b7heu\u00b7er", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ART", "NN", "CARD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schau den Hercules, der seine sieges-fahn", "tokens": ["So", "schau", "den", "Her\u00b7cu\u00b7les", ",", "der", "sei\u00b7ne", "sie\u00b7ges\u00b7fahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf ihre k\u00f6pffe pflantzt, in diesem kupffer an.", "tokens": ["Auf", "ih\u00b7re", "k\u00f6pf\u00b7fe", "pflantzt", ",", "in", "die\u00b7sem", "kupf\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}