{"textgrid.poem.48586": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Herrn Jacob Sperlingen und Jungfrauen Anna Schelhammer in Hamburg", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, ich bin dir f\u00fcr gewi\u00df", "tokens": ["Freund", ",", "ich", "bin", "dir", "f\u00fcr", "ge\u00b7wi\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gut zu tun weit mehr als di\u00df,", "tokens": ["gut", "zu", "tun", "weit", "mehr", "als", "di\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "ADJD", "PIAT", "KOKOM", "PDS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich deines Hymens Feier,", "tokens": ["da\u00df", "ich", "dei\u00b7nes", "Hy\u00b7mens", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der dir heut' ein Fest bestellt,", "tokens": ["der", "dir", "heut'", "ein", "Fest", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das dem Himmel wol gef\u00e4llt,", "tokens": ["das", "dem", "Him\u00b7mel", "wol", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "zieren soll mit meiner Leier.", "tokens": ["zie\u00b7ren", "soll", "mit", "mei\u00b7ner", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dreimal hat dir, Wassermann,", "tokens": ["Drei\u00b7mal", "hat", "dir", ",", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zynthius Bescheid getan,", "tokens": ["Zynt\u00b7hi\u00b7us", "Be\u00b7scheid", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "dreimal will es Winter werden,", "tokens": ["drei\u00b7mal", "will", "es", "Win\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seit ich dein ", "tokens": ["seit", "ich", "dein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "und mit dir fing an zu gehn", "tokens": ["und", "mit", "dir", "fing", "an", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gegen ", "tokens": ["ge\u00b7gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Damals stund auch dir dein Sin", "tokens": ["Da\u00b7mals", "stund", "auch", "dir", "dein", "Sin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "weit mit mir zu fahren hin,", "tokens": ["weit", "mit", "mir", "zu", "fah\u00b7ren", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo der junge Tag erwachet,", "tokens": ["wo", "der", "jun\u00b7ge", "Tag", "er\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo das teure ", "tokens": ["wo", "das", "teu\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "etwas Solches hat zu tun,", "tokens": ["et\u00b7was", "Sol\u00b7ches", "hat", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "das uns Allen Freude machet.", "tokens": ["das", "uns", "Al\u00b7len", "Freu\u00b7de", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach der Zeit so hastu dich", "tokens": ["Nach", "der", "Zeit", "so", "has\u00b7tu", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ganz gesetzet gegen mich.", "tokens": ["ganz", "ge\u00b7set\u00b7zet", "ge\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin ganz erpicht auf Z\u00fcge:", "tokens": ["Ich", "bin", "ganz", "er\u00b7picht", "auf", "Z\u00fc\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "du erw\u00e4hlest dir die Ruh',", "tokens": ["du", "er\u00b7w\u00e4h\u00b7lest", "dir", "die", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und der Ruhe Rast darzu", "tokens": ["und", "der", "Ru\u00b7he", "Rast", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "giebt sich dir in voller Gn\u00fcge.", "tokens": ["giebt", "sich", "dir", "in", "vol\u00b7ler", "Gn\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df gew\u00fcndschte Hindern\u00fc\u00df", "tokens": ["Di\u00df", "ge\u00b7w\u00fcnd\u00b7schte", "Hin\u00b7der\u00b7n\u00fc\u00df"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00f6rdert dich, das ist gewi\u00df.", "tokens": ["f\u00f6r\u00b7dert", "dich", ",", "das", "ist", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein' Gedanken, Tun und Lassen,", "tokens": ["Dein'", "Ge\u00b7dan\u00b7ken", ",", "Tun", "und", "Las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, was du bist und hast,", "tokens": ["Al\u00b7les", ",", "was", "du", "bist", "und", "hast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das giebst du in einer Last", "tokens": ["das", "giebst", "du", "in", "ei\u00b7ner", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "deiner Liebsten zu umfassen.", "tokens": ["dei\u00b7ner", "Liebs\u00b7ten", "zu", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott, der wei\u00df, wie mirs noch geht.", "tokens": ["Gott", ",", "der", "wei\u00df", ",", "wie", "mirs", "noch", "geht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "PWAV", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun, ihr wisset, da\u00df ihr steht:", "tokens": ["Nun", ",", "ihr", "wis\u00b7set", ",", "da\u00df", "ihr", "steht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ich besteh' in Unbestande.", "tokens": ["ich", "be\u00b7steh'", "in", "Un\u00b7be\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich verkn\u00fcpft ein freies Band:", "tokens": ["Dich", "ver\u00b7kn\u00fcpft", "ein", "frei\u00b7es", "Band", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ich, wie los mir ist die Hand,", "tokens": ["ich", ",", "wie", "los", "mir", "ist", "die", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "bin verstrickt auf weite Lande.", "tokens": ["bin", "ver\u00b7strickt", "auf", "wei\u00b7te", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wol euch, \u00fcbel mir, da\u00df ich", "tokens": ["Wol", "euch", ",", "\u00fc\u00b7bel", "mir", ",", "da\u00df", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADJD", "PPER", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "so verwegen bin auf mich!", "tokens": ["so", "ver\u00b7we\u00b7gen", "bin", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott, der mag uns beiderseiten,", "tokens": ["Gott", ",", "der", "mag", "uns", "bei\u00b7der\u00b7sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weil es ja mu\u00df sein getan,", "tokens": ["weil", "es", "ja", "mu\u00df", "sein", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dich mit Segen sehen an,", "tokens": ["dich", "mit", "Se\u00b7gen", "se\u00b7hen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "mich mit Hulden auch begleiten.", "tokens": ["mich", "mit", "Hul\u00b7den", "auch", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Komm' ich wieder wol zu dir,", "tokens": ["Komm'", "ich", "wie\u00b7der", "wol", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "so gelingt es dir und mir,", "tokens": ["so", "ge\u00b7lingt", "es", "dir", "und", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dir, da\u00df du mich kaust empfangen,", "tokens": ["dir", ",", "da\u00df", "du", "mich", "kaust", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mir, da\u00df eine lange Schaar", "tokens": ["mir", ",", "da\u00df", "ei\u00b7ne", "lan\u00b7ge", "Schaar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "deiner Kinder, Paar und Paar,", "tokens": ["dei\u00b7ner", "Kin\u00b7der", ",", "Paar", "und", "Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "recht entgegen k\u00f6mmt gegangen.", "tokens": ["recht", "ent\u00b7ge\u00b7gen", "k\u00f6mmt", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}