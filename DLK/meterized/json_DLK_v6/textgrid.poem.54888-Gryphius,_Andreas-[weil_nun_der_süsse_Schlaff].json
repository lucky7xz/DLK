{"textgrid.poem.54888": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "[weil nun der s\u00fcsse Schlaff]", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil nun der s\u00fcsse Schlaff die m\u00fcden Augen schleust/", "tokens": ["Weil", "nun", "der", "s\u00fcs\u00b7se", "Schlaff", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "schleust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die beschw\u00e4rzte Nacht gleich einem Strom herfleust/", "tokens": ["Und", "die", "be\u00b7schw\u00e4rz\u00b7te", "Nacht", "gleich", "ei\u00b7nem", "Strom", "her\u00b7fleust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil dicke Finstern\u00fc\u00df die frembden Tr\u00e4um einf\u00fchret/", "tokens": ["Weil", "di\u00b7cke", "Fins\u00b7ter\u00b7n\u00fc\u00df", "die", "fremb\u00b7den", "Tr\u00e4um", "ein\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der gebundne Leib nichts f\u00fchlt/ nichts kennt/ nichts r\u00fchret.", "tokens": ["Und", "der", "ge\u00b7bund\u00b7ne", "Leib", "nichts", "f\u00fchlt", "/", "nichts", "kennt", "/", "nichts", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIS", "VVFIN", "$(", "PIS", "VVFIN", "$(", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ko i O s\u00fcsse Ruh'! erquicke meine Seel!", "tokens": ["So", "ko", "i", "O", "s\u00fcs\u00b7se", "Ruh'", "!", "er\u00b7qui\u00b7cke", "mei\u00b7ne", "Seel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "NE", "ADJA", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die nur durch dich/ nach dir in dieser Marter-H\u00f6hl", "tokens": ["Die", "nur", "durch", "dich", "/", "nach", "dir", "in", "die\u00b7ser", "Mar\u00b7ter\u00b7H\u00f6hl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "$(", "APPR", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit steten Seuffzen schmacht/ ko i/ wann mein Tag vergangen/", "tokens": ["Mit", "ste\u00b7ten", "Seuff\u00b7zen", "schmacht", "/", "ko", "i", "/", "wann", "mein", "Tag", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "NE", "NE", "$(", "PWAV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und ich die Ewigkeit geruffen anzufangen.", "tokens": ["Und", "ich", "die", "E\u00b7wig\u00b7keit", "ge\u00b7ruf\u00b7fen", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Leb' in mir/ wann der Tod das kalte Fleisch bestrickt/", "tokens": ["Leb'", "in", "mir", "/", "wann", "der", "Tod", "das", "kal\u00b7te", "Fleisch", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "$(", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und mir die lange Nacht beyd' Augen zugedr\u00fcckt.", "tokens": ["Und", "mir", "die", "lan\u00b7ge", "Nacht", "beyd'", "Au\u00b7gen", "zu\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gib da\u00df ich sicher lieg' und nichts mich m\u00f6g erschrecken/", "tokens": ["Gib", "da\u00df", "ich", "si\u00b7cher", "lieg'", "und", "nichts", "mich", "m\u00f6g", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "ADJD", "VVFIN", "KON", "PIS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn an dem grossen Tag du alle wirst erwecken.", "tokens": ["Wenn", "an", "dem", "gros\u00b7sen", "Tag", "du", "al\u00b7le", "wirst", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn la\u00df mich dich o Sonn/ o Licht/ das niemand schaut/", "tokens": ["Denn", "la\u00df", "mich", "dich", "o", "Sonn", "/", "o", "Licht", "/", "das", "nie\u00b7mand", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "FM", "NN", "$(", "FM", "NN", "$(", "ART", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der noch das Elend hier im Thal der Thr\u00e4nen baut/", "tokens": ["Der", "noch", "das", "E\u00b7lend", "hier", "im", "Thal", "der", "Thr\u00e4\u00b7nen", "baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit immer neuer Freud' in diesen Gliedern sehen;", "tokens": ["Mit", "im\u00b7mer", "neu\u00b7er", "Freud'", "in", "die\u00b7sen", "Glie\u00b7dern", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "La\u00df unter deinem Fu\u00df was hier dich pflegt zu schm\u00e4hen", "tokens": ["La\u00df", "un\u00b7ter", "dei\u00b7nem", "Fu\u00df", "was", "hier", "dich", "pflegt", "zu", "schm\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PRELS", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zutretten/ und zuknickt/ mein fr\u00f6lich Lustbild seyn/", "tokens": ["Zu\u00b7tret\u00b7ten", "/", "und", "zu\u00b7knickt", "/", "mein", "fr\u00f6\u00b7lich", "Lust\u00b7bild", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "VVFIN", "$(", "PPOSAT", "ADJD", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und f\u00fchre mich ins Hau\u00df der seel'gen Ruhst\u00e4tt ein.", "tokens": ["Und", "f\u00fch\u00b7re", "mich", "ins", "Hau\u00df", "der", "seel'\u00b7gen", "Ruh\u00b7st\u00e4tt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}