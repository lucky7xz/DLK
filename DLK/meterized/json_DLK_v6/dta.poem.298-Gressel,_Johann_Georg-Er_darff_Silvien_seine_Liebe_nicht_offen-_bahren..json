{"dta.poem.298": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er darff  Silvien  seine Liebe nicht offen-  \n bahren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vergn\u00fcgen heist nur Schauen/", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "nur", "Schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ist weiter nichts verg\u00f6nnt;", "tokens": ["Mir", "ist", "wei\u00b7ter", "nichts", "ver\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich darf nicht auf Hoffnung bauen/", "tokens": ["Ich", "darf", "nicht", "auf", "Hoff\u00b7nung", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Hertz ohne H\u00fclffe brennt.", "tokens": ["Mein", "Hertz", "oh\u00b7ne", "H\u00fclf\u00b7fe", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich darf nichts von Lieben sagen/", "tokens": ["Ich", "darf", "nichts", "von", "Lie\u00b7ben", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "ADJA", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Pein nicht klagen/", "tokens": ["Die", "Pein", "nicht", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Welche kein Auffh\u00f6ren kennt.", "tokens": ["Wel\u00b7che", "kein", "Auff\u00b7h\u00f6\u00b7ren", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Silvia lacht meine Flammen/", "tokens": ["Sil\u00b7via", "lacht", "mei\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sie verwirfft die reine Gluth/", "tokens": ["Sie", "ver\u00b7wirfft", "die", "rei\u00b7ne", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie will meinen Brand verdammen", "tokens": ["Sie", "will", "mei\u00b7nen", "Brand", "ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sie gleich die Wirckung thut.", "tokens": ["Ob", "sie", "gleich", "die", "Wir\u00b7ckung", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr Gesichte Flammen blitzet/", "tokens": ["Ihr", "Ge\u00b7sich\u00b7te", "Flam\u00b7men", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mehr erhitzet", "tokens": ["Und", "mehr", "er\u00b7hit\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das ohndem entflamte Blut.", "tokens": ["Das", "ohn\u00b7dem", "ent\u00b7flam\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch mein Hertze sey zu frieden/", "tokens": ["Doch", "mein", "Hert\u00b7ze", "sey", "zu", "frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VAFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebt dich gleich dis M\u00e4dgen nicht", "tokens": ["Liebt", "dich", "gleich", "dis", "M\u00e4d\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum bist du all- und jeden", "tokens": ["Da\u00b7rum", "bist", "du", "all", "und", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "TRUNC", "KON", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zum Ziele aufgericht.", "tokens": ["Nicht", "zum", "Zie\u00b7le", "auf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4sset dich schon diese f\u00e4hlen/", "tokens": ["L\u00e4s\u00b7set", "dich", "schon", "die\u00b7se", "f\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach solchen Qu\u00e4hlen", "tokens": ["Nach", "sol\u00b7chen", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.7": {"text": "Scheinet wol ein ander Licht.", "tokens": ["Schei\u00b7net", "wol", "ein", "an\u00b7der", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum so fasse dis Entschliessen/", "tokens": ["Drum", "so", "fas\u00b7se", "dis", "Ent\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PDS", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du sie nicht lieben wilt:", "tokens": ["Da\u00df", "du", "sie", "nicht", "lie\u00b7ben", "wilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der N\u00e4chte Finsternissen/", "tokens": ["Nach", "der", "N\u00e4ch\u00b7te", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf ein leeres Schatten-Bild", "tokens": ["Auf", "ein", "lee\u00b7res", "Schat\u00b7ten\u00b7Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Folgt ein helles Sonnen-Scheinen/", "tokens": ["Folgt", "ein", "hel\u00b7les", "Son\u00b7nen\u00b7Schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So Schmertz und Weinen", "tokens": ["So", "Schmertz", "und", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mit vergn\u00fcgten Blicken stilt.", "tokens": ["Mit", "ver\u00b7gn\u00fcg\u00b7ten", "Bli\u00b7cken", "stilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}