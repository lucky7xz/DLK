{"textgrid.poem.67445": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Psyche, schiffend mit Delphinen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwohin ruderst Du, Psyche, von zwei Delphinen gef\u00fchret?\u00ab", "tokens": ["\u00bb", "wo\u00b7hin", "ru\u00b7derst", "Du", ",", "Psy\u00b7che", ",", "von", "zwei", "Del\u00b7phi\u00b7nen", "ge\u00b7f\u00fch\u00b7ret", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "CARD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbueber des Lebens Strom gleit' ich, o Wanderer, hin.", "tokens": ["\u00bb", "ue\u00b7ber", "des", "Le\u00b7bens", "Strom", "gleit'", "ich", ",", "o", "Wan\u00b7de\u00b7rer", ",", "hin", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Gl\u00fccklich wurden auf ihm mir Musen liebende F\u00fchrer", "tokens": ["Gl\u00fcck\u00b7lich", "wur\u00b7den", "auf", "ihm", "mir", "Mu\u00b7sen", "lie\u00b7ben\u00b7de", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PPER", "PPER", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und zur sicheren Fahrt Ruder und Steuer verliehn.\u00ab", "tokens": ["Und", "zur", "si\u00b7che\u00b7ren", "Fahrt", "Ru\u00b7der", "und", "Steu\u00b7er", "ver\u00b7liehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}