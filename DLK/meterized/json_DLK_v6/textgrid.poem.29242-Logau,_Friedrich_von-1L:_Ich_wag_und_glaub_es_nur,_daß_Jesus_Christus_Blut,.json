{"textgrid.poem.29242": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wag und glaub es nur, da\u00df Jesus Christus Blut,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wag und glaub es nur, da\u00df Jesus Christus Blut,", "tokens": ["Ich", "wag", "und", "glaub", "es", "nur", ",", "da\u00df", "Je\u00b7sus", "Chris\u00b7tus", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu tilgen meine Schuld, sey gar genug und gut;", "tokens": ["Zu", "til\u00b7gen", "mei\u00b7ne", "Schuld", ",", "sey", "gar", "ge\u00b7nug", "und", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer wil, der wasche sich durch eigner Wercke schwitzen;", "tokens": ["Wer", "wil", ",", "der", "wa\u00b7sche", "sich", "durch", "eig\u00b7ner", "Wer\u00b7cke", "schwit\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "ART", "ADJA", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im sterben wird man sehn, was Blut, was Schwei\u00df wird n\u00fctzen.", "tokens": ["Im", "ster\u00b7ben", "wird", "man", "sehn", ",", "was", "Blut", ",", "was", "Schwei\u00df", "wird", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "