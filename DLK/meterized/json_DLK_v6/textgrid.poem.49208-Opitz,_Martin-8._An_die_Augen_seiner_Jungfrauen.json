{"textgrid.poem.49208": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "8. An die Augen seiner Jungfrauen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leitsternen meines Haupts, und meiner jungen Zeit,", "tokens": ["Leits\u00b7ter\u00b7nen", "mei\u00b7nes", "Haupts", ",", "und", "mei\u00b7ner", "jun\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die als Planeten sind gesetzet meinem Leben,", "tokens": ["Die", "als", "Pla\u00b7ne\u00b7ten", "sind", "ge\u00b7set\u00b7zet", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihr Augen, wann ich euch so freundlich sehe schweben,", "tokens": ["Ihr", "Au\u00b7gen", ",", "wann", "ich", "euch", "so", "freund\u00b7lich", "se\u00b7he", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bin ich als entz\u00fcckt und kenne gantz kein Leid;", "tokens": ["So", "bin", "ich", "als", "ent\u00b7z\u00fcckt", "und", "ken\u00b7ne", "gantz", "kein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "VVPP", "KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann ihr beschliest in euch ein' hohe Liebligkeit", "tokens": ["Dann", "ihr", "be\u00b7schliest", "in", "euch", "ein'", "ho\u00b7he", "Lieb\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lieblich' Hoheit; ihr, ihr k\u00f6nnt alleine geben", "tokens": ["Und", "lieb\u00b7lich'", "Ho\u00b7heit", ";", "ihr", ",", "ihr", "k\u00f6nnt", "al\u00b7lei\u00b7ne", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "PPER", "$,", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gen\u00fcge, rechte Lust; wornach wir M\u00e4nner streben", "tokens": ["Ge\u00b7n\u00fc\u00b7ge", ",", "rech\u00b7te", "Lust", ";", "wor\u00b7nach", "wir", "M\u00e4n\u00b7ner", "stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PWAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das habt ihr, o mein Liecht, vor allem weit und breit.", "tokens": ["Das", "habt", "ihr", ",", "o", "mein", "Liecht", ",", "vor", "al\u00b7lem", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "APPR", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Natura selber liegt im Tunckeln fast begraben", "tokens": ["Na\u00b7tu\u00b7ra", "sel\u00b7ber", "liegt", "im", "Tun\u00b7ckeln", "fast", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "APPRART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mangelt ihres Liechts von wegen ihrer Gaben,", "tokens": ["Und", "man\u00b7gelt", "ih\u00b7res", "Liechts", "von", "we\u00b7gen", "ih\u00b7rer", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gantz versamblet sind in solcher engen Statt;", "tokens": ["Die", "gantz", "ver\u00b7samb\u00b7let", "sind", "in", "sol\u00b7cher", "en\u00b7gen", "Statt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch ist sie enge nicht und kan sich weit ergiessen,", "tokens": ["Doch", "ist", "sie", "en\u00b7ge", "nicht", "und", "kan", "sich", "weit", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PTKNEG", "KON", "VMFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja were gro\u00df genung, fast alles einzuschliessen,", "tokens": ["Ja", "we\u00b7re", "gro\u00df", "ge\u00b7nung", ",", "fast", "al\u00b7les", "ein\u00b7zu\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ADJD", "NN", "$,", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sich mein' arme Seel' in ihr verirret hat.", "tokens": ["Weil", "sich", "mein'", "ar\u00b7me", "Seel'", "in", "ihr", "ver\u00b7ir\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}