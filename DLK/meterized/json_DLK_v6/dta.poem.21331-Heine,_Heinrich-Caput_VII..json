{"dta.poem.21331": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er nahte sich, und mit dem Beil", "tokens": ["Er", "nah\u00b7te", "sich", ",", "und", "mit", "dem", "Beil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerschmetterte er die armen", "tokens": ["Zer\u00b7schmet\u00b7ter\u00b7te", "er", "die", "ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Skelette des Aberglaubens, er schlug", "tokens": ["Ske\u00b7let\u00b7te", "des", "A\u00b7berg\u00b7lau\u00b7bens", ",", "er", "schlug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie nieder ohn\u2019 Erbarmen.", "tokens": ["Sie", "nie\u00b7der", "ohn'", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es dr\u00f6hnte der Hiebe Wiederhall", "tokens": ["Es", "dr\u00f6hn\u00b7te", "der", "Hie\u00b7be", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus allen Gew\u00f6lben, entsetzlich, \u2013", "tokens": ["Aus", "al\u00b7len", "Ge\u00b7w\u00f6l\u00b7ben", ",", "ent\u00b7setz\u00b7lich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Blutstr\u00f6me schossen aus meiner Brust,", "tokens": ["Blut\u00b7str\u00f6\u00b7me", "schos\u00b7sen", "aus", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich erwachte pl\u00f6tzlich.", "tokens": ["Und", "ich", "er\u00b7wach\u00b7te", "pl\u00f6tz\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}