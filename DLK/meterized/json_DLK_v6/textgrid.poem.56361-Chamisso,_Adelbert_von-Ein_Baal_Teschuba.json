{"textgrid.poem.56361": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Baal Teschuba", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch hatte der Rabbiner nicht begonnen", "tokens": ["Noch", "hat\u00b7te", "der", "Rab\u00b7bi\u00b7ner", "nicht", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu unterrichten, im gedr\u00e4ngten Kreise", "tokens": ["Zu", "un\u00b7ter\u00b7rich\u00b7ten", ",", "im", "ge\u00b7dr\u00e4ng\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sch\u00fcler hatte sich Gespr\u00e4ch entsponnen;", "tokens": ["Der", "Sch\u00fc\u00b7ler", "hat\u00b7te", "sich", "Ge\u00b7spr\u00e4ch", "ent\u00b7spon\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gespr\u00e4ch von jenem r\u00e4tselhaften Greise,", "tokens": ["Ge\u00b7spr\u00e4ch", "von", "je\u00b7nem", "r\u00e4t\u00b7sel\u00b7haf\u00b7ten", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der in die Synagoge war gekommen", "tokens": ["Der", "in", "die", "Syn\u00b7a\u00b7go\u00b7ge", "war", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fast eigent\u00fcmlich schauerlicher Weise;", "tokens": ["Fast", "ei\u00b7gen\u00b7t\u00fcm\u00b7lich", "schau\u00b7er\u00b7li\u00b7cher", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der auf der Trauerbank den Platz genommen,", "tokens": ["Der", "auf", "der", "Trau\u00b7er\u00b7bank", "den", "Platz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Str\u00e4fling gleich, and\u00e4chtig immerdar,", "tokens": ["Dem", "Str\u00e4f\u00b7ling", "gleich", ",", "an\u00b7d\u00e4ch\u00b7tig", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Vorbild der Erbauung allen Frommen,", "tokens": ["Ein", "Vor\u00b7bild", "der", "Er\u00b7bau\u00b7ung", "al\u00b7len", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wie das Schlu\u00dfgebet gesprochen war,", "tokens": ["Und", "wie", "das", "Schlu\u00df\u00b7ge\u00b7bet", "ge\u00b7spro\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aufspringend mit befremdlicher Geberde,", "tokens": ["Auf\u00b7sprin\u00b7gend", "mit", "be\u00b7fremd\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sein Haupt verh\u00fcllt im faltigen Talar,", "tokens": ["Sein", "Haupt", "ver\u00b7h\u00fcllt", "im", "fal\u00b7ti\u00b7gen", "Ta\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sich quer am Eingang auf die harte Erde", "tokens": ["Sich", "quer", "am", "Ein\u00b7gang", "auf", "die", "har\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Vor allen niederst\u00fcrzend hingestreckt,", "tokens": ["Vor", "al\u00b7len", "nie\u00b7ders\u00b7t\u00fcr\u00b7zend", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Auf da\u00df mit F\u00fc\u00dfen er getreten werde.", "tokens": ["Auf", "da\u00df", "mit", "F\u00fc\u00b7\u00dfen", "er", "ge\u00b7tre\u00b7ten", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch keiner tat's, denn jeder wich erschreckt", "tokens": ["Doch", "kei\u00b7ner", "tat's", ",", "denn", "je\u00b7der", "wich", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zur Seite, da\u00df den Starren er vermeide,", "tokens": ["Zur", "Sei\u00b7te", ",", "da\u00df", "den", "Star\u00b7ren", "er", "ver\u00b7mei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den erst der letzten Schritte Hall erweckt.", "tokens": ["Den", "erst", "der", "letz\u00b7ten", "Schrit\u00b7te", "Hall", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Pole m\u00fc\u00dft er sein nach seinem Kleide,", "tokens": ["Ein", "Po\u00b7le", "m\u00fc\u00dft", "er", "sein", "nach", "sei\u00b7nem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPOSAT", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Doch haben, die ihn sprachen, ausgesagt,", "tokens": ["Doch", "ha\u00b7ben", ",", "die", "ihn", "spra\u00b7chen", ",", "aus\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Da\u00df ihn die deutsche Mundart unterscheide.", "tokens": ["Da\u00df", "ihn", "die", "deut\u00b7sche", "Mund\u00b7art", "un\u00b7ter\u00b7schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Nach seinem Namen haben sie gefragt,", "tokens": ["Nach", "sei\u00b7nem", "Na\u00b7men", "ha\u00b7ben", "sie", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Worauf er seufzend Antwort nicht gegeben;", "tokens": ["Wo\u00b7rauf", "er", "seuf\u00b7zend", "Ant\u00b7wort", "nicht", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Sie haben, mehr zu fragen, nicht gewagt.", "tokens": ["Sie", "ha\u00b7ben", ",", "mehr", "zu", "fra\u00b7gen", ",", "nicht", "ge\u00b7wagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da trat, wie so die Sch\u00fcler sprachen, eben", "tokens": ["Da", "trat", ",", "wie", "so", "die", "Sch\u00fc\u00b7ler", "spra\u00b7chen", ",", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Der Greis herein, dem Winter zu vergleichen", "tokens": ["Der", "Greis", "her\u00b7ein", ",", "dem", "Win\u00b7ter", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Von jugendlichem Fr\u00fchlingsreis umgeben.", "tokens": ["Von", "ju\u00b7gend\u00b7li\u00b7chem", "Fr\u00fch\u00b7lings\u00b7reis", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Es sahn die Ringsverstummenden ihn schleichen", "tokens": ["Es", "sahn", "die", "Rings\u00b7ver\u00b7stum\u00b7men\u00b7den", "ihn", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dem letzten Platze zu, um den er bat,", "tokens": ["Dem", "letz\u00b7ten", "Plat\u00b7ze", "zu", ",", "um", "den", "er", "bat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ihn sollte da das heil'ge Wort erreichen.", "tokens": ["Ihn", "soll\u00b7te", "da", "das", "heil'\u00b7ge", "Wort", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und der Rabbiner sich erhebend trat", "tokens": ["Und", "der", "Rab\u00b7bi\u00b7ner", "sich", "er\u00b7he\u00b7bend", "trat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mit ernstem Worte zu dem seltnen Gast:", "tokens": ["Mit", "erns\u00b7tem", "Wor\u00b7te", "zu", "dem", "selt\u00b7nen", "Gast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "\u00bbhier gilt es, auszustreuen gute Saat.", "tokens": ["\u00bb", "hier", "gilt", "es", ",", "aus\u00b7zu\u00b7streu\u00b7en", "gu\u00b7te", "Saat", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wie du im Tempel dich betragen hast,", "tokens": ["Wie", "du", "im", "Tem\u00b7pel", "dich", "be\u00b7tra\u00b7gen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Erscheint vielleicht in zweifelhaftem Lichte", "tokens": ["Er\u00b7scheint", "viel\u00b7leicht", "in", "zwei\u00b7fel\u00b7haf\u00b7tem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Dem, der den Gang des Lebens nicht erfa\u00dft;", "tokens": ["Dem", ",", "der", "den", "Gang", "des", "Le\u00b7bens", "nicht", "er\u00b7fa\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Was aber dich bewogen, das berichte", "tokens": ["Was", "a\u00b7ber", "dich", "be\u00b7wo\u00b7gen", ",", "das", "be\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Du diesen hier, damit auch sie es wissen;", "tokens": ["Du", "die\u00b7sen", "hier", ",", "da\u00b7mit", "auch", "sie", "es", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "$,", "KOUS", "ADV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ich fodre deine d\u00fcstere Geschichte.", "tokens": ["Ich", "fod\u00b7re", "dei\u00b7ne", "d\u00fcs\u00b7te\u00b7re", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Gar mancher ist der Weisheit nicht beflissen,", "tokens": ["Gar", "man\u00b7cher", "ist", "der", "Weis\u00b7heit", "nicht", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der wahrlich anders w\u00fcrde sein, verst\u00fcnd er", "tokens": ["Der", "wahr\u00b7lich", "an\u00b7ders", "w\u00fcr\u00b7de", "sein", ",", "ver\u00b7st\u00fcnd", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADV", "VAFIN", "VAINF", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Den Ernst der Tat im strafenden Gewissen.\u00ab \u2013", "tokens": ["Den", "Ernst", "der", "Tat", "im", "stra\u00b7fen\u00b7den", "Ge\u00b7wis\u00b7sen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "\u00bbich bin ein Baal Teschuba, bin ein S\u00fcnder,", "tokens": ["\u00bb", "ich", "bin", "ein", "Baal", "Te\u00b7schu\u00b7ba", ",", "bin", "ein", "S\u00fcn\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NE", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Der wallend durch das Elend Bu\u00dfe tut,", "tokens": ["Der", "wal\u00b7lend", "durch", "das", "E\u00b7lend", "Bu\u00b7\u00dfe", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und jetzt der eignen Missetat Verk\u00fcnder.", "tokens": ["Und", "jetzt", "der", "eig\u00b7nen", "Mis\u00b7se\u00b7tat", "Ver\u00b7k\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Nach meinem Namen forschet nicht, der ruht", "tokens": ["Nach", "mei\u00b7nem", "Na\u00b7men", "for\u00b7schet", "nicht", ",", "der", "ruht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Bei meinen Hinterla\u00dfnen, Weib und Kindern,", "tokens": ["Bei", "mei\u00b7nen", "Hin\u00b7ter\u00b7la\u00df\u00b7nen", ",", "Weib", "und", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Und liegt bei Haus und Hof und Hab und Gut.", "tokens": ["Und", "liegt", "bei", "Haus", "und", "Hof", "und", "Hab", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Ich handelte