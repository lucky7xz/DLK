{"dta.poem.9591": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Augen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljsette/ leih' mir eins von deinen sch\u00f6nen augen/", "tokens": ["Ljset\u00b7te", "/", "leih'", "mir", "eins", "von", "dei\u00b7nen", "sch\u00f6\u00b7nen", "au\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "PPER", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Denn meine wollen gar zur liebe nicht mehr taugen/", "tokens": ["Denn", "mei\u00b7ne", "wol\u00b7len", "gar", "zur", "lie\u00b7be", "nicht", "mehr", "tau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VMFIN", "ADV", "APPRART", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hegen keine glut/ kein feuer/ keinen brand/", "tokens": ["Sie", "he\u00b7gen", "kei\u00b7ne", "glut", "/", "kein", "feu\u00b7er", "/", "kei\u00b7nen", "brand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein funcke l\u00e4st sich mehr in ihren \u00e4pffeln finden/", "tokens": ["Kein", "fun\u00b7cke", "l\u00e4st", "sich", "mehr", "in", "ih\u00b7ren", "\u00e4pf\u00b7feln", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was lust und liebreitz heist/ ist ihnen unbekant/", "tokens": ["Was", "lust", "und", "lieb\u00b7reitz", "heist", "/", "ist", "ih\u00b7nen", "un\u00b7be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "ADJD", "VAFIN", "$(", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alle lockung will aus ihren gr\u00e4ntzen schwinden.", "tokens": ["Und", "al\u00b7le", "lo\u00b7ckung", "will", "aus", "ih\u00b7ren", "gr\u00e4nt\u00b7zen", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein finstres auge wei\u00df von keiner brunst zu sagen/", "tokens": ["Mein", "finst\u00b7res", "au\u00b7ge", "wei\u00df", "von", "kei\u00b7ner", "brunst", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es f\u00e4ngt im selben kaum vor abend an zu tagen;", "tokens": ["Es", "f\u00e4ngt", "im", "sel\u00b7ben", "kaum", "vor", "a\u00b7bend", "an", "zu", "ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ADV", "APPR", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hingegen deines f\u00fchrt nur lauter sonnen-schein/", "tokens": ["Hin\u00b7ge\u00b7gen", "dei\u00b7nes", "f\u00fchrt", "nur", "lau\u00b7ter", "son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die flammen finden hier den zuwachs ihrer st\u00e4rcke/", "tokens": ["Die", "flam\u00b7men", "fin\u00b7den", "hier", "den", "zu\u00b7wachs", "ih\u00b7rer", "st\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "ADV", "ART", "CARD", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Cupido nimmt es selbst f\u00fcr sich zur wohnung ein/", "tokens": ["Cu\u00b7pi\u00b7do", "nimmt", "es", "selbst", "f\u00fcr", "sich", "zur", "woh\u00b7nung", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PRF", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und thut durch dessen krafft die gr\u00f6sten wunder-wercke.", "tokens": ["Und", "thut", "durch", "des\u00b7sen", "krafft", "die", "gr\u00f6s\u00b7ten", "wun\u00b7der\u00b7\u00b7wer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gib mir/ Lisette/ dann von deinen augen-strahlen/", "tokens": ["Gib", "mir", "/", "Li\u00b7set\u00b7te", "/", "dann", "von", "dei\u00b7nen", "au\u00b7gen\u00b7strah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "NE", "$(", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe lust/ dir einst die blicke zu bezahlen/", "tokens": ["Ich", "ha\u00b7be", "lust", "/", "dir", "einst", "die", "bli\u00b7cke", "zu", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$(", "PPER", "ADV", "ART", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Womit du mich so fest umzogen und ger\u00fchrt;", "tokens": ["Wo\u00b7mit", "du", "mich", "so", "fest", "um\u00b7zo\u00b7gen", "und", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADJD", "VVIZU", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein aug ist viel zu schwach die sinnen zu ber\u00fccken/", "tokens": ["Mein", "aug", "ist", "viel", "zu", "schwach", "die", "sin\u00b7nen", "zu", "be\u00b7r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "ART", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach deines/ das zugleich blitz/ pfeil und bogen f\u00fchrt/", "tokens": ["Ach", "dei\u00b7nes", "/", "das", "zu\u00b7gleich", "blitz", "/", "pfeil", "und", "bo\u00b7gen", "f\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "$(", "PDS", "ADV", "ADJD", "$(", "VVIMP", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Taugt gar zu sch\u00f6n f\u00fcr mich/ dein hertze zu bestricken!", "tokens": ["Taugt", "gar", "zu", "sch\u00f6n", "f\u00fcr", "mich", "/", "dein", "hert\u00b7ze", "zu", "be\u00b7stri\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "APPR", "PPER", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}