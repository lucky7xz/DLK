{"textgrid.poem.67234": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Beben, wie die Freude bebet,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Beben, wie die Freude bebet,", "tokens": ["Mit", "Be\u00b7ben", ",", "wie", "die", "Freu\u00b7de", "be\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dankbar segnend dein Geschick,", "tokens": ["Und", "dank\u00b7bar", "seg\u00b7nend", "dein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von k\u00fchner Ahndung neu belebet,", "tokens": ["Von", "k\u00fch\u00b7ner", "Ahn\u00b7dung", "neu", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Bruderliebe Herz und Blick;", "tokens": ["Voll", "Bru\u00b7der\u00b7lie\u00b7be", "Herz", "und", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So, Bruder, tritt in unsre Mitte", "tokens": ["So", ",", "Bru\u00b7der", ",", "tritt", "in", "uns\u00b7re", "Mit\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schw\u00f6r' den schauervollen Eid,", "tokens": ["So", "schw\u00f6r'", "den", "schau\u00b7er\u00b7vol\u00b7len", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder ist, nach Maurersitte,", "tokens": ["Und", "je\u00b7der", "ist", ",", "nach", "Mau\u00b7rer\u00b7sit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herzensfreund zu seyn bereit;", "tokens": ["Dein", "Her\u00b7zens\u00b7freund", "zu", "seyn", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VAINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und willig, Habe, Blut und Leben,", "tokens": ["Und", "wil\u00b7lig", ",", "Ha\u00b7be", ",", "Blut", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm diesen Bruderku\u00df zum Pfand!", "tokens": ["Nimm", "die\u00b7sen", "Bru\u00b7der\u00b7ku\u00df", "zum", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich, und jeden hinzugeben,", "tokens": ["F\u00fcr", "dich", ",", "und", "je\u00b7den", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich, wie du, mit uns verband.", "tokens": ["Der", "sich", ",", "wie", "du", ",", "mit", "uns", "ver\u00b7band", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "PWAV", "PPER", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-----+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Auch dir sei Habe, Blut und Leben", "tokens": ["Auch", "dir", "sei", "Ha\u00b7be", ",", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu theu'r f\u00fcr deine Br\u00fcder nicht,", "tokens": ["Zu", "theu'r", "f\u00fcr", "dei\u00b7ne", "Br\u00fc\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Freud' und Demuth es zu geben,", "tokens": ["Mit", "Freud'", "und", "De\u00b7muth", "es", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das, Bruder, ist des Maurers Pflicht!", "tokens": ["Das", ",", "Bru\u00b7der", ",", "ist", "des", "Mau\u00b7rers", "Pflicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! rauh und steil sind unsre Pfade", "tokens": ["Ach", "!", "rauh", "und", "steil", "sind", "uns\u00b7re", "Pfa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und harte K\u00e4mpfe k\u00e4mpfen wir;", "tokens": ["Und", "har\u00b7te", "K\u00e4mp\u00b7fe", "k\u00e4mp\u00b7fen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fliehst du den Kampf, fliehst du die Pfade,", "tokens": ["Fliehst", "du", "den", "Kampf", ",", "fliehst", "du", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wehe! wehe! wehe! dir.", "tokens": ["Dann", "we\u00b7he", "!", "we\u00b7he", "!", "we\u00b7he", "!", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJD", "$.", "ADJD", "$.", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Getrost! du fliehst sie nicht. Beginne", "tokens": ["Ge\u00b7trost", "!", "du", "fliehst", "sie", "nicht", ".", "Be\u00b7gin\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Muth und Vorsicht deine Bahn,", "tokens": ["Mit", "Muth", "und", "Vor\u00b7sicht", "dei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dringe zu des Gipfels Zinne,", "tokens": ["Und", "drin\u00b7ge", "zu", "des", "Gip\u00b7fels", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu der nur Hochgeweih'te nah'n.", "tokens": ["Zu", "der", "nur", "Hoch\u00b7ge\u00b7weih'\u00b7te", "nah'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "ADV", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die St\u00e4rke st\u00fctze deine Rechte,", "tokens": ["Die", "St\u00e4r\u00b7ke", "st\u00fct\u00b7ze", "dei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn machtlos sie im Streite ficht;", "tokens": ["Wenn", "macht\u00b7los", "sie", "im", "Strei\u00b7te", "ficht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Irrsals und des Zweifels N\u00e4chte", "tokens": ["Des", "Irr\u00b7sals", "und", "des", "Zwei\u00b7fels", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhelle dir der Weisheit Licht.", "tokens": ["Er\u00b7hel\u00b7le", "dir", "der", "Weis\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Schon sank die H\u00fclle! Sieh, es winket", "tokens": ["Schon", "sank", "die", "H\u00fcl\u00b7le", "!", "Sieh", ",", "es", "win\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NE", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir fern Aurorens junger Schein,", "tokens": ["Dir", "fern", "Au\u00b7ro\u00b7rens", "jun\u00b7ger", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch grauer Nebel wallt und sinket", "tokens": ["Doch", "grau\u00b7er", "Ne\u00b7bel", "wallt", "und", "sin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00fcllt in D\u00e4mmerung dich ein!", "tokens": ["Und", "h\u00fcllt", "in", "D\u00e4m\u00b7me\u00b7rung", "dich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So wallte Nebel einst, und deckte", "tokens": ["So", "wall\u00b7te", "Ne\u00b7bel", "einst", ",", "und", "deck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Tempels Heiligthum; es bebt", "tokens": ["Des", "Tem\u00b7pels", "Hei\u00b7lig\u00b7thum", ";", "es", "bebt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der S\u00f6hne Levi Schaar; sie schreckte", "tokens": ["Der", "S\u00f6h\u00b7ne", "Le\u00b7vi", "Schaar", ";", "sie", "schreck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott, dessen Schauer sie umschwebt.", "tokens": ["Gott", ",", "des\u00b7sen", "Schau\u00b7er", "sie", "um\u00b7schwebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da schwiegen Psalter, schwiegen Lieder;", "tokens": ["Da", "schwie\u00b7gen", "Psal\u00b7ter", ",", "schwie\u00b7gen", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da flehte Salomon; da go\u00df", "tokens": ["Da", "fleh\u00b7te", "Sa\u00b7lo\u00b7mon", ";", "da", "go\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Strom des Lichtes sich hernieder,", "tokens": ["Ein", "Strom", "des", "Lich\u00b7tes", "sich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der in des Weisen Seele flo\u00df.", "tokens": ["Der", "in", "des", "Wei\u00b7sen", "See\u00b7le", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So quill' auch dir des Lichtes Quelle,", "tokens": ["So", "quill'", "auch", "dir", "des", "Lich\u00b7tes", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ergie\u00df' im vollen Strome sich,", "tokens": ["Er\u00b7gie\u00df'", "im", "vol\u00b7len", "Stro\u00b7me", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verscheuche Nebel, und erhelle", "tokens": ["Ver\u00b7scheu\u00b7che", "Ne\u00b7bel", ",", "und", "er\u00b7hel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kr\u00e4ft'ge und belebe dich!", "tokens": ["Und", "kr\u00e4ft'\u00b7ge", "und", "be\u00b7le\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wohl dir, in unsrer Br\u00fcder Kreise!", "tokens": ["Wohl", "dir", ",", "in", "uns\u00b7rer", "Br\u00fc\u00b7der", "Krei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl uns! wir feiern diesen Tag!", "tokens": ["Wohl", "uns", "!", "wir", "fei\u00b7ern", "die\u00b7sen", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm folge, nach der V\u00e4ter Weise,", "tokens": ["Ihm", "fol\u00b7ge", ",", "nach", "der", "V\u00e4\u00b7ter", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein froh bekr\u00e4nzter Abend nach.", "tokens": ["Ein", "froh", "be\u00b7kr\u00e4nz\u00b7ter", "A\u00b7bend", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Bei unserm Freudenmahl' erneue", "tokens": ["Bei", "un\u00b7serm", "Freu\u00b7den\u00b7mahl'", "er\u00b7neu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der volle Becher unser Band;", "tokens": ["Der", "vol\u00b7le", "Be\u00b7cher", "un\u00b7ser", "Band", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freud' erschein' uns! Wahrheit, Treue,", "tokens": ["Die", "Freud'", "er\u00b7schein'", "uns", "!", "Wahr\u00b7heit", ",", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und edle Zucht an ihrer Hand.", "tokens": ["Und", "ed\u00b7le", "Zucht", "an", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Dann schallen festlich unsre Lieder,", "tokens": ["Dann", "schal\u00b7len", "fest\u00b7lich", "uns\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir trinken ferner Br\u00fcder Gl\u00fcck,", "tokens": ["Wir", "trin\u00b7ken", "fer\u00b7ner", "Br\u00fc\u00b7der", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und blicken auf bedr\u00e4ngte Br\u00fcder,", "tokens": ["Und", "bli\u00b7cken", "auf", "be\u00b7dr\u00e4ng\u00b7te", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lindern freudig ihr Geschick.", "tokens": ["Und", "lin\u00b7dern", "freu\u00b7dig", "ihr", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}