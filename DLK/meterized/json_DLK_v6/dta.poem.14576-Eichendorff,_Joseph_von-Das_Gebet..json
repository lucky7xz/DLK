{"dta.poem.14576": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wen hat nicht einmal Angst befallen,", "tokens": ["Wen", "hat", "nicht", "ein\u00b7mal", "Angst", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Tr\u00fcbni\u00df ihn gefangen h\u00e4lt,", "tokens": ["Wenn", "Tr\u00fcb\u00b7ni\u00df", "ihn", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als m\u00fc\u00dft' er ewig rastlos wallen", "tokens": ["Als", "m\u00fc\u00dft'", "er", "e\u00b7wig", "rast\u00b7los", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach einer wunderbaren Welt?", "tokens": ["Nach", "ei\u00b7ner", "wun\u00b7der\u00b7ba\u00b7ren", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "All' Freunde sind lang fortgezogen,", "tokens": ["All'", "Freun\u00b7de", "sind", "lang", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fr\u00fchling weint in einem fort,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "weint", "in", "ei\u00b7nem", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eine Br\u00fccke ist der Regenbogen", "tokens": ["Ei\u00b7ne", "Br\u00fc\u00b7cke", "ist", "der", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zum friedlich sichern Heimathsport.", "tokens": ["Zum", "fried\u00b7lich", "si\u00b7chern", "Hei\u00b7math\u00b7sport", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}