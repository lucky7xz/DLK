{"textgrid.poem.61624": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil man in der Regenflut", "tokens": ["Weil", "man", "in", "der", "Re\u00b7gen\u00b7flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen schier ertr\u00e4nke,", "tokens": ["Drau\u00b7\u00dfen", "schier", "er\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sammeln wir uns wohlgemut", "tokens": ["Sam\u00b7meln", "wir", "uns", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends in der Schenke.", "tokens": ["A\u00b7bends", "in", "der", "Schen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lodernd prasseln im Kamin", "tokens": ["Lo\u00b7dernd", "pras\u00b7seln", "im", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrre Lorbeer\u00e4ste,", "tokens": ["D\u00fcr\u00b7re", "Lor\u00b7bee\u00b7r\u00e4s\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Wein von Santorin", "tokens": ["Und", "der", "Wein", "von", "San\u00b7to\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4rmt das Herz der G\u00e4ste.", "tokens": ["W\u00e4rmt", "das", "Herz", "der", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Freunde, kommt, und plaudern wir!", "tokens": ["Freun\u00b7de", ",", "kommt", ",", "und", "plau\u00b7dern", "wir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich Homeros' Helden", "tokens": ["Gleich", "Ho\u00b7me\u00b7ros'", "Hel\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Abenteuer habt auch ihr", "tokens": ["A\u00b7bent\u00b7eu\u00b7er", "habt", "auch", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher Fahrt zu melden.", "tokens": ["Man\u00b7cher", "Fahrt", "zu", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gebt Bericht, wo sich im Meer", "tokens": ["Gebt", "Be\u00b7richt", ",", "wo", "sich", "im", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "PWAV", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Segel bl\u00e4hte,", "tokens": ["Eu\u00b7er", "Se\u00b7gel", "bl\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Welch Gebiet ihr saht umher,", "tokens": ["Welch", "Ge\u00b7biet", "ihr", "saht", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher Menschen St\u00e4dte;", "tokens": ["Wel\u00b7cher", "Men\u00b7schen", "St\u00e4d\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wie ihr aus Zyklopenhand", "tokens": ["Wie", "ihr", "aus", "Zyk\u00b7lo\u00b7pen\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nur mit Not euch l\u00f6stet,", "tokens": ["Nur", "mit", "Not", "euch", "l\u00f6s\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie euch im Ph\u00e4akenland", "tokens": ["Wie", "euch", "im", "Ph\u00e4\u00b7a\u00b7ken\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6ne Fraun getr\u00f6stet.", "tokens": ["Sch\u00f6\u00b7ne", "Fraun", "ge\u00b7tr\u00f6s\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Manchen hielt vielleicht sogar,", "tokens": ["Man\u00b7chen", "hielt", "viel\u00b7leicht", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dank dem raschen Gotte!", "tokens": ["Dank", "dem", "ra\u00b7schen", "Got\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein entfesselt Lockenhaar", "tokens": ["Ein", "ent\u00b7fes\u00b7selt", "Lo\u00b7cken\u00b7haar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Kalypsos Grotte;", "tokens": ["In", "Ka\u00b7ly\u00b7psos", "Grot\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Ach, und das erfuhrt ihr auch,", "tokens": ["Ach", ",", "und", "das", "er\u00b7fuhrt", "ihr", "auch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was es hei\u00dft, mit Tr\u00e4nen", "tokens": ["Was", "es", "hei\u00dft", ",", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der Heimat fernem Rauch,", "tokens": ["Nach", "der", "Hei\u00b7mat", "fer\u00b7nem", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Uly\u00df, sich sehnen.", "tokens": ["Wie", "U\u00b7ly\u00df", ",", "sich", "seh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}