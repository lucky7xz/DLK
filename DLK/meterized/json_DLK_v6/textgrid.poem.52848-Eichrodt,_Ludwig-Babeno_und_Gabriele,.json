{"textgrid.poem.52848": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Babeno und Gabriele,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon lag die Nacht in tiefer Stille", "tokens": ["Schon", "lag", "die", "Nacht", "in", "tie\u00b7fer", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und feierlich auf der Natur,", "tokens": ["Und", "fei\u00b7er\u00b7lich", "auf", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sparsam aus der Wolkenh\u00fclle", "tokens": ["Und", "spar\u00b7sam", "aus", "der", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah Luna nieder in die Flur.", "tokens": ["Sah", "Lu\u00b7na", "nie\u00b7der", "in", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da trat Babeno - gro\u00dfer Ahnen", "tokens": ["Da", "trat", "Ba\u00b7be\u00b7no", "gro\u00b7\u00dfer", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$(", "ADJA", "NN"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Ein Spr\u00f6\u00dfling - unter's Sternenzelt,", "tokens": ["Ein", "Spr\u00f6\u00df\u00b7ling", "un\u00b7ter's", "Ster\u00b7nen\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hin zu den n\u00e4chtlichen Balkanen,", "tokens": ["Hin", "zu", "den", "n\u00e4cht\u00b7li\u00b7chen", "Bal\u00b7ka\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bis Liebchen ihn umschlungen h\u00e4lt.", "tokens": ["Bis", "Lieb\u00b7chen", "ihn", "um\u00b7schlun\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie wandeln von dem Zephyrwinde", "tokens": ["Sie", "wan\u00b7deln", "von", "dem", "Ze\u00b7phyr\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umlispelt ", "tokens": ["Um\u00b7lis\u00b7pelt"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Durch Silbertannen-Labyrinthe", "tokens": ["Durch", "Sil\u00b7ber\u00b7tan\u00b7nen\u00b7La\u00b7by\u00b7rin\u00b7the"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Felsenh\u00fcgel hin.", "tokens": ["Zu", "ei\u00b7nem", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da war's, wo sich um alte Tr\u00fcmmer", "tokens": ["Da", "wa\u00b7r's", ",", "wo", "sich", "um", "al\u00b7te", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Von einer Burg ", "tokens": ["Von", "ei\u00b7ner", "Burg"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und aus dem \u00f6den Felsenzimmer", "tokens": ["Und", "aus", "dem", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts, als der Eule Grablied drang.", "tokens": ["Nichts", ",", "als", "der", "Eu\u00b7le", "Grab\u00b7lied", "drang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es blickte hin zu dem Gem\u00e4uer", "tokens": ["Es", "blick\u00b7te", "hin", "zu", "dem", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4gdlein voller Bangigkeit;", "tokens": ["Das", "M\u00e4gd\u00b7lein", "vol\u00b7ler", "Ban\u00b7gig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am ", "tokens": ["Am"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Von oben zagt die Weiblichkeit.", "tokens": ["Von", "o\u00b7ben", "zagt", "die", "Weib\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, sprach sie, grausig geh'n Gespenster", "tokens": ["Ach", ",", "sprach", "sie", ",", "grau\u00b7sig", "geh'n", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort oben auf der Ritter Grab,", "tokens": ["Dort", "o\u00b7ben", "auf", "der", "Rit\u00b7ter", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wilder Mann blickt aus dem Fenster", "tokens": ["Ein", "wil\u00b7der", "Mann", "blickt", "aus", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der H\u00f6he f\u00fcrchterlich herab.", "tokens": ["Der", "H\u00f6\u00b7he", "f\u00fcrch\u00b7ter\u00b7lich", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Todten ruh'n - so sprach Babeno,", "tokens": ["Die", "Tod\u00b7ten", "ruh'n", "so", "sprach", "Ba\u00b7be\u00b7no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort oben ist ", "tokens": ["Dort", "o\u00b7ben", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Menschen w\u00fcrgt. Komm, la\u00df uns gehn o", "tokens": ["Der", "Men\u00b7schen", "w\u00fcrgt", ".", "Komm", ",", "la\u00df", "uns", "gehn", "o"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "FM"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Den sanftgewundnen Pfad hinan!", "tokens": ["Den", "sanft\u00b7ge\u00b7wund\u00b7nen", "Pfad", "hi\u00b7nan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie wandelte durch schwarze G\u00e4nge", "tokens": ["Sie", "wan\u00b7del\u00b7te", "durch", "schwar\u00b7ze", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An ihn geschmiegt und minnetraut,", "tokens": ["An", "ihn", "ge\u00b7schmiegt", "und", "min\u00b7ne\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dort in die hohe Felsenenge,", "tokens": ["Dort", "in", "die", "ho\u00b7he", "Fel\u00b7sen\u00b7en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Schreckni\u00df den Ruin umgraut.", "tokens": ["Wo", "Schrec\u00b7kni\u00df", "den", "Ru\u00b7in", "um\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Hu da! - aus einer offnen Nische", "tokens": ["Hu", "da", "!", "aus", "ei\u00b7ner", "off\u00b7nen", "Ni\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Springt pfeilschnell eine Blutgestalt,", "tokens": ["Springt", "pfeil\u00b7schnell", "ei\u00b7ne", "Blut\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jungfrau bebt in das Geb\u00fcsche,", "tokens": ["Die", "Jung\u00b7frau", "bebt", "in", "das", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ihr ", "tokens": ["Da", "ihr"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Wer seid Ihr, da\u00df Ihr mich zu st\u00f6ren", "tokens": ["Wer", "seid", "Ihr", ",", "da\u00df", "Ihr", "mich", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesen Stunden Euch erfrecht?", "tokens": ["In", "die\u00b7sen", "Stun\u00b7den", "Euch", "er\u00b7frecht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Frevels S\u00fcnde sollt ihr h\u00f6ren", "tokens": ["Des", "Fre\u00b7vels", "S\u00fcn\u00b7de", "sollt", "ihr", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlen, wie ein Geist sich r\u00e4cht!", "tokens": ["Und", "f\u00fch\u00b7len", ",", "wie", "ein", "Geist", "sich", "r\u00e4cht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sein Schwert zieht k\u00fchnlich stracks Babeno", "tokens": ["Sein", "Schwert", "zieht", "k\u00fchn\u00b7lich", "stracks", "Ba\u00b7be\u00b7no"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "St\u00fcrzt, aufgeregt von Heldenmuth,", "tokens": ["St\u00fcrzt", ",", "auf\u00b7ge\u00b7regt", "von", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entgegen sich dem Nachtgespeno -", "tokens": ["Ent\u00b7ge\u00b7gen", "sich", "dem", "Nacht\u00b7ge\u00b7spe\u00b7no"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unhold sinkt - es str\u00f6mt sein ", "tokens": ["Der", "Un\u00b7hold", "sinkt", "es", "str\u00f6mt", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und Todesangst umfa\u00dft im Mieder", "tokens": ["Und", "To\u00b7de\u00b7sangst", "um\u00b7fa\u00dft", "im", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des M\u00e4dchens Herz - der Seele los", "tokens": ["Des", "M\u00e4d\u00b7chens", "Herz", "der", "See\u00b7le", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00e4llt es, vom Schlag", "tokens": ["F\u00e4llt", "es", ",", "vom", "Schlag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Auf's mordumflossne Felsenmoos", "tokens": ["Auf's", "mor\u00b7dum\u00b7floss\u00b7ne", "Fel\u00b7sen\u00b7moos"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Da wirft Babeno weg den Stahle", "tokens": ["Da", "wirft", "Ba\u00b7be\u00b7no", "weg", "den", "Stah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eilt zu der zerst\u00f6rten Braut; -", "tokens": ["Und", "eilt", "zu", "der", "zer\u00b7st\u00f6r\u00b7ten", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, ausgerungen hat die Fahle,", "tokens": ["Ach", ",", "aus\u00b7ge\u00b7run\u00b7gen", "hat", "die", "Fah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Todesschwei\u00df umrinnt die Haut.", "tokens": ["Nur", "To\u00b7des\u00b7schwei\u00df", "um\u00b7rinnt", "die", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie? - ", "tokens": ["Wie", "?"], "token_info": ["word", "punct", "word"], "pos": ["PWAV", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Gerechter Gott! - entrissen mir?", "tokens": ["Ge\u00b7rech\u00b7ter", "Gott", "!", "ent\u00b7ris\u00b7sen", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ruft er und ringt empor die Seele,", "tokens": ["Ruft", "er", "und", "ringt", "em\u00b7por", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, Traute, mich trennt Nichts", "tokens": ["Nein", ",", "Trau\u00b7te", ",", "mich", "trennt", "Nichts"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nicht Schicksal, nicht des Todes Pfeile", "tokens": ["Nicht", "Schick\u00b7sal", ",", "nicht", "des", "To\u00b7des", "Pfei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entrei\u00dfen mir dein treues Herz,", "tokens": ["Ent\u00b7rei\u00b7\u00dfen", "mir", "dein", "treu\u00b7es", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich folge dir -! Auf Erden heile", "tokens": ["Ich", "fol\u00b7ge", "dir", "!", "Auf", "Er\u00b7den", "hei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$.", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Er sprach's und nimmt das Schwert verletzlich,", "tokens": ["Er", "sprach's", "und", "nimmt", "das", "Schwert", "ver\u00b7letz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das noch vom Blut des Unholds raucht,", "tokens": ["Das", "noch", "vom", "Blut", "des", "Un\u00b7holds", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchbohrt die treue Brust, und pl\u00f6tzlich", "tokens": ["Durch\u00b7bohrt", "die", "treu\u00b7e", "Brust", ",", "und", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist seine Seele schon verhaucht.", "tokens": ["Ist", "sei\u00b7ne", "See\u00b7le", "schon", "ver\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "So starb Baben' und Gabriele,", "tokens": ["So", "starb", "Baben'", "und", "Ga\u00b7bri\u00b7e\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {