{"textgrid.poem.58828": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "47. Vertrauen auf Gott", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gutes tut und trauet dann,", "tokens": ["Wer", "Gu\u00b7tes", "tut", "und", "trau\u00b7et", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn er fehlt, nicht trauen kann:", "tokens": ["Und", "wenn", "er", "fehlt", ",", "nicht", "trau\u00b7en", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein solch Vertrauen ist nicht reine;", "tokens": ["Ein", "solch", "Ver\u00b7trau\u00b7en", "ist", "nicht", "rei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Glaube sieht auf Gott alleine.", "tokens": ["Der", "Glau\u00b7be", "sieht", "auf", "Gott", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}