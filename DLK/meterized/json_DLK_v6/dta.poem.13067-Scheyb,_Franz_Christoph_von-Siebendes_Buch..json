{"dta.poem.13067": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wei\u00df, da\u00df ihr ihn auch so wohl, als sie besizet;", "tokens": ["\u201e", "ich", "wei\u00df", ",", "da\u00df", "ihr", "ihn", "auch", "so", "wohl", ",", "als", "sie", "be\u00b7si\u00b7zet", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "620\u201dDa\u00df er den Tugend-Chor so wohl als sie besch\u00fczet.", "tokens": ["\"", "Da\u00df", "er", "den", "Tu\u00b7gen\u00b7dChor", "so", "wohl", "als", "sie", "be\u00b7sch\u00fc\u00b7zet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ADV", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ebey diesem bleiben wir; den sezen wir uns vor;", "tokens": ["\u201e", "bey", "die\u00b7sem", "blei\u00b7ben", "wir", ";", "den", "se\u00b7zen", "wir", "uns", "vor", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "VVFIN", "PPER", "$.", "ART", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201emir scheint, ", "tokens": ["\u201e", "mir", "scheint", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "\u201eda\u00df das, was man vor ihn zum Ruhm, zur Ehr errichte,", "tokens": ["\u201e", "da\u00df", "das", ",", "was", "man", "vor", "ihn", "zum", "Ruhm", ",", "zur", "Ehr", "er\u00b7rich\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PDS", "$,", "PRELS", "PIS", "APPR", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso viel ihr eignes Herz, als selber ihn verpflichte.", "tokens": ["\u201e", "so", "viel", "ihr", "eig\u00b7nes", "Herz", ",", "als", "sel\u00b7ber", "ihn", "ver\u00b7pflich\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}