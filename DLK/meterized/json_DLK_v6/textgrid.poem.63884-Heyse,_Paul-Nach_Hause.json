{"textgrid.poem.63884": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Nach Hause", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebste, da ich heut im Regenzwielicht", "tokens": ["Liebs\u00b7te", ",", "da", "ich", "heut", "im", "Re\u00b7gen\u00b7zwie\u00b7licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Von dir ging, noch unsern Buben herzte,", "tokens": ["Von", "dir", "ging", ",", "noch", "un\u00b7sern", "Bu\u00b7ben", "herz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die gro\u00dfen M\u00e4dchen sehr verschlafen", "tokens": ["Und", "die", "gro\u00b7\u00dfen", "M\u00e4d\u00b7chen", "sehr", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir zum Abschied Mund und Wange boten,", "tokens": ["Mir", "zum", "Ab\u00b7schied", "Mund", "und", "Wan\u00b7ge", "bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dann der Morgenwind mit frost'gem Schauer,", "tokens": ["Dann", "der", "Mor\u00b7gen\u00b7wind", "mit", "frost'\u00b7gem", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Gar nicht lenzhaft, mein Gesicht umspr\u00fchte \u2013", "tokens": ["Gar", "nicht", "lenz\u00b7haft", ",", "mein", "Ge\u00b7sicht", "um\u00b7spr\u00fch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sinnlos schien ich mir und aberwitzig,", "tokens": ["Sinn\u00b7los", "schien", "ich", "mir", "und", "a\u00b7berw\u00b7it\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df ich fortging, weil der Arzt geraten,", "tokens": ["Da\u00df", "ich", "fort\u00b7ging", ",", "weil", "der", "Arzt", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Alten Gram in neuer Luft zu heilen.", "tokens": ["Al\u00b7ten", "Gram", "in", "neu\u00b7er", "Luft", "zu", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Der Gesundheit, ach, des Friedens Quelle,", "tokens": ["Der", "Ge\u00b7sund\u00b7heit", ",", "ach", ",", "des", "Frie\u00b7dens", "Quel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Flie\u00dft sie einzig nicht im Bann des Hauses?", "tokens": ["Flie\u00dft", "sie", "ein\u00b7zig", "nicht", "im", "Bann", "des", "Hau\u00b7ses", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Auch der sch\u00f6nsten Ferne fremdes Treiben,", "tokens": ["Auch", "der", "sch\u00f6ns\u00b7ten", "Fer\u00b7ne", "frem\u00b7des", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Farbenbunt gesch\u00e4ftig Weltgewimmel \u2013", "tokens": ["Far\u00b7ben\u00b7bunt", "ge\u00b7sch\u00e4f\u00b7tig", "Welt\u00b7ge\u00b7wim\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Was dem wunden Herzen kann es bieten,", "tokens": ["Was", "dem", "wun\u00b7den", "Her\u00b7zen", "kann", "es", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Das ihm besser nicht daheim erbl\u00fchte?", "tokens": ["Das", "ihm", "bes\u00b7ser", "nicht", "da\u00b7heim", "er\u00b7bl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und so dr\u00fcckt' ich, trutzend und verdrossen,", "tokens": ["Und", "so", "dr\u00fcckt'", "ich", ",", "trut\u00b7zend", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Fest mich in den dumpfen Fensterwinkel,", "tokens": ["Fest", "mich", "in", "den", "dum\u00b7pfen", "Fens\u00b7ter\u00b7win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Scheinbar schlafend. Doch den Schein benutzte", "tokens": ["Schein\u00b7bar", "schla\u00b7fend", ".", "Doch", "den", "Schein", "be\u00b7nutz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ein vergn\u00fcglich Flitterwochenp\u00e4rchen,", "tokens": ["Ein", "ver\u00b7gn\u00fcg\u00b7lich", "Flit\u00b7ter\u00b7wo\u00b7chen\u00b7p\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Das der Kellner im Hotel vermutlich", "tokens": ["Das", "der", "Kell\u00b7ner", "im", "Ho\u00b7tel", "ver\u00b7mut\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Allzufr\u00fch geweckt aus Liebestr\u00e4umen.", "tokens": ["All\u00b7zu\u00b7fr\u00fch", "ge\u00b7weckt", "aus", "Lie\u00b7be\u00b7str\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Denn sie hatten viel geheime Dinge", "tokens": ["Denn", "sie", "hat\u00b7ten", "viel", "ge\u00b7hei\u00b7me", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Sich ins Ohr zu fl\u00fcstern, sich die H\u00e4nde", "tokens": ["Sich", "ins", "Ohr", "zu", "fl\u00fcs\u00b7tern", ",", "sich", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Zu zerdr\u00fccken, und im Tunnel vollends", "tokens": ["Zu", "zer\u00b7dr\u00fc\u00b7cken", ",", "und", "im", "Tun\u00b7nel", "vol\u00b7lends"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "H\u00f6rt' ich's wie verstohlne K\u00fcsse zwitschern.", "tokens": ["H\u00f6rt'", "ich's", "wie", "ver\u00b7stohl\u00b7ne", "K\u00fcs\u00b7se", "zwit\u00b7schern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Ich zwar, \u00fcber solche Menschlichkeiten", "tokens": ["Ich", "zwar", ",", "\u00fc\u00b7ber", "sol\u00b7che", "Menschlich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.27": {"text": "Milde denkend, dr\u00fcckte gern ein Auge", "tokens": ["Mil\u00b7de", "den\u00b7kend", ",", "dr\u00fcck\u00b7te", "gern", "ein", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Dr\u00fcber zu, und blinzelnd mit dem andern", "tokens": ["Dr\u00fc\u00b7ber", "zu", ",", "und", "blin\u00b7zelnd", "mit", "dem", "an\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$,", "KON", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Sah ich in die glimmende Morgenr\u00f6te.", "tokens": ["Sah", "ich", "in", "die", "glim\u00b7men\u00b7de", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Doch mein Zartsinn half nicht lang. Ein zweites", "tokens": ["Doch", "mein", "Zar\u00b7tsinn", "half", "nicht", "lang", ".", "Ein", "zwei\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "$.", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "P\u00e4rchen stieg mit Kind und Kinderw\u00e4rtrin", "tokens": ["P\u00e4r\u00b7chen", "stieg", "mit", "Kind", "und", "Kin\u00b7der\u00b7w\u00e4r\u00b7trin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Zu uns ein, die Frucht der Honigwochen", "tokens": ["Zu", "uns", "ein", ",", "die", "Frucht", "der", "Ho\u00b7nig\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Zappelnd auf dem Scho\u00df, und nicht zum Kosen,", "tokens": ["Zap\u00b7pelnd", "auf", "dem", "Scho\u00df", ",", "und", "nicht", "zum", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Nicht mehr aufgelegt zum H\u00e4ndedr\u00fccken,", "tokens": ["Nicht", "mehr", "auf\u00b7ge\u00b7legt", "zum", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Da das \u00c4ffchen steter Wartung brauchte.", "tokens": ["Da", "das", "\u00c4ff\u00b7chen", "ste\u00b7ter", "War\u00b7tung", "brauch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Es dem guten Gro\u00dfpapa zu zeigen,", "tokens": ["Es", "dem", "gu\u00b7ten", "Gro\u00df\u00b7pa\u00b7pa", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Willig litten sie die Reiseplage,", "tokens": ["Wil\u00b7lig", "lit\u00b7ten", "sie", "die", "Rei\u00b7se\u00b7pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Mit unendlichem Ger\u00e4t beladen,", "tokens": ["Mit", "un\u00b7end\u00b7li\u00b7chem", "Ge\u00b7r\u00e4t", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Fl\u00e4schchen, T\u00f6pfchen, Klapper, M\u00fcschenpfanne,", "tokens": ["Fl\u00e4\u00b7schchen", ",", "T\u00f6pf\u00b7chen", ",", "Klap\u00b7per", ",", "M\u00fc\u00b7schen\u00b7pfan\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "\u00dcbersorglich stets, doch \u00fcberselig,", "tokens": ["\u00dc\u00b7bers\u00b7org\u00b7lich", "stets", ",", "doch", "\u00fc\u00b7ber\u00b7se\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Weil der goldne Liebling sehr manierlich", "tokens": ["Weil", "der", "gold\u00b7ne", "Lieb\u00b7ling", "sehr", "ma\u00b7nier\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Sich betrug und jedermann ihn lobte.", "tokens": ["Sich", "be\u00b7trug", "und", "je\u00b7der\u00b7mann", "ihn", "lob\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Wieder Halt, und neue Menschen. Tr\u00e4llernd", "tokens": ["Wie\u00b7der", "Halt", ",", "und", "neu\u00b7e", "Men\u00b7schen", ".", "Tr\u00e4l\u00b7lernd"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "KON", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Steigt ein muntrer junger Mann, in flottem", "tokens": ["Steigt", "ein", "mun\u00b7trer", "jun\u00b7ger", "Mann", ",", "in", "flot\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Gro\u00dfkarriertem Anzug, in den Wagen,", "tokens": ["Gro\u00df\u00b7kar\u00b7rier\u00b7tem", "An\u00b7zug", ",", "in", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Bald mit seinem \u00e4ltern Gegen\u00fcber", "tokens": ["Bald", "mit", "sei\u00b7nem", "\u00e4l\u00b7tern", "Ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "In Gespr\u00e4ch vertieft und rascher Freundschaft.", "tokens": ["In", "Ge\u00b7spr\u00e4ch", "ver\u00b7tieft", "und", "ra\u00b7scher", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Beide handelten mit gemischten Waren,", "tokens": ["Bei\u00b7de", "han\u00b7del\u00b7ten", "mit", "ge\u00b7mischten", "Wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.49": {"text": "Und nachdem der J\u00fcngre stolz berichtet,", "tokens": ["Und", "nach\u00b7dem", "der", "J\u00fcng\u00b7re", "stolz", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Heut in Koburg harre sein das Br\u00e4utchen,", "tokens": ["Heut", "in", "Ko\u00b7burg", "har\u00b7re", "sein", "das", "Br\u00e4ut\u00b7chen", ","], "token_info": ["word",