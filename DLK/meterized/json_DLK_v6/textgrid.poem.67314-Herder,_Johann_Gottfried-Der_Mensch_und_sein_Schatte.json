{"textgrid.poem.67314": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch und sein Schatte", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsage, was hab' ich mit Dir?", "tokens": ["\u00bb", "sa\u00b7ge", ",", "was", "hab'", "ich", "mit", "Dir", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist vor und hinter mir,", "tokens": ["Du", "bist", "vor", "und", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "KON", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oeder Schatte, schwarzer Geist,", "tokens": ["O\u00b7e\u00b7der", "Schat\u00b7te", ",", "schwar\u00b7zer", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mein Nichts mir immer weist.\u00ab", "tokens": ["Der", "mein", "Nichts", "mir", "im\u00b7mer", "weist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbtadelst Du, o Freund, ein Bild,", "tokens": ["\u00bb", "ta\u00b7delst", "Du", ",", "o", "Freund", ",", "ein", "Bild", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Dein Wesen Dir enth\u00fcllt?", "tokens": ["Das", "Dein", "We\u00b7sen", "Dir", "ent\u00b7h\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne jenes Lichtes Bahn", "tokens": ["Oh\u00b7ne", "je\u00b7nes", "Lich\u00b7tes", "Bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist Du Schatte um und an.", "tokens": ["Bist", "Du", "Schat\u00b7te", "um", "und", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Steht die Sonne dort vor Dir,", "tokens": ["Steht", "die", "Son\u00b7ne", "dort", "vor", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleich' ich hinterm R\u00fccken hier;", "tokens": ["Schleich'", "ich", "hin\u00b7term", "R\u00fc\u00b7cken", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird sie Dir im R\u00fccken stehn,", "tokens": ["Wird", "sie", "Dir", "im", "R\u00fc\u00b7cken", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird Dein Schatte vor Dir gehn.", "tokens": ["Wird", "Dein", "Schat\u00b7te", "vor", "Dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deines Lebens Sonnenlicht", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist Vernunft; die fliehe nicht!", "tokens": ["Ist", "Ver\u00b7nunft", ";", "die", "flie\u00b7he", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "ART", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird sie Dir im R\u00fccken stehn,", "tokens": ["Wird", "sie", "Dir", "im", "R\u00fc\u00b7cken", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird Dein Schatte vor Dir gehn.\u00ab", "tokens": ["Wird", "Dein", "Schat\u00b7te", "vor", "Dir", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}