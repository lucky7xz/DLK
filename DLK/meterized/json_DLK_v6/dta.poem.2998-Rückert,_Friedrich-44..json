{"dta.poem.2998": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist unwandelbar als Wahrheit ausgemacht?", "tokens": ["Was", "ist", "un\u00b7wan\u00b7del\u00b7bar", "als", "Wahr\u00b7heit", "aus\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Allem Nichts f\u00fcrwahr, was Menschenwitz erdacht.", "tokens": ["Von", "Al\u00b7lem", "Nichts", "f\u00fcr\u00b7wahr", ",", "was", "Men\u00b7schen\u00b7witz", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIS", "ADV", "$,", "PWS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wunder der Natur, die Thaten der Geschichte,", "tokens": ["Die", "Wun\u00b7der", "der", "Na\u00b7tur", ",", "die", "Tha\u00b7ten", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheinen jeden Tag dem Geist in neuem Lichte.", "tokens": ["Er\u00b7schei\u00b7nen", "je\u00b7den", "Tag", "dem", "Geist", "in", "neu\u00b7em", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie dort Erscheinungen und hier Er\u00e4ugnisse,", "tokens": ["Wie", "dort", "Er\u00b7schei\u00b7nun\u00b7gen", "und", "hier", "Er\u00b7\u00e4ug\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wechseln Meinungen und Ueberzeugnisse.", "tokens": ["So", "wech\u00b7seln", "Mei\u00b7nun\u00b7gen", "und", "Ue\u00b7ber\u00b7zeug\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Glaubensbekenntnisse und Wissenschaftsgeb\u00e4ude,", "tokens": ["Glau\u00b7bens\u00b7be\u00b7kennt\u00b7nis\u00b7se", "und", "Wis\u00b7sen\u00b7schafts\u00b7ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Des ewig wandelnden Weltgeistes Spiel und Freude.", "tokens": ["Des", "e\u00b7wig", "wan\u00b7deln\u00b7den", "Welt\u00b7geis\u00b7tes", "Spiel", "und", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Du aber la\u00df, was ihn erfreut, dich nicht betr\u00fcben!", "tokens": ["Du", "a\u00b7ber", "la\u00df", ",", "was", "ihn", "er\u00b7freut", ",", "dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er spielt sein Spiel mit dir, um deine Kraft zu \u00fcben.", "tokens": ["Er", "spielt", "sein", "Spiel", "mit", "dir", ",", "um", "dei\u00b7ne", "Kraft", "zu", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo ihn dein Ringen hat mit geist'ger Form gebunden,", "tokens": ["Wo", "ihn", "dein", "Rin\u00b7gen", "hat", "mit", "geist'\u00b7ger", "Form", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da hast du Wahrheit f\u00fcr den Augenblick gefunden.", "tokens": ["Da", "hast", "du", "Wahr\u00b7heit", "f\u00fcr", "den", "Au\u00b7gen\u00b7blick", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}