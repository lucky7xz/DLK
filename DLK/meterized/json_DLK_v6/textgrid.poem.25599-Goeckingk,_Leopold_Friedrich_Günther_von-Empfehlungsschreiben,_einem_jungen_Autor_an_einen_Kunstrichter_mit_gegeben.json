{"textgrid.poem.25599": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Empfehlungsschreiben, einem jungen Autor an einen Kunstrichter mit gegeben", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ueberbringer ist ein Thor,", "tokens": ["Der", "Ue\u00b7ber\u00b7brin\u00b7ger", "ist", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Plagt mich, ihn zu recommendiren,", "tokens": ["Plagt", "mich", ",", "ihn", "zu", "re\u00b7com\u00b7men\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat bei sich Hundert Friedrichsd'or,", "tokens": ["Hat", "bei", "sich", "Hun\u00b7dert", "Fried\u00b7richs\u00b7d'\u00b7or", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "NE", "NE", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und bittet, ihn zu recensiren.", "tokens": ["Und", "bit\u00b7tet", ",", "ihn", "zu", "re\u00b7cen\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}