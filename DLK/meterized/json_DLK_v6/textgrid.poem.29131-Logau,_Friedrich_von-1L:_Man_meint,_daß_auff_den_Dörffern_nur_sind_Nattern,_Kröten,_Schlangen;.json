{"textgrid.poem.29131": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man meint, da\u00df auff den D\u00f6rffern nur sind Nattern, Kr\u00f6ten, Schlangen;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man meint, da\u00df auff den D\u00f6rffern nur sind Nattern, Kr\u00f6ten, Schlangen;", "tokens": ["Man", "meint", ",", "da\u00df", "auff", "den", "D\u00f6rf\u00b7fern", "nur", "sind", "Nat\u00b7tern", ",", "Kr\u00f6\u00b7ten", ",", "Schlan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mit diesen W\u00fcrmen ist man mehr in St\u00e4dten noch befangen.", "tokens": ["Mit", "die\u00b7sen", "W\u00fcr\u00b7men", "ist", "man", "mehr", "in", "St\u00e4d\u00b7ten", "noch", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIS", "ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Dort weichen sie, wann sie man jagt und fliehen in die L\u00f6cher;", "tokens": ["Dort", "wei\u00b7chen", "sie", ",", "wann", "sie", "man", "jagt", "und", "flie\u00b7hen", "in", "die", "L\u00f6\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Hier finden sie sich um uns her, im Hau\u00df, auff Ga\u00df, in Gl\u00e4chern.", "tokens": ["Hier", "fin\u00b7den", "sie", "sich", "um", "uns", "her", ",", "im", "Hau\u00df", ",", "auff", "Ga\u00df", ",", "in", "Gl\u00e4\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "$,", "APPRART", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}