{"textgrid.poem.26742": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zweierlei Genien sinds, die dich durchs Leben geleiten,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zweierlei Genien sinds, die dich durchs Leben geleiten,", "tokens": ["Zwei\u00b7er\u00b7lei", "Ge\u00b7ni\u00b7en", "sinds", ",", "die", "dich", "durchs", "Le\u00b7ben", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wohl dir, wenn sie vereint helfend zur Seite dir stehn!", "tokens": ["Wohl", "dir", ",", "wenn", "sie", "ver\u00b7eint", "hel\u00b7fend", "zur", "Sei\u00b7te", "dir", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit erheiterndem Spiel verk\u00fcrzt dir der eine die Reise,", "tokens": ["Mit", "er\u00b7hei\u00b7tern\u00b7dem", "Spiel", "ver\u00b7k\u00fcrzt", "dir", "der", "ei\u00b7ne", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ART", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Leichter an seinem Arm werden dir Schicksal und Pflicht.", "tokens": ["Leich\u00b7ter", "an", "sei\u00b7nem", "Arm", "wer\u00b7den", "dir", "Schick\u00b7sal", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Unter Scherz und Gespr\u00e4ch begleitet er bis an die Kluft dich,", "tokens": ["Un\u00b7ter", "Scherz", "und", "Ge\u00b7spr\u00e4ch", "be\u00b7glei\u00b7tet", "er", "bis", "an", "die", "Kluft", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PPER", "$,"], "meter": "--+--+-+--++-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wo an der Ewigkeit Meer schaudernd der Sterbliche steht.", "tokens": ["Wo", "an", "der", "E\u00b7wig\u00b7keit", "Meer", "schau\u00b7dernd", "der", "Sterb\u00b7li\u00b7che", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Hier empf\u00e4ngt dich entschlossen und ernst und schweigend der andre,", "tokens": ["Hier", "emp\u00b7f\u00e4ngt", "dich", "ent\u00b7schlos\u00b7sen", "und", "ernst", "und", "schwei\u00b7gend", "der", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "KON", "ADJD", "ART", "ADJA", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Tr\u00e4gt mit gigantischem Arm \u00fcber die Tiefe dich hin.", "tokens": ["Tr\u00e4gt", "mit", "gi\u00b7gan\u00b7ti\u00b7schem", "Arm", "\u00fc\u00b7ber", "die", "Tie\u00b7fe", "dich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nimmer widme dich einem allein. Vertraue dem erstern", "tokens": ["Nim\u00b7mer", "wid\u00b7me", "dich", "ei\u00b7nem", "al\u00b7lein", ".", "Ver\u00b7trau\u00b7e", "dem", "ers\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "$.", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Deine W\u00fcrde nicht an, nimmer dem andern dein Gl\u00fcck.", "tokens": ["Dei\u00b7ne", "W\u00fcr\u00b7de", "nicht", "an", ",", "nim\u00b7mer", "dem", "an\u00b7dern", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}