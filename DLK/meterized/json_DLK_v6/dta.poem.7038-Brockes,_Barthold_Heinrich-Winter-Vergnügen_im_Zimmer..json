{"dta.poem.7038": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Vergn\u00fcgen  \n im Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann draussen die erstarrte Welt", "tokens": ["Wann", "draus\u00b7sen", "die", "er\u00b7starr\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit scharffem Frost, der dunckle Winter schrecket,", "tokens": ["Mit", "scharf\u00b7fem", "Frost", ",", "der", "dunck\u00b7le", "Win\u00b7ter", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn schroffes Eis das harte Feld", "tokens": ["Wenn", "schrof\u00b7fes", "Eis", "das", "har\u00b7te", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit rauhen Schollen dr\u00fcckt und decket;", "tokens": ["Mit", "rau\u00b7hen", "Schol\u00b7len", "dr\u00fcckt", "und", "de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verg\u00f6nnet mir des Sch\u00f6pffers G\u00fcte,", "tokens": ["Ver\u00b7g\u00f6n\u00b7net", "mir", "des", "Sch\u00f6pf\u00b7fers", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df, mit Bequemlichkeit und ruhigem Gem\u00fcthe,", "tokens": ["Da\u00df", ",", "mit", "Be\u00b7quem\u00b7lich\u00b7keit", "und", "ru\u00b7hi\u00b7gem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ich ein vergn\u00fcglich Feur, in meinem Zimmer,", "tokens": ["Ich", "ein", "ver\u00b7gn\u00fcg\u00b7lich", "Feur", ",", "in", "mei\u00b7nem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den w\u00e4rmenden Camin mit Lust erleuchten seh.", "tokens": ["Den", "w\u00e4r\u00b7men\u00b7den", "Ca\u00b7min", "mit", "Lust", "er\u00b7leuch\u00b7ten", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es steigt der regen Flammen Schimmer", "tokens": ["Es", "steigt", "der", "re\u00b7gen", "Flam\u00b7men", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Roth, gelb und wei\u00df zugleich, hell-lodernd in die H\u00f6h;", "tokens": ["Roth", ",", "gelb", "und", "wei\u00df", "zu\u00b7gleich", ",", "hell\u00b7lo\u00b7dernd", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "ADV", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wovon durch jeden Sinn, der ihre Krafft versp\u00fcret,", "tokens": ["Wo\u00b7von", "durch", "je\u00b7den", "Sinn", ",", "der", "ih\u00b7re", "Krafft", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich Freuden-voll erquickt werd\u2019 und ger\u00fchret.", "tokens": ["Ich", "Freu\u00b7den\u00b7voll", "er\u00b7quickt", "werd'", "und", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich sehe die getheilten Spitzen,", "tokens": ["Ich", "se\u00b7he", "die", "ge\u00b7theil\u00b7ten", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Um f\u00fcr den scharffen Frost mich gleichsam zu besch\u00fctzen", "tokens": ["Um", "f\u00fcr", "den", "scharf\u00b7fen", "Frost", "mich", "gleich\u00b7sam", "zu", "be\u00b7sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit reger Aemsigkeit sich aufwerts schwingen.", "tokens": ["Mit", "re\u00b7ger", "A\u00b7em\u00b7sig\u00b7keit", "sich", "auf\u00b7werts", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.16": {"text": "Ich f\u00fchle durch die starre Brust", "tokens": ["Ich", "f\u00fch\u00b7le", "durch", "die", "star\u00b7re", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein sanfftes Wohl, und eine laue Lust", "tokens": ["Ein", "sanff\u00b7tes", "Wohl", ",", "und", "ei\u00b7ne", "lau\u00b7e", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "In meinen gantzen C\u00f6rper dringen,", "tokens": ["In", "mei\u00b7nen", "gant\u00b7zen", "C\u00f6r\u00b7per", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und, was durch K\u00e4lte starr, erfrischen.", "tokens": ["Und", ",", "was", "durch", "K\u00e4l\u00b7te", "starr", ",", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPR", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ich h\u00f6r' ein muntres Rauschen, Zischen,", "tokens": ["Ich", "h\u00f6r'", "ein", "mun\u00b7tres", "Rau\u00b7schen", ",", "Zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein durch die schnelle Loh erregtes Schallen,", "tokens": ["Ein", "durch", "die", "schnel\u00b7le", "Loh", "er\u00b7reg\u00b7tes", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mit offtmahls unterbrochnen Knallen,", "tokens": ["Mit", "offt\u00b7mahls", "un\u00b7ter\u00b7broch\u00b7nen", "Knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der in dem Holtz verschr\u00e4nckt- und schnell befreyten Lufft,", "tokens": ["Der", "in", "dem", "Holtz", "ver\u00b7schr\u00e4n\u00b7ck\u00b7t", "und", "schnell", "be\u00b7frey\u00b7ten", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "TRUNC", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Wodurch, bald hier bald dort, gesprengte Funcken fliegen.", "tokens": ["Wo\u00b7durch", ",", "bald", "hier", "bald", "dort", ",", "ge\u00b7spreng\u00b7te", "Fun\u00b7cken", "flie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "ADV", "ADV", "ADV", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich rieche den gesunden Dufft", "tokens": ["Ich", "rie\u00b7che", "den", "ge\u00b7sun\u00b7den", "Dufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der fetten Fichten mit Vergn\u00fcgen.", "tokens": ["Der", "fet\u00b7ten", "Fich\u00b7ten", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Es schmeckt bey dieser Zeit das holde Kraut, der Thee,", "tokens": ["Es", "schmeckt", "bey", "die\u00b7ser", "Zeit", "das", "hol\u00b7de", "Kraut", ",", "der", "Thee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den ich in gr\u00fcner Farb\u2019 in feinen Sch\u00e4lchen seh\u2019,", "tokens": ["Den", "ich", "in", "gr\u00fc\u00b7ner", "Fa\u00b7rb'", "in", "fei\u00b7nen", "Sch\u00e4l\u00b7chen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Den kalten Lippen wol. Bald w\u00e4rmt ein heisser Wein,", "tokens": ["Den", "kal\u00b7ten", "Lip\u00b7pen", "wol", ".", "Bald", "w\u00e4rmt", "ein", "heis\u00b7ser", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Voll s\u00fcsser S\u00e4urlichkeit und Lust, Hand, Mund und Magen.", "tokens": ["Voll", "s\u00fcs\u00b7ser", "S\u00e4ur\u00b7lich\u00b7keit", "und", "Lust", ",", "Hand", ",", "Mund", "und", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man sieht mit Lust zu Tische tragen", "tokens": ["Man", "sieht", "mit", "Lust", "zu", "Ti\u00b7sche", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Castanien, die s\u00fcsse Winter-Kost;", "tokens": ["Cas\u00b7ta\u00b7ni\u00b7en", ",", "die", "s\u00fcs\u00b7se", "Win\u00b7ter\u00b7Kost", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und was der Anmuth mehr, die auch im strengen Frost", "tokens": ["Und", "was", "der", "An\u00b7muth", "mehr", ",", "die", "auch", "im", "stren\u00b7gen", "Frost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Uns unser GOTT, der liebe Vater, schencket.", "tokens": ["Uns", "un\u00b7ser", "GoTT", ",", "der", "lie\u00b7be", "Va\u00b7ter", ",", "schen\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}