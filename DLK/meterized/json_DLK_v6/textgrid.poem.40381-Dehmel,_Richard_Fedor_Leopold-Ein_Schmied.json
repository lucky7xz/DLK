{"textgrid.poem.40381": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Schmied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Schmiedemeister werden;", "tokens": ["Ich", "kann", "Schmie\u00b7de\u00b7meis\u00b7ter", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "knuff! sagt mein Hammer und saust,", "tokens": ["knuff", "!", "sagt", "mein", "Ham\u00b7mer", "und", "saust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "dann springen die Funken vor Freude", "tokens": ["dann", "sprin\u00b7gen", "die", "Fun\u00b7ken", "vor", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "um meine ru\u00dfige Faust", "tokens": ["um", "mei\u00b7ne", "ru\u00b7\u00dfi\u00b7ge", "Faust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "bis an den Blasebalg.", "tokens": ["bis", "an", "den", "Bla\u00b7se\u00b7balg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Blasebalg, was st\u00f6hnst du?", "tokens": ["Herr", "Bla\u00b7se\u00b7balg", ",", "was", "st\u00f6hnst", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur zu! die Glut gesch\u00fcrt!", "tokens": ["Nur", "zu", "!", "die", "Glut", "ge\u00b7sch\u00fcrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df die Schlacken nur spucken,", "tokens": ["Und", "la\u00df", "die", "Schla\u00b7cken", "nur", "spu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wenn meine Zange drin r\u00fchrt;", "tokens": ["wenn", "mei\u00b7ne", "Zan\u00b7ge", "drin", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "gut Eisen will auf den Ambos!", "tokens": ["gut", "Ei\u00b7sen", "will", "auf", "den", "Am\u00b7bos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem soll ich den R\u00fccken klopfen,", "tokens": ["Dem", "soll", "ich", "den", "R\u00fc\u00b7cken", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dann lacht er und tr\u00e4llert ein Lied:", "tokens": ["dann", "lacht", "er", "und", "tr\u00e4l\u00b7lert", "ein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Lieb Hammergel\u00e4ut, lieb Hammergel\u00e4ut,", "tokens": ["Lieb", "Ham\u00b7mer\u00b7ge\u00b7l\u00e4ut", ",", "lieb", "Ham\u00b7mer\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "gut Eisen dankt dem Schmied,", "tokens": ["gut", "Ei\u00b7sen", "dankt", "dem", "Schmied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "er klopft es hart zu Stahl.", "tokens": ["er", "klopft", "es", "hart", "zu", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum streut's vor Freude Funken", "tokens": ["Drum", "streut's", "vor", "Freu\u00b7de", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und h\u00fcpft bei jedem Streich;", "tokens": ["und", "h\u00fcpft", "bei", "je\u00b7dem", "Streich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Heuchler und Halunken,", "tokens": ["die", "Heuch\u00b7ler", "und", "Ha\u00b7lun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die klopft er windelweich,", "tokens": ["die", "klopft", "er", "win\u00b7del\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "knuff, der kleine Held.", "tokens": ["knuff", ",", "der", "klei\u00b7ne", "Held", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}