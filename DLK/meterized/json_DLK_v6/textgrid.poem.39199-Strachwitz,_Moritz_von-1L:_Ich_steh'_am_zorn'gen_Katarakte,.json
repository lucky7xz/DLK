{"textgrid.poem.39199": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich steh' am zorn'gen Katarakte,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh' am zorn'gen Katarakte,", "tokens": ["Ich", "steh'", "am", "zorn'\u00b7gen", "Ka\u00b7ta\u00b7rak\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz ist still und traumbeschwert,", "tokens": ["Mein", "Herz", "ist", "still", "und", "traum\u00b7be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hirn ist m\u00fcd' vom Donnertakte,", "tokens": ["Mein", "Hirn", "ist", "m\u00fcd'", "vom", "Don\u00b7ner\u00b7tak\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Auge starr hinabgekehrt.", "tokens": ["Mein", "Au\u00b7ge", "starr", "hin\u00b7ab\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kann's nicht lassen, hinzustarren,", "tokens": ["Ich", "kann's", "nicht", "las\u00b7sen", ",", "hin\u00b7zu\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich die Woge ewig j\u00fcngt", "tokens": ["Wie", "sich", "die", "Wo\u00b7ge", "e\u00b7wig", "j\u00fcngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ewig in die Felsenbarren", "tokens": ["Und", "e\u00b7wig", "in", "die", "Fel\u00b7sen\u00b7bar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verzweiflungsvoll herniederspringt.", "tokens": ["Ver\u00b7zwei\u00b7flungs\u00b7voll", "her\u00b7nie\u00b7der\u00b7springt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein unabl\u00e4ssig Rollen,", "tokens": ["Es", "ist", "ein", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein nie verbrodelndes Gekoch',", "tokens": ["Ein", "nie", "ver\u00b7bro\u00b7deln\u00b7des", "Ge\u00b7koch'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit Ewigkeiten ist's erschollen,", "tokens": ["Seit", "E\u00b7wig\u00b7kei\u00b7ten", "ist's", "er\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ewigkeiten schallt es noch.", "tokens": ["Und", "E\u00b7wig\u00b7kei\u00b7ten", "schallt", "es", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du wilder Sohn des Felsenspaltes,", "tokens": ["Du", "wil\u00b7der", "Sohn", "des", "Fel\u00b7sen\u00b7spal\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Strom! Ich wei\u00df es, was Dich qu\u00e4lt,", "tokens": ["O", "Strom", "!", "Ich", "wei\u00df", "es", ",", "was", "Dich", "qu\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df ein Lied, ein ernstes, altes,", "tokens": ["Ich", "wei\u00df", "ein", "Lied", ",", "ein", "erns\u00b7tes", ",", "al\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir hat's die Fei am Quell erz\u00e4hlt: \u2013", "tokens": ["Mir", "hat's", "die", "Fei", "am", "Quell", "er\u00b7z\u00e4hlt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u2013 Zur Zeit der G\u00f6tter und der Riesen,", "tokens": ["\u2013", "Zur", "Zeit", "der", "G\u00f6t\u00b7ter", "und", "der", "Rie\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da str\u00f6mtest Du von Anbeginn", "tokens": ["Da", "str\u00f6m\u00b7test", "Du", "von", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In blumenreichen Paradiesen,", "tokens": ["In", "blu\u00b7men\u00b7rei\u00b7chen", "Pa\u00b7ra\u00b7die\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein g\u00f6ttergleicher Strom dahin.", "tokens": ["Ein", "g\u00f6t\u00b7ter\u00b7glei\u00b7cher", "Strom", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du aber warst ein trotziger St\u00fcrmer,", "tokens": ["Du", "a\u00b7ber", "warst", "ein", "trot\u00b7zi\u00b7ger", "St\u00fcr\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir frommte nicht der eb'ne Pfad,", "tokens": ["Dir", "fromm\u00b7te", "nicht", "der", "eb'\u00b7ne", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du w\u00e4rest gern, ein Berget\u00fcrmer,", "tokens": ["Du", "w\u00e4\u00b7rest", "gern", ",", "ein", "Ber\u00b7ge\u00b7t\u00fcr\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ew'gen G\u00f6ttern selbst genaht.", "tokens": ["Den", "ew'\u00b7gen", "G\u00f6t\u00b7tern", "selbst", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du wolltest k\u00fchn den Schleier heben,", "tokens": ["Du", "woll\u00b7test", "k\u00fchn", "den", "Schlei\u00b7er", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der von der Gottheit Scheitel rollt,", "tokens": ["Der", "von", "der", "Got\u00b7theit", "Schei\u00b7tel", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil Du's nicht erreicht im Leben,", "tokens": ["Und", "weil", "Du's", "nicht", "er\u00b7reicht", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So hast Du's durch den Tod gewollt.", "tokens": ["So", "hast", "Du's", "durch", "den", "Tod", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "APPR", "ART", "NN", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und aus dem Bette schwoll Dein Wasser,", "tokens": ["Und", "aus", "dem", "Bet\u00b7te", "schwoll", "Dein", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du warfest in dies Klippengrab,", "tokens": ["Du", "war\u00b7fest", "in", "dies", "Klip\u00b7pen\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein rasch entschloss'ner Lebenshasser,", "tokens": ["Ein", "rasch", "ent\u00b7schloss'\u00b7ner", "Le\u00b7bens\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selbstmordend, h\u00e4uptlings Dich hinab.", "tokens": ["Selbst\u00b7mor\u00b7dend", ",", "h\u00e4upt\u00b7lings", "Dich", "hin\u00b7ab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du warst der erste Erdenpilger,", "tokens": ["Du", "warst", "der", "ers\u00b7te", "Er\u00b7den\u00b7pil\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich zerst\u00f6rt' aus eig'ner Macht,", "tokens": ["Der", "sich", "zer\u00b7st\u00f6rt'", "aus", "eig'\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du warst der erste Selbstvertilger,", "tokens": ["Du", "warst", "der", "ers\u00b7te", "Selbst\u00b7ver\u00b7til\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der erste Selbstmord war vollbracht.", "tokens": ["Der", "ers\u00b7te", "Selbst\u00b7mord", "war", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und sahst Du nun erf\u00fcllt Dein Hoffen,", "tokens": ["Und", "sahst", "Du", "nun", "er\u00b7f\u00fcllt", "Dein", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sahst Du den Himmel, ward er Dein? \u2013", "tokens": ["Sahst", "Du", "den", "Him\u00b7mel", ",", "ward", "er", "Dein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch immer steht der Abgrund offen,", "tokens": ["Noch", "im\u00b7mer", "steht", "der", "Ab\u00b7grund", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer donnerst Du hinein.", "tokens": ["Noch", "im\u00b7mer", "don\u00b7nerst", "Du", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Das ist die Strafe von den G\u00f6ttern", "tokens": ["Das", "ist", "die", "Stra\u00b7fe", "von", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr die titanisch frevle Lust,", "tokens": ["F\u00fcr", "die", "ti\u00b7ta\u00b7nisch", "frev\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df im best\u00e4ndigen Zerschmettern", "tokens": ["Da\u00df", "im", "be\u00b7st\u00e4n\u00b7di\u00b7gen", "Zer\u00b7schmet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du doch best\u00e4ndig ", "tokens": ["Du", "doch", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Nie sah man Rast in Deinem Schlunde,", "tokens": ["Nie", "sah", "man", "Rast", "in", "Dei\u00b7nem", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Du Dein Haupt hineingebeugt,", "tokens": ["Seit", "Du", "Dein", "Haupt", "hin\u00b7ein\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du stirbst zehnmal in der Sekunde,", "tokens": ["Du", "stirbst", "zehn\u00b7mal", "in", "der", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zehnmal wirst Du neu gezeugt.", "tokens": ["Und", "zehn\u00b7mal", "wirst", "Du", "neu", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Stets mu\u00dft Du wandern, rollen, streben,", "tokens": ["Stets", "mu\u00dft", "Du", "wan\u00b7dern", ",", "rol\u00b7len", ",", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ahasver mit Doppelnot,", "tokens": ["Ein", "A\u00b7has\u00b7ver", "mit", "Dop\u00b7pel\u00b7not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ein ew'ger Tod im Leben,", "tokens": ["Es", "ist", "ein", "ew'\u00b7ger", "Tod", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ew'ges Leben in dem Tod. \u2013", "tokens": ["Ein", "ew'\u00b7ges", "Le\u00b7ben", "in", "dem", "Tod", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ich sehe, wie im immer schnellern", "tokens": ["Ich", "se\u00b7he", ",", "wie", "im", "im\u00b7mer", "schnel\u00b7lern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schnellern Sturz Du ringend bangst,", "tokens": ["Und", "schnel\u00b7lern", "Sturz", "Du", "rin\u00b7gend", "bangst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", 