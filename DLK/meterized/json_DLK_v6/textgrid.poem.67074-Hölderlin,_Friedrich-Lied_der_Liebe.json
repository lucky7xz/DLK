{"textgrid.poem.67074": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Engelfreuden ahndend, wallen", "tokens": ["En\u00b7gel\u00b7freu\u00b7den", "ahn\u00b7dend", ",", "wal\u00b7len"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVPP", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir hinaus auf Gottes Flur,", "tokens": ["Wir", "hin\u00b7aus", "auf", "Got\u00b7tes", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APZR", "APPR", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df von Jubel widerhallen", "tokens": ["Da\u00df", "von", "Ju\u00b7bel", "wi\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6hn und Tiefen der Natur.", "tokens": ["H\u00f6hn", "und", "Tie\u00b7fen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute soll kein Auge tr\u00fcbe,", "tokens": ["Heu\u00b7te", "soll", "kein", "Au\u00b7ge", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorge nicht hienieden sein,", "tokens": ["Sor\u00b7ge", "nicht", "hien\u00b7ie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedes Wesen soll der Liebe", "tokens": ["Je\u00b7des", "We\u00b7sen", "soll", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei und froh, wie wir, sich weihn!", "tokens": ["Frei", "und", "froh", ",", "wie", "wir", ",", "sich", "weihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singt den Jubel, Schwestern, Br\u00fcder,", "tokens": ["Singt", "den", "Ju\u00b7bel", ",", "Schwes\u00b7tern", ",", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest geschlungen, Hand in Hand!", "tokens": ["Fest", "ge\u00b7schlun\u00b7gen", ",", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hand in Hand das Lied der Lieder,", "tokens": ["Hand", "in", "Hand", "das", "Lied", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig an der Liebe Band!", "tokens": ["Se\u00b7lig", "an", "der", "Lie\u00b7be", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steigt hinauf am Rebenh\u00fcgel,", "tokens": ["Steigt", "hin\u00b7auf", "am", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blickt hinab ins Schattental!", "tokens": ["Blickt", "hin\u00b7ab", "ins", "Schat\u00b7ten\u00b7tal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00dcberall der Liebe Fl\u00fcgel,", "tokens": ["\u00dc\u00b7be\u00b7rall", "der", "Lie\u00b7be", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hold und herrlich \u00fcberall!", "tokens": ["Hold", "und", "herr\u00b7lich", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe lehrt das L\u00fcftchen kosen", "tokens": ["Lie\u00b7be", "lehrt", "das", "L\u00fcft\u00b7chen", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Blumen auf der Au,", "tokens": ["Mit", "den", "Blu\u00b7men", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockt zu jungen Fr\u00fchlingsrosen", "tokens": ["Lockt", "zu", "jun\u00b7gen", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Wolke Morgentau,", "tokens": ["Aus", "der", "Wol\u00b7ke", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe ziehet Well an Welle", "tokens": ["Lie\u00b7be", "zie\u00b7het", "Well", "an", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundlich murmelnd n\u00e4her hin,", "tokens": ["Freund\u00b7lich", "mur\u00b7melnd", "n\u00e4\u00b7her", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leitet aus der Kluft die Quelle", "tokens": ["Lei\u00b7tet", "aus", "der", "Kluft", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sanft hinab ins Wiesengr\u00fcn.", "tokens": ["Sanft", "hin\u00b7ab", "ins", "Wie\u00b7sen\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Berge kn\u00fcpft mit ehrner Kette", "tokens": ["Ber\u00b7ge", "kn\u00fcpft", "mit", "ehr\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe an das Firmament,", "tokens": ["Lie\u00b7be", "an", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Donner ruft sie an die St\u00e4tte,", "tokens": ["Don\u00b7ner", "ruft", "sie", "an", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Sand die Pflanze brennt.", "tokens": ["Wo", "der", "Sand", "die", "Pflan\u00b7ze", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die hehre Sonne leitet", "tokens": ["Um", "die", "heh\u00b7re", "Son\u00b7ne", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie die treuen Sterne her,", "tokens": ["Sie", "die", "treu\u00b7en", "Ster\u00b7ne", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgsam ihrem Winke gleitet", "tokens": ["Folg\u00b7sam", "ih\u00b7rem", "Win\u00b7ke", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Strom ins weite Meer.", "tokens": ["Je\u00b7der", "Strom", "ins", "wei\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe wallt durch Ozeane,", "tokens": ["Lie\u00b7be", "wallt", "durch", "O\u00b7zea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Durch der d\u00fcrren W\u00fcste Sand,", "tokens": ["Durch", "der", "d\u00fcr\u00b7ren", "W\u00fcs\u00b7te", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blutet an der Schlachtenfahne,", "tokens": ["Blu\u00b7tet", "an", "der", "Schlach\u00b7ten\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigt hinab ins Totenland!", "tokens": ["Steigt", "hin\u00b7ab", "ins", "To\u00b7ten\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe tr\u00fcmmert Felsen nieder,", "tokens": ["Lie\u00b7be", "tr\u00fcm\u00b7mert", "Fel\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zaubert Paradiese hin,", "tokens": ["Zau\u00b7bert", "Pa\u00b7ra\u00b7die\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaffet Erd und Himmel wieder \u2013", "tokens": ["Schaf\u00b7fet", "Erd", "und", "Him\u00b7mel", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttlich, wie im Anbeginn.", "tokens": ["G\u00f6tt\u00b7lich", ",", "wie", "im", "An\u00b7be\u00b7ginn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebe schwingt den Seraphsfl\u00fcgel,", "tokens": ["Lie\u00b7be", "schwingt", "den", "Se\u00b7raphs\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Gott der G\u00f6tter thront,", "tokens": ["Wo", "der", "Gott", "der", "G\u00f6t\u00b7ter", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lohnt die Tr\u00e4n am Felsenh\u00fcgel,", "tokens": ["Lohnt", "die", "Tr\u00e4n", "am", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann der Richter einst belohnt,", "tokens": ["Wann", "der", "Rich\u00b7ter", "einst", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die K\u00f6nigsst\u00fchle tr\u00fcmmern,", "tokens": ["Wann", "die", "K\u00f6\u00b7nigs\u00b7st\u00fch\u00b7le", "tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin ist jede Scheidewand,", "tokens": ["Hin", "ist", "je\u00b7de", "Schei\u00b7de\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Biedre Herzen heller schimmern,", "tokens": ["Bied\u00b7re", "Her\u00b7zen", "hel\u00b7ler", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reiner, denn der Krone Tand.", "tokens": ["Rei\u00b7ner", ",", "denn", "der", "Kro\u00b7ne", "Tand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00dft die Scheidestunde schlagen,", "tokens": ["La\u00dft", "die", "Schei\u00b7des\u00b7tun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft des W\u00fcrgers Fl\u00fcgel wehn!", "tokens": ["La\u00dft", "des", "W\u00fcr\u00b7gers", "Fl\u00fc\u00b7gel", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcder, dr\u00fcben wird es tagen!", "tokens": ["Br\u00fc\u00b7der", ",", "dr\u00fc\u00b7ben", "wird", "es", "ta\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwestern, dort ist Wiedersehn!", "tokens": ["Schwes\u00b7tern", ",", "dort", "ist", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jauchzt dem heiligsten der Triebe,", "tokens": ["Jauchzt", "dem", "hei\u00b7ligs\u00b7ten", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Den der Gott der G\u00f6tter gab,", "tokens": ["Den", "der", "Gott", "der", "G\u00f6t\u00b7ter", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Br\u00fcder, Schwestern, jauchzt der Liebe,", "tokens": ["Br\u00fc\u00b7der", ",", "Schwes\u00b7tern", ",", "jauchzt", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie besieget Zeit und Grab!", "tokens": ["Sie", "be\u00b7sie\u00b7get", "Zeit", "und", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}