{"textgrid.poem.49525": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Cupido stahl der Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cupido stahl der Mutter", "tokens": ["Cu\u00b7pi\u00b7do", "stahl", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Den sch\u00f6nsten Ring f\u00fcr Psychen;", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Ring", "f\u00fcr", "Psy\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbarg sich dann in Eile", "tokens": ["Ver\u00b7barg", "sich", "dann", "in", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meines M\u00e4dchen Auge,", "tokens": ["In", "mei\u00b7nes", "M\u00e4d\u00b7chen", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du hast dich, sagt ich, \u00fcbel,", "tokens": ["Du", "hast", "dich", ",", "sagt", "ich", ",", "\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du kleiner Dieb, verborgen.", "tokens": ["Du", "klei\u00b7ner", "Dieb", ",", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich suchte, w\u00e4r ich Amor,", "tokens": ["Ich", "such\u00b7te", ",", "w\u00e4r", "ich", "A\u00b7mor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Herz, mich zu verbergen.", "tokens": ["Ein", "Herz", ",", "mich", "zu", "ver\u00b7ber\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein Herz, versetzt er l\u00e4chelnd,", "tokens": ["Ein", "Herz", ",", "ver\u00b7setzt", "er", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Verbirgt mich freylich be\u00dfer.", "tokens": ["Ver\u00b7birgt", "mich", "frey\u00b7lich", "be\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So sey es, kluger Knabe,", "tokens": ["So", "sey", "es", ",", "klu\u00b7ger", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So sey es dann das deine,", "tokens": ["So", "sey", "es", "dann", "das", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "In das ich mich verberge!", "tokens": ["In", "das", "ich", "mich", "ver\u00b7ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}