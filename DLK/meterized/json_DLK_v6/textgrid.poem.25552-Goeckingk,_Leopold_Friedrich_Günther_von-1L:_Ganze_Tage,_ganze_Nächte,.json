{"textgrid.poem.25552": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganze Tage, ganze N\u00e4chte,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganze Tage, ganze N\u00e4chte,", "tokens": ["Gan\u00b7ze", "Ta\u00b7ge", ",", "gan\u00b7ze", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitz' ich hier, auf meine Rechte", "tokens": ["Sitz'", "ich", "hier", ",", "auf", "mei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Kummerschwere Haupt gest\u00fctzt;", "tokens": ["Die\u00b7ses", "Kum\u00b7mer\u00b7schwe\u00b7re", "Haupt", "ge\u00b7st\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sitze weinend, und betr\u00fcbe", "tokens": ["Sit\u00b7ze", "wei\u00b7nend", ",", "und", "be\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinen Geist, da\u00df deine Liebe", "tokens": ["Mei\u00b7nen", "Geist", ",", "da\u00df", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun ein Andrer, falsches Herz! besitzt.", "tokens": ["Nun", "ein", "A\u00b7ndrer", ",", "fal\u00b7sches", "Herz", "!", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Th\u00f6richt such' ich da nach Gr\u00fcnden,", "tokens": ["Th\u00f6\u00b7richt", "such'", "ich", "da", "nach", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Hoffnung, Grund zu finden,", "tokens": ["Wo", "die", "Hoff\u00b7nung", ",", "Grund", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie so k\u00fchn sie immer sey, verzagt.", "tokens": ["Wie", "so", "k\u00fchn", "sie", "im\u00b7mer", "sey", ",", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Kann ich mir begreiflich machen,", "tokens": ["Kann", "ich", "mir", "be\u00b7greif\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Was die Seele nie im Wachen,", "tokens": ["Was", "die", "See\u00b7le", "nie", "im", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Selbst im Traum' zu denken nicht gewagt?", "tokens": ["Selbst", "im", "Traum'", "zu", "den\u00b7ken", "nicht", "ge\u00b7wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Sage mir, da\u00df Vaterbitten,", "tokens": ["Sa\u00b7ge", "mir", ",", "da\u00df", "Va\u00b7ter\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mutterthr\u00e4nen dich bestritten,", "tokens": ["Mut\u00b7ter\u00b7thr\u00e4\u00b7nen", "dich", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df dein Kummer deinen Muth verzehrt,", "tokens": ["Da\u00df", "dein", "Kum\u00b7mer", "dei\u00b7nen", "Muth", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Da\u00df sie unter Thr\u00e4neng\u00fcssen", "tokens": ["Da\u00df", "sie", "un\u00b7ter", "Thr\u00e4\u00b7nen\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dir die Hand nur weggerissen,", "tokens": ["Dir", "die", "Hand", "nur", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Aber da\u00df dein Herz noch mir geh\u00f6rt.", "tokens": ["A\u00b7ber", "da\u00df", "dein", "Herz", "noch", "mir", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Sage das! ich will es glauben,", "tokens": ["Sa\u00b7ge", "das", "!", "ich", "will", "es", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Will mir das Bewu\u00dftseyn rauben,", "tokens": ["Will", "mir", "das", "Be\u00b7wu\u00df\u00b7tseyn", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df ich selbst den falschen Balsam gab;", "tokens": ["Da\u00df", "ich", "selbst", "den", "fal\u00b7schen", "Bal\u00b7sam", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Denn bei so viel tausend Schw\u00fcren,", "tokens": ["Denn", "bei", "so", "viel", "tau\u00b7send", "Schw\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Ungetreue! dich verlieren,", "tokens": ["Un\u00b7ge\u00b7treu\u00b7e", "!", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "O! das foltert langsam mich ins Grab.", "tokens": ["O", "!", "das", "fol\u00b7tert", "lang\u00b7sam", "mich", "ins", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Oder kannst du jene Scenen,", "tokens": ["O\u00b7der", "kannst", "du", "je\u00b7ne", "Sce\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Jenes Schmachten, dieses Sehnen,", "tokens": ["Je\u00b7nes", "Schmach\u00b7ten", ",", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Jene Seligkeit, und diese Pein,", "tokens": ["Je\u00b7ne", "Se\u00b7lig\u00b7keit", ",", "und", "die\u00b7se", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Kannst du die mit deinem Bilde", "tokens": ["Kannst", "du", "die", "mit", "dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Tilgen in mir? Sey so milde!", "tokens": ["Til\u00b7gen", "in", "mir", "?", "Sey", "so", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$.", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Meine letzte Bitte soll es seyn.", "tokens": ["Mei\u00b7ne", "letz\u00b7te", "Bit\u00b7te", "soll", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Kannst du das nicht, Ungetreue!", "tokens": ["Kannst", "du", "das", "nicht", ",", "Un\u00b7ge\u00b7treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "PTKNEG", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Nun wohlan! sieh her und freue", "tokens": ["Nun", "wo\u00b7hlan", "!", "sieh", "her", "und", "freu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "VVIMP", "PTKVZ", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Deines Werkes, meiner Qualen, dich!", "tokens": ["Dei\u00b7nes", "Wer\u00b7kes", ",", "mei\u00b7ner", "Qua\u00b7len", ",", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "Wen ein schleichend Gift verzehret,", "tokens": ["Wen", "ein", "schlei\u00b7chend", "Gift", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Stirbt entsetzlich, doch verheeret", "tokens": ["Stirbt", "ent\u00b7setz\u00b7lich", ",", "doch", "ver\u00b7hee\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Nicht entsetzlicher der Kummer mich?", "tokens": ["Nicht", "ent\u00b7setz\u00b7li\u00b7cher", "der", "Kum\u00b7mer", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Glaube nicht, da\u00df vor dem Grabe", "tokens": ["Glau\u00b7be", "nicht", ",", "da\u00df", "vor", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Je die\u00df Herz gezittert habe,", "tokens": ["Je", "die\u00df", "Herz", "ge\u00b7zit\u00b7tert", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ohne Klopfen geht es noch dahin!", "tokens": ["Oh\u00b7ne", "Klop\u00b7fen", "geht", "es", "noch", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Gern verzeiht es deine T\u00fccke,", "tokens": ["Gern", "ver\u00b7zeiht", "es", "dei\u00b7ne", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Lie\u00df es dich nur nicht zur\u00fccke,", "tokens": ["Lie\u00df", "es", "dich", "nur", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Und zur\u00fcck \u2013 als meine M\u00f6rderin!", "tokens": ["Und", "zu\u00b7r\u00fcck", "\u2013", "als", "mei\u00b7ne", "M\u00f6r\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$(", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}