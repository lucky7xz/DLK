{"textgrid.poem.34930": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Hausfrieden", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele Weiber, viele Fl\u00f6he,", "tokens": ["Vie\u00b7le", "Wei\u00b7ber", ",", "vie\u00b7le", "Fl\u00f6\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele Fl\u00f6he, vieles Jucken \u2013", "tokens": ["Vie\u00b7le", "Fl\u00f6\u00b7he", ",", "vie\u00b7les", "Ju\u00b7cken", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIS", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tun sich heimlich dir ein Wehe,", "tokens": ["Tun", "sich", "heim\u00b7lich", "dir", "ein", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darfst du dennnoch dich nicht mucken.", "tokens": ["Darfst", "du", "denn\u00b7noch", "dich", "nicht", "mu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn sie r\u00e4chen, schelmisch l\u00e4chelnd,", "tokens": ["Denn", "sie", "r\u00e4\u00b7chen", ",", "schel\u00b7misch", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zur Nachtzeit \u2013 Willst du dr\u00fccken", "tokens": ["Sich", "zur", "Nacht\u00b7zeit", "\u2013", "Willst", "du", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "$(", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ans Herze, lieber\u00f6chelnd,", "tokens": ["Sie", "ans", "Her\u00b7ze", ",", "lie\u00b7be\u00b7r\u00f6\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPRART", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, da drehn sie dir den R\u00fccken.", "tokens": ["Ach", ",", "da", "drehn", "sie", "dir", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}