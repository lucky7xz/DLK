{"dta.poem.23847": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Aus der 17. Epistel des Horatii  \n 1. Buchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den Morgen-Schlaaf nicht willig kanst\nverlassen/", "tokens": ["Wenn", "du", "den", "Mor\u00b7gen\u00b7Schlaaf", "nicht", "wil\u00b7lig", "kanst", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungedultig wirst/ wenn sich auf allen Strassen/", "tokens": ["Und", "un\u00b7ge\u00b7dul\u00b7tig", "wirst", "/", "wenn", "sich", "auf", "al\u00b7len", "Stras\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$(", "KOUS", "PRF", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein gro\u00df Get\u00fcmmel regt/ so sitze wo du bist/", "tokens": ["Ein", "gro\u00df", "Ge\u00b7t\u00fcm\u00b7mel", "regt", "/", "so", "sit\u00b7ze", "wo", "du", "bist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dencke da\u00df man auch zu - - - - - - gl\u00fccklich ist.", "tokens": ["Und", "den\u00b7cke", "da\u00df", "man", "auch", "zu", "gl\u00fcck\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PIS", "ADV", "PTKZU", "$(", "$(", "$(", "$(", "$(", "$(", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Vergn\u00fcgen ist nicht nur an Geld und Gut gebunden;", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "ist", "nicht", "nur", "an", "Geld", "und", "Gut", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der hat eben nicht das schlimste Theil gefunden/", "tokens": ["Und", "der", "hat", "e\u00b7ben", "nicht", "das", "schlims\u00b7te", "Theil", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob gleich kein Zeiten-Buch von seinen Thaten schreibt/", "tokens": ["Ob", "gleich", "kein", "Zei\u00b7ten\u00b7Buch", "von", "sei\u00b7nen", "Tha\u00b7ten", "schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der in der Einsamkeit den stillen Wandel treibt.", "tokens": ["Der", "in", "der", "Ein\u00b7sam\u00b7keit", "den", "stil\u00b7len", "Wan\u00b7del", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.9": {"text": "Jedennoch wenn du dir/ und auch zugleich den Deinen", "tokens": ["Je\u00b7den\u00b7noch", "wenn", "du", "dir", "/", "und", "auch", "zu\u00b7gleich", "den", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPER", "$(", "KON", "ADV", "ADV", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wilst mehr zu gute thun/ so must du da erscheinen/", "tokens": ["Wilst", "mehr", "zu", "gu\u00b7te", "thun", "/", "so", "must", "du", "da", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "VVINF", "$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo man der F\u00fcrsten Huld/ weil doch des Himmels-", "tokens": ["Wo", "man", "der", "F\u00fcrs\u00b7ten", "Huld", "/", "weil", "doch", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "NN", "$(", "KOUS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie gro\u00df/ uns klein gemacht/ in Demuth suchen mu\u00df.", "tokens": ["Sie", "gro\u00df", "/", "uns", "klein", "ge\u00b7macht", "/", "in", "De\u00b7muth", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "PPER", "ADJD", "VVPP", "$(", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "K\u00f6nt Aristippus Kraut und schlechte Kost vertragen/", "tokens": ["K\u00f6nt", "A\u00b7ris\u00b7tip\u00b7pus", "Kraut", "und", "schlech\u00b7te", "Kost", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So w\u00fcrd\u2019 er/ gleich als ich/ nicht viel nach F\u00fcrsten fra-", "tokens": ["So", "w\u00fcrd'", "er", "/", "gleich", "als", "ich", "/", "nicht", "viel", "nach", "F\u00fcrs\u00b7ten", "fra"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADV", "KOUS", "PPER", "$(", "PTKNEG", "ADV", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Rief dort Diogenes; doch jener seumte nicht/", "tokens": ["Rief", "dort", "Dio\u00b7ge\u00b7nes", ";", "doch", "je\u00b7ner", "seum\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "$.", "KON", "PDS", "VVFIN", "PTKNEG", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Und hatte dergestalt die Antwort eingerichtt:", "tokens": ["Und", "hat\u00b7te", "der\u00b7ge\u00b7stalt", "die", "Ant\u00b7wort", "ein\u00b7ge\u00b7richtt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn sich Diogenes bey F\u00fcrsten d\u00fcrffte weisen/", "tokens": ["Wenn", "sich", "Dio\u00b7ge\u00b7nes", "bey", "F\u00fcrs\u00b7ten", "d\u00fcrff\u00b7te", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "So w\u00fcrd\u2019 er etwas mehr als Zugem\u00fcse speisen.", "tokens": ["So", "w\u00fcrd'", "er", "et\u00b7was", "mehr", "als", "Zu\u00b7ge\u00b7m\u00fc\u00b7se", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mich d\u00fcnckt er hatte recht; Denn/ sprach er/ was ich", "tokens": ["Mich", "d\u00fcnckt", "er", "hat\u00b7te", "recht", ";", "Denn", "/", "sprach", "er", "/", "was", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "$.", "KON", "$(", "VVFIN", "PPER", "$(", "PWS", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Schl\u00e4gt mir zum Vortheil aus. Dir sieht der P\u00f6bel zu.", "tokens": ["Schl\u00e4gt", "mir", "zum", "Vor\u00b7theil", "aus", ".", "Dir", "sieht", "der", "P\u00f6\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich opfre meinen Dienst den Grossen/ die hingegen", "tokens": ["Ich", "opf\u00b7re", "mei\u00b7nen", "Dienst", "den", "Gros\u00b7sen", "/", "die", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$(", "ART", "ADV"], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.22": {"text": "Mit mehr als ich bedarf/ mich mildiglich verpflegen/", "tokens": ["Mit", "mehr", "als", "ich", "be\u00b7darf", "/", "mich", "mil\u00b7dig\u00b7lich", "ver\u00b7pfle\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "PPER", "VVFIN", "$(", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein Tisch/ mein Hau\u00df und Stall/ ist kostbar aufge-", "tokens": ["Mein", "Tisch", "/", "mein", "Hau\u00df", "und", "Stall", "/", "ist", "kost\u00b7bar", "auf\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$(", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und du/ der mir vorhin/ mein Schmeicheln vorger\u00fcckt/", "tokens": ["Und", "du", "/", "der", "mir", "vor\u00b7hin", "/", "mein", "Schmei\u00b7cheln", "vor\u00b7ge\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PRELS", "PPER", "ADV", "$(", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und glaubst dir fehle nichts/ must derer Gnade leben/", "tokens": ["Und", "glaubst", "dir", "feh\u00b7le", "nichts", "/", "must", "de\u00b7rer", "Gna\u00b7de", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PIS", "$(", "VMFIN", "PDS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die aus Barmhertzigkeit dir schmahle Bissen geben.", "tokens": ["Die", "aus", "Barm\u00b7hert\u00b7zig\u00b7keit", "dir", "schmah\u00b7le", "Bis\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "In allerley Gestalt/ in was f\u00fcr einem Stand/", "tokens": ["In", "al\u00b7ler\u00b7ley", "Ge\u00b7stalt", "/", "in", "was", "f\u00fcr", "ei\u00b7nem", "Stand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "APPR", "PRELS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "An was f\u00fcr einem Ort sich Aristippus fandt/", "tokens": ["An", "was", "f\u00fcr", "ei\u00b7nem", "Ort", "sich", "A\u00b7ris\u00b7tip\u00b7pus", "fandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "PRF", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da war er ohne Zwang/ bereit sich zu beqvemen/", "tokens": ["Da", "war", "er", "oh\u00b7ne", "Zwang", "/", "be\u00b7reit", "sich", "zu", "be\u00b7qve\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$(", "ADJD", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dem Gl\u00fccke nachzugehn/ und auch verlieb zu nehmen.", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "nach\u00b7zu\u00b7gehn", "/", "und", "auch", "ver\u00b7lieb", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "KON", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch wenn Diogenes/ wenn dieses Affen-Bild/", "tokens": ["Doch", "wenn", "Dio\u00b7ge\u00b7nes", "/", "wenn", "die\u00b7ses", "Af\u00b7fen\u00b7Bild", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "$(", "KOUS", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "Der seinen armen Stoltz in doppel-Tuch verh\u00fcllt/", "tokens": ["Der", "sei\u00b7nen", "ar\u00b7men", "Stoltz", "in", "dop\u00b7pel\u00b7Tuch", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "In andre Lebens-Art sich w\u00fcrdig solte f\u00fcgen/", "tokens": ["In", "and\u00b7re", "Le\u00b7bens\u00b7Art", "sich", "w\u00fcr\u00b7dig", "sol\u00b7te", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So w\u00fcrde mich gewi\u00df die Meynung sehr betriegen.", "tokens": ["So", "w\u00fcr\u00b7de", "mich", "ge\u00b7wi\u00df", "die", "Mey\u00b7nung", "sehr", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Mann/ wie jener war/ bleibt allgemach beliebt/", "tokens": ["Ein", "Mann", "/", "wie", "je\u00b7ner", "war", "/", "bleibt", "all\u00b7ge\u00b7mach", "be\u00b7liebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOKOM", "PDS", "VAFIN", "$(", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Er borgt nicht fremden Glantz der ihm ein. Ansehn giebt;", "tokens": ["Er", "borgt", "nicht", "frem\u00b7den", "Glantz", "der", "ihm", "ein", ".", "An\u00b7sehn", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "ART", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Im Kittel wie in Sam\u0303t wei\u00df er sich aufzuf\u00fchren/", "tokens": ["Im", "Kit\u00b7tel", "wie", "in", "Sam\u0303t", "wei\u00df", "er", "sich", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPR", "NE", "VVFIN", "PPER", "PRF", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der andre wil f\u00fcr Angst in seinem Zeug verfrieren/", "tokens": ["Der", "and\u00b7re", "wil", "f\u00fcr", "Angst", "in", "sei\u00b7nem", "Zeug", "ver\u00b7frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und schreyt: mein alter Rock der wird mir besser stehn!", "tokens": ["Und", "schreyt", ":", "mein", "al\u00b7ter", "Rock", "der", "wird", "mir", "bes\u00b7ser", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "ART", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Gebt ihm den alten Rock/ und la\u00dft den Narren gehn.", "tokens": ["Gebt", "ihm", "den", "al\u00b7ten", "Rock", "/", "und", "la\u00dft", "den", "Nar\u00b7ren", "gehn", "."], "token_info": ["word"