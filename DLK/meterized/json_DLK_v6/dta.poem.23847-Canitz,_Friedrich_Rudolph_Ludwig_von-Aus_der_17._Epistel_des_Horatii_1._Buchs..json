", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ART", "$(", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die Mutter hat kein Brodt/ die Schwester keinen Mann/", "tokens": ["Die", "Mut\u00b7ter", "hat", "kein", "Brodt", "/", "die", "Schwes\u00b7ter", "kei\u00b7nen", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Weil ich nicht Unterhalt noch Brautschatz geben kan.", "tokens": ["Weil", "ich", "nicht", "Un\u00b7ter\u00b7halt", "noch", "Braut\u00b7schatz", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.65": {"text": "Mein Freund/ man kennt die Kunst/ du suchst was zu er-", "tokens": ["Mein", "Freund", "/", "man", "kennt", "die", "Kunst", "/", "du", "suchst", "was", "zu", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PIS", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.66": {"text": "Doch wisse/ neben dir stehn andre deines gleichen/", "tokens": ["Doch", "wis\u00b7se", "/", "ne\u00b7ben", "dir", "stehn", "and\u00b7re", "dei\u00b7nes", "glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "APPR", "PPER", "VVFIN", "PIS", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+