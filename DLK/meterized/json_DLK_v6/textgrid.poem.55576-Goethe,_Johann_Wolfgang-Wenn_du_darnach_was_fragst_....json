{"textgrid.poem.55576": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wenn du darnach was fragst ...", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du darnach was fragst,", "tokens": ["Wenn", "du", "dar\u00b7nach", "was", "fragst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PWS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir waren hier.", "tokens": ["Wir", "wa\u00b7ren", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du, der du nach uns kommen magst,", "tokens": ["Du", ",", "der", "du", "nach", "uns", "kom\u00b7men", "magst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab wenigstens so frisches Blut", "tokens": ["Hab", "we\u00b7nigs\u00b7tens", "so", "fri\u00b7sches", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sei so leidlich fromm und gut", "tokens": ["Und", "sei", "so", "leid\u00b7lich", "fromm", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und leidlich gl\u00fccklich als wie wir!", "tokens": ["Und", "leid\u00b7lich", "gl\u00fcck\u00b7lich", "als", "wie", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KOKOM", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}