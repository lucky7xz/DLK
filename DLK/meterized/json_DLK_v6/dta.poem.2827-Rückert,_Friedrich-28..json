{"dta.poem.2827": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo Ueberlieferung ununterbrochen waltet,", "tokens": ["Wo", "Ue\u00b7berl\u00b7ie\u00b7fe\u00b7rung", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird an der Bildung Stamm leicht Blatt aus Blatt entfaltet.", "tokens": ["Wird", "an", "der", "Bil\u00b7dung", "Stamm", "leicht", "Blatt", "aus", "Blatt", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "ADJD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00fcler nimmt getreu von seinem Lehrer an,", "tokens": ["Der", "Sch\u00fc\u00b7ler", "nimmt", "ge\u00b7treu", "von", "sei\u00b7nem", "Leh\u00b7rer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was der von seinem, der von seinem hat empfahn.", "tokens": ["Was", "der", "von", "sei\u00b7nem", ",", "der", "von", "sei\u00b7nem", "hat", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "PPOSAT", "$,", "PRELS", "APPR", "PPOSAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So bis zum letzten l\u00e4uft der Funken durch die Kette,", "tokens": ["So", "bis", "zum", "letz\u00b7ten", "l\u00e4uft", "der", "Fun\u00b7ken", "durch", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob unmittelbar er ihn vom ersten h\u00e4tte.", "tokens": ["Al\u00b7sob", "un\u00b7mit\u00b7tel\u00b7bar", "er", "ihn", "vom", "ers\u00b7ten", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ist nun der gliedernde Zusammenhang gesprengt,", "tokens": ["Ist", "nun", "der", "glie\u00b7dern\u00b7de", "Zu\u00b7sam\u00b7men\u00b7hang", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wei\u00df keiner mehr, von wem, was und wie ers empf\u00e4ngt.", "tokens": ["Wei\u00df", "kei\u00b7ner", "mehr", ",", "von", "wem", ",", "was", "und", "wie", "ers", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "APPR", "PIS", "$,", "PWS", "KON", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zu seinen Lehrern hat ein Sch\u00fcler dieser Zeit", "tokens": ["Zu", "sei\u00b7nen", "Leh\u00b7rern", "hat", "ein", "Sch\u00fc\u00b7ler", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Gegenwart und die Vergangenheit.", "tokens": ["Die", "gan\u00b7ze", "Ge\u00b7gen\u00b7wart", "und", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}