{"textgrid.poem.28074": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gilt ietzt nichts so hoch, als nichts; die Eitelkeit", "tokens": ["Es", "gilt", "ietzt", "nichts", "so", "hoch", ",", "als", "nichts", ";", "die", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "ADJD", "$,", "KOUS", "PIS", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat an sich alle Welt, Gesch\u00e4ffte, Leute, Zeit,", "tokens": ["Hat", "an", "sich", "al\u00b7le", "Welt", ",", "Ge\u00b7sch\u00e4ff\u00b7te", ",", "Leu\u00b7te", ",", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df gegen Nichts ist nichts die reiche Seligkeit.", "tokens": ["Da\u00df", "ge\u00b7gen", "Nichts", "ist", "nichts", "die", "rei\u00b7che", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "VAFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}