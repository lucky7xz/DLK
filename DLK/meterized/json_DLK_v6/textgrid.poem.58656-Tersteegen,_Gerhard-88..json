{"textgrid.poem.58656": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So fern in Welt und Eigenheit bin ich von Gott gegangen,", "tokens": ["So", "fern", "in", "Welt", "und", "Ei\u00b7gen\u00b7heit", "bin", "ich", "von", "Gott", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "In Unruh, Furcht und Mangel sitzt mein armer Geist gefangen;", "tokens": ["In", "Un\u00b7ruh", ",", "Furcht", "und", "Man\u00b7gel", "sitzt", "mein", "ar\u00b7mer", "Geist", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wann werd' ich endlich finden Ruh daheim in 's Vaters Scho\u00df?", "tokens": ["Wann", "werd'", "ich", "end\u00b7lich", "fin\u00b7den", "Ruh", "da\u00b7heim", "in", "'s", "Va\u00b7ters", "Scho\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "ADV", "APPR", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ich streit' und seufz': \u00bbHerr, hilf mir doch, sonst werd' ich nimmer los!\u00ab", "tokens": ["Ich", "streit'", "und", "seuf\u00b7z'", ":", "\u00bb", "Herr", ",", "hilf", "mir", "doch", ",", "sonst", "werd'", "ich", "nim\u00b7mer", "los", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "NN", "$,", "VVIMP", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Beschwerte Seel', verzage nicht, wer glaubt, dem wird's gelingen;", "tokens": ["Be\u00b7schwer\u00b7te", "Seel'", ",", "ver\u00b7za\u00b7ge", "nicht", ",", "wer", "glaubt", ",", "dem", "wird's", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$,", "PWS", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Gott spricht: \u00bbIch will dir helfen los, ich will dich wieder bringen", "tokens": ["Gott", "spricht", ":", "\u00bb", "Ich", "will", "dir", "hel\u00b7fen", "los", ",", "ich", "will", "dich", "wie\u00b7der", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PPER", "VVINF", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Aus dir und deinem Jammerstand", "tokens": ["Aus", "dir", "und", "dei\u00b7nem", "Jam\u00b7mer\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In mich, dein wahres Vaterland.\u00ab", "tokens": ["In", "mich", ",", "dein", "wah\u00b7res", "Va\u00b7ter\u00b7land", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da schlie\u00dft du dann in sichrer Ruh", "tokens": ["Da", "schlie\u00dft", "du", "dann", "in", "sich\u00b7rer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So sanft die frohen Augen zu!", "tokens": ["So", "sanft", "die", "fro\u00b7hen", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}