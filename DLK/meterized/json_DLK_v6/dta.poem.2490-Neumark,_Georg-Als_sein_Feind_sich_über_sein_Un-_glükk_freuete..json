{"dta.poem.2490": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sein Feind sich \u00fcber sein Un-  \n gl\u00fckk freuete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Was lachstu Schadenfroh/ da\u00df ich nun bin beladen", "tokens": ["Was", "lachs\u00b7tu", "Scha\u00b7den\u00b7froh", "/", "da\u00df", "ich", "nun", "bin", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit grosser Ungl\u00fckslast? O Mensche d\u00e4nkstu nicht", "tokens": ["Mit", "gros\u00b7ser", "Un\u00b7gl\u00fcks\u00b7last", "?", "O", "Men\u00b7sche", "d\u00e4nks\u00b7tu", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df solches mir von Gott ist worden zugericht?", "tokens": ["Da\u00df", "sol\u00b7ches", "mir", "von", "Gott", "ist", "wor\u00b7den", "zu\u00b7ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VAFIN", "VAPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Betracht es weil dich auch kan treffen dieser Schaden.", "tokens": ["Be\u00b7tracht", "es", "weil", "dich", "auch", "kan", "tref\u00b7fen", "die\u00b7ser", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER