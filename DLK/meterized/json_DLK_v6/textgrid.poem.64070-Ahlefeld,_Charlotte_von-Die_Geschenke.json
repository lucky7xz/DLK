{"textgrid.poem.64070": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschenke", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um in der Ferne meiner zu gedenken,", "tokens": ["Um", "in", "der", "Fer\u00b7ne", "mei\u00b7ner", "zu", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bedarfst Du wohl der \u00e4ussern Zeichen nicht.", "tokens": ["Be\u00b7darfst", "Du", "wohl", "der", "\u00e4us\u00b7sern", "Zei\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Deiner Brust unsterblich mich zu denken,", "tokens": ["In", "Dei\u00b7ner", "Brust", "uns\u00b7terb\u00b7lich", "mich", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Macht mir Dein Schwur zur ewig heil'gen Pflicht,", "tokens": ["Macht", "mir", "Dein", "Schwur", "zur", "e\u00b7wig", "heil'\u00b7gen", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "VVFIN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und doch darfst Du die Gaben nicht verschm\u00e4hen,", "tokens": ["Und", "doch", "darfst", "Du", "die", "Ga\u00b7ben", "nicht", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Womit ich w\u00fcnsche Dich geschm\u00fcckt zu sehen.", "tokens": ["Wo\u00b7mit", "ich", "w\u00fcn\u00b7sche", "Dich", "ge\u00b7schm\u00fcckt", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So nimm den Ring von meinem Haar umgeben", "tokens": ["So", "nimm", "den", "Ring", "von", "mei\u00b7nem", "Haar", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lass ihn nie von Deiner theuern Hand;", "tokens": ["Und", "lass", "ihn", "nie", "von", "Dei\u00b7ner", "theu\u00b7ern", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sei Dein Talisman im wilden Leben,", "tokens": ["Er", "sei", "Dein", "Ta\u00b7lis\u00b7man", "im", "wil\u00b7den", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der Erinnrung goldnes Unterpfand;", "tokens": ["Und", "der", "E\u00b7rinn\u00b7rung", "gold\u00b7nes", "Un\u00b7ter\u00b7pfand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und auch noch dann wenn jede Hoffnung schwindet,", "tokens": ["Und", "auch", "noch", "dann", "wenn", "je\u00b7de", "Hoff\u00b7nung", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei er der Kreis, der magisch uns verbindet.", "tokens": ["Sei", "er", "der", "Kreis", ",", "der", "ma\u00b7gisch", "uns", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und nimm die Uhr, die Dir mit leisem Schlage", "tokens": ["Und", "nimm", "die", "Uhr", ",", "die", "Dir", "mit", "lei\u00b7sem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verklungne Stunden wiederholen kann;", "tokens": ["Ver\u00b7klung\u00b7ne", "Stun\u00b7den", "wie\u00b7der\u00b7ho\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach h\u00e4tte sie die Macht, vergangne ", "tokens": ["Ach", "h\u00e4t\u00b7te", "sie", "die", "Macht", ",", "ver\u00b7gang\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns zu erneun, wie kostbar w\u00e4r' sie dann!", "tokens": ["Uns", "zu", "er\u00b7neun", ",", "wie", "kost\u00b7bar", "w\u00e4r'", "sie", "dann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch an die Flucht der Zeit darf sie Dich mahnen,", "tokens": ["Doch", "an", "die", "Flucht", "der", "Zeit", "darf", "sie", "Dich", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und eine bessre Zukunft wird Dir ahnen.", "tokens": ["Und", "ei\u00b7ne", "bess\u00b7re", "Zu\u00b7kunft", "wird", "Dir", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Nadel nimm, geziert mit Edelsteinen,", "tokens": ["Die", "Na\u00b7del", "nimm", ",", "ge\u00b7ziert", "mit", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trage sie an Deiner treuen Brust.", "tokens": ["Und", "tra\u00b7ge", "sie", "an", "Dei\u00b7ner", "treu\u00b7en", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie wird der Welt als leerer Schmuck erscheinen,", "tokens": ["Sie", "wird", "der", "Welt", "als", "lee\u00b7rer", "Schmuck", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn fremd ist ihr die schmerzlich s\u00fcsse Lust,", "tokens": ["Denn", "fremd", "ist", "ihr", "die", "schmerz\u00b7lich", "s\u00fcs\u00b7se", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Womit die Liebe sucht, in holden Bildern", "tokens": ["Wo\u00b7mit", "die", "Lie\u00b7be", "sucht", ",", "in", "hol\u00b7den", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der reinen Gunst, des Scheidens Weh zu mildern.", "tokens": ["Der", "rei\u00b7nen", "Gunst", ",", "des", "Schei\u00b7dens", "Weh", "zu", "mil\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Bewahre heilig, was ich Dir gegeben,", "tokens": ["Be\u00b7wah\u00b7re", "hei\u00b7lig", ",", "was", "ich", "Dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ach \u2013 wer weiss, ob wir uns wiedersehn,", "tokens": ["Denn", "ach", "\u2013", "wer", "weiss", ",", "ob", "wir", "uns", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$(", "PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob unsre Wege durch das weite Leben", "tokens": ["Ob", "uns\u00b7re", "We\u00b7ge", "durch", "das", "wei\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht nach ", "tokens": ["Nicht", "nach"], "token_info": ["word", "word"], "pos": ["PTKNEG", "APPR"], "meter": "++", "measure": "spondeus"}, "line.5": {"text": "Wo fern von Dir, in still verschwiegnen Thr\u00e4nen,", "tokens": ["Wo", "fern", "von", "Dir", ",", "in", "still", "ver\u00b7schwieg\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich heimlich aufzehrt meines Herzens Sehnen.", "tokens": ["Mich", "heim\u00b7lich", "auf\u00b7zehrt", "mei\u00b7nes", "Her\u00b7zens", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So nimm sie denn, die freundlichen Geschenke,", "tokens": ["So", "nimm", "sie", "denn", ",", "die", "freund\u00b7li\u00b7chen", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Dir des Abschieds dunkle Stunde bringt.", "tokens": ["Die", "Dir", "des", "Ab\u00b7schieds", "dunk\u00b7le", "Stun\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In der Erinn'rung theure Schatten senke", "tokens": ["In", "der", "Er\u00b7inn'\u00b7rung", "theu\u00b7re", "Schat\u00b7ten", "sen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den nassen Blick, wenn Dich der Gram bezwingt,", "tokens": ["Den", "nas\u00b7sen", "Blick", ",", "wenn", "Dich", "der", "Gram", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird mein Bild Dich liebevoll umschweben,", "tokens": ["Dann", "wird", "mein", "Bild", "Dich", "lie\u00b7be\u00b7voll", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und die Vergangenheit auf's neue Dir beleben.", "tokens": ["Und", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "auf's", "neu\u00b7e", "Dir", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}