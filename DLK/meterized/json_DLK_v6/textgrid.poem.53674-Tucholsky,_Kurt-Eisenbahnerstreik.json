{"textgrid.poem.53674": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Eisenbahnerstreik", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unn\u00f6tig.", "tokens": ["Un\u00b7n\u00f6\u00b7tig", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Aber ohne jedes Recht.", "tokens": ["A\u00b7ber", "oh\u00b7ne", "je\u00b7des", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Frau, die Kinder wollen Schuhe.", "tokens": ["Die", "Frau", ",", "die", "Kin\u00b7der", "wol\u00b7len", "Schu\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wi\u00dft ihr, wie solcher Dienst den K\u00f6rper schw\u00e4cht?", "tokens": ["Wi\u00dft", "ihr", ",", "wie", "sol\u00b7cher", "Dienst", "den", "K\u00f6r\u00b7per", "schw\u00e4cht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tag\u2013, Nachtschicht und das bi\u00dfchen Ruhe.", "tokens": ["Tag", "\u2013", ",", "Nacht\u00b7schicht", "und", "das", "bi\u00df\u00b7chen", "Ru\u00b7he", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "$,", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, standet ihr schon mal am F\u00fchrerstand?", "tokens": ["Ja", ",", "stan\u00b7det", "ihr", "schon", "mal", "am", "F\u00fch\u00b7rer\u00b7stand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kessel gl\u00fcht \u2013 es ziehn die Winde.", "tokens": ["Der", "Kes\u00b7sel", "gl\u00fcht", "\u2013", "es", "ziehn", "die", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00df-kalt, kalt-hei\u00df wird seine F\u00fchrerhand . . .", "tokens": ["Hei\u00df\u00b7kalt", ",", "kal\u00b7thei\u00df", "wird", "sei\u00b7ne", "F\u00fch\u00b7rer\u00b7hand", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo ist sein Sinn! Bei seinem Kinde?", "tokens": ["Wo", "ist", "sein", "Sinn", "!", "Bei", "sei\u00b7nem", "Kin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo ist sein Sinn? Die Augen sp\u00e4hn: \u00bbFahrt frei!\u00ab", "tokens": ["Wo", "ist", "sein", "Sinn", "?", "Die", "Au\u00b7gen", "sp\u00e4hn", ":", "\u00bb", "Fahrt", "frei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VVINF", "$.", "$(", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er darf nicht einen Griff vers\u00e4umen.", "tokens": ["Er", "darf", "nicht", "ei\u00b7nen", "Griff", "ver\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sieht das Vorsignal und Weiche III \u2013", "tokens": ["Er", "sieht", "das", "Vor\u00b7sig\u00b7nal", "und", "Wei\u00b7che", "I\u00b7iI", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "CARD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ihr k\u00f6nnt auf weichen Polstern tr\u00e4umen,", "tokens": ["Ihr", "k\u00f6nnt", "auf", "wei\u00b7chen", "Pols\u00b7tern", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wollt ihr nicht sichere Fahrt durch euer Land?", "tokens": ["Wollt", "ihr", "nicht", "si\u00b7che\u00b7re", "Fahrt", "durch", "eu\u00b7er", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie soll der Dienst tun mit den Sorgen?", "tokens": ["Wie", "soll", "der", "Dienst", "tun", "mit", "den", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zweihundert Leben in der einen Hand \u2013", "tokens": ["Zwei\u00b7hun\u00b7dert", "Le\u00b7ben", "in", "der", "ei\u00b7nen", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und dieser Hand will keiner, keiner borgen?", "tokens": ["und", "die\u00b7ser", "Hand", "will", "kei\u00b7ner", ",", "kei\u00b7ner", "bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PIS", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er hats nicht leicht der Mann vom Fl\u00fcgelrad.", "tokens": ["Er", "hats", "nicht", "leicht", "der", "Mann", "vom", "Fl\u00fc\u00b7gel\u00b7rad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stets droht der Tod. Er soll nicht ein Mal fehlen.", "tokens": ["Stets", "droht", "der", "Tod", ".", "Er", "soll", "nicht", "ein", "Mal", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr tuts f\u00fcr euch. Macht seine Kinder satt!", "tokens": ["Ihr", "tuts", "f\u00fcr", "euch", ".", "Macht", "sei\u00b7ne", "Kin\u00b7der", "satt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$.", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer f\u00fcnf Milliarden f\u00fcr die Reichswehr hat:", "tokens": ["Wer", "f\u00fcnf", "Mil\u00b7li\u00b7ar\u00b7den", "f\u00fcr", "die", "Reichs\u00b7wehr", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "der darf uns nichts von Sparsamkeit erz\u00e4hlen!", "tokens": ["der", "darf", "uns", "nichts", "von", "Spar\u00b7sam\u00b7keit", "er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}