{"textgrid.poem.45982": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie so leis die Bl\u00e4tter wehn]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so leis die Bl\u00e4tter wehn", "tokens": ["Wie", "so", "leis", "die", "Bl\u00e4t\u00b7ter", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem lieben stillen Hain,", "tokens": ["In", "dem", "lie\u00b7ben", "stil\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonne will schon schlafen gehn,", "tokens": ["Son\u00b7ne", "will", "schon", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft ihr goldnes Hemdelein", "tokens": ["L\u00e4\u00dft", "ihr", "gold\u00b7nes", "Hem\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinken auf den gr\u00fcnen Rasen", "tokens": ["Sin\u00b7ken", "auf", "den", "gr\u00fc\u00b7nen", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die schlanken Hirsche grasen", "tokens": ["Wo", "die", "schlan\u00b7ken", "Hir\u00b7sche", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem roten Abendschein.", "tokens": ["In", "dem", "ro\u00b7ten", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gute Nacht, Heiapopeia", "tokens": ["Gu\u00b7te", "Nacht", ",", "He\u00b7ia\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Singt, Gockel, Hinkel und Gackeleia.", "tokens": ["Singt", ",", "Go\u00b7ckel", ",", "Hin\u00b7kel", "und", "Ga\u00b7cke\u00b7lei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In der Quellen klarer Flut", "tokens": ["In", "der", "Quel\u00b7len", "kla\u00b7rer", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt kein Fischlein mehr sein Spiel,", "tokens": ["Treibt", "kein", "Fisc\u00b7hlein", "mehr", "sein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes sucht, wo es ruht,", "tokens": ["Je\u00b7des", "sucht", ",", "wo", "es", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein gew\u00f6hnlich Ort und Ziel", "tokens": ["Sein", "ge\u00b7w\u00f6hn\u00b7lich", "Ort", "und", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und entschlummert \u00fcberm Lauschen", "tokens": ["Und", "ent\u00b7schlum\u00b7mert", "\u00fc\u00b7berm", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Wellen leises Rauschen", "tokens": ["Auf", "der", "Wel\u00b7len", "lei\u00b7ses", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwischen bunten Kieseln k\u00fchl.", "tokens": ["Zwi\u00b7schen", "bun\u00b7ten", "Kie\u00b7seln", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gute Nacht, Heiapopeia", "tokens": ["Gu\u00b7te", "Nacht", ",", "He\u00b7ia\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Singt, Gockel, Hinkel und Gackeleia.", "tokens": ["Singt", ",", "Go\u00b7ckel", ",", "Hin\u00b7kel", "und", "Ga\u00b7cke\u00b7lei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schlank schaut auf der Felsenwand", "tokens": ["Schlank", "schaut", "auf", "der", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich die Glockenblume um,", "tokens": ["Sich", "die", "Glo\u00b7cken\u00b7blu\u00b7me", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn versp\u00e4tet \u00fcber Land", "tokens": ["Denn", "ver\u00b7sp\u00e4\u00b7tet", "\u00fc\u00b7ber", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ein Bienchen mit Gebrumm,", "tokens": ["Will", "ein", "Bien\u00b7chen", "mit", "Ge\u00b7brumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich zur Nachtherberge melden", "tokens": ["Sich", "zur", "Nacht\u00b7her\u00b7ber\u00b7ge", "mel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den zarten blauen Zelten,", "tokens": ["In", "den", "zar\u00b7ten", "blau\u00b7en", "Zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schl\u00fcpft hinein und wird ganz stumm.", "tokens": ["Schl\u00fcpft", "hin\u00b7ein", "und", "wird", "ganz", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gute Nacht, Heiapopeia", "tokens": ["Gu\u00b7te", "Nacht", ",", "He\u00b7ia\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Singt, Gockel, Hinkel und Gackeleia.", "tokens": ["Singt", ",", "Go\u00b7ckel", ",", "Hin\u00b7kel", "und", "Ga\u00b7cke\u00b7lei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "V\u00f6glein, euer schwaches Nest", "tokens": ["V\u00f6\u00b7glein", ",", "eu\u00b7er", "schwa\u00b7ches", "Nest"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das Abendlied vollbracht", "tokens": ["Ist", "das", "A\u00b7ben\u00b7dlied", "voll\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird wie eine Burg so fest.", "tokens": ["Wird", "wie", "ei\u00b7ne", "Burg", "so", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromme V\u00f6glein sch\u00fctzt zur Nacht,", "tokens": ["From\u00b7me", "V\u00f6\u00b7glein", "sch\u00fctzt", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gegen Katz und Marderkrallen,", "tokens": ["Ge\u00b7gen", "Katz", "und", "Mar\u00b7der\u00b7kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Schlaf sie \u00fcberfallen,", "tokens": ["Die", "im", "Schlaf", "sie", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott, der \u00fcber alle wacht.", "tokens": ["Gott", ",", "der", "\u00fc\u00b7ber", "al\u00b7le", "wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gute Nacht, Heiapopeia", "tokens": ["Gu\u00b7te", "Nacht", ",", "He\u00b7ia\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Singt, Gockel, Hinkel und Gackeleia.", "tokens": ["Singt", ",", "Go\u00b7ckel", ",", "Hin\u00b7kel", "und", "Ga\u00b7cke\u00b7lei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Treuer Gott, du bist nicht weit,", "tokens": ["Treu\u00b7er", "Gott", ",", "du", "bist", "nicht", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so ziehn wir ohne Harm", "tokens": ["Und", "so", "ziehn", "wir", "oh\u00b7ne", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die wilde Einsamkeit,", "tokens": ["In", "die", "wil\u00b7de", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Hofes eitelm Schwarm.", "tokens": ["Aus", "des", "Ho\u00b7fes", "ei\u00b7telm", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du wirst uns die H\u00fctte bauen,", "tokens": ["Du", "wirst", "uns", "die", "H\u00fct\u00b7te", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df wir fromm und voll Vertrauen", "tokens": ["Da\u00df", "wir", "fromm", "und", "voll", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sicher ruhn in deinem Arm.", "tokens": ["Si\u00b7cher", "ruhn", "in", "dei\u00b7nem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gute Nacht, Heiapopeia", "tokens": ["Gu\u00b7te", "Nacht", ",", "He\u00b7ia\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Singt, Gockel, Hinkel und Gackeleia.", "tokens": ["Singt", ",", "Go\u00b7ckel", ",", "Hin\u00b7kel", "und", "Ga\u00b7cke\u00b7lei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}