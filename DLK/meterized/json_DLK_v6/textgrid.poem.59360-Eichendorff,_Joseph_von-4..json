{"textgrid.poem.59360": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist's, was mich ganz verst\u00f6ret:", "tokens": ["Das", "ist's", ",", "was", "mich", "ganz", "ver\u00b7st\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df die Nacht nicht Ruhe h\u00e4lt,", "tokens": ["Da\u00df", "die", "Nacht", "nicht", "Ru\u00b7he", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn zu atmen aufgeh\u00f6ret", "tokens": ["Wenn", "zu", "at\u00b7men", "auf\u00b7ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange schon die m\u00fcde Welt.", "tokens": ["Lan\u00b7ge", "schon", "die", "m\u00fc\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df die Glocken, die da schlagen,", "tokens": ["Da\u00df", "die", "Glo\u00b7cken", ",", "die", "da", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Wald der leise Wind", "tokens": ["Und", "im", "Wald", "der", "lei\u00b7se", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Nacht von neuem klagen", "tokens": ["Je\u00b7de", "Nacht", "von", "neu\u00b7em", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mein liebes, s\u00fc\u00dfes Kind.", "tokens": ["Um", "mein", "lie\u00b7bes", ",", "s\u00fc\u00b7\u00dfes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df mein Herz nicht konnte brechen", "tokens": ["Da\u00df", "mein", "Herz", "nicht", "konn\u00b7te", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem letzten Todesku\u00df,", "tokens": ["Bei", "dem", "letz\u00b7ten", "To\u00b7des\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich wie im Wahnsinn sprechen", "tokens": ["Da\u00df", "ich", "wie", "im", "Wahn\u00b7sinn", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun in irren Liedern mu\u00df.", "tokens": ["Nun", "in", "ir\u00b7ren", "Lie\u00b7dern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}