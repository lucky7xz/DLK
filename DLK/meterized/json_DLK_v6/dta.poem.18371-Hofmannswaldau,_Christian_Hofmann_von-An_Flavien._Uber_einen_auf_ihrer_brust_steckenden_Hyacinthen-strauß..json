{"dta.poem.18371": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Flavien.  \n Uber einen auf ihrer brust steckenden  \n Hyacinthen-strau\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wilst die weisse brust zu einem garten machen/", "tokens": ["Du", "wilst", "die", "weis\u00b7se", "brust", "zu", "ei\u00b7nem", "gar\u00b7ten", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir tr\u00e4gt das gute land schon Hyacinthen ein.", "tokens": ["Dir", "tr\u00e4gt", "das", "gu\u00b7te", "land", "schon", "Hya\u00b7cin\u00b7then", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Doch sol die fruchtbarkeit dein Eden stets bewachen;", "tokens": ["Doch", "sol", "die", "frucht\u00b7bar\u00b7keit", "dein", "E\u00b7den", "stets", "be\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df/ o Flavia/ mich deinen g\u00e4rtner seyn.", "tokens": ["So", "la\u00df", "/", "o", "Fla\u00b7via", "/", "mich", "dei\u00b7nen", "g\u00e4rt\u00b7ner", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$(", "FM", "NE", "$(", "PPER", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich will dir treu und flei\u00df mit hand und mund versprechen/", "tokens": ["Ich", "will", "dir", "treu", "und", "flei\u00df", "mit", "hand", "und", "mund", "ver\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nimm meine k\u00fcsse nur statt thau und regens an.", "tokens": ["Nimm", "mei\u00b7ne", "k\u00fcs\u00b7se", "nur", "statt", "thau", "und", "re\u00b7gens", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "APPR", "NE", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wird dein g\u00e4rtner gleich zuweilen blumen brechen/", "tokens": ["Und", "wird", "dein", "g\u00e4rt\u00b7ner", "gleich", "zu\u00b7wei\u00b7len", "blu\u00b7men", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So dencke/ da\u00df er dir auch blumen pflantzen kan.", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "er", "dir", "auch", "blu\u00b7men", "pflant\u00b7zen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}