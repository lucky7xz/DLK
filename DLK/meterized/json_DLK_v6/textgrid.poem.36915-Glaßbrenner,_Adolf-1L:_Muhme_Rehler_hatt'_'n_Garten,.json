{"textgrid.poem.36915": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Muhme Rehler hatt' 'n Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Muhme Rehler hatt' 'n Garten,", "tokens": ["Muh\u00b7me", "Reh\u00b7ler", "hatt'", "'n", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, 'n wundersch\u00f6nen Garten!", "tokens": ["Ach", ",", "'n", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Gar\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der war vergittert.", "tokens": ["Und", "der", "war", "ver\u00b7git\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In dem Garten stand ein Baum,", "tokens": ["In", "dem", "Gar\u00b7ten", "stand", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wundersch\u00f6ner Baum!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Baum", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der gibt Dir nicht Schatten.", "tokens": ["Der", "gibt", "Dir", "nicht", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf dem Baum, da war ein Zweig,", "tokens": ["Auf", "dem", "Baum", ",", "da", "war", "ein", "Zweig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wundersch\u00f6ner Zweig!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Zweig", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein sind nicht die Fr\u00fcchte.", "tokens": ["Dein", "sind", "nicht", "die", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auf dem Zweig da war ein Nest,", "tokens": ["Auf", "dem", "Zweig", "da", "war", "ein", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wundersch\u00f6nes Nest!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Nest", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur f\u00fcr gro\u00dfe V\u00f6gel.", "tokens": ["Nur", "f\u00fcr", "gro\u00b7\u00dfe", "V\u00f6\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "In dem Nest, da lag ein Ei!", "tokens": ["In", "dem", "Nest", ",", "da", "lag", "ein", "Ei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wundersch\u00f6nes Ei!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Ei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist Dein, mein S\u00f6hnchen.", "tokens": ["Das", "ist", "Dein", ",", "mein", "S\u00f6hn\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "In dem Ei, da steht geschrieben:", "tokens": ["In", "dem", "Ei", ",", "da", "steht", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du sollst Deinen K\u00f6nig lieben.", "tokens": ["Du", "sollst", "Dei\u00b7nen", "K\u00f6\u00b7nig", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}