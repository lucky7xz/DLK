{"textgrid.poem.42002": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer je die Macht der keuschen Lieb' erfuhr,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer je die Macht der keuschen Lieb' erfuhr,", "tokens": ["Wer", "je", "die", "Macht", "der", "keu\u00b7schen", "Lieb'", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem wird ihr Hauch im Busen ewig wohnen;", "tokens": ["Dem", "wird", "ihr", "Hauch", "im", "Bu\u00b7sen", "e\u00b7wig", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die zarte Brust hegt ", "tokens": ["Die", "zar\u00b7te", "Brust", "hegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Durchs ganze Leben folgt sie unsrer Spur,", "tokens": ["Durchs", "gan\u00b7ze", "Le\u00b7ben", "folgt", "sie", "uns\u00b7rer", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Dornen bald und bald mit Bl\u00fcthenkronen;", "tokens": ["Mit", "Dor\u00b7nen", "bald", "und", "bald", "mit", "Bl\u00fc\u00b7then\u00b7kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch mag sie z\u00fcrnen, mag sie l\u00e4chelnd lohnen,", "tokens": ["Doch", "mag", "sie", "z\u00fcr\u00b7nen", ",", "mag", "sie", "l\u00e4\u00b7chelnd", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr huldigt stets die edlere Natur.", "tokens": ["Ihr", "hul\u00b7digt", "stets", "die", "ed\u00b7le\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie schweigt der Schmerz, den sie uns einst gegeben,", "tokens": ["Nie", "schweigt", "der", "Schmerz", ",", "den", "sie", "uns", "einst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freude nie, die sie uns einst gew\u00e4hrte,", "tokens": ["Die", "Freu\u00b7de", "nie", ",", "die", "sie", "uns", "einst", "ge\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kurz ist die Lust, doch ewig das Gef\u00fchl.", "tokens": ["Kurz", "ist", "die", "Lust", ",", "doch", "e\u00b7wig", "das", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Welt zu Welt mit uns emporzuschweben", "tokens": ["Von", "Welt", "zu", "Welt", "mit", "uns", "em\u00b7por\u00b7zu\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Folgt uns ihr Strahl als leuchtender Gef\u00e4hrte;", "tokens": ["Folgt", "uns", "ihr", "Strahl", "als", "leuch\u00b7ten\u00b7der", "Ge\u00b7f\u00e4hr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Seyn ist Werden, Ewigkeit ihr Ziel.", "tokens": ["Ihr", "Seyn", "ist", "Wer\u00b7den", ",", "E\u00b7wig\u00b7keit", "ihr", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VAFIN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}