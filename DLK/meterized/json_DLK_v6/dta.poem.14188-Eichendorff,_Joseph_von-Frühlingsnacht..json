{"dta.poem.14188": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und der Mond, die Sterne sagen's", "tokens": ["Und", "der", "Mond", ",", "die", "Ster\u00b7ne", "sa\u00b7gen's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "NE"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und in Tr\u00e4umen rauscht's der Hain", "tokens": ["Und", "in", "Tr\u00e4u\u00b7men", "rauscht's", "der", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Nachtigallen schlagen's:", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen's", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie ist Deine, sie ist Dein!", "tokens": ["Sie", "ist", "Dei\u00b7ne", ",", "sie", "ist", "Dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}