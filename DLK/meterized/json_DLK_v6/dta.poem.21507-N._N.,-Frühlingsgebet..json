{"dta.poem.21507": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wieder wallen die s\u00fc\u00dfen L\u00fcfte", "tokens": ["Wie\u00b7der", "wal\u00b7len", "die", "s\u00fc\u00b7\u00dfen", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und den farbigen Brautkranz", "tokens": ["Und", "den", "far\u00b7bi\u00b7gen", "Braut\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Flicht die Erde, die ewig junge", "tokens": ["Flicht", "die", "Er\u00b7de", ",", "die", "e\u00b7wig", "jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wieder in\u2019s perlenglitzernde Haar;", "tokens": ["Wie\u00b7der", "in's", "per\u00b7leng\u00b7lit\u00b7zern\u00b7de", "Haar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Aufleuchtend ergl\u00fcht", "tokens": ["Auf\u00b7leuch\u00b7tend", "er\u00b7gl\u00fcht"], "token_info": ["word", "word"], "pos": ["VVPP", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Zu neuer Freude das Auge,", "tokens": ["Zu", "neu\u00b7er", "Freu\u00b7de", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das zum Staube sich tr\u00fcb\u2019 gesenkt;", "tokens": ["Das", "zum", "Stau\u00b7be", "sich", "tr\u00fcb'", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hoffend wendet das Herz sich", "tokens": ["Hof\u00b7fend", "wen\u00b7det", "das", "Herz", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Der Zukunft zu,", "tokens": ["Der", "Zu\u00b7kunft", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die sich golden aufthut,", "tokens": ["Die", "sich", "gol\u00b7den", "auf\u00b7thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Und auf die Lippen dr\u00e4ngt,", "tokens": ["Und", "auf", "die", "Lip\u00b7pen", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Innig gefl\u00fcstert", "tokens": ["In\u00b7nig", "ge\u00b7fl\u00fcs\u00b7tert"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sich das tiefste Gebet der Seele.", "tokens": ["Sich", "das", "tiefs\u00b7te", "Ge\u00b7bet", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Selten in ", "tokens": ["Sel\u00b7ten", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ist der fr\u00f6hliche Lenz gekehrt,", "tokens": ["Ist", "der", "fr\u00f6h\u00b7li\u00b7che", "Lenz", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und meine Bl\u00fcthen", "tokens": ["Und", "mei\u00b7ne", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hast du mit Schauer umweht und Frost,", "tokens": ["Hast", "du", "mit", "Schau\u00b7er", "um\u00b7weht", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "VVFIN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Finster waltendes Schicksal:", "tokens": ["Fins\u00b7ter", "wal\u00b7ten\u00b7des", "Schick\u00b7sal", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Hast mich fr\u00fch hinausgedr\u00e4ngt,", "tokens": ["Hast", "mich", "fr\u00fch", "hin\u00b7aus\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem Leben zu k\u00e4mpfen,", "tokens": ["Mit", "dem", "Le\u00b7ben", "zu", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und strenge Nothwendigkeit", "tokens": ["Und", "stren\u00b7ge", "Noth\u00b7wen\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Verscheuchte die s\u00fc\u00dfen Bilder,", "tokens": ["Ver\u00b7scheuch\u00b7te", "die", "s\u00fc\u00b7\u00dfen", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Welche die Dichtung spinnt,", "tokens": ["Wel\u00b7che", "die", "Dich\u00b7tung", "spinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Die sorgenlose, die ewig", "tokens": ["Die", "sor\u00b7gen\u00b7lo\u00b7se", ",", "die", "e\u00b7wig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Heitere G\u00f6ttin.", "tokens": ["Hei\u00b7te\u00b7re", "G\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Gabst du den Kampf, ich habe gek\u00e4mpft!", "tokens": ["Gabst", "du", "den", "Kampf", ",", "ich", "ha\u00b7be", "ge\u00b7k\u00e4mpft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wirst du die Sonne mir verh\u00fcllen,", "tokens": ["Wirst", "du", "die", "Son\u00b7ne", "mir", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Dunkel werd\u2019 ich suchen den Weg \u2014", "tokens": ["Im", "Dun\u00b7kel", "werd'", "ich", "su\u00b7chen", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eins nur begehre ich.", "tokens": ["Eins", "nur", "be\u00b7geh\u00b7re", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df mir die Seele frei von Bitterni\u00df,", "tokens": ["La\u00df", "mir", "die", "See\u00b7le", "frei", "von", "Bit\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df mir immer traut und verst\u00e4ndlich", "tokens": ["Da\u00df", "mir", "im\u00b7mer", "traut", "und", "ver\u00b7st\u00e4nd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "ADJD"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Sprache sei,", "tokens": ["Die", "Spra\u00b7che", "sei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die der Mai spricht,", "tokens": ["Die", "der", "Mai", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da\u00df keine Rose vergebens", "tokens": ["Da\u00df", "kei\u00b7ne", "Ro\u00b7se", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den k\u00f6stlichen Hauch mir entgegenwehe,", "tokens": ["Den", "k\u00f6st\u00b7li\u00b7chen", "Hauch", "mir", "ent\u00b7ge\u00b7gen\u00b7we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Kein Lied,", "tokens": ["Kein", "Lied", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Das freier Kehle wirbelnd entsteigt,", "tokens": ["Das", "frei\u00b7er", "Keh\u00b7le", "wir\u00b7belnd", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Ungeh\u00f6rt an das Ohr mir schlage \u2026", "tokens": ["Un\u00b7ge\u00b7h\u00f6rt", "an", "das", "Ohr", "mir", "schla\u00b7ge", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00df mir die Seele frei von Neid,", "tokens": ["La\u00df", "mir", "die", "See\u00b7le", "frei", "von", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich gl\u00fccklichere Lippen", "tokens": ["La\u00df", "mich", "gl\u00fcck\u00b7li\u00b7che\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00fcrfen seh\u2019n der Freude Labetrunk", "tokens": ["Schl\u00fcr\u00b7fen", "seh'n", "der", "Freu\u00b7de", "La\u00b7be\u00b7trunk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und dann ruhig zur\u00fcckkehren", "tokens": ["Und", "dann", "ru\u00b7hig", "zu\u00b7r\u00fcck\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unter die Last der Arbeit,", "tokens": ["Un\u00b7ter", "die", "Last", "der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "In den eisernen Dienst der Pflicht.", "tokens": ["In", "den", "ei\u00b7ser\u00b7nen", "Dienst", "der", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}