{"textgrid.poem.40710": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[jung gefallen wollen, wer wird's schelten?]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung gefallen wollen, wer wird's schelten?", "tokens": ["Jung", "ge\u00b7fal\u00b7len", "wol\u00b7len", ",", "wer", "wird's", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VMINF", "$,", "PWS", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alt gefallen k\u00f6nnen, mehr wird's gelten;", "tokens": ["Alt", "ge\u00b7fal\u00b7len", "k\u00f6n\u00b7nen", ",", "mehr", "wird's", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMINF", "$,", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Da\u00df dir Beifall jetzt und einst nicht fehle,", "tokens": ["Da\u00df", "dir", "Bei\u00b7fall", "jetzt", "und", "einst", "nicht", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "KON", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das Arcanum such' in deiner Seele.", "tokens": ["Das", "Ar\u00b7ca\u00b7num", "such'", "in", "dei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}