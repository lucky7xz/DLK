{"dta.poem.20923": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rosemunde an Siegreichen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Held sey itzt \u00fcmkr\u00e4ntzt von tausend Lor-", "tokens": ["Mein", "Held", "sey", "itzt", "\u00fcm\u00b7kr\u00e4ntzt", "von", "tau\u00b7send", "Lor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "berzweigen/", "tokens": ["berz\u00b7wei\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Es stellen Ost und West sich zinsbar bey dir", "tokens": ["Es", "stel\u00b7len", "Ost", "und", "West", "sich", "zins\u00b7bar", "bey", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "PRF", "ADJD", "APPR", "PPER"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "ein/", "tokens": ["ein", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Es m\u00fcsse sich die Welt vor deinem Throne neigen/", "tokens": ["Es", "m\u00fcs\u00b7se", "sich", "die", "Welt", "vor", "dei\u00b7nem", "Thro\u00b7ne", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und aller V\u00f6lcker Gold dir Cron und Scepter seyn.", "tokens": ["Und", "al\u00b7ler", "V\u00f6l\u00b7cker", "Gold", "dir", "Cron", "und", "Scep\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "PPER", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "W\u00fcnscht deiner M\u00e4gde Magd die nichts hat zugeweh-", "tokens": ["W\u00fcnscht", "dei\u00b7ner", "M\u00e4g\u00b7de", "Magd", "die", "nichts", "hat", "zu\u00b7ge\u00b7weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN", "ART", "PIS", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Und auch nichts w\u00fcrdig ist: Was aber will ein Brieff/", "tokens": ["Und", "auch", "nichts", "w\u00fcr\u00b7dig", "ist", ":", "Was", "a\u00b7ber", "will", "ein", "Brieff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADJD", "VAFIN", "$.", "PWS", "ADV", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ungemeiner Hand und eyfrigen Begehren/", "tokens": ["Von", "un\u00b7ge\u00b7mei\u00b7ner", "Hand", "und", "ey\u00b7fri\u00b7gen", "Be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der heut\u2019 \u00fcm sieben Uhr in meine H\u00e4nde lieff?", "tokens": ["Der", "heut'", "\u00fcm", "sie\u00b7ben", "Uhr", "in", "mei\u00b7ne", "H\u00e4n\u00b7de", "lieff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "CARD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mich daucht\u2019 ich s\u00e4\u00dfe schon umbzirckt von Nacht und", "tokens": ["Mich", "daucht'", "ich", "s\u00e4\u00b7\u00dfe", "schon", "um\u00b7bz\u00b7irckt", "von", "Nacht", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "VVPP", "APPR", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Schatten/", "tokens": ["Schat\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Es blickte mich kein Stern mit seinen Zwinckern an/", "tokens": ["Es", "blick\u00b7te", "mich", "kein", "Stern", "mit", "sei\u00b7nen", "Zwin\u00b7ckern", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So darf ich/ wie es scheint/ fast in ein Licht gerathen/", "tokens": ["So", "darf", "ich", "/", "wie", "es", "scheint", "/", "fast", "in", "ein", "Licht", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$(", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem auch die Sonne selbst sich nicht vergleichen kan.", "tokens": ["Dem", "auch", "die", "Son\u00b7ne", "selbst", "sich", "nicht", "ver\u00b7glei\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADV", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich hofft\u2019 ich lege nu in meiner Ruh vergraben/", "tokens": ["Ich", "hofft'", "ich", "le\u00b7ge", "nu", "in", "mei\u00b7ner", "Ruh", "ver\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es kennte mich vielleicht der nechste Nachtbar nicht/", "tokens": ["Es", "kenn\u00b7te", "mich", "viel\u00b7leicht", "der", "nechs\u00b7te", "Nacht\u00b7bar", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So soll ich nunmehr selbst in mir Verr\u00e4ther haben/", "tokens": ["So", "soll", "ich", "nun\u00b7mehr", "selbst", "in", "mir", "Ver\u00b7r\u00e4\u00b7ther", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und zeucht ein schlechtes Lied mich in das Tage Licht.", "tokens": ["Und", "zeucht", "ein", "schlech\u00b7tes", "Lied", "mich", "in", "das", "Ta\u00b7ge", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich wei\u00df nicht wie mir ist und was ich soll beginnen/", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "mir", "ist", "und", "was", "ich", "soll", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "KON", "PWS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ob Aug\u2019 und Ohre mir die Zauberey bestrickt/", "tokens": ["Ob", "Aug'", "und", "Oh\u00b7re", "mir", "die", "Zau\u00b7be\u00b7rey", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ob mich ein todter Schlaf hat \u00fcberreden k\u00f6nnen/", "tokens": ["Ob", "mich", "ein", "tod\u00b7ter", "Schlaf", "hat", "\u00fc\u00b7berr\u00b7e\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df Siegerich mich kennt/ und mir ein Schreiben", "tokens": ["Da\u00df", "Sie\u00b7ge\u00b7rich", "mich", "kennt", "/", "und", "mir", "ein", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "schickt.", "tokens": ["schickt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Wie woll\u2019 ich aber doch nicht meinen Augen trauen?", "tokens": ["Wie", "woll'", "ich", "a\u00b7ber", "doch", "nicht", "mei\u00b7nen", "Au\u00b7gen", "trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich wach\u2019 und schlafe nicht/ ich rede mit Verstandt/", "tokens": ["Ich", "wach'", "und", "schla\u00b7fe", "nicht", "/", "ich", "re\u00b7de", "mit", "Ver\u00b7standt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich kann den kleinen Brief erbrechen und beschauen/", "tokens": ["Ich", "kann", "den", "klei\u00b7nen", "Brief", "er\u00b7bre\u00b7chen", "und", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und h\u00f6re diesen Freund/ den du hast abgesandt.", "tokens": ["Und", "h\u00f6\u00b7re", "die\u00b7sen", "Freund", "/", "den", "du", "hast", "ab\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$(", "ART", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es ist kein Bild vor mir/ ich f\u00fchl\u2019 ein wahres Wesen/", "tokens": ["Es", "ist", "kein", "Bild", "vor", "mir", "/", "ich", "f\u00fchl'", "ein", "wah\u00b7res", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PPER", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df das dieses Wachs ein hohes Siegel ist/", "tokens": ["Ich", "wei\u00df", "das", "die\u00b7ses", "Wachs", "ein", "ho\u00b7hes", "Sie\u00b7gel", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich k\u00fcsse was ich itzt von grosser Hand gelesen/", "tokens": ["Ich", "k\u00fcs\u00b7se", "was", "ich", "itzt", "von", "gros\u00b7ser", "Hand", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie aber/ da\u00df man mich zuschauen auserkiest?", "tokens": ["Wie", "a\u00b7ber", "/", "da\u00df", "man", "mich", "zu\u00b7schau\u00b7en", "aus\u00b7er\u00b7kiest", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "KOUS", "PIS", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mich/ eine schlechte Magd/ und arm von allen Sch\u00e4-", "tokens": ["Mich", "/", "ei\u00b7ne", "schlech\u00b7te", "Magd", "/", "und", "arm", "von", "al\u00b7len", "Sch\u00e4"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "ART", "ADJA", "NN", "$(", "KON", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.36": {"text": "Die sonsten die Natur den Frauen beygelegt/", "tokens": ["Die", "sons\u00b7ten", "die", "Na\u00b7tur", "den", "Frau\u00b7en", "bey\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mich/ die sich sch\u00e4men mu\u00df sich in den Orth zusetzen/", "tokens": ["Mich", "/", "die", "sich", "sch\u00e4\u00b7men", "mu\u00df", "sich", "in", "den", "Orth", "zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PRF", "VVINF", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wo Sch\u00f6nheit und Verstand zusammen seyn gepregt.", "tokens": ["Wo", "Sch\u00f6n\u00b7heit", "und", "Ver\u00b7stand", "zu\u00b7sam\u00b7men", "seyn", "ge\u00b7pregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mein schwaches Auge kan die Strahlen nicht vertrage\u0303/", "tokens": ["Mein", "schwa\u00b7ches", "Au\u00b7ge", "kan", "die", "Strah\u00b7len", "nicht", "ver\u00b7trag\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein schlechter Zeug/ wie ich/ w\u00fcnscht keinen hellen", "tokens": ["Ein", "schlech\u00b7ter", "Zeug", "/", "wie", "ich", "/", "w\u00fcnscht", "kei\u00b7nen", "hel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "PPER", "$(", "VVFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Tag/", "tokens": ["Tag", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Es darf sich ja das Wachs nicht in die Sonne wagen/", "tokens": ["Es", "darf", "sich", "ja", "das", "Wachs", "nicht", "in", "die", "Son\u00b7ne", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Man wei\u00df wohl da\u00df ein Gla\u00df die Gluth nicht leiden", "tokens": ["Man", "wei\u00df", "wohl", "da\u00df", "ein", "Gla\u00df", "die", "Gluth", "nicht", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "KOUS", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "mag.", "tokens": ["mag."], "token_info": ["abbreviation"], "pos": ["NE"]}, "line.45": {"text": "Der Schatten ist mein Freund/ dazu ich bin gebohren/", "tokens": ["Der", "Schat\u00b7ten", "ist", "mein", "Freund", "/", "da\u00b7zu", "ich", "bin", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PAV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Es bleibt die Einsamkeit mein bestes Vaterland/", "tokens": ["Es", "bleibt", "die", "Ein\u00b7sam\