{"textgrid.poem.37300": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[nun, da die Fr\u00fchlingsblumen wieder bl\u00fchen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, da die Fr\u00fchlingsblumen wieder bl\u00fchen,", "tokens": ["Nun", ",", "da", "die", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "wie\u00b7der", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In milder Luft die wei\u00dfen Wolken ziehen,", "tokens": ["In", "mil\u00b7der", "Luft", "die", "wei\u00b7\u00dfen", "Wol\u00b7ken", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denk ich mit Wehmut deiner Lieb und G\u00fcte,", "tokens": ["Denk", "ich", "mit", "Weh\u00b7mut", "dei\u00b7ner", "Lieb", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du s\u00fc\u00dfes M\u00e4dchen, das so fr\u00fch verbl\u00fchte.", "tokens": ["Du", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ",", "das", "so", "fr\u00fch", "ver\u00b7bl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du liebtest nicht der Feste L\u00e4rm und Gaffen,", "tokens": ["Du", "lieb\u00b7test", "nicht", "der", "Fes\u00b7te", "L\u00e4rm", "und", "Gaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erw\u00e4hltest dir daheim ein stilles Schaffen,", "tokens": ["Er\u00b7w\u00e4hl\u00b7test", "dir", "da\u00b7heim", "ein", "stil\u00b7les", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Sorge und Geduld, das Dienen, Geben,", "tokens": ["Die", "Sor\u00b7ge", "und", "Ge\u00b7duld", ",", "das", "Die\u00b7nen", ",", "Ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein innigliches Nurf\u00fcrandreleben.", "tokens": ["Ein", "in\u00b7nig\u00b7li\u00b7ches", "Nur\u00b7f\u00fc\u00b7rand\u00b7re\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "So teiltest du in deines Vaters Haus", "tokens": ["So", "teil\u00b7test", "du", "in", "dei\u00b7nes", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Himmelsfrieden deiner Seele aus.", "tokens": ["Den", "Him\u00b7mels\u00b7frie\u00b7den", "dei\u00b7ner", "See\u00b7le", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bald aber kamen schwere, schwere Zeiten.", "tokens": ["Bald", "a\u00b7ber", "ka\u00b7men", "schwe\u00b7re", ",", "schwe\u00b7re", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wir mu\u00dften dir die Lagerstatt bereiten;", "tokens": ["Wir", "mu\u00df\u00b7ten", "dir", "die", "La\u00b7ger\u00b7statt", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wir sahn, wie deine lieben Wangen bleichten,", "tokens": ["Wir", "sahn", ",", "wie", "dei\u00b7ne", "lie\u00b7ben", "Wan\u00b7gen", "bleich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sahn deiner Augen wundersames Leuchten;", "tokens": ["Sahn", "dei\u00b7ner", "Au\u00b7gen", "wun\u00b7der\u00b7sa\u00b7mes", "Leuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wir weinten in der Stille, denn wir wu\u00dften,", "tokens": ["Wir", "wein\u00b7ten", "in", "der", "Stil\u00b7le", ",", "denn", "wir", "wu\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df wir nun bald auf ewig scheiden mu\u00dften.", "tokens": ["Da\u00df", "wir", "nun", "bald", "auf", "e\u00b7wig", "schei\u00b7den", "mu\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Du klagtest nicht. Voll Milde und Erbarmen", "tokens": ["Du", "klag\u00b7test", "nicht", ".", "Voll", "Mil\u00b7de", "und", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Gedachtest du der bittern Not der Armen,", "tokens": ["Ge\u00b7dach\u00b7test", "du", "der", "bit\u00b7tern", "Not", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Gabst ihnen deine ganze kleine Habe", "tokens": ["Gabst", "ih\u00b7nen", "dei\u00b7ne", "gan\u00b7ze", "klei\u00b7ne", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und seufztest tief, da\u00df so gering die Gabe.", "tokens": ["Und", "seufz\u00b7test", "tief", ",", "da\u00df", "so", "ge\u00b7ring", "die", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Es war die letzte Nacht und nah das Ende;", "tokens": ["Es", "war", "die", "letz\u00b7te", "Nacht", "und", "nah", "das", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wir k\u00fc\u00dften dir die zarten wei\u00dfen H\u00e4nde;", "tokens": ["Wir", "k\u00fc\u00df\u00b7ten", "dir", "die", "zar\u00b7ten", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Du sprachst, lebt wohl, in deiner stillen Weise,", "tokens": ["Du", "sprachst", ",", "lebt", "wohl", ",", "in", "dei\u00b7ner", "stil\u00b7len", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und: oh, die sch\u00f6nen Blumen! riefst du leise.", "tokens": ["Und", ":", "oh", ",", "die", "sch\u00f6\u00b7nen", "Blu\u00b7men", "!", "riefst", "du", "lei\u00b7se", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "FM", "$,", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Dann war's vorbei. Die gro\u00dfen Augensterne,", "tokens": ["Dann", "wa\u00b7r's", "vor\u00b7bei", ".", "Die", "gro\u00b7\u00dfen", "Au\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Weit, unbeweglich, starrten in die Ferne,", "tokens": ["Weit", ",", "un\u00b7be\u00b7weg\u00b7lich", ",", "starr\u00b7ten", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Indes um deine Lippen, halbgeschlossen,", "tokens": ["In\u00b7des", "um", "dei\u00b7ne", "Lip\u00b7pen", ",", "halb\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ein kindlichernstes L\u00e4cheln ausgegossen.", "tokens": ["Ein", "kind\u00b7li\u00b7cherns\u00b7tes", "L\u00e4\u00b7cheln", "aus\u00b7ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So lagst du da, als h\u00e4ttest du entz\u00fcckt", "tokens": ["So", "lagst", "du", "da", ",", "als", "h\u00e4t\u00b7test", "du", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und staunend eine neue Welt erblickt.", "tokens": ["Und", "stau\u00b7nend", "ei\u00b7ne", "neu\u00b7e", "Welt", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wo bist du nun, du s\u00fc\u00dfes Kind, geblieben?", "tokens": ["Wo", "bist", "du", "nun", ",", "du", "s\u00fc\u00b7\u00dfes", "Kind", ",", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Bist du ein Bild im Denken deiner Lieben?", "tokens": ["Bist", "du", "ein", "Bild", "im", "Den\u00b7ken", "dei\u00b7ner", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Hast du die wei\u00dfen Schwingen ausgebreitet,", "tokens": ["Hast", "du", "die", "wei\u00b7\u00dfen", "Schwin\u00b7gen", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und zogst hinauf von Engelshand geleitet", "tokens": ["Und", "zogst", "hin\u00b7auf", "von", "En\u00b7gels\u00b7hand", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Zu jener Gottesstadt im Paradiese,", "tokens": ["Zu", "je\u00b7ner", "Got\u00b7tes\u00b7stadt", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wo auf der heiligstillen Bl\u00fctenwiese", "tokens": ["Wo", "auf", "der", "hei\u00b7lig\u00b7stil\u00b7len", "Bl\u00fc\u00b7ten\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Fernher in feierlichem Zug die Frommen", "tokens": ["Fern\u00b7her", "in", "fei\u00b7er\u00b7li\u00b7chem", "Zug", "die", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Anbetend zu dem Bild des Lammes kommen?", "tokens": ["An\u00b7be\u00b7tend", "zu", "dem", "Bild", "des", "Lam\u00b7mes", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wo du auch seist; im Herzen bleibst du mein.", "tokens": ["Wo", "du", "auch", "seist", ";", "im", "Her\u00b7zen", "bleibst", "du", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$.", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Was Gutes in mir lebt, dein ist's allein.", "tokens": ["Was", "Gu\u00b7tes", "in", "mir", "lebt", ",", "dein", "ist's", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "VVFIN", "$,", "PPOSAT", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}