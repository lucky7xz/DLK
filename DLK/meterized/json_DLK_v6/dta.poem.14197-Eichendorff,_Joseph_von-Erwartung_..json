{"dta.poem.14197": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Erwartung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schauert nun im Grunde", "tokens": ["Wie", "schau\u00b7ert", "nun", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der tiefsten Seele mich!", "tokens": ["Der", "tiefs\u00b7ten", "See\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie \u00f6de ist die Runde", "tokens": ["Wie", "\u00f6\u00b7de", "ist", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und einsam ohne Dich!", "tokens": ["Und", "ein\u00b7sam", "oh\u00b7ne", "Dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}