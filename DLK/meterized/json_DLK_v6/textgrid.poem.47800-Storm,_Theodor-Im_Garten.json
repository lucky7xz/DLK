{"textgrid.poem.47800": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcte, h\u00fcte den Fu\u00df und die H\u00e4nde,", "tokens": ["H\u00fc\u00b7te", ",", "h\u00fc\u00b7te", "den", "Fu\u00df", "und", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Eh sie ber\u00fchren das \u00e4rmste Ding!", "tokens": ["Eh", "sie", "be\u00b7r\u00fch\u00b7ren", "das", "\u00e4rms\u00b7te", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn du zertrittst eine h\u00e4\u00dfliche Raupe", "tokens": ["Denn", "du", "zer\u00b7trittst", "ei\u00b7ne", "h\u00e4\u00df\u00b7li\u00b7che", "Rau\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und t\u00f6test den sch\u00f6nsten Schmetterling.", "tokens": ["Und", "t\u00f6\u00b7test", "den", "sch\u00f6ns\u00b7ten", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}