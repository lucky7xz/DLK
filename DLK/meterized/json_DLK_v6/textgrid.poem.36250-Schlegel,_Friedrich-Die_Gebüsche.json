{"textgrid.poem.36250": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Geb\u00fcsche", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wehet k\u00fchl und leise", "tokens": ["Es", "we\u00b7het", "k\u00fchl", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Luft durch dunkle Auen,", "tokens": ["Die", "Luft", "durch", "dunk\u00b7le", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nur der Himmel l\u00e4chelt", "tokens": ["Und", "nur", "der", "Him\u00b7mel", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus tausend hellen Augen.", "tokens": ["Aus", "tau\u00b7send", "hel\u00b7len", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es regt nur Eine Seele", "tokens": ["Es", "regt", "nur", "Ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich in der Meere Brausen,", "tokens": ["Sich", "in", "der", "Mee\u00b7re", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und in den leisen Worten,", "tokens": ["Und", "in", "den", "lei\u00b7sen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die durch die Bl\u00e4tter rauschen.", "tokens": ["Die", "durch", "die", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So t\u00f6nt in Welle Welle,", "tokens": ["So", "t\u00f6nt", "in", "Wel\u00b7le", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wo Geister heimlich trauren;", "tokens": ["Wo", "Geis\u00b7ter", "heim\u00b7lich", "trau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So folgen Worte Worten,", "tokens": ["So", "fol\u00b7gen", "Wor\u00b7te", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wo Geister Leben hauchen.", "tokens": ["Wo", "Geis\u00b7ter", "Le\u00b7ben", "hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Durch alle T\u00f6ne t\u00f6net", "tokens": ["Durch", "al\u00b7le", "T\u00f6\u00b7ne", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Im bunten Erdentraume", "tokens": ["Im", "bun\u00b7ten", "Er\u00b7den\u00b7trau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein leiser Ton gezogen,", "tokens": ["Ein", "lei\u00b7ser", "Ton", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "F\u00fcr den, der heimlich lauschet.", "tokens": ["F\u00fcr", "den", ",", "der", "heim\u00b7lich", "lau\u00b7schet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}