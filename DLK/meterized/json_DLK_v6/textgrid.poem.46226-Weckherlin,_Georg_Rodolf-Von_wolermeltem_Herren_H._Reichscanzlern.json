{"textgrid.poem.46226": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von wolermeltem Herren H. Reichscanzlern", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kan mit solcher kraft und wunderreichem schein", "tokens": ["Er", "kan", "mit", "sol\u00b7cher", "kraft", "und", "wun\u00b7der\u00b7rei\u00b7chem", "schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die forcht in unserm feind, den mut in uns vermehren,", "tokens": ["die", "forcht", "in", "un\u00b7serm", "feind", ",", "den", "mut", "in", "uns", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ihn als eine sonn, nicht einen ", "tokens": ["da\u00df", "ihn", "als", "ei\u00b7ne", "sonn", ",", "nicht", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ART", "ADV", "$,", "PTKNEG", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit wunder m\u00e4niglich mu\u00df sehen, segnen, ehren.", "tokens": ["mit", "wun\u00b7der", "m\u00e4\u00b7nig\u00b7lich", "mu\u00df", "se\u00b7hen", ",", "seg\u00b7nen", ",", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zufl\u00fcchtig suchen ihn reich und arm, gro\u00df und klein,", "tokens": ["Zu\u00b7fl\u00fcch\u00b7tig", "su\u00b7chen", "ihn", "reich", "und", "arm", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die pfleget seines rats und trosts er zu gewehren,", "tokens": ["die", "pfle\u00b7get", "sei\u00b7nes", "rats", "und", "trosts", "er", "zu", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df er, als der welt ", "tokens": ["da\u00df", "er", ",", "als", "der", "welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "allf\u00e4hig, allein wert zu r\u00fchmen, zu begehren.", "tokens": ["all\u00b7f\u00e4\u00b7hig", ",", "al\u00b7lein", "wert", "zu", "r\u00fch\u00b7men", ",", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum verbleibet er, ein wunder aller zeit,", "tokens": ["Da\u00b7rum", "ver\u00b7blei\u00b7bet", "er", ",", "ein", "wun\u00b7der", "al\u00b7ler", "zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "durch seiner gotsforcht, treu und weisheit edle thaten", "tokens": ["durch", "sei\u00b7ner", "gots\u00b7forcht", ",", "treu", "und", "weis\u00b7heit", "ed\u00b7le", "tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "NN", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der inhalt aller lehr, der probstein weiser leut;", "tokens": ["der", "in\u00b7halt", "al\u00b7ler", "lehr", ",", "der", "probs\u00b7tein", "wei\u00b7ser", "leut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des fridens lehr und lieb, ein beispil der soldaten,", "tokens": ["Des", "fri\u00b7dens", "lehr", "und", "lieb", ",", "ein", "bei\u00b7spil", "der", "sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "$,", "ART", "PIS", "ART", "VMFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "der spigel guter r\u00e4t, sigweiser in dem streit,", "tokens": ["der", "spi\u00b7gel", "gu\u00b7ter", "r\u00e4t", ",", "sig\u00b7wei\u00b7ser", "in", "dem", "streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der tugend ganze sum, lehrbuch der potentaten.", "tokens": ["der", "tu\u00b7gend", "gan\u00b7ze", "sum", ",", "lehr\u00b7buch", "der", "po\u00b7ten\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NE", "$,", "NN", "ART", "ADJA", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}