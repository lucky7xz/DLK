{"textgrid.poem.36782": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Armer Tempel, wie droht dein Rundgew\u00f6lbe zu st\u00fcrzen,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armer Tempel, wie droht dein Rundgew\u00f6lbe zu st\u00fcrzen,", "tokens": ["Ar\u00b7mer", "Tem\u00b7pel", ",", "wie", "droht", "dein", "Rund\u00b7ge\u00b7w\u00f6l\u00b7be", "zu", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nur das m\u00e4cht'ge Geb\u00e4lk h\u00e4lt dein verwittert Geb\u00e4u.", "tokens": ["Nur", "das", "m\u00e4cht'\u00b7ge", "Ge\u00b7b\u00e4lk", "h\u00e4lt", "dein", "ver\u00b7wit\u00b7tert", "Ge\u00b7b\u00e4u", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "VVFIN", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn die Weisheit nichts gilt, die heilende, f\u00e4llt auch der Tempel,", "tokens": ["Wenn", "die", "Weis\u00b7heit", "nichts", "gilt", ",", "die", "hei\u00b7len\u00b7de", ",", "f\u00e4llt", "auch", "der", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "$,", "ART", "ADJA", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und aus Sonderbarkeit h\u00e4lt man das Ding noch so so.", "tokens": ["Und", "aus", "Son\u00b7der\u00b7bar\u00b7keit", "h\u00e4lt", "man", "das", "Ding", "noch", "so", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}