{"dta.poem.17140": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "241.  Die geheime Seelenflucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Herodes ist der Feind; Der Joseph der Verstand/", "tokens": ["He\u00b7ro\u00b7des", "ist", "der", "Feind", ";", "Der", "Jo\u00b7se\u00b7ph", "der", "Ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Dem macht GOtt die Gefahr im Traum (im Geist)", "tokens": ["Dem", "macht", "Gott", "die", "Ge\u00b7fahr", "im", "Traum", "(", "im", "Geist", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "NN", "APPRART", "NN", "$(", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "bekandt.", "tokens": ["be\u00b7kandt", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Die Welt ist Bethlehem Egypten Einsamkeit:", "tokens": ["Die", "Welt", "ist", "Beth\u00b7le\u00b7hem", "E\u00b7gyp\u00b7ten", "Ein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fleuch meine Seele fleuch/ sonst stirbestu f\u00fcr Leyd.", "tokens": ["Fleuch", "mei\u00b7ne", "See\u00b7le", "fleuch", "/", "sonst", "stir\u00b7be\u00b7stu", "f\u00fcr", "Le