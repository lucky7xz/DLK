{"textgrid.poem.57847": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verraten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu L\u00fcneburg auf dem Kalkberg", "tokens": ["Zu", "L\u00fc\u00b7ne\u00b7burg", "auf", "dem", "Kalk\u00b7berg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da trug ich das Schandenkleid;", "tokens": ["Da", "trug", "ich", "das", "Schan\u00b7den\u00b7kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu L\u00fcneburg schob ich den Karren", "tokens": ["Zu", "L\u00fc\u00b7ne\u00b7burg", "schob", "ich", "den", "Kar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In Kummer und Herzeleid.", "tokens": ["In", "Kum\u00b7mer", "und", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich schob ihn sieben Jahre,", "tokens": ["Ich", "schob", "ihn", "sie\u00b7ben", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Karren und der war schwer;", "tokens": ["Der", "Kar\u00b7ren", "und", "der", "war", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Ketten an meinen F\u00fc\u00dfen", "tokens": ["Die", "Ket\u00b7ten", "an", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die dr\u00fcckten mich allzusehr.", "tokens": ["Die", "dr\u00fcck\u00b7ten", "mich", "all\u00b7zu\u00b7sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und als ich kam wieder nach Hause,", "tokens": ["Und", "als", "ich", "kam", "wie\u00b7der", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wandtest du ab dein Gesicht;", "tokens": ["Da", "wand\u00b7test", "du", "ab", "dein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Um dich kam ich in Schande,", "tokens": ["Um", "dich", "kam", "ich", "in", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Du aber kennst mich nicht.", "tokens": ["Du", "a\u00b7ber", "kennst", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es wurden meine H\u00e4nde", "tokens": ["Es", "wur\u00b7den", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um dich von Blute rot;", "tokens": ["Um", "dich", "von", "Blu\u00b7te", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um dich schob ich den Karren", "tokens": ["Um", "dich", "schob", "ich", "den", "Kar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In Kummer und in Not.", "tokens": ["In", "Kum\u00b7mer", "und", "in", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So fahre denn hin, du Falsche,", "tokens": ["So", "fah\u00b7re", "denn", "hin", ",", "du", "Fal\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es soll mich nicht gereun;", "tokens": ["Es", "soll", "mich", "nicht", "ge\u00b7reun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu L\u00fcneburg auf dem Kalkberg", "tokens": ["Zu", "L\u00fc\u00b7ne\u00b7burg", "auf", "dem", "Kalk\u00b7berg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da wird mein Ende sein.", "tokens": ["Da", "wird", "mein", "En\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}