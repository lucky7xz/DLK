{"textgrid.poem.31688": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blume starb auf eis'gen Feldern,", "tokens": ["Die", "Blu\u00b7me", "starb", "auf", "eis'\u00b7gen", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein einsam Herz ist dumpf und schwer,", "tokens": ["Mein", "ein\u00b7sam", "Herz", "ist", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Adler in \u00f6den W\u00e4ldern,", "tokens": ["Ich", "bin", "ein", "Ad\u00b7ler", "in", "\u00f6\u00b7den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine arme M\u00f6we auf w\u00fcstem Meer.", "tokens": ["Ei\u00b7ne", "ar\u00b7me", "M\u00f6\u00b7we", "auf", "w\u00fcs\u00b7tem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O la\u00df den Fr\u00fchling wieder t\u00f6nen,", "tokens": ["O", "la\u00df", "den", "Fr\u00fch\u00b7ling", "wie\u00b7der", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du lieber Gott, und meinen Sinn \u2013", "tokens": ["Du", "lie\u00b7ber", "Gott", ",", "und", "mei\u00b7nen", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O tr\u00f6ste ihn mit einer sch\u00f6nen", "tokens": ["O", "tr\u00f6s\u00b7te", "ihn", "mit", "ei\u00b7ner", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja M\u00f6win oder Adlerin!", "tokens": ["Ja", "M\u00f6\u00b7win", "o\u00b7der", "Ad\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}