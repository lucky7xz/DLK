{"textgrid.poem.48145": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und diese Sehnsucht nach dem gr\u00fcnen Meer", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und diese Sehnsucht nach dem gr\u00fcnen Meer", "tokens": ["Und", "die\u00b7se", "Sehn\u00b7sucht", "nach", "dem", "gr\u00fc\u00b7nen", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nach Capris duftgetr\u00e4nkten Tr\u00fcmmerfeldern,", "tokens": ["nach", "Cap\u00b7ris", "duft\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Tr\u00fcm\u00b7mer\u00b7fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nach seinen Vignen, schattig, traubenschwer,", "tokens": ["nach", "sei\u00b7nen", "Vig\u00b7nen", ",", "schat\u00b7tig", ",", "trau\u00b7ben\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nach seinen Oelbaum- und Cacteenw\u00e4ldern:", "tokens": ["nach", "sei\u00b7nen", "O\u00b7el\u00b7baum", "und", "Cac\u00b7teen\u00b7w\u00e4l\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die alte Sehnsucht, die uns s\u00fcdw\u00e4rts trieb", "tokens": ["Die", "al\u00b7te", "Sehn\u00b7sucht", ",", "die", "uns", "s\u00fcd\u00b7w\u00e4rts", "trieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und nicht erlosch an Barjas Rosenborden,", "tokens": ["und", "nicht", "er\u00b7losch", "an", "Bar\u00b7jas", "Ro\u00b7sen\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die hei\u00df lebendig uns im Herzen blieb", "tokens": ["die", "hei\u00df", "le\u00b7ben\u00b7dig", "uns", "im", "Her\u00b7zen", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und ihre Netze spann im grauen Norden \u2013", "tokens": ["und", "ih\u00b7re", "Net\u00b7ze", "spann", "im", "grau\u00b7en", "Nor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "heut wird sie Herr! \u2013 Den vollen Becher Weins", "tokens": ["heut", "wird", "sie", "Herr", "!", "\u2013", "Den", "vol\u00b7len", "Be\u00b7cher", "Weins"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bringe ich ihr. Wir wollen wieder wandern", "tokens": ["brin\u00b7ge", "ich", "ihr", ".", "Wir", "wol\u00b7len", "wie\u00b7der", "wan\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PPER", "VMFIN", "ADV", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und S\u00fcdlands Sonne trinken, bis wir eins", "tokens": ["und", "S\u00fcd\u00b7lands", "Son\u00b7ne", "trin\u00b7ken", ",", "bis", "wir", "eins"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVINF", "$,", "KOUS", "PPER", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "erloschen und ertrunken sind im andern.", "tokens": ["er\u00b7lo\u00b7schen", "und", "er\u00b7trun\u00b7ken", "sind", "im", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVINF", "VAFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann senkt sie still das Haupt zum klaren Grund,", "tokens": ["Dann", "senkt", "sie", "still", "das", "Haupt", "zum", "kla\u00b7ren", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die bunten Muscheln schimmern ihr zu F\u00fc\u00dfen,", "tokens": ["die", "bun\u00b7ten", "Mu\u00b7scheln", "schim\u00b7mern", "ihr", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und den geschlossenen Sirenenmund", "tokens": ["und", "den", "ge\u00b7schlos\u00b7se\u00b7nen", "Si\u00b7re\u00b7nen\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "werden des Golfes k\u00fchle Lippen k\u00fcssen.", "tokens": ["wer\u00b7den", "des", "Gol\u00b7fes", "k\u00fch\u00b7le", "Lip\u00b7pen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}