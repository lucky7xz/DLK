{"textgrid.poem.32955": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. [Treib's, wie du willst]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treib's, wie du willst,", "tokens": ["Treib's", ",", "wie", "du", "willst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "es ist nicht recht!", "tokens": ["es", "ist", "nicht", "recht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "sei klug, sei dumm,", "tokens": ["sei", "klug", ",", "sei", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "sei brav, sei schlecht!", "tokens": ["sei", "brav", ",", "sei", "schlecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein jedes Ding", "tokens": ["Ein", "je\u00b7des", "Ding"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "hat jeden Zweck!", "tokens": ["hat", "je\u00b7den", "Zweck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer immer fragt,", "tokens": ["Wer", "im\u00b7mer", "fragt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "kommt nie vom Fleck!", "tokens": ["kommt", "nie", "vom", "Fleck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und wer nicht selbst", "tokens": ["Und", "wer", "nicht", "selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ein Urteil hat", "tokens": ["ein", "Ur\u00b7teil", "hat"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und was drauf wagt,", "tokens": ["und", "was", "drauf", "wagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "sitzt immer ... patt!", "tokens": ["sitzt", "im\u00b7mer", "...", "patt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}