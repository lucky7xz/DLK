{"textgrid.poem.62589": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Wenn das Wasser in den N\u00e4chten stille, klar und ruhig fleu\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Wasser in den N\u00e4chten stille, klar und ruhig fleu\u00dft,", "tokens": ["Wenn", "das", "Was\u00b7ser", "in", "den", "N\u00e4ch\u00b7ten", "stil\u00b7le", ",", "klar", "und", "ru\u00b7hig", "fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sich der helle Glanz der Sterne in denselben lieblich wei\u00dft.", "tokens": ["Sich", "der", "hel\u00b7le", "Glanz", "der", "Ster\u00b7ne", "in", "den\u00b7sel\u00b7ben", "lieb\u00b7lich", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PDAT", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also, ist ein frommer Christ in der Kreuzes-Noth gelassen;", "tokens": ["Al\u00b7so", ",", "ist", "ein", "from\u00b7mer", "Christ", "in", "der", "Kreu\u00b7zes\u00b7Noth", "ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "H\u00e4lt er Gott geruhig still; wei\u00df er sich getrost zu fassen,", "tokens": ["H\u00e4lt", "er", "Gott", "ge\u00b7ru\u00b7hig", "still", ";", "wei\u00df", "er", "sich", "ge\u00b7trost", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "PTKVZ", "$.", "VVFIN", "PPER", "PRF", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "O! so funckelt Gottes Gnade; Ey so w\u00fcrkt die Kraft des Herrn", "tokens": ["O", "!", "so", "fun\u00b7ckelt", "Got\u00b7tes", "Gna\u00b7de", ";", "Ey", "so", "w\u00fcrkt", "die", "Kraft", "des", "Herrn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "NN", "NN", "$.", "NN", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Hell und stark in seiner Seele, wie der sch\u00f6ne Abend-Stern.", "tokens": ["Hell", "und", "stark", "in", "sei\u00b7ner", "See\u00b7le", ",", "wie", "der", "sch\u00f6\u00b7ne", "A\u00b7ben\u00b7dS\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}