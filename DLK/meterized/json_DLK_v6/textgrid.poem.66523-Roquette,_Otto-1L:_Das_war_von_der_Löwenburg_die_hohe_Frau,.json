{"textgrid.poem.66523": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war von der L\u00f6wenburg die hohe Frau,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war von der L\u00f6wenburg die hohe Frau,", "tokens": ["Das", "war", "von", "der", "L\u00f6\u00b7wen\u00b7burg", "die", "ho\u00b7he", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die hatt' einen einzigen Sohn;", "tokens": ["Die", "hatt'", "ei\u00b7nen", "ein\u00b7zi\u00b7gen", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur nach waidlichem Werk, mit dem Morgenthau,", "tokens": ["Nur", "nach", "waid\u00b7li\u00b7chem", "Werk", ",", "mit", "dem", "Mor\u00b7gent\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stand sein Sinn, bei des Jagdhorns Ton.", "tokens": ["Stand", "sein", "Sinn", ",", "bei", "des", "Jagd\u00b7horns", "Ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Die Gr\u00e4fin dachte, wie mach' ich dich zahm,", "tokens": ["Die", "Gr\u00e4\u00b7fin", "dach\u00b7te", ",", "wie", "mach'", "ich", "dich", "zahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mein junger Falke du?", "tokens": ["Mein", "jun\u00b7ger", "Fal\u00b7ke", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis da\u00df ihr der Schalk zu H\u00fclfe kam,", "tokens": ["Bis", "da\u00df", "ihr", "der", "Schalk", "zu", "H\u00fcl\u00b7fe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Der sprach, und sie lachte dazu.", "tokens": ["Der", "sprach", ",", "und", "sie", "lach\u00b7te", "da\u00b7zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie lachte, doch fa\u00dft sie sich ernsten Muth:", "tokens": ["Sie", "lach\u00b7te", ",", "doch", "fa\u00dft", "sie", "sich", "erns\u00b7ten", "Muth", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So sollst du mein Bote sein!", "tokens": ["So", "sollst", "du", "mein", "Bo\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dies Brieflein geb' ich in deine Hut,", "tokens": ["Dies", "Brie\u00b7flein", "geb'", "ich", "in", "dei\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An des Pfalzgrafen T\u00f6chterlein.", "tokens": ["An", "des", "Pfalz\u00b7gra\u00b7fen", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann auf, in den Wald, und zu wagendem Spiel", "tokens": ["Dann", "auf", ",", "in", "den", "Wald", ",", "und", "zu", "wa\u00b7gen\u00b7dem", "Spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mit Herrn Godilo, meinem Sohn!", "tokens": ["Mit", "Herrn", "Go\u00b7di\u00b7lo", ",", "mei\u00b7nem", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kommen wir noch zu fr\u00f6hlichem Ziel,", "tokens": ["Und", "kom\u00b7men", "wir", "noch", "zu", "fr\u00f6h\u00b7li\u00b7chem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So habe der Schalk seinen Lohn!", "tokens": ["So", "ha\u00b7be", "der", "Schalk", "sei\u00b7nen", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Herr Godilo jagte durch Wald und Geheg,", "tokens": ["Herr", "Go\u00b7di\u00b7lo", "jag\u00b7te", "durch", "Wald", "und", "Ge\u00b7heg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ward zornig vor M\u00fcdigkeit:", "tokens": ["Ward", "zor\u00b7nig", "vor", "M\u00fc\u00b7dig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du Schalk, hast verwirrt mir Weg und Steg!", "tokens": ["Du", "Schalk", ",", "hast", "ver\u00b7wirrt", "mir", "Weg", "und", "Steg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VAFIN", "ADJD", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00e4r' ein Br\u00fcnnlein dem Durst nur bereit! \u2013", "tokens": ["W\u00e4r'", "ein", "Br\u00fcnn\u00b7lein", "dem", "Durst", "nur", "be\u00b7reit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Das Br\u00fcnnlein, da flie\u00dft gar k\u00fchl und gemach,", "tokens": ["Das", "Br\u00fcnn\u00b7lein", ",", "da", "flie\u00dft", "gar", "k\u00fchl", "und", "ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und winkt uns hinunter zum Rhein,", "tokens": ["Und", "winkt", "uns", "hin\u00b7un\u00b7ter", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Zum Willkomm gr\u00fc\u00dft unter'm Schattendach", "tokens": ["Zum", "Will\u00b7komm", "gr\u00fc\u00dft", "un\u00b7ter'm", "Schat\u00b7ten\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Des Winzers T\u00f6chterlein.", "tokens": ["Des", "Win\u00b7zers", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Godilo kannte kein Jagen mehr,", "tokens": ["Herr", "Go\u00b7di\u00b7lo", "kann\u00b7te", "kein", "Ja\u00b7gen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "War am Br\u00fcnnlein in jeder Fr\u00fch':", "tokens": ["War", "am", "Br\u00fcnn\u00b7lein", "in", "je\u00b7der", "Fr\u00fch'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Um dein nur, o Jungfrau, komm' ich daher,", "tokens": ["Um", "dein", "nur", ",", "o", "Jung\u00b7frau", ",", "komm'", "ich", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch dankst du mir nicht die M\u00fch'!", "tokens": ["Doch", "dankst", "du", "mir", "nicht", "die", "M\u00fch'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbbin nichts, als des Winzers T\u00f6chterlein,", "tokens": ["\u00bb", "bin", "nichts", ",", "als", "des", "Win\u00b7zers", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wei\u00df nichts, was euch lohnte den Gang!\u00ab", "tokens": ["Wei\u00df", "nichts", ",", "was", "euch", "lohn\u00b7te", "den", "Gang", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "So sollst du im Schlo\u00df mir die Herrin sein,", "tokens": ["So", "sollst", "du", "im", "Schlo\u00df", "mir", "die", "Her\u00b7rin", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die ganz mir die Seele bezwang! \u2013", "tokens": ["Die", "ganz", "mir", "die", "See\u00b7le", "be\u00b7zwang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Die Gr\u00e4fin wohl auf den S\u00f6ller schritt,", "tokens": ["Die", "Gr\u00e4\u00b7fin", "wohl", "auf", "den", "S\u00f6l\u00b7ler", "schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Pfalzgraf an ihrer Seit'.", "tokens": ["Der", "Pfalz\u00b7graf", "an", "ih\u00b7rer", "Seit'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu der L\u00f6wenburg ein Gefolge ritt,", "tokens": ["Zu", "der", "L\u00f6\u00b7wen\u00b7burg", "ein", "Ge\u00b7fol\u00b7ge", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und sie winkten und lachten Beid'.", "tokens": ["Und", "sie", "wink\u00b7ten", "und", "lach\u00b7ten", "Beid'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbherr Godilo, da\u00df sie des Grafen Kind,", "tokens": ["\u00bb", "herr", "Go\u00b7di\u00b7lo", ",", "da\u00df", "sie", "des", "Gra\u00b7fen", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Das schaffe dir keinen Gram!", "tokens": ["Das", "schaf\u00b7fe", "dir", "kei\u00b7nen", "Gram", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Willkommen, mein T\u00f6chterlein hochgesinnt,", "tokens": ["Will\u00b7kom\u00b7men", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", "hoch\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Halt' mir meinen Falken zahm!\u00ab", "tokens": ["Halt'", "mir", "mei\u00b7nen", "Fal\u00b7ken", "zahm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}