{"textgrid.poem.65813": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "87", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebt wohl! Dich halt' ich nicht; bist mir zu teuer;", "tokens": ["Lebt", "wohl", "!", "Dich", "halt'", "ich", "nicht", ";", "bist", "mir", "zu", "teu\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und, f\u00fcrcht' ich, deines Wertes wohl gedenk.", "tokens": ["Und", ",", "f\u00fcrcht'", "ich", ",", "dei\u00b7nes", "Wer\u00b7tes", "wohl", "ge\u00b7denk", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Freibrief deines Selbst wird dein Befreier,", "tokens": ["Der", "Frei\u00b7brief", "dei\u00b7nes", "Selbst", "wird", "dein", "Be\u00b7frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Recht an dich ist allzu eng beschr\u00e4nkt.", "tokens": ["Mein", "Recht", "an", "dich", "ist", "all\u00b7zu", "eng", "be\u00b7schr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn wie bes\u00e4\u00df ich dich als durch dein Geben?", "tokens": ["Denn", "wie", "be\u00b7s\u00e4\u00df", "ich", "dich", "als", "durch", "dein", "Ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PRF", "KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Welch ein Verdienst erw\u00fcrb mir solche G\u00fcter?", "tokens": ["Welch", "ein", "Ver\u00b7dienst", "er\u00b7w\u00fcrb", "mir", "sol\u00b7che", "G\u00fc\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Grund so holder Gunst fehlt meinem Leben:", "tokens": ["Der", "Grund", "so", "hol\u00b7der", "Gunst", "fehlt", "mei\u00b7nem", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und so kehrt mein Geschenk zum Eigner wieder.", "tokens": ["Und", "so", "kehrt", "mein", "Ge\u00b7schenk", "zum", "Eig\u00b7ner", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Fremd war dein Wert dir selbst, als du dich brachtest;", "tokens": ["Fremd", "war", "dein", "Wert", "dir", "selbst", ",", "als", "du", "dich", "brach\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Ich, der Beschenkte, wohl zu hoch gemessen;", "tokens": ["Ich", ",", "der", "Be\u00b7schenk\u00b7te", ",", "wohl", "zu", "hoch", "ge\u00b7mes\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So f\u00e4llt die Gabe, die im Wahn du machtest,", "tokens": ["So", "f\u00e4llt", "die", "Ga\u00b7be", ",", "die", "im", "Wahn", "du", "mach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dir wieder heim nach reiferem Ermessen.", "tokens": ["Dir", "wie\u00b7der", "heim", "nach", "rei\u00b7fe\u00b7rem", "Er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So hab' ich dich gehabt nur wie im Fieber,", "tokens": ["So", "hab'", "ich", "dich", "ge\u00b7habt", "nur", "wie", "im", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VAPP", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Im Traum ein K\u00f6nig! wachend ist's vor\u00fcber.", "tokens": ["Im", "Traum", "ein", "K\u00f6\u00b7nig", "!", "wa\u00b7chend", "ist's", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "VVPP", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}