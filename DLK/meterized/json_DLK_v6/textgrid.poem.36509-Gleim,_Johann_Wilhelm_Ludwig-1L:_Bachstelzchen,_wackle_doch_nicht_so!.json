{"textgrid.poem.36509": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bachstelzchen, wackle doch nicht so!", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bachstelzchen, wackle doch nicht so!", "tokens": ["Bach\u00b7stelz\u00b7chen", ",", "wack\u00b7le", "doch", "nicht", "so", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rief eine St\u00f6rchin, die, nicht ihres Lebens froh,", "tokens": ["Rief", "ei\u00b7ne", "St\u00f6r\u00b7chin", ",", "die", ",", "nicht", "ih\u00b7res", "Le\u00b7bens", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "$,", "PTKNEG", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit ernstem Gang, auf einer Wiese ging!", "tokens": ["Mit", "erns\u00b7tem", "Gang", ",", "auf", "ei\u00b7ner", "Wie\u00b7se", "ging", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bachstelzchen, Bruder Lustig, kleines Ding,", "tokens": ["Bach\u00b7stelz\u00b7chen", ",", "Bru\u00b7der", "Lus\u00b7tig", ",", "klei\u00b7nes", "Ding", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "So wackle doch nicht immer so!", "tokens": ["So", "wack\u00b7le", "doch", "nicht", "im\u00b7mer", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft siebenmal die weise Frau. \u2013 Das kleine Ding", "tokens": ["Ruft", "sie\u00b7ben\u00b7mal", "die", "wei\u00b7se", "Frau", ".", "\u2013", "Das", "klei\u00b7ne", "Ding"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00f6rt's endlich, sieht sich um;", "tokens": ["H\u00f6rt's", "end\u00b7lich", ",", "sieht", "sich", "um", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fragt:", "tokens": ["Fragt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Sind sie dumm?", "tokens": ["Sind", "sie", "dumm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Es scheint, sie stehen in Gedanken,", "tokens": ["Es", "scheint", ",", "sie", "ste\u00b7hen", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zanken sich mit ihrem Gott!", "tokens": ["Und", "zan\u00b7ken", "sich", "mit", "ih\u00b7rem", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie haben's ja, Frau St\u00f6rchin, keine Not;", "tokens": ["Sie", "ha\u00b7ben's", "ja", ",", "Frau", "St\u00f6r\u00b7chin", ",", "kei\u00b7ne", "Not", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "NE", "$,", "PIAT", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}}}}}