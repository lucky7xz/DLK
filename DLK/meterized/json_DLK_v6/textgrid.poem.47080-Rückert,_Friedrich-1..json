{"textgrid.poem.47080": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun aber will ich sehn, ob man mit Armen", "tokens": ["Nun", "a\u00b7ber", "will", "ich", "sehn", ",", "ob", "man", "mit", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Poesie kann in die Wolken reichen", "tokens": ["Der", "Poe\u00b7sie", "kann", "in", "die", "Wol\u00b7ken", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und niederholen aus des Lichtes Reichen", "tokens": ["Und", "nie\u00b7der\u00b7ho\u00b7len", "aus", "des", "Lich\u00b7tes", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trostsch\u00e4tze f\u00fcr ein Herz, das will verarmen;", "tokens": ["Trost\u00b7sch\u00e4t\u00b7ze", "f\u00fcr", "ein", "Herz", ",", "das", "will", "ver\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sehn will ich, ob Begeist'rung mit den warmen", "tokens": ["Sehn", "will", "ich", ",", "ob", "Be\u00b7geist'\u00b7rung", "mit", "den", "war\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "PPER", "$,", "KOUS", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gluthauchen kann des Grabes Th\u00fcr erweichen,", "tokens": ["Glut\u00b7hau\u00b7chen", "kann", "des", "Gra\u00b7bes", "Th\u00fcr", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df lebensfrisch daraus hervorgehn Leichen,", "tokens": ["Da\u00df", "le\u00b7bens\u00b7frisch", "da\u00b7raus", "her\u00b7vor\u00b7gehn", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PAV", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eingesargt der Tod hat ohn' Erbarmen;", "tokens": ["Die", "ein\u00b7ge\u00b7sargt", "der", "Tod", "hat", "ohn'", "Er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sehn, ob aus Liebesrosen, Trauernesseln", "tokens": ["Sehn", ",", "ob", "aus", "Lie\u00b7bes\u00b7ro\u00b7sen", ",", "Trau\u00b7er\u00b7nes\u00b7seln"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "NN", "$,", "NN"], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Noch Kr\u00e4nze flechten k\u00f6nnen die Kam\u00f6nen,", "tokens": ["Noch", "Kr\u00e4n\u00b7ze", "flech\u00b7ten", "k\u00f6n\u00b7nen", "die", "Ka\u00b7m\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit ein fliehend Schattenbild zu fesseln;", "tokens": ["Da\u00b7mit", "ein", "flie\u00b7hend", "Schat\u00b7ten\u00b7bild", "zu", "fes\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob man erbauen kann aus Zaubert\u00f6nen", "tokens": ["Ob", "man", "er\u00b7bau\u00b7en", "kann", "aus", "Zau\u00b7ber\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Demantschlo\u00df, darin auf Saphirfesseln", "tokens": ["Ein", "De\u00b7mant\u00b7schlo\u00df", ",", "da\u00b7rin", "auf", "Sa\u00b7phir\u00b7fes\u00b7seln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sitz' engelgleich die Sch\u00f6nste aller Sch\u00f6nen.", "tokens": ["Sitz'", "en\u00b7gel\u00b7gleich", "die", "Sch\u00f6ns\u00b7te", "al\u00b7ler", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}