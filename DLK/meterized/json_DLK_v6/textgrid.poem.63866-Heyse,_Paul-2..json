{"textgrid.poem.63866": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tage schleichen tr\u00fcb dahin, und auf die Sonne hoffst du noch?", "tokens": ["Die", "Ta\u00b7ge", "schlei\u00b7chen", "tr\u00fcb", "da\u00b7hin", ",", "und", "auf", "die", "Son\u00b7ne", "hoffst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da\u00df Goldgespinst die Parze spinn' am dunklen Rocken, hoffst du noch?", "tokens": ["Da\u00df", "Gold\u00b7ge\u00b7spinst", "die", "Par\u00b7ze", "spinn'", "am", "dunk\u00b7len", "Ro\u00b7cken", ",", "hoffst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Du wei\u00dft, das ist des Alters Los: es wiederholt sich Schlimmes nur;", "tokens": ["Du", "wei\u00dft", ",", "das", "ist", "des", "Al\u00b7ters", "Los", ":", "es", "wie\u00b7der\u00b7holt", "sich", "Schlim\u00b7mes", "nur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "NE", "$.", "PPER", "VVFIN", "PRF", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Was lieblich war zu Anbeginn, da\u00df sich's erneure, hoffst du noch?", "tokens": ["Was", "lieb\u00b7lich", "war", "zu", "An\u00b7be\u00b7ginn", ",", "da\u00df", "sich's", "er\u00b7neu\u00b7re", ",", "hoffst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "APPR", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Das Gr\u00f6\u00dfte, was das Leben beut: Kunst, Liebe, Frieden gab es dir,", "tokens": ["Das", "Gr\u00f6\u00df\u00b7te", ",", "was", "das", "Le\u00b7ben", "beut", ":", "Kunst", ",", "Lie\u00b7be", ",", "Frie\u00b7den", "gab", "es", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Und einen neuen Vollgewinn aus seinem F\u00fcllhorn hoffst du noch?", "tokens": ["Und", "ei\u00b7nen", "neu\u00b7en", "Voll\u00b7ge\u00b7winn", "aus", "sei\u00b7nem", "F\u00fcll\u00b7horn", "hoffst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Ist's nicht genug, da\u00df Schlummer dir die letzte ew'ge Nacht verhei\u00dft,", "tokens": ["Ist's", "nicht", "ge\u00b7nug", ",", "da\u00df", "Schlum\u00b7mer", "dir", "die", "letz\u00b7te", "ew'\u00b7ge", "Nacht", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "$,", "KOUS", "NN", "PPER", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und einen bunten Traum darin, du Anspruchsvoller, hoffst du noch?", "tokens": ["Und", "ei\u00b7nen", "bun\u00b7ten", "Traum", "da\u00b7rin", ",", "du", "An\u00b7spruchs\u00b7vol\u00b7ler", ",", "hoffst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$,", "PPER", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}