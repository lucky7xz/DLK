{"textgrid.poem.26367": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich, Balzer, hatte zwanzig Jahr]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, Balzer, hatte zwanzig Jahr,", "tokens": ["Ich", ",", "Bal\u00b7zer", ",", "hat\u00b7te", "zwan\u00b7zig", "Jahr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach mein Vater: \u00bbBalthasar,", "tokens": ["Da", "sprach", "mein", "Va\u00b7ter", ":", "\u00bb", "Balt\u00b7ha\u00b7sar", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute ist grad ein Regentag,", "tokens": ["Heu\u00b7te", "ist", "grad", "ein", "Re\u00b7gen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Weshalb ich mit dir reden mag.", "tokens": ["We\u00b7shalb", "ich", "mit", "dir", "re\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVINF", "VMFIN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Man bleibt dann gern einmal im Zimmer;", "tokens": ["Man", "bleibt", "dann", "gern", "ein\u00b7mal", "im", "Zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's sch\u00f6n ist, tut man das nicht immer.", "tokens": ["Wenn's", "sch\u00f6n", "ist", ",", "tut", "man", "das", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "$,", "VVFIN", "PIS", "PDS", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist jetzt zwanzig Jahre eben,", "tokens": ["Du", "bist", "jetzt", "zwan\u00b7zig", "Jah\u00b7re", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einmal wird man's nur im Leben,", "tokens": ["Und", "ein\u00b7mal", "wird", "man's", "nur", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Mutter ist dir l\u00e4ngst gestorben,", "tokens": ["Die", "Mut\u00b7ter", "ist", "dir", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Balzer, bleib mir unverdorben.", "tokens": ["O", "Bal\u00b7zer", ",", "bleib", "mir", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vor Weibern h\u00fcte dich und Wein", "tokens": ["Vor", "Wei\u00b7bern", "h\u00fc\u00b7te", "dich", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df mir auch die Karten sein!", "tokens": ["Und", "la\u00df", "mir", "auch", "die", "Kar\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich will nicht sagen, man kann eben", "tokens": ["Ich", "will", "nicht", "sa\u00b7gen", ",", "man", "kann", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PIS", "VMFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ohne diese Dinge leben,", "tokens": ["Auch", "oh\u00b7ne", "die\u00b7se", "Din\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur h\u00fcte dich, mein lieber Sohn,", "tokens": ["Nur", "h\u00fc\u00b7te", "dich", ",", "mein", "lie\u00b7ber", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nimm dir nicht zu viel davon!", "tokens": ["Und", "nimm", "dir", "nicht", "zu", "viel", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "PTKA", "PIS", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch immer sollst du furchtlos bleiben;", "tokens": ["Doch", "im\u00b7mer", "sollst", "du", "furcht\u00b7los", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nat\u00fcrlich ist's, sich zu beweiben,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "ist's", ",", "sich", "zu", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Weil, was der Mensch niemals probiert,", "tokens": ["Weil", ",", "was", "der", "Mensch", "nie\u00b7mals", "pro\u00b7biert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sich ewig vor ihm selbst geniert.\u00ab", "tokens": ["Sich", "e\u00b7wig", "vor", "ihm", "selbst", "ge\u00b7niert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Vater gab mir einen Ku\u00df:", "tokens": ["Der", "Va\u00b7ter", "gab", "mir", "ei\u00b7nen", "Ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbund nicht nur deshalb, weil man mu\u00df,", "tokens": ["\u00bb", "und", "nicht", "nur", "des\u00b7halb", ",", "weil", "man", "mu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PTKNEG", "ADV", "PAV", "$,", "KOUS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mein Sohn, nimm warm das Leben hin.\u00ab", "tokens": ["Mein", "Sohn", ",", "nimm", "warm", "das", "Le\u00b7ben", "hin", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "ADJD", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und abends fuhr ich nach Berlin.", "tokens": ["Und", "a\u00b7bends", "fuhr", "ich", "nach", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}