{"dta.poem.13048": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eber\u00fchr\u2019 ich endlich auch mein Amt und meine Pflicht,", "tokens": ["\u201e", "be\u00b7r\u00fchr'", "ich", "end\u00b7lich", "auch", "mein", "Amt", "und", "mei\u00b7ne", "Pflicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edie schon f\u00fcr mich das Wort, bevor ich rede, spricht:", "tokens": ["\u201e", "die", "schon", "f\u00fcr", "mich", "das", "Wort", ",", "be\u00b7vor", "ich", "re\u00b7de", ",", "spricht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "ADV", "APPR", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "415\u201dWer wiederredet mir? kann es mir nicht geziemen,", "tokens": ["\"", "Wer", "wie\u00b7der\u00b7re\u00b7det", "mir", "?", "kann", "es", "mir", "nicht", "ge\u00b7zie\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201egleich wie dem ganzen Krei\u00df, mich selber auch zu r\u00fchmen?", "tokens": ["\u201e", "gleich", "wie", "dem", "gan\u00b7zen", "Krei\u00df", ",", "mich", "sel\u00b7ber", "auch", "zu", "r\u00fch\u00b7men", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "(bey diesen Worten fieng der Grei\u00df zu l\u00e4cheln an,", "tokens": ["(", "bey", "die\u00b7sen", "Wor\u00b7ten", "fi\u00b7eng", "der", "Grei\u00df", "zu", "l\u00e4\u00b7cheln", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Als h\u00e4tt er diese Frag aus stillem Scherz gethan)", "tokens": ["Als", "h\u00e4tt", "er", "die\u00b7se", "Frag", "aus", "stil\u00b7lem", "Scherz", "ge\u00b7than", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PDAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso will ich, ohne mich durch Prahlen auszubreiten,", "tokens": ["\u201e", "so", "will", "ich", ",", "oh\u00b7ne", "mich", "durch", "Prah\u00b7len", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "$,", "KOUI", "PRF", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "420\u201dZu meinem eignen Lob durch diese Frage schreiten:", "tokens": ["\"", "Zu", "mei\u00b7nem", "eig\u00b7nen", "Lob", "durch", "die\u00b7se", "Fra\u00b7ge", "schrei\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewer hat von euch mich nicht in jedem Fall gefragt?", "tokens": ["\u201e", "wer", "hat", "von", "euch", "mich", "nicht", "in", "je\u00b7dem", "Fall", "ge\u00b7fragt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "APPR", "PPER", "PRF", "PTKNEG", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewer hat es nicht befolgt, wann ich ein Wort gesagt?", "tokens": ["\u201e", "wer", "hat", "es", "nicht", "be\u00b7folgt", ",", "wann", "ich", "ein", "Wort", "ge\u00b7sagt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}