{"textgrid.poem.34604": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie das erste mahl k\u00fc\u00dfte", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe/ Sylvia/ dich einmahl nur gek\u00fc\u00dft/", "tokens": ["Ich", "ha\u00b7be", "/", "Syl\u00b7via", "/", "dich", "ein\u00b7mahl", "nur", "ge\u00b7k\u00fc\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "NE", "$(", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und meynt' ich w\u00fcrde mir mein kranckes hertze laben;", "tokens": ["Und", "meynt'", "ich", "w\u00fcr\u00b7de", "mir", "mein", "kran\u00b7ckes", "hert\u00b7ze", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O! aber weit gefehlt! ich wei\u00df nicht/ wo es ist/", "tokens": ["O", "!", "a\u00b7ber", "weit", "ge\u00b7fehlt", "!", "ich", "wei\u00df", "nicht", "/", "wo", "es", "ist", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "VVPP", "$.", "PPER", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und glaub/ es hat sich gar in deinen mund begraben.", "tokens": ["Und", "glaub", "/", "es", "hat", "sich", "gar", "in", "dei\u00b7nen", "mund", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VAFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann meine krafft ist hin: Ich f\u00fchle nichts als glut/", "tokens": ["Dann", "mei\u00b7ne", "krafft", "ist", "hin", ":", "Ich", "f\u00fch\u00b7le", "nichts", "als", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und st\u00fcrbe/ wenn ich nicht aus liebe leben m\u00fcste.", "tokens": ["Und", "st\u00fcr\u00b7be", "/", "wenn", "ich", "nicht", "aus", "lie\u00b7be", "le\u00b7ben", "m\u00fcs\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! wann ein eintzig ku\u00df so grosse w\u00fcrckung thut/", "tokens": ["Ach", "!", "wann", "ein", "eint\u00b7zig", "ku\u00df", "so", "gros\u00b7se", "w\u00fcr\u00b7ckung", "thut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "ADJD", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie solte mir geschehn/ wann ich dich zehnmal k\u00fc\u00dfte?", "tokens": ["Wie", "sol\u00b7te", "mir", "ge\u00b7schehn", "/", "wann", "ich", "dich", "zehn\u00b7mal", "k\u00fc\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVPP", "$(", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}