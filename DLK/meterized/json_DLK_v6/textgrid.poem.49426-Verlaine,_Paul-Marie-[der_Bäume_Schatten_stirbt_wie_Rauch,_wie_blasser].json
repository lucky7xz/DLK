{"textgrid.poem.49426": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[der B\u00e4ume Schatten stirbt wie Rauch, wie blasser]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der B\u00e4ume Schatten stirbt wie Rauch, wie blasser,", "tokens": ["Der", "B\u00e4u\u00b7me", "Schat\u00b7ten", "stirbt", "wie", "Rauch", ",", "wie", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KOKOM", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im nebeltr\u00fcben Wasser,", "tokens": ["Im", "ne\u00b7belt\u00b7r\u00fc\u00b7ben", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wann aus den L\u00fcften tief versteckt im Laube", "tokens": ["Wann", "aus", "den", "L\u00fcf\u00b7ten", "tief", "ver\u00b7steckt", "im", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "S\u00fcss klagt die Turteltaube.", "tokens": ["S\u00fcss", "klagt", "die", "Tur\u00b7tel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sehr gleicht, blasser Wandrer, deinem Bilde", "tokens": ["Wie", "sehr", "gleicht", ",", "blas\u00b7ser", "Wand\u00b7rer", ",", "dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies bleichende Gefilde.", "tokens": ["Dies", "blei\u00b7chen\u00b7de", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie weint in den Gezweigen schwermuttrunken", "tokens": ["Wie", "weint", "in", "den", "Ge\u00b7zwei\u00b7gen", "schwer\u00b7mut\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Hoffen, das versunken.", "tokens": ["Dein", "Hof\u00b7fen", ",", "das", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}