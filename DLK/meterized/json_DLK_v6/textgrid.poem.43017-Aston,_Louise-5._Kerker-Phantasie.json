{"textgrid.poem.43017": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "5. Kerker-Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt vor mir das Wort des Herrn,", "tokens": ["Es", "liegt", "vor", "mir", "das", "Wort", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bibel, aufgeschlagen;", "tokens": ["Die", "Bi\u00b7bel", ",", "auf\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daraus gemahnt mich, bleich und fern,", "tokens": ["Da\u00b7raus", "ge\u00b7mahnt", "mich", ",", "bleich", "und", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist von alten Tagen.", "tokens": ["Der", "Geist", "von", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du hast Erl\u00f6sung prophezeit!", "tokens": ["Du", "hast", "Er\u00b7l\u00f6\u00b7sung", "pro\u00b7phe\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erl\u00f6sung bringst du nicht \u2013", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "bringst", "du", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die verhei\u00df'ne Seligkeit", "tokens": ["Und", "die", "ver\u00b7hei\u00df'\u00b7ne", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nur ein Traumgesicht!", "tokens": ["Ist", "nur", "ein", "Traum\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich starrt es an, das Wort des Herrn,", "tokens": ["Mich", "starrt", "es", "an", ",", "das", "Wort", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nie mir Trost gew\u00e4hrte.", "tokens": ["Das", "nie", "mir", "Trost", "ge\u00b7w\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir strahlte nie der gold'ne Stern,", "tokens": ["Mir", "strahl\u00b7te", "nie", "der", "gold'\u00b7ne", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bethlehem verkl\u00e4rte.", "tokens": ["Der", "Beth\u00b7le\u00b7hem", "ver\u00b7kl\u00e4r\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich mahnt's unheimlich, graunerf\u00fcllt,", "tokens": ["Mich", "mahnt's", "un\u00b7heim\u00b7lich", ",", "grau\u00b7ner\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bringt den Tod mir nah',", "tokens": ["Und", "bringt", "den", "Tod", "mir", "nah'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das schmerzentstellte G\u00f6tterbild,", "tokens": ["Das", "schmer\u00b7zent\u00b7stell\u00b7te", "G\u00f6t\u00b7ter\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Kreuz auf Golgatha!", "tokens": ["Das", "Kreuz", "auf", "Gol\u00b7ga\u00b7tha", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Kreuzestod, die Grabesnacht,", "tokens": ["Der", "Kreu\u00b7ze\u00b7stod", ",", "die", "Gra\u00b7bes\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die finstern Bilder alle;", "tokens": ["Die", "fins\u00b7tern", "Bil\u00b7der", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Angst, die bang und betend wacht,", "tokens": ["Die", "Angst", ",", "die", "bang", "und", "be\u00b7tend", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor neuem S\u00fcndenfalle;", "tokens": ["Vor", "neu\u00b7em", "S\u00fcn\u00b7den\u00b7fal\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Bu\u00dfe, die sich selbst kasteit,", "tokens": ["Die", "Bu\u00b7\u00dfe", ",", "die", "sich", "selbst", "kas\u00b7teit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Himmels Strafgericht!", "tokens": ["Des", "Him\u00b7mels", "Straf\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O, meines Kerkers Einsamkeit", "tokens": ["O", ",", "mei\u00b7nes", "Ker\u00b7kers", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Begr\u00fc\u00dft kein rettend Licht!", "tokens": ["Be\u00b7gr\u00fc\u00dft", "kein", "ret\u00b7tend", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Kreuz \u2013 ich f\u00fchle seine Last,", "tokens": ["Das", "Kreuz", "\u2013", "ich", "f\u00fch\u00b7le", "sei\u00b7ne", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein d\u00e4monisch Walten \u2013", "tokens": ["Wie", "ein", "d\u00e4\u00b7mo\u00b7nisch", "Wal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von seiner Macht bin ich erfa\u00dft,", "tokens": ["Von", "sei\u00b7ner", "Macht", "bin", "ich", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unrettbar festgehalten.", "tokens": ["Un\u00b7rett\u00b7bar", "fest\u00b7ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Es bindet mich f\u00fcr Ewigkeit", "tokens": ["Es", "bin\u00b7det", "mich", "f\u00fcr", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Weihe heil'ger Spruch,", "tokens": ["Der", "Wei\u00b7he", "heil'\u00b7ger", "Spruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und namenlosem Schmerz geweiht", "tokens": ["Und", "na\u00b7men\u00b7lo\u00b7sem", "Schmerz", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat mich dies Himmelsbuch!", "tokens": ["Hat", "mich", "dies", "Him\u00b7mels\u00b7buch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Wie es von je bestanden!", "tokens": ["Wie", "es", "von", "je", "be\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich dies Herrenthum geschm\u00e4ht,", "tokens": ["Weil", "ich", "dies", "Her\u00b7ren\u00b7thum", "ge\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seufz' ich in schweren Banden.", "tokens": ["Seuf\u00b7z'", "ich", "in", "schwe\u00b7ren", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch meine Seele bleibe frei", "tokens": ["Doch", "mei\u00b7ne", "See\u00b7le", "blei\u00b7be", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Fesseln und Gef\u00e4ngni\u00df,", "tokens": ["Trotz", "Fes\u00b7seln", "und", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und trage in der Sklaverei", "tokens": ["Und", "tra\u00b7ge", "in", "der", "Skla\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bewu\u00dft ein gro\u00df' Verh\u00e4ngni\u00df!", "tokens": ["Be\u00b7wu\u00dft", "ein", "gro\u00df'", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}