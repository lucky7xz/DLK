{"textgrid.poem.49965": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Ei geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ostern ist zwar schon vorbei,", "tokens": ["Os\u00b7tern", "ist", "zwar", "schon", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Also dies kein Osterei;", "tokens": ["Al\u00b7so", "dies", "kein", "Os\u00b7te\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wer sagt, es sei kein Segen,", "tokens": ["Doch", "wer", "sagt", ",", "es", "sei", "kein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn im Mai die Hasen legen?", "tokens": ["Wenn", "im", "Mai", "die", "Ha\u00b7sen", "le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Pfanne, aus dem Schmalz", "tokens": ["Aus", "der", "Pfan\u00b7ne", ",", "aus", "dem", "Schmalz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmeckt ein Eilein jedenfalls,", "tokens": ["Schmeckt", "ein", "Ei\u00b7lein", "je\u00b7den\u00b7falls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und kurzum, mich t\u00e4t's gaudieren,", "tokens": ["Und", "kur\u00b7zum", ",", "mich", "t\u00e4t's", "gau\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir dies Ei zu pr\u00e4sentieren,", "tokens": ["Dir", "dies", "Ei", "zu", "pr\u00e4\u00b7sen\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zugleich t\u00e4t es mich kitzeln,", "tokens": ["Und", "zu\u00b7gleich", "t\u00e4t", "es", "mich", "kit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Dir ein R\u00e4tsel drauf zu kritzeln.", "tokens": ["Dir", "ein", "R\u00e4t\u00b7sel", "drauf", "zu", "krit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sophisten und die Pfaffen", "tokens": ["Die", "So\u00b7phis\u00b7ten", "und", "die", "Pfaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stritten sich mit viel Geschrei:", "tokens": ["Strit\u00b7ten", "sich", "mit", "viel", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was hat Gott zuerst erschaffen,", "tokens": ["Was", "hat", "Gott", "zu\u00b7erst", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl die Henne? wohl das Ei?", "tokens": ["Wohl", "die", "Hen\u00b7ne", "?", "wohl", "das", "Ei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4re das so schwer zu l\u00f6sen?", "tokens": ["W\u00e4\u00b7re", "das", "so", "schwer", "zu", "l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erstlich ward ein Ei erdacht:", "tokens": ["Erst\u00b7lich", "ward", "ein", "Ei", "er\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch weil noch kein Huhn gewesen,", "tokens": ["Doch", "weil", "noch", "kein", "Huhn", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "VAPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Schatz, so hat's der Has gebracht.", "tokens": ["Schatz", ",", "so", "hat's", "der", "Has", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}