{"dta.poem.16498": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "159.  Die ledigkeit ist wie GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch wo du ledig bist/ das Wasser quillt au\u00df dir/", "tokens": ["Mensch", "wo", "du", "le\u00b7dig", "bist", "/", "das", "Was\u00b7ser", "quillt", "au\u00df", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "ADJD", "VAFIN", "$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So wol als au\u00df dem Brunn der Ewigkeit herf\u00fcr.", "tokens": ["So", "wol", "als", "au\u00df", "dem", "Brunn", "der", "E\u00b7wig\u00b7keit", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}