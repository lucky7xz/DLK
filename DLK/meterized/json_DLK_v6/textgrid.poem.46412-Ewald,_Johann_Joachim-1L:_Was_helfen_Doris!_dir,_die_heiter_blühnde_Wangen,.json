{"textgrid.poem.46412": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was helfen Doris! dir, die heiter bl\u00fchnde Wangen,", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was helfen Doris! dir, die heiter bl\u00fchnde Wangen,", "tokens": ["Was", "hel\u00b7fen", "Do\u00b7ris", "!", "dir", ",", "die", "hei\u00b7ter", "bl\u00fchn\u00b7de", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$.", "PPER", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Lippen stiller Reitz, die sch\u00f6n gew\u00f6lbte Brust,", "tokens": ["Der", "Lip\u00b7pen", "stil\u00b7ler", "Reitz", ",", "die", "sch\u00f6n", "ge\u00b7w\u00f6lb\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Haar wo Zephirs sich in blonden Locken fangen?", "tokens": ["Das", "Haar", "wo", "Ze\u00b7phirs", "sich", "in", "blon\u00b7den", "Lo\u00b7cken", "fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "NE", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du scheuchst die Grazien, den Amor und die Lust.", "tokens": ["Du", "scheuchst", "die", "Gra\u00b7zi\u00b7en", ",", "den", "A\u00b7mor", "und", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergeblich hat Natur dir Sch\u00f6nheit beygeleget,", "tokens": ["Ver\u00b7geb\u00b7lich", "hat", "Na\u00b7tur", "dir", "Sch\u00f6n\u00b7heit", "bey\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sie dir nicht Gef\u00fchl im Lenz der Jahre giebt;", "tokens": ["Wenn", "sie", "dir", "nicht", "Ge\u00b7f\u00fchl", "im", "Lenz", "der", "Jah\u00b7re", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn nicht die Z\u00e4rtlichkeit den jungen Busen reget,", "tokens": ["Wenn", "nicht", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "den", "jun\u00b7gen", "Bu\u00b7sen", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bl\u00fchst du den Blumen gleich die nur das Auge liebt.", "tokens": ["Bl\u00fchst", "du", "den", "Blu\u00b7men", "gleich", "die", "nur", "das", "Au\u00b7ge", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}