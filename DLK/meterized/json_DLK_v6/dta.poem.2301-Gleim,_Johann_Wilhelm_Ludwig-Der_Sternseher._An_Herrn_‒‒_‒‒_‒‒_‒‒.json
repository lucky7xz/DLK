{"dta.poem.2301": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sternseher.  \n An  \n Herrn  \u2012\u2012 \u2012\u2012 \u2012\u2012 \u2012\u2012", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kenner aller Welten,", "tokens": ["Der", "Ken\u00b7ner", "al\u00b7ler", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der in dem Sterngew\u00f6lbe", "tokens": ["Der", "in", "dem", "Stern\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kometen und Trabanten", "tokens": ["Ko\u00b7me\u00b7ten", "und", "Tra\u00b7ban\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und neue Sonnen suchet,", "tokens": ["Und", "neu\u00b7e", "Son\u00b7nen", "su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ohne Scherz und Liebe", "tokens": ["Und", "oh\u00b7ne", "Scherz", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch alle N\u00e4chte wachet,", "tokens": ["Durch", "al\u00b7le", "N\u00e4ch\u00b7te", "wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bewog mich i\u00fcngst am Abend", "tokens": ["Be\u00b7wog", "mich", "i\u00b7\u00fcngst", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu frieren und zu wachen.", "tokens": ["Zu", "frie\u00b7ren", "und", "zu", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den holen Raum des Himmels", "tokens": ["Den", "ho\u00b7len", "Raum", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Erhell\u2019ten tausend Sterne,", "tokens": ["Er\u00b7hell'\u00b7ten", "tau\u00b7send", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie tausend helle Lampen", "tokens": ["Wie", "tau\u00b7send", "hel\u00b7le", "Lam\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Den weissen Saal erhellen. ", "tokens": ["Den", "weis\u00b7sen", "Saal", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie brannten in dem Blauen,", "tokens": ["Sie", "brann\u00b7ten", "in", "dem", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und warfen kleine Stralen,", "tokens": ["Und", "war\u00b7fen", "klei\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Wie Lichter Stralen werfen;", "tokens": ["Wie", "Lich\u00b7ter", "Stra\u00b7len", "wer\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und oft sah ich, verwundernd,", "tokens": ["Und", "oft", "sah", "ich", ",", "ver\u00b7wun\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Wie sie sich selber putzten.", "tokens": ["Wie", "sie", "sich", "sel\u00b7ber", "putz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Sie brannten still und sicher,", "tokens": ["Sie", "brann\u00b7ten", "still", "und", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Bis Lun", "tokens": ["Bis", "Lun"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Den Abgrund heller machte;", "tokens": ["Den", "Ab\u00b7grund", "hel\u00b7ler", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Schnell waren von der Menge", "tokens": ["Schnell", "wa\u00b7ren", "von", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die kleinsten ausgel\u00f6schet.", "tokens": ["Die", "kleins\u00b7ten", "aus\u00b7ge\u00b7l\u00f6\u00b7schet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ich rief dem Mond entgegen:", "tokens": ["Ich", "rief", "dem", "Mond", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "So dulde doch, Tiranne,", "tokens": ["So", "dul\u00b7de", "doch", ",", "Ti\u00b7ran\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Bei deinem grossen Schimmer,", "tokens": ["Bei", "dei\u00b7nem", "gros\u00b7sen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Die kleinen Himmelslichter!", "tokens": ["Die", "klei\u00b7nen", "Him\u00b7mels\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Allein der Sternbeseher", "tokens": ["Al\u00b7lein", "der", "Stern\u00b7be\u00b7se\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Beseufzte meine Dumheit,", "tokens": ["Be\u00b7seufz\u00b7te", "mei\u00b7ne", "Dum\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und rief beim letzten Seufzer:", "tokens": ["Und", "rief", "beim", "letz\u00b7ten", "Seuf\u00b7zer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Du Dummer, steh doch stille!", "tokens": ["Du", "Dum\u00b7mer", ",", "steh", "doch", "stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Ich stand; er rief: Steh veste!", "tokens": ["Ich", "stand", ";", "er", "rief", ":", "Steh", "ves\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und legt auf meine Schulter", "tokens": ["Und", "legt", "auf", "mei\u00b7ne", "Schul\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Ein Rohr, als wollt er schiessen.", "tokens": ["Ein", "Rohr", ",", "als", "wollt", "er", "schies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ich bat ihn um mein Leben,", "tokens": ["Ich", "bat", "ihn", "um", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Allein ich mu\u00df ietzt lachen,", "tokens": ["Al\u00b7lein", "ich", "mu\u00df", "ietzt", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Es schlt ihm Rohr und Pulver,", "tokens": ["Es", "schlt", "ihm", "Rohr", "und", "Pul\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Denn die vermeinte Flinte,", "tokens": ["Denn", "die", "ver\u00b7mein\u00b7te", "Flin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Das Rohr auf meiner Schulter,", "tokens": ["Das", "Rohr", "auf", "mei\u00b7ner", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "War nur ein langes Auge,", "tokens": ["War", "nur", "ein", "lan\u00b7ges", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Womit er durch die L\u00fcfte", "tokens": ["Wo\u00b7mit", "er", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Den Mond herunter holte.", "tokens": ["Den", "Mond", "her\u00b7un\u00b7ter", "hol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Er holt\u2019 ihn auch herunter,", "tokens": ["Er", "holt'", "ihn", "auch", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Und sah ihn in der N\u00e4he,", "tokens": ["Und", "sah", "ihn", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Und sprach: Ich will im Monde,", "tokens": ["Und", "sprach", ":", "Ich", "will", "im", "Mon\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Die Th\u00e4ler voller Tannen,", "tokens": ["Die", "Th\u00e4\u00b7ler", "vol\u00b7ler", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Und alle W\u00e4lder z\u00e4len;", "tokens": ["Und", "al\u00b7le", "W\u00e4l\u00b7der", "z\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ich will die Berge messen,", "tokens": ["Ich", "will", "die", "Ber\u00b7ge", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Und alle Fl\u00fcsse z\u00e4len.", "tokens": ["Und", "al\u00b7le", "Fl\u00fcs\u00b7se", "z\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Er z\u00e4lte schon bis zwanzig;", "tokens": ["Er", "z\u00e4l\u00b7te", "schon", "bis", "zwan\u00b7zig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Allein, indem er z\u00e4lte,", "tokens": ["Al\u00b7lein", ",", "in\u00b7dem", "er", "z\u00e4l\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Erhub er schnell die Stimme,", "tokens": ["Er\u00b7hub", "er", "schnell", "die", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Und rief, wie W\u00e4chter rufen:", "tokens": ["Und", "rief", ",", "wie", "W\u00e4ch\u00b7ter", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Im Monde wohnen M\u00e4dchens!", "tokens": ["Im", "Mon\u00b7de", "woh\u00b7nen", "M\u00e4d\u00b7chens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Er, der noch nie gel\u00e4chelt,", "tokens": ["Er", ",", "der", "noch", "nie", "ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Fing pl\u00f6tzlich an zu lachen,", "tokens": ["Fing", "pl\u00f6tz\u00b7lich", "an", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Und sahe nach dem Monde,", "tokens": ["Und", "sa\u00b7he", "nach", "dem", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Und lachte pl\u00f6tzlich wieder,", "tokens": ["Und", "lach\u00b7te", "pl\u00f6tz\u00b7lich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Und sprach, noch halb im Lachen:", "tokens": ["Und", "sprach", ",", "noch", "halb", "im", "La\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Ich sehe kleine M\u00e4dchens;", "tokens": ["Ich", "se\u00b7he", "klei\u00b7ne", "M\u00e4d\u00b7chens", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Sie tanzen unter Knaben,", "tokens": ["Sie", "tan\u00b7zen", "un\u00b7ter", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Sie tanzen nach Figuren,", "tokens": ["Sie", "tan\u00b7zen", "nach", "Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Nach Winkeln und Quadraten,", "tokens": ["Nach", "Win\u00b7keln", "und", "Quad\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.63": {"text": "Nach Zirkeln und Ovalen,", "tokens": ["Nach", "Zir\u00b7keln", "und", "O\u00b7va\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Und spielen mit dem Zirkel,", "tokens": ["Und", "spie\u00b7len", "mit", "dem", "Zir\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Und stehn auf hohen Gipfeln,", "tokens": ["Und", "stehn", "auf", "ho\u00b7hen", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Und sehn mit l\u00e4ngern Augen,", "tokens": ["Und", "sehn", "mit", "l\u00e4n\u00b7gern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Als Neuton und Copernik.", "tokens": ["Als", "Neu\u00b7ton", "und", "Co\u00b7per\u00b7nik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.68": {"text": "Ich habe nie mit M\u00e4dchens", "tokens": ["Ich", "ha\u00b7be", "nie", "mit", "M\u00e4d\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.69": {"text": "Getanzet noch gespielet;", "tokens": ["Ge\u00b7tan\u00b7zet", "noch", "ge\u00b7spie\u00b7let", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "O! k\u00f6nnt ich doch im Monde", "tokens": ["O", "!", "k\u00f6nnt", "ich", "doch", "im", "Mon\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Mit diesen M\u00e4dchens spielen.", "tokens": ["Mit", "die\u00b7sen", "M\u00e4d\u00b7chens", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Ach lieber Sternbeseher!", "tokens": ["Ach", "lie\u00b7ber", "Stern\u00b7be\u00b7se\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.73": {"text": "So sprach ich, bl\u00f6d\u2019 und furchtsam,", "tokens": ["So", "sprach", "ich", ",", "bl\u00f6d'", "und", "furcht\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.74": {"text": "Ach la\u00df mir doch die M\u00e4dchen", "tokens": ["Ach", "la\u00df", "mir", "doch", "die", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.75": {"text": "Mit meinem Auge sehen.", "tokens": ["Mit", "mei\u00b7nem", "Au\u00b7ge", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.76": {"text": "Gleich grif er an mein Auge,", "tokens": ["Gleich", "grif", "er", "an", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.77": {"text": "Und sprach, wie Zaub\u2019rer sprechen:", "tokens": ["Und", "sprach", ",", "wie", "Zaub'\u00b7rer", "spre\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.78": {"text": "Dis Auge werde l\u00e4nger.", "tokens": ["Dis", "Au\u00b7ge", "wer\u00b7de", "l\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.79": {"text": "Indem er dieses sagte,", "tokens": ["In\u00b7dem", "er", "die\u00b7ses", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.80": {"text": "Lie\u00df ein vergn\u00fcgtes M\u00e4dchen,", "tokens": ["Lie\u00df", "ein", "ver\u00b7gn\u00fcg\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.81": {"text": "Das mich und ihn beschaute,", "tokens": ["Das", "mich", "und", "ihn", "be\u00b7schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.82": {"text": "Das mich und ihn verlachte,", "tokens": ["Das", "mich", "und", "ihn", "ver\u00b7lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.83": {"text": "Die schwarzen Augen funkeln.", "tokens": ["Die", "schwar\u00b7zen", "Au\u00b7gen", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.84": {"text": "Schnell rief ich: Weg vom Auge!", "tokens": ["Schnell", "rief", "ich", ":", "Weg", "vom", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.85": {"text": "Mein Auge soll nicht wachsen.", "tokens": ["Mein", "Au\u00b7ge", "soll", "nicht", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.86": {"text": "Besieh du deine M\u00e4dchens.", "tokens": ["Be\u00b7sieh", "du", "dei\u00b7ne", "M\u00e4d\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.87": {"text": "Ich will mit diesem spielen.", "tokens": ["Ich", "will", "mit", "die\u00b7sem", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}