{"textgrid.poem.34426": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[und wenn dein L\u00e4cheln unter die Leute f\u00e4llt]", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn dein L\u00e4cheln unter die Leute f\u00e4llt \u2013", "tokens": ["Und", "wenn", "dein", "L\u00e4\u00b7cheln", "un\u00b7ter", "die", "Leu\u00b7te", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sie lesen es wie goldene Scherben auf,", "tokens": ["sie", "le\u00b7sen", "es", "wie", "gol\u00b7de\u00b7ne", "Scher\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "sie danken dir wie frohe Kinder,", "tokens": ["sie", "dan\u00b7ken", "dir", "wie", "fro\u00b7he", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "schreiten mit hellerem Auge weiter.", "tokens": ["schrei\u00b7ten", "mit", "hel\u00b7le\u00b7rem", "Au\u00b7ge", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "An deiner Seite schweigend und ernst nur ich,", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "schwei\u00b7gend", "und", "ernst", "nur", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "ADV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dem du die leichte Hand in den Arm gelegt ...", "tokens": ["dem", "du", "die", "leich\u00b7te", "Hand", "in", "den", "Arm", "ge\u00b7legt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O fernes Gold der lieben Sterne \u2013", "tokens": ["O", "fer\u00b7nes", "Gold", "der", "lie\u00b7ben", "Ster\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "goldene Locken an meiner Schulter!", "tokens": ["gol\u00b7de\u00b7ne", "Lo\u00b7cken", "an", "mei\u00b7ner", "Schul\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}