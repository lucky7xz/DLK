{"textgrid.poem.35906": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da gethan des Tages Pflicht", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da gethan des Tages Pflicht", "tokens": ["Da", "ge\u00b7than", "des", "Ta\u00b7ges", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Werk entsinkt den H\u00e4nden,", "tokens": ["Und", "das", "Werk", "ent\u00b7sinkt", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich nun im D\u00e4mmerlicht", "tokens": ["Will", "ich", "nun", "im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich vom L\u00e4rm des Tages wenden;", "tokens": ["Mich", "vom", "L\u00e4rm", "des", "Ta\u00b7ges", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Freuden such' ich nicht.", "tokens": ["Sei\u00b7ne", "Freu\u00b7den", "such'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wanke mit gebeugtem Muth", "tokens": ["Wan\u00b7ke", "mit", "ge\u00b7beug\u00b7tem", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcden Schritt's zum stillen Haine,", "tokens": ["M\u00fc\u00b7den", "Schritt's", "zum", "stil\u00b7len", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der treue Bruder ruht \u2013", "tokens": ["Wo", "der", "treu\u00b7e", "Bru\u00b7der", "ruht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O, der viel beweinte Eine,", "tokens": ["O", ",", "der", "viel", "be\u00b7wein\u00b7te", "Ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "VVFIN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er, so bieder, fromm und gut.", "tokens": ["Er", ",", "so", "bie\u00b7der", ",", "fromm", "und", "gut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit mit Trauer tief erf\u00fcllt", "tokens": ["Seit", "mit", "Trau\u00b7er", "tief", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sein Tod mein inn'res Leben,", "tokens": ["Hat", "sein", "Tod", "mein", "inn'\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df fortan mein \u00e4u\u00dfres Bild", "tokens": ["Mu\u00df", "for\u00b7tan", "mein", "\u00e4u\u00df\u00b7res", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Immer davon Zeugni\u00df geben,", "tokens": ["Im\u00b7mer", "da\u00b7von", "Zeug\u00b7ni\u00df", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh' ich nur in Schwarz geh\u00fcllt.", "tokens": ["Geh'", "ich", "nur", "in", "Schwarz", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zweimal hat der Lenz gebl\u00fcht,", "tokens": ["Zwei\u00b7mal", "hat", "der", "Lenz", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Sommer brachte Garben,", "tokens": ["Und", "der", "Som\u00b7mer", "brach\u00b7te", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Herbst ist still vergl\u00fcht,", "tokens": ["Und", "der", "Herbst", "ist", "still", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Seit mit ihm die Blumen starben", "tokens": ["Seit", "mit", "ihm", "die", "Blu\u00b7men", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um mich her wie im Gem\u00fcth.", "tokens": ["Um", "mich", "her", "wie", "im", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun zum dritten Mal der Nord", "tokens": ["Nun", "zum", "drit\u00b7ten", "Mal", "der", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt sein Grab mit Eis ummauert,", "tokens": ["H\u00e4lt", "sein", "Grab", "mit", "Eis", "um\u00b7mau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat das B\u00e4umlein ausgedorrt,", "tokens": ["Hat", "das", "B\u00e4um\u00b7lein", "aus\u00b7ge\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das an seinem H\u00fcgel trauert,", "tokens": ["Das", "an", "sei\u00b7nem", "H\u00fc\u00b7gel", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem schmerzlich lieben Ort.", "tokens": ["An", "dem", "schmerz\u00b7lich", "lie\u00b7ben", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber immer frisch mein Leid,", "tokens": ["A\u00b7ber", "im\u00b7mer", "frisch", "mein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer gleich des Herzens Gr\u00e4men,", "tokens": ["Im\u00b7mer", "gleich", "des", "Her\u00b7zens", "Gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie sagen, da\u00df die Zeit", "tokens": ["Ob", "sie", "sa\u00b7gen", ",", "da\u00df", "die", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df dem Schmerz die Sch\u00e4rfe nehmen;", "tokens": ["Mu\u00df", "dem", "Schmerz", "die", "Sch\u00e4r\u00b7fe", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tiefes Leid kennt keine Zeit.", "tokens": ["Tie\u00b7fes", "Leid", "kennt", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht da\u00df Gott der Herr ihn nahm", "tokens": ["Nicht", "da\u00df", "Gott", "der", "Herr", "ihn", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem leidenvollen Leben,", "tokens": ["Aus", "dem", "lei\u00b7den\u00b7vol\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber da\u00df zu sp\u00e4t ich kam,", "tokens": ["A\u00b7ber", "da\u00df", "zu", "sp\u00e4t", "ich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKA", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ersehnten Trost zu geben:", "tokens": ["Den", "er\u00b7sehn\u00b7ten", "Trost", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das erf\u00fcllt mein Herz mit Gram.", "tokens": ["Das", "er\u00b7f\u00fcllt", "mein", "Herz", "mit", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbm\u00fcde bin ich, geh' zur Ruh,\u00ab", "tokens": ["\u00bb", "m\u00fc\u00b7de", "bin", "ich", ",", "geh'", "zur", "Ruh", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang ich in der Jugend Tagen.", "tokens": ["Sang", "ich", "in", "der", "Ju\u00b7gend", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschlie\u00dfe beide Augen zu!\u00ab", "tokens": ["\u00bb", "schlie\u00b7\u00dfe", "bei\u00b7de", "Au\u00b7gen", "zu", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird nun bald der Tod mir sagen.", "tokens": ["Wird", "nun", "bald", "der", "Tod", "mir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, mein Gott! das walte Du! \u2013", "tokens": ["Herr", ",", "mein", "Gott", "!", "das", "wal\u00b7te", "Du", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}