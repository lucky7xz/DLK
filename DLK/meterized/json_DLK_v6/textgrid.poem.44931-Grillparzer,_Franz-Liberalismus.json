{"textgrid.poem.44931": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Liberalismus", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lern erst, was Freiheit will zu Recht bedeuten,", "tokens": ["Lern", "erst", ",", "was", "Frei\u00b7heit", "will", "zu", "Recht", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh Wort und Wahlspruch du entlehnst von ihr:", "tokens": ["Eh", "Wort", "und", "Wahl\u00b7spruch", "du", "ent\u00b7lehnst", "von", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht nur, da\u00df selbst du dienstbar keinem zweiten,", "tokens": ["Nicht", "nur", ",", "da\u00df", "selbst", "du", "dienst\u00b7bar", "kei\u00b7nem", "zwei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ADV", "PPER", "ADJD", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, auch kein zweiter dir.", "tokens": ["Nein", ",", "auch", "kein", "zwei\u00b7ter", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PIAT", "ADJA", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}