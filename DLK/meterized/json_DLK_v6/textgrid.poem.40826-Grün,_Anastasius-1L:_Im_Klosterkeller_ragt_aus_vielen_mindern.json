{"textgrid.poem.40826": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Klosterkeller ragt aus vielen mindern", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Klosterkeller ragt aus vielen mindern", "tokens": ["Im", "Klos\u00b7ter\u00b7kel\u00b7ler", "ragt", "aus", "vie\u00b7len", "min\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein riesig Fa\u00df, wie Vater unter Kindern;", "tokens": ["Ein", "rie\u00b7sig", "Fa\u00df", ",", "wie", "Va\u00b7ter", "un\u00b7ter", "Kin\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum nehmen sie's nicht krumm, da\u00df es zu ihnen", "tokens": ["Drum", "neh\u00b7men", "sie's", "nicht", "krumm", ",", "da\u00df", "es", "zu", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich also stolzen Wortes mag erk\u00fchnen:", "tokens": ["Sich", "al\u00b7so", "stol\u00b7zen", "Wor\u00b7tes", "mag", "er\u00b7k\u00fch\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbich bin mit Fug der Abt in eurem Orden!", "tokens": ["\u00bb", "ich", "bin", "mit", "Fug", "der", "Abt", "in", "eu\u00b7rem", "Or\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn wem ist solch Pr\u00e4latenb\u00e4uchlein worden,", "tokens": ["Denn", "wem", "ist", "solch", "Pr\u00e4\u00b7la\u00b7ten\u00b7b\u00e4uch\u00b7lein", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PIAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein also rundgew\u00f6lbtes, kugelfeistes?", "tokens": ["Ein", "al\u00b7so", "rund\u00b7ge\u00b7w\u00f6lb\u00b7tes", ",", "ku\u00b7gel\u00b7feis\u00b7tes", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer ist von euch, gleich mir, so voll des Geistes?", "tokens": ["Wer", "ist", "von", "euch", ",", "gleich", "mir", ",", "so", "voll", "des", "Geis\u00b7tes", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "$,", "ADV", "PPER", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr f\u00fchlt's und kniet von Demut voll im Kreise", "tokens": ["Ihr", "f\u00fchlt's", "und", "kniet", "von", "De\u00b7mut", "voll", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tief unter mir nach frommer Br\u00fcder Weise,", "tokens": ["Tief", "un\u00b7ter", "mir", "nach", "from\u00b7mer", "Br\u00fc\u00b7der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sollt' aufs Haupt der Abt die Hand euch legen", "tokens": ["Als", "sollt'", "aufs", "Haupt", "der", "Abt", "die", "Hand", "euch", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber euch nun sprechen seinen Segen.", "tokens": ["Und", "\u00fc\u00b7ber", "euch", "nun", "spre\u00b7chen", "sei\u00b7nen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und \u00f6ffn' ich meine Lippen, wahrlich, Allen", "tokens": ["Und", "\u00f6ffn'", "ich", "mei\u00b7ne", "Lip\u00b7pen", ",", "wahr\u00b7lich", ",", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "$,", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird meine Lehr' und Predigt wohlgefallen;", "tokens": ["Wird", "mei\u00b7ne", "Lehr'", "und", "Pre\u00b7digt", "wohl\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn voll und kr\u00e4ftig dem Pr\u00e4latenmunde", "tokens": ["Denn", "voll", "und", "kr\u00e4f\u00b7tig", "dem", "Pr\u00e4\u00b7la\u00b7ten\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entquillt die unverf\u00e4lschte goldne Kunde.", "tokens": ["Ent\u00b7quillt", "die", "un\u00b7ver\u00b7f\u00e4lschte", "gold\u00b7ne", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Seht meinen Leib in brauner Kutte prangen,", "tokens": ["Seht", "mei\u00b7nen", "Leib", "in", "brau\u00b7ner", "Kut\u00b7te", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wanst gespannt in blanke G\u00fcrtelspangen;", "tokens": ["Den", "Wanst", "ge\u00b7spannt", "in", "blan\u00b7ke", "G\u00fcr\u00b7tel\u00b7span\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus nied'rem Stamm hat mich der Herr erhoben,", "tokens": ["Aus", "nie\u00b7d'\u00b7rem", "Stamm", "hat", "mich", "der", "Herr", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df ihr in mir m\u00f6gt seine Wunder loben!", "tokens": ["Da\u00df", "ihr", "in", "mir", "m\u00f6gt", "sei\u00b7ne", "Wun\u00b7der", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn meine V\u00e4ter sind nur Winzerleute,", "tokens": ["Denn", "mei\u00b7ne", "V\u00e4\u00b7ter", "sind", "nur", "Win\u00b7zer\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als B\u00e4urin dient noch Mutter Erde heute;", "tokens": ["Als", "B\u00e4u\u00b7rin", "dient", "noch", "Mut\u00b7ter", "Er\u00b7de", "heu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADV", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der einz'ge Unstern in dem Spiele:", "tokens": ["Das", "ist", "der", "einz'\u00b7ge", "Uns\u00b7tern", "in", "dem", "Spie\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach eine Mutter nur und V\u00e4ter viele!", "tokens": ["Ach", "ei\u00b7ne", "Mut\u00b7ter", "nur", "und", "V\u00e4\u00b7ter", "vie\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "ADV", "KON", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Kindheit Garten, Eltern, Br\u00fcder alle", "tokens": ["Der", "Kind\u00b7heit", "Gar\u00b7ten", ",", "El\u00b7tern", ",", "Br\u00fc\u00b7der", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlie\u00df um die Clausur ich dieser Halle!", "tokens": ["Ver\u00b7lie\u00df", "um", "die", "Clau\u00b7sur", "ich", "die\u00b7ser", "Hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der Jugendfreiheit sonn'ger Schw\u00fcle", "tokens": ["Und", "aus", "der", "Ju\u00b7gend\u00b7frei\u00b7heit", "sonn'\u00b7ger", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trat ich zum Klosterfrieden dieser K\u00fchle.", "tokens": ["Trat", "ich", "zum", "Klos\u00b7ter\u00b7frie\u00b7den", "die\u00b7ser", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dort mu\u00dft' am Stab bergan ich klimmend keuchen,", "tokens": ["Dort", "mu\u00dft'", "am", "Stab", "ber\u00b7gan", "ich", "klim\u00b7mend", "keu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier dehn' ich mich auf breitem Bett von Eichen;", "tokens": ["Hier", "dehn'", "ich", "mich", "auf", "brei\u00b7tem", "Bett", "von", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort hab' ich jeder Wolke bang gezittert,", "tokens": ["Dort", "hab'", "ich", "je\u00b7der", "Wol\u00b7ke", "bang", "ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier h\u00f6r' ich's kaum, wenn's oben sturmgewittert.", "tokens": ["Hier", "h\u00f6r'", "ich's", "kaum", ",", "wenn's", "o\u00b7ben", "sturm\u00b7ge\u00b7wit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ganz eingesessen meinem Lehngest\u00fchle,", "tokens": ["Ganz", "ein\u00b7ge\u00b7ses\u00b7sen", "mei\u00b7nem", "Lehn\u00b7ge\u00b7st\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird mir das Aufstehn schwer aus seinem Pf\u00fchle;", "tokens": ["Wird", "mir", "das", "Auf\u00b7stehn", "schwer", "aus", "sei\u00b7nem", "Pf\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da m\u00fcssen sie von hinten, kaum zu glauben,", "tokens": ["Da", "m\u00fcs\u00b7sen", "sie", "von", "hin\u00b7ten", ",", "kaum", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich in die H\u00f6he mit der Winde schrauben!", "tokens": ["Mich", "in", "die", "H\u00f6\u00b7he", "mit", "der", "Win\u00b7de", "schrau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nur wenn der Lenz um jene H\u00fcgel gl\u00fchte,", "tokens": ["Nur", "wenn", "der", "Lenz", "um", "je\u00b7ne", "H\u00fc\u00b7gel", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jede Rebe sprie\u00dft in voller Bl\u00fcthe,", "tokens": ["Und", "je\u00b7de", "Re\u00b7be", "sprie\u00dft", "in", "vol\u00b7ler", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wird mir's fast zu eng in diesen Hallen,", "tokens": ["Da", "wird", "mir's", "fast", "zu", "eng", "in", "die\u00b7sen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mein Pr\u00e4latenblut beginnt zu wallen!", "tokens": ["Und", "mein", "Pr\u00e4\u00b7la\u00b7ten\u00b7blut", "be\u00b7ginnt", "zu", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Vom Leibe m\u00f6cht' ich meine Kutte l\u00f6sen", "tokens": ["Vom", "Lei\u00b7be", "m\u00f6cht'", "ich", "mei\u00b7ne", "Kut\u00b7te", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder Rebe sein, wie ich's gewesen,", "tokens": ["Und", "wie\u00b7der", "Re\u00b7be", "sein", ",", "wie", "ich's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAINF", "$,", "PWAV", "PIS", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf meinem Herzen auch mein Str\u00e4u\u00dfchen bringen", "tokens": ["Auf", "mei\u00b7nem", "Her\u00b7zen", "auch", "mein", "Str\u00e4u\u00df\u00b7chen", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen Arm um eine Schwester schlingen!\u00ab", "tokens": ["Und", "mei\u00b7nen", "Arm", "um", "ei\u00b7ne", "Schwes\u00b7ter", "schlin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}