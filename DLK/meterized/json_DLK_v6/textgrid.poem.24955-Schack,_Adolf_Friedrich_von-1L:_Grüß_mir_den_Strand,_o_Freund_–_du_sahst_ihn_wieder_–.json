{"textgrid.poem.24955": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gr\u00fc\u00df mir den Strand, o Freund \u2013 du sahst ihn wieder \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df mir den Strand, o Freund \u2013 du sahst ihn wieder \u2013", "tokens": ["Gr\u00fc\u00df", "mir", "den", "Strand", ",", "o", "Freund", "\u2013", "du", "sahst", "ihn", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "FM", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den ernsten Zeugen meiner fr\u00fchsten Lieder,", "tokens": ["Den", "erns\u00b7ten", "Zeu\u00b7gen", "mei\u00b7ner", "fr\u00fchs\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich den ersten Jugendtraum getr\u00e4umt;", "tokens": ["Wo", "ich", "den", "ers\u00b7ten", "Ju\u00b7gend\u00b7traum", "ge\u00b7tr\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den hoch umrauschten Strand, den klippenvollen,", "tokens": ["Den", "hoch", "um\u00b7rauschten", "Strand", ",", "den", "klip\u00b7pen\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Um den, wenn sturmgepeitscht die Wogen rollen,", "tokens": ["Um", "den", ",", "wenn", "sturm\u00b7ge\u00b7peitscht", "die", "Wo\u00b7gen", "rol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "$,", "KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wilde Nordsee wallt und sch\u00e4umt.", "tokens": ["Die", "wil\u00b7de", "Nord\u00b7see", "wallt", "und", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Oed' ist die K\u00fcste, ohne Wald und Gr\u00fcne;", "tokens": ["O\u00b7ed'", "ist", "die", "K\u00fcs\u00b7te", ",", "oh\u00b7ne", "Wald", "und", "Gr\u00fc\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur d\u00fcstre Tannen wachsen auf der D\u00fcne;", "tokens": ["Nur", "d\u00fcst\u00b7re", "Tan\u00b7nen", "wach\u00b7sen", "auf", "der", "D\u00fc\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Winde schwankt das d\u00fcrre Farrenkraut,", "tokens": ["Im", "Win\u00b7de", "schwankt", "das", "d\u00fcr\u00b7re", "Far\u00b7ren\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hie und da aus einzler F\u00f6hren Mitte", "tokens": ["Und", "hie", "und", "da", "aus", "einz\u00b7ler", "F\u00f6h\u00b7ren", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erhebt sich einsam eine Fischerh\u00fctte,", "tokens": ["Er\u00b7hebt", "sich", "ein\u00b7sam", "ei\u00b7ne", "Fi\u00b7scher\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die auf die Brandung niederschaut.", "tokens": ["Die", "auf", "die", "Bran\u00b7dung", "nie\u00b7der\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und an dem Strande ragt mit morschen Zinnen", "tokens": ["Und", "an", "dem", "Stran\u00b7de", "ragt", "mit", "mor\u00b7schen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schlo\u00df, um das die Sturmverk\u00fcnderinnen,", "tokens": ["Ein", "Schlo\u00df", ",", "um", "das", "die", "Sturm\u00b7ver\u00b7k\u00fcn\u00b7de\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00f6ven, kreisen im gezackten Flug \u2013", "tokens": ["Die", "M\u00f6\u00b7ven", ",", "krei\u00b7sen", "im", "ge\u00b7zack\u00b7ten", "Flug", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einst, o wie oft blickt' ich aus seinem Turme", "tokens": ["Einst", ",", "o", "wie", "oft", "blickt'", "ich", "aus", "sei\u00b7nem", "Tur\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "KOKOM", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aufs Meer hinab, das im Dezembersturme", "tokens": ["Aufs", "Meer", "hin\u00b7ab", ",", "das", "im", "De\u00b7zem\u00b7ber\u00b7stur\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zum Riff empor die Wogen schlug!", "tokens": ["Zum", "Riff", "em\u00b7por", "die", "Wo\u00b7gen", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auf der Fluten ewig regem Tosen", "tokens": ["Und", "auf", "der", "Flu\u00b7ten", "e\u00b7wig", "re\u00b7gem", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Glitt mir der Blick, bis wo im Grenzenlosen", "tokens": ["Glitt", "mir", "der", "Blick", ",", "bis", "wo", "im", "Gren\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel mit dem Wellenspiel verschmolz;", "tokens": ["Der", "Him\u00b7mel", "mit", "dem", "Wel\u00b7len\u00b7spiel", "ver\u00b7schmolz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O, also noch in unerme\u00dfne Weiten", "tokens": ["O", ",", "al\u00b7so", "noch", "in", "un\u00b7er\u00b7me\u00df\u00b7ne", "Wei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sah ich das Leben sich vor mir verbreiten", "tokens": ["Sah", "ich", "das", "Le\u00b7ben", "sich", "vor", "mir", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "APPR", "PPER", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In meiner Jugend erstem Stolz.", "tokens": ["In", "mei\u00b7ner", "Ju\u00b7gend", "ers\u00b7tem", "Stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Geist durchflog die uferlosen R\u00e4ume;", "tokens": ["Mein", "Geist", "durch\u00b7flog", "die", "u\u00b7fer\u00b7lo\u00b7sen", "R\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jener Brandung wogten meine Tr\u00e4ume;", "tokens": ["Auf", "je\u00b7ner", "Bran\u00b7dung", "wog\u00b7ten", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In jeder Welle, die sich sch\u00e4umend brach,", "tokens": ["In", "je\u00b7der", "Wel\u00b7le", ",", "die", "sich", "sch\u00e4u\u00b7mend", "brach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Klang mir ein Wort, das mir von hohen Dingen,", "tokens": ["Klang", "mir", "ein", "Wort", ",", "das", "mir", "von", "ho\u00b7hen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Von gro\u00dfen Thaten, k\u00fcnftig zu vollbringen,", "tokens": ["Von", "gro\u00b7\u00dfen", "Tha\u00b7ten", ",", "k\u00fcnf\u00b7tig", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit mahnender Verhei\u00dfung sprach.", "tokens": ["Mit", "mah\u00b7nen\u00b7der", "Ver\u00b7hei\u00b7\u00dfung", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Wolken brachten in den grauen Falten", "tokens": ["Die", "Wol\u00b7ken", "brach\u00b7ten", "in", "den", "grau\u00b7en", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir Wunderbilder mit und Traumgestalten,", "tokens": ["Mir", "Wun\u00b7der\u00b7bil\u00b7der", "mit", "und", "Traum\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes Schiff, das fern am Himmelsrand", "tokens": ["Und", "je\u00b7des", "Schiff", ",", "das", "fern", "am", "Him\u00b7mels\u00b7rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufstieg, vom Duft der Ferne noch umnachtet,", "tokens": ["Auf\u00b7stieg", ",", "vom", "Duft", "der", "Fer\u00b7ne", "noch", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War mir mit einem Schatz befrachtet", "tokens": ["War", "mir", "mit", "ei\u00b7nem", "Schatz", "be\u00b7frach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einem fernen Zauberland.", "tokens": ["Aus", "ei\u00b7nem", "fer\u00b7nen", "Zau\u00b7ber\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie oft auf meinem sturmgewohnten Kahne", "tokens": ["Wie", "oft", "auf", "mei\u00b7nem", "sturm\u00b7ge\u00b7wohn\u00b7ten", "Kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fuhr ich hinaus, umwirbelt vom Orkane,", "tokens": ["Fuhr", "ich", "hin\u00b7aus", ",", "um\u00b7wir\u00b7belt", "vom", "Or\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn \u00fcbers Meer der Nord die Gei\u00dfel schwang", "tokens": ["Wenn", "\u00fc\u00b7bers", "Meer", "der", "Nord", "die", "Gei\u00b7\u00dfel", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bald mich auf emp\u00f6rter Wellen Nacken", "tokens": ["Und", "bald", "mich", "auf", "em\u00b7p\u00f6r\u00b7ter", "Wel\u00b7len", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entgegenschleuderte den Klippenzacken,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7schleu\u00b7der\u00b7te", "den", "Klip\u00b7pen\u00b7za\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Bald abw\u00e4rts in die Tiefe schlang.", "tokens": ["Bald", "ab\u00b7w\u00e4rts", "in", "die", "Tie\u00b7fe", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Meer, heil'ges Meer! In deinem Wetterbrausen", "tokens": ["Meer", ",", "heil'\u00b7ges", "Meer", "!", "In", "dei\u00b7nem", "Wet\u00b7ter\u00b7brau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "H\u00f6rt' ich die Donnerworte Gottes sausen,", "tokens": ["H\u00f6rt'", "ich", "die", "Don\u00b7ner\u00b7wor\u00b7te", "Got\u00b7tes", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah die Blitze seiner Herrlichkeit;", "tokens": ["Ich", "sah", "die", "Blit\u00b7ze", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den m\u00e4cht'gen Puls des Weltalls f\u00fchlt' ich klopfen;", "tokens": ["Den", "m\u00e4cht'\u00b7gen", "Puls", "des", "Welt\u00b7alls", "f\u00fchlt'", "ich", "klop\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Unendlichkeit warst du; wie Wassertropfen", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "warst", "du", ";", "wie", "Was\u00b7ser\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zerrannen in dich Raum und Zeit!", "tokens": ["Zer\u00b7ran\u00b7nen", "in", "dich", "Raum", "und", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und staunend blickt' ich in die Wunderf\u00fclle;", "tokens": ["Und", "stau\u00b7nend", "blickt'", "ich", "in", "die", "Wun\u00b7der\u00