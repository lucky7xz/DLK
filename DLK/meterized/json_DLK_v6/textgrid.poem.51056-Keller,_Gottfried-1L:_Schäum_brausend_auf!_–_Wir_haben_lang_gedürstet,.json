{"textgrid.poem.51056": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00e4um brausend auf! \u2013 Wir haben lang ged\u00fcrstet,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4um brausend auf! \u2013 Wir haben lang ged\u00fcrstet,", "tokens": ["Sch\u00e4um", "brau\u00b7send", "auf", "!", "\u2013", "Wir", "ha\u00b7ben", "lang", "ge\u00b7d\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Goldpokal, nach einem jungen Wein,", "tokens": ["Du", "Gold\u00b7po\u00b7kal", ",", "nach", "ei\u00b7nem", "jun\u00b7gen", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da traf in dir ein guter Jahrgang ein,", "tokens": ["Da", "traf", "in", "dir", "ein", "gu\u00b7ter", "Jahr\u00b7gang", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir haben was getrunken, was geb\u00fcrstet!", "tokens": ["Wir", "ha\u00b7ben", "was", "ge\u00b7trun\u00b7ken", ",", "was", "ge\u00b7b\u00fcrs\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch immer ragt Zwing-Uri hoch gefirstet,", "tokens": ["Noch", "im\u00b7mer", "ragt", "Zwing\u00b7U\u00b7ri", "hoch", "ge\u00b7firs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch ist die Zeit ein stummer Totenschrein,", "tokens": ["Noch", "ist", "die", "Zeit", "ein", "stum\u00b7mer", "To\u00b7ten\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Schl\u00e4fer harrt auf seinen Osterschein \u2013", "tokens": ["Der", "Schl\u00e4\u00b7fer", "harrt", "auf", "sei\u00b7nen", "Os\u00b7ter\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Wecker bist vor vielen du gef\u00fcrstet!", "tokens": ["Zum", "We\u00b7cker", "bist", "vor", "vie\u00b7len", "du", "ge\u00b7f\u00fcrs\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn nach Sturm der Friedensbogen lacht,", "tokens": ["Doch", "wenn", "nach", "Sturm", "der", "Frie\u00b7dens\u00b7bo\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn der D\u00e4monen finstre Schar bezwungen,", "tokens": ["Wenn", "der", "D\u00e4\u00b7mo\u00b7nen", "finst\u00b7re", "Schar", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur\u00fcckgescheucht in ihres Ursprungs Nacht:", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7scheucht", "in", "ih\u00b7res", "Ur\u00b7sprungs", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann soll dein Lied, das uns nur Sturm gesungen,", "tokens": ["Dann", "soll", "dein", "Lied", ",", "das", "uns", "nur", "Sturm", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst voll erbl\u00fchn in reicher Fr\u00fchlingspracht:", "tokens": ["Erst", "voll", "er\u00b7bl\u00fchn", "in", "rei\u00b7cher", "Fr\u00fch\u00b7lings\u00b7pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur durch den Winter wird der Lenz errungen!", "tokens": ["Nur", "durch", "den", "Win\u00b7ter", "wird", "der", "Lenz", "er\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}