{"dta.poem.8004": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Im sch\u00f6nsten Garten wallten", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Gar\u00b7ten", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Buhlen, Hand in Hand,", "tokens": ["Zwei", "Buh\u00b7len", ",", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwo bleiche, kranke Gestalten,", "tokens": ["Zwo", "blei\u00b7che", ",", "kran\u00b7ke", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie sa\u00dfen in\u2019s Blumenland.", "tokens": ["Sie", "sa\u00b7\u00dfen", "in's", "Blu\u00b7men\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie k\u00fc\u00dften sich auf die Wangen", "tokens": ["Sie", "k\u00fc\u00df\u00b7ten", "sich", "auf", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und k\u00fc\u00dften sich auf den Mund,", "tokens": ["Und", "k\u00fc\u00df\u00b7ten", "sich", "auf", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie hielten sich fest umfangen,", "tokens": ["Sie", "hiel\u00b7ten", "sich", "fest", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie wurden jung und gesund.", "tokens": ["Sie", "wur\u00b7den", "jung", "und", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Zwei Gl\u00f6cklein klangen helle,", "tokens": ["Zwei", "Gl\u00f6c\u00b7klein", "klan\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Traum entschwand zur Stund\u2019;", "tokens": ["Der", "Traum", "ent\u00b7schwand", "zur", "Stund'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lag in der Klosterzelle,", "tokens": ["Sie", "lag", "in", "der", "Klos\u00b7ter\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er fern in Thurmes Grund.", "tokens": ["Er", "fern", "in", "Thur\u00b7mes", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}