{"textgrid.poem.61128": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5. Die K\u00fcsse", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Saloniki war es nicht,", "tokens": ["In", "Sa\u00b7lo\u00b7ni\u00b7ki", "war", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht war's im schmucken St\u00e4dtchen,", "tokens": ["Nicht", "wa\u00b7r's", "im", "schmu\u00b7cken", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im armen Wlachenlande liebt'", "tokens": ["Im", "ar\u00b7men", "Wla\u00b7chen\u00b7lan\u00b7de", "liebt'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich einer Witwe M\u00e4dchen.", "tokens": ["Ich", "ei\u00b7ner", "Wit\u00b7we", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt schm\u00fccke, Mutter, schm\u00fcck' das Haus", "tokens": ["Jetzt", "schm\u00fc\u00b7cke", ",", "Mut\u00b7ter", ",", "schm\u00fcck", "das", "Haus"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schm\u00fccke deinen Garten!", "tokens": ["Und", "schm\u00fc\u00b7cke", "dei\u00b7nen", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Tochter dein so hold und fein", "tokens": ["Die", "Toch\u00b7ter", "dein", "so", "hold", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll mich als Braut erwarten.", "tokens": ["Soll", "mich", "als", "Braut", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie hat die Lippen rosenrot", "tokens": ["Sie", "hat", "die", "Lip\u00b7pen", "ro\u00b7sen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4rbt mit rotem Scheine;", "tokens": ["Ge\u00b7f\u00e4rbt", "mit", "ro\u00b7tem", "Schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich neigte mich und k\u00fc\u00dfte sie", "tokens": ["Ich", "neig\u00b7te", "mich", "und", "k\u00fc\u00df\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4rbte auch die meinen.", "tokens": ["Und", "f\u00e4rb\u00b7te", "auch", "die", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In dreien Fl\u00fcssen wusch ich sie", "tokens": ["In", "drei\u00b7en", "Fl\u00fcs\u00b7sen", "wusch", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00e4rbte rot die Fl\u00fcsse", "tokens": ["Und", "f\u00e4rb\u00b7te", "rot", "die", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00e4rbte rot das Meer dazu", "tokens": ["Und", "f\u00e4rb\u00b7te", "rot", "das", "Meer", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihre roten K\u00fcsse.", "tokens": ["Durch", "ih\u00b7re", "ro\u00b7ten", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}