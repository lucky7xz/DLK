{"textgrid.poem.43676": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Honigernte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft euch nicht l\u00e4nger bitten!", "tokens": ["La\u00dft", "euch", "nicht", "l\u00e4n\u00b7ger", "bit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es giebt gar s\u00fc\u00dfen Schmaus;", "tokens": ["Es", "giebt", "gar", "s\u00fc\u00b7\u00dfen", "Schmaus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Honig wird geschnitten!", "tokens": ["Der", "Ho\u00b7nig", "wird", "ge\u00b7schnit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinaus, aufs Dorf hinaus!", "tokens": ["Hin\u00b7aus", ",", "aufs", "Dorf", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie hell doch aus den Waben", "tokens": ["Wie", "hell", "doch", "aus", "den", "Wa\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fc\u00dfe Honig flie\u00dft!", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Ho\u00b7nig", "flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Anblick schon mu\u00df laben,", "tokens": ["Der", "An\u00b7blick", "schon", "mu\u00df", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch eh' der Gaum genie\u00dft.", "tokens": ["Noch", "eh'", "der", "Gaum", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun l\u00e4\u00dft sich leicht erkl\u00e4ren,", "tokens": ["Nun", "l\u00e4\u00dft", "sich", "leicht", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum man gern ihn schleckt,", "tokens": ["Wa\u00b7rum", "man", "gern", "ihn", "schleckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch warum den B\u00e4ren", "tokens": ["Und", "auch", "wa\u00b7rum", "den", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gut der Honig schmeckt.", "tokens": ["So", "gut", "der", "Ho\u00b7nig", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}