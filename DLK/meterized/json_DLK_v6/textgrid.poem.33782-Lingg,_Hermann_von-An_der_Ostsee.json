{"textgrid.poem.33782": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "An der Ostsee", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Ufer, eh' der Tag anbricht,", "tokens": ["Am", "U\u00b7fer", ",", "eh'", "der", "Tag", "an\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt man die Welle klagen,", "tokens": ["H\u00f6rt", "man", "die", "Wel\u00b7le", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zum Nix schleicht dann der Nebelwicht,", "tokens": ["Zum", "Nix", "schleicht", "dann", "der", "Ne\u00b7bel\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00e4hlt von alten Tagen.", "tokens": ["Er\u00b7z\u00e4hlt", "von", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie Eis und Flut zusammenkracht,", "tokens": ["Wie", "Eis", "und", "Flut", "zu\u00b7sam\u00b7men\u00b7kracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Nordlicht und Gewitter,", "tokens": ["Wie", "Nord\u00b7licht", "und", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So trafen einst in scharfer Schlacht", "tokens": ["So", "tra\u00b7fen", "einst", "in", "schar\u00b7fer", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Heiden und Christenritter.", "tokens": ["Sich", "Hei\u00b7den", "und", "Chris\u00b7ten\u00b7rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da brach der drachengefl\u00fcgelte Helm,", "tokens": ["Da", "brach", "der", "dra\u00b7chen\u00b7ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Helm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ins Meer sank Herta's Wagen;", "tokens": ["Ins", "Meer", "sank", "Her\u00b7ta's", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Starken zwang der kluge Schelm,", "tokens": ["Den", "Star\u00b7ken", "zwang", "der", "klu\u00b7ge", "Schelm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Helden wurden erschlagen.", "tokens": ["Die", "Hel\u00b7den", "wur\u00b7den", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Letzte, der der Schlacht entrann,", "tokens": ["Der", "Letz\u00b7te", ",", "der", "der", "Schlacht", "ent\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war ein wunder Skalde,", "tokens": ["Es", "war", "ein", "wun\u00b7der", "Skal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: \u00bbO tragt mich sterbenden Mann", "tokens": ["Er", "sprach", ":", "\u00bb", "O", "tragt", "mich", "ster\u00b7ben\u00b7den", "Mann"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zum k\u00fchlen gr\u00fcnen Walde!", "tokens": ["Zum", "k\u00fch\u00b7len", "gr\u00fc\u00b7nen", "Wal\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im k\u00fchlen Waldgrund m\u00f6cht' ich ruhn,", "tokens": ["Im", "k\u00fch\u00b7len", "Wald\u00b7grund", "m\u00f6cht'", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Wodan's letzter Eiche", "tokens": ["An", "Wo\u00b7dan's", "letz\u00b7ter", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6cht' ich den letzten Atem tun,", "tokens": ["M\u00f6cht'", "ich", "den", "letz\u00b7ten", "A\u00b7tem", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dorthin legt meine Leiche.\u00ab", "tokens": ["Dor\u00b7thin", "legt", "mei\u00b7ne", "Lei\u00b7che", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sprach's, da kam das Heergesind", "tokens": ["Er", "sprach's", ",", "da", "kam", "das", "Heer\u00b7ge\u00b7sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siegreicher Christenboten,", "tokens": ["Sieg\u00b7rei\u00b7cher", "Chris\u00b7ten\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie tauften ihn, sein Blick war blind,", "tokens": ["Sie", "tauf\u00b7ten", "ihn", ",", "sein", "Blick", "war", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie tauften einen Toten.", "tokens": ["Sie", "tauf\u00b7ten", "ei\u00b7nen", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nicht Runen grub man auf sein Grab,", "tokens": ["Nicht", "Ru\u00b7nen", "grub", "man", "auf", "sein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kreuz stand auf den D\u00fcnen;", "tokens": ["Ein", "Kreuz", "stand", "auf", "den", "D\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ri\u00df die Flut das Kreuz herab,", "tokens": ["Da", "ri\u00df", "die", "Flut", "das", "Kreuz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab vom Grab des H\u00fcnen.", "tokens": ["Her\u00b7ab", "vom", "Grab", "des", "H\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}