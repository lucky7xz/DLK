{"dta.poem.14886": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Einen wunderbaren Hofstaat", "tokens": ["Ei\u00b7nen", "wun\u00b7der\u00b7ba\u00b7ren", "Hof\u00b7staat"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Prinzessin dorten f\u00fchret,", "tokens": ["Die", "Prin\u00b7zes\u00b7sin", "dor\u00b7ten", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.3": {"text": "Hat ein'n wunderlichen Alten,", "tokens": ["Hat", "ein'n", "wun\u00b7der\u00b7li\u00b7chen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der das ganze Haus regieret.", "tokens": ["Der", "das", "gan\u00b7ze", "Haus", "re\u00b7gie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Mantel tr\u00e4gt der Alte,", "tokens": ["Ei\u00b7nen", "Man\u00b7tel", "tr\u00e4gt", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schillernd bunt in allen Farben", "tokens": ["Schil\u00b7lernd", "bunt", "in", "al\u00b7len", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit unz\u00e4hligen Zierrathen,", "tokens": ["Mit", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Zier\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Spielzeug hat er in den Falten", "tokens": ["Spiel\u00b7zeug", "hat", "er", "in", "den", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Scheint der Monden helle drau\u00dfen,", "tokens": ["Scheint", "der", "Mon\u00b7den", "hel\u00b7le", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wolken fliegen \u00fcber'm Grunde:", "tokens": ["Wol\u00b7ken", "flie\u00b7gen", "\u00fc\u00b7ber'm", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "F\u00e4ngt er drau\u00dfen an zu hausen,", "tokens": ["F\u00e4ngt", "er", "drau\u00b7\u00dfen", "an", "zu", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kramt sein Spielzeug aus zur Stunde.", "tokens": ["Kramt", "sein", "Spiel\u00b7zeug", "aus", "zur", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und das Spielzeug um den Alten", "tokens": ["Und", "das", "Spiel\u00b7zeug", "um", "den", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "R\u00fchrt sich bald beim Mondenscheine,", "tokens": ["R\u00fchrt", "sich", "bald", "beim", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Zupfet ihn beim langen Barte,", "tokens": ["Zup\u00b7fet", "ihn", "beim", "lan\u00b7gen", "Bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schlingt um ihn die bunten Kreise", "tokens": ["Schlingt", "um", "ihn", "die", "bun\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Auch die Bl\u00fcmlein nach ihm langen,", "tokens": ["Auch", "die", "Bl\u00fcm\u00b7lein", "nach", "ihm", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "M\u00f6chten doch sich sittsam zeigen,", "tokens": ["M\u00f6ch\u00b7ten", "doch", "sich", "sitt\u00b7sam", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Zieh'n verstohlen ihn beim Mantel,", "tokens": ["Zieh'n", "ver\u00b7stoh\u00b7len", "ihn", "beim", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Lachen dann in sich gar heimlich.", "tokens": ["La\u00b7chen", "dann", "in", "sich", "gar", "heim\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und ringsum die ganze Runde", "tokens": ["Und", "ring\u00b7sum", "die", "gan\u00b7ze", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}