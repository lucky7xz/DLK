{"textgrid.poem.55950": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hab ich das gef\u00fchlt was Abschied hei\u00dft.", "tokens": ["Wie", "hab", "ich", "das", "ge\u00b7f\u00fchlt", "was", "Ab\u00b7schied", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDS", "VVPP", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wei\u00df ichs noch: ein dunkles unverwundnes", "tokens": ["Wie", "wei\u00df", "ichs", "noch", ":", "ein", "dunk\u00b7les", "un\u00b7ver\u00b7wund\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "$.", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "grausames Etwas, das ein Sch\u00f6nverbundnes", "tokens": ["grau\u00b7sa\u00b7mes", "Et\u00b7was", ",", "das", "ein", "Sch\u00f6n\u00b7ver\u00b7bund\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "noch einmal zeigt und hinh\u00e4lt und zerrei\u00dft.", "tokens": ["noch", "ein\u00b7mal", "zeigt", "und", "hin\u00b7h\u00e4lt", "und", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie war ich ohne Wehr, dem zuzuschauen,", "tokens": ["Wie", "war", "ich", "oh\u00b7ne", "Wehr", ",", "dem", "zu\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NN", "$,", "PRELS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das, da es mich, mich rufend, gehen lie\u00df,", "tokens": ["das", ",", "da", "es", "mich", ",", "mich", "ru\u00b7fend", ",", "ge\u00b7hen", "lie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PRF", "$,", "PPER", "VVPP", "$,", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zur\u00fcckblieb, so als w\u00e4rens alle Frauen", "tokens": ["zu\u00b7r\u00fcck\u00b7blieb", ",", "so", "als", "w\u00e4\u00b7rens", "al\u00b7le", "Frau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "KOKOM", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und dennoch klein und wei\u00df und nichts als dies:", "tokens": ["und", "den\u00b7noch", "klein", "und", "wei\u00df", "und", "nichts", "als", "dies", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "VVFIN", "KON", "PIS", "KOKOM", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Winken, schon nicht mehr auf mich bezogen,", "tokens": ["Ein", "Win\u00b7ken", ",", "schon", "nicht", "mehr", "auf", "mich", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein leise Weiterwinkendes \u2013 , schon kaum", "tokens": ["ein", "lei\u00b7se", "Wei\u00b7ter\u00b7win\u00b7ken\u00b7des", "\u2013", ",", "schon", "kaum"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erkl\u00e4rbar mehr: vielleicht ein Pflaumenbaum,", "tokens": ["er\u00b7kl\u00e4r\u00b7bar", "mehr", ":", "viel\u00b7leicht", "ein", "Pflau\u00b7men\u00b7baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "von dem ein Kuckuck hastig abgeflogen.", "tokens": ["von", "dem", "ein", "Ku\u00b7ckuck", "has\u00b7tig", "ab\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}