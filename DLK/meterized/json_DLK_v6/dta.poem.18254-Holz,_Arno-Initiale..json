{"dta.poem.18254": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Initiale.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blondes Vollweib, das durch die W\u00e4lder strich;", "tokens": ["Ein", "blon\u00b7des", "Voll\u00b7weib", ",", "das", "durch", "die", "W\u00e4l\u00b7der", "strich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch heut ist l\u00e4ngst ihr schlotternder Busen", "tokens": ["Doch", "heut", "ist", "l\u00e4ngst", "ihr", "schlot\u00b7tern\u00b7der", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Platt wie ein Pl\u00e4ttbrett!", "tokens": ["Platt", "wie", "ein", "Pl\u00e4tt\u00b7brett", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Das gute Frauchen hat zu viel Thee geschluckt", "tokens": ["Das", "gu\u00b7te", "Frau\u00b7chen", "hat", "zu", "viel", "Thee", "ge\u00b7schluckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und leidet nun an Husten und Heiserkeit;", "tokens": ["Und", "lei\u00b7det", "nun", "an", "Hus\u00b7ten", "und", "Hei\u00b7ser\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich aber frage, wann wird sie wieder", "tokens": ["Ich", "a\u00b7ber", "fra\u00b7ge", ",", "wann", "wird", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PWAV", "VAFIN", "PPER", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Saugrob wie Luther?", "tokens": ["Sau\u00b7grob", "wie", "Lu\u00b7ther", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}