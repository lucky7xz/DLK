{"textgrid.poem.37612": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist des alten K\u00f6nigs (Friedrichs des Gro\u00dfen)", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Berge ohne Sorgen", "tokens": ["Auf", "dem", "Ber\u00b7ge", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt der gro\u00dfe K\u00f6nig krank,", "tokens": ["Sitzt", "der", "gro\u00b7\u00dfe", "K\u00f6\u00b7nig", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mit M\u00fch' den Kopf erhoben,", "tokens": ["Hat", "mit", "M\u00fch'", "den", "Kopf", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht zur Sonne hell und klar.", "tokens": ["Sieht", "zur", "Son\u00b7ne", "hell", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat die Sonne fast geblendet,", "tokens": ["Hat", "die", "Son\u00b7ne", "fast", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat zwei Sonnen im Gesicht,", "tokens": ["Hat", "zwei", "Son\u00b7nen", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ganz ruhig er da denket:", "tokens": ["Und", "ganz", "ru\u00b7hig", "er", "da", "den\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbsonne, bin bald n\u00e4her dir!\u00ab", "tokens": ["\u00bb", "son\u00b7ne", ",", "bin", "bald", "n\u00e4\u00b7her", "dir", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will sein Testament nun machen,", "tokens": ["Will", "sein", "Tes\u00b7ta\u00b7ment", "nun", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Findet, da\u00df er gar nichts hab,", "tokens": ["Fin\u00b7det", ",", "da\u00df", "er", "gar", "nichts", "hab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles seinen Unterthanen", "tokens": ["Al\u00b7les", "sei\u00b7nen", "Un\u00b7ter\u00b7tha\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er mit milden H\u00e4nden gab.", "tokens": ["Er", "mit", "mil\u00b7den", "H\u00e4n\u00b7den", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur den heil'gen, alten K\u00f6rper,", "tokens": ["Nur", "den", "heil'\u00b7gen", ",", "al\u00b7ten", "K\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df kein Mi\u00dfbrauch ihn entehr',", "tokens": ["Da\u00df", "kein", "Mi\u00df\u00b7brauch", "ihn", "ent\u00b7ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man ihn nicht m\u00f6chte st\u00f6ren,", "tokens": ["Da\u00df", "man", "ihn", "nicht", "m\u00f6ch\u00b7te", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' er fernen vom Verkehr.", "tokens": ["M\u00f6cht'", "er", "fer\u00b7nen", "vom", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die mit Liebe an ihm hingen", "tokens": ["Die", "mit", "Lie\u00b7be", "an", "ihm", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Noth mit ihm getheilt,", "tokens": ["Al\u00b7le", "Noth", "mit", "ihm", "ge\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine H\u00fcndlein vor ihm springen,", "tokens": ["Sei\u00b7ne", "H\u00fcnd\u00b7lein", "vor", "ihm", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher ist voraus geeilt.", "tokens": ["Man\u00b7cher", "ist", "vo\u00b7raus", "ge\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In der N\u00e4h' sind sie begraben,", "tokens": ["In", "der", "N\u00e4h'", "sind", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er schreibt mit eigner Hand:", "tokens": ["Und", "er", "schreibt", "mit", "eig\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbunter ihnen sei begraben", "tokens": ["\u00bb", "un\u00b7ter", "ih\u00b7nen", "sei", "be\u00b7gra\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch mein Leichnam ohne Pracht.\u00ab", "tokens": ["Auch", "mein", "Leich\u00b7nam", "oh\u00b7ne", "Pracht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbdenn die meine \u00c4ltern scheinen,", "tokens": ["\u00bb", "denn", "die", "mei\u00b7ne", "\u00c4l\u00b7tern", "schei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machten mir den ersten Schmerz,", "tokens": ["Mach\u00b7ten", "mir", "den", "ers\u00b7ten", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich zu beerben meinen,", "tokens": ["Die", "mich", "zu", "be\u00b7er\u00b7ben", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erben nicht mein gro\u00dfes Herz.\u00ab", "tokens": ["Er\u00b7ben", "nicht", "mein", "gro\u00b7\u00dfes", "Herz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Demuthvoll blieb unerf\u00fcllet", "tokens": ["De\u00b7muth\u00b7voll", "blieb", "un\u00b7er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Willens tr\u00fcber Ernst,", "tokens": ["Die\u00b7ses", "Wil\u00b7lens", "tr\u00fc\u00b7ber", "Ernst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndung damals schon enth\u00fcllte,", "tokens": ["Ahn\u00b7dung", "da\u00b7mals", "schon", "ent\u00b7h\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die Klugheit nicht entfernt.", "tokens": ["Was", "die", "Klug\u00b7heit", "nicht", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nein, zum gro\u00dfen Erbbegr\u00e4bni\u00df", "tokens": ["Nein", ",", "zum", "gro\u00b7\u00dfen", "Erb\u00b7be\u00b7gr\u00e4b\u00b7ni\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt der Erb' ihn selbst mit Pracht,", "tokens": ["Tr\u00e4gt", "der", "Erb'", "ihn", "selbst", "mit", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es zeiget dies Begegni\u00df,", "tokens": ["Und", "es", "zei\u00b7get", "dies", "Be\u00b7geg\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er schon ganz anders dacht'.", "tokens": ["Da\u00df", "er", "schon", "ganz", "an\u00b7ders", "dacht'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aus dem Marmorgrab der Kirche,", "tokens": ["Aus", "dem", "Mar\u00b7mor\u00b7grab", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ihn keine Neigung band,", "tokens": ["Wo", "ihn", "kei\u00b7ne", "Nei\u00b7gung", "band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam er oft zur\u00fcck geirret,", "tokens": ["Kam", "er", "oft", "zu\u00b7r\u00fcck", "ge\u00b7ir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Thieren, ihm bekannt.", "tokens": ["Zu", "den", "Thie\u00b7ren", ",", "ihm", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sch\u00fcttelt dann mit seinem Kopfe,", "tokens": ["Sch\u00fct\u00b7telt", "dann", "mit", "sei\u00b7nem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drohet mit dem Kr\u00fcckenstock,", "tokens": ["Dro\u00b7het", "mit", "dem", "Kr\u00fc\u00b7cken\u00b7stock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn's nicht alte Weiber logen,", "tokens": ["Wenn's", "nicht", "al\u00b7te", "Wei\u00b7ber", "lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die glauben an ihn noch.", "tokens": ["Denn", "die", "glau\u00b7ben", "an", "ihn", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Einmal wollt' er ruhig schlafen", "tokens": ["Ein\u00b7mal", "wollt'", "er", "ru\u00b7hig", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem engen K\u00e4mmerleim,", "tokens": ["In", "dem", "en\u00b7gen", "K\u00e4m\u00b7mer\u00b7leim", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Kommen Leute an mit Fackeln,", "tokens": ["Kom\u00b7men", "Leu\u00b7te", "an", "mit", "Fa\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie stellen sich da fein.", "tokens": ["Und", "sie", "stel\u00b7len", "sich", "da", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Kommt ein K\u00f6nig und ein Kaiser,", "tokens": ["Kommt", "ein", "K\u00f6\u00b7nig", "und", "ein", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine sch\u00f6ne K\u00f6nigin,", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprechen zu einander leiser,", "tokens": ["Spre\u00b7chen", "zu", "ein\u00b7an\u00b7der", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reichen sich die H\u00e4nde hin.", "tokens": ["Rei\u00b7chen", "sich", "die", "H\u00e4n\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Bald ist Krieg im ganzen Lande,", "tokens": ["Bald", "ist", "Krieg", "im", "gan\u00b7zen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner wei\u00df noch recht warum,", "tokens": ["Kei\u00b7ner", "wei\u00df", "noch", "recht", "wa\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Truppen schon voll Schande", "tokens": ["Als", "die", "Trup\u00b7pen", "schon", "voll", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laufen um die Erd' herum", "tokens": ["Lau\u00b7fen", "um", "die", "Erd'", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Als vom Sarge dieses Helden", "tokens": ["Als", "vom", "Sar\u00b7ge", "die\u00b7ses", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt ein fremder Held das Schwert.", "tokens": ["Nimmt", "ein", "frem\u00b7der", "Held", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4te Zeiten werden melden,", "tokens": ["Sp\u00e4\u00b7te", "Zei\u00b7ten", "wer\u00b7den", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was das heil'ge Schwert einst werth.", "tokens": ["Was", "das", "heil'\u00b7ge", "Schwert", "einst", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Unruhvoll mu\u00df er nun wandern", "tokens": ["Un\u00b7ruh\u00b7voll", "mu\u00df", "er", "nun", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem eilend fl\u00fccht'gen Heer,", "tokens": ["Mit", "dem", "ei\u00b7lend", "fl\u00fccht'\u00b7gen", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er Stiefeln mu\u00dfte tragen,", "tokens": ["Weil", "er", "Stie\u00b7feln", "mu\u00df\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wurde ihm das Laufen schwer.", "tokens": ["Wur\u00b7de", "ihm", "das", "Lau\u00b7fen", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und von einem zu dem andern", "tokens": ["Und", "von", "ei\u00b7nem", "zu", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab er seinen besten Rath,", "tokens": ["Gab", "er", "sei\u00b7nen", "bes\u00b7ten", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber immer mu\u00df er wandern,", "tokens": ["A\u00b7ber", "im\u00b7mer", "mu\u00df", "er", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil noch keiner danach that.", "tokens": ["Weil", "noch", "kei\u00b7ner", "da\u00b7nach", "that", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Gar kein Feldherr wollt' ihn kennen,", "tokens": ["Gar", "kein", "Feld\u00b7herr", "wollt'", "ihn", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser warf ihn aus dem Haus,", "tokens": ["Die\u00b7ser", "warf", "ihn", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser wollte mehr noch k\u00f6nnen,", "tokens": ["Die\u00b7ser", "woll\u00b7te", "mehr", "noch", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meint, man k\u00e4m' damit nicht aus.", "tokens": ["Meint", ",", "man", "k\u00e4m'", "da\u00b7mit", "nicht", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIS", "VVFIN", "PAV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Endlich kam er zu dem Dichter,", "tokens": ["End\u00b7lich", "kam", "er", "zu", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch, weil er franz\u00f6sisch sprach;", "tokens": ["Doch", ",", "weil", "er", "fran\u00b7z\u00f6\u00b7sisch", "sprach", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Rief er auch den kritschen Richter", "tokens": ["Rief", "er", "auch", "den", "krit\u00b7schen", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem sch\u00f6nen Himmel wach.", "tokens": ["Aus", "dem", "sch\u00f6\u00b7nen", "Him\u00b7mel", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Doch der sprach von Lesearten,", "tokens": ["Doch", "der", "sprach", "von", "Le\u00b7se\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich konnt' ihn nicht verstehn,", "tokens": ["Und", "ich", "konnt'", "ihn", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Metrik sollt' ich halten,", "tokens": ["Auf", "die", "Met\u00b7rik", "sollt'", "ich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Geist mu\u00dft' weiter gehn.", "tokens": ["Und", "der", "Geist", "mu\u00dft'", "wei\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Doch der Kritiker wollt' bleiben,", "tokens": ["Doch", "der", "Kri\u00b7ti\u00b7ker", "wollt'", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich mu\u00dfte schreiben nun,", "tokens": ["Und", "ich", "mu\u00df\u00b7te", "schrei\u00b7ben", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, womit ich nicht kann treiben", "tokens": ["Ach", ",", "wo\u00b7mit", "ich", "nicht", "kann", "trei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Backofen einen Hund!", "tokens": ["Aus", "dem", "Back\u00b7o\u00b7fen", "ei\u00b7nen", "Hund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}