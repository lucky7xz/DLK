{"textgrid.poem.55371": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Homer wieder Homer", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scharfsinnig habt ihr, wie ihr seid,", "tokens": ["Scharf\u00b7sin\u00b7nig", "habt", "ihr", ",", "wie", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von aller Verehrung uns befreit,", "tokens": ["Von", "al\u00b7ler", "Ver\u00b7eh\u00b7rung", "uns", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wir bekannten \u00fcberfrei,", "tokens": ["Und", "wir", "be\u00b7kann\u00b7ten", "\u00fc\u00b7berf\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Ilias nur ein Flickwerk sei.", "tokens": ["Da\u00df", "I\u00b7lias", "nur", "ein", "Flick\u00b7werk", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6g unser Abfall niemand kr\u00e4nken;", "tokens": ["M\u00f6g", "un\u00b7ser", "Ab\u00b7fall", "nie\u00b7mand", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Jugend wei\u00df uns zu entz\u00fcnden,", "tokens": ["Denn", "Ju\u00b7gend", "wei\u00df", "uns", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wir ", "tokens": ["Da\u00df", "wir"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Als Ganzes freudig ", "tokens": ["Als", "Gan\u00b7zes", "freu\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}}}}}