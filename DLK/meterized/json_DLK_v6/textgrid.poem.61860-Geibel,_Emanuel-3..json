{"textgrid.poem.61860": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Berge wandelt", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die warme Fr\u00fchlingsnacht,", "tokens": ["Die", "war\u00b7me", "Fr\u00fch\u00b7lings\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wogen die wilden Wasser,", "tokens": ["Da", "wo\u00b7gen", "die", "wil\u00b7den", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Eis der Gletscher kracht.", "tokens": ["Das", "Eis", "der", "Glet\u00b7scher", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So wogt mein Herz, so schwillt mein Herz,", "tokens": ["So", "wogt", "mein", "Herz", ",", "so", "schwillt", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe dein gedacht;", "tokens": ["Ich", "ha\u00b7be", "dein", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00dcber die Berge wandelt", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die warme Fr\u00fchlingsnacht.", "tokens": ["Die", "war\u00b7me", "Fr\u00fch\u00b7lings\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}