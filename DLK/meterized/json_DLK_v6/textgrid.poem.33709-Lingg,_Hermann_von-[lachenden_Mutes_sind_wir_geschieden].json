{"textgrid.poem.33709": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "[lachenden Mutes sind wir geschieden]", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lachenden Mutes sind wir geschieden,", "tokens": ["La\u00b7chen\u00b7den", "Mu\u00b7tes", "sind", "wir", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ahnten nicht, da\u00df es f\u00fcr immer war.", "tokens": ["Ahn\u00b7ten", "nicht", ",", "da\u00df", "es", "f\u00fcr", "im\u00b7mer", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werd' ich dich nie mehr sehen hienieden?", "tokens": ["Werd'", "ich", "dich", "nie", "mehr", "se\u00b7hen", "hien\u00b7ie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Seltsam ist es und wunderbar!", "tokens": ["Selt\u00b7sam", "ist", "es", "und", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Scherzend den letzten Ku\u00df dir vom Munde", "tokens": ["Scher\u00b7zend", "den", "letz\u00b7ten", "Ku\u00df", "dir", "vom", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "K\u00fc\u00dft' ich in lachender, gl\u00fccklicher Stunde.", "tokens": ["K\u00fc\u00dft'", "ich", "in", "la\u00b7chen\u00b7der", ",", "gl\u00fcck\u00b7li\u00b7cher", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort wo die Wolken so leuchtend scheinen,", "tokens": ["Dort", "wo", "die", "Wol\u00b7ken", "so", "leuch\u00b7tend", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dort auf seligem Inselland", "tokens": ["Dort", "auf", "se\u00b7li\u00b7gem", "In\u00b7sel\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Wandeln wir einst in Myrtenhainen,", "tokens": ["Wan\u00b7deln", "wir", "einst", "in", "Myr\u00b7ten\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Liebende Schatten Hand in Hand.", "tokens": ["Lie\u00b7ben\u00b7de", "Schat\u00b7ten", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da\u00df man auf ewig sich trennen m\u00fcsse,", "tokens": ["Da\u00df", "man", "auf", "e\u00b7wig", "sich", "tren\u00b7nen", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJD", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nimmermehr glauben das z\u00e4rtliche K\u00fcsse.", "tokens": ["Nim\u00b7mer\u00b7mehr", "glau\u00b7ben", "das", "z\u00e4rt\u00b7li\u00b7che", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}