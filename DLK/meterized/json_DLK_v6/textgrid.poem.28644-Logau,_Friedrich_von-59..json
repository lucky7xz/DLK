{"textgrid.poem.28644": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Reichthum ist die Frau; die Tugend ist die Magd;", "tokens": ["Das", "Reicht\u00b7hum", "ist", "die", "Frau", ";", "die", "Tu\u00b7gend", "ist", "die", "Magd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mit der Magd, der triffts, es f\u00fcr die Frau gewagt.", "tokens": ["Der", "mit", "der", "Magd", ",", "der", "triffts", ",", "es", "f\u00fcr", "die", "Frau", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}