{"textgrid.poem.24883": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mittagsruhe bei Magnesia", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da lagern um des Brunnens k\u00fchle Flut", "tokens": ["Da", "la\u00b7gern", "um", "des", "Brun\u00b7nens", "k\u00fch\u00b7le", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wegem\u00fcden Karawanen;", "tokens": ["Die", "we\u00b7ge\u00b7m\u00fc\u00b7den", "Ka\u00b7ra\u00b7wa\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sanft \u00fcber ihnen bricht die Sonnenglut", "tokens": ["Sanft", "\u00fc\u00b7ber", "ih\u00b7nen", "bricht", "die", "Son\u00b7nen\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Schatten sich im Laube der Platanen,", "tokens": ["Zum", "Schat\u00b7ten", "sich", "im", "Lau\u00b7be", "der", "Pla\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und rings, entb\u00fcrdet von der Waren Last,", "tokens": ["Und", "rings", ",", "ent\u00b7b\u00fcr\u00b7det", "von", "der", "Wa\u00b7ren", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Genie\u00dft Kamel und Ro\u00df der Mittagsrast.", "tokens": ["Ge\u00b7nie\u00dft", "Ka\u00b7mel", "und", "Ro\u00df", "der", "Mit\u00b7tags\u00b7rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umher der turbanh\u00e4upt'gen Wandrer viel,", "tokens": ["Um\u00b7her", "der", "tur\u00b7ban\u00b7h\u00e4upt'\u00b7gen", "Wand\u00b7rer", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Rauch aus Wasserpfeifen blasen;", "tokens": ["Die", "Rauch", "aus", "Was\u00b7ser\u00b7pfei\u00b7fen", "bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom fernen Tigris der und der vom Nil,", "tokens": ["Vom", "fer\u00b7nen", "Tig\u00b7ris", "der", "und", "der", "vom", "Nil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "ART", "KON", "ART", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der aus des Sudan innersten Oasen;", "tokens": ["Der", "aus", "des", "Su\u00b7dan", "in\u00b7ners\u00b7ten", "O\u00b7a\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kurz nur ihr Rasten; wenn sie wieder gehn,", "tokens": ["Kurz", "nur", "ihr", "Ras\u00b7ten", ";", "wenn", "sie", "wie\u00b7der", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$.", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wird ihre Spur der W\u00fcstenwind verwehn.", "tokens": ["Wird", "ih\u00b7re", "Spur", "der", "W\u00fcs\u00b7ten\u00b7wind", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und du, den wilder Drang von Land zu Land", "tokens": ["Und", "du", ",", "den", "wil\u00b7der", "Drang", "von", "Land", "zu", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hinjagt mit ruhelosem Schritte,", "tokens": ["Hin\u00b7jagt", "mit", "ru\u00b7he\u00b7lo\u00b7sem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einsam, verlassen hier und unbekannt", "tokens": ["Ein\u00b7sam", ",", "ver\u00b7las\u00b7sen", "hier", "und", "un\u00b7be\u00b7kannt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "ADV", "KON", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In all der fremden M\u00e4nner Mitte!", "tokens": ["In", "all", "der", "frem\u00b7den", "M\u00e4n\u00b7ner", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht einer ahnt den Trieb, der, nie gestillt,", "tokens": ["Nicht", "ei\u00b7ner", "ahnt", "den", "Trieb", ",", "der", ",", "nie", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich immer neu geb\u00e4rend, dich erf\u00fcllt.", "tokens": ["Sich", "im\u00b7mer", "neu", "ge\u00b7b\u00e4\u00b7rend", ",", "dich", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVPP", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O diese Welt so gro\u00df, du selbst so klein,", "tokens": ["O", "die\u00b7se", "Welt", "so", "gro\u00df", ",", "du", "selbst", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADV", "ADJD", "$,", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und doch dein W\u00fcnschen, Ringen, Streben", "tokens": ["Und", "doch", "dein", "W\u00fcn\u00b7schen", ",", "Rin\u00b7gen", ",", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch unerme\u00dflicher als sie! Halt ein!", "tokens": ["Noch", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7cher", "als", "sie", "!", "Halt", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Zu eng daf\u00fcr sind Zeit und Raum und Leben!", "tokens": ["Zu", "eng", "da\u00b7f\u00fcr", "sind", "Zeit", "und", "Raum", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PAV", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir alle, die wir kommen, die wir gehn,", "tokens": ["Wir", "al\u00b7le", ",", "die", "wir", "kom\u00b7men", ",", "die", "wir", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie bald wird unsern Staub der Wind verwehn!", "tokens": ["Wie", "bald", "wird", "un\u00b7sern", "Staub", "der", "Wind", "ver\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}