{"dta.poem.14600": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck auf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ich gelang' einst zu dem stillen Gipfel,", "tokens": ["Auch", "ich", "ge\u00b7lang'", "einst", "zu", "dem", "stil\u00b7len", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor dem mich schaudert in geheimer Lust.", "tokens": ["Vor", "dem", "mich", "schau\u00b7dert", "in", "ge\u00b7hei\u00b7mer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tief unten rauschen da des Lebens Wipfel", "tokens": ["Tief", "un\u00b7ten", "rau\u00b7schen", "da", "des", "Le\u00b7bens", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal dunkelr\u00fchrend an die Brust,", "tokens": ["Noch", "ein\u00b7mal", "dun\u00b7kel\u00b7r\u00fch\u00b7rend", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird es unten still im weiten Grunde", "tokens": ["Dann", "wird", "es", "un\u00b7ten", "still", "im", "wei\u00b7ten", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und oben leuchtet streng des Himmels Runde.", "tokens": ["Und", "o\u00b7ben", "leuch\u00b7tet", "streng", "des", "Him\u00b7mels", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}