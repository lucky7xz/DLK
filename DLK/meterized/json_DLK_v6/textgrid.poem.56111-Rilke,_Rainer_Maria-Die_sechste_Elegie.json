{"textgrid.poem.56111": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die sechste Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feigenbaum, seit wie lange schon ists mir bedeutend,", "tokens": ["Fei\u00b7gen\u00b7baum", ",", "seit", "wie", "lan\u00b7ge", "schon", "ists", "mir", "be\u00b7deu\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "KOKOM", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "wie du die Bl\u00fcte beinah ganz \u00fcberschl\u00e4gst", "tokens": ["wie", "du", "die", "Bl\u00fc\u00b7te", "bei\u00b7nah", "ganz", "\u00fc\u00b7bersc\u00b7hl\u00e4gst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und hinein in die zeitig entschlossene Frucht,", "tokens": ["und", "hin\u00b7ein", "in", "die", "zei\u00b7tig", "ent\u00b7schlos\u00b7se\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "unger\u00fchmt, dr\u00e4ngst dein reines Geheimnis.", "tokens": ["un\u00b7ge\u00b7r\u00fchmt", ",", "dr\u00e4ngst", "dein", "rei\u00b7nes", "Ge\u00b7heim\u00b7nis", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wie der Font\u00e4ne Rohr treibt dein gebognes Gezweig", "tokens": ["Wie", "der", "Fon\u00b7t\u00e4\u00b7ne", "Rohr", "treibt", "dein", "ge\u00b7bog\u00b7nes", "Ge\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "abw\u00e4rts den Saft und hinan: und er springt aus dem Schlaf,", "tokens": ["ab\u00b7w\u00e4rts", "den", "Saft", "und", "hi\u00b7nan", ":", "und", "er", "springt", "aus", "dem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "fast nicht erwachend, ins Gl\u00fcck seiner s\u00fc\u00dfesten Leistung.", "tokens": ["fast", "nicht", "er\u00b7wa\u00b7chend", ",", "ins", "Gl\u00fcck", "sei\u00b7ner", "s\u00fc\u00b7\u00dfes\u00b7ten", "Leis\u00b7tung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Sieh: wie der Gott in den Schwan.", "tokens": ["Sieh", ":", "wie", "der", "Gott", "in", "den", "Schwan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "...... Wir aber verweilen,", "tokens": ["......", "Wir", "a\u00b7ber", "ver\u00b7wei\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "ach, uns r\u00fchmt es zu bl\u00fchn, und ins versp\u00e4tete Innre", "tokens": ["ach", ",", "uns", "r\u00fchmt", "es", "zu", "bl\u00fchn", ",", "und", "ins", "ver\u00b7sp\u00e4\u00b7te\u00b7te", "Inn\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["XY", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "unserer endlichen Frucht gehn wir verraten hinein.", "tokens": ["un\u00b7se\u00b7rer", "end\u00b7li\u00b7chen", "Frucht", "gehn", "wir", "ver\u00b7ra\u00b7ten", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Wenigen steigt so stark der Andrang des Handelns,", "tokens": ["We\u00b7ni\u00b7gen", "steigt", "so", "stark", "der", "An\u00b7drang", "des", "Han\u00b7delns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da\u00df sie schon anstehn und gl\u00fchn in der F\u00fclle des Herzens,", "tokens": ["da\u00df", "sie", "schon", "an\u00b7stehn", "und", "gl\u00fchn", "in", "der", "F\u00fcl\u00b7le", "des", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "wenn die Verf\u00fchrung zum Bl\u00fchn wie gelinderte Nachtluft", "tokens": ["wenn", "die", "Ver\u00b7f\u00fch\u00b7rung", "zum", "Bl\u00fchn", "wie", "ge\u00b7lin\u00b7der\u00b7te", "Nacht\u00b7luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "KOKOM", "ADJA", "NN"], "meter": "+--+--+--+--++", "measure": "dactylic.tetra.plus"}, "line.15": {"text": "ihnen die Jugend des Munds, ihnen die Lider ber\u00fchrt:", "tokens": ["ih\u00b7nen", "die", "Ju\u00b7gend", "des", "Munds", ",", "ih\u00b7nen", "die", "Li\u00b7der", "be\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Helden vielleicht und den fr\u00fche Hin\u00fcberbestimmten,", "tokens": ["Hel\u00b7den", "viel\u00b7leicht", "und", "den", "fr\u00fc\u00b7he", "Hin\u00b7\u00fc\u00b7ber\u00b7be\u00b7stimm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.17": {"text": "denen der g\u00e4rtnernde Tod anders die Adern verbiegt.", "tokens": ["de\u00b7nen", "der", "g\u00e4rt\u00b7nern\u00b7de", "Tod", "an\u00b7ders", "die", "A\u00b7dern", "ver\u00b7biegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Diese st\u00fcrzen dahin: dem eigenen L\u00e4cheln", "tokens": ["Die\u00b7se", "st\u00fcr\u00b7zen", "da\u00b7hin", ":", "dem", "ei\u00b7ge\u00b7nen", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PAV", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "sind sie voran, wie das Rossegespann in den milden", "tokens": ["sind", "sie", "vo\u00b7ran", ",", "wie", "das", "Ros\u00b7se\u00b7ge\u00b7spann", "in", "den", "mil\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.20": {"text": "muldigen Bildern von Karnak dem siegenden K\u00f6nig.", "tokens": ["mul\u00b7di\u00b7gen", "Bil\u00b7dern", "von", "Kar\u00b7nak", "dem", "sie\u00b7gen\u00b7den", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wunderlich nah ist der Held doch den jugendlich Toten. Dauern", "tokens": ["Wun\u00b7der\u00b7lich", "nah", "ist", "der", "Held", "doch", "den", "ju\u00b7gend\u00b7lich", "To\u00b7ten", ".", "Dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$.", "NN"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "ficht ihn nicht an. Sein Aufgang ist Dasein; best\u00e4ndig", "tokens": ["ficht", "ihn", "nicht", "an", ".", "Sein", "Auf\u00b7gang", "ist", "Da\u00b7sein", ";", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "NN", "$.", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nimmt er sich fort und tritt ins ver\u00e4nderte Sternbild", "tokens": ["nimmt", "er", "sich", "fort", "und", "tritt", "ins", "ver\u00b7\u00e4n\u00b7der\u00b7te", "Stern\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "seiner steten Gefahr. Dort f\u00e4nden ihn wenige. Aber,", "tokens": ["sei\u00b7ner", "ste\u00b7ten", "Ge\u00b7fahr", ".", "Dort", "f\u00e4n\u00b7den", "ihn", "we\u00b7ni\u00b7ge", ".", "A\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PIS", "$.", "KON", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "das uns finster verschweigt, das pl\u00f6tzlich begeisterte Schicksal", "tokens": ["das", "uns", "fins\u00b7ter", "ver\u00b7schweigt", ",", "das", "pl\u00f6tz\u00b7lich", "be\u00b7geis\u00b7ter\u00b7te", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "singt ihn hinein in den Sturm seiner aufrauschenden Welt.", "tokens": ["singt", "ihn", "hin\u00b7ein", "in", "den", "Sturm", "sei\u00b7ner", "auf\u00b7rau\u00b7schen\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "H\u00f6r ich doch keinen wie ", "tokens": ["H\u00f6r", "ich", "doch", "kei\u00b7nen", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "PIAT", "KOKOM"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "mit der str\u00f6menden Luft sein verdunkelter Ton.", "tokens": ["mit", "der", "str\u00f6\u00b7men\u00b7den", "Luft", "sein", "ver\u00b7dun\u00b7kel\u00b7ter", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Dann, wie verb\u00e4rg ich mich gern vor der Sehnsucht: O w\u00e4r ich,", "tokens": ["Dann", ",", "wie", "ver\u00b7b\u00e4rg", "ich", "mich", "gern", "vor", "der", "Sehn\u00b7sucht", ":", "O", "w\u00e4r", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$.", "NE", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "w\u00e4r ich ein Knabe und d\u00fcrft es noch werden und s\u00e4\u00dfe", "tokens": ["w\u00e4r", "ich", "ein", "Kna\u00b7be", "und", "d\u00fcrft", "es", "noch", "wer\u00b7den", "und", "s\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "VMFIN", "PPER", "ADV", "VAINF", "KON", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "in die k\u00fcnftigen Arme gest\u00fctzt und l\u00e4se von Simson,", "tokens": ["in", "die", "k\u00fcnf\u00b7ti\u00b7gen", "Ar\u00b7me", "ge\u00b7st\u00fctzt", "und", "l\u00e4\u00b7se", "von", "Sim\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "wie seine Mutter erst nichts und dann alles gebar.", "tokens": ["wie", "sei\u00b7ne", "Mut\u00b7ter", "erst", "nichts", "und", "dann", "al\u00b7les", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PIS", "KON", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "War er nicht Held schon in dir, o Mutter, begann nicht", "tokens": ["War", "er", "nicht", "Held", "schon", "in", "dir", ",", "o", "Mut\u00b7ter", ",", "be\u00b7gann", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "dort schon, in dir, seine herrische Auswahl?", "tokens": ["dort", "schon", ",", "in", "dir", ",", "sei\u00b7ne", "her\u00b7ri\u00b7sche", "Aus\u00b7wahl", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Tausende brauten im Schoo\u00df und wollten ", "tokens": ["Tau\u00b7sen\u00b7de", "brau\u00b7ten", "im", "Schoo\u00df", "und", "woll\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "VMFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "aber sieh: er ergriff und lie\u00df aus \u2013, w\u00e4hlte und konnte.", "tokens": ["a\u00b7ber", "sieh", ":", "er", "er\u00b7griff", "und", "lie\u00df", "aus", "\u2013", ",", "w\u00e4hl\u00b7te", "und", "konn\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$(", "$,", "VVFIN", "KON", "VMFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Und wenn er S\u00e4ulen zerstie\u00df, so wars, da er ausbrach", "tokens": ["Und", "wenn", "er", "S\u00e4u\u00b7len", "zer\u00b7stie\u00df", ",", "so", "wars", ",", "da", "er", "aus\u00b7brach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,", "ADV", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "aus der Welt deines Leibs in die engere Welt, wo er weiter", "tokens": ["aus", "der", "Welt", "dei\u00b7nes", "Leibs", "in", "die", "en\u00b7ge\u00b7re", "Welt", ",", "wo", "er", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "w\u00e4hlte und konnte. O M\u00fctter der Helden, o Ursprung", "tokens": ["w\u00e4hl\u00b7te", "und", "konn\u00b7te", ".", "O", "M\u00fct\u00b7ter", "der", "Hel\u00b7den", ",", "o", "Ur\u00b7sprung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VMFIN", "$.", "NE", "NN", "ART", "NN", "$,", "FM", "NN"], "meter": "+--+--+--+-++-", "measure": "dactylic.tri.plus"}, "line.8": {"text": "rei\u00dfender Str\u00f6me! Ihr Schluchten, in die sich", "tokens": ["rei\u00b7\u00dfen\u00b7der", "Str\u00f6\u00b7me", "!", "Ihr", "Schluch\u00b7ten", ",", "in", "die", "sich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PRF"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "hoch von dem Herzrand, klagend,", "tokens": ["hoch", "von", "dem", "Herz\u00b7rand", ",", "kla\u00b7gend", 