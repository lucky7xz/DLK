{"textgrid.poem.42580": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4. Gespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund immer nur vom alten Recht?", "tokens": ["\u00bb", "und", "im\u00b7mer", "nur", "vom", "al\u00b7ten", "Recht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du so st\u00f6rrig bist!\u00ab", "tokens": ["Wie", "du", "so", "st\u00f6r\u00b7rig", "bist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin des ", "tokens": ["Ich", "bin", "des"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Weil es ein ", "tokens": ["Weil", "es", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbdas Be\u00dfre, nicht das ", "tokens": ["\u00bb", "das", "Be\u00df\u00b7re", ",", "nicht", "das"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PTKNEG", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zu r\u00fchmen, sei dir Pflicht!\u00ab", "tokens": ["Zu", "r\u00fch\u00b7men", ",", "sei", "dir", "Pflicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Guten hab ich sichre Spur,", "tokens": ["Vom", "Gu\u00b7ten", "hab", "ich", "sich\u00b7re", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Be\u00dfren leider! nicht.", "tokens": ["Vom", "Be\u00df\u00b7ren", "lei\u00b7der", "!", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbwenn ich dir's aber weisen kann,", "tokens": ["\u00bb", "wenn", "ich", "dir's", "a\u00b7ber", "wei\u00b7sen", "kann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So merk und trau auf mich!\u00ab", "tokens": ["So", "merk", "und", "trau", "auf", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "KON", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schw\u00f6r auf keinen einzeln Mann,", "tokens": ["Ich", "schw\u00f6r", "auf", "kei\u00b7nen", "ein\u00b7zeln", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "\u00bbist weiser Rat dir kein Gewinn,", "tokens": ["\u00bb", "ist", "wei\u00b7ser", "Rat", "dir", "kein", "Ge\u00b7winn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo z\u00fcndest du dein Licht?\u00ab", "tokens": ["Wo", "z\u00fcn\u00b7dest", "du", "dein", "Licht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich halt es mit dem schlichten Sinn,", "tokens": ["Ich", "halt", "es", "mit", "dem", "schlich\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der aus dem Volke spricht.", "tokens": ["Der", "aus", "dem", "Vol\u00b7ke", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbich sehe, da\u00df du wenig wei\u00dft", "tokens": ["\u00bb", "ich", "se\u00b7he", ",", "da\u00df", "du", "we\u00b7nig", "wei\u00dft"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Schwung und Sch\u00f6pferkraft.\u00ab", "tokens": ["Von", "Schwung", "und", "Sch\u00f6p\u00b7fer\u00b7kraft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lobe mir den stillen Geist,", "tokens": ["Ich", "lo\u00b7be", "mir", "den", "stil\u00b7len", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der m\u00e4hlich wirkt und schafft.", "tokens": ["Der", "m\u00e4h\u00b7lich", "wirkt", "und", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbder echte Geist schwingt sich empor", "tokens": ["\u00bb", "der", "ech\u00b7te", "Geist", "schwingt", "sich", "em\u00b7por"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rafft die Zeit sich nach.\u00ab", "tokens": ["Und", "rafft", "die", "Zeit", "sich", "nach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was nicht von innen keimt hervor,", "tokens": ["Was", "nicht", "von", "in\u00b7nen", "keimt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist in der Wurzel schwach.", "tokens": ["Ist", "in", "der", "Wur\u00b7zel", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdu hast das Ganze nicht erfa\u00dft,", "tokens": ["\u00bb", "du", "hast", "das", "Gan\u00b7ze", "nicht", "er\u00b7fa\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du meinst es l\u00f6blich, doch du hast", "tokens": ["Du", "meinst", "es", "l\u00f6b\u00b7lich", ",", "doch", "du", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}