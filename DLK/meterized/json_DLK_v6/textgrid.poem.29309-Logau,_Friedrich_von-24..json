{"textgrid.poem.29309": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott sey Danck f\u00fcr mein Geh\u00f6re!", "tokens": ["Gott", "sey", "Danck", "f\u00fcr", "mein", "Ge\u00b7h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der verleih, da\u00df seine Lehre", "tokens": ["Der", "ver\u00b7leih", ",", "da\u00df", "sei\u00b7ne", "Leh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr, als was die Welt mir singt,", "tokens": ["Mehr", ",", "als", "was", "die", "Welt", "mir", "singt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets in meinen Ohren klingt!", "tokens": ["Stets", "in", "mei\u00b7nen", "Oh\u00b7ren", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}