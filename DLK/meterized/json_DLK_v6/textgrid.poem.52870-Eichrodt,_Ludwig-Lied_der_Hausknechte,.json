{"textgrid.poem.52870": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Hausknechte,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind die Hausknecht', wir,", "tokens": ["Wir", "sind", "die", "Haus\u00b7knecht'", ",", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir k\u00f6nnen nichts daf\u00fcr,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nichts", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen z'recht;", "tokens": ["Wir", "kom\u00b7men", "z'\u00b7recht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Leben in Saus und Braus,", "tokens": ["Le\u00b7ben", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wer nicht geh\u00f6rt in's Haus,", "tokens": ["Wer", "nicht", "ge\u00b7h\u00f6rt", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den schmei\u00dfen ", "tokens": ["Den", "schmei\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wir, die Hausknecht'!", "tokens": ["Wir", ",", "die", "Haus\u00b7knecht'", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Fuhrleut sind unsre Freund,", "tokens": ["Fuhr\u00b7leut", "sind", "uns\u00b7re", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn die Laterne scheint,", "tokens": ["Wenn", "die", "La\u00b7ter\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durch d' Mittern\u00e4cht';", "tokens": ["Durch", "d'", "Mit\u00b7ter\u00b7n\u00e4cht'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Messer besitzen wir,", "tokens": ["Mes\u00b7ser", "be\u00b7sit\u00b7zen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sattelg\u00e4ul sitzen wir,", "tokens": ["Sat\u00b7tel\u00b7g\u00e4ul", "sit\u00b7zen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Uhren stibitzen wir,", "tokens": ["Uh\u00b7ren", "sti\u00b7bit\u00b7zen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Alle Hausknecht'!", "tokens": ["Al\u00b7le", "Haus\u00b7knecht'", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Lange Weil' haben wir,", "tokens": ["Lan\u00b7ge", "Weil'", "ha\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Alle Weil' zahlen wir,", "tokens": ["Al\u00b7le", "Weil'", "zah\u00b7len", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was wir verzecht:", "tokens": ["Was", "wir", "ver\u00b7zecht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ulmerk\u00f6pf rauchen wir,", "tokens": ["Ul\u00b7mer\u00b7k\u00f6pf", "rau\u00b7chen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Haber verkaufen wir,", "tokens": ["Ha\u00b7ber", "ver\u00b7kau\u00b7fen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Handg'lenk verstauchen wir,", "tokens": ["Hand\u00b7g'\u00b7lenk", "ver\u00b7stau\u00b7chen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hui Hau Hausknecht'.", "tokens": ["Hui", "Hau", "Haus\u00b7knecht'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Lieblich sind wir und z\u00e4h", "tokens": ["Lieb\u00b7lich", "sind", "wir", "und", "z\u00e4h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gegen das andere", "tokens": ["Ge\u00b7gen", "das", "an\u00b7de\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dienstbotgeschlecht:", "tokens": ["Dienst\u00b7bot\u00b7ge\u00b7schlecht", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Machen ihm freundlich Platz,", "tokens": ["Ma\u00b7chen", "ihm", "freund\u00b7lich", "Platz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schnell sind wir wie der Spatz,", "tokens": ["Schnell", "sind", "wir", "wie", "der", "Spatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und es hat seinen Schatz", "tokens": ["Und", "es", "hat", "sei\u00b7nen", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Jeder Hausknecht!", "tokens": ["Je\u00b7der", "Haus\u00b7knecht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}