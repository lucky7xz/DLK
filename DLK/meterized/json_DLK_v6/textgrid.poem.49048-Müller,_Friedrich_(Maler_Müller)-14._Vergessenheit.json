{"textgrid.poem.49048": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "14. Vergessenheit", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Muse, deren anspruchlosen T\u00f6nen,", "tokens": ["Die", "Mu\u00b7se", ",", "de\u00b7ren", "an\u00b7spruch\u00b7lo\u00b7sen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deiner Huld geweckt, ein Lied gelang,", "tokens": ["Von", "dei\u00b7ner", "Huld", "ge\u00b7weckt", ",", "ein", "Lied", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sie, der Trennung Stunden zu versch\u00f6nen,", "tokens": ["Als", "sie", ",", "der", "Tren\u00b7nung", "Stun\u00b7den", "zu", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die s\u00fc\u00dfen Freuden der Erinnrung sang,", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den", "der", "E\u00b7rinn\u00b7rung", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sel'ger Kindheit Heiligthum zu kr\u00f6nen,", "tokens": ["Und", "sel'\u00b7ger", "Kind\u00b7heit", "Hei\u00b7lig\u00b7thum", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich k\u00fchnen Flugs durch d\u00fcstre Fernen schwang;", "tokens": ["Sich", "k\u00fch\u00b7nen", "Flugs", "durch", "d\u00fcst\u00b7re", "Fer\u00b7nen", "schwang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie reichet zu des goldnen Festes Feier,", "tokens": ["Sie", "rei\u00b7chet", "zu", "des", "gold\u00b7nen", "Fes\u00b7tes", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir dankbar jetzt die frisch bekr\u00e4nzte Leier.", "tokens": ["Mir", "dank\u00b7bar", "jetzt", "die", "frisch", "be\u00b7kr\u00e4nz\u00b7te", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nicht Erinnrung t\u00f6nen ihre Saiten,", "tokens": ["Doch", "nicht", "E\u00b7rinn\u00b7rung", "t\u00f6\u00b7nen", "ih\u00b7re", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie klagen nicht entschwundner Freuden Glanz;", "tokens": ["Sie", "kla\u00b7gen", "nicht", "ent\u00b7schwund\u00b7ner", "Freu\u00b7den", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es soll ihr Spiel kein Trauerton begleiten,", "tokens": ["Es", "soll", "ihr", "Spiel", "kein", "Trau\u00b7er\u00b7ton", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie locke nicht der stillen Wehmuth Kranz;", "tokens": ["Sie", "lo\u00b7cke", "nicht", "der", "stil\u00b7len", "Weh\u00b7muth", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Bl\u00fcthen leis' den Strom hinab entgleiten,", "tokens": ["Wie", "Bl\u00fc\u00b7then", "leis'", "den", "Strom", "hin\u00b7ab", "ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und heiter flieh'n im leichter Wellen Tanz:", "tokens": ["Und", "hei\u00b7ter", "flieh'n", "im", "leich\u00b7ter", "Wel\u00b7len", "Tanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schwebe, was Vergangenheit geboren,", "tokens": ["So", "schwe\u00b7be", ",", "was", "Ver\u00b7gan\u00b7gen\u00b7heit", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur Lethe hin, im stillen Flug der Horen!", "tokens": ["Zur", "Le\u00b7the", "hin", ",", "im", "stil\u00b7len", "Flug", "der", "Ho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch scheidend wink' aus ihrer Fluthen Spiegel", "tokens": ["Noch", "schei\u00b7dend", "wink'", "aus", "ih\u00b7rer", "Flut\u00b7hen", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Rose, die der Gl\u00fcckliche einst fand,", "tokens": ["Die", "Ro\u00b7se", ",", "die", "der", "Gl\u00fcck\u00b7li\u00b7che", "einst", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jede Freude, die mit raschem Fl\u00fcgel", "tokens": ["Und", "je\u00b7de", "Freu\u00b7de", ",", "die", "mit", "ra\u00b7schem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Fr\u00f6hlichen im Kreis der Freunde schwand;", "tokens": ["Dem", "Fr\u00f6h\u00b7li\u00b7chen", "im", "Kreis", "der", "Freun\u00b7de", "schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch harmlos blicke von der Hoffnung H\u00fcgel,", "tokens": ["Doch", "harm\u00b7los", "bli\u00b7cke", "von", "der", "Hoff\u00b7nung", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Wandrer dann frey in der Zukunft Land,", "tokens": ["Der", "Wand\u00b7rer", "dann", "frey", "in", "der", "Zu\u00b7kunft", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und jeder Schmerz, der wild die Brust umschlungen,", "tokens": ["Und", "je\u00b7der", "Schmerz", ",", "der", "wild", "die", "Brust", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der werde sanft durch Lethe's Macht bezwungen!", "tokens": ["Der", "wer\u00b7de", "sanft", "durch", "Lethe's", "Macht", "be\u00b7zwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Es sinken eitlen Wahnes Forderungen,", "tokens": ["Es", "sin\u00b7ken", "eit\u00b7len", "Wah\u00b7nes", "For\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die T\u00e4uschung, die dem Herzen Fesseln gab,", "tokens": ["Die", "T\u00e4u\u00b7schung", ",", "die", "dem", "Her\u00b7zen", "Fes\u00b7seln", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Opfer, die das Vorurtheil erzwungen,", "tokens": ["Die", "Op\u00b7fer", ",", "die", "das", "Vor\u00b7urt\u00b7heil", "er\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrer Fluthen ewig schweigend Grab!", "tokens": ["In", "ih\u00b7rer", "Flut\u00b7hen", "e\u00b7wig", "schwei\u00b7gend", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur Kr\u00e4nze, von der Liebe-Hand geschlungen,", "tokens": ["Nur", "Kr\u00e4n\u00b7ze", ",", "von", "der", "Lie\u00b7be\u00b7Hand", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gleiten unverletzt den Strom hinab,", "tokens": ["Die", "glei\u00b7ten", "un\u00b7ver\u00b7letzt", "den", "Strom", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bl\u00fchend werden wir sie wieder finden,", "tokens": ["Und", "bl\u00fc\u00b7hend", "wer\u00b7den", "wir", "sie", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn jedes Wahnes Zauberfarben schwinden!", "tokens": ["Wenn", "je\u00b7des", "Wah\u00b7nes", "Zau\u00b7berf\u00b7ar\u00b7ben", "schwin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O du! die in des Lebens D\u00e4rmmerungen,", "tokens": ["O", "du", "!", "die", "in", "des", "Le\u00b7bens", "D\u00e4rm\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu uns aus sch\u00f6n'rer Heimath her gesandt,", "tokens": ["Zu", "uns", "aus", "sch\u00f6n'\u00b7rer", "Hei\u00b7math", "her", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oft in des Schicksals Labyrinth verschlungen,", "tokens": ["Oft", "in", "des", "Schick\u00b7sals", "La\u00b7by\u00b7rinth", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch reinen Sinn den sichern Ausgang fand,", "tokens": ["Durch", "rei\u00b7nen", "Sinn", "den", "si\u00b7chern", "Aus\u00b7gang", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und oft vom Schmerz bek\u00e4mpft, doch nie bezwungen,", "tokens": ["Und", "oft", "vom", "Schmerz", "be\u00b7k\u00e4mpft", ",", "doch", "nie", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVPP", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Vorzeit Bild mit Trauerflor umwand;", "tokens": ["Der", "Vor\u00b7zeit", "Bild", "mit", "Trau\u00b7er\u00b7flor", "um\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O! senk' auch du des Lebens finstre Loose,", "tokens": ["O", "!", "senk'", "auch", "du", "des", "Le\u00b7bens", "finst\u00b7re", "Loo\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf ewig heute in der Fluthen Schoo\u00dfe!", "tokens": ["Auf", "e\u00b7wig", "heu\u00b7te", "in", "der", "Flut\u00b7hen", "Schoo\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo rei\u00dft mich hin des Herzens trunknes W\u00e4hnen?", "tokens": ["Wo", "rei\u00dft", "mich", "hin", "des", "Her\u00b7zens", "trunk\u00b7nes", "W\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Ich sehe dich vom Letheduft umwallt \u2013", "tokens": ["\u2013", "Ich", "se\u00b7he", "dich", "vom", "Le\u00b7the\u00b7duft", "um\u00b7wallt", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch Ein Mahl flieh'n der Vorzeit d\u00fcstre Scenen", "tokens": ["Noch", "Ein", "Mahl", "flieh'n", "der", "Vor\u00b7zeit", "d\u00fcst\u00b7re", "Sce\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vor dir vor\u00fcber in des Traums Gestalt; \u2013", "tokens": ["Vor", "dir", "vor\u00b7\u00fc\u00b7ber", "in", "des", "Traums", "Ge\u00b7stalt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es rauscht \u2013 und alle Schmerzen, alle Thr\u00e4nen", "tokens": ["Es", "rauscht", "\u2013", "und", "al\u00b7le", "Schmer\u00b7zen", ",", "al\u00b7le", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versinken ewig, wie ein Ton verhallt!", "tokens": ["Ver\u00b7sin\u00b7ken", "e\u00b7wig", ",", "wie", "ein", "Ton", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Woge flieht \u2013 und rein und silberhelle", "tokens": ["Die", "Wo\u00b7ge", "flieht", "\u2013", "und", "rein", "und", "sil\u00b7ber\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "ADJD", "KON", "NN"], "me