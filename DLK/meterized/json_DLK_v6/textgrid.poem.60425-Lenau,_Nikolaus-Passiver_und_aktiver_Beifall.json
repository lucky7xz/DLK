{"textgrid.poem.60425": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Passiver und aktiver Beifall", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der scharfe Geist hat euch geschwind durchdrungen,", "tokens": ["Der", "schar\u00b7fe", "Geist", "hat", "euch", "ge\u00b7schwind", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bald empf\u00e4ngt er eure Huldigungen;", "tokens": ["Und", "bald", "emp\u00b7f\u00e4ngt", "er", "eu\u00b7re", "Hul\u00b7di\u00b7gun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den tiefen aber sollt ihr selbst durchdringen,", "tokens": ["Den", "tie\u00b7fen", "a\u00b7ber", "sollt", "ihr", "selbst", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum wird ihm eure Liebe sp\u00e4t gelingen.", "tokens": ["Drum", "wird", "ihm", "eu\u00b7re", "Lie\u00b7be", "sp\u00e4t", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}