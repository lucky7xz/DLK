{"dta.poem.3172": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die hier am lautesten erschollen und erklungen,", "tokens": ["Die", "hier", "am", "lau\u00b7tes\u00b7ten", "er\u00b7schol\u00b7len", "und", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sind die Namen hin? verschollen und verklungen!", "tokens": ["Wo", "sind", "die", "Na\u00b7men", "hin", "?", "ver\u00b7schol\u00b7len", "und", "ver\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo sind, die sich so voll erschlossen und erbl\u00fcht,", "tokens": ["Wo", "sind", ",", "die", "sich", "so", "voll", "er\u00b7schlos\u00b7sen", "und", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Knospen unsres Ruhms? verschlossen und verbl\u00fcht.", "tokens": ["Die", "Knos\u00b7pen", "uns\u00b7res", "Ruhms", "?", "ver\u00b7schlos\u00b7sen", "und", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo, die so freudenhell erglommen und erstrahlt,", "tokens": ["Wo", ",", "die", "so", "freu\u00b7den\u00b7hell", "er\u00b7glom\u00b7men", "und", "er\u00b7strahlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "ADV", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonnen unsrer Lust? Verglommen und verstrahlt.", "tokens": ["Die", "Son\u00b7nen", "uns\u00b7rer", "Lust", "?", "Ver\u00b7glom\u00b7men", "und", "ver\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wohin ist alles das, wor\u00fcber und worbei", "tokens": ["Wo\u00b7hin", "ist", "al\u00b7les", "das", ",", "wo\u00b7r\u00fc\u00b7ber", "und", "wor\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PIS", "PDS", "$,", "PWAV", "KON", "PWAV"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wir waren stolz und froh? vor\u00fcber und vorbei.", "tokens": ["Wir", "wa\u00b7ren", "stolz", "und", "froh", "?", "vor\u00b7\u00fc\u00b7ber", "und", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}