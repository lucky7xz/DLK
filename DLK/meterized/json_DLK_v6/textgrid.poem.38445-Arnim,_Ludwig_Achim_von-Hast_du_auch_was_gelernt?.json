{"textgrid.poem.38445": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hast du auch was gelernt?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wacker M\u00e4gdlein bin ich ja,", "tokens": ["Wa\u00b7cker", "M\u00e4gd\u00b7lein", "bin", "ich", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rothe Str\u00fcmpflein hab ich an,", "tokens": ["Ro\u00b7the", "Str\u00fcm\u00b7pflein", "hab", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann stricken, kann nehen,", "tokens": ["Kann", "stri\u00b7cken", ",", "kann", "ne\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Kann Haspel gut drehen,", "tokens": ["Kann", "Has\u00b7pel", "gut", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kann noch wohl was mehr!", "tokens": ["Kann", "noch", "wohl", "was", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PWS", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}