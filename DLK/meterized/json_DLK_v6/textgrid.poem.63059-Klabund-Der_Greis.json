{"textgrid.poem.63059": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Blicke sind von Tr\u00e4nen schwer,", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "sind", "von", "Tr\u00e4\u00b7nen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Meine F\u00fc\u00dfe tragen mich nicht mehr.", "tokens": ["Mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "tra\u00b7gen", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine H\u00e4nde sind zur Faust geballt,", "tokens": ["Mei\u00b7ne", "H\u00e4n\u00b7de", "sind", "zur", "Faust", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die sich zitternd um den Kn\u00fcppel krallt.", "tokens": ["Die", "sich", "zit\u00b7ternd", "um", "den", "Kn\u00fcp\u00b7pel", "krallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "W\u00e4ren meine Arme nicht so schwach,", "tokens": ["W\u00e4\u00b7ren", "mei\u00b7ne", "Ar\u00b7me", "nicht", "so", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrf ich ihn dem blonden Knaben nach,", "tokens": ["W\u00fcrf", "ich", "ihn", "dem", "blon\u00b7den", "Kna\u00b7ben", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der die Zunge grinsend nach mir bleckt.", "tokens": ["Der", "die", "Zun\u00b7ge", "grin\u00b7send", "nach", "mir", "bleckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich wollte, da\u00df mit mir die ganze Welt verreckt...", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "mit", "mir", "die", "gan\u00b7ze", "Welt", "ver\u00b7reckt", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}