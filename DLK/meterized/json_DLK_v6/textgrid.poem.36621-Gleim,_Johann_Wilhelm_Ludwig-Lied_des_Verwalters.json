{"textgrid.poem.36621": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Verwalters", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' ich da nicht meine Grille?", "tokens": ["H\u00f6r'", "ich", "da", "nicht", "mei\u00b7ne", "Gril\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Schnitter, mir singt sie so fr\u00fch!", "tokens": ["Schnit\u00b7ter", ",", "mir", "singt", "sie", "so", "fr\u00fch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Stille, Schnitter, stille, stille!", "tokens": ["Stil\u00b7le", ",", "Schnit\u00b7ter", ",", "stil\u00b7le", ",", "stil\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so lieblich singet sie!", "tokens": ["Wie", "so", "lieb\u00b7lich", "sin\u00b7get", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich ihre Weisheit lerne,", "tokens": ["Da\u00df", "ich", "ih\u00b7re", "Weis\u00b7heit", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum singt sie mir so fr\u00fch,", "tokens": ["Da\u00b7rum", "singt", "sie", "mir", "so", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich h\u00f6re sie so gerne,", "tokens": ["Und", "ich", "h\u00f6\u00b7re", "sie", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum singt sie mir so fr\u00fch.", "tokens": ["Da\u00b7rum", "singt", "sie", "mir", "so", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihre Weisheit ist: Zufrieden", "tokens": ["Ih\u00b7re", "Weis\u00b7heit", "ist", ":", "Zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Gabe Gottes seyn!", "tokens": ["Mit", "der", "Ga\u00b7be", "Got\u00b7tes", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, ist Wenig uns beschieden,", "tokens": ["Und", ",", "ist", "We\u00b7nig", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey dem Wenigen uns freun!", "tokens": ["Bey", "dem", "We\u00b7ni\u00b7gen", "uns", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Thau getrunken nur ein wenig", "tokens": ["Thau", "ge\u00b7trun\u00b7ken", "nur", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADV", "ART", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat das Grillchen, und ist doch", "tokens": ["Hat", "das", "Grill\u00b7chen", ",", "und", "ist", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So vergn\u00fcgt, als wie ein K\u00f6nig,", "tokens": ["So", "ver\u00b7gn\u00fcgt", ",", "als", "wie", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und begr\u00fc\u00dft uns, singt uns noch.", "tokens": ["Und", "be\u00b7gr\u00fc\u00dft", "uns", ",", "singt", "uns", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}