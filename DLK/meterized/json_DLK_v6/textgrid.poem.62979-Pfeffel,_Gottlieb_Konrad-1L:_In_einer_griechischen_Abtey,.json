{"textgrid.poem.62979": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer griechischen Abtey,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer griechischen Abtey,", "tokens": ["In", "ei\u00b7ner", "grie\u00b7chi\u00b7schen", "Ab\u00b7tey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Fu\u00df des hohen Tabors, n\u00e4hrte", "tokens": ["Am", "Fu\u00df", "des", "ho\u00b7hen", "Ta\u00b7bors", ",", "n\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Prior einen Papagey,", "tokens": ["Der", "Pri\u00b7or", "ei\u00b7nen", "Pa\u00b7pa\u00b7gey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den er das Ave singen lehrte.", "tokens": ["Den", "er", "das", "A\u00b7ve", "sin\u00b7gen", "lehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er sang die Hymne so geschickt,", "tokens": ["Er", "sang", "die", "Hym\u00b7ne", "so", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihn das fromme Volk entz\u00fcckt", "tokens": ["Da\u00df", "ihn", "das", "from\u00b7me", "Volk", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr als Sanct Rochus Hund verehrte.", "tokens": ["Mehr", "als", "Sanct", "Ro\u00b7chus", "Hund", "ver\u00b7ehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "VVFIN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Prior starb. Die Reislust wacht", "tokens": ["Der", "Pri\u00b7or", "starb", ".", "Die", "Reis\u00b7lust", "wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Virtuosen auf; er kehrte", "tokens": ["Im", "Vir\u00b7tu\u00b7o\u00b7sen", "auf", ";", "er", "kehr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit leisem Flug, bey dunkler Nacht", "tokens": ["Mit", "lei\u00b7sem", "Flug", ",", "bey", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ins alte Vaterland zur\u00fccke.", "tokens": ["Ins", "al\u00b7te", "Va\u00b7ter\u00b7land", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er stellte sich dem Hofe dar.", "tokens": ["Er", "stell\u00b7te", "sich", "dem", "Ho\u00b7fe", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Adler, der zu gutem Gl\u00fccke", "tokens": ["Der", "Ad\u00b7ler", ",", "der", "zu", "gu\u00b7tem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Freund der edeln Tonkunst war,", "tokens": ["Ein", "Freund", "der", "e\u00b7deln", "Ton\u00b7kunst", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Erhob, als er in der Kapelle", "tokens": ["Er\u00b7hob", ",", "als", "er", "in", "der", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Sich h\u00f6ren lie\u00df, ihn auf der Stelle", "tokens": ["Sich", "h\u00f6\u00b7ren", "lie\u00df", ",", "ihn", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "VVFIN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "An des verstorbnen Mufti Platz.", "tokens": ["An", "des", "ver\u00b7storb\u00b7nen", "Muf\u00b7ti", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So hohe W\u00fcrden hatte Matz", "tokens": ["So", "ho\u00b7he", "W\u00fcr\u00b7den", "hat\u00b7te", "Matz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sich auch im Traume nicht versprochen.", "tokens": ["Sich", "auch", "im", "Trau\u00b7me", "nicht", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Doch Ehre bl\u00e4ht, Gewalt macht k\u00fchn;", "tokens": ["Doch", "Eh\u00b7re", "bl\u00e4ht", ",", "Ge\u00b7walt", "macht", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Das neue Haupt des Sanhedrin", "tokens": ["Das", "neu\u00b7e", "Haupt", "des", "San\u00b7hed\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Gebahr gleich in den ersten Wochen", "tokens": ["Ge\u00b7bahr", "gleich", "in", "den", "ers\u00b7ten", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Grille, seine Psalmodie", "tokens": ["Die", "Gril\u00b7le", ",", "sei\u00b7ne", "Psal\u00b7mo\u00b7die"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Bey allen V\u00f6geln einzuf\u00fchren.", "tokens": ["Bey", "al\u00b7len", "V\u00f6\u00b7geln", "ein\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der frohe K\u00f6nig billigt sie;", "tokens": ["Der", "fro\u00b7he", "K\u00f6\u00b7nig", "bil\u00b7ligt", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Waldgesang, die Liturgie", "tokens": ["Der", "Wald\u00b7ge\u00b7sang", ",", "die", "Li\u00b7tur\u00b7gie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Des Herzens konnt ihn nicht mehr r\u00fchren,", "tokens": ["Des", "Her\u00b7zens", "konnt", "ihn", "nicht", "mehr", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "War f\u00fcr sein Ohr Kakophonie:", "tokens": ["War", "f\u00fcr", "sein", "Ohr", "Ka\u00b7ko\u00b7pho\u00b7nie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und zudem ist ja reformieren", "tokens": ["Und", "zu\u00b7dem", "ist", "ja", "re\u00b7for\u00b7mie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Der F\u00fcrsten Steckenpferd. Sogleich", "tokens": ["Der", "F\u00fcrs\u00b7ten", "Ste\u00b7cken\u00b7pferd", ".", "Sog\u00b7leich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Lie\u00df er in seinem ganzen Reich", "tokens": ["Lie\u00df", "er", "in", "sei\u00b7nem", "gan\u00b7zen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Den neuen Canon publizieren.", "tokens": ["Den", "neu\u00b7en", "Ca\u00b7non", "pu\u00b7bli\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Nun sch\u00fctzte zwar der V\u00f6gel Chor", "tokens": ["Nun", "sch\u00fctz\u00b7te", "zwar", "der", "V\u00f6\u00b7gel", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Die hergebrachten Rechte vor;", "tokens": ["Die", "her\u00b7ge\u00b7brach\u00b7ten", "Rech\u00b7te", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Allein da half kein Protestieren.", "tokens": ["Al\u00b7lein", "da", "half", "kein", "Pro\u00b7tes\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Der Mufti drohte mit dem Bann:", "tokens": ["Der", "Muf\u00b7ti", "droh\u00b7te", "mit", "dem", "Bann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Der Sultan sprach vom Strangulieren,", "tokens": ["Der", "Sul\u00b7tan", "sprach", "vom", "Stran\u00b7gu\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und kurz, das neue Lied begann.", "tokens": ["Und", "kurz", ",", "das", "neu\u00b7e", "Lied", "be\u00b7gann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die S\u00e4nger wetzten sich den Schnabel", "tokens": ["Die", "S\u00e4n\u00b7ger", "wetz\u00b7ten", "sich", "den", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und orgelten mit Angst und Pein", "tokens": ["Und", "or\u00b7gel\u00b7ten", "mit", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Das tollste Wirrwarr durch den Hayn,", "tokens": ["Das", "tolls\u00b7te", "Wirr\u00b7warr", "durch", "den", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Das seit der Symphonie zu Babel", "tokens": ["Das", "seit", "der", "Sym\u00b7pho\u00b7nie", "zu", "Ba\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Auf unserm Erdenrund erscholl.", "tokens": ["Auf", "un\u00b7serm", "Er\u00b7den\u00b7rund", "er\u00b7scholl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Den Vorsang f\u00fchrten andachtsvoll", "tokens": ["Den", "Vor\u00b7sang", "f\u00fchr\u00b7ten", "an\u00b7dachts\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der Storch, der welsche Hahn, die Eule,", "tokens": ["Der", "Storch", ",", "der", "wel\u00b7sche", "Hahn", ",", "die", "Eu\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Die Gans, der Kuckuck und der Pfau:", "tokens": ["Die", "Gans", ",", "der", "Ku\u00b7ckuck", "und", "der", "Pfau", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Sie kollerten sich braun und blau,", "tokens": ["Sie", "kol\u00b7ler\u00b7ten", "sich", "braun", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und f\u00fcllten, durch ihr Klaggeheule,", "tokens": ["Und", "f\u00fcll\u00b7ten", ",", "durch", "ihr", "Klag\u00b7ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Das Land auf eine halbe Meile.", "tokens": ["Das", "Land", "auf", "ei\u00b7ne", "hal\u00b7be", "Mei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Ein weiser Rabe, lahm und grau", "tokens": ["Ein", "wei\u00b7ser", "Ra\u00b7be", ",", "lahm", "und", "grau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Vor Alter, sa\u00df bey dem Monarchen", "tokens": ["Vor", "Al\u00b7ter", ",", "sa\u00df", "bey", "dem", "Mon\u00b7ar\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.52": {"text": "Und schwieg. Mit zornigem Gesicht", "tokens": ["Und", "schwieg", ".", "Mit", "zor\u00b7ni\u00b7gem", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Sprach der Depot zum Patriarchen:", "tokens": ["Sprach", "der", "De\u00b7pot", "zum", "Pat\u00b7ri\u00b7ar\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.54": {"text": "Rebelle, warum singst du nicht?", "tokens": ["Re\u00b7bel\u00b7le", ",", "wa\u00b7rum", "singst", "du", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.55": {"text": "Weil dein Gebot mein Herz emp\u00f6ret,", "tokens": ["Weil", "dein", "Ge\u00b7bot", "mein", "Herz", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Versetzt der Alte: glaube mir,", "tokens": ["Ver\u00b7setzt", "der", "Al\u00b7te", ":", "glau\u00b7be", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"tex