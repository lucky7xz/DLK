ER", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "Ihr V\u00f6gel, singet eure Lieder", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "sin\u00b7get", "eu\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "In eurem angebohrnen Ton!", "tokens": ["In", "eu\u00b7rem", "an\u00b7ge\u00b7bohr\u00b7nen", "Ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.71": {"text": "Itzt drangen sie in dichten Kreisen", "tokens": ["Itzt", "dran\u00b7gen", "sie", "in", "dich\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.72": {"text": "Entz\u00fcckt um des Monarchen Thron", "tokens": ["Ent\u00b7z\u00fcckt", "um", "des", "Mon\u00b7ar\u00b7chen", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Und lobten Gott nach tausend Weisen.", "tokens": ["Und", "lob\u00b7ten", "Gott", "nach", "tau\u00b7send", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Der majest\u00e4tische Choral", "tokens": ["Der", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "Cho\u00b7ral"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.75": {"text": "Steigt wallend in die lichten Sph\u00e4ren.", "tokens": ["Steigt", "wal\u00b7lend", "in", "die"