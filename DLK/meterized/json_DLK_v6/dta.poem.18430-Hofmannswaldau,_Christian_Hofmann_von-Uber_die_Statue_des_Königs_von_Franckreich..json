{"dta.poem.18430": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Statue des K\u00f6nigs von  \n Franckreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gedr\u00fccktes Gallien/ di\u00df denckmal zeigt dir an/", "tokens": ["Ge\u00b7dr\u00fcck\u00b7tes", "Gal\u00b7li\u00b7en", "/", "di\u00df", "denck\u00b7mal", "zeigt", "dir", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PDS", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du dich ferner nicht des joches kanst erwehren;", "tokens": ["Da\u00df", "du", "dich", "fer\u00b7ner", "nicht", "des", "jo\u00b7ches", "kanst", "er\u00b7weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn einst dein K\u00f6nig dich nicht weiter dr\u00fccken kan/", "tokens": ["Wenn", "einst", "dein", "K\u00f6\u00b7nig", "dich", "nicht", "wei\u00b7ter", "dr\u00fc\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird er dennoch dich durch diese last beschweren.", "tokens": ["So", "wird", "er", "den\u00b7noch", "dich", "durch", "die\u00b7se", "last", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p