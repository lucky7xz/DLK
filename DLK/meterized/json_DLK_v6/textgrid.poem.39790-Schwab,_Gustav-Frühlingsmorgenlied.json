{"textgrid.poem.39790": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmorgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel erwart' ich gar von euch", "tokens": ["Viel", "er\u00b7wart'", "ich", "gar", "von", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frischen Morgenl\u00fcften,", "tokens": ["Fri\u00b7schen", "Mor\u00b7gen\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Viel von dir, du Waldgestr\u00e4uch", "tokens": ["Viel", "von", "dir", ",", "du", "Wald\u00b7ges\u00b7tr\u00e4uch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Lenzesd\u00fcften.", "tokens": ["Mit", "den", "Len\u00b7zes\u00b7d\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Erstlich sollt ihr meine Brust,", "tokens": ["Erst\u00b7lich", "sollt", "ihr", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die erkrankte, heilen,", "tokens": ["Die", "er\u00b7krank\u00b7te", ",", "hei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sollt ihr neue Lebenslust,", "tokens": ["Sollt", "ihr", "neu\u00b7e", "Le\u00b7bens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Neue Kraft ertheilen.", "tokens": ["Neu\u00b7e", "Kraft", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn dieses ist geschehn,", "tokens": ["Und", "wenn", "die\u00b7ses", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich doch nicht leben,", "tokens": ["M\u00f6cht'", "ich", "doch", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn nicht euer Duft und Wehn", "tokens": ["Wenn", "nicht", "eu\u00b7er", "Duft", "und", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Be\u00dfres wollte geben.", "tokens": ["Be\u00df\u00b7res", "woll\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dichters Brust hat nicht genug", "tokens": ["Dich\u00b7ters", "Brust", "hat", "nicht", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An gesundem Hauchen,", "tokens": ["An", "ge\u00b7sun\u00b7dem", "Hau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will, aus jedem Athemzug", "tokens": ["Will", ",", "aus", "je\u00b7dem", "A\u00b7them\u00b7zug"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ein Lied ihr tauchen.", "tokens": ["Soll", "ein", "Lied", "ihr", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Solche Lieder, Morgenwind!", "tokens": ["Sol\u00b7che", "Lie\u00b7der", ",", "Mor\u00b7gen\u00b7wind", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst du in ihm wecken,", "tokens": ["Sollst", "du", "in", "ihm", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die mit junger Saat geschwind", "tokens": ["Die", "mit", "jun\u00b7ger", "Saat", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Lippen decken.", "tokens": ["Sei\u00b7ne", "Lip\u00b7pen", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und du Duft von Blum' und Laub,", "tokens": ["Und", "du", "Duft", "von", "Blum'", "und", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sollst dich drauf ergie\u00dfen,", "tokens": ["Sollst", "dich", "drauf", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df mit zartem Bl\u00fctenstaub", "tokens": ["Da\u00df", "mit", "zar\u00b7tem", "Bl\u00fc\u00b7ten\u00b7staub"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie befruchtet sprie\u00dfen.", "tokens": ["Sie", "be\u00b7fruch\u00b7tet", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Bis ihm wird die frohe Brust", "tokens": ["Bis", "ihm", "wird", "die", "fro\u00b7he", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz genesen wallen,", "tokens": ["Ganz", "ge\u00b7ne\u00b7sen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Und das reife Lied mit Lust", "tokens": ["Und", "das", "rei\u00b7fe", "Lied", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Lippe fallen.", "tokens": ["Von", "der", "Lip\u00b7pe", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}