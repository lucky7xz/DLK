{"textgrid.poem.36327": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern von Eitelkeit und innerm Trug,", "tokens": ["Fern", "von", "Ei\u00b7tel\u00b7keit", "und", "in\u00b7nerm", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nahe dich mit Andacht jedem Buch,", "tokens": ["Na\u00b7he", "dich", "mit", "An\u00b7dacht", "je\u00b7dem", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo des Herzens stille Wahrheitskraft", "tokens": ["Wo", "des", "Her\u00b7zens", "stil\u00b7le", "Wahr\u00b7heits\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Neu die Welt der Liebe sich erschafft.", "tokens": ["Neu", "die", "Welt", "der", "Lie\u00b7be", "sich", "er\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Betend wie am Altar Gottes Licht,", "tokens": ["Be\u00b7tend", "wie", "am", "Al\u00b7tar", "Got\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPRART", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "So vernimm das heilige Gedicht,", "tokens": ["So", "ver\u00b7nimm", "das", "hei\u00b7li\u00b7ge", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wo des Lebens schmerzlich sch\u00f6nes Spiel", "tokens": ["Wo", "des", "Le\u00b7bens", "schmerz\u00b7lich", "sch\u00f6\u00b7nes", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Dich zur\u00fccksenkt in das ewige Gef\u00fchl.", "tokens": ["Dich", "zu\u00b7r\u00fcc\u00b7ksenkt", "in", "das", "e\u00b7wi\u00b7ge", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Nur der Sehnsucht flie\u00dft der Sch\u00f6nheit Quell,", "tokens": ["Nur", "der", "Sehn\u00b7sucht", "flie\u00dft", "der", "Sch\u00f6n\u00b7heit", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Nur der Demut scheint die Wahrheit hell.", "tokens": ["Nur", "der", "De\u00b7mut", "scheint", "die", "Wahr\u00b7heit", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}