{"textgrid.poem.50935": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bete in der Fr\u00fche", "tokens": ["Ich", "be\u00b7te", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und jeden Abend wieder,", "tokens": ["Und", "je\u00b7den", "A\u00b7bend", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit ich fromm ergl\u00fche,", "tokens": ["Da\u00b7mit", "ich", "fromm", "er\u00b7gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hafisens s\u00fc\u00dfe Lieder.", "tokens": ["Ha\u00b7fi\u00b7sens", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich murmle sie best\u00e4ndig", "tokens": ["Ich", "murm\u00b7le", "sie", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Pharis\u00e4ermunde;", "tokens": ["Im", "Pha\u00b7ri\u00b7s\u00e4er\u00b7mun\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn sie sind nicht lebendig", "tokens": ["Denn", "sie", "sind", "nicht", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf meiner Seelen Grunde.", "tokens": ["Auf", "mei\u00b7ner", "See\u00b7len", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie einst ich meinem Gotte", "tokens": ["Wie", "einst", "ich", "mei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tugend und Treu versprochen", "tokens": ["Tu\u00b7gend", "und", "Treu", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und t\u00e4glich ihm zum Spotte", "tokens": ["Und", "t\u00e4g\u00b7lich", "ihm", "zum", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dennoch mein Wort gebrochen,", "tokens": ["Den\u00b7noch", "mein", "Wort", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So brech ich jetzo wieder", "tokens": ["So", "brech", "ich", "jet\u00b7zo", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Wort, das ich gegeben,", "tokens": ["Mein", "Wort", ",", "das", "ich", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und halle heuchelnd wider", "tokens": ["Und", "hal\u00b7le", "heu\u00b7chelnd", "wi\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hafisens Jubelleben,", "tokens": ["Ha\u00b7fi\u00b7sens", "Ju\u00b7bel\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Indes ich kalt und n\u00fcchtern", "tokens": ["In\u00b7des", "ich", "kalt", "und", "n\u00fcch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gramvoll mich erbittre,", "tokens": ["Und", "gram\u00b7voll", "mich", "er\u00b7bitt\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Indes ich stumm und sch\u00fcchtern", "tokens": ["In\u00b7des", "ich", "stumm", "und", "sch\u00fcch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Herzen zittre!", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "zitt\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich f\u00fchl's, nach allen Seiten", "tokens": ["Ich", "f\u00fchl's", ",", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Heuchelei vom B\u00f6sen;", "tokens": ["Ist", "Heu\u00b7che\u00b7lei", "vom", "B\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum gilt's, das eigne Streiten", "tokens": ["Drum", "gilt's", ",", "das", "eig\u00b7ne", "Strei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Pfaffentum erl\u00f6sen!", "tokens": ["Von", "Pfaf\u00b7fen\u00b7tum", "er\u00b7l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hast Freude du empfangen,", "tokens": ["Hast", "Freu\u00b7de", "du", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So freu dich ohne Prahlen!", "tokens": ["So", "freu", "dich", "oh\u00b7ne", "Prah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und will dich Nacht umfangen,", "tokens": ["Und", "will", "dich", "Nacht", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00e4m nicht dich ihrer Qualen!", "tokens": ["Sch\u00e4m", "nicht", "dich", "ih\u00b7rer", "Qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}