{"textgrid.poem.31200": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Anti-Hiob", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon Heine meinte: die Menge thut's,", "tokens": ["Schon", "Hei\u00b7ne", "mein\u00b7te", ":", "die", "Men\u00b7ge", "thut's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und im Fr\u00fchling bl\u00fchten die Quitten \u2013", "tokens": ["Und", "im", "Fr\u00fch\u00b7ling", "bl\u00fch\u00b7ten", "die", "Quit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der alte Mann aus dem Lande Uz", "tokens": ["Der", "al\u00b7te", "Mann", "aus", "dem", "Lan\u00b7de", "Uz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat nicht umsonst gelitten.", "tokens": ["Hat", "nicht", "um\u00b7sonst", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erst gestern hat man ihn aufgestellt", "tokens": ["Erst", "ge\u00b7stern", "hat", "man", "ihn", "auf\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Als modischen Dalai Lama,", "tokens": ["Als", "mo\u00b7di\u00b7schen", "Da\u00b7lai", "La\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schluchzend liest nun die ganze Welt", "tokens": ["Und", "schluch\u00b7zend", "liest", "nun", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein primitives Drama.", "tokens": ["Sein", "pri\u00b7mi\u00b7ti\u00b7ves", "Dra\u00b7ma", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "In seinem Namen als Schutzpatron", "tokens": ["In", "sei\u00b7nem", "Na\u00b7men", "als", "Schutz\u00b7pat\u00b7ron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KOUS", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seziren sich tragisch die Reimer \u2013", "tokens": ["Se\u00b7zi\u00b7ren", "sich", "tra\u00b7gisch", "die", "Rei\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O du katzengr\u00e4ulicher Buddhaton,", "tokens": ["O", "du", "kat\u00b7zen\u00b7gr\u00e4u\u00b7li\u00b7cher", "Budd\u00b7ha\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Kenne die Pappenheimer!", "tokens": ["Ken\u00b7ne", "die", "Pap\u00b7pen\u00b7hei\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Schlagt todt die Sonne, wenn sie gl\u00fcht,", "tokens": ["Schlagt", "todt", "die", "Son\u00b7ne", ",", "wenn", "sie", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit pessimistischen Kn\u00fcppeln!", "tokens": ["Mit", "pes\u00b7si\u00b7mis\u00b7ti\u00b7schen", "Kn\u00fcp\u00b7peln", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So lange noch eine Rose bl\u00fcht,", "tokens": ["So", "lan\u00b7ge", "noch", "ei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lass ich mir mein Herz nicht verkr\u00fcppeln!", "tokens": ["Lass", "ich", "mir", "mein", "Herz", "nicht", "ver\u00b7kr\u00fcp\u00b7peln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}