{"textgrid.poem.34240": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Urgeheimni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Schmerz entsteht? Nicht anders, mein Freund, als das Leben:", "tokens": ["Wie", "der", "Schmerz", "ent\u00b7steht", "?", "Nicht", "an\u00b7ders", ",", "mein", "Freund", ",", "als", "das", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$.", "PTKNEG", "ADV", "$,", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Thut der Finger dir weh, schied er vom Leibe sich ab,", "tokens": ["Thut", "der", "Fin\u00b7ger", "dir", "weh", ",", "schied", "er", "vom", "Lei\u00b7be", "sich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die S\u00e4fte beginnen, im Gliede gesondert zu kreisen;", "tokens": ["Und", "die", "S\u00e4f\u00b7te", "be\u00b7gin\u00b7nen", ",", "im", "Glie\u00b7de", "ge\u00b7son\u00b7dert", "zu", "krei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "APPRART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Aber so ist auch der Mensch, f\u00fcrcht' ich, ein Schmerz nur in Gott.", "tokens": ["A\u00b7ber", "so", "ist", "auch", "der", "Mensch", ",", "f\u00fcrcht'", "ich", ",", "ein", "Schmerz", "nur", "in", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}