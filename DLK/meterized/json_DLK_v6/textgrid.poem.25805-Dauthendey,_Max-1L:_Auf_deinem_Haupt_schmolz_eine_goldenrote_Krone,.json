{"textgrid.poem.25805": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf deinem Haupt schmolz eine goldenrote Krone,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf deinem Haupt schmolz eine goldenrote Krone,", "tokens": ["Auf", "dei\u00b7nem", "Haupt", "schmolz", "ei\u00b7ne", "gol\u00b7den\u00b7ro\u00b7te", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davon gl\u00fcht nun dein Haar so goldenrot und stolz.", "tokens": ["Da\u00b7von", "gl\u00fcht", "nun", "dein", "Haar", "so", "gol\u00b7den\u00b7rot", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus deinen Augen zieht das stille herbe Lied", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "zieht", "das", "stil\u00b7le", "her\u00b7be", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der tiefen ungeweinten Tr\u00e4nen.", "tokens": ["Der", "tie\u00b7fen", "un\u00b7ge\u00b7wein\u00b7ten", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schliefen denn niemals Sonnenstrahlen auf deinen Lippen?", "tokens": ["Schlie\u00b7fen", "denn", "nie\u00b7mals", "Son\u00b7nen\u00b7strah\u00b7len", "auf", "dei\u00b7nen", "Lip\u00b7pen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Man k\u00f6nnte w\u00e4hnen,", "tokens": ["Man", "k\u00f6nn\u00b7te", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du habest nie dich selbst gesehn,", "tokens": ["Du", "ha\u00b7best", "nie", "dich", "selbst", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So arm bist du.", "tokens": ["So", "arm", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}