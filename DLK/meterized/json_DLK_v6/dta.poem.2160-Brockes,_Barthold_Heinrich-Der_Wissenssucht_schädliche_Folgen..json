{"dta.poem.2160": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wissenssucht sch\u00e4dliche  \n Folgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mehr als zu viel geschmeichelt wird, doch hat die Wahr-", "tokens": ["Mehr", "als", "zu", "viel", "ge\u00b7schmei\u00b7chelt", "wird", ",", "doch", "hat", "die", "Wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "APPR", "PIS", "VVPP", "VAFIN", "$,", "ADV", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "heit ganz und gar", "tokens": ["heit", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bey solchem Zustand sich verloren, ein unaufh\u00f6rlichs", "tokens": ["Bey", "sol\u00b7chem", "Zu\u00b7stand", "sich", "ver\u00b7lo\u00b7ren", ",", "ein", "un\u00b7auf\u00b7h\u00f6r\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVPP", "$,", "ART", "ADJA"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Zanken, Keifen", "tokens": ["Zan\u00b7ken", ",", "Kei\u00b7fen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und Widerspruch sich eingefunden. Ein solch Begreifen", "tokens": ["Und", "Wi\u00b7der\u00b7spruch", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ".", "Ein", "solch", "Be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PRF", "VVPP", "$.", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ist mit Recht", "tokens": ["ist", "mit", "Recht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ein wahrer Rechtgeiz wohl zu nennen,", "tokens": ["Ein", "wah\u00b7rer", "Recht\u00b7geiz", "wohl", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da wir, mit Ausschlu\u00df aller andern, das stets bestrittne", "tokens": ["Da", "wir", ",", "mit", "Aus\u00b7schlu\u00df", "al\u00b7ler", "an\u00b7dern", ",", "das", "stets", "be\u00b7stritt\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "PIAT", "ADJA", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Recht allein", "tokens": ["Recht", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "F\u00fcr uns allein behaupten wollen, und nichts dem Rest", "tokens": ["F\u00fcr", "uns", "al\u00b7lein", "be\u00b7haup\u00b7ten", "wol\u00b7len", ",", "und", "nichts", "dem", "Rest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVINF", "VMINF", "$,", "KON", "PIS", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "der Menschen g\u00f6nnen.", "tokens": ["der", "Men\u00b7schen", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Ein jeder will unbillig klug, und ganz alleine weise seyn.", "tokens": ["Ein", "je\u00b7der", "will", "un\u00b7bil\u00b7lig", "klug", ",", "und", "ganz", "al\u00b7lei\u00b7ne", "wei\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADJD", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Die\u00df w\u00fcrde nimmermehr geschehen, wenn man, von", "tokens": ["Die\u00df", "w\u00fcr\u00b7de", "nim\u00b7mer\u00b7mehr", "ge\u00b7sche\u00b7hen", ",", "wenn", "man", ",", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PIS", "$,", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "seiner Nichtigkeit", "tokens": ["sei\u00b7ner", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "In \u00fcberf\u00fchrender Erkenntni\u00df, auf g\u00f6ttliche Beschaffenheit", "tokens": ["In", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren\u00b7der", "Er\u00b7kennt\u00b7ni\u00df", ",", "auf", "g\u00f6tt\u00b7li\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Jm Anfang unserer Betrachtung mit Ehrfurcht sein Ge-", "tokens": ["Jm", "An\u00b7fang", "un\u00b7se\u00b7rer", "Be\u00b7trach\u00b7tung", "mit", "Ehr\u00b7furcht", "sein", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "sicht gelenket;", "tokens": ["sicht", "ge\u00b7len\u00b7ket", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Indem man denn ohn allen Zweifel in tiefster Ehrer-", "tokens": ["In\u00b7dem", "man", "denn", "ohn", "al\u00b7len", "Zwei\u00b7fel", "in", "tiefs\u00b7ter", "Eh\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "bietigkeit", "tokens": ["bie\u00b7tig\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "In das anbetungsw\u00fcrdig\u2019 All sein ganzes Nichts hinein-", "tokens": ["In", "das", "an\u00b7be\u00b7tungs\u00b7w\u00fcr\u00b7dig'", "All", "sein", "gan\u00b7zes", "Nichts", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "gesenket,", "tokens": ["ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.22": {"text": "Und seine gro\u00dfe Majest\u00e4t in einer andachtvollen Stille", "tokens": ["Und", "sei\u00b7ne", "gro\u00b7\u00dfe", "Ma\u00b7jes\u00b7t\u00e4t", "in", "ei\u00b7ner", "an\u00b7dacht\u00b7vol\u00b7len", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.23": {"text": "Verehrt und angebetet h\u00e4tte; auch w\u00fcrde nimmermehr", "tokens": ["Ver\u00b7ehrt", "und", "an\u00b7ge\u00b7be\u00b7tet", "h\u00e4t\u00b7te", ";", "auch", "w\u00fcr\u00b7de", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "$.", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "der Wille", "tokens": ["der", "Wil\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Mit andern Menschen sich gezanket, so \u00e4rgerlich sich", "tokens": ["Mit", "an\u00b7dern", "Men\u00b7schen", "sich", "ge\u00b7zan\u00b7ket", ",", "so", "\u00e4r\u00b7ger\u00b7lich", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$,", "ADV", "ADJD", "PRF"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "nicht entzweyet,", "tokens": ["nicht", "ent\u00b7zwe\u00b7yet", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Vielmehr dahin vereinet haben, sich mit einander zu be-", "tokens": ["Viel\u00b7mehr", "da\u00b7hin", "ver\u00b7ei\u00b7net", "ha\u00b7ben", ",", "sich", "mit", "ein\u00b7an\u00b7der", "zu", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "VVPP", "VAINF", "$,", "PRF", "APPR", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "streben,", "tokens": ["stre\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Gemeinschaftlich des H\u00f6chsten Wunder bewundernd fr\u00f6lich", "tokens": ["Ge\u00b7mein\u00b7schaft\u00b7lich", "des", "H\u00f6chs\u00b7ten", "Wun\u00b7der", "be\u00b7wun\u00b7dernd", "fr\u00f6\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADJD", "ADJD"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "zu erheben,", "tokens": ["zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Und auch mit unsern Nebenmenschen zugleich in Ei-", "tokens": ["Und", "auch", "mit", "un\u00b7sern", "Ne\u00b7ben\u00b7men\u00b7schen", "zu\u00b7gleich", "in", "Ei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.32": {"text": "nigkeit zu leben", "tokens": ["nig\u00b7keit", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}}}}}