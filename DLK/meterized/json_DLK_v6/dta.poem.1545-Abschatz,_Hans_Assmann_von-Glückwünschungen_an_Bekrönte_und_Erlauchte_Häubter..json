{"dta.poem.1545": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es leb Ertz-Hertzog CARL/ und werde Carlen gleich/", "tokens": ["Es", "leb", "Ertz\u00b7Hert\u00b7zog", "CaRL", "/", "und", "wer\u00b7de", "Car\u00b7len", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$(", "KON", "VAFIN", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Franckreich zittern macht ob unserm Oesterreich/", "tokens": ["Der", "Fran\u00b7ck\u00b7reich", "zit\u00b7tern", "macht", "ob", "un\u00b7serm", "O\u00b7es\u00b7ter\u00b7reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vor dem der stoltze T\u00fcrck aus deutschen Gr\u00e4ntzen weich!", "tokens": ["Vor", "dem", "der", "stolt\u00b7ze", "T\u00fcrck", "aus", "deut\u00b7schen", "Gr\u00e4nt\u00b7zen", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lilje m\u00fc\u00df ihm welck/ der Monde werden bleich!", "tokens": ["Die", "Lil\u00b7je", "m\u00fc\u00df", "ihm", "welck", "/", "der", "Mon\u00b7de", "wer\u00b7den", "bleich", "!"], "token_info": ["word", "w