{"textgrid.poem.46616": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie der Strahl der gl\u00fchende zischt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Strahl der gl\u00fchende zischt", "tokens": ["Wie", "der", "Strahl", "der", "gl\u00fc\u00b7hen\u00b7de", "zischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und des Zimmers Luft erfrischt,", "tokens": ["Und", "des", "Zim\u00b7mers", "Luft", "er\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Besprengt mit saurer Feuchte,", "tokens": ["Be\u00b7sprengt", "mit", "sau\u00b7rer", "Feuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis seine Glut erlischt;", "tokens": ["Bis", "sei\u00b7ne", "Glut", "er\u00b7lischt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnte von dem hei\u00dfen Schmerz", "tokens": ["K\u00f6nn\u00b7te", "von", "dem", "hei\u00b7\u00dfen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So gel\u00f6schet sein mein Herz,", "tokens": ["So", "ge\u00b7l\u00f6\u00b7schet", "sein", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ausgel\u00f6scht! das d\u00e4uchte", "tokens": ["Und", "aus\u00b7ge\u00b7l\u00f6scht", "!", "das", "d\u00e4uch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVPP", "$.", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir k\u00fchl ein Liebesscherz.", "tokens": ["Mir", "k\u00fchl", "ein", "Lie\u00b7bes\u00b7scherz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}