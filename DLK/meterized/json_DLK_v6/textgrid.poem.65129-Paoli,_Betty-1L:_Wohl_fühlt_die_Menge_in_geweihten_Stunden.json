{"textgrid.poem.65129": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl f\u00fchlt die Menge in geweihten Stunden", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl f\u00fchlt die Menge in geweihten Stunden", "tokens": ["Wohl", "f\u00fchlt", "die", "Men\u00b7ge", "in", "ge\u00b7weih\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Macht, womit ein Geist, von Gott gesendet,", "tokens": ["Die", "Macht", ",", "wo\u00b7mit", "ein", "Geist", ",", "von", "Gott", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Ideales lichten Trost ihr spendet,", "tokens": ["Des", "I\u00b7dea\u00b7les", "lich\u00b7ten", "Trost", "ihr", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die Fessel l\u00fcftend, die sie h\u00e4lt gebunden!", "tokens": ["Die", "Fes\u00b7sel", "l\u00fcf\u00b7tend", ",", "die", "sie", "h\u00e4lt", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ob sie tausend Kr\u00e4nze ihm gewunden,", "tokens": ["Doch", "ob", "sie", "tau\u00b7send", "Kr\u00e4n\u00b7ze", "ihm", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "CARD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und staunend ihre Blicke zu ihm wendet,", "tokens": ["Und", "stau\u00b7nend", "ih\u00b7re", "Bli\u00b7cke", "zu", "ihm", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist es doch nur sein ", "tokens": ["Ist", "es", "doch", "nur", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sein tiefstes ", "tokens": ["Sein", "tiefs\u00b7tes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Nur Wen'ge gibt's, die wissen und empfinden,", "tokens": ["Nur", "Wen'\u00b7ge", "gibt's", ",", "die", "wis\u00b7sen", "und", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PRELS", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sich, da\u00df er zu den Erkor'nen z\u00e4hle,", "tokens": ["Was", "sich", ",", "da\u00df", "er", "zu", "den", "Er\u00b7kor'\u00b7nen", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In einem K\u00fcnstler mu\u00df zusammenfinden:", "tokens": ["In", "ei\u00b7nem", "K\u00fcnst\u00b7ler", "mu\u00df", "zu\u00b7sam\u00b7men\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Herz, im Lieben stark und stark im Hassen,", "tokens": ["Ein", "Herz", ",", "im", "Lie\u00b7ben", "stark", "und", "stark", "im", "Has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein k\u00f6niglicher Geist und eine Seele,", "tokens": ["Ein", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Geist", "und", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gro\u00df genug, das Gr\u00f6\u00dfte selbst zu fassen!", "tokens": ["Die", "gro\u00df", "ge\u00b7nug", ",", "das", "Gr\u00f6\u00df\u00b7te", "selbst", "zu", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}