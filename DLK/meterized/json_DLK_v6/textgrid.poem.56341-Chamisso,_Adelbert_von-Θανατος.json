{"textgrid.poem.56341": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "\u0398\u03b1\u03bd\u03b1\u03c4\u03bf\u03c2", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meiner Mutter H\u00fctte, \u2013 la\u00dft mich weinen!", "tokens": ["In", "mei\u00b7ner", "Mut\u00b7ter", "H\u00fct\u00b7te", ",", "\u2013", "la\u00dft", "mich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "$(", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, bringt die alten Tr\u00e4nen mir zur\u00fcck,", "tokens": ["Ja", ",", "bringt", "die", "al\u00b7ten", "Tr\u00e4\u00b7nen", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr alten Bilder, wollt ihr mir erscheinen! \u2013", "tokens": ["Ihr", "al\u00b7ten", "Bil\u00b7der", ",", "wollt", "ihr", "mir", "er\u00b7schei\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meiner Mutter H\u00fctte war das Gl\u00fcck;", "tokens": ["In", "mei\u00b7ner", "Mut\u00b7ter", "H\u00fct\u00b7te", "war", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe schaffte still mit leiser Hand", "tokens": ["Die", "Lie\u00b7be", "schaff\u00b7te", "still", "mit", "lei\u00b7ser", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und leuchtet' \u00fcber uns im Mutterblick.", "tokens": ["Und", "leuch\u00b7tet'", "\u00fc\u00b7ber", "uns", "im", "Mut\u00b7ter\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da hing ein seltsam Bildnis an der Wand,", "tokens": ["Da", "hing", "ein", "selt\u00b7sam", "Bild\u00b7nis", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Davor wir lernten unsre H\u00e4nde falten", "tokens": ["Da\u00b7vor", "wir", "lern\u00b7ten", "uns\u00b7re", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und Worte sprechen, die ich nicht verstand;", "tokens": ["Und", "Wor\u00b7te", "spre\u00b7chen", ",", "die", "ich", "nicht", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und hatten wir am Tag uns fromm verhalten,", "tokens": ["Und", "hat\u00b7ten", "wir", "am", "Tag", "uns", "fromm", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So nahten unsern Wiegen sich die Tr\u00e4ume", "tokens": ["So", "nah\u00b7ten", "un\u00b7sern", "Wie\u00b7gen", "sich", "die", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Als lichter Engel segnende Gestalten.", "tokens": ["Als", "lich\u00b7ter", "En\u00b7gel", "seg\u00b7nen\u00b7de", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Vor unsrer H\u00fctte lagen sonn'ge R\u00e4ume,", "tokens": ["Vor", "uns\u00b7rer", "H\u00fct\u00b7te", "la\u00b7gen", "sonn'\u00b7ge", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Um diese breiteten ein duft'ges Zelt", "tokens": ["Um", "die\u00b7se", "brei\u00b7te\u00b7ten", "ein", "duft'\u00b7ges", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die dichten Reihen hoher Lindenb\u00e4ume.", "tokens": ["Die", "dich\u00b7ten", "Rei\u00b7hen", "ho\u00b7her", "Lin\u00b7den\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Noch war der Umkreis unsre ganze Welt,", "tokens": ["Noch", "war", "der", "Um\u00b7kreis", "uns\u00b7re", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und von dem Bache jenseits l\u00e4ngs dem Hage", "tokens": ["Und", "von", "dem", "Ba\u00b7che", "jen\u00b7seits", "l\u00e4ngs", "dem", "Ha\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die \u00e4u\u00dferste der Grenzen uns gestellt;", "tokens": ["Die", "\u00e4u\u00b7\u00dfers\u00b7te", "der", "Gren\u00b7zen", "uns", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und hier am Ufer stand ich lange Tage,", "tokens": ["Und", "hier", "am", "U\u00b7fer", "stand", "ich", "lan\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Hier zog und hielt mich wie ein b\u00f6ser Traum", "tokens": ["Hier", "zog", "und", "hielt", "mich", "wie", "ein", "b\u00f6\u00b7ser", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mit fieberhaft erh\u00f6htem Herzensschlage,", "tokens": ["Mit", "fie\u00b7ber\u00b7haft", "er\u00b7h\u00f6h\u00b7tem", "Her\u00b7zens\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Zu schaun hin\u00fcber nach dem fernen Saum,", "tokens": ["Zu", "schaun", "hin\u00b7\u00fc\u00b7ber", "nach", "dem", "fer\u00b7nen", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Dem blauen Nebelring, beschr\u00e4nkend dort", "tokens": ["Dem", "blau\u00b7en", "Ne\u00b7bel\u00b7ring", ",", "be\u00b7schr\u00e4n\u00b7kend", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Den gr\u00fcnen, weiten, ausgespannten Raum;", "tokens": ["Den", "gr\u00fc\u00b7nen", ",", "wei\u00b7ten", ",", "aus\u00b7ge\u00b7spann\u00b7ten", "Raum", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Zu sehnen mich hin\u00fcber fort und fort", "tokens": ["Zu", "seh\u00b7nen", "mich", "hin\u00b7\u00fc\u00b7ber", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "In jene r\u00e4tselhafte blaue Weite,", "tokens": ["In", "je\u00b7ne", "r\u00e4t\u00b7sel\u00b7haf\u00b7te", "blau\u00b7e", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Der Schranke z\u00fcrnend, die mich hielt am Ort.", "tokens": ["Der", "Schran\u00b7ke", "z\u00fcr\u00b7nend", ",", "die", "mich", "hielt", "am", "Ort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Da dacht ich: w\u00e4rst du erst auf jener Seite", "tokens": ["Da", "dacht", "ich", ":", "w\u00e4rst", "du", "erst", "auf", "je\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Des Wassers! dieses Wasser aber mu\u00df", "tokens": ["Des", "Was\u00b7sers", "!", "die\u00b7ses", "Was\u00b7ser", "a\u00b7ber", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PDAT", "NN", "ADV", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "So tief nicht sein. Ich war mit mir im Streite.", "tokens": ["So", "tief", "nicht", "sein", ".", "Ich", "war", "mit", "mir", "im", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "VAINF", "$.", "PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Bald reifte der Gedanke zum Entschlu\u00df,", "tokens": ["Bald", "reif\u00b7te", "der", "Ge\u00b7dan\u00b7ke", "zum", "Ent\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Ich stieg hinein, es wuchs mir das Vertrauen,", "tokens": ["Ich", "stieg", "hin\u00b7ein", ",", "es", "wuchs", "mir", "das", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Es trug an jenes Ufer mich mein Fu\u00df.", "tokens": ["Es", "trug", "an", "je\u00b7nes", "U\u00b7fer", "mich", "mein", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und vorw\u00e4rts, ohne hinter mich zu schauen,", "tokens": ["Und", "vor\u00b7w\u00e4rts", ",", "oh\u00b7ne", "hin\u00b7ter", "mich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "In grader Richtung hub ich an zu wallen", "tokens": ["In", "gra\u00b7der", "Rich\u00b7tung", "hub", "ich", "an", "zu", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Dem blauen Streifen zu durch bl\u00fchnde Auen.", "tokens": ["Dem", "blau\u00b7en", "Strei\u00b7fen", "zu", "durch", "bl\u00fchn\u00b7de", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der Mutter Nachruf h\u00f6rt ich wohl erschallen", "tokens": ["Der", "Mut\u00b7ter", "Nach\u00b7ruf", "h\u00f6rt", "ich", "wohl", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und, wie ich unaufhaltsam vorw\u00e4rts schritt,", "tokens": ["Und", ",", "wie", "ich", "un\u00b7auf\u00b7halt\u00b7sam", "vor\u00b7w\u00e4rts", "schritt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "In schauerliche Stille bald verhallen.", "tokens": ["In", "schau\u00b7er\u00b7li\u00b7che", "Stil\u00b7le", "bald", "ver\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Gr\u00fcn ward der Boden rings um meinen Tritt,", "tokens": ["Gr\u00fcn", "ward", "der", "Bo\u00b7den", "rings", "um", "mei\u00b7nen", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.41": {"text": "Da vor mich hin, so wie ich vorw\u00e4rts drang,", "tokens": ["Da", "vor", "mich", "hin", ",", "so", "wie", "ich", "vor\u00b7w\u00e4rts", "drang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der blaue Nebel fern und ferner glitt.", "tokens": ["Der", "blau\u00b7e", "Ne\u00b7bel", "fern", "und", "fer\u00b7ner", "glitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und wie ich so im Zauberkreise rang,", "tokens": ["Und", "wie", "ich", "so", "im", "Zau\u00b7ber\u00b7krei\u00b7se", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Besann ich mich; da war ich m\u00fcd und alt,", "tokens": ["Be\u00b7sann", "ich", "mich", ";", "da", "war", "ich", "m\u00fcd", "und", "alt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Die Heimat hinter mir verschwunden lang.", "tokens": ["Die", "Hei\u00b7mat", "hin\u00b7ter", "mir", "ver\u00b7schwun\u00b7den", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und vorw\u00e4rts, unabl\u00e4ssig vorw\u00e4rts galt", "tokens": ["Und", "vor\u00b7w\u00e4rts", ",", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "vor\u00b7w\u00e4rts", "galt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Es durchzudringen; wie die Hoffnung schwand,", "tokens": ["Es", "durch\u00b7zu\u00b7drin\u00b7gen", ";", "wie", "die", "Hoff\u00b7nung", "schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Da \u00e4nderte der Boden die Gestalt.", "tokens": ["Da", "\u00e4n\u00b7der\u00b7te", "der", "Bo\u00b7den", "die", "Ge\u00b7stalt", "."], 