{"textgrid.poem.38506": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Ossian", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn oft, in Stunden heiliger Mitternacht,", "tokens": ["Wenn", "oft", ",", "in", "Stun\u00b7den", "hei\u00b7li\u00b7ger", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Ohr dem Flugschlag deines Gesanges horcht,", "tokens": ["Mein", "Ohr", "dem", "Flug\u00b7schlag", "dei\u00b7nes", "Ge\u00b7san\u00b7ges", "horcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und der Vorzeit goldne Bilder", "tokens": ["Und", "der", "Vor\u00b7zeit", "gold\u00b7ne", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die begeisterte Seele schweben!", "tokens": ["Um", "die", "be\u00b7geis\u00b7ter\u00b7te", "See\u00b7le", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann rinnt die Thr\u00e4ne! h\u00fcllt doch Vergessenheit", "tokens": ["Dann", "rinnt", "die", "Thr\u00e4\u00b7ne", "!", "h\u00fcllt", "doch", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "ADV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Barden Teutons, ach! schon Jahrhunderte,", "tokens": ["Die", "Bar\u00b7den", "Teu\u00b7tons", ",", "ach", "!", "schon", "Jahr\u00b7hun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ITJ", "$.", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Werth vieleicht mit dir, o Vater!", "tokens": ["Werth", "vie\u00b7leicht", "mit", "dir", ",", "o", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um der Unsterblichkeit Kranz zu ringen!", "tokens": ["Um", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "Kranz", "zu", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "--+---+-+-", "measure": "anapaest.init"}}}}}