{"textgrid.poem.59475": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die R\u00e4uberbr\u00fcder", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvor\u00fcber ist der blut'ge Strau\u00df,", "tokens": ["\u00bb", "vor\u00b7\u00fc\u00b7ber", "ist", "der", "blut'\u00b7ge", "Strau\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ist's so still, nun ruh dich aus.\u00ab", "tokens": ["Hier", "ist's", "so", "still", ",", "nun", "ruh", "dich", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbvom Tal her\u00fcber kommt die Luft;", "tokens": ["\u00bb", "vom", "Tal", "her\u00b7\u00fc\u00b7ber", "kommt", "die", "Luft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Horch, h\u00f6rst du nichts? Die Mutter ruft.\u00ab", "tokens": ["Horch", ",", "h\u00f6rst", "du", "nichts", "?", "Die", "Mut\u00b7ter", "ruft", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PIS", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdie Mutter ist ja lange tot,", "tokens": ["\u00bb", "die", "Mut\u00b7ter", "ist", "ja", "lan\u00b7ge", "tot", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eine Glocke klingt durchs Morgenrot.\u00ab", "tokens": ["Ei\u00b7ne", "Glo\u00b7cke", "klingt", "durchs", "Mor\u00b7gen\u00b7rot", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bblieb Mutter, hab nicht solches Leid,", "tokens": ["\u00bb", "lieb", "Mut\u00b7ter", ",", "hab", "nicht", "sol\u00b7ches", "Leid", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "VAFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein wildes Leben mich gereut. \u2013\u00ab", "tokens": ["Mein", "wil\u00b7des", "Le\u00b7ben", "mich", "ge\u00b7reut", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwas sinkst du auf die Knie ins Gras?", "tokens": ["\u00bb", "was", "sinkst", "du", "auf", "die", "Knie", "ins", "Gras", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deine Augen dunkeln, du wirst so bla\u00df.\u00ab \u2013", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "dun\u00b7keln", ",", "du", "wirst", "so", "bla\u00df", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$(", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Es war von Blut der Grund so rot,", "tokens": ["Es", "war", "von", "Blut", "der", "Grund", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der R\u00e4uber lag im Grase tot.", "tokens": ["Der", "R\u00e4u\u00b7ber", "lag", "im", "Gra\u00b7se", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da k\u00fc\u00dft der Bruder den bleichen Mund:", "tokens": ["Da", "k\u00fc\u00dft", "der", "Bru\u00b7der", "den", "blei\u00b7chen", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdich liebt ich recht aus Herzensgrund.\u00ab", "tokens": ["\u00bb", "dich", "liebt", "ich", "recht", "aus", "Her\u00b7zens\u00b7grund", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Vom Fels dann scho\u00df er noch einmal", "tokens": ["Vom", "Fels", "dann", "scho\u00df", "er", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warf die B\u00fcchse tief ins Tal.", "tokens": ["Und", "warf", "die", "B\u00fcch\u00b7se", "tief", "ins", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drauf schritt er durch den Wald zur Stadt:", "tokens": ["Drauf", "schritt", "er", "durch", "den", "Wald", "zur", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbihr Herrn, ich bin des Lebens satt.", "tokens": ["\u00bb", "ihr", "Herrn", ",", "ich", "bin", "des", "Le\u00b7bens", "satt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hie ist mein Haupt, nun richtet bald,", "tokens": ["Hie", "ist", "mein", "Haupt", ",", "nun", "rich\u00b7tet", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Bruder legt mich in den Wald.\u00ab", "tokens": ["Zum", "Bru\u00b7der", "legt", "mich", "in", "den", "Wald", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}