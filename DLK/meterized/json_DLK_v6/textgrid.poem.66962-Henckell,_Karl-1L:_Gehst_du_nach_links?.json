{"textgrid.poem.66962": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gehst du nach links?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehst du nach links?", "tokens": ["Gehst", "du", "nach", "links", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gehst du nach rechts?", "tokens": ["Gehst", "du", "nach", "rechts", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Fragt mich der Mund des Herrn, des Knechts.", "tokens": ["Fragt", "mich", "der", "Mund", "des", "Herrn", ",", "des", "Knechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie zeigen stolz auf ihren Haufen", "tokens": ["Sie", "zei\u00b7gen", "stolz", "auf", "ih\u00b7ren", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wispern schon von \u00dcberlaufen.", "tokens": ["Und", "wis\u00b7pern", "schon", "von", "\u00dc\u00b7berl\u00b7au\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lasse alle beide stehn,", "tokens": ["Ich", "las\u00b7se", "al\u00b7le", "bei\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um meinen eigenen Weg zu gehn.", "tokens": ["Um", "mei\u00b7nen", "ei\u00b7ge\u00b7nen", "Weg", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}