{"textgrid.poem.24542": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das in dem F\u00fcrstenthum Gotha wundersch\u00f6n gelegene Georgen-Thal", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Annehmliches Georgen Thal!", "tokens": ["An\u00b7nehm\u00b7li\u00b7ches", "Ge\u00b7or\u00b7gen", "Thal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beliebtes Lust-", "tokens": ["Be\u00b7lieb\u00b7tes", "Lust"], "token_info": ["word", "word"], "pos": ["ADJA", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Sch\u00f6nheit baut sich selbst in dir", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "baut", "sich", "selbst", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den steten Wohnungs-Saal.", "tokens": ["Den", "ste\u00b7ten", "Woh\u00b7nungs\u00b7Saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In deinen Anmuths vollen Auen", "tokens": ["In", "dei\u00b7nen", "An\u00b7muths", "vol\u00b7len", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan man sich zwar vergn\u00fcgt/ doch niemahls m\u00fcde schauen.", "tokens": ["Kan", "man", "sich", "zwar", "ver\u00b7gn\u00fcgt", "/", "doch", "nie\u00b7mahls", "m\u00fc\u00b7de", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "VVPP", "$(", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich habe dich nur halb gesehn/", "tokens": ["Ich", "ha\u00b7be", "dich", "nur", "halb", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sage doch entz\u00fcckt: dir wei\u00df nichts gleich zu gehn.", "tokens": ["Und", "sa\u00b7ge", "doch", "ent\u00b7z\u00fcckt", ":", "dir", "wei\u00df", "nichts", "gleich", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja wenn ", "tokens": ["Ja", "wenn"], "token_info": ["word", "word"], "pos": ["PTKANT", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Sie h\u00e4tte sich in deinem Wald", "tokens": ["Sie", "h\u00e4t\u00b7te", "sich", "in", "dei\u00b7nem", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Wohnung ausgelesen.", "tokens": ["Die", "Woh\u00b7nung", "aus\u00b7ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Selbst alle Ruh hat hier den Aufenthalt.", "tokens": ["Selbst", "al\u00b7le", "Ruh", "hat", "hier", "den", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "An deinen Teichen/", "tokens": ["An", "dei\u00b7nen", "Tei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Wo die Forellen streichen/", "tokens": ["Wo", "die", "Fo\u00b7rel\u00b7len", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "An deinen Wiesen/", "tokens": ["An", "dei\u00b7nen", "Wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "De\u00dfgleichen nie kein Mahler noch gepriesen/", "tokens": ["De\u00df\u00b7glei\u00b7chen", "nie", "kein", "Mah\u00b7ler", "noch", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "An deinen Fr\u00fcchten/", "tokens": ["An", "dei\u00b7nen", "Fr\u00fcch\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und andern lieblichen Gerichten/", "tokens": ["Und", "an\u00b7dern", "lieb\u00b7li\u00b7chen", "Ge\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "An deinen Zweigen/", "tokens": ["An", "dei\u00b7nen", "Zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Die so viel Augen-Weid als gr\u00fcne Bl\u00e4tter zeigen/", "tokens": ["Die", "so", "viel", "Au\u00b7gen\u00b7Weid", "als", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "An allen hab' ich das erblickt/", "tokens": ["An", "al\u00b7len", "hab'", "ich", "das", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "PDS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Was ich nie sattsam r\u00fchmen kan/", "tokens": ["Was", "ich", "nie", "satt\u00b7sam", "r\u00fch\u00b7men", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und mich doch stets entz\u00fcckt.", "tokens": ["Und", "mich", "doch", "stets", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Ich sahe dich so mit Verwundrung an/", "tokens": ["Ich", "sa\u00b7he", "dich", "so", "mit", "Ver\u00b7wund\u00b7rung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und dachte: mu\u00df man dich nur unvergleichlich nennen/", "tokens": ["Und", "dach\u00b7te", ":", "mu\u00df", "man", "dich", "nur", "un\u00b7ver\u00b7gleich\u00b7lich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VMFIN", "PIS", "PRF", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So kan man recht in dir den Ober-Herrn erkennen.", "tokens": ["So", "kan", "man", "recht", "in", "dir", "den", "O\u00b7ber\u00b7Herrn", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vergn\u00fcgt Georgen Thal!", "tokens": ["Ver\u00b7gn\u00fcgt", "Ge\u00b7or\u00b7gen", "Thal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Du pflegst uns selber das Ergetzen", "tokens": ["Du", "pflegst", "uns", "sel\u00b7ber", "das", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Im Uberflusse vorzusetzen.", "tokens": ["Im", "U\u00b7ber\u00b7flus\u00b7se", "vor\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "H\u00f6chstsch\u00e4tzbares Georgen Thal!", "tokens": ["H\u00f6chst\u00b7sch\u00e4tz\u00b7ba\u00b7res", "Ge\u00b7or\u00b7gen", "Thal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wo der Gerechtigkeit ihr Strahl", "tokens": ["Wo", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "ihr", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sich mit Leutseligkeit verm\u00e4hlet.", "tokens": ["Sich", "mit", "Leut\u00b7se\u00b7lig\u00b7keit", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Zu andrer Schande schreib den Ruhm in Marmor ein:", "tokens": ["Zu", "an\u00b7drer", "Schan\u00b7de", "schreib", "den", "Ruhm", "in", "Mar\u00b7mor", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verst\u00e4ndig/ billig/ klug/ gerecht/", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "/", "bil\u00b7lig", "/", "klug", "/", "ge\u00b7recht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Di\u00df alles mu\u00df in dir beysammen seyn/", "tokens": ["Di\u00df", "al\u00b7les", "mu\u00df", "in", "dir", "bey\u00b7sam\u00b7men", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Was alles sonst in manchen Amte fehlet.", "tokens": ["Was", "al\u00b7les", "sonst", "in", "man\u00b7chen", "Am\u00b7te", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Du sch\u00f6nes Feld/ Georgen Thal!", "tokens": ["Du", "sch\u00f6\u00b7nes", "Feld", "/", "Ge\u00b7or\u00b7gen", "Thal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und wenn ich tausend mahl", "tokens": ["Und", "wenn", "ich", "tau\u00b7send", "mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "CARD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "An deine Seltenheit gedencke/", "tokens": ["An", "dei\u00b7ne", "Sel\u00b7ten\u00b7heit", "ge\u00b7den\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Wenn nach des Himmels G\u00fctigkeit/", "tokens": ["Wenn", "nach", "des", "Him\u00b7mels", "G\u00fc\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die so viel Augen-Lust durch dich den Menschen beut/", "tokens": ["Die", "so", "viel", "Au\u00b7gen\u00b7Lust", "durch", "dich", "den", "Men\u00b7schen", "beut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich die Gedancken lencke.", "tokens": ["Ich", "die", "Ge\u00b7dan\u00b7cken", "len\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "So denck ich nichts/ als tausendmahl:", "tokens": ["So", "denck", "ich", "nichts", "/", "als", "tau\u00b7send\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIS", "$(", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Du sch\u00f6ner Ort Georgen Thal.", "tokens": ["Du", "sch\u00f6\u00b7ner", "Ort", "Ge\u00b7or\u00b7gen", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}