{"textgrid.poem.46925": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich betet' \u00fcber meinem Kinde]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich betet' \u00fcber meinem Kinde:", "tokens": ["Wenn", "ich", "be\u00b7tet'", "\u00fc\u00b7ber", "mei\u00b7nem", "Kin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr, erhalte du sie mir!", "tokens": ["Herr", ",", "er\u00b7hal\u00b7te", "du", "sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Stimme sprach in mir gelinde:", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "sprach", "in", "mir", "ge\u00b7lin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn es gut ist ihr und dir.", "tokens": ["Wenn", "es", "gut", "ist", "ihr", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Anders f\u00fcgst du's nun mit meinem Kinde,", "tokens": ["An\u00b7ders", "f\u00fcgst", "du's", "nun", "mit", "mei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herr, als ich es bat von dir;", "tokens": ["Herr", ",", "als", "ich", "es", "bat", "von", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Stimme spricht in mir gelinde", "tokens": ["Doch", "die", "Stim\u00b7me", "spricht", "in", "mir", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es gut sei mir und ihr", "tokens": ["Da\u00df", "es", "gut", "sei", "mir", "und", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PPER", "KON", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df es gut der Mutter und dem Kinde", "tokens": ["Da\u00df", "es", "gut", "der", "Mut\u00b7ter", "und", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sei, getrennt zu werden hier,", "tokens": ["Sei", ",", "ge\u00b7trennt", "zu", "wer\u00b7den", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVPP", "PTKZU", "VAINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fass' ich nicht; die Stimme spricht gelinde,", "tokens": ["Fass'", "ich", "nicht", ";", "die", "Stim\u00b7me", "spricht", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es gut ist, Herr, vor dir.", "tokens": ["Da\u00df", "es", "gut", "ist", ",", "Herr", ",", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}