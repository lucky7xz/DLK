{"dta.poem.2113": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich ihrer sch\u00f6nen B\u00fcrger geschwinden Flug, und", "tokens": ["In\u00b7dem", "ich", "ih\u00b7rer", "sch\u00f6\u00b7nen", "B\u00fcr\u00b7ger", "ge\u00b7schwin\u00b7den", "Flug", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "helle Ch\u00f6re", "tokens": ["hel\u00b7le", "Ch\u00f6\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Harmonisch modulirend singen und lieblich gurgeln, sch", "tokens": ["Har\u00b7mo\u00b7nisch", "mo\u00b7du\u00b7li\u00b7rend", "sin\u00b7gen", "und", "lieb\u00b7lich", "gur\u00b7geln", ",", "sch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJD", "VVINF", "KON", "ADJD", "VVINF", "$,", "ADJD"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "und h\u00f6re.", "tokens": ["und", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Hier\u00fcber mehrt sich mein Vergn\u00fcgen und die ger\u00fchrte", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "mehrt", "sich", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "und", "die", "ge\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "PPOSAT", "NN", "KON", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Seele denkt,", "tokens": ["See\u00b7le", "denkt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wie liebreich der, der mir nicht nur das Aug\u2019, auch das", "tokens": ["Wie", "lieb\u00b7reich", "der", ",", "der", "mir", "nicht", "nur", "das", "Aug'", ",", "auch", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geh\u00f6r geschenkt.", "tokens": ["Ge\u00b7h\u00f6r", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Seh ich darinn zugleich so manche gef\u00e4rbte Schmetter-", "tokens": ["Seh", "ich", "da\u00b7rinn", "zu\u00b7gleich", "so", "man\u00b7che", "ge\u00b7f\u00e4rb\u00b7te", "Schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "ADV", "ADV", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "linge fliegen,", "tokens": ["lin\u00b7ge", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Vermehrt, an den lebendgen Blumen im Luftreich, sich", "tokens": ["Ver\u00b7mehrt", ",", "an", "den", "le\u00b7bend\u00b7gen", "Blu\u00b7men", "im", "Luft\u00b7reich", ",", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "PRF"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.12": {"text": "noch mein Vergn\u00fcgen.", "tokens": ["noch", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Darauf erblick ich hoher Berge mit Laub und Kraut be-", "tokens": ["Da\u00b7rauf", "er\u00b7blick", "ich", "ho\u00b7her", "Ber\u00b7ge", "mit", "Laub", "und", "Kraut", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "kr\u00e4nzte Gipfel,", "tokens": ["kr\u00e4nz\u00b7te", "Gip\u00b7fel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Dann, theils durch junges Laub gebogner, theils lieblich", "tokens": ["Dann", ",", "theils", "durch", "jun\u00b7ges", "Laub", "ge\u00b7bog\u00b7ner", ",", "theils", "lieb\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ADJA", "NN", "ADJA", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "bl\u00fchnder B\u00e4ume Wipfel,", "tokens": ["bl\u00fchn\u00b7der", "B\u00e4u\u00b7me", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Dann gr\u00fcne Schatten niedrer B\u00fcsche, vom Schall der", "tokens": ["Dann", "gr\u00fc\u00b7ne", "Schat\u00b7ten", "nie\u00b7drer", "B\u00fc\u00b7sche", ",", "vom", "Schall", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Nachtigall belebt,", "tokens": ["Nach\u00b7ti\u00b7gall", "be\u00b7lebt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Die um ihr erst verfertigt Nestchen mit klingendem Ver-", "tokens": ["Die", "um", "ihr", "erst", "ver\u00b7fer\u00b7tigt", "Nest\u00b7chen", "mit", "klin\u00b7gen\u00b7dem", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADV", "VVPP", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "gn\u00fcgen schwebt.", "tokens": ["gn\u00fc\u00b7gen", "schwebt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Dann sieht mein Blick mit neuer Lust ein gr\u00fcnes wallend", "tokens": ["Dann", "sieht", "mein", "Blick", "mit", "neu\u00b7er", "Lust", "ein", "gr\u00fc\u00b7nes", "wal\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Aehrenmeer,", "tokens": ["A\u00b7eh\u00b7ren\u00b7meer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Dann auf den bunt bebl\u00fcmten Wiesen, von fetter Milch", "tokens": ["Dann", "auf", "den", "bunt", "be\u00b7bl\u00fcm\u00b7ten", "Wie\u00b7sen", ",", "von", "fet\u00b7ter", "Milch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "und Wolle schwer,", "tokens": ["und", "Wol\u00b7le", "schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Von K\u00fchen, Schafen und von Ziegen, von L\u00e4mmern,", "tokens": ["Von", "K\u00fc\u00b7hen", ",", "Scha\u00b7fen", "und", "von", "Zie\u00b7gen", ",", "von", "L\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Ochsen und von Pferden,", "tokens": ["Och\u00b7sen", "und", "von", "Pfer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Die sich, um uns zu n\u00e4hren, n\u00e4hren, zufriedne unge-", "tokens": ["Die", "sich", ",", "um", "uns", "zu", "n\u00e4h\u00b7ren", ",", "n\u00e4h\u00b7ren", ",", "zu\u00b7fried\u00b7ne", "un\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "z\u00e4hlte Heerden.", "tokens": ["z\u00e4hl\u00b7te", "Heer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Dann seh ich, von den k\u00fchlen B\u00e4chen, die klare nimmer", "tokens": ["Dann", "seh", "ich", ",", "von", "den", "k\u00fch\u00b7len", "B\u00e4\u00b7chen", ",", "die", "kla\u00b7re", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "stille Flut,", "tokens": ["stil\u00b7le", "Flut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Durch Wiesen, die sie tr\u00e4nkt, sich schl\u00e4ngeln, worauf", "tokens": ["Durch", "Wie\u00b7sen", ",", "die", "sie", "tr\u00e4nkt", ",", "sich", "schl\u00e4n\u00b7geln", ",", "wo\u00b7rauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRF", "VVINF", "$,", "PWAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "bald hier des Himmels Glut,", "tokens": ["bald", "hier", "des", "Him\u00b7mels", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}