{"textgrid.poem.32834": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fragst, warum die\u00df artge T\u00e4ubgen lacht?", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, warum die\u00df artge T\u00e4ubgen lacht?", "tokens": ["Du", "fragst", ",", "wa\u00b7rum", "die\u00df", "art\u00b7ge", "T\u00e4ub\u00b7gen", "lacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sahst du, was es vorhergemacht?", "tokens": ["Sahst", "du", ",", "was", "es", "vor\u00b7her\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schn\u00e4belte sein Weibchen da:", "tokens": ["Es", "schn\u00e4\u00b7bel\u00b7te", "sein", "Weib\u00b7chen", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdar\u00fcber lacht man,\u00ab fragst du? \u00bbJa.\u00ab", "tokens": ["\u00bb", "da\u00b7r\u00fc\u00b7ber", "lacht", "man", ",", "\u00ab", "fragst", "du", "?", "\u00bb", "Ja", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PIS", "$,", "$(", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sieh nur hin! es schn\u00e4belt wiederum,", "tokens": ["Da", "sieh", "nur", "hin", "!", "es", "schn\u00e4\u00b7belt", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sieht sich lachend nach uns um.", "tokens": ["Und", "sieht", "sich", "la\u00b7chend", "nach", "uns", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glaub mir, es lacht uns sp\u00f6ttisch an,", "tokens": ["Glaub", "mir", ",", "es", "lacht", "uns", "sp\u00f6t\u00b7tisch", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wirs ihm noch nicht nachgethan.", "tokens": ["Da\u00df", "wirs", "ihm", "noch", "nicht", "nach\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}