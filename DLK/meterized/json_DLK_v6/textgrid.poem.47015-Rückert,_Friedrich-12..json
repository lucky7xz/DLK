{"textgrid.poem.47015": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Vater ist ein reicher Mann im Lande,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "ist", "ein", "rei\u00b7cher", "Mann", "im", "Lan\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seine \u00c4cker liegen allerorten,", "tokens": ["Und", "sei\u00b7ne", "\u00c4\u00b7cker", "lie\u00b7gen", "al\u00b7le\u00b7ror\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier steht sein Haus mit Hallen, Hof und Pforten,", "tokens": ["Hier", "steht", "sein", "Haus", "mit", "Hal\u00b7len", ",", "Hof", "und", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier kann ich wohnen, d\u00e4cht' ich, ohne Schande.", "tokens": ["Hier", "kann", "ich", "woh\u00b7nen", ",", "d\u00e4cht'", "ich", ",", "oh\u00b7ne", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch sonst noch hat er, nicht gebaut auf Sande,", "tokens": ["Auch", "sonst", "noch", "hat", "er", ",", "nicht", "ge\u00b7baut", "auf", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "$,", "PTKNEG", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Haus im Grund hier, eins im Grunde dorten;", "tokens": ["Ein", "Haus", "im", "Grund", "hier", ",", "eins", "im", "Grun\u00b7de", "dor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,", "PIS", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4r' mir keiner recht von den drei Orten,", "tokens": ["Und", "w\u00e4r'", "mir", "kei\u00b7ner", "recht", "von", "den", "drei", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "ADJD", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So kommt noch leicht ein vierter Kauf zu stande.", "tokens": ["So", "kommt", "noch", "leicht", "ein", "vier\u00b7ter", "Kauf", "zu", "stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und will ich in kein fertig Haus mich setzen,", "tokens": ["Und", "will", "ich", "in", "kein", "fer\u00b7tig", "Haus", "mich", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PIAT", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hat er einen Wald mit manchem Baume,", "tokens": ["So", "hat", "er", "ei\u00b7nen", "Wald", "mit", "man\u00b7chem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mancher Berg mit Steinbruch ist ihm eigen.", "tokens": ["Und", "man\u00b7cher", "Berg", "mit", "Stein\u00b7bruch", "ist", "ihm", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann gibt es Zimmerleut' hier und Steinmetzen,", "tokens": ["Dann", "gibt", "es", "Zim\u00b7mer\u00b7leut'", "hier", "und", "Stein\u00b7met\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die baun ein Haus mir mit Gela\u00df und Raume,", "tokens": ["Die", "baun", "ein", "Haus", "mir", "mit", "Ge\u00b7la\u00df", "und", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin man auch tanzen kann den Hochzeitsreigen.\u00ab", "tokens": ["Drin", "man", "auch", "tan\u00b7zen", "kann", "den", "Hoch\u00b7zeits\u00b7rei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "ADV", "VVINF", "VMFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}