{"textgrid.poem.33427": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das furchtbar zu den frohen Tischen trat", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das furchtbar zu den frohen Tischen trat", "tokens": ["Das", "furcht\u00b7bar", "zu", "den", "fro\u00b7hen", "Ti\u00b7schen", "trat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und das Gem\u00fcth in wilde Schrecken h\u00fcllte.", "tokens": ["Und", "das", "Ge\u00b7m\u00fcth", "in", "wil\u00b7de", "Schre\u00b7cken", "h\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier wu\u00dften selbst die G\u00f6tter keinen Rath", "tokens": ["Hier", "wu\u00df\u00b7ten", "selbst", "die", "G\u00f6t\u00b7ter", "kei\u00b7nen", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der die beklommne Brust mit Trost erf\u00fcllte.", "tokens": ["Der", "die", "be\u00b7klomm\u00b7ne", "Brust", "mit", "Trost", "er\u00b7f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geheimni\u00dfvoll war dieses Unholds Pfad", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "war", "die\u00b7ses", "Un\u00b7holds", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Wuth kein Flehn und keine Gabe stillte;", "tokens": ["Des", "Wuth", "kein", "Flehn", "und", "kei\u00b7ne", "Ga\u00b7be", "still\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war der Tod, der dieses Lustgelag", "tokens": ["Es", "war", "der", "Tod", ",", "der", "die\u00b7ses", "Lust\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit Angst und Schmerz und Thr\u00e4nen unterbrach.", "tokens": ["Mit", "Angst", "und", "Schmerz", "und", "Thr\u00e4\u00b7nen", "un\u00b7ter\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf ewig nun von allem abgeschieden,", "tokens": ["Auf", "e\u00b7wig", "nun", "von", "al\u00b7lem", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hier das Herz in s\u00fc\u00dfer Wollust regt,", "tokens": ["Was", "hier", "das", "Herz", "in", "s\u00fc\u00b7\u00dfer", "Wol\u00b7lust", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Getrennt von den Geliebten, die hienieden", "tokens": ["Ge\u00b7trennt", "von", "den", "Ge\u00b7lieb\u00b7ten", ",", "die", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergebne Sehnsucht, langes Weh bewegt,", "tokens": ["Ver\u00b7geb\u00b7ne", "Sehn\u00b7sucht", ",", "lan\u00b7ges", "Weh", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schien matter Traum dem Todten nur beschieden,", "tokens": ["Schien", "mat\u00b7ter", "Traum", "dem", "Tod\u00b7ten", "nur", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ohnm\u00e4chtiges Ringen nur ihm auferlegt.", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7ges", "Rin\u00b7gen", "nur", "ihm", "auf\u00b7er\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Zerbrochen war die Woge des Genusses", "tokens": ["Zer\u00b7bro\u00b7chen", "war", "die", "Wo\u00b7ge", "des", "Ge\u00b7nus\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Am Felsen des unendlichen Verdrusses.", "tokens": ["Am", "Fel\u00b7sen", "des", "un\u00b7end\u00b7li\u00b7chen", "Ver\u00b7drus\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit k\u00fchnem Geist und hoher Sinnenglut", "tokens": ["Mit", "k\u00fch\u00b7nem", "Geist", "und", "ho\u00b7her", "Sin\u00b7nen\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versch\u00f6nte sich der Mensch die grause Larve,", "tokens": ["Ver\u00b7sch\u00f6n\u00b7te", "sich", "der", "Mensch", "die", "grau\u00b7se", "Lar\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sanfter J\u00fcngling l\u00f6scht das Licht und ruht \u2013", "tokens": ["Ein", "sanf\u00b7ter", "J\u00fcng\u00b7ling", "l\u00f6scht", "das", "Licht", "und", "ruht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sanft wird das Ende, wie ein Wehn der Harfe.", "tokens": ["Sanft", "wird", "das", "En\u00b7de", ",", "wie", "ein", "Wehn", "der", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erinnerung schmilzt in k\u00fchler Schattenflut,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "schmilzt", "in", "k\u00fch\u00b7ler", "Schat\u00b7ten\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So sang das Lied dem traurigen Bedarfe.", "tokens": ["So", "sang", "das", "Lied", "dem", "trau\u00b7ri\u00b7gen", "Be\u00b7dar\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch unentr\u00e4thselt blieb die ewge Nacht,", "tokens": ["Doch", "un\u00b7en\u00b7tr\u00e4th\u00b7selt", "blieb", "die", "ew\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das ernste Zeichen einer fernen Macht.", "tokens": ["Das", "erns\u00b7te", "Zei\u00b7chen", "ei\u00b7ner", "fer\u00b7nen", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der J\u00fcngling bist du, der seit langer Zeit", "tokens": ["Der", "J\u00fcng\u00b7ling", "bist", "du", ",", "der", "seit", "lan\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf unsern Gr\u00e4bern steht in tiefen Sinnen;", "tokens": ["Auf", "un\u00b7sern", "Gr\u00e4\u00b7bern", "steht", "in", "tie\u00b7fen", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein tr\u00f6stlich Zeichen in der Dunkelheit \u2013", "tokens": ["Ein", "tr\u00f6st\u00b7lich", "Zei\u00b7chen", "in", "der", "Dun\u00b7kel\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der h\u00f6hern Menschheit freudiges Beginnen.", "tokens": ["Der", "h\u00f6\u00b7hern", "Menschheit", "freu\u00b7di\u00b7ges", "Be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was uns gesenkt in tiefe Traurigkeit", "tokens": ["Was", "uns", "ge\u00b7senkt", "in", "tie\u00b7fe", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zieht uns mit s\u00fc\u00dfer Sehnsucht nun von hinnen.", "tokens": ["Zieht", "uns", "mit", "s\u00fc\u00b7\u00dfer", "Sehn\u00b7sucht", "nun", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Tode ward das ewge Leben kund,", "tokens": ["Im", "To\u00b7de", "ward", "das", "ew\u00b7ge", "Le\u00b7ben", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du bist der Tod und machst uns erst gesund.", "tokens": ["Du", "bist", "der", "Tod", "und", "machst", "uns", "erst", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gehoben ist der Stein \u2013", "tokens": ["Ge\u00b7ho\u00b7ben", "ist", "der", "Stein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschheit ist erstanden \u2013", "tokens": ["Die", "Menschheit", "ist", "er\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir alle bleiben dein", "tokens": ["Wir", "al\u00b7le", "blei\u00b7ben", "dein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchlen keine Banden.", "tokens": ["Und", "f\u00fch\u00b7len", "kei\u00b7ne", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der herbste Kummer fleucht", "tokens": ["Der", "herbs\u00b7te", "Kum\u00b7mer", "fleucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor deiner goldnen Schaale,", "tokens": ["Vor", "dei\u00b7ner", "gold\u00b7nen", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn Erd und Leben weicht", "tokens": ["Wenn", "Erd", "und", "Le\u00b7ben", "weicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Im letzten Abendmahle.", "tokens": ["Im", "letz\u00b7ten", "A\u00b7bend\u00b7mah\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zur Hochzeit ruft der Tod \u2013", "tokens": ["Zur", "Hoch\u00b7zeit", "ruft", "der", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Lampen brennen helle \u2013", "tokens": ["Die", "Lam\u00b7pen", "bren\u00b7nen", "hel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfraun sind zur Stelle \u2013", "tokens": ["Die", "Jung\u00b7fraun", "sind", "zur", "Stel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um Oel ist keine Noth \u2013", "tokens": ["Um", "O\u00b7el", "ist", "kei\u00b7ne", "Noth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erkl\u00e4nge doch die Ferne", "tokens": ["Er\u00b7kl\u00e4n\u00b7ge", "doch", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von deinem Zuge schon,", "tokens": ["Von", "dei\u00b7nem", "Zu\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ruften uns die Sterne", "tokens": ["Und", "ruf\u00b7ten", "uns", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Menschenzung' und Ton.", "tokens": ["Mit", "Men\u00b7schen\u00b7zung'", "und", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nach dir, Maria, heben", "tokens": ["Nach", "dir", ",", "Ma\u00b7ria", ",", "he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPER", "$,", "NE", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schon tausend Herzen sich.", "tokens": ["Schon", "tau\u00b7send", "Her\u00b7zen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In diesem Schattenleben", "tokens": ["In", "die\u00b7sem", "Schat\u00b7ten\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlangten sie nur dich.", "tokens": ["Ver\u00b7lang\u00b7ten", "sie", "nur", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie hoffen zu genesen", "tokens": ["Sie", "hof\u00b7fen", "zu", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Mit ahndungsvoller Lust \u2013", "tokens": ["Mit", "ahn\u00b7dungs\u00b7vol\u00b7ler", "Lust", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dr\u00fcckst du sie, heilges Wesen,", "tokens": ["Dr\u00fcckst", "du", "sie", ",", "heil\u00b7ges", "We\u00b7sen", ","], "token_info": 