{"textgrid.poem.48685": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Da\u00df Alles eitel sei", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, sprichst du, ist es wol, darauf du dich bem\u00fchst?", "tokens": ["Was", ",", "sprichst", "du", ",", "ist", "es", "wol", ",", "da\u00b7rauf", "du", "dich", "be\u00b7m\u00fchst", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "$,", "PAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kunst, Ehre, Reichtum, Lust, die L\u00fcften gleich und G\u00fcssen", "tokens": ["Kunst", ",", "Eh\u00b7re", ",", "Reich\u00b7tum", ",", "Lust", ",", "die", "L\u00fcf\u00b7ten", "gleich", "und", "G\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "ART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit uns selbst schie\u00dfen hin? Ich auch, Freund, bin geflissen", "tokens": ["mit", "uns", "selbst", "schie\u00b7\u00dfen", "hin", "?", "Ich", "auch", ",", "Freund", ",", "bin", "ge\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PTKVZ", "$.", "PPER", "ADV", "$,", "NN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf eben diesen Sinn, auf den du weislich siehst.", "tokens": ["auf", "e\u00b7ben", "die\u00b7sen", "Sinn", ",", "auf", "den", "du", "weis\u00b7lich", "siehst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "$,", "APPR", "ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df es mehr als wol, da\u00df Alles eitel ist.", "tokens": ["Ich", "wei\u00df", "es", "mehr", "als", "wol", ",", "da\u00df", "Al\u00b7les", "ei\u00b7tel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "KOKOM", "ADV", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie aber k\u00f6mmt es doch, da\u00df wider unser Wissen", "tokens": ["Wie", "a\u00b7ber", "k\u00f6mmt", "es", "doch", ",", "da\u00df", "wi\u00b7der", "un\u00b7ser", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wir etwas, das nicht ist, doch sch\u00f6ne hei\u00dfen m\u00fcssen,", "tokens": ["wir", "et\u00b7was", ",", "das", "nicht", "ist", ",", "doch", "sch\u00f6\u00b7ne", "hei\u00b7\u00dfen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PTKNEG", "VAFIN", "$,", "ADV", "ADJA", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df der ein Anders tut, ein Anders ihm erliest?", "tokens": ["da\u00df", "der", "ein", "An\u00b7ders", "tut", ",", "ein", "An\u00b7ders", "ihm", "er\u00b7liest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Unvollkommenheit vollkommen werden wollen,", "tokens": ["In", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", "voll\u00b7kom\u00b7men", "wer\u00b7den", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das machet unsern Sinn auf Neues so geschwollen,", "tokens": ["das", "ma\u00b7chet", "un\u00b7sern", "Sinn", "auf", "Neu\u00b7es", "so", "ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "erf\u00fcllet auf den Schein, am leichten Winde schwer,", "tokens": ["er\u00b7f\u00fcl\u00b7let", "auf", "den", "Schein", ",", "am", "leich\u00b7ten", "Win\u00b7de", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "an vollem Mangel reich. Wer kan von Herzen sagen:", "tokens": ["an", "vol\u00b7lem", "Man\u00b7gel", "reich", ".", "Wer", "kan", "von", "Her\u00b7zen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$.", "PWS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin vergn\u00fcgt in mir, wei\u00df weder Lust noch Klagen.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "in", "mir", ",", "wei\u00df", "we\u00b7der", "Lust", "noch", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPER", "$,", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie eitel Alles ist, der Mensch ist eitel mehr!", "tokens": ["Wie", "ei\u00b7tel", "Al\u00b7les", "ist", ",", "der", "Mensch", "ist", "ei\u00b7tel", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VAFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}