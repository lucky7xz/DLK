{"dta.poem.20415": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Abbildung einer tugendhafften  \n wittib.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie ist ein rauten-zweig/ an den kein gifft sich leget/", "tokens": ["Sie", "ist", "ein", "rau\u00b7ten\u00b7zweig", "/", "an", "den", "kein", "gifft", "sich", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "PIAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein feld/ das ehren-prei\u00df vor liebe-st\u00f6ckel tr\u00e4get:", "tokens": ["Ein", "feld", "/", "das", "eh\u00b7ren\u00b7prei\u00df", "vor", "lie\u00b7be\u00b7st\u00f6\u00b7ckel", "tr\u00e4\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein keuscher wunder-strahl der sonnen wahrer zucht/", "tokens": ["Ein", "keu\u00b7scher", "wun\u00b7der\u00b7strahl", "der", "son\u00b7nen", "wah\u00b7rer", "zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da die gelegenheit sich zu verbergen sucht.", "tokens": ["Da", "die", "ge\u00b7le\u00b7gen\u00b7heit", "sich", "zu", "ver\u00b7ber\u00b7gen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein reines paradi\u00df/ und engel auch daneben/", "tokens": ["Ein", "rei\u00b7nes", "pa\u00b7ra\u00b7di\u00df", "/", "und", "en\u00b7gel", "auch", "da\u00b7ne\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "NN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das keine schlange l\u00e4st in seiner gegend schweben:", "tokens": ["Das", "kei\u00b7ne", "schlan\u00b7ge", "l\u00e4st", "in", "sei\u00b7ner", "ge\u00b7gend", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "APPR", "PPOSAT", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein ancker/ der nach thau des milden himmels schmeckt:", "tokens": ["Ein", "an\u00b7cker", "/", "der", "nach", "thau", "des", "mil\u00b7den", "him\u00b7mels", "schmeckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein gleiches spiegel-gla\u00df/ da\u00df nicht die welt befleckt:", "tokens": ["Ein", "glei\u00b7ches", "spie\u00b7gel\u00b7gla\u00df", "/", "da\u00df", "nicht", "die", "welt", "be\u00b7fleckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein pfenning/ der nicht mehr als ein gepr\u00e4ge leidet:", "tokens": ["Ein", "pfen\u00b7ning", "/", "der", "nicht", "mehr", "als", "ein", "ge\u00b7pr\u00e4\u00b7ge", "lei\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein schaaff/ da\u00df allzeit sich in himmels-schl\u00fcsseln weidet;", "tokens": ["Ein", "schaaff", "/", "da\u00df", "all\u00b7zeit", "sich", "in", "him\u00b7mels\u00b7schl\u00fcs\u00b7seln", "wei\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "KOUS", "ADV", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dessen keuscher leib nur einen hirten kennt;", "tokens": ["Und", "des\u00b7sen", "keu\u00b7scher", "leib", "nur", "ei\u00b7nen", "hir\u00b7ten", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein weisses ehren-licht/ so GOtt zu dienste brennt.", "tokens": ["Ein", "weis\u00b7ses", "eh\u00b7ren\u00b7licht", "/", "so", "Gott", "zu", "diens\u00b7te", "brennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "NN", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein geist/ der sich mit nichts als einsamkeit verm\u00e4hlet:", "tokens": ["Ein", "geist", "/", "der", "sich", "mit", "nichts", "als", "ein\u00b7sam\u00b7keit", "ver\u00b7m\u00e4h\u00b7let", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPR", "PIS", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein hertze von der zucht zu einem thron erw\u00e4hlet:", "tokens": ["Ein", "hert\u00b7ze", "von", "der", "zucht", "zu", "ei\u00b7nem", "thron", "er\u00b7w\u00e4h\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der laster sterbe-hau\u00df/ der tugend schlo\u00df und sitz/", "tokens": ["Der", "las\u00b7ter", "ster\u00b7be\u00b7hau\u00df", "/", "der", "tu\u00b7gend", "schlo\u00df", "und", "sitz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "KON", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Daran der himmel schreibt: allhier wohnt nichts als witz.", "tokens": ["Da\u00b7ran", "der", "him\u00b7mel", "schreibt", ":", "all\u00b7hier", "wohnt", "nichts", "als", "witz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein bergwerck/ so nur gold der reinigkeit will leiden/", "tokens": ["Ein", "berg\u00b7werck", "/", "so", "nur", "gold", "der", "rei\u00b7nig\u00b7keit", "will", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Und falsche rechnung heist von seinen grentzen scheiden.", "tokens": ["Und", "fal\u00b7sche", "rech\u00b7nung", "heist", "von", "sei\u00b7nen", "grent\u00b7zen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein flu\u00df/ der weder wind noch fremdes wasser tr\u00fcbt/", "tokens": ["Ein", "flu\u00df", "/", "der", "we\u00b7der", "wind", "noch", "frem\u00b7des", "was\u00b7ser", "tr\u00fcbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KON", "VAFIN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und GOtt/ von dem er kommt/ sich wieder gantz ergiebt.", "tokens": ["Und", "Gott", "/", "von", "dem", "er", "kommt", "/", "sich", "wie\u00b7der", "gantz", "er\u00b7giebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "$(", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein etwas/ das fast nichts der freyheit gleiche sch\u00e4tzet/", "tokens": ["Ein", "et\u00b7was", "/", "das", "fast", "nichts", "der", "frey\u00b7heit", "glei\u00b7che", "sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "PDS", "ADV", "PIS", "ART", "NN", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und keiner steine glantz vor dieses kleinod setzet;", "tokens": ["Und", "kei\u00b7ner", "stei\u00b7ne", "glantz", "vor", "die\u00b7ses", "klei\u00b7nod", "set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein haus/ in dem der geist von oben hofstadt h\u00e4lt/", "tokens": ["Ein", "haus", "/", "in", "dem", "der", "geist", "von", "o\u00b7ben", "hof\u00b7stadt", "h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "ART", "NN", "APPR", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und alles diesem wirth als magd zu fusse f\u00e4llt.", "tokens": ["Und", "al\u00b7les", "die\u00b7sem", "wirth", "als", "magd", "zu", "fus\u00b7se", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "ADJD", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein leit-stern zu der zucht/ ein sturm vor b\u00f6se l\u00fcste/", "tokens": ["Ein", "leit\u00b7stern", "zu", "der", "zucht", "/", "ein", "sturm", "vor", "b\u00f6\u00b7se", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der tugend festes land/ der s\u00fcnden eine w\u00fcste.", "tokens": ["Der", "tu\u00b7gend", "fes\u00b7tes", "land", "/", "der", "s\u00fcn\u00b7den", "ei\u00b7ne", "w\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Itzt schlie\u00df ich dieses bild in keusche reimen ein/", "tokens": ["Itzt", "schlie\u00df", "ich", "die\u00b7ses", "bild", "in", "keu\u00b7sche", "rei\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "ADJA", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es ist ein heiligthum/ das nicht gek\u00fc\u00dft will seyn.", "tokens": ["Es", "ist", "ein", "hei\u00b7lig\u00b7thum", "/", "das", "nicht", "ge\u00b7k\u00fc\u00dft", "will", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PDS", "PTKNEG", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}