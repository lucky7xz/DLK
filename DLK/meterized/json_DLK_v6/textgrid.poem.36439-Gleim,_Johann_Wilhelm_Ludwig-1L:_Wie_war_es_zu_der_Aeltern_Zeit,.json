{"textgrid.poem.36439": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie war es zu der Aeltern Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie war es zu der Aeltern Zeit,", "tokens": ["Wie", "war", "es", "zu", "der", "A\u00b7el\u00b7tern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn sich ein sch\u00f6nes Paar gefrei't?", "tokens": ["Wenn", "sich", "ein", "sch\u00f6\u00b7nes", "Paar", "ge\u00b7frei't", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ehestand war voller Streit,", "tokens": ["Der", "E\u00b7hes\u00b7tand", "war", "vol\u00b7ler", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Einigkeit die erste Liebe.", "tokens": ["Voll", "Ei\u00b7nig\u00b7keit", "die", "ers\u00b7te", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie geht es in der iungen Welt,", "tokens": ["Wie", "geht", "es", "in", "der", "i\u00b7un\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn sich ein sch\u00f6nes Paar gef\u00e4llt?", "tokens": ["Wenn", "sich", "ein", "sch\u00f6\u00b7nes", "Paar", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bald es eh'lich sich gesellt,", "tokens": ["So", "bald", "es", "eh'\u00b7lich", "sich", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verwandeln sich die ersten Triebe.", "tokens": ["Ver\u00b7wan\u00b7deln", "sich", "die", "ers\u00b7ten", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der allerw\u00fcrdigste Genu\u00df,", "tokens": ["Der", "al\u00b7ler\u00b7w\u00fcr\u00b7digs\u00b7te", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein s\u00fcsser und verschwiegner Ku\u00df", "tokens": ["Ein", "s\u00fcs\u00b7ser", "und", "ver\u00b7schwieg\u00b7ner", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird bitter durch das W\u00f6rtlein: Mu\u00df.", "tokens": ["Wird", "bit\u00b7ter", "durch", "das", "W\u00f6rt\u00b7lein", ":", "Mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$.", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dis war der klugen Alten Glaube.", "tokens": ["Dis", "war", "der", "klu\u00b7gen", "Al\u00b7ten", "Glau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich glaub' es mit und sage dis:", "tokens": ["Ich", "glaub'", "es", "mit", "und", "sa\u00b7ge", "dis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor meinen Mund schmekkt ganz gewi\u00df", "tokens": ["Vor", "mei\u00b7nen", "Mund", "schmekkt", "ganz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Schm\u00e4tzgen noch einmal so s\u00fc\u00df,", "tokens": ["Ein", "Schm\u00e4tz\u00b7gen", "noch", "ein\u00b7mal", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ich es sch\u00f6nen Lippen raube.", "tokens": ["Wenn", "ich", "es", "sch\u00f6\u00b7nen", "Lip\u00b7pen", "rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wem seine Liebe gl\u00fckken soll,", "tokens": ["Wem", "sei\u00b7ne", "Lie\u00b7be", "gl\u00fck\u00b7ken", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der pr\u00fcfe Herz und Nieren wol,", "tokens": ["Der", "pr\u00fc\u00b7fe", "Herz", "und", "Nie\u00b7ren", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst wird sein Haus von Kummer voll,", "tokens": ["Sonst", "wird", "sein", "Haus", "von", "Kum\u00b7mer", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leer von Lust, und Scherz, und Liebe.", "tokens": ["Und", "leer", "von", "Lust", ",", "und", "Scherz", ",", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann geht es so, wer wei\u00df nicht? wie,", "tokens": ["Dann", "geht", "es", "so", ",", "wer", "wei\u00df", "nicht", "?", "wie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "VVFIN", "PTKNEG", "$.", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie w\u00fcnscht sich den, er w\u00fcnscht sich die,", "tokens": ["Sie", "w\u00fcnscht", "sich", "den", ",", "er", "w\u00fcnscht", "sich", "die", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "$,", "PPER", "VVFIN", "PRF", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und beide finden ohne M\u00fch", "tokens": ["Und", "bei\u00b7de", "fin\u00b7den", "oh\u00b7ne", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ernehrer der entstandnen Triebe.", "tokens": ["Er\u00b7neh\u00b7rer", "der", "ent\u00b7stand\u00b7nen", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Beispiel ist mir schon bewu\u00dft,", "tokens": ["Ein", "Bei\u00b7spiel", "ist", "mir", "schon", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Lieb' und ohne Lust", "tokens": ["Und", "oh\u00b7ne", "Lieb'", "und", "oh\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt Martha: Sch\u00e4tzgen, komm, du mu\u00dft;", "tokens": ["H\u00f6rt", "Mar\u00b7tha", ":", "Sch\u00e4tz\u00b7gen", ",", "komm", ",", "du", "mu\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NE", "$.", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und folgt langsam mit kleinen Schritten.", "tokens": ["Und", "folgt", "lang\u00b7sam", "mit", "klei\u00b7nen", "Schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn sich Bellander eingestellt,", "tokens": ["Wenn", "sich", "Bel\u00b7lan\u00b7der", "ein\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich vor viel geschikkter h\u00e4lt,", "tokens": ["Der", "sich", "vor", "viel", "ge\u00b7schikk\u00b7ter", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von dem sie selbst dis Urteil f\u00e4llt,", "tokens": ["Von", "dem", "sie", "selbst", "dis", "Ur\u00b7teil", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So l\u00e4\u00dft sie sich nicht lange bitten.", "tokens": ["So", "l\u00e4\u00dft", "sie", "sich", "nicht", "lan\u00b7ge", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was w\u00fcrket nicht der schn\u00f6de Zwang!", "tokens": ["Was", "w\u00fcr\u00b7ket", "nicht", "der", "schn\u00f6\u00b7de", "Zwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4lligkeiten ohne Dank,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig\u00b7kei\u00b7ten", "oh\u00b7ne", "Dank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und oft sein ganzes Lebelang", "tokens": ["Und", "oft", "sein", "gan\u00b7zes", "Le\u00b7be\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkenntni\u00df eh'licher Beschwerden.", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "eh'\u00b7li\u00b7cher", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es lebe, was sich scherzend \u00fcbt!", "tokens": ["Es", "le\u00b7be", ",", "was", "sich", "scher\u00b7zend", "\u00fcbt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lebe, was sich mir ergiebt!", "tokens": ["Es", "le\u00b7be", ",", "was", "sich", "mir", "er\u00b7giebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch dabei die Freiheit liebt,", "tokens": ["Und", "doch", "da\u00b7bei", "die", "Frei\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freiheit, meinen Schatz auf Erden.", "tokens": ["Die", "Frei\u00b7heit", ",", "mei\u00b7nen", "Schatz", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}