{"textgrid.poem.57387": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja du bist es, du komst, s\u00fcsse Verneuerin,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja du bist es, du komst, s\u00fcsse Verneuerin,", "tokens": ["Ja", "du", "bist", "es", ",", "du", "komst", ",", "s\u00fcs\u00b7se", "Ver\u00b7neu\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ach Erinrung der Zeit, die floh.", "tokens": ["Ach", "E\u00b7rin\u00b7rung", "der", "Zeit", ",", "die", "floh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Inniger freust du mich oft, als die Erblickung mich,", "tokens": ["In\u00b7ni\u00b7ger", "freust", "du", "mich", "oft", ",", "als", "die", "Er\u00b7bli\u00b7ckung", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als mich Stimmen, des Menschen freun.", "tokens": ["Als", "mich", "Stim\u00b7men", ",", "des", "Men\u00b7schen", "freun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Du erschafst mir kein Bild von dem Verschwundenen,", "tokens": ["Du", "er\u00b7schafst", "mir", "kein", "Bild", "von", "dem", "Ver\u00b7schwun\u00b7de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Scheinst au wandeln in Wirkliches.", "tokens": ["Scheinst", "au", "wan\u00b7deln", "in", "Wirk\u00b7li\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "L\u00e4ngeres Leben wird uns, Gute, wenn uns den Schmerz.", "tokens": ["L\u00e4n\u00b7ge\u00b7res", "Le\u00b7ben", "wird", "uns", ",", "Gu\u00b7te", ",", "wenn", "uns", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Wiederkehr des Genossnen scheucht:", "tokens": ["Wie\u00b7der\u00b7kehr", "des", "Ge\u00b7noss\u00b7nen", "scheucht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Denn die Stunde, die uns traurig umw\u00f6lkt, geh\u00f6rt", "tokens": ["Denn", "die", "Stun\u00b7de", ",", "die", "uns", "trau\u00b7rig", "um\u00b7w\u00f6lkt", ",", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Zu den Stunden des Lebens nicht.", "tokens": ["Zu", "den", "Stun\u00b7den", "des", "Le\u00b7bens", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wie am Feste, das sie damals ihr feyerten,", "tokens": ["Wie", "am", "Fes\u00b7te", ",", "das", "sie", "da\u00b7mals", "ihr", "fe\u00b7yer\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Da noch Freyheit die Freyheit war,", "tokens": ["Da", "noch", "Frey\u00b7heit", "die", "Frey\u00b7heit", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "In den Kr\u00e4nzen umher auf den elisischen,", "tokens": ["In", "den", "Kr\u00e4n\u00b7zen", "um\u00b7her", "auf", "den", "e\u00b7li\u00b7si\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Fehlern Blumen an Blumen sich", "tokens": ["Feh\u00b7lern", "Blu\u00b7men", "an", "Blu\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "PRF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Lachend reihten, so reihn sich mit vereinter Hand", "tokens": ["La\u00b7chend", "reih\u00b7ten", ",", "so", "reihn", "sich", "mit", "ver\u00b7ein\u00b7ter", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Jene s\u00fcssen Erwachenden,", "tokens": ["Je\u00b7ne", "s\u00fcs\u00b7sen", "Er\u00b7wa\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.17": {"text": "Die aus der Nacht des Vergangs mir die Erinnerung", "tokens": ["Die", "aus", "der", "Nacht", "des", "Ver\u00b7gangs", "mir", "die", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PPER", "ART", "NN"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.18": {"text": "Vor der Seele vor\u00fcberf\u00fchrt.", "tokens": ["Vor", "der", "See\u00b7le", "vor\u00b7\u00fc\u00b7berf\u00b7\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Kiesen soll ich daraus, singen mit trunknem Ton", "tokens": ["Kie\u00b7sen", "soll", "ich", "da\u00b7raus", ",", "sin\u00b7gen", "mit", "trunk\u00b7nem", "Ton"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PAV", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Eine der Sonnen, die einst mir schien.", "tokens": ["Ei\u00b7ne", "der", "Son\u00b7nen", ",", "die", "einst", "mir", "schien", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Kann ich es? Wer sich im Strom frischet, bemerket die", "tokens": ["Kann", "ich", "es", "?", "Wer", "sich", "im", "Strom", "fri\u00b7schet", ",", "be\u00b7mer\u00b7ket", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "$.", "PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "ART"], "meter": "+---+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "K\u00fchlung einzelner Wellen nicht.", "tokens": ["K\u00fch\u00b7lung", "ein\u00b7zel\u00b7ner", "Wel\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}