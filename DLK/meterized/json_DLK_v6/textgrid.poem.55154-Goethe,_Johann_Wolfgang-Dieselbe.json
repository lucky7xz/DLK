{"textgrid.poem.55154": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00dft mich scheinen, bis ich werde;", "tokens": ["So", "la\u00dft", "mich", "schei\u00b7nen", ",", "bis", "ich", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht mir das wei\u00dfe Kleid nicht aus!", "tokens": ["Zieht", "mir", "das", "wei\u00b7\u00dfe", "Kleid", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich eile von der sch\u00f6nen Erde", "tokens": ["Ich", "ei\u00b7le", "von", "der", "sch\u00f6\u00b7nen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab in jenes feste Haus.", "tokens": ["Hin\u00b7ab", "in", "je\u00b7nes", "fes\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort ruh ich eine kleine Stille,", "tokens": ["Dort", "ruh", "ich", "ei\u00b7ne", "klei\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann \u00f6ffnet sich der frische Blick,", "tokens": ["Dann", "\u00f6ff\u00b7net", "sich", "der", "fri\u00b7sche", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lasse dann die reine H\u00fclle,", "tokens": ["Ich", "las\u00b7se", "dann", "die", "rei\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den G\u00fcrtel und den Kranz zur\u00fcck.", "tokens": ["Den", "G\u00fcr\u00b7tel", "und", "den", "Kranz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und jene himmlischen Gestalten,", "tokens": ["Und", "je\u00b7ne", "himm\u00b7li\u00b7schen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fragen nicht nach Mann und Weib,", "tokens": ["Sie", "fra\u00b7gen", "nicht", "nach", "Mann", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keine Kleider, keine Falten", "tokens": ["Und", "kei\u00b7ne", "Klei\u00b7der", ",", "kei\u00b7ne", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umgeben den verkl\u00e4rten Leib.", "tokens": ["Um\u00b7ge\u00b7ben", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Zwar lebt ich ohne Sorg und M\u00fche,", "tokens": ["Zwar", "lebt", "ich", "oh\u00b7ne", "Sorg", "und", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch f\u00fchlt ich tiefen Schmerz genung.", "tokens": ["Doch", "f\u00fchlt", "ich", "tie\u00b7fen", "Schmerz", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Kummer altert ich zu fr\u00fche;", "tokens": ["Vor", "Kum\u00b7mer", "al\u00b7tert", "ich", "zu", "fr\u00fc\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht mich auf ewig wieder jung!", "tokens": ["Macht", "mich", "auf", "e\u00b7wig", "wie\u00b7der", "jung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}