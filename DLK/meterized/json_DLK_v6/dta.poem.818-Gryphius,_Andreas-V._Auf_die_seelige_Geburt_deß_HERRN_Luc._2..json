{"dta.poem.818": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "V.  Auf die seelige Geburt de\u00df HERRN  \n Luc. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Schawe h\u00f6chster K\u00f6nig schawe/ wie vnm\u00e4ssig mich ge- ", "tokens": ["Scha\u00b7we", "h\u00f6chs\u00b7ter", "K\u00f6\u00b7nig", "scha\u00b7we", "/", "wie", "vn\u00b7m\u00e4s\u00b7sig", "mich", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$(", "PWAV", "ADJD", "PPER", "TRUNC"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "sch\u00e4tzet ", "tokens": ["sch\u00e4t\u00b7zet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der ergrimmte F\u00fcrst der Erden/ mit weh\u2019/ ach vnd angst", "tokens": ["Der", "er\u00b7grimm\u00b7te", "F\u00fcrst", "der", "Er\u00b7den", "/", "mit", "weh'", "/", "ach", "vnd", "angst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$(", "APPR", "NE", "$(", "XY", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "vnd leidt! ", "tokens": ["vnd", "leidt", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Schawe wie mich itzt vmbh\u00fcllet hat die Nacht der traw-", "tokens": ["Scha\u00b7we", "wie", "mich", "itzt", "vmb\u00b7h\u00fcl\u00b7let", "hat", "die", "Nacht", "der", "tra\u00b7w"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "ADV", "VVPP", "VAFIN", "ART", "NN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "rigkeit ", "tokens": ["rig\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Schawe wie ich in dem Stalle der bedrengnis eingesetzet.", "tokens": ["Scha\u00b7we", "wie", "ich", "in", "dem", "Stal\u00b7le", "der", "be\u00b7dreng\u00b7nis", "ein\u00b7ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wird denn nicht mein bl\u00f6des Hertze durch die s\u00fcsse Frewd-", "tokens": ["Wird", "denn", "nicht", "mein", "bl\u00f6\u00b7des", "Hert\u00b7ze", "durch", "die", "s\u00fcs\u00b7se", "Fre\u00b7wd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "ergetzet ", "tokens": ["er\u00b7get\u00b7zet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die von allen V\u00f6lckern abnimbt/ schrecken/ pein/ vnd", "tokens": ["Die", "von", "al\u00b7len", "V\u00f6l\u00b7ckern", "ab\u00b7nimbt", "/", "schre\u00b7cken", "/", "pein", "/", "vnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$(", "VVINF", "$(", "NE", "$(", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "zwang vnd streitt? ", "tokens": ["zwang", "vnd", "streitt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Werd\u2019 in mir doch new-gebohr\u1ebd. Herr di\u00df ist die rechte zeit.", "tokens": ["Werd'", "in", "mir", "doch", "ne\u00b7wge\u00b7bohr\u1ebd", ".", "Herr", "di\u00df", "ist", "die", "rech\u00b7te", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "NE", "$.", "NN", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Weil die Furcht mich hart-bedr\u00e4ngten hatt bi\u00df auff den todt", "tokens": ["Weil", "die", "Furcht", "mich", "har\u00b7tbe\u00b7dr\u00e4ng\u00b7ten", "hatt", "bi\u00df", "auff", "den", "todt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJA", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "verletzet. ", "tokens": ["ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Vmb mich blitzt der Himmel Flamme/ kaltes zittern f\u00e4lt", "tokens": ["Vmb", "mich", "blitzt", "der", "Him\u00b7mel", "Flam\u00b7me", "/", "kal\u00b7tes", "zit\u00b7tern", "f\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "NN", "$(", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "mich an. ", "tokens": ["mich", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Zeige da\u00df durch deinen Frieden ich nun dem gefallen kan", "tokens": ["Zei\u00b7ge", "da\u00df", "durch", "dei\u00b7nen", "Frie\u00b7den", "ich", "nun", "dem", "ge\u00b7fal\u00b7len", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ART", "VVINF", "VMFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der/ da\u00df Er die Welt erschaffen/ sich so h\u00e4fftig offt be-", "tokens": ["Der", "/", "da\u00df", "Er", "die", "Welt", "er\u00b7schaf\u00b7fen", "/", "sich", "so", "h\u00e4ff\u00b7tig", "offt", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$(", "PRF", "ADV", "ADJD", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "schweret. ", "tokens": ["schwe\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Wol! ich seh\u2019 er ist vers\u00f6hnet singt! jhr Engel schar\u1ebd sing\u2019t", "tokens": ["Wol", "!", "ich", "seh'", "er", "ist", "ver\u00b7s\u00f6h\u00b7net", "singt", "!", "jhr", "En\u00b7gel", "scha\u00b7r\u1ebd", "sing't"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "VVFIN", "$.", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dem sey Ehre/ der vns Frieden/ der vn\u00df Frewde wider", "tokens": ["Dem", "sey", "Eh\u00b7re", "/", "der", "vns", "Frie\u00b7den", "/", "der", "vn\u00df", "Frew\u00b7de", "wi\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$(", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "bringt/ ", "tokens": ["bringt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd den heissen zorn ausleschet/ der wie liechte Glutt ver-", "tokens": ["Vnd", "den", "heis\u00b7sen", "zorn", "aus\u00b7le\u00b7schet", "/", "der", "wie", "liech\u00b7te", "Glutt", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$(", "ART", "KOKOM", "ADJA", "NN", "TRUNC"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "zehret. ", "tokens": ["zeh\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}