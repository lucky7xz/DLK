{"textgrid.poem.61896": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allgewaltig aus Nordosten", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allgewaltig aus Nordosten", "tokens": ["All\u00b7ge\u00b7wal\u00b7tig", "aus", "Nord\u00b7os\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Braust der M\u00e4rzwind \u00fcber Land,", "tokens": ["Braust", "der", "M\u00e4r\u00b7zwind", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es bebt in ihren Pfosten", "tokens": ["Und", "es", "bebt", "in", "ih\u00b7ren", "Pfos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Hauses Giebelwand.", "tokens": ["Mei\u00b7nes", "Hau\u00b7ses", "Gie\u00b7bel\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die Schl\u00f6te mit Gewimmer", "tokens": ["Durch", "die", "Schl\u00f6\u00b7te", "mit", "Ge\u00b7wim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fegt der losgela\u00dfne Hauch,", "tokens": ["Fegt", "der", "los\u00b7ge\u00b7la\u00df\u00b7ne", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcb verzuckt des Herdes Schimmer,", "tokens": ["Tr\u00fcb", "ver\u00b7zuckt", "des", "Her\u00b7des", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Halle f\u00fcllt der Rauch.", "tokens": ["Und", "die", "Hal\u00b7le", "f\u00fcllt", "der", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ziegel prasseln, T\u00fcren schlagen,", "tokens": ["Zie\u00b7gel", "pras\u00b7seln", ",", "T\u00fc\u00b7ren", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrres Astwerk kracht und bricht,", "tokens": ["D\u00fcr\u00b7res", "Ast\u00b7werk", "kracht", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in all das Unbehagen", "tokens": ["Doch", "in", "all", "das", "Un\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelt meine Mus' und spricht:", "tokens": ["L\u00e4\u00b7chelt", "mei\u00b7ne", "Mus'", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbnur getrost! Sich zu erneuen", "tokens": ["\u00bb", "nur", "ge\u00b7trost", "!", "Sich", "zu", "er\u00b7neu\u00b7en"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VVPP", "$.", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringt die Welt im Jugenddrang;", "tokens": ["Ringt", "die", "Welt", "im", "Ju\u00b7gend\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darfst die kurze Not nicht scheuen,", "tokens": ["Darfst", "die", "kur\u00b7ze", "Not", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauh ist jeder \u00dcbergang.", "tokens": ["Rauh", "ist", "je\u00b7der", "\u00dc\u00b7ber\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf den Braus des w\u00fcsten Tages", "tokens": ["Auf", "den", "Braus", "des", "w\u00fcs\u00b7ten", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgt der Lenz im Goldgewand;", "tokens": ["Folgt", "der", "Lenz", "im", "Gold\u00b7ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Merk' es dir, Poet, und sag' es", "tokens": ["Merk'", "es", "dir", ",", "Po\u00b7et", ",", "und", "sag'", "es"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem deutschen Vaterland!\u00ab", "tokens": ["Dei\u00b7nem", "deut\u00b7schen", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}