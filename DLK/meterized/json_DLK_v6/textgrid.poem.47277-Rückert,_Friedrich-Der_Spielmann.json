{"textgrid.poem.47277": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Spielmann", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Spielmann stimmt seine Geigen", "tokens": ["Der", "Spiel\u00b7mann", "stimmt", "sei\u00b7ne", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und spricht zu ihr:", "tokens": ["Und", "spricht", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbdu sollst dein Kunstst\u00fcck zeigen,", "tokens": ["\u00bb", "du", "sollst", "dein", "Kunst\u00b7st\u00fcck", "zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, geh mit mir!\u00ab", "tokens": ["Komm", ",", "geh", "mit", "mir", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Spielmann geht mit ihr vor ein Schlo\u00df;", "tokens": ["Der", "Spiel\u00b7mann", "geht", "mit", "ihr", "vor", "ein", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "'s ist Nacht, der Spielmann fiedelt drauf los.", "tokens": ["'s", "ist", "Nacht", ",", "der", "Spiel\u00b7mann", "fie\u00b7delt", "drauf", "los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Der Spielmann sagt: \u00bb's ist nicht genug,", "tokens": ["Der", "Spiel\u00b7mann", "sagt", ":", "\u00bb", "'s", "ist", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich mu\u00df fiedeln noch einen Zug.\u00ab", "tokens": ["Ich", "mu\u00df", "fie\u00b7deln", "noch", "ei\u00b7nen", "Zug", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Vor dem Schlo\u00df ist ein Garten,", "tokens": ["Vor", "dem", "Schlo\u00df", "ist", "ein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit B\u00e4um' und Pflanzen;", "tokens": ["Mit", "B\u00e4um'", "und", "Pflan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die k\u00f6nnen die Zeit nicht erwarten,", "tokens": ["Die", "k\u00f6n\u00b7nen", "die", "Zeit", "nicht", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu tanzen.", "tokens": ["Zu