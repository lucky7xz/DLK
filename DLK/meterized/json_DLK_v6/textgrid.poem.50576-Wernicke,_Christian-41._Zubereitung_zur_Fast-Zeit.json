{"textgrid.poem.50576": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "41. Zubereitung zur Fast-Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es r\u00fchmt' ein ", "tokens": ["Es", "r\u00fchmt'", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Mit Austern, Smerlen, St\u00f6r, Karussen, Karpen, Kressen,", "tokens": ["Mit", "Aus\u00b7tern", ",", "Smer\u00b7len", ",", "St\u00f6r", ",", "Ka\u00b7rus\u00b7sen", ",", "Kar\u00b7pen", ",", "Kres\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit trucknem Lachs und Ahl', und mehr dergleichen Essen,", "tokens": ["Mit", "truck\u00b7nem", "Lachs", "und", "Ahl'", ",", "und", "mehr", "derg\u00b7lei\u00b7chen", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "KON", "ADV", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich auf die ", "tokens": ["Sich", "auf", "die"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Geschicket, und die K\u00fcch' im Kloster voll gespicket:", "tokens": ["Ge\u00b7schi\u00b7cket", ",", "und", "die", "K\u00fc\u00b7ch'", "im", "Klos\u00b7ter", "voll", "ge\u00b7spi\u00b7cket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}