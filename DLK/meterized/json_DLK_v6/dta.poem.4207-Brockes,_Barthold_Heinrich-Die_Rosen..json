{"dta.poem.4207": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So kann ich denn, GOtt Lob! von neuen", "tokens": ["So", "kann", "ich", "denn", ",", "Gott", "Lob", "!", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "NN", "NN", "$.", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erden sch\u00f6nste Zucht und Zier,", "tokens": ["Der", "Er\u00b7den", "sch\u00f6ns\u00b7te", "Zucht", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen, voller Liebreiz, hier", "tokens": ["Die", "Ro\u00b7sen", ",", "vol\u00b7ler", "Lieb\u00b7reiz", ",", "hier"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer holden R\u00f6hte sehn, und ihrer s\u00fcssen Gluht mich", "tokens": ["In", "ih\u00b7rer", "hol\u00b7den", "R\u00f6h\u00b7te", "sehn", ",", "und", "ih\u00b7rer", "s\u00fcs\u00b7sen", "Gluht", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "KON", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "freuen.", "tokens": ["freu\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "So dacht ich, als mein j\u00fcngstes Kind, zu meinem inni-", "tokens": ["So", "dacht", "ich", ",", "als", "mein", "j\u00fcngs\u00b7tes", "Kind", ",", "zu", "mei\u00b7nem", "in\u00b7ni"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gen Behagen,", "tokens": ["gen", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Auf meinem Schreibe-Tisch vor mir derselben viel zu Hauf", "tokens": ["Auf", "mei\u00b7nem", "Schrei\u00b7be\u00b7Tisch", "vor", "mir", "der\u00b7sel\u00b7ben", "viel", "zu", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "PDS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "getragen.", "tokens": ["ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich setzte mich an diesem Ort", "tokens": ["Ich", "setz\u00b7te", "mich", "an", "die\u00b7sem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesen Fr\u00fchlings-Kindern nieder,", "tokens": ["Bey", "die\u00b7sen", "Fr\u00fch\u00b7lings\u00b7Kin\u00b7dern", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrt\u2019 auf sie die angefangnen Lieder,", "tokens": ["Und", "f\u00fchrt'", "auf", "sie", "die", "an\u00b7ge\u00b7fang\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll innrer Regung, weiter fort:", "tokens": ["Voll", "inn\u00b7rer", "Re\u00b7gung", ",", "wei\u00b7ter", "fort", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus ihren holden Bl\u00e4ttern strahlet,", "tokens": ["Aus", "ih\u00b7ren", "hol\u00b7den", "Bl\u00e4t\u00b7tern", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erhebt sich rings umher und bricht", "tokens": ["Er\u00b7hebt", "sich", "rings", "um\u00b7her", "und", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein fr\u00f6hlichs Feur, ein lieblichs Licht.", "tokens": ["Ein", "fr\u00f6h\u00b7lichs", "Feur", ",", "ein", "lieb\u00b7lichs", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Himmels-Farbe scheinet sie von Fingern der Natur", "tokens": ["Mit", "Him\u00b7mels\u00b7Fa\u00b7rbe", "schei\u00b7net", "sie", "von", "Fin\u00b7gern", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "gemahlet.", "tokens": ["ge\u00b7mah\u00b7let", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Man sieht mit innigem Vergn\u00fcgen,", "tokens": ["Man", "sieht", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Wenn einige beysammen liegen,", "tokens": ["Wenn", "ei\u00b7ni\u00b7ge", "bey\u00b7sam\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVIZU", "VVFIN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nicht nur der sch\u00f6nen C\u00f6rper Pracht;", "tokens": ["Nicht", "nur", "der", "sch\u00f6\u00b7nen", "C\u00f6r\u00b7per", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Etwas scheint sie zu umgeben,", "tokens": ["Ein", "Et\u00b7was", "scheint", "sie", "zu", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das gleichsam unsern Geist anlacht;", "tokens": ["Das", "gleich\u00b7sam", "un\u00b7sern", "Geist", "an\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Schimmer scheint um sie zu schweben,", "tokens": ["Ein", "Schim\u00b7mer", "scheint", "um", "sie", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der sie und ihren Schmuck an sich noch sch\u00f6ner macht.", "tokens": ["Der", "sie", "und", "ih\u00b7ren", "Schmuck", "an", "sich", "noch", "sch\u00f6\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "APPR", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es l\u00e4\u00dft, als wenn was aus ihr steiget,", "tokens": ["Es", "l\u00e4\u00dft", ",", "als", "wenn", "was", "aus", "ihr", "stei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sich verbirgt, und dennoch zeiget.", "tokens": ["Das", "sich", "ver\u00b7birgt", ",", "und", "den\u00b7noch", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So wie man um ein Licht, das brennet,", "tokens": ["So", "wie", "man", "um", "ein", "Licht", ",", "das", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft illuminirt, entz\u00fcndet,", "tokens": ["Die", "Luft", "il\u00b7lu\u00b7mi\u00b7nirt", ",", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durchstrahlet und erleuchtet, findet,", "tokens": ["Durch\u00b7strah\u00b7let", "und", "er\u00b7leuch\u00b7tet", ",", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch den Schimmer nicht erkennet;", "tokens": ["Und", "doch", "den", "Schim\u00b7mer", "nicht", "er\u00b7ken\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So scheints, als ob ein r\u00f6htlich Licht", "tokens": ["So", "scheints", ",", "als", "ob", "ein", "r\u00f6ht\u00b7lich", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus sch\u00f6nen Bl\u00e4tter-reichen Rosen,", "tokens": ["Aus", "sch\u00f6\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um Geist und Auge liebzukosen,", "tokens": ["Um", "Geist", "und", "Au\u00b7ge", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Halb sicht- und halb unsichtbar, bricht.", "tokens": ["Halb", "sicht", "und", "halb", "un\u00b7sicht\u00b7bar", ",", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "TRUNC", "KON", "ADJD", "ADJD", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Die\u00df mischt sich mit dem s\u00fcssen Duft,", "tokens": ["Die\u00df", "mischt", "sich", "mit", "dem", "s\u00fcs\u00b7sen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der gleicher Weise nicht zu sehen,", "tokens": ["Der", "glei\u00b7cher", "Wei\u00b7se", "nicht", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und wovon dennoch in die Luft", "tokens": ["Und", "wo\u00b7von", "den\u00b7noch", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich unauf h\u00f6rlich Theil\u2019 erh\u00f6hen.", "tokens": ["Sich", "un\u00b7auf", "h\u00f6r\u00b7lich", "Theil'", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr Kugel-f\u00f6rmiges Geb\u00e4ude, von Bl\u00e4ttern wunder-", "tokens": ["Ihr", "Ku\u00b7gel\u00b7f\u00f6r\u00b7mi\u00b7ges", "Ge\u00b7b\u00e4u\u00b7de", ",", "von", "Bl\u00e4t\u00b7tern", "wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "barlich vest,", "tokens": ["bar\u00b7lich", "vest", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In einem solchen engen Raum", "tokens": ["In", "ei\u00b7nem", "sol\u00b7chen", "en\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vereint, verschr\u00e4nket und gepre\u00dft,", "tokens": ["Ver\u00b7eint", ",", "ver\u00b7schr\u00e4n\u00b7ket", "und", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist \u00f6fters am Gewicht so schwehr,", "tokens": ["Ist", "\u00f6f\u00b7ters", "am", "Ge\u00b7wicht", "so", "schwehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es der starre Stengel kaum,", "tokens": ["Da\u00df", "es", "der", "star\u00b7re", "Sten\u00b7gel", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wie es scheinet, tragen kann, ist er gleich von besondrer", "tokens": ["So", "wie", "es", "schei\u00b7net", ",", "tra\u00b7gen", "kann", ",", "ist", "er", "gleich", "von", "be\u00b7sond\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "St\u00e4rke,", "tokens": ["St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "(so ich zu gleicher Zeit bemerke.)", "tokens": ["(", "so", "ich", "zu", "glei\u00b7cher", "Zeit", "be\u00b7mer\u00b7ke", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Wenn man nun diese s\u00fcsse Last", "tokens": ["Wenn", "man", "nun", "die\u00b7se", "s\u00fcs\u00b7se", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit spitzen Fingern angefa\u00dft,", "tokens": ["Mit", "spit\u00b7zen", "Fin\u00b7gern", "an\u00b7ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und schwebend vor die Nase h\u00e4lt,", "tokens": ["Und", "schwe\u00b7bend", "vor", "die", "Na\u00b7se", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So, da\u00df sich beyde kaum ber\u00fchren;", "tokens": ["So", ",", "da\u00df", "sich", "bey\u00b7de", "kaum", "be\u00b7r\u00fch\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So ist auf dieser ganzen Welt", "tokens": ["So", "ist", "auf", "die\u00b7ser", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Fast nichts so liebliches zu sp\u00