{"dta.poem.23826": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ludovicus Pius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Weil Ludwigs Mildigkeit die Kirchen wohl verpflegt/", "tokens": ["Weil", "Lud\u00b7wigs", "Mil\u00b7dig\u00b7keit", "die", "Kir\u00b7chen", "wohl", "ver\u00b7pflegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird billig ihm das Lob des Frommen beygelegt/", "tokens": ["Wird", "bil\u00b7lig", "ihm", "das", "Lob", "des", "From\u00b7men", "bey\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Vater folgt er nach in allen seinen Reichen/", "tokens": ["Dem", "Va\u00b7ter", "folgt", "er", "nach", "in", "al\u00b7len", "sei\u00b7nen", "Rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df aber eh\u2019 er stirbt/ noch seinen Kindern weichen.", "tokens": ["Mu\u00df", "a\u00b7ber", "eh'", "er", "stirbt", "/", "noch", "sei\u00b7nen", "Kin\u00b7dern", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "