{"textgrid.poem.33462": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Das Vorstadtkabarett", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschwei\u00dfte Kellnerk\u00f6pfe ragen in dem Saal", "tokens": ["Ver\u00b7schwei\u00df\u00b7te", "Kell\u00b7ner\u00b7k\u00f6p\u00b7fe", "ra\u00b7gen", "in", "dem", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie S\u00e4ulenspitzen hoch und \u00fcberm\u00e4chtig.", "tokens": ["Wie", "S\u00e4u\u00b7len\u00b7spit\u00b7zen", "hoch", "und", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlauste Burschen kichern niedertr\u00e4chtig,", "tokens": ["Ver\u00b7laus\u00b7te", "Bur\u00b7schen", "ki\u00b7chern", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und helle M\u00e4dchen blicken h\u00fcbsch brutal.", "tokens": ["Und", "hel\u00b7le", "M\u00e4d\u00b7chen", "bli\u00b7cken", "h\u00fcbsch", "bru\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ferne Frauen sind so sehr erregt ...", "tokens": ["Und", "fer\u00b7ne", "Frau\u00b7en", "sind", "so", "sehr", "er\u00b7regt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie haben hundert rote runde H\u00e4nde,", "tokens": ["Sie", "ha\u00b7ben", "hun\u00b7dert", "ro\u00b7te", "run\u00b7de", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geb\u00e4rdelose, gro\u00dfe, ohne Ende", "tokens": ["Ge\u00b7b\u00e4r\u00b7de\u00b7lo\u00b7se", ",", "gro\u00b7\u00dfe", ",", "oh\u00b7ne", "En\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihren hohen bunten Bauch gelegt.", "tokens": ["Um", "ih\u00b7ren", "ho\u00b7hen", "bun\u00b7ten", "Bauch", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die meisten Menschen trinken gelbes Bier.", "tokens": ["Die", "meis\u00b7ten", "Men\u00b7schen", "trin\u00b7ken", "gel\u00b7bes", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verrauchte Kr\u00e4mer glotzen grau und bieder.", "tokens": ["Ver\u00b7rauch\u00b7te", "Kr\u00e4\u00b7mer", "glot\u00b7zen", "grau", "und", "bie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein feines Fr\u00e4ulein singt gemeine Lieder.", "tokens": ["Ein", "fei\u00b7nes", "Fr\u00e4u\u00b7lein", "singt", "ge\u00b7mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein junger Jude spielt ganz gern Klavier.", "tokens": ["Ein", "jun\u00b7ger", "Ju\u00b7de", "spielt", "ganz", "gern", "Kla\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}