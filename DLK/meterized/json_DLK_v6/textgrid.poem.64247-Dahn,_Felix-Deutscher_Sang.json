{"textgrid.poem.64247": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Sang", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem deutschen Volk hat Gott gegeben", "tokens": ["Dem", "deut\u00b7schen", "Volk", "hat", "Gott", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Harfenspiel von reichstem Klang,", "tokens": ["Ein", "Har\u00b7fen\u00b7spiel", "von", "reichs\u00b7tem", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Ruh'n und Ringen, Tod und Leben", "tokens": ["Da\u00df", "Ruh'n", "und", "Rin\u00b7gen", ",", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns weihend schm\u00fccke der Gesang.", "tokens": ["Uns", "wei\u00b7hend", "schm\u00fc\u00b7cke", "der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So singe denn, du deutsche Jugend,", "tokens": ["So", "sin\u00b7ge", "denn", ",", "du", "deut\u00b7sche", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von allem, was das Herz dir schwellt:", "tokens": ["Von", "al\u00b7lem", ",", "was", "das", "Herz", "dir", "schwellt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Frauensch\u00f6ne, Mannestugend,", "tokens": ["Von", "Frau\u00b7en\u00b7sch\u00f6\u00b7ne", ",", "Man\u00b7nes\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von freud'ger Herrlichkeit der Welt:", "tokens": ["Von", "freu\u00b7d'\u00b7ger", "Herr\u00b7lich\u00b7keit", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von wahrer Liebe ew'ger Dauer,", "tokens": ["Von", "wah\u00b7rer", "Lie\u00b7be", "ew'\u00b7ger", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von echter Freundschaft Gold und Erz,", "tokens": ["Von", "ech\u00b7ter", "Freund\u00b7schaft", "Gold", "und", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von frommer Ahnung heil'gem Schauer,", "tokens": ["Von", "from\u00b7mer", "Ah\u00b7nung", "heil'\u00b7gem", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ew'gen Sehnens Gl\u00fcck und Schmerz:", "tokens": ["Von", "ew'\u00b7gen", "Seh\u00b7nens", "Gl\u00fcck", "und", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von Fr\u00fchlingsglanz, von Waldeswonne,", "tokens": ["Von", "Fr\u00fch\u00b7lings\u00b7glanz", ",", "von", "Wal\u00b7des\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wanderlust Land aus, Land ein,", "tokens": ["Von", "Wan\u00b7der\u00b7lust", "Land", "aus", ",", "Land", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und von dem Lieblingssohn der Sonne,", "tokens": ["Und", "von", "dem", "Lieb\u00b7lings\u00b7sohn", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Verge\u00dft ihn nicht! \u2013 vom goldnen Wein.", "tokens": ["\u2013", "Ver\u00b7ge\u00dft", "ihn", "nicht", "!", "\u2013", "vom", "gold\u00b7nen", "Wein", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, singt von allem Hohen, Sch\u00f6nen! \u2013", "tokens": ["Ja", ",", "singt", "von", "al\u00b7lem", "Ho\u00b7hen", ",", "Sch\u00f6\u00b7nen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "PIS", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Eines Sanges pflegt zumeist,", "tokens": ["Doch", "Ei\u00b7nes", "San\u00b7ges", "pflegt", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begeisternd, brausend soll er t\u00f6nen:", "tokens": ["Be\u00b7geis\u00b7ternd", ",", "brau\u00b7send", "soll", "er", "t\u00f6\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sang vom deutschen Heldengeist!", "tokens": ["Der", "Sang", "vom", "deut\u00b7schen", "Hel\u00b7den\u00b7geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Lied von Mannespflicht und Ehre,", "tokens": ["Das", "Lied", "von", "Man\u00b7nes\u00b7pflicht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Treue, die kein Schrecken zwingt,", "tokens": ["Von", "Treu\u00b7e", ",", "die", "kein", "Schre\u00b7cken", "zwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die jauchzend in der Feinde Speere,", "tokens": ["Die", "jauch\u00b7zend", "in", "der", "Fein\u00b7de", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tod den Sieg erk\u00e4mpfend, springt!", "tokens": ["Im", "Tod", "den", "Sieg", "er\u00b7k\u00e4mp\u00b7fend", ",", "springt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur wer da sterben will wie leben", "tokens": ["Nur", "wer", "da", "ster\u00b7ben", "will", "wie", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADV", "VVINF", "VMFIN", "KOKOM", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dieses Lied, dem keines gleich, \u2013", "tokens": ["F\u00fcr", "die\u00b7ses", "Lied", ",", "dem", "kei\u00b7nes", "gleich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PIS", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur der ist wert es anzuheben,", "tokens": ["Nur", "der", "ist", "wert", "es", "an\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied vom Kaiser und vom Reich!", "tokens": ["Das", "Lied", "vom", "Kai\u00b7ser", "und", "vom", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}