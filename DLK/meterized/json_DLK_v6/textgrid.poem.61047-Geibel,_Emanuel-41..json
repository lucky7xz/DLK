{"textgrid.poem.61047": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, wie's geschieht,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie's", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df, was mein Herz auch singt,", "tokens": ["Da\u00df", ",", "was", "mein", "Herz", "auch", "singt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir immerdar ins Lied", "tokens": ["Mir", "im\u00b7mer\u00b7dar", "ins", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Klang der Liebe klingt;", "tokens": ["Ein", "Klang", "der", "Lie\u00b7be", "klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich nicht schweigen kann", "tokens": ["Da\u00df", "ich", "nicht", "schwei\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von ihrem Paradies,", "tokens": ["Von", "ih\u00b7rem", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wiewohl aus seinem Bann", "tokens": ["Wie\u00b7wohl", "aus", "sei\u00b7nem", "Bann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man lange mich verstie\u00df.", "tokens": ["Man", "lan\u00b7ge", "mich", "ver\u00b7stie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann ahn' ich selber kaum:", "tokens": ["Dann", "ahn'", "ich", "sel\u00b7ber", "kaum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sing' ich von k\u00fcnft'gem Gl\u00fcck?", "tokens": ["Sing'", "ich", "von", "k\u00fcnft'\u00b7gem", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sing' ich den s\u00fc\u00dfen Traum", "tokens": ["Sing'", "ich", "den", "s\u00fc\u00b7\u00dfen", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Jugend mir zur\u00fcck?", "tokens": ["Der", "Ju\u00b7gend", "mir", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}