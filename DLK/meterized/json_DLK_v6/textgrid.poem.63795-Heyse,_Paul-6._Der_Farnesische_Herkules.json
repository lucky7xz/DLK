{"textgrid.poem.63795": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Der Farnesische Herkules", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein schwellend Gebirge von Fleisch und Muskeln! Am Kopf nur", "tokens": ["Welch", "ein", "schwel\u00b7lend", "Ge\u00b7bir\u00b7ge", "von", "Fleisch", "und", "Mus\u00b7keln", "!", "Am", "Kopf", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$.", "APPRART", "NN", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Kam er ein wenig zu kurz; enge sind Sch\u00e4del und Stirn.", "tokens": ["Kam", "er", "ein", "we\u00b7nig", "zu", "kurz", ";", "en\u00b7ge", "sind", "Sch\u00e4\u00b7del", "und", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "PIS", "PTKA", "ADJD", "$.", "ADJA", "VAFIN", "NE", "KON", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch so schuf ihn Natur mit Bedacht; ein Kl\u00fcgerer h\u00e4tte", "tokens": ["Doch", "so", "schuf", "ihn", "Na\u00b7tur", "mit", "Be\u00b7dacht", ";", "ein", "Kl\u00fc\u00b7ge\u00b7rer", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So fruchtlosem Gesch\u00e4ft schwerlich das Leben geweiht,", "tokens": ["So", "frucht\u00b7lo\u00b7sem", "Ge\u00b7sch\u00e4ft", "schwer\u00b7lich", "das", "Le\u00b7ben", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Nicht vom Schmutze ges\u00e4ubert die Welt, von w\u00fcstem Geziefer,", "tokens": ["Nicht", "vom", "Schmut\u00b7ze", "ge\u00b7s\u00e4u\u00b7bert", "die", "Welt", ",", "von", "w\u00fcs\u00b7tem", "Ge\u00b7zie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Noch prometheischen Trotz rettend vom Geier befreit.", "tokens": ["Noch", "pro\u00b7me\u00b7thei\u00b7schen", "Trotz", "ret\u00b7tend", "vom", "Gei\u00b7er", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Aber erkennst du denn nicht, halbg\u00f6ttischer Tor: des Augias", "tokens": ["A\u00b7ber", "er\u00b7kennst", "du", "denn", "nicht", ",", "halb\u00b7g\u00f6t\u00b7ti\u00b7scher", "Tor", ":", "des", "Au\u00b7gias"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADJA", "NN", "$.", "ART", "NN"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Stall f\u00fcllt wieder sich an, wieder erg\u00e4nzt sich die Zahl", "tokens": ["Stall", "f\u00fcllt", "wie\u00b7der", "sich", "an", ",", "wie\u00b7der", "er\u00b7g\u00e4nzt", "sich", "die", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PRF", "PTKVZ", "$,", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Grimmiger Hydrah\u00e4upter; es kreischen die Stymphaliden,", "tokens": ["Grim\u00b7mi\u00b7ger", "Hyd\u00b7ra\u00b7h\u00e4up\u00b7ter", ";", "es", "krei\u00b7schen", "die", "Stymp\u00b7ha\u00b7li\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Kraft und Gewalt aufs neu' schmieden in Bande den Geist.", "tokens": ["Kraft", "und", "Ge\u00b7walt", "aufs", "neu'", "schmie\u00b7den", "in", "Ban\u00b7de", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "ADJA", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Darum senkst du nun freilich das Haupt in zweifelnder Schwermut;", "tokens": ["Da\u00b7rum", "senkst", "du", "nun", "frei\u00b7lich", "das", "Haupt", "in", "zwei\u00b7feln\u00b7der", "Schwer\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Doch nicht g\u00e4nzlich umsonst hast du die Kr\u00e4fte bew\u00e4hrt.", "tokens": ["Doch", "nicht", "g\u00e4nz\u00b7lich", "um\u00b7sonst", "hast", "du", "die", "Kr\u00e4f\u00b7te", "be\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Gl\u00fcck bei Weibern tr\u00e4gt es dir ein; es liebten die sch\u00f6nen", "tokens": ["Gl\u00fcck", "bei", "Wei\u00b7bern", "tr\u00e4gt", "es", "dir", "ein", ";", "es", "lieb\u00b7ten", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Seelen sogar von je diesen athletischen Wuchs.", "tokens": ["See\u00b7len", "so\u00b7gar", "von", "je", "die\u00b7sen", "ath\u00b7le\u00b7ti\u00b7schen", "Wuchs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Mit so geringem Verstande gepaart, und Omphale setzt auf", "tokens": ["Mit", "so", "ge\u00b7rin\u00b7gem", "Ver\u00b7stan\u00b7de", "ge\u00b7paart", ",", "und", "Om\u00b7pha\u00b7le", "setzt", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$,", "KON", "NN", "VVFIN", "APPR"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Solch stiernackigen Freund gerne den z\u00e4rtlichen Fu\u00df.", "tokens": ["Solch", "stier\u00b7na\u00b7cki\u00b7gen", "Freund", "ger\u00b7ne", "den", "z\u00e4rt\u00b7li\u00b7chen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Ja, im Olymp, wo Hebe, die Zierlichschwebende furchtlos", "tokens": ["Ja", ",", "im", "O\u00b7lymp", ",", "wo", "He\u00b7be", ",", "die", "Zier\u00b7lich\u00b7schwe\u00b7ben\u00b7de", "furcht\u00b7los"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "$,", "PWAV", "NE", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Dir in die schwielige Faust br\u00e4utlich ihr Patschchen gelegt,", "tokens": ["Dir", "in", "die", "schwie\u00b7li\u00b7ge", "Faust", "br\u00e4ut\u00b7lich", "ihr", "Pat\u00b7schchen", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Stiftest du Zwietracht fast. An ihrem gewaltigen Kriegsgott", "tokens": ["Stif\u00b7test", "du", "Zwiet\u00b7racht", "fast", ".", "An", "ih\u00b7rem", "ge\u00b7wal\u00b7ti\u00b7gen", "Kriegs\u00b7gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Schielt nun Venus vorbei, neidet der Kleinen ihr Gl\u00fcck.", "tokens": ["Schielt", "nun", "Ve\u00b7nus", "vor\u00b7bei", ",", "nei\u00b7det", "der", "Klei\u00b7nen", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Fast wird eifers\u00fcchtig der Vater der Menschen und G\u00f6tter,", "tokens": ["Fast", "wird", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "der", "Va\u00b7ter", "der", "Men\u00b7schen", "und", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.22": {"text": "Da leutseligen Blicks Juno den Neuling begr\u00fc\u00dft.", "tokens": ["Da", "leut\u00b7se\u00b7li\u00b7gen", "Blicks", "Ju\u00b7no", "den", "Neu\u00b7ling", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "Nur die Grazien fl\u00fcchten entsetzt; es r\u00fcmpfet Minerva", "tokens": ["Nur", "die", "Gra\u00b7zi\u00b7en", "fl\u00fcch\u00b7ten", "ent\u00b7setzt", ";", "es", "r\u00fcmp\u00b7fet", "Mi\u00b7ner\u00b7va"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVPP", "$.", "PPER", "VVFIN", "NE"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.24": {"text": "H\u00f6hnisch die Lippe: \u00bbWarum lie\u00df man den Hausknecht herein?\u00ab", "tokens": ["H\u00f6h\u00b7nisch", "die", "Lip\u00b7pe", ":", "\u00bb", "Wa\u00b7rum", "lie\u00df", "man", "den", "Haus\u00b7knecht", "her\u00b7ein", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "PWAV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}