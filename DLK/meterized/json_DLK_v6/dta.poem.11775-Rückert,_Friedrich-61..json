{"dta.poem.11775": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4ssest billig dir dein eignes Gut gefallen,", "tokens": ["Du", "l\u00e4s\u00b7sest", "bil\u00b7lig", "dir", "dein", "eig\u00b7nes", "Gut", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nicht ruhmredig mu\u00dft du es anpreisen allen.", "tokens": ["Doch", "nicht", "ruhm\u00b7re\u00b7dig", "mu\u00dft", "du", "es", "an\u00b7prei\u00b7sen", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VMFIN", "PPER", "PPER", "VVFIN", "PIS", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "So lob' im Stillen dir dein Weib auch, das ist gut,", "tokens": ["So", "lob'", "im", "Stil\u00b7len", "dir", "dein", "Weib", "auch", ",", "das", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "ADV", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht andern! es ist auch ein St\u00fcck von deinem Gut.", "tokens": ["Nicht", "an\u00b7dern", "!", "es", "ist", "auch", "ein", "St\u00fcck", "von", "dei\u00b7nem", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Hauptst\u00fcck deines Guts, dein h\u00f6chstes Gut mit Recht;", "tokens": ["Ein", "Haupt\u00b7st\u00fcck", "dei\u00b7nes", "Guts", ",", "dein", "h\u00f6chs\u00b7tes", "Gut", "mit", "Recht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des freue dich als Mann, und bet's nicht an als Knecht!", "tokens": ["Des", "freu\u00b7e", "dich", "als", "Mann", ",", "und", "bet's", "nicht", "an", "als", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "KOUS", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}