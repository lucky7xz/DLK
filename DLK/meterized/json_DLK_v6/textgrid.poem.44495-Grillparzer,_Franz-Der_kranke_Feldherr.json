{"textgrid.poem.44495": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der kranke Feldherr", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist verwundet, tragt ihn aus der Schlacht!", "tokens": ["Er", "ist", "ver\u00b7wun\u00b7det", ",", "tragt", "ihn", "aus", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein tapfrer K\u00e4mpe wars, ein k\u00fchner F\u00fchrer,", "tokens": ["Ein", "tapf\u00b7rer", "K\u00e4m\u00b7pe", "wars", ",", "ein", "k\u00fch\u00b7ner", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der vorfocht in der Finsterlinge Schar.", "tokens": ["Der", "vor\u00b7focht", "in", "der", "Fins\u00b7ter\u00b7lin\u00b7ge", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun aber traf ein Pfeil des Lichtgotts ihn", "tokens": ["Nun", "a\u00b7ber", "traf", "ein", "Pfeil", "des", "Licht\u00b7gotts", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fuhr mit Macht hindurch, bis dahin, wo,", "tokens": ["Und", "fuhr", "mit", "Macht", "hin\u00b7durch", ",", "bis", "da\u00b7hin", ",", "wo", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PAV", "$,", "KON", "PAV", "$,", "PWAV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Tief unter Herz und Brust, sich Leber, Milz", "tokens": ["Tief", "un\u00b7ter", "Herz", "und", "Brust", ",", "sich", "Le\u00b7ber", ",", "Milz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,", "PRF", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Magen, Galle, Nieren, tierscher Greul,", "tokens": ["Und", "Ma\u00b7gen", ",", "Gal\u00b7le", ",", "Nie\u00b7ren", ",", "tier\u00b7scher", "Greul", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und doch der Sitz des Lebens solcher Herrn", "tokens": ["Und", "doch", "der", "Sitz", "des", "Le\u00b7bens", "sol\u00b7cher", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit schicksalsschwangern Windungen begegnen.", "tokens": ["Mit", "schick\u00b7sals\u00b7schwan\u00b7gern", "Win\u00b7dun\u00b7gen", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Pfeil jedoch, der ihn ins Leben traf,", "tokens": ["Der", "Pfeil", "je\u00b7doch", ",", "der", "ihn", "ins", "Le\u00b7ben", "traf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war die Botschaft, da\u00df der Legitimen einer,", "tokens": ["Es", "war", "die", "Bot\u00b7schaft", ",", "da\u00df", "der", "Le\u00b7gi\u00b7ti\u00b7men", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kopfabschneider Mahmud, Tods verblichen,", "tokens": ["Der", "Ko\u00b7pfab\u00b7schnei\u00b7der", "Mah\u00b7mud", ",", "Tods", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nun ein anderer der Legitimen,", "tokens": ["Und", "nun", "ein", "an\u00b7de\u00b7rer", "der", "Le\u00b7gi\u00b7ti\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Polenw\u00fcrger Nikolaus, gewillt,", "tokens": ["Der", "Po\u00b7len\u00b7w\u00fcr\u00b7ger", "Ni\u00b7ko\u00b7laus", ",", "ge\u00b7willt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kraft seines alt von Gott entspro\u00dfnen Rechts,", "tokens": ["Kraft", "sei\u00b7nes", "alt", "von", "Gott", "ent\u00b7spro\u00df\u00b7nen", "Rechts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu stehlen, was der T\u00fcrk vor Jahren stahl.", "tokens": ["Zu", "steh\u00b7len", ",", "was", "der", "T\u00fcrk", "vor", "Jah\u00b7ren", "stahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das fuhr dem Mann, der, weil vom Wind geschwellt,", "tokens": ["Das", "fuhr", "dem", "Mann", ",", "der", ",", "weil", "vom", "Wind", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich f\u00fcr das Segel hielt des Schiffes dieser Welt,", "tokens": ["Sich", "f\u00fcr", "das", "Se\u00b7gel", "hielt", "des", "Schif\u00b7fes", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der seine Kraft, sein Schwert, durch Spitze, Schleifen", "tokens": ["Der", "sei\u00b7ne", "Kraft", ",", "sein", "Schwert", ",", "durch", "Spit\u00b7ze", ",", "Schlei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bis zu des Fadens D\u00fcnnheit abgenutzt", "tokens": ["Bis", "zu", "des", "Fa\u00b7dens", "D\u00fcnn\u00b7heit", "ab\u00b7ge\u00b7nutzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und machtlos stand der Macht nun gegen\u00fcber,", "tokens": ["Und", "macht\u00b7los", "stand", "der", "Macht", "nun", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das fuhr ihm wie ein Blitzstrahl durchs Gehirn,", "tokens": ["Das", "fuhr", "ihm", "wie", "ein", "Blitz\u00b7strahl", "durchs", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und warf ihn nieder, wo er annoch liegt.", "tokens": ["Und", "warf", "ihn", "nie\u00b7der", ",", "wo", "er", "an\u00b7noch", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00dft ihn betrachten uns: Ein feiner Mann!", "tokens": ["La\u00dft", "ihn", "be\u00b7trach\u00b7ten", "uns", ":", "Ein", "fei\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die hohe Stirn, sie barg gewi\u00df Verstand,", "tokens": ["Die", "ho\u00b7he", "Stirn", ",", "sie", "barg", "ge\u00b7wi\u00df", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist Verstand ein doppeldeutig Ding,", "tokens": ["Doch", "ist", "Ver\u00b7stand", "ein", "dop\u00b7pel\u00b7deu\u00b7tig", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Diener, der nur gut durch seinen Herrn.", "tokens": ["Ein", "Die\u00b7ner", ",", "der", "nur", "gut", "durch", "sei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist der nun, der gebeut, kein reiner Wille,", "tokens": ["Ist", "der", "nun", ",", "der", "ge\u00b7beut", ",", "kein", "rei\u00b7ner", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "$,", "PRELS", "VVFIN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein richtger Sinn, der Pfad und Wege weist,", "tokens": ["Kein", "richt\u00b7ger", "Sinn", ",", "der", "Pfad", "und", "We\u00b7ge", "weist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "D\u00fcnkt ihm sein Ziel Erkl\u00fcgeln, statt Erkennen,", "tokens": ["D\u00fcnkt", "ihm", "sein", "Ziel", "Er\u00b7kl\u00fc\u00b7geln", ",", "statt", "Er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "M\u00f6gt ihr ihn Fluch und keine Gabe nennen.", "tokens": ["M\u00f6gt", "ihr", "ihn", "Fluch", "und", "kei\u00b7ne", "Ga\u00b7be", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und auch ein Herz, es spricht aus diesen Z\u00fcgen.", "tokens": ["Und", "auch", "ein", "Herz", ",", "es", "spricht", "aus", "die\u00b7sen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der war nicht taub f\u00fcr seines N\u00e4chsten Leid!", "tokens": ["Der", "war", "nicht", "taub", "f\u00fcr", "sei\u00b7nes", "N\u00e4chs\u00b7ten", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn anders nicht der Stolz, die Eitelkeit,", "tokens": ["Wenn", "an\u00b7ders", "nicht", "der", "Stolz", ",", "die", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gelagert in den hochgezognen Brauen,", "tokens": ["Ge\u00b7la\u00b7gert", "in", "den", "hoch\u00b7ge\u00b7zog\u00b7nen", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verschlossen seines F\u00fchlens weiches Ohr,", "tokens": ["Ver\u00b7schlos\u00b7sen", "sei\u00b7nes", "F\u00fch\u00b7lens", "wei\u00b7ches", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn bannten in des Hochmuts stumme Nacht.", "tokens": ["Ihn", "bann\u00b7ten", "in", "des", "Hoch\u00b7muts", "stum\u00b7me", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O, ewger Fluch bevorzugter Naturen,", "tokens": ["O", ",", "ew\u00b7ger", "Fluch", "be\u00b7vor\u00b7zug\u00b7ter", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bevorzugt als begabt, als hochgestellt,", "tokens": ["Be\u00b7vor\u00b7zugt", "als", "be\u00b7gabt", ",", "als", "hoch\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Statt auf betretnem V\u00f6lkerweg voran,", "tokens": ["Statt", "auf", "be\u00b7tret\u00b7nem", "V\u00f6l\u00b7ker\u00b7weg", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf launisch-ausgew\u00e4hlt, einsamer Bahn", "tokens": ["Auf", "lau\u00b7nischaus\u00b7ge\u00b7w\u00e4hlt", ",", "ein\u00b7sa\u00b7mer", "Bahn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Zu suchen, was der Welt gemeinsam frommt.", "tokens": ["Zu", "su\u00b7chen", ",", "was", "der", "Welt", "ge\u00b7mein\u00b7sam", "frommt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Beim Anfang t\u00f6nen noch verwandte Stimmen,", "tokens": ["Beim", "An\u00b7fang", "t\u00f6\u00b7nen", "noch", "ver\u00b7wand\u00b7te", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mahnende Leiter aus der n\u00e4chsten N\u00e4he,", "tokens": ["Mah\u00b7nen\u00b7de", "Lei\u00b7ter", "aus", "der", "n\u00e4chs\u00b7ten", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Doch immer weiter abseits geht der Pfad", "tokens": ["Doch", "im\u00b7mer", "wei\u00b7ter", "ab\u00b7seits", "geht", "der", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Durch Dickicht und Geb\u00fcsch. Mit sich allein,", "tokens": ["Durch", "Di\u00b7ckicht", "und", "Ge\u00b7b\u00fcsch", ".", "Mit", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "PRF", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Hat der Gedanke keinen Ma\u00dfstab mehr", "tokens": ["Hat", "der", "Ge\u00b7dan\u00b7ke", "kei\u00b7nen"