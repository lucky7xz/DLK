{"textgrid.poem.30547": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bonosus ist ein St\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bonosus ist ein St\u00fccke", "tokens": ["Bo\u00b7no\u00b7sus", "ist", "ein", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In grosser L\u00e4ng und Dicke;", "tokens": ["In", "gros\u00b7ser", "L\u00e4ng", "und", "Di\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ladet man mit Speise", "tokens": ["Das", "la\u00b7det", "man", "mit", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gemeinlich Centner-weise;", "tokens": ["Ge\u00b7mein\u00b7lich", "Cent\u00b7ner\u00b7wei\u00b7se", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "St\u00f6st Wein mit Wein zusammen,", "tokens": ["St\u00f6st", "Wein", "mit", "Wein", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So speit es dicke Flammen;", "tokens": ["So", "speit", "es", "di\u00b7cke", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zwar niemand wird gekr\u00e4ncket,", "tokens": ["Zwar", "nie\u00b7mand", "wird", "ge\u00b7kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur j\u00e4mmerlich best\u00e4ncket.", "tokens": ["Nur", "j\u00e4m\u00b7mer\u00b7lich", "be\u00b7st\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}