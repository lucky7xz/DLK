{"textgrid.poem.47896": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die besten der Westen", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die besten der Westen", "tokens": ["Die", "bes\u00b7ten", "der", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die Felder/ die W\u00e4lder jhr Laubwerck verneuen/", "tokens": ["die", "Fel\u00b7der", "/", "die", "W\u00e4l\u00b7der", "jhr", "Laub\u00b7werck", "ver\u00b7neu\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "die Auen behauen die gehen zu heuen/", "tokens": ["die", "Au\u00b7en", "be\u00b7hau\u00b7en", "die", "ge\u00b7hen", "zu", "heu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "die Pferde der Erde Gew\u00e4chse zerkeuen/", "tokens": ["die", "Pfer\u00b7de", "der", "Er\u00b7de", "Ge\u00b7w\u00e4ch\u00b7se", "zer\u00b7keu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Gartenkinder gl\u00e4ntzen", "tokens": ["Die", "Gar\u00b7ten\u00b7kin\u00b7der", "gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "in bunten Sommerrock/", "tokens": ["in", "bun\u00b7ten", "Som\u00b7mer\u00b7rock", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "die Traubenbl\u00fct im Lentzen", "tokens": ["die", "Trau\u00b7ben\u00b7bl\u00fct", "im", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "belobet ihren Stock.", "tokens": ["be\u00b7lo\u00b7bet", "ih\u00b7ren", "Stock", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00e4tter von Wetter mit rauschen laut schallen/", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "von", "Wet\u00b7ter", "mit", "rau\u00b7schen", "laut", "schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gesch\u00fctze mit Blitze die L\u00fcffte durchknallen/", "tokens": ["Ge\u00b7sch\u00fct\u00b7ze", "mit", "Blit\u00b7ze", "die", "L\u00fcff\u00b7te", "durch\u00b7knal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "die ruffenden/ puffenden Paucken erhallen/", "tokens": ["die", "ruf\u00b7fen\u00b7den", "/", "puf\u00b7fen\u00b7den", "Pau\u00b7cken", "er\u00b7hal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Posaunen/ Carthaunen in L\u00e4geren schallen/", "tokens": ["Po\u00b7sau\u00b7nen", "/", "Car\u00b7thau\u00b7nen", "in", "L\u00e4\u00b7ge\u00b7ren", "schal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Man r\u00fcstet sich zum schlagen/", "tokens": ["Man", "r\u00fcs\u00b7tet", "sich", "zum", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "verschantzet sich im Feld/", "tokens": ["ver\u00b7schant\u00b7zet", "sich", "im", "Feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "eh den geladnen Wagen/", "tokens": ["eh", "den", "ge\u00b7lad\u00b7nen", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "die glasebr\u00fccke helt.", "tokens": ["die", "gla\u00b7se\u00b7br\u00fc\u00b7cke", "helt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt singen/ h\u00f6rt klingen die Englischen Machten/", "tokens": ["H\u00f6rt", "sin\u00b7gen", "/", "h\u00f6rt", "klin\u00b7gen", "die", "Eng\u00b7li\u00b7schen", "Mach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$(", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "wie mutig/ wie blutig nach Schlachten sie trachten/", "tokens": ["wie", "mu\u00b7tig", "/", "wie", "blu\u00b7tig", "nach", "Schlach\u00b7ten", "sie", "trach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "PWAV", "ADJD", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie d\u00fcrften/ die F\u00fcrsten der Teuffel nichts achten/", "tokens": ["Sie", "d\u00fcrf\u00b7ten", "/", "die", "F\u00fcrs\u00b7ten", "der", "Teuf\u00b7fel", "nichts", "ach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ART", "NN", "ART", "NN", "PIS", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie m\u00e4chtig/ sie pr\u00e4chtig besiegen in Schlachten.", "tokens": ["Sie", "m\u00e4ch\u00b7tig", "/", "sie", "pr\u00e4ch\u00b7tig", "be\u00b7sie\u00b7gen", "in", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "PPER", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Geber aller Siege", "tokens": ["Der", "Ge\u00b7ber", "al\u00b7ler", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sol mein Gedichte sein/", "tokens": ["sol", "mein", "Ge\u00b7dich\u00b7te", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der Vatter aller Kriege/", "tokens": ["der", "Vat\u00b7ter", "al\u00b7ler", "Krie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcrst Lucifer tritt ein.", "tokens": ["F\u00fcrst", "Lu\u00b7ci\u00b7fer", "tritt", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}