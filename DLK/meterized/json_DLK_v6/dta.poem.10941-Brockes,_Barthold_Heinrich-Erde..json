{"dta.poem.10941": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00e4rst, o Luft, wenn ich nicht w\u00e4r,", "tokens": ["Du", "w\u00e4rst", ",", "o", "Luft", ",", "wenn", "ich", "nicht", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Laub, von Bl\u00fcht, Geruch und Bl\u00e4ttern leer.", "tokens": ["Von", "Laub", ",", "von", "Bl\u00fcht", ",", "Ge\u00b7ruch", "und", "Bl\u00e4t\u00b7tern", "leer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch mich gereichen Zweig und Aeste", "tokens": ["Durch", "mich", "ge\u00b7rei\u00b7chen", "Zweig", "und", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den B\u00fcrgern deines Reichs zum Neste,", "tokens": ["Den", "B\u00fcr\u00b7gern", "dei\u00b7nes", "Reichs", "zum", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kurz, es stammet deine Zier,", "tokens": ["Und", "kurz", ",", "es", "stam\u00b7met", "dei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Pracht allein von mir.", "tokens": ["Und", "al\u00b7le", "Pracht", "al\u00b7lein", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}