{"textgrid.poem.31760": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Heuler und W\u00fchler", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hie\u00dfen Whig und hie\u00dfen Tory,", "tokens": ["Hie\u00b7\u00dfen", "Whig", "und", "hie\u00b7\u00dfen", "To\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hie\u00dfen Welf und Gibelline.", "tokens": ["Hie\u00b7\u00dfen", "Welf", "und", "Gi\u00b7bel\u00b7li\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zu K\u00f6ln am Vater Rheine", "tokens": ["Doch", "zu", "K\u00f6ln", "am", "Va\u00b7ter", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "APPRART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfen Heuler sie und W\u00fchler.", "tokens": ["Hei\u00b7\u00dfen", "Heu\u00b7ler", "sie", "und", "W\u00fch\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wildrepublikan'sche Eber", "tokens": ["Wild\u00b7re\u00b7pu\u00b7bli\u00b7kan'\u00b7sche", "E\u00b7ber"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fchlen einerseits die W\u00fchler;", "tokens": ["W\u00fch\u00b7len", "ei\u00b7ner\u00b7seits", "die", "W\u00fch\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Konstitutionelle W\u00f6lfe", "tokens": ["Kons\u00b7ti\u00b7tu\u00b7ti\u00b7o\u00b7nel\u00b7le", "W\u00f6l\u00b7fe"], "token_info": ["word", "word"], "pos": ["VMFIN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Heulen schlimmer noch die Heuler.", "tokens": ["Heu\u00b7len", "schlim\u00b7mer", "noch", "die", "Heu\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch Get\u00f6se! Herzersch\u00fctternd.", "tokens": ["Welch", "Ge\u00b7t\u00f6\u00b7se", "!", "Her\u00b7zer\u00b7sch\u00fct\u00b7ternd", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$.", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, vom Dom zum k\u00f6ln'schen Zelte,", "tokens": ["Ja", ",", "vom", "Dom", "zum", "k\u00f6ln'\u00b7schen", "Zel\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, vom T\u00fcrmchen bis zum Bay'nhaus", "tokens": ["Ja", ",", "vom", "T\u00fcrm\u00b7chen", "bis", "zum", "Bay'\u00b7nhaus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Poltert's, rast es, grunzt und brummt es.", "tokens": ["Pol\u00b7tert's", ",", "rast", "es", ",", "grunzt", "und", "brummt", "es", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oh, entsetzlich ist's! \u2013 Kam sonst der", "tokens": ["Oh", ",", "ent\u00b7setz\u00b7lich", "ist's", "!", "\u2013", "Kam", "sonst", "der"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "$.", "$(", "NE", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abend, sieh, da k\u00fc\u00dften wir und", "tokens": ["A\u00b7bend", ",", "sieh", ",", "da", "k\u00fc\u00df\u00b7ten", "wir", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "KON"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Tanzten, streuten Blumen, sangen", "tokens": ["Tanz\u00b7ten", ",", "streu\u00b7ten", "Blu\u00b7men", ",", "san\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vivat, Vivat, Hosianna!", "tokens": ["Vi\u00b7vat", ",", "Vi\u00b7vat", ",", "Ho\u00b7si\u00b7an\u00b7na", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Gingen froh zum alten Vater,", "tokens": ["Gin\u00b7gen", "froh", "zum", "al\u00b7ten", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkrat Thibus, dort die R\u00f6mer", "tokens": ["Trink\u00b7rat", "Thi\u00b7bus", ",", "dort", "die", "R\u00f6\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keck zu schwingen, bis da\u00df Eos", "tokens": ["Keck", "zu", "schwin\u00b7gen", ",", "bis", "da\u00df", "E\u00b7os"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKZU", "VVINF", "$,", "KON", "KOUS", "NE"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Stieg empor mit Rosenfingern.", "tokens": ["Stieg", "em\u00b7por", "mit", "Ro\u00b7sen\u00b7fin\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber jetzt? \u2013 O Zeus, du wei\u00dft es:", "tokens": ["A\u00b7ber", "jetzt", "?", "\u2013", "O", "Zeus", ",", "du", "wei\u00dft", "es", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Ebern, unter W\u00f6lfen,", "tokens": ["Un\u00b7ter", "E\u00b7bern", ",", "un\u00b7ter", "W\u00f6l\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich leide, unter diesen", "tokens": ["Was", "ich", "lei\u00b7de", ",", "un\u00b7ter", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschen souver\u00e4nen Bestien.", "tokens": ["Deut\u00b7schen", "sou\u00b7ve\u00b7r\u00e4\u00b7nen", "Be\u00b7sti\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}