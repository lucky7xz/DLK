{"dta.poem.7521": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thu was du kannst, und la\u00df das andre dem, der's kann", "tokens": ["Thu", "was", "du", "kannst", ",", "und", "la\u00df", "das", "and\u00b7re", "dem", ",", "der's", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "VMFIN", "$,", "KON", "VVIMP", "ART", "PIS", "ART", "$,", "PDS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu jedem ganzen Werk geh\u00f6rt ein ganzer Mann.", "tokens": ["Zu", "je\u00b7dem", "gan\u00b7zen", "Werk", "ge\u00b7h\u00f6rt", "ein", "gan\u00b7zer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwo H\u00e4lften machen zwar ein Ganzes, aber merk:", "tokens": ["Zwo", "H\u00e4lf\u00b7ten", "ma\u00b7chen", "zwar", "ein", "Gan\u00b7zes", ",", "a\u00b7ber", "merk", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus halb und halb gethan entsteht kein ganzes Werk.", "tokens": ["Aus", "halb", "und", "halb", "ge\u00b7than", "ent\u00b7steht", "kein", "gan\u00b7zes", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "VVPP", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer halb und halb gesund, der mag nur krank sich nennen;", "tokens": ["Wer", "halb", "und", "halb", "ge\u00b7sund", ",", "der", "mag", "nur", "krank", "sich", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVPP", "$,", "PRELS", "VMFIN", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gar nicht kennen wir, was halb und halb wir kennen.", "tokens": ["Und", "gar", "nicht", "ken\u00b7nen", "wir", ",", "was", "halb", "und", "halb", "wir", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn etwas Ganzes w\u00fcrd' aus noch so vielen Halben,", "tokens": ["Wenn", "et\u00b7was", "Gan\u00b7zes", "w\u00fcrd'", "aus", "noch", "so", "vie\u00b7len", "Hal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "APPR", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ganz gut! es wimmelt jetzt von Halben allenthalben.", "tokens": ["Ganz", "gut", "!", "es", "wim\u00b7melt", "jetzt", "von", "Hal\u00b7ben", "al\u00b7len\u00b7thal\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In jeder Halbheit wohnt ein Trieb zur Uebertreibung;", "tokens": ["In", "je\u00b7der", "Halb\u00b7heit", "wohnt", "ein", "Trieb", "zur", "Ue\u00b7bert\u00b7rei\u00b7bung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei Uebertreibung bleibt nicht aus die Unterbleibung.", "tokens": ["Bei", "Ue\u00b7bert\u00b7rei\u00b7bung", "bleibt", "nicht", "aus", "die", "Un\u00b7ter\u00b7blei\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zuwenig und zuviel ist beides ein Verdru\u00df;", "tokens": ["Zu\u00b7we\u00b7nig", "und", "zu\u00b7viel", "ist", "bei\u00b7des", "ein", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PIS", "VAFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So fehl ist \u00fcberm Ziel wie unterm Ziel ein Schu\u00df.", "tokens": ["So", "fehl", "ist", "\u00fc\u00b7berm", "Ziel", "wie", "un\u00b7term", "Ziel", "ein", "Schu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "KOKOM", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zuwenig und zuviel ist gleichsehr unvollkommen;", "tokens": ["Zu\u00b7we\u00b7nig", "und", "zu\u00b7viel", "ist", "gleich\u00b7sehr", "un\u00b7voll\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PIS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Ernst ist und im Spiel das rechte Ma\u00df willkommen.", "tokens": ["Im", "Ernst", "ist", "und", "im", "Spiel", "das", "rech\u00b7te", "Ma\u00df", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "KON", "APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}