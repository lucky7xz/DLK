{"textgrid.poem.62052": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[o mag ein Engel Dir die Schrift diktieren]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O mag ein Engel Dir die Schrift diktieren,", "tokens": ["O", "mag", "ein", "En\u00b7gel", "Dir", "die", "Schrift", "dik\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df jedes Wort mir Wonne sei und Lust,", "tokens": ["Da\u00df", "je\u00b7des", "Wort", "mir", "Won\u00b7ne", "sei", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Engel Deine Feder f\u00fchren,", "tokens": ["Ein", "En\u00b7gel", "Dei\u00b7ne", "Fe\u00b7der", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Zauber drinnen leben unbewu\u00dft!", "tokens": ["Ein", "Zau\u00b7ber", "drin\u00b7nen", "le\u00b7ben", "un\u00b7be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Damit, wenn ich das Siegel l\u00f6se,", "tokens": ["Da\u00b7mit", ",", "wenn", "ich", "das", "Sie\u00b7gel", "l\u00f6\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gl\u00fcck sich ungetr\u00fcbt daraus ergie\u00dft,", "tokens": ["Das", "Gl\u00fcck", "sich", "un\u00b7ge\u00b7tr\u00fcbt", "da\u00b7raus", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und keine Wolke, keine b\u00f6se,", "tokens": ["Und", "kei\u00b7ne", "Wol\u00b7ke", ",", "kei\u00b7ne", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist von Deinem Geiste liest.", "tokens": ["Mein", "Geist", "von", "Dei\u00b7nem", "Geis\u00b7te", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}