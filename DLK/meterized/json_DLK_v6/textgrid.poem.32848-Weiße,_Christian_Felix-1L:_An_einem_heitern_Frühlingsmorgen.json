{"textgrid.poem.32848": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: An einem heitern Fr\u00fchlingsmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem heitern Fr\u00fchlingsmorgen", "tokens": ["An", "ei\u00b7nem", "hei\u00b7tern", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trat Doris fr\u00fch erwacht ans offne Fenster hin;", "tokens": ["Trat", "Do\u00b7ris", "fr\u00fch", "er\u00b7wacht", "ans", "off\u00b7ne", "Fens\u00b7ter", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein ihr alter Geck Crispin,", "tokens": ["Al\u00b7lein", "ihr", "al\u00b7ter", "Geck", "Cris\u00b7pin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schnarcht noch empfindungslos, und tr\u00e4umet g\u00fcldne Sorgen.", "tokens": ["Schnarcht", "noch", "emp\u00b7fin\u00b7dungs\u00b7los", ",", "und", "tr\u00e4u\u00b7met", "g\u00fcld\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie f\u00fchlet halb des Morgens Freuden,", "tokens": ["Sie", "f\u00fch\u00b7let", "halb", "des", "Mor\u00b7gens", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch nur halb \u2013 \u2013 indem sah sie in s\u00fc\u00dfer M\u00fch", "tokens": ["Je\u00b7doch", "nur", "halb", "\u2013", "\u2013", "in\u00b7dem", "sah", "sie", "in", "s\u00fc\u00b7\u00dfer", "M\u00fch"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$(", "$(", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Den Spatz bey seiner muntern Sie,", "tokens": ["Den", "Spatz", "bey", "sei\u00b7ner", "mun\u00b7tern", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie liebten sich so oft, man mu\u00dfte sie beneiden.", "tokens": ["Sie", "lieb\u00b7ten", "sich", "so", "oft", ",", "man", "mu\u00df\u00b7te", "sie", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch weis ich nicht, was Chloen fehlte:", "tokens": ["Auch", "weis", "ich", "nicht", ",", "was", "Chloen", "fehl\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PPER", "PTKNEG", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sie ward best\u00fcrzt, ganz roth, fing laut zu seufzen an;", "tokens": ["Sie", "ward", "be\u00b7st\u00fcrzt", ",", "ganz", "roth", ",", "fing", "laut", "zu", "seuf\u00b7zen", "an", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "ADJD", "$,", "VVFIN", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sah bald ins Feld, bald nach dem Mann,", "tokens": ["Sah", "bald", "ins", "Feld", ",", "bald", "nach", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der noch im Traum sein Geld und Wechsel \u00fcberz\u00e4hlte.", "tokens": ["Der", "noch", "im", "Traum", "sein", "Geld", "und", "Wech\u00b7sel", "\u00fc\u00b7berz\u00b7\u00e4hl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zuletzt st\u00f6rt sie mit hei\u00dfen K\u00fcssen", "tokens": ["Zu\u00b7letzt", "st\u00f6rt", "sie", "mit", "hei\u00b7\u00dfen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und s\u00fc\u00dfem Ungest\u00fcm ihn in der Tr\u00e4ume Lauf,", "tokens": ["Und", "s\u00fc\u00b7\u00dfem", "Un\u00b7ge\u00b7st\u00fcm", "ihn", "in", "der", "Tr\u00e4u\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er springt best\u00fcrzt vom Lager auf,", "tokens": ["Er", "springt", "be\u00b7st\u00fcrzt", "vom", "La\u00b7ger", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreyt: wo ist der Dieb, der mir mein Gut entrissen?", "tokens": ["Und", "schreyt", ":", "wo", "ist", "der", "Dieb", ",", "der", "mir", "mein", "Gut", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie zieht ihn mit beredten Schweigen", "tokens": ["Sie", "zieht", "ihn", "mit", "be\u00b7red\u00b7ten", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ans Fenster hin, und seufzt: ach M\u00e4nnchen, glaubest du,", "tokens": ["Ans", "Fens\u00b7ter", "hin", ",", "und", "seufzt", ":", "ach", "M\u00e4nn\u00b7chen", ",", "glau\u00b7best", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "XY", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(schon seh ich eine Stunde zu,)", "tokens": ["(", "schon", "seh", "ich", "ei\u00b7ne", "Stun\u00b7de", "zu", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit diese V\u00f6gelchen sich so verliebt bezeigen.", "tokens": ["Seit", "die\u00b7se", "V\u00f6\u00b7gel\u00b7chen", "sich", "so", "ver\u00b7liebt", "be\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "F\u00fcr Bosheit f\u00e4ngt er an zu beben,", "tokens": ["F\u00fcr", "Bos\u00b7heit", "f\u00e4ngt", "er", "an", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sperrt weit die Augen auf, und schreyt erstaunungsvoll:", "tokens": ["Sperrt", "weit", "die", "Au\u00b7gen", "auf", ",", "und", "schreyt", "er\u00b7stau\u00b7nungs\u00b7voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbwie? sag mir, N\u00e4rrin, bist du toll?", "tokens": ["\u00bb", "wie", "?", "sag", "mir", ",", "N\u00e4r\u00b7rin", ",", "bist", "du", "toll", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibst du in Ewigkeit an Kinderpossen kleben?\u00ab", "tokens": ["Bleibst", "du", "in", "E\u00b7wig\u00b7keit", "an", "Kin\u00b7der\u00b7pos\u00b7sen", "kle\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}