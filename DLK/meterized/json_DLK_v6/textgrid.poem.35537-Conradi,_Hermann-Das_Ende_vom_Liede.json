{"textgrid.poem.35537": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende vom Liede", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen K\u00fcnsten dieser Welt die erste \u2013", "tokens": ["Von", "al\u00b7len", "K\u00fcns\u00b7ten", "die\u00b7ser", "Welt", "die", "ers\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allen K\u00fcnsten dieser Welt die schwerste,", "tokens": ["Von", "al\u00b7len", "K\u00fcns\u00b7ten", "die\u00b7ser", "Welt", "die", "schwers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bist du ", "tokens": ["Und", "bist", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ist alles Wurst, was jemals du gewesen,", "tokens": ["Ist", "al\u00b7les", "Wurst", ",", "was", "je\u00b7mals", "du", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VAFIN", "$,", "PRELS", "ADV", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du geliebt, geha\u00dft, getan, gefehlt, gewollt,", "tokens": ["Was", "du", "ge\u00b7liebt", ",", "ge\u00b7ha\u00dft", ",", "ge\u00b7tan", ",", "ge\u00b7fehlt", ",", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VMPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob sich dein Leben prunkvoll aufgerollt,", "tokens": ["Ob", "sich", "dein", "Le\u00b7ben", "prunk\u00b7voll", "auf\u00b7ge\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ob du f\u00fcr andre warst bequemer Besen.", "tokens": ["Ob", "du", "f\u00fcr", "and\u00b7re", "warst", "be\u00b7que\u00b7mer", "Be\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ob Sklave oder Herr \u2013 ", "tokens": ["Ob", "Skla\u00b7ve", "o\u00b7der", "Herr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen k\u00f6nnen \u2013 und nicht dran ersticken,", "tokens": ["Ver\u00b7ges\u00b7sen", "k\u00f6n\u00b7nen", "\u2013", "und", "nicht", "dran", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$(", "KON", "PTKNEG", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinunterschlucken, lachen, weiterkr\u00fccken,", "tokens": ["Hin\u00b7un\u00b7ter\u00b7schlu\u00b7cken", ",", "la\u00b7chen", ",", "wei\u00b7ter\u00b7kr\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Leben weiter noch ein dutzendmal.", "tokens": ["Ins", "Le\u00b7ben", "wei\u00b7ter", "noch", "ein", "dut\u00b7zend\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann tut's ja nichts! \u2013 Nun gut! Ich will's probieren,", "tokens": ["Dann", "tut's", "ja", "nichts", "!", "\u2013", "Nun", "gut", "!", "Ich", "will's", "pro\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$.", "$(", "ADV", "ADJD", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den letzten Lorbeerkranz will ich entbl\u00e4ttern,", "tokens": ["Den", "letz\u00b7ten", "Lor\u00b7beer\u00b7kranz", "will", "ich", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das letzte Amulett will ich zerschmettern,", "tokens": ["Das", "letz\u00b7te", "A\u00b7mu\u00b7lett", "will", "ich", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie man vergi\u00dft, will ich genau studieren.", "tokens": ["Wie", "man", "ver\u00b7gi\u00dft", ",", "will", "ich", "ge\u00b7nau", "stu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und eines Tages dann \u2013 ist mir's gegl\u00fcckt,", "tokens": ["Und", "ei\u00b7nes", "Ta\u00b7ges", "dann", "\u2013", "ist", "mir's", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$(", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich atme auf in grenzenloser Leere", "tokens": ["Ich", "at\u00b7me", "auf", "in", "gren\u00b7zen\u00b7lo\u00b7ser", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und breche in die Knie und bete: Kehre,", "tokens": ["Und", "bre\u00b7che", "in", "die", "Knie", "und", "be\u00b7te", ":", "Keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O kehre wieder, die du mich entz\u00fcckt:", "tokens": ["O", "keh\u00b7re", "wie\u00b7der", ",", "die", "du", "mich", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Geliebte S\u00fcnde, die ich froh beging,", "tokens": ["Ge\u00b7lieb\u00b7te", "S\u00fcn\u00b7de", ",", "die", "ich", "froh", "be\u00b7ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geliebte Reue, die ich k\u00fchn genossen. \u2013 \u2013", "tokens": ["Ge\u00b7lieb\u00b7te", "Reu\u00b7e", ",", "die", "ich", "k\u00fchn", "ge\u00b7nos\u00b7sen", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gemach, mein Freund! Dein Schicksal ist beschlossen", "tokens": ["Ge\u00b7mach", ",", "mein", "Freund", "!", "Dein", "Schick\u00b7sal", "ist", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um dich sch\u00fcrzt sich des Vergessens Ring.", "tokens": ["Und", "um", "dich", "sch\u00fcrzt", "sich", "des", "Ver\u00b7ges\u00b7sens", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}