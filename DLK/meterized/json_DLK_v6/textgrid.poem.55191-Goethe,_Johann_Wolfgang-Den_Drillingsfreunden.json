{"textgrid.poem.55191": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Den Drillingsfreunden", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abgebildete", "tokens": ["Der", "Ab\u00b7ge\u00b7bil\u00b7de\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vergleicht sich billig", "tokens": ["Ver\u00b7gleicht", "sich", "bil\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Heil'gem ", "tokens": ["Heil'\u00b7gem"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dieweil er willig", "tokens": ["Die\u00b7weil", "er", "wil\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dem Stern, der ostenher", "tokens": ["Dem", "Stern", ",", "der", "os\u00b7ten\u00b7her"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wahrhaft erschienen,", "tokens": ["Wahr\u00b7haft", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Auf allen Wegen war", "tokens": ["Auf", "al\u00b7len", "We\u00b7gen", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bereit zu dienen.", "tokens": ["Be\u00b7reit", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Bildner gleichenfalls", "tokens": ["Der", "Bild\u00b7ner", "glei\u00b7chen\u00b7falls"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vergleicht sich eben", "tokens": ["Ver\u00b7gleicht", "sich", "e\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dem Reiter, der den Hals", "tokens": ["Dem", "Rei\u00b7ter", ",", "der", "den", "Hals"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Darangegeben,", "tokens": ["Da\u00b7ran\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ein Held geworden,", "tokens": ["Ein", "Held", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Durch seine Manneskraft", "tokens": ["Durch", "sei\u00b7ne", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ritter vom Orden.", "tokens": ["Rit\u00b7ter", "vom", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Darum zusammen sie", "tokens": ["Da\u00b7rum", "zu\u00b7sam\u00b7men", "sie"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Euch nun verehren,", "tokens": ["Euch", "nun", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die zum Vergangenen", "tokens": ["Die", "zum", "Ver\u00b7gan\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Mutig sich kehren,", "tokens": ["Mu\u00b7tig", "sich", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Stein, Heil'ge, Samt und Gold \u2013", "tokens": ["Stein", ",", "Heil'\u00b7ge", ",", "Samt", "und", "Gold", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "M\u00e4nniglich strebend", "tokens": ["M\u00e4n\u00b7nig\u00b7lich", "stre\u00b7bend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und altem Tage hold \u2013", "tokens": ["Und", "al\u00b7tem", "Ta\u00b7ge", "hold", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Fr\u00f6hlich belebend.", "tokens": ["Fr\u00f6h\u00b7lich", "be\u00b7le\u00b7bend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}