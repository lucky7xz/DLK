{"textgrid.poem.39514": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Blumen sind die Kinder in den Reichen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen sind die Kinder in den Reichen", "tokens": ["Die", "Blu\u00b7men", "sind", "die", "Kin\u00b7der", "in", "den", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der lieblichen Natur: sie d\u00fcrfen kommen,", "tokens": ["Der", "lieb\u00b7li\u00b7chen", "Na\u00b7tur", ":", "sie", "d\u00fcr\u00b7fen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am hohen Thron selbst freundlich aufgenommen;", "tokens": ["Am", "ho\u00b7hen", "Thron", "selbst", "freund\u00b7lich", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum wag' ich heut, Dir diese darzureichen.", "tokens": ["Drum", "wag'", "ich", "heut", ",", "Dir", "die\u00b7se", "dar\u00b7zu\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lies Huldigung in diesen zarten Zeichen:", "tokens": ["Lies", "Hul\u00b7di\u00b7gung", "in", "die\u00b7sen", "zar\u00b7ten", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Fl\u00fcstern der Gef\u00fchle sei's vernommen,", "tokens": ["Wie", "Fl\u00fcs\u00b7tern", "der", "Ge\u00b7f\u00fch\u00b7le", "sei's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie f\u00fcr Dich in Farb' und Duft entglommen,", "tokens": ["Wie", "sie", "f\u00fcr", "Dich", "in", "Fa\u00b7rb'", "und", "Duft", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-++-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wenn sie vor Deiner Sch\u00f6nheit nicht erbleichen.", "tokens": ["Wenn", "sie", "vor", "Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "nicht", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Dich zuerst gef\u00fchrt in's holde Leben,", "tokens": ["Der", "Dich", "zu\u00b7erst", "ge\u00b7f\u00fchrt", "in's", "hol\u00b7de", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die K\u00f6nigin der Anmuth und der Sitten.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "An\u00b7muth", "und", "der", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O m\u00f6chte, wenn Dich alle K\u00fcnst' umringen,", "tokens": ["O", "m\u00f6ch\u00b7te", ",", "wenn", "Dich", "al\u00b7le", "K\u00fcnst'", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In der, die mich Thalia lehrt, mein Streben", "tokens": ["In", "der", ",", "die", "mich", "Tha\u00b7lia", "lehrt", ",", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PRF", "NE", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Oft Blumen Dir erziehn in Winters Mitten!", "tokens": ["Oft", "Blu\u00b7men", "Dir", "er\u00b7ziehn", "in", "Win\u00b7ters", "Mit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVINF", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}