{"dta.poem.110": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Amalien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Amalie ein Brief/ von meiner Hand geschrieben/", "tokens": ["A\u00b7ma\u00b7lie", "ein", "Brief", "/", "von", "mei\u00b7ner", "Hand", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Eilt mit geschwinden Flug durch Sehnsucht fortgetrieben/", "tokens": ["Eilt", "mit", "ge\u00b7schwin\u00b7den", "Flug", "durch", "Sehn\u00b7sucht", "fort\u00b7ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als jene Taube fliegt/ die ", "tokens": ["Als", "je\u00b7ne", "Tau\u00b7be", "fliegt", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deiner Wohnung hin/ und wil die Ehr-Furcht zeigen/", "tokens": ["Zu", "dei\u00b7ner", "Woh\u00b7nung", "hin", "/", "und", "wil", "die", "Ehr\u00b7Furcht", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mich verbindlich macht dem\u00fchtig sich zu neigen/", "tokens": ["Die", "mich", "ver\u00b7bind\u00b7lich", "macht", "de\u00b7m\u00fch\u00b7tig", "sich", "zu", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "ADJD", "PRF", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo deinen Nahmen zeigt auch meines Feindes Wand.", "tokens": ["Wo", "dei\u00b7nen", "Nah\u00b7men", "zeigt", "auch", "mei\u00b7nes", "Fein\u00b7des", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil man die G\u00f6tter mu\u00df mit grosser Ehr-Furcht ehren", "tokens": ["Weil", "man", "die", "G\u00f6t\u00b7ter", "mu\u00df", "mit", "gros\u00b7ser", "Ehr\u00b7Furcht", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wirst du deinem Knecht als G\u00f6ttin nicht verwehren/", "tokens": ["So", "wirst", "du", "dei\u00b7nem", "Knecht", "als", "G\u00f6t\u00b7tin", "nicht", "ver\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KOUS", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df er dem Hand Altar beliebten Weyrauch schenckt/", "tokens": ["Da\u00df", "er", "dem", "Hand", "Al\u00b7tar", "be\u00b7lieb\u00b7ten", "Wey\u00b7rauch", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie man ihn sonst der Mund und andern Gliedern weyhet.", "tokens": ["Wie", "man", "ihn", "sonst", "der", "Mund", "und", "an\u00b7dern", "Glie\u00b7dern", "wey\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was hier in Armuth wird anjetzo ausgestreuet/", "tokens": ["Was", "hier", "in", "Ar\u00b7muth", "wird", "an\u00b7je\u00b7tzo", "aus\u00b7ge\u00b7streu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zeigt an/ da\u00df meine Brust allzeit an dich gedenckt.", "tokens": ["Zeigt", "an", "/", "da\u00df", "mei\u00b7ne", "Brust", "all\u00b7zeit", "an", "dich", "ge\u00b7denckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier reich ich nun den Kern der sch\u00f6nsten R\u00e4ucher-Kertzen/", "tokens": ["Hier", "reich", "ich", "nun", "den", "Kern", "der", "sch\u00f6ns\u00b7ten", "R\u00e4u\u00b7cher\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn weil dein Nahrungs-Oehl dem starck ent flam\u0303ten Hertzen", "tokens": ["Denn", "weil", "dein", "Nah\u00b7rungs\u00b7Oehl", "dem", "starck", "ent", "flam\u0303ten", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "VVFIN", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Den kr\u00e4fftigsten Geruch und s\u00fcsse Anmuth giebt/", "tokens": ["Den", "kr\u00e4ff\u00b7tigs\u00b7ten", "Ge\u00b7ruch", "und", "s\u00fcs\u00b7se", "An\u00b7muth", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "So gebe ich es dir zum Opffer deiner H\u00e4nde/", "tokens": ["So", "ge\u00b7be", "ich", "es", "dir", "zum", "Opf\u00b7fer", "dei\u00b7ner", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Davor zum Gegen-Gru\u00df mir deines wieder sende/", "tokens": ["Da\u00b7vor", "zum", "Ge\u00b7gen\u00b7Gru\u00df", "mir", "dei\u00b7nes", "wie\u00b7der", "sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PPER", "PPOSAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und la\u00df den Wahl-Spruch seyn: ", "tokens": ["Und", "la\u00df", "den", "Wahl\u00b7Spruch", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "geliebt.", "tokens": ["ge\u00b7liebt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}