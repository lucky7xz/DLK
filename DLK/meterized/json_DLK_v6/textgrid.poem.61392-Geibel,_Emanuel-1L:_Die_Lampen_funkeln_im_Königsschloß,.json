{"textgrid.poem.61392": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lampen funkeln im K\u00f6nigsschlo\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lampen funkeln im K\u00f6nigsschlo\u00df,", "tokens": ["Die", "Lam\u00b7pen", "fun\u00b7keln", "im", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es klingen die Fl\u00f6ten und Geigen;", "tokens": ["Es", "klin\u00b7gen", "die", "Fl\u00f6\u00b7ten", "und", "Gei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Des K\u00f6nigs sch\u00f6nes T\u00f6chterlein", "tokens": ["Des", "K\u00f6\u00b7nigs", "sch\u00f6\u00b7nes", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tanzt drinnen den Hochzeitreigen.", "tokens": ["Tanzt", "drin\u00b7nen", "den", "Hoch\u00b7zei\u00b7trei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00e4gt im Haare den Myrtenkranz,", "tokens": ["Sie", "tr\u00e4gt", "im", "Haa\u00b7re", "den", "Myr\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch wandelt sie stumm und befangen;", "tokens": ["Doch", "wan\u00b7delt", "sie", "stumm", "und", "be\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie tr\u00e4gt an der Brust die bl\u00fchende Ros',", "tokens": ["Sie", "tr\u00e4gt", "an", "der", "Brust", "die", "bl\u00fc\u00b7hen\u00b7de", "Ros'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch sind ihr so bleich die Wangen.", "tokens": ["Doch", "sind", "ihr", "so", "bleich", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie tanzt mit dem fremden K\u00f6nigssohn,", "tokens": ["Sie", "tanzt", "mit", "dem", "frem\u00b7den", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er geht in Purpur und Seide;", "tokens": ["Er", "geht", "in", "Pur\u00b7pur", "und", "Sei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch sch\u00f6ner, tausendmal sch\u00f6ner war", "tokens": ["Doch", "sch\u00f6\u00b7ner", ",", "tau\u00b7send\u00b7mal", "sch\u00f6\u00b7ner", "war"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Knab' im Pagenkleide.", "tokens": ["Der", "Knab'", "im", "Pa\u00b7gen\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am goldnen Tisch zw\u00f6lf Jungfraun stehn,", "tokens": ["Am", "gold\u00b7nen", "Tisch", "zw\u00f6lf", "Jung\u00b7fraun", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den perlenden Wein zu kredenzen;", "tokens": ["Den", "per\u00b7len\u00b7den", "Wein", "zu", "kre\u00b7den\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zw\u00f6lf Pagen schwingen sich um das Paar", "tokens": ["Zw\u00f6lf", "Pa\u00b7gen", "schwin\u00b7gen", "sich", "um", "das", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit lodernden Fackeln und Kr\u00e4nzen.", "tokens": ["Mit", "lo\u00b7dern\u00b7den", "Fa\u00b7ckeln", "und", "Kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Urpl\u00f6tzlich l\u00f6schen die Fackeln aus,", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "l\u00f6\u00b7schen", "die", "Fa\u00b7ckeln", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Urpl\u00f6tzlich verstummen die Geigen;", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "ver\u00b7stum\u00b7men", "die", "Gei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der alte K\u00f6nig f\u00e4hrt vom Sitz:", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "f\u00e4hrt", "vom", "Sitz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbsagt an, was soll dies Schweigen?\u00ab", "tokens": ["\u00bb", "sagt", "an", ",", "was", "soll", "dies", "Schwei\u00b7gen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PWS", "VMFIN", "PDS", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbherr K\u00f6nig, nicht entbrenn' in Zorn,", "tokens": ["\u00bb", "herr", "K\u00f6\u00b7nig", ",", "nicht", "ent\u00b7brenn'", "in", "Zorn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir d\u00fcrfen nicht blasen und streichen;", "tokens": ["Wir", "d\u00fcr\u00b7fen", "nicht", "bla\u00b7sen", "und", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Meermann harft vor dem Schlosse dein,", "tokens": ["Der", "Meer\u00b7mann", "harft", "vor", "dem", "Schlos\u00b7se", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Meermann m\u00fcssen wir weichen.\u00ab", "tokens": ["Dem", "Meer\u00b7mann", "m\u00fcs\u00b7sen", "wir", "wei\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und horch, empor vom Meere weht", "tokens": ["Und", "horch", ",", "em\u00b7por", "vom", "Mee\u00b7re", "weht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "PTKVZ", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein s\u00fc\u00dfes, trauriges Schallen,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", ",", "trau\u00b7ri\u00b7ges", "Schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schleicht so sacht durch die d\u00e4mmernde Nacht", "tokens": ["Es", "schleicht", "so", "sacht", "durch", "die", "d\u00e4m\u00b7mern\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herein in die festlichen Hallen.", "tokens": ["Her\u00b7ein", "in", "die", "fest\u00b7li\u00b7chen", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Es schleicht so sacht in das Ohr der Braut;", "tokens": ["Es", "schleicht", "so", "sacht", "in", "das", "Ohr", "der", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr ist, als ob aus der Tiefe,", "tokens": ["Ihr", "ist", ",", "als", "ob", "aus", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ob aus der Tiefe mit Allgewalt", "tokens": ["Als", "ob", "aus", "der", "Tie\u00b7fe", "mit", "All\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der liebste Buhle sie riefe.", "tokens": ["Der", "liebs\u00b7te", "Buh\u00b7le", "sie", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ihr quellen die Augen, sie wei\u00df nicht warum,", "tokens": ["Ihr", "quel\u00b7len", "die", "Au\u00b7gen", ",", "sie", "wei\u00df", "nicht", "wa\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie mu\u00df in Tr\u00e4nen zerflie\u00dfen;", "tokens": ["Sie", "mu\u00df", "in", "Tr\u00e4\u00b7nen", "zer\u00b7flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aus ihren Locken der Myrtenkranz", "tokens": ["Aus", "ih\u00b7ren", "Lo\u00b7cken", "der", "Myr\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00e4llt welk zu ihren F\u00fc\u00dfen.", "tokens": ["F\u00e4llt", "welk", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dem K\u00f6nig rieselt's durch Mark und Bein,", "tokens": ["Dem", "K\u00f6\u00b7nig", "rie\u00b7selt's", "durch", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er fleucht entsetzt vor dem Schalle;", "tokens": ["Er", "fleucht", "ent\u00b7setzt", "vor", "dem", "Schal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es eilt der fremde K\u00f6nigssohn", "tokens": ["Es", "eilt", "der", "frem\u00b7de", "K\u00f6\u00b7nigs\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seinen Rossen im Stalle.", "tokens": ["Nach", "sei\u00b7nen", "Ros\u00b7sen", "im", "Stal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Im Saale liegt die bleiche Braut,", "tokens": ["Im", "Saa\u00b7le", "liegt", "die", "blei\u00b7che", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ist das Herz zersprungen;", "tokens": ["Ihr", "ist", "das", "Herz", "zer\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Morgen tr\u00fcb in die Fenster graut,", "tokens": ["Der", "Mor\u00b7gen", "tr\u00fcb", "in", "die", "Fens\u00b7ter", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Meermanns Harf' ist verklungen.", "tokens": ["Des", "Meer\u00b7manns", "Ha\u00b7rf'", "ist", "ver\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}