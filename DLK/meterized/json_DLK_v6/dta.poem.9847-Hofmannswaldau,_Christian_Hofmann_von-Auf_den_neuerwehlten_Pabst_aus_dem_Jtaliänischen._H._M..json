{"dta.poem.9847": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den neuerwehlten Pabst aus dem  \n Jtali\u00e4nischen.  \n H. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wird Vidon ein Pabst? ihr heilgen purpur-m\u00fctzen/", "tokens": ["Ach", "wird", "Vi\u00b7don", "ein", "Pabst", "?", "ihr", "heil\u00b7gen", "pur\u00b7pur\u00b7m\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "NE", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entr\u00f6thet ihr euch nicht zu cr\u00f6nen den Vidon?", "tokens": ["Ent\u00b7r\u00f6\u00b7thet", "ihr", "euch", "nicht", "zu", "cr\u00f6\u00b7nen", "den", "Vi\u00b7don", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll die dreyfache cron auf dessen haupte sitzen/", "tokens": ["Soll", "die", "drey\u00b7fa\u00b7che", "cron", "auf", "des\u00b7sen", "haup\u00b7te", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "PRELAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der als der m\u00f6rder haupt verdienet andern lohn?", "tokens": ["Der", "als", "der", "m\u00f6r\u00b7der", "haupt", "ver\u00b7die\u00b7net", "an\u00b7dern", "lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein m\u00f6nch der in der welt stets suchte sein vergn\u00fcgen/", "tokens": ["Ein", "m\u00f6nch", "der", "in", "der", "welt", "stets", "such\u00b7te", "sein", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Bischoff der zugleich ein j\u00fcd\u2019scher Simon war/", "tokens": ["Ein", "Bi\u00b7schoff", "der", "zu\u00b7gleich", "ein", "j\u00fcd'\u00b7scher", "Si\u00b7mon", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ART", "ADJA", "NE", "VAFIN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ein Nuntius/ der nie/ was heimlich war/ verschwiegen/", "tokens": ["Ein", "Nun\u00b7ti\u00b7us", "/", "der", "nie", "/", "was", "heim\u00b7lich", "war", "/", "ver\u00b7schwie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADV", "$(", "PWS", "ADJD", "VAFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein abgesandter/ doch aus Simous falscher schaar;", "tokens": ["Ein", "ab\u00b7ge\u00b7sand\u00b7ter", "/", "doch", "aus", "Si\u00b7mous", "fal\u00b7scher", "schaar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Solt dieser auf den stuhl? der mir mein leben ende?", "tokens": ["Solt", "die\u00b7ser", "auf", "den", "stuhl", "?", "der", "mir", "mein", "le\u00b7ben", "en\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "APPR", "ART", "NN", "$.", "ART", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der meine Tyber kehr in ein blutrothes bad?", "tokens": ["Der", "mei\u00b7ne", "Ty\u00b7ber", "kehr", "in", "ein", "blut\u00b7ro\u00b7thes", "bad", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mich schrecken des Vidons blutd\u00fcrst\u2019ge m\u00f6rder-h\u00e4nde/", "tokens": ["Mich", "schre\u00b7cken", "des", "Vi\u00b7dons", "blut\u00b7d\u00fcr\u00b7st'\u00b7ge", "m\u00f6r\u00b7der\u00b7h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ich h\u00f6chst-betr\u00fcbtes Rom/ wo such ich klugen rath?", "tokens": ["Ich", "h\u00f6chst\u00b7be\u00b7tr\u00fcb\u00b7tes", "Rom", "/", "wo", "such", "ich", "klu\u00b7gen", "rath", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$(", "PWAV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die thr\u00e4nen fliessen nicht umsonst von meinen wangen;", "tokens": ["Die", "thr\u00e4\u00b7nen", "flies\u00b7sen", "nicht", "um\u00b7sonst", "von", "mei\u00b7nen", "wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die ursach ist bekandt: mir zittert bein und haut:", "tokens": ["Die", "ur\u00b7sach", "ist", "be\u00b7kandt", ":", "mir", "zit\u00b7tert", "bein", "und", "haut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn der die mutter oft mit pr\u00fcgeln hat empfangen/", "tokens": ["Denn", "der", "die", "mut\u00b7ter", "oft", "mit", "pr\u00fc\u00b7geln", "hat", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der wird sich auch nicht scheun zu pr\u00fcgeln seine braut.", "tokens": ["Der", "wird", "sich", "auch", "nicht", "scheun", "zu", "pr\u00fc\u00b7geln", "sei\u00b7ne", "braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "PTKNEG", "VVINF", "PTKZU", "VVINF", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}