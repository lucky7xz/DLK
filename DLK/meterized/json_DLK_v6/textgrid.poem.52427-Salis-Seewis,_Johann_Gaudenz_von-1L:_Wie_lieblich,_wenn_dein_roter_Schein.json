{"textgrid.poem.52427": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieblich, wenn dein roter Schein", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich, wenn dein roter Schein", "tokens": ["Wie", "lieb\u00b7lich", ",", "wenn", "dein", "ro\u00b7ter", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den stillen See bemalt,", "tokens": ["Den", "stil\u00b7len", "See", "be\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in den taubesprengten Hain", "tokens": ["Und", "in", "den", "tau\u00b7be\u00b7spreng\u00b7ten", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Bl\u00fctenzweige strahlt;", "tokens": ["Durch", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7ge", "strahlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf goldner Wogenflut des Korns", "tokens": ["Auf", "gold\u00b7ner", "Wo\u00b7gen\u00b7flut", "des", "Korns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Leicht hin und wieder schl\u00fcpft,", "tokens": ["Leicht", "hin", "und", "wie\u00b7der", "schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und funkelnd auf des Wiesenborns", "tokens": ["Und", "fun\u00b7kelnd", "auf", "des", "Wie\u00b7sen\u00b7borns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umsch\u00e4umtem Silber h\u00fcpft!", "tokens": ["Um\u00b7sch\u00e4um\u00b7tem", "Sil\u00b7ber", "h\u00fcpft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie lieblich, wenn er mit dem Bach", "tokens": ["Wie", "lieb\u00b7lich", ",", "wenn", "er", "mit", "dem", "Bach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumenau durchspielt,", "tokens": ["Die", "Blu\u00b7me\u00b7nau", "durch\u00b7spielt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sich durch das Holunderdach", "tokens": ["Und", "sich", "durch", "das", "Ho\u00b7lun\u00b7der\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In meine Laube stiehlt;", "tokens": ["In", "mei\u00b7ne", "Lau\u00b7be", "stiehlt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn wolligkrauser W\u00f6lkchen Heer", "tokens": ["Wenn", "wol\u00b7lig\u00b7krau\u00b7ser", "W\u00f6lk\u00b7chen", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Purpur \u00fcberzieht", "tokens": ["Sein", "Pur\u00b7pur", "\u00fc\u00b7berz\u00b7ieht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, rot vom Wiederschein, das Meer", "tokens": ["Und", ",", "rot", "vom", "Wie\u00b7der\u00b7schein", ",", "das", "Meer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADJD", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Lavastr\u00f6me gl\u00fcht!", "tokens": ["Wie", "La\u00b7vas\u00b7tr\u00f6\u00b7me", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Pracht, wenn du der Berge Blau", "tokens": ["O", "Pracht", ",", "wenn", "du", "der", "Ber\u00b7ge", "Blau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnem Saume zierst,", "tokens": ["Mit", "gold\u00b7nem", "Sau\u00b7me", "zierst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bevor du dich ins matte Grau", "tokens": ["Be\u00b7vor", "du", "dich", "ins", "mat\u00b7te", "Grau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der D\u00e4mmerung verlierst!", "tokens": ["Der", "D\u00e4m\u00b7me\u00b7rung", "ver\u00b7lierst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch wundersch\u00f6ner str\u00f6mt die Flut", "tokens": ["Noch", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "str\u00f6mt", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von deinem Rosenlicht", "tokens": ["Von", "dei\u00b7nem", "Ro\u00b7sen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem M\u00e4dchen unterm Halmenhut", "tokens": ["Dem", "M\u00e4d\u00b7chen", "un\u00b7term", "Hal\u00b7men\u00b7hut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins bl\u00fchende Gesicht.", "tokens": ["Ins", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn bei der Heidelerchen Sang", "tokens": ["Wenn", "bei", "der", "Hei\u00b7de\u00b7ler\u00b7chen", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein letzter Strahl erstirbt,", "tokens": ["Dein", "letz\u00b7ter", "Strahl", "er\u00b7stirbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Totenacker, leis' und bang',", "tokens": ["Im", "To\u00b7te\u00b7na\u00b7cker", ",", "leis'", "und", "bang'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch die Cikade zirpt;", "tokens": ["Noch", "die", "Ci\u00b7ka\u00b7de", "zirpt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann l\u00e4chelt die Vergangenheit", "tokens": ["Dann", "l\u00e4\u00b7chelt", "die", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch der Erinn'rung Flor,", "tokens": ["Durch", "der", "Er\u00b7inn'\u00b7rung", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "In mildem Lichte steigt der Zeit", "tokens": ["In", "mil\u00b7dem", "Lich\u00b7te", "steigt", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verblichnes Bild empor.", "tokens": ["Ver\u00b7blich\u00b7nes", "Bild", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Aus deines Kranzes Rosen taut", "tokens": ["Aus", "dei\u00b7nes", "Kran\u00b7zes", "Ro\u00b7sen", "taut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wehm\u00fctiges Gef\u00fchl;", "tokens": ["Weh\u00b7m\u00fc\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Spiegel stiller Ahndung schaut", "tokens": ["Im", "Spie\u00b7gel", "stil\u00b7ler", "Ahn\u00b7dung", "schaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist der Wallfahrt Ziel;", "tokens": ["Mein", "Geist", "der", "Wall\u00b7fahrt", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Hauch der Hoffnung k\u00fchl umweht,", "tokens": ["Vom", "Hauch", "der", "Hoff\u00b7nung", "k\u00fchl", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00dft er Gram und Schmerz;", "tokens": ["Ver\u00b7gi\u00dft", "er", "Gram", "und", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Erde rings um ihn vergeht,", "tokens": ["Die", "Er\u00b7de", "rings", "um", "ihn", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er schwingt sich himmelw\u00e4rts.", "tokens": ["Er", "schwingt", "sich", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}