{"textgrid.poem.49153": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie schwer ist doch die letzte Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schwer ist doch die letzte Stunde,", "tokens": ["Wie", "schwer", "ist", "doch", "die", "letz\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Leben mit dem Tode ringt,", "tokens": ["Da", "Le\u00b7ben", "mit", "dem", "To\u00b7de", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wenn der Geist schwebt in dem Munde", "tokens": ["Denn", "wenn", "der", "Geist", "schwebt", "in", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jetzund in die Luft sich schwingt,", "tokens": ["Und", "je\u00b7tzund", "in", "die", "Luft", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kaum das Ohr ein Wort mehr h\u00f6rt", "tokens": ["Da", "kaum", "das", "Ohr", "ein", "Wort", "mehr", "h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schon die Augen halb verkehrt.", "tokens": ["Und", "schon", "die", "Au\u00b7gen", "halb", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach schwerer Kampf, o hartes Streiten,", "tokens": ["Ach", "schwe\u00b7rer", "Kampf", ",", "o", "har\u00b7tes", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darauf die Seeligkeit sich gr\u00fcndt!", "tokens": ["Da\u00b7rauf", "die", "See\u00b7lig\u00b7keit", "sich", "gr\u00fcndt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Feind dringt an von allen Seiten,", "tokens": ["Der", "Feind", "dringt", "an", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Feind, der mehr als tausend sind,", "tokens": ["Der", "Feind", ",", "der", "mehr", "als", "tau\u00b7send", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "CARD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der st\u00f6rt dir immerfort die Ruh", "tokens": ["Der", "st\u00f6rt", "dir", "im\u00b7mer\u00b7fort", "die", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schreit dir deine S\u00fcnde zu.", "tokens": ["Und", "schreit", "dir", "dei\u00b7ne", "S\u00fcn\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was hast du, armer Mensch, hierwider?", "tokens": ["Was", "hast", "du", ",", "ar\u00b7mer", "Mensch", ",", "hier\u00b7wi\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herz das klagt sich selber an,", "tokens": ["Dein", "Herz", "das", "klagt", "sich", "sel\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schl\u00e4gst vor Gott die Augen nieder.", "tokens": ["Du", "schl\u00e4gst", "vor", "Gott", "die", "Au\u00b7gen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist hier, der dir helfen kann,", "tokens": ["Wer", "ist", "hier", ",", "der", "dir", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wofern es nicht dein Jesus thut,", "tokens": ["Wo\u00b7fern", "es", "nicht", "dein", "Je\u00b7sus", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dir erstanden ist zu gut?", "tokens": ["Der", "dir", "er\u00b7stan\u00b7den", "ist", "zu", "gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Desselben Leiden, Tod und Schmerzen", "tokens": ["Des\u00b7sel\u00b7ben", "Lei\u00b7den", ",", "Tod", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halt' im Ged\u00e4chtnis steif und fest,", "tokens": ["Halt'", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "steif", "und", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mit dem besprich dich in dem Herzen,", "tokens": ["Mit", "dem", "be\u00b7sprich", "dich", "in", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dich der Mund nicht reden l\u00e4\u00dft;", "tokens": ["Wenn", "dich", "der", "Mund", "nicht", "re\u00b7den", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Seufzerlein in diesem Hort", "tokens": ["Ein", "Seuf\u00b7zer\u00b7lein", "in", "die\u00b7sem", "Hort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Gott das angenehmste Wort.", "tokens": ["Ist", "Gott", "das", "an\u00b7ge\u00b7nehms\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlag' alles Eitle aus den Sinnen,", "tokens": ["Schlag'", "al\u00b7les", "Eit\u00b7le", "aus", "den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb herzlich Gute Nacht der Welt,", "tokens": ["Gieb", "herz\u00b7lich", "Gu\u00b7te", "Nacht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Gott dich fordert ab von hinnen.", "tokens": ["Wenn", "Gott", "dich", "for\u00b7dert", "ab", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "PTKVZ", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mach', da\u00df dein Haus sei so bestellt,", "tokens": ["Mach'", ",", "da\u00df", "dein", "Haus", "sei", "so", "be\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da\u00df deine Seele rein und frei,", "tokens": ["Da\u00df", "dei\u00b7ne", "See\u00b7le", "rein", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und voll allein von Christo sei.", "tokens": ["Und", "voll", "al\u00b7lein", "von", "Chris\u00b7to", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann kann dich keine S\u00fcnd' erschrecken;", "tokens": ["Dann", "kann", "dich", "kei\u00b7ne", "S\u00fcnd'", "er\u00b7schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Teufel, keine H\u00f6lle nicht.", "tokens": ["Kein", "Teu\u00b7fel", ",", "kei\u00b7ne", "H\u00f6l\u00b7le", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Jesu kannst du dich verstecken", "tokens": ["In", "Je\u00b7su", "kannst", "du", "dich", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bergen vor dem Zorngericht.", "tokens": ["Und", "ber\u00b7gen", "vor", "dem", "Zorn\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie seelig ist dann der daran,", "tokens": ["Wie", "see\u00b7lig", "ist", "dann", "der", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Der stets an Jesum denken kann!", "tokens": ["Der", "stets", "an", "Je\u00b7sum", "den\u00b7ken", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}