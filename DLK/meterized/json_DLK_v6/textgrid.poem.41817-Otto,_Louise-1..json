{"textgrid.poem.41817": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin bereit! willst Du hinweg mich rufen", "tokens": ["Ich", "bin", "be\u00b7reit", "!", "willst", "Du", "hin\u00b7weg", "mich", "ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dieser Erde, nimm, mein Gott, mich hin.", "tokens": ["Von", "die\u00b7ser", "Er\u00b7de", ",", "nimm", ",", "mein", "Gott", ",", "mich", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVIMP", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Tod ist meiner Seele nur Gewinn:", "tokens": ["Der", "Tod", "ist", "mei\u00b7ner", "See\u00b7le", "nur", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchrt empor zu neuen Lebensstufen.", "tokens": ["Er", "f\u00fchrt", "em\u00b7por", "zu", "neu\u00b7en", "Le\u00b7bens\u00b7stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die m\u00e4cht'gen Sch\u00f6pferworte, die mich schufen,", "tokens": ["Die", "m\u00e4cht'\u00b7gen", "Sch\u00f6p\u00b7fer\u00b7wor\u00b7te", ",", "die", "mich", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich erf\u00fcllt mit selbstbewu\u00dftem Sinn \u2013", "tokens": ["Die", "mich", "er\u00b7f\u00fcllt", "mit", "selbst\u00b7be\u00b7wu\u00df\u00b7tem", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie werden seit der Welten Anbeginn", "tokens": ["Sie", "wer\u00b7den", "seit", "der", "Wel\u00b7ten", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einem ew'gen Echo fortgerufen.", "tokens": ["In", "ei\u00b7nem", "ew'\u00b7gen", "E\u00b7cho", "fort\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie t\u00f6nen, wirken fort ohn' End', ohn' Ende,", "tokens": ["Sie", "t\u00f6\u00b7nen", ",", "wir\u00b7ken", "fort", "ohn'", "End'", ",", "ohn'", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rings in dem Riesenpulsschlag der Natur,", "tokens": ["Rings", "in", "dem", "Rie\u00b7sen\u00b7puls\u00b7schlag", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Als immer neuverj\u00fcngte Lebensspende.", "tokens": ["Als", "im\u00b7mer", "neu\u00b7ver\u00b7j\u00fcng\u00b7te", "Le\u00b7bens\u00b7spen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und \u00fcber mir, im himmlischen Azur,", "tokens": ["Und", "\u00fc\u00b7ber", "mir", ",", "im", "himm\u00b7li\u00b7schen", "A\u00b7zur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort, wo auch meines Geistes Sonnenwende,", "tokens": ["Dort", ",", "wo", "auch", "mei\u00b7nes", "Geis\u00b7tes", "Son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann nicht verwehen ihre heil'ge Spur.", "tokens": ["Kann", "nicht", "ver\u00b7we\u00b7hen", "ih\u00b7re", "heil'\u00b7ge", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}