{"dta.poem.19474": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Lautenstimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Schlummernd j\u00fcngst in Waldesraum", "tokens": ["Schlum\u00b7mernd", "j\u00fcngst", "in", "Wal\u00b7des\u00b7raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ich einen h\u00fcbschen Traum:", "tokens": ["Hatt'", "ich", "ei\u00b7nen", "h\u00fcb\u00b7schen", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Etwas regt sich in der Hecke,", "tokens": ["Et\u00b7was", "regt", "sich", "in", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas klimpert im Verstecke.", "tokens": ["Et\u00b7was", "klim\u00b7pert", "im", "Ver\u00b7ste\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gestr\u00e4uch mit leiser Hand", "tokens": ["Das", "Ge\u00b7str\u00e4uch", "mit", "lei\u00b7ser", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Theilt' ich, bis das Nest ich fand:", "tokens": ["Theilt'", "ich", ",", "bis", "das", "Nest", "ich", "fand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kinder, rings im Grase sitzend,", "tokens": ["Kin\u00b7der", ",", "rings", "im", "Gra\u00b7se", "sit\u00b7zend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den hellen Augen blitzend!", "tokens": ["Mit", "den", "hel\u00b7len", "Au\u00b7gen", "blit\u00b7zend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rutschend auf dem nackten Knie,", "tokens": ["Rut\u00b7schend", "auf", "dem", "nack\u00b7ten", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmten eine Laute sie \u2014", "tokens": ["Stimm\u00b7ten", "ei\u00b7ne", "Lau\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201esagt, was lagert ihr im Runde?", "tokens": ["\u201e", "sagt", ",", "was", "la\u00b7gert", "ihr", "im", "Run\u00b7de", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprecht, was schaffet ihr im Bunde?\u201c", "tokens": ["Sprecht", ",", "was", "schaf\u00b7fet", "ihr", "im", "Bun\u00b7de", "?", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf das zarte Werk erpicht,", "tokens": ["Auf", "das", "zar\u00b7te", "Werk", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rten sie die Frage nicht.", "tokens": ["H\u00f6r\u00b7ten", "sie", "die", "Fra\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eseht, wie ist sie zugerichtet!", "tokens": ["\u201e", "seht", ",", "wie", "ist", "sie", "zu\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundgerissen! Fast vernichtet!\u201c", "tokens": ["Wund\u00b7ge\u00b7ris\u00b7sen", "!", "Fast", "ver\u00b7nich\u00b7tet", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Emsig ward geklopft, gesp\u00e4ht,", "tokens": ["Em\u00b7sig", "ward", "ge\u00b7klopft", ",", "ge\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Saiten flink gedreht,", "tokens": ["An", "den", "Sai\u00b7ten", "flink", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00dfen eine tiefer klingen,", "tokens": ["Lie\u00b7\u00dfen", "ei\u00b7ne", "tie\u00b7fer", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00dfen eine hohe springen, \u2014", "tokens": ["Lie\u00b7\u00dfen", "ei\u00b7ne", "ho\u00b7he", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Endlich klang die Laute rein", "tokens": ["End\u00b7lich", "klang", "die", "Lau\u00b7te", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Kinder spielten fein,", "tokens": ["Und", "die", "Kin\u00b7der", "spiel\u00b7ten", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ich aus dem Traum erwachte", "tokens": ["Bis", "ich", "aus", "dem", "Traum", "er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und mir seinen Sinn bedachte:", "tokens": ["Und", "mir", "sei\u00b7nen", "Sinn", "be\u00b7dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dumpf entschlummert, jetzo hell,", "tokens": ["Dumpf", "ent\u00b7schlum\u00b7mert", ",", "jet\u00b7zo", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz ein anderer Gesell!", "tokens": ["Ganz", "ein", "an\u00b7de\u00b7rer", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Kinder ohne Fehle", "tokens": ["Was", "die", "Kin\u00b7der", "oh\u00b7ne", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimmten, es war meine Seele!", "tokens": ["Stimm\u00b7ten", ",", "es", "war", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}