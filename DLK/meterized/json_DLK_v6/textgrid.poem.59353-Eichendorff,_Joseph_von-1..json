{"textgrid.poem.59353": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es qualmt' der eitle Markt in Staub und Schw\u00fcle,", "tokens": ["Es", "qualmt'", "der", "eit\u00b7le", "Markt", "in", "Staub", "und", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So klanglos \u00f6de wallend auf und nieder,", "tokens": ["So", "klang\u00b7los", "\u00f6\u00b7de", "wal\u00b7lend", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie dacht ich da an meine Berge wieder,", "tokens": ["Wie", "dacht", "ich", "da", "an", "mei\u00b7ne", "Ber\u00b7ge", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An frischen Sang, Felsquell und Waldesk\u00fchle!", "tokens": ["An", "fri\u00b7schen", "Sang", ",", "Fels\u00b7quell", "und", "Wal\u00b7des\u00b7k\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch steht ein Turm dort \u00fcber dem Gew\u00fchle,", "tokens": ["Doch", "steht", "ein", "Turm", "dort", "\u00fc\u00b7ber", "dem", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der andre Zeiten sah und be\u00dfre Br\u00fcder,", "tokens": ["Der", "and\u00b7re", "Zei\u00b7ten", "sah", "und", "be\u00df\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Kreuz treu halten seine Riesenglieder,", "tokens": ["Das", "Kreuz", "treu", "hal\u00b7ten", "sei\u00b7ne", "Rie\u00b7sen\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie auch der Menschlein Flut den Fels umsp\u00fcle.", "tokens": ["Wie", "auch", "der", "Mensc\u00b7hlein", "Flut", "den", "Fels", "um\u00b7sp\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das war mein Hafen auf der weiten W\u00fcste,", "tokens": ["Das", "war", "mein", "Ha\u00b7fen", "auf", "der", "wei\u00b7ten", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft kniet ich betend in des Domes Mitte,", "tokens": ["Oft", "kniet", "ich", "be\u00b7tend", "in", "des", "Do\u00b7mes", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort hab ich dich, mein liebes Kind, gefunden;", "tokens": ["Dort", "hab", "ich", "dich", ",", "mein", "lie\u00b7bes", "Kind", ",", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Himmelsbote wohl, der so mich gr\u00fc\u00dfte:", "tokens": ["Ein", "Him\u00b7mels\u00b7bo\u00b7te", "wohl", ",", "der", "so", "mich", "gr\u00fc\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbverzweifle nicht! die Sch\u00f6nheit und die Sitte", "tokens": ["\u00bb", "ver\u00b7zweif\u00b7le", "nicht", "!", "die", "Sch\u00f6n\u00b7heit", "und", "die", "Sit\u00b7te"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKNEG", "$.", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind noch von der Erde nicht verschwunden.\u00ab", "tokens": ["Sie", "sind", "noch", "von", "der", "Er\u00b7de", "nicht", "ver\u00b7schwun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}