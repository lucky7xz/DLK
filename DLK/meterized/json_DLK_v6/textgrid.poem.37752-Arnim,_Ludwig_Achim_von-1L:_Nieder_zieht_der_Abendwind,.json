{"textgrid.poem.37752": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nieder zieht der Abendwind,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nieder zieht der Abendwind,", "tokens": ["Nie\u00b7der", "zieht", "der", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiegt in Schlaf manch sch\u00f6nes Kind,", "tokens": ["Wiegt", "in", "Schlaf", "manch", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6scht die Lichter,", "tokens": ["L\u00f6scht", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Doch es weckt der Vollmondglanz", "tokens": ["Doch", "es", "weckt", "der", "Voll\u00b7mond\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen zu dem Abendtanz,", "tokens": ["Blu\u00b7men", "zu", "dem", "A\u00b7bend\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmlische Gesichter.", "tokens": ["Himm\u00b7li\u00b7sche", "Ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Blumen springen aus dem Bett,", "tokens": ["Blu\u00b7men", "sprin\u00b7gen", "aus", "dem", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Waschen sich im Tau so nett", "tokens": ["Wa\u00b7schen", "sich", "im", "Tau", "so", "nett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich schm\u00fccken;", "tokens": ["Und", "sich", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Manches krause weiche Blatt", "tokens": ["Man\u00b7ches", "krau\u00b7se", "wei\u00b7che", "Blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich erst neu entfaltet hat", "tokens": ["Sich", "erst", "neu", "ent\u00b7fal\u00b7tet", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahnendem Entz\u00fccken.", "tokens": ["Ah\u00b7nen\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jede sich im Bach besieht,", "tokens": ["Je\u00b7de", "sich", "im", "Bach", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun sie hin zum Tanze zieht,", "tokens": ["Nun", "sie", "hin", "zum", "Tan\u00b7ze", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie gl\u00e4nze.", "tokens": ["Ob", "sie", "gl\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und das B\u00e4chlein wird so glatt,", "tokens": ["Und", "das", "B\u00e4ch\u00b7lein", "wird", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder zugemurmelt hat:", "tokens": ["Je\u00b7der", "zu\u00b7ge\u00b7mur\u00b7melt", "hat", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbamor bringt dir Kr\u00e4nze.\u00ab", "tokens": ["\u00bb", "a\u00b7mor", "bringt", "dir", "Kr\u00e4n\u00b7ze", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Alle Blumen schwesterlich", "tokens": ["Al\u00b7le", "Blu\u00b7men", "schwes\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dfen, k\u00fcssen, herzen sich", "tokens": ["Gr\u00fc\u00b7\u00dfen", ",", "k\u00fcs\u00b7sen", ",", "her\u00b7zen", "sich"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier im Kreise.", "tokens": ["Hier", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Jede wartet auf den Gott,", "tokens": ["Je\u00b7de", "war\u00b7tet", "auf", "den", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der so oft nur leichten Spott", "tokens": ["Der", "so", "oft", "nur", "leich\u00b7ten", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt nach seiner Weise.", "tokens": ["Giebt", "nach", "sei\u00b7ner", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nachtigall ist auch bestellt,", "tokens": ["Nach\u00b7ti\u00b7gall", "ist", "auch", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im Laub verstecket h\u00e4lt,", "tokens": ["Sich", "im", "Laub", "ver\u00b7ste\u00b7cket", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielt zum Tanze;", "tokens": ["Spielt", "zum", "Tan\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und ein jedes Gartenbeet,", "tokens": ["Und", "ein", "je\u00b7des", "Gar\u00b7ten\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon voll sch\u00f6ner T\u00e4nzer steht,", "tokens": ["Schon", "voll", "sch\u00f6\u00b7ner", "T\u00e4n\u00b7zer", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Vollmondglanze.", "tokens": ["In", "dem", "Voll\u00b7mond\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch die Frauen sehen kalt", "tokens": ["Doch", "die", "Frau\u00b7en", "se\u00b7hen", "kalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Herren jung und alt,", "tokens": ["Auf", "die", "Her\u00b7ren", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich br\u00fcsten;", "tokens": ["Und", "sich", "br\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Denn ein Gott, der gilt viel mehr,", "tokens": ["Denn", "ein", "Gott", ",", "der", "gilt", "viel", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der Nachbarn Lustverkehr,", "tokens": ["Als", "der", "Nach\u00b7barn", "Lust\u00b7ver\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zum Tanz sich r\u00fcsten.", "tokens": ["Die", "zum", "Tanz", "sich", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wagt sich nicht zum Tanz hinaus,", "tokens": ["Wagt", "sich", "nicht", "zum", "Tanz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht vergessen;", "tokens": ["Steht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Doch ihr Duft die Luft durchzieht,", "tokens": ["Doch", "ihr", "Duft", "die", "Luft", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Feuerwurm ergl\u00fcht,", "tokens": ["Und", "der", "Feu\u00b7er\u00b7wurm", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Fliegt ihr zu vermessen.", "tokens": ["Fliegt", "ihr", "zu", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Amor ist der Feuerwurm,", "tokens": ["A\u00b7mor", "ist", "der", "Feu\u00b7er\u00b7wurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Licht, das l\u00f6scht kein Sturm,", "tokens": ["Und", "sein", "Licht", ",", "das", "l\u00f6scht", "kein", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machts nur heller;", "tokens": ["Machts", "nur", "hel\u00b7ler", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und er leuchtet Liebchen vor,", "tokens": ["Und", "er", "leuch\u00b7tet", "Lieb\u00b7chen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchrt sie selbst zum Tanz vors Thor,", "tokens": ["F\u00fchrt", "sie", "selbst", "zum", "Tanz", "vors", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Tanz rauscht schneller.", "tokens": ["Und", "der", "Tanz", "rauscht", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Eintracht schien im bunten Saal,", "tokens": ["Ein\u00b7tracht", "schien", "im", "bun\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwietracht kommt zu aller Qual,", "tokens": ["Zwiet\u00b7racht", "kommt", "zu", "al\u00b7ler", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den beiden;", "tokens": ["Mit", "den", "bei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Weil der Gott von Lust und Leid", "tokens": ["Weil", "der", "Gott", "von", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer zuflog, sucht der Neid", "tokens": ["Ei\u00b7ner", "zu\u00b7flog", ",", "sucht", "der", "Neid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie mit List zu scheiden.", "tokens": ["Sie", "mit", "List", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Wie sie zornge Blicke richt',", "tokens": ["Wie", "sie", "zorn\u00b7ge", "Bli\u00b7cke", "richt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist verlegen;", "tokens": ["Ist", "ver\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Stetes Lachen l\u00e4\u00dft nicht gut,", "tokens": ["Ste\u00b7tes", "La\u00b7chen", "l\u00e4\u00dft", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar zu traurig sie nun thut,", "tokens": ["Gar", "zu", "trau\u00b7rig", "sie", "nun", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df sich viel bewegen.", "tokens": ["Mu\u00df", "sich", "viel", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Ob wir schon viel kl\u00fcger sind,", "tokens": ["Ob", "wir", "schon", "viel", "kl\u00fc\u00b7ger", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als dies liebe wei\u00dfe Kind,", "tokens": ["Als", "dies", "lie\u00b7be", "wei\u00b7\u00dfe", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft ", "tokens": ["Ruft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Kommt es uns doch nimmer ein,", "tokens": ["Kommt", "es", "uns", "doch", "nim\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor k\u00f6nne unser sein", "tokens": ["A\u00b7mor", "k\u00f6n\u00b7ne", "un\u00b7ser", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPOSAT", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem G\u00f6tterthrone.", "tokens": ["Auf", "dem", "G\u00f6t\u00b7ter\u00b7thro\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Doch wir bleiben hier allein,", "tokens": ["Doch", "wir", "blei\u00b7ben", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil wir ganz geruchlos rein", "tokens": ["Weil", "wir", "ganz", "ge\u00b7ruch\u00b7los", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen locken;", "tokens": ["Kei\u00b7nen", "lo\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "So die ", "tokens": ["So", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Weil sie niemand nehmen will,", "tokens": ["Weil", "sie", "nie\u00b7mand", "neh\u00b7men", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trotz der gro\u00dfen Glocken.", "tokens": ["Trotz", "der", "gro\u00b7\u00dfen", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Wie ein V\u00f6glein h\u00e4ngt am Zweig,", "tokens": ["Wie", "ein", "V\u00f6\u00b7glein", "h\u00e4ngt", "am", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Hat den Kelch ihm zugewandt", "tokens": ["Hat", "den", "Kelch", "ihm", "zu\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht von Ehre und von Stand,", "tokens": ["Spricht", "von", "Eh\u00b7re", "und", "von", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von dem Gewissen.", "tokens": ["Und", "von", "dem", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Nachtgev\u00f6gel ohne Zahl,", "tokens": ["Nacht\u00b7ge\u00b7v\u00f6\u00b7gel", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Zorne;", "tokens": ["In", "dem", "Zor\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Jedem ihre Dornen reicht,", "tokens": ["Je\u00b7dem", "ih\u00b7re", "Dor\u00b7nen", "reicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er an dem Gott hinstreicht,", "tokens": ["Da\u00df", "er", "an", "dem", "Gott", "hins\u00b7treicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihn blutig sporne.", "tokens": ["Und", "ihn", "blu\u00b7tig", "spor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Rittersporn und Eisenhut", "tokens": ["Rit\u00b7ter\u00b7sporn", "und", "Ei\u00b7sen\u00b7hut"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hlet sie im wilden Muth,", "tokens": ["W\u00e4h\u00b7let", "sie", "im", "wil\u00b7den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Fechten;", "tokens": ["Zu", "dem", "Fech\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Und das Tausendg\u00fcldenkraut", "tokens": ["Und", "das", "Tau\u00b7send\u00b7g\u00fcl\u00b7den\u00b7kraut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bietet sie zur Werbung laut,", "tokens": ["Bie\u00b7tet", "sie", "zur", "Wer\u00b7bung", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Lohn den Knechten.", "tokens": ["Als", "ein", "Lohn", "den", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Gleich der hohen dunklen Stadt,", "tokens": ["Gleich", "der", "ho\u00b7hen", "dunk\u00b7len", "