ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Schaden:", "tokens": ["Oh\u00b7ne", "Scha\u00b7den", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00bbst\u00f6ren lasse ich mich nicht,", "tokens": ["\u00bb", "st\u00f6\u00b7ren", "las\u00b7se", "ich", "mich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nne jeder ihren Wicht,", "tokens": ["G\u00f6n\u00b7ne", "je\u00b7der", "ih\u00b7ren", "Wicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ein Gott der Gnaden.\u00ab", "tokens": ["Bin", "ein", "Gott", "der", "Gna\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Nachtviole hebt das Haupt,", "tokens": ["Nacht\u00b7vi\u00b7o\u00b7le", "hebt", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Amors Feuer sanft bestaubt", "tokens": ["A\u00b7mors", "Feu\u00b7er", "sanft", "be\u00b7staubt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Wangen:", "tokens": ["Ih\u00b7re", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00bbjeder regt der Gott die Brust,", "tokens": ["\u00bb", "je\u00b7der", "regt", "der", "Gott", "die", "Brust", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nnt dies Heute meiner Lust,", "tokens": ["G\u00f6nnt", "dies", "Heu\u00b7te", "mei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00dft mich einmal prangen.", "tokens": ["La\u00dft", "mich", "ein\u00b7mal", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Morgen ist ein andrer Tag,", "tokens": ["Mor\u00b7gen", "ist", "ein", "an\u00b7drer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo er andre lieben mag", "tokens": ["Wo", "er", "and\u00b7re", "lie\u00b7ben", "mag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach Gefallen;", "tokens": ["Nach", "Ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Zeigt nur, da\u00df ihr w\u00fcrdig seid", "tokens": ["Zeigt", "nur", ",", "da\u00df", "ihr", "w\u00fcr\u00b7dig", "seid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Liebe, die sich weiht", "tokens": ["Die\u00b7ser", "Lie\u00b7be", ",", "die", "sich", "weiht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der ", "tokens": ["In", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.20": {"line.1": {"text": "Frau Peone kl\u00fcglich denk", "tokens": ["Frau", "Pe\u00b7o\u00b7ne", "kl\u00fcg\u00b7lich", "denk"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An das goldene Geschenk,", "tokens": ["An", "das", "gol\u00b7de\u00b7ne", "Ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heb' den Schleier,", "tokens": ["Heb'", "den", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sieh die Flamme an dem Platz,", "tokens": ["Sieh", "die", "Flam\u00b7me", "an", "dem", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der jetzt tr\u00e4gt den reichen Schatz,", "tokens": ["Der", "jetzt", "tr\u00e4gt", "den", "rei\u00b7chen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heb' ihn auf den Freier;", "tokens": ["Heb'", "ihn", "auf", "den", "Frei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "Rose, sieh des Sternes Schein,", "tokens": ["Ro\u00b7se", ",", "sieh", "des", "Ster\u00b7nes", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er will ein Komet nun sein,", "tokens": ["Er", "will", "ein", "Ko\u00b7met", "nun", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er will schie\u00dfen,", "tokens": ["Er", "will", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Spann die weichen Bl\u00e4tter aus,", "tokens": ["Spann", "die", "wei\u00b7chen", "Bl\u00e4t\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00e4llt der Stern dir nicht ins Haus,", "tokens": ["F\u00e4llt", "der", "Stern", "dir", "nicht", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00e4llt er dir zu F\u00fc\u00dfen.", "tokens": ["F\u00e4llt", "er", "dir", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Und ihr Lilien, seht herab,", "tokens": ["Und", "ihr", "Li\u00b7li\u00b7en", ",", "seht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steht er nicht auf einem Grab,", "tokens": ["Steht", "er", "nicht", "auf", "ei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht die Flammen,", "tokens": ["Seht", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sieh ihn, der mit Irrlichtschein", "tokens": ["Sieh", "ihn", ",", "der", "mit", "Irr\u00b7licht\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinkt in deinen Kelch hinein,", "tokens": ["Sinkt", "in", "dei\u00b7nen", "Kelch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nacht bringt euch zusammen.", "tokens": ["Nacht", "bringt", "euch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}