{"textgrid.poem.39793": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Himmels Sch\u00fctzlinge, die S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels Sch\u00fctzlinge, die S\u00e4nger", "tokens": ["Des", "Him\u00b7mels", "Sch\u00fctz\u00b7lin\u00b7ge", ",", "die", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Erd' und ihrer Lieblichkeit,", "tokens": ["Der", "Erd'", "und", "ih\u00b7rer", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df das Geschick sonst g\u00fctig l\u00e4nger", "tokens": ["Hie\u00df", "das", "Ge\u00b7schick", "sonst", "g\u00fc\u00b7tig", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Verweilen in der fl\u00fccht'gen Zeit.", "tokens": ["Ver\u00b7wei\u00b7len", "in", "der", "fl\u00fccht'\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es gab den graugelockten Greisen", "tokens": ["Es", "gab", "den", "grau\u00b7ge\u00b7lock\u00b7ten", "Grei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die junge Leier in den Arm,", "tokens": ["Die", "jun\u00b7ge", "Lei\u00b7er", "in", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df sie Wein und Liebe preisen", "tokens": ["Und", "lie\u00df", "sie", "Wein", "und", "Lie\u00b7be", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von langer Sp\u00e4tlingssonne warm.", "tokens": ["Von", "lan\u00b7ger", "Sp\u00e4t\u00b7lings\u00b7son\u00b7ne", "warm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die unersch\u00f6pften Lieder sang", "tokens": ["Die", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Lie\u00b7der", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4chelte, wenn nicht im Zorne", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", ",", "wenn", "nicht", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leier, Freiheit fodernd, klang;", "tokens": ["Die", "Lei\u00b7er", ",", "Frei\u00b7heit", "fo\u00b7dernd", ",", "klang", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, warum ri\u00df vom Quell der Musen,", "tokens": ["Ach", ",", "wa\u00b7rum", "ri\u00df", "vom", "Quell", "der", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus der treuen Liebe Wacht,", "tokens": ["Und", "aus", "der", "treu\u00b7en", "Lie\u00b7be", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von des Herzensfreundes Busen", "tokens": ["Und", "von", "des", "Her\u00b7zens\u00b7freun\u00b7des", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich fr\u00fch die schwarze Mitternacht?", "tokens": ["Dich", "fr\u00fch", "die", "schwar\u00b7ze", "Mit\u00b7ter\u00b7nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir fragen nicht \u2013 du warst der Bote", "tokens": ["Wir", "fra\u00b7gen", "nicht", "\u2013", "du", "warst", "der", "Bo\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines Volkes Auferstehn,", "tokens": ["Von", "ei\u00b7nes", "Vol\u00b7kes", "Auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesandt noch vor dem Morgenrote,", "tokens": ["Ge\u00b7sandt", "noch", "vor", "dem", "Mor\u00b7gen\u00b7ro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei der k\u00fchlen L\u00fcfte Wehn.", "tokens": ["Und", "bei", "der", "k\u00fch\u00b7len", "L\u00fcf\u00b7te", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch hat dein Sang sich aufgeschwungen,", "tokens": ["Doch", "hat", "dein", "Sang", "sich", "auf\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eh' der Tag im Osten graut;", "tokens": ["Noch", "eh'", "der", "Tag", "im", "Os\u00b7ten", "graut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt ist die Sonne durchgedrungen:", "tokens": ["Jetzt", "ist", "die", "Son\u00b7ne", "durch\u00b7ge\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl dir, du hast sie noch geschaut.", "tokens": ["Wohl", "dir", ",", "du", "hast", "sie", "noch", "ge\u00b7schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Hauch in deinen Liedern lebte,", "tokens": ["Der", "Hauch", "in", "dei\u00b7nen", "Lie\u00b7dern", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der einst Hellenenbrust geschwellt,", "tokens": ["Der", "einst", "Hel\u00b7le\u00b7nen\u00b7brust", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem verkl\u00e4rten Auge schwebte", "tokens": ["Vor", "dem", "ver\u00b7kl\u00e4r\u00b7ten", "Au\u00b7ge", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jugendvolkes G\u00f6tterwelt.", "tokens": ["Des", "Ju\u00b7gend\u00b7vol\u00b7kes", "G\u00f6t\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und deine Sendung war vollendet;", "tokens": ["Und", "dei\u00b7ne", "Sen\u00b7dung", "war", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da trat aus der Gestalten Chor", "tokens": ["Da", "trat", "aus", "der", "Ge\u00b7stal\u00b7ten", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sanfte J\u00fcngling, abgewendet.", "tokens": ["Der", "sanf\u00b7te", "J\u00fcng\u00b7ling", ",", "ab\u00b7ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit der gesenkten Fackel vor.", "tokens": ["Mit", "der", "ge\u00b7senk\u00b7ten", "Fa\u00b7ckel", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Still griffest du zum Wanderstabe,", "tokens": ["Still", "grif\u00b7fest", "du", "zum", "Wan\u00b7der\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du zogst noch durch dein Erdenland,", "tokens": ["Du", "zogst", "noch", "durch", "dein", "Er\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gr\u00fc\u00dftest auf dem Weg zum Grabe", "tokens": ["Und", "gr\u00fc\u00df\u00b7test", "auf", "dem", "Weg", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch manches Herz, das dich verstand.", "tokens": ["Noch", "man\u00b7ches", "Herz", ",", "das", "dich", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und schiedst, und lie\u00dfest deine Lieben;", "tokens": ["Und", "schiedst", ",", "und", "lie\u00b7\u00dfest", "dei\u00b7ne", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein reicher Morgen war gelebt;", "tokens": ["Dein", "rei\u00b7cher", "Mor\u00b7gen", "war", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns aber ist dein Lied geblieben,", "tokens": ["Uns", "a\u00b7ber", "ist", "dein", "Lied", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das durch die Brust lebendig bebt.", "tokens": ["Das", "durch", "die", "Brust", "le\u00b7ben\u00b7dig", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}