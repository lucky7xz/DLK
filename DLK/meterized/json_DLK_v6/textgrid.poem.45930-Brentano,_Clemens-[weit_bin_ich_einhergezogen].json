{"textgrid.poem.45930": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[weit bin ich einhergezogen]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit bin ich einhergezogen", "tokens": ["Weit", "bin", "ich", "ein\u00b7her\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Berg und \u00fcber Tal,", "tokens": ["\u00dc\u00b7ber", "Berg", "und", "\u00fc\u00b7ber", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der treue Himmelsbogen", "tokens": ["Der", "treu\u00b7e", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er umgibt mich \u00fcberall.", "tokens": ["Er", "um\u00b7gibt", "mich", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter Eichen, unter Buchen,", "tokens": ["Un\u00b7ter", "Ei\u00b7chen", ",", "un\u00b7ter", "Bu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem wilden Wasserfall", "tokens": ["An", "dem", "wil\u00b7den", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich nun die Herberg suchen", "tokens": ["Mu\u00df", "ich", "nun", "die", "Her\u00b7berg", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei der lieb Frau Nachtigall.", "tokens": ["Bei", "der", "lieb", "Frau", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die im br\u00fcnst'gen Abendliede", "tokens": ["Die", "im", "br\u00fcnst'\u00b7gen", "A\u00b7ben\u00b7dlie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre G\u00e4ste wohl bedenkt,", "tokens": ["Ih\u00b7re", "G\u00e4s\u00b7te", "wohl", "be\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sich Schlaf und Traum und Friede", "tokens": ["Bis", "sich", "Schlaf", "und", "Traum", "und", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die m\u00fcde Seele senkt.", "tokens": ["Auf", "die", "m\u00fc\u00b7de", "See\u00b7le", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich h\u00f6r' dieselben Klagen", "tokens": ["Und", "ich", "h\u00f6r'", "die\u00b7sel\u00b7ben", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich h\u00f6r' dieselbe Lust", "tokens": ["Und", "ich", "h\u00f6r'", "die\u00b7sel\u00b7be", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und ich f\u00fchl' das Herz mir schlagen", "tokens": ["Und", "ich", "f\u00fchl'", "das", "Herz", "mir", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier wie dort in meiner Brust.", "tokens": ["Hier", "wie", "dort", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus dem Flu\u00df, der mir zu F\u00fc\u00dfen", "tokens": ["Aus", "dem", "Flu\u00df", ",", "der", "mir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt mit freudigem Gebraus,", "tokens": ["Spielt", "mit", "freu\u00b7di\u00b7gem", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich dieselben Sterne gr\u00fc\u00dfen", "tokens": ["Mich", "die\u00b7sel\u00b7ben", "Ster\u00b7ne", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so bin ich hier zu Haus.", "tokens": ["Und", "so", "bin", "ich", "hier", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Echo nimm dir recht zu Herzen", "tokens": ["E\u00b7cho", "nimm", "dir", "recht", "zu", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erlern' die Melodie", "tokens": ["Und", "er\u00b7lern'", "die", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Freuden, meiner Schmerzen:", "tokens": ["Mei\u00b7ner", "Freu\u00b7den", ",", "mei\u00b7ner", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ameleya! Ameley!", "tokens": ["A\u00b7me\u00b7le\u00b7ya", "!", "A\u00b7me\u00b7ley", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00fchet stolz ihr K\u00f6nigskerzen,", "tokens": ["Bl\u00fc\u00b7het", "stolz", "ihr", "K\u00f6\u00b7nigs\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ameleya! Ameley!", "tokens": ["A\u00b7me\u00b7le\u00b7ya", "!", "A\u00b7me\u00b7ley", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wunderinseln, sel'ge Augen,", "tokens": ["Wun\u00b7de\u00b7rin\u00b7seln", ",", "sel'\u00b7ge", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ein liebes Antlitz sehn,", "tokens": ["Die", "ein", "lie\u00b7bes", "Ant\u00b7litz", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Monde untertauchen,", "tokens": ["In", "dem", "Mon\u00b7de", "un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Sonne auferstehn.", "tokens": ["In", "der", "Son\u00b7ne", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sonn und Mond, ihr lichten H\u00fcgel,", "tokens": ["Sonn", "und", "Mond", ",", "ihr", "lich\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dfet ein die ird'sche Kluft", "tokens": ["Schlie\u00b7\u00dfet", "ein", "die", "ird'\u00b7sche", "Kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Leben senkt den Fl\u00fcgel", "tokens": ["Und", "das", "Le\u00b7ben", "senkt", "den", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Traumes Zaubergruft.", "tokens": ["In", "des", "Trau\u00b7mes", "Zau\u00b7ber\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo die Tiefe sich entsiegelt,", "tokens": ["Wo", "die", "Tie\u00b7fe", "sich", "ent\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Liebe frank und frei", "tokens": ["Und", "die", "Lie\u00b7be", "frank", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der ganzen Seele spiegelt", "tokens": ["In", "der", "gan\u00b7zen", "See\u00b7le", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ameleya! Ameley!", "tokens": ["A\u00b7me\u00b7le\u00b7ya", "!", "A\u00b7me\u00b7ley", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}