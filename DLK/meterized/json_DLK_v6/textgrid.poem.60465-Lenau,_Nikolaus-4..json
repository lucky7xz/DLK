{"textgrid.poem.60465": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Donners Kl\u00e4ngen lauschet", "tokens": ["In", "des", "Don\u00b7ners", "Kl\u00e4n\u00b7gen", "lau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziska der verwandten Seele,", "tokens": ["Zis\u00b7ka", "der", "ver\u00b7wand\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ein Mann ihm naht behutsam,", "tokens": ["Als", "ein", "Mann", "ihm", "naht", "be\u00b7hut\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprechend aus ged\u00e4mpfter Kehle:", "tokens": ["Spre\u00b7chend", "aus", "ge\u00b7d\u00e4mpf\u00b7ter", "Keh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwelche Wonne mu\u00df durchs gro\u00dfe", "tokens": ["\u00bb", "wel\u00b7che", "Won\u00b7ne", "mu\u00df", "durchs", "gro\u00b7\u00dfe"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "VMFIN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz dem Donnergotte wallen,", "tokens": ["Herz", "dem", "Don\u00b7ner\u00b7got\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er l\u00e4\u00dft die starke Stimme", "tokens": ["Wenn", "er", "l\u00e4\u00dft", "die", "star\u00b7ke", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzend durch die L\u00fcfte schallen!", "tokens": ["Jauch\u00b7zend", "durch", "die", "L\u00fcf\u00b7te", "schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welche Wonne in der Feldschlacht", "tokens": ["Wel\u00b7che", "Won\u00b7ne", "in", "der", "Feld\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcht durchs edle Heldenmark", "tokens": ["Gl\u00fcht", "durchs", "ed\u00b7le", "Hel\u00b7den\u00b7mark"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einem Mann wie du, o Ziska,", "tokens": ["Ei\u00b7nem", "Mann", "wie", "du", ",", "o", "Zis\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPER", "$,", "FM", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Aber s\u00fc\u00dfre Wonne gibt es,", "tokens": ["A\u00b7ber", "s\u00fc\u00df\u00b7re", "Won\u00b7ne", "gibt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als sie wird dem Helden kund,", "tokens": ["Als", "sie", "wird", "dem", "Hel\u00b7den", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, wie Wetter kalte Schlo\u00dfen,", "tokens": ["Der", ",", "wie", "Wet\u00b7ter", "kal\u00b7te", "Schlo\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichen hagelt auf den Grund:", "tokens": ["Lei\u00b7chen", "ha\u00b7gelt", "auf", "den", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00dfre Wonne, Liebeswonne;", "tokens": ["S\u00fc\u00df\u00b7re", "Won\u00b7ne", ",", "Lie\u00b7bes\u00b7won\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dein Herz ihr nie geschlagen,", "tokens": ["Hat", "dein", "Herz", "ihr", "nie", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als du einst am K\u00f6nigshofe", "tokens": ["Als", "du", "einst", "am", "K\u00f6\u00b7nigs\u00b7ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebtest in begl\u00fcckten Tagen?", "tokens": ["Leb\u00b7test", "in", "be\u00b7gl\u00fcck\u00b7ten", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nigin Sophia sandte", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "So\u00b7phia", "sand\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mich zu dir und deinem Grimme,", "tokens": ["Mich", "zu", "dir", "und", "dei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich in der Brust dir wecke", "tokens": ["Da\u00df", "ich", "in", "der", "Brust", "dir", "we\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine holde Friedensstimme;", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "Frie\u00b7denss\u00b7tim\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nigin Sophia sendet", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "So\u00b7phia", "sen\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "VVFIN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Einen Gru\u00df dir und die Kunde:", "tokens": ["Ei\u00b7nen", "Gru\u00df", "dir", "und", "die", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Isabella, die du liebtest,", "tokens": ["I\u00b7sa\u00b7bel\u00b7la", ",", "die", "du", "lieb\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trauert sich um dich zugrunde.", "tokens": ["Trau\u00b7ert", "sich", "um", "dich", "zu\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als ich scheidend stieg zu Rosse,", "tokens": ["Als", "ich", "schei\u00b7dend", "stieg", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich noch die Edeldame", "tokens": ["Sah", "ich", "noch", "die", "E\u00b7del\u00b7da\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkend ihr gebleichtes Antlitz,", "tokens": ["Sen\u00b7kend", "ihr", "ge\u00b7bleich\u00b7tes", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still verzehrt von Liebesgrame.", "tokens": ["Still", "ver\u00b7zehrt", "von", "Lie\u00b7bes\u00b7gra\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Eilend spornt ich meinen Renner,", "tokens": ["Ei\u00b7lend", "spornt", "ich", "mei\u00b7nen", "Ren\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn die sch\u00f6nste Frau indessen", "tokens": ["Denn", "die", "sch\u00f6ns\u00b7te", "Frau", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welket rasch und unaufhaltsam,", "tokens": ["Wel\u00b7ket", "rasch", "und", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirbt, wenn du sie hast vergessen.", "tokens": ["Stirbt", ",", "wenn", "du", "sie", "hast", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Kehre heim, dir ist vergeben;", "tokens": ["Keh\u00b7re", "heim", ",", "dir", "ist", "ver\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df des Glaubens wilde Streiter,", "tokens": ["La\u00df", "des", "Glau\u00b7bens", "wil\u00b7de", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm der Liebe sichern Himmel,", "tokens": ["Nimm", "der", "Lie\u00b7be", "si\u00b7chern", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dir winkt vielleicht kein zweiter.\u00ab", "tokens": ["Denn", "dir", "winkt", "viel\u00b7leicht", "kein", "zwei\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Also fl\u00fcsternd sprach der Bote,", "tokens": ["Al\u00b7so", "fl\u00fcs\u00b7ternd", "sprach", "der", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheu sich schmiegend an die F\u00f6hre;", "tokens": ["Scheu", "sich", "schmie\u00b7gend", "an", "die", "F\u00f6h\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm entgegnet Ziska leise,", "tokens": ["Ihm", "ent\u00b7geg\u00b7net", "Zis\u00b7ka", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es kein Hussite h\u00f6re:", "tokens": ["Da\u00df", "es", "kein", "Hus\u00b7si\u00b7te", "h\u00f6\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbo sie sterbe! als das reinste", "tokens": ["\u00bb", "o", "sie", "ster\u00b7be", "!", "als", "das", "reins\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "PPER", "VVFIN", "$.", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Opfer sei sie hingegeben", "tokens": ["Op\u00b7fer", "sei", "sie", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die Freiheit, der ich opfre", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", ",", "der", "ich", "opf\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Freude, all mein Leben.", "tokens": ["Je\u00b7de", "Freu\u00b7de", ",", "all", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Isabella, Stern der Liebe,", "tokens": ["I\u00b7sa\u00b7bel\u00b7la", ",", "Stern", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinke! \u2013 meinem Pfade mu\u00df", "tokens": ["Sin\u00b7ke", "!", "\u2013", "mei\u00b7nem", "Pfa\u00b7de", "mu\u00df"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "PPOSAT", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchten nur des Zornes Fackel; \u2013", "tokens": ["Leuch\u00b7ten", "nur", "des", "Zor\u00b7nes", "Fa\u00b7ckel", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring ihr meinen letzten Gru\u00df!", "tokens": ["Bring", "ihr", "mei\u00b7nen", "letz\u00b7ten", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch nun raffe dich von hinnen,", "tokens": ["Doch", "nun", "raf\u00b7fe", "dich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eile, Bote, und entweiche,", "tokens": ["Ei\u00b7le", ",", "Bo\u00b7te", ",", "und", "ent\u00b7wei\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil du nanntest einen Namen,", "tokens": ["Weil", "du", "nann\u00b7test", "ei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dich sch\u00fctzt vor meinem Streiche!\u00ab", "tokens": ["Der", "dich", "sch\u00fctzt", "vor", "mei\u00b7nem", "Strei\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}