{"textgrid.poem.47117": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns beiden ist hier die Luft zu schwer", "tokens": ["Uns", "bei\u00b7den", "ist", "hier", "die", "Luft", "zu", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "VAFIN", "ADV", "ART", "NN", "PTKA", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Land voll Sturmesgetose,", "tokens": ["Im", "Land", "voll", "Stur\u00b7mes\u00b7ge\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir der Nachtigall, und noch mehr", "tokens": ["Mir", "der", "Nach\u00b7ti\u00b7gall", ",", "und", "noch", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "KON", "ADV", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Meiner Freundin, der Rose.", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", ",", "der", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Die Ros' ist worden krank und bleich,", "tokens": ["Die", "Ros'", "ist", "wor\u00b7den", "krank", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich bin rauh geworden.", "tokens": ["Und", "ich", "bin", "rauh", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O d\u00fcrften wir wandern allzugleich", "tokens": ["O", "d\u00fcrf\u00b7ten", "wir", "wan\u00b7dern", "all\u00b7zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gen S\u00fcden aus dem Norden!", "tokens": ["Gen", "S\u00fc\u00b7den", "aus", "dem", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O da\u00df ein goldbeschwingter Wind", "tokens": ["O", "da\u00df", "ein", "gold\u00b7be\u00b7schwing\u00b7ter", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns beide n\u00e4hm' auf die Fl\u00fcgel,", "tokens": ["Uns", "bei\u00b7de", "n\u00e4hm'", "auf", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und tr\u00fcge dahin uns fr\u00fchlingslind", "tokens": ["Und", "tr\u00fc\u00b7ge", "da\u00b7hin", "uns", "fr\u00fch\u00b7lings\u00b7lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "PPER", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur Stadt der sieben H\u00fcgel.", "tokens": ["Zur", "Stadt", "der", "sie\u00b7ben", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00dcber die sieben H\u00fcgel dahin,", "tokens": ["\u00dc\u00b7ber", "die", "sie\u00b7ben", "H\u00fc\u00b7gel", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "PAV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dort, wo die L\u00fcfte sind reiner,", "tokens": ["Dort", ",", "wo", "die", "L\u00fcf\u00b7te", "sind", "rei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Noch immer steht dahin mein Sinn,", "tokens": ["Noch", "im\u00b7mer", "steht", "da\u00b7hin", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Gebirg' der Lateiner.", "tokens": ["Zum", "Ge\u00b7bir\u00b7g'", "der", "La\u00b7tei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort sa\u00df ich einen Sommer so froh,", "tokens": ["Dort", "sa\u00df", "ich", "ei\u00b7nen", "Som\u00b7mer", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Doch mu\u00dft' ich der Lieb' entbehren;", "tokens": ["Doch", "mu\u00dft'", "ich", "der", "Lieb'", "ent\u00b7beh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie wohl erst m\u00fc\u00dft' es mir werden, wo", "tokens": ["Wie", "wohl", "erst", "m\u00fc\u00dft'", "es", "mir", "wer\u00b7den", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "PPER", "PPER", "VAINF", "$,", "PWAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir dort vereinigt w\u00e4ren!", "tokens": ["Wir", "dort", "ver\u00b7ei\u00b7nigt", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}