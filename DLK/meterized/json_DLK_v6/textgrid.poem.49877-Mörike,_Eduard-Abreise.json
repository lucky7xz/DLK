{"textgrid.poem.49877": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Abreise", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fertig schon zur Abfahrt steht der Wagen,", "tokens": ["Fer\u00b7tig", "schon", "zur", "Ab\u00b7fahrt", "steht", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das Posthorn bl\u00e4st zum letzten Male.", "tokens": ["Und", "das", "Post\u00b7horn", "bl\u00e4st", "zum", "letz\u00b7ten", "Ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sagt, wo bleibt der vierte Mann so lange?", "tokens": ["Sagt", ",", "wo", "bleibt", "der", "vier\u00b7te", "Mann", "so", "lan\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruft ihn, soll er nicht dahinten bleiben!", "tokens": ["Ruft", "ihn", ",", "soll", "er", "nicht", "da\u00b7hin\u00b7ten", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "PPER", "PTKNEG", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u2013 Indes f\u00e4llt ein rascher Sommerregen;", "tokens": ["\u2013", "In\u00b7des", "f\u00e4llt", "ein", "ra\u00b7scher", "Som\u00b7mer\u00b7re\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eh man hundert z\u00e4hlt, ist er vor\u00fcber;", "tokens": ["Eh", "man", "hun\u00b7dert", "z\u00e4hlt", ",", "ist", "er", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "CARD", "VVFIN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Fast zu kurz, den hei\u00dfen Staub zu l\u00f6schen;", "tokens": ["Fast", "zu", "kurz", ",", "den", "hei\u00b7\u00dfen", "Staub", "zu", "l\u00f6\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch auch diese Letzung ist willkommen.", "tokens": ["Doch", "auch", "die\u00b7se", "Let\u00b7zung", "ist", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "K\u00fchlung f\u00fcllt und Wohlgeruch den weiten", "tokens": ["K\u00fch\u00b7lung", "f\u00fcllt", "und", "Wohl\u00b7ge\u00b7ruch", "den", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Platz und an den H\u00e4usern ringsum \u00f6ffnet", "tokens": ["Platz", "und", "an", "den", "H\u00e4u\u00b7sern", "ring\u00b7sum", "\u00f6ff\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sich ein Blumenfenster um das andre.", "tokens": ["Sich", "ein", "Blu\u00b7men\u00b7fens\u00b7ter", "um", "das", "and\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Endlich kommt der junge Mann. Geschwinde!", "tokens": ["End\u00b7lich", "kommt", "der", "jun\u00b7ge", "Mann", ".", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Eingestiegen! \u2013 Und fort rollt der Wagen.", "tokens": ["Ein\u00b7ge\u00b7stie\u00b7gen", "!", "\u2013", "Und", "fort", "rollt", "der", "Wa\u00b7gen", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Aber sehet, auf dem nassen Pflaster", "tokens": ["A\u00b7ber", "se\u00b7het", ",", "auf", "dem", "nas\u00b7sen", "Pflas\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Vor dem Posthaus, wo er stillgehalten,", "tokens": ["Vor", "dem", "Post\u00b7haus", ",", "wo", "er", "still\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "L\u00e4\u00dft er einen trocknen Fleck zur\u00fccke,", "tokens": ["L\u00e4\u00dft", "er", "ei\u00b7nen", "trock\u00b7nen", "Fleck", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Lang und breit, sogar die R\u00e4der sieht man", "tokens": ["Lang", "und", "breit", ",", "so\u00b7gar", "die", "R\u00e4\u00b7der", "sieht", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Angezeigt und wo die Pferde standen.", "tokens": ["An\u00b7ge\u00b7zeigt", "und", "wo", "die", "Pfer\u00b7de", "stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Aber dort in jenem h\u00fcbschen Hause,", "tokens": ["A\u00b7ber", "dort", "in", "je\u00b7nem", "h\u00fcb\u00b7schen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Drin der J\u00fcngling sich so lang verweilte,", "tokens": ["Drin", "der", "J\u00fcng\u00b7ling", "sich", "so", "lang", "ver\u00b7weil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Steht ein M\u00e4dchen hinterm Fensterladen,", "tokens": ["Steht", "ein", "M\u00e4d\u00b7chen", "hin\u00b7term", "Fens\u00b7ter\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Blicket auf die wei\u00df gela\u00dfne Stelle,", "tokens": ["Bli\u00b7cket", "auf", "die", "wei\u00df", "ge\u00b7la\u00df\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "H\u00e4lt ihr T\u00fcchlein vors Gesicht und weinet.", "tokens": ["H\u00e4lt", "ihr", "T\u00fcch\u00b7lein", "vors", "Ge\u00b7sicht", "und", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Mag es ihr so ernst sein? Ohne Zweifel;", "tokens": ["Mag", "es", "ihr", "so", "ernst", "sein", "?", "Oh\u00b7ne", "Zwei\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VAINF", "$.", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Doch der Jammer wird nicht lange w\u00e4hren:", "tokens": ["Doch", "der", "Jam\u00b7mer", "wird", "nicht", "lan\u00b7ge", "w\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "M\u00e4dchenaugen, wi\u00dft ihr, trocknen hurtig,", "tokens": ["M\u00e4d\u00b7chen\u00b7au\u00b7gen", ",", "wi\u00dft", "ihr", ",", "trock\u00b7nen", "hur\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Und eh auf dem Markt die Steine wieder", "tokens": ["Und", "eh", "auf", "dem", "Markt", "die", "Stei\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Alle hell geworden von der Sonne,", "tokens": ["Al\u00b7le", "hell", "ge\u00b7wor\u00b7den", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VAPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "K\u00f6nnet ihr den Wildfang lachen h\u00f6ren.", "tokens": ["K\u00f6n\u00b7net", "ihr", "den", "Wild\u00b7fang", "la\u00b7chen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}