{"textgrid.poem.28622": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Schwestern, lacht ihr nicht der alber-klugen Lappen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Schwestern, lacht ihr nicht der alber-klugen Lappen,", "tokens": ["Ihr", "Schwes\u00b7tern", ",", "lacht", "ihr", "nicht", "der", "al\u00b7ber\u00b7klu\u00b7gen", "Lap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Damen sperren ein als wie in blinde Kappen", "tokens": ["Die", "Da\u00b7men", "sper\u00b7ren", "ein", "als", "wie", "in", "blin\u00b7de", "Kap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "KOUS", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und halten gar f\u00fcr sch\u00f6n, wann unsre sch\u00f6nste Zier,", "tokens": ["Und", "hal\u00b7ten", "gar", "f\u00fcr", "sch\u00f6n", ",", "wann", "uns\u00b7re", "sch\u00f6ns\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJD", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sch\u00f6nen Augen Liecht, steht selten f\u00fcr der Th\u00fcr?", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Liecht", ",", "steht", "sel\u00b7ten", "f\u00fcr", "der", "Th\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach denckt doch, denckt doch nach! durch finstres sauer sehen", "tokens": ["Ach", "denckt", "doch", ",", "denckt", "doch", "nach", "!", "durch", "finst\u00b7res", "sau\u00b7er", "se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "PTKVZ", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist Liebe nie gestifft, und nie kein Bund geschehen;", "tokens": ["Ist", "Lie\u00b7be", "nie", "ge\u00b7stifft", ",", "und", "nie", "kein", "Bund", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$,", "KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann Damen steht es zu, da\u00df ihrer \u00c4uglein Schein", "tokens": ["Dann", "Da\u00b7men", "steht", "es", "zu", ",", "da\u00df", "ih\u00b7rer", "\u00c4ug\u00b7lein", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Soll wie das Firmament frey zu beschauen seyn", "tokens": ["Soll", "wie", "das", "Fir\u00b7ma\u00b7ment", "frey", "zu", "be\u00b7schau\u00b7en", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "ADJD", "PTKZU", "VVINF", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von iedem, der da wil. Was dienen uns die Strahlen", "tokens": ["Von", "ie\u00b7dem", ",", "der", "da", "wil", ".", "Was", "die\u00b7nen", "uns", "die", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "VMFIN", "$.", "PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Sonne bey der Nacht? Wer lobt de\u00df K\u00fcnstlers mahlen,", "tokens": ["Der", "Son\u00b7ne", "bey", "der", "Nacht", "?", "Wer", "lobt", "de\u00df", "K\u00fcnst\u00b7lers", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Daf\u00fcr ein Umhang schwebt? Soll die, die lebt und lacht,", "tokens": ["Da\u00b7f\u00fcr", "ein", "Um\u00b7hang", "schwebt", "?", "Soll", "die", ",", "die", "lebt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$.", "VMFIN", "ART", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch f\u00fcr der rechten Zeit de\u00df sterbens schwartze Nacht", "tokens": ["Noch", "f\u00fcr", "der", "rech\u00b7ten", "Zeit", "de\u00df", "ster\u00b7bens", "schwart\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr ins Gesichte ziehn? Kans dann Natur auch leiden,", "tokens": ["Ihr", "ins", "Ge\u00b7sich\u00b7te", "ziehn", "?", "Kans", "dann", "Na\u00b7tur", "auch", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$.", "VMFIN", "ADV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das so man sch\u00e4nden soll und soll zu brauchen meiden,", "tokens": ["Das", "so", "man", "sch\u00e4n\u00b7den", "soll", "und", "soll", "zu", "brau\u00b7chen", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "VVINF", "VMFIN", "KON", "VMFIN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was sie zu brauchen gab? Wer munter um sich schaut,", "tokens": ["Was", "sie", "zu", "brau\u00b7chen", "gab", "?", "Wer", "mun\u00b7ter", "um", "sich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "PWS", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der gibt von sich an Tag, da\u00df er ihm selbsten traut", "tokens": ["Der", "gibt", "von", "sich", "an", "Tag", ",", "da\u00df", "er", "ihm", "selbs\u00b7ten", "traut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und gut Gewissen hat, das sich f\u00fcr nichts entsetzet", "tokens": ["Und", "gut", "Ge\u00b7wis\u00b7sen", "hat", ",", "das", "sich", "f\u00fcr", "nichts", "ent\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,", "PRELS", "PRF", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und nicht zu fliehen denckt, dieweil es nicht verletzet.", "tokens": ["Und", "nicht", "zu", "flie\u00b7hen", "denckt", ",", "die\u00b7weil", "es", "nicht", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Auge, das nicht kan ein fremdes Auge sehn,", "tokens": ["Ein", "Au\u00b7ge", ",", "das", "nicht", "kan", "ein", "frem\u00b7des", "Au\u00b7ge", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wei\u00df, was geschehen ist, wei\u00df, was soll noch geschehn.", "tokens": ["Wei\u00df", ",", "was", "ge\u00b7sche\u00b7hen", "ist", ",", "wei\u00df", ",", "was", "soll", "noch", "ge\u00b7schehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVPP", "VAFIN", "$,", "VVFIN", "$,", "PWS", "VMFIN", "ADV", "VVPP", "$."], "meter": "+--+--+-++-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Das nicht zu r\u00fchmen ist. Nein! nein! soll dieses gelten,", "tokens": ["Das", "nicht", "zu", "r\u00fch\u00b7men", "ist", ".", "Nein", "!", "nein", "!", "soll", "die\u00b7ses", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$.", "PTKANT", "$.", "PTKANT", "$.", "VMFIN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die allerbeste Kunst zu tilgen und zu schelten,", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7te", "Kunst", "zu", "til\u00b7gen", "und", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wodurch sich Damen sonst fein spielen ein, mit List", "tokens": ["Wo\u00b7durch", "sich", "Da\u00b7men", "sonst", "fein", "spie\u00b7len", "ein", ",", "mit", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "NN", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bezaubern einen Sinn, der sonst noch hatte Frist?", "tokens": ["Be\u00b7zau\u00b7bern", "ei\u00b7nen", "Sinn", ",", "der", "sonst", "noch", "hat\u00b7te", "Frist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das mu\u00df mit nichten seyn! der Augen klare Blicke", "tokens": ["Das", "mu\u00df", "mit", "nich\u00b7ten", "seyn", "!", "der", "Au\u00b7gen", "kla\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PIS", "VAINF", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sind unsre st\u00e4rckste Krafft, sind unsre Band und Stricke,", "tokens": ["Sind", "uns\u00b7re", "st\u00e4rcks\u00b7te", "Krafft", ",", "sind", "uns\u00b7re", "Band", "und", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dadurch uns f\u00e4llt ins Garn ein Wild, das uns gef\u00e4llt", "tokens": ["Da\u00b7durch", "uns", "f\u00e4llt", "ins", "Garn", "ein", "Wild", ",", "das", "uns", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sonst zu unsrer Gunst sich etwa fl\u00fcchtig stellt.", "tokens": ["Und", "sonst", "zu", "uns\u00b7rer", "Gunst", "sich", "et\u00b7wa", "fl\u00fcch\u00b7tig", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ietzt decken wir sie zu; ietzt lassen wir sie schissen,", "tokens": ["Ietzt", "de\u00b7cken", "wir", "sie", "zu", ";", "ietzt", "las\u00b7sen", "wir", "sie", "schis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nach dem wir diesen schnell und jenen langsam wissen;", "tokens": ["Nach", "dem", "wir", "die\u00b7sen", "schnell", "und", "je\u00b7nen", "lang\u00b7sam", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDAT", "ADJD", "KON", "PDS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hier brauchen wir den Sporn; dort brauchen wir den Zaum;", "tokens": ["Hier", "brau\u00b7chen", "wir", "den", "Sporn", ";", "dort", "brau\u00b7chen", "wir", "den", "Zaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wir halten jenen an und geben diesem Raum.", "tokens": ["Wir", "hal\u00b7ten", "je\u00b7nen", "an", "und", "ge\u00b7ben", "die\u00b7sem", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PTKVZ", "KON", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So jener sich was scheut, wil uns und ihm nicht trauen,", "tokens": ["So", "je\u00b7ner", "sich", "was", "scheut", ",", "wil", "uns", "und", "ihm", "nicht", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "PRF", "PWS", "VVFIN", "$,", "VMFIN", "PPER", "KON", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So \u00f6ffnen wir das Licht durch freundlich gegen-schauen,", "tokens": ["So", "\u00f6ff\u00b7nen", "wir", "das", "Licht", "durch", "freund\u00b7lich", "ge\u00b7gen\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erleuchten seinen Sinn, entz\u00fcnden ihm ein Heiss,", "tokens": ["Er\u00b7leuch\u00b7ten", "sei\u00b7nen", "Sinn", ",", "ent\u00b7z\u00fcn\u00b7den", "ihm", "ein", "Heiss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dadurch in ihm zerschmelzt der Zagheit kaltes Ei\u00df;", "tokens": ["Da\u00b7durch", "in", "ihm", "zer\u00b7schmelzt", "der", "Zag\u00b7heit", "kal\u00b7tes", "Ei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer eifrig seiner Brunst halb w\u00fctig nach wil hencken,", "tokens": ["Wer", "eif\u00b7rig", "sei\u00b7ner", "Brunst", "halb", "w\u00fc\u00b7tig", "nach", "wil", "hen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "ADJD", "ADJD", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mu\u00df blitzlich seinen Mut auf Ehrerbittung lencken,", "tokens": ["Mu\u00df", "blitz\u00b7lich", "sei\u00b7nen", "Mut", "auf", "Ehr\u00b7er\u00b7bit\u00b7tung", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wann unsrer Augen Glantz mit Wolcken \u00fcberzeucht,", "tokens": ["Wann", "uns\u00b7rer", "Au\u00b7gen", "Glantz", "mit", "Wol\u00b7cken", "\u00fc\u00b7ber\u00b7zeucht", ","], "token_info": ["word", 