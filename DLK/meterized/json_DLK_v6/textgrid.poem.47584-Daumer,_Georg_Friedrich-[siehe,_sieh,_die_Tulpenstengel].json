{"textgrid.poem.47584": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[siehe, sieh, die Tulpenstengel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, sieh, die Tulpenstengel,", "tokens": ["Sie\u00b7he", ",", "sieh", ",", "die", "Tul\u00b7pen\u00b7sten\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese frechen Ketzerlein,", "tokens": ["Die\u00b7se", "fre\u00b7chen", "Ket\u00b7zer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heben ihre bunten Becher", "tokens": ["He\u00b7ben", "ih\u00b7re", "bun\u00b7ten", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und begehren Trunk und Wein.", "tokens": ["Und", "be\u00b7geh\u00b7ren", "Trunk", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, der Ost, der freche Buhle,", "tokens": ["Sieh", ",", "der", "Ost", ",", "der", "fre\u00b7che", "Buh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Rose, wo er weht,", "tokens": ["Je\u00b7der", "Ro\u00b7se", ",", "wo", "er", "weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmeichelt er den jungen Busen", "tokens": ["Schmei\u00b7chelt", "er", "den", "jun\u00b7gen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem engen Miederlein.", "tokens": ["Aus", "dem", "en\u00b7gen", "Mie\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Dieses lockere Gesindel", "tokens": ["Die\u00b7ses", "lo\u00b7cke\u00b7re", "Ge\u00b7sin\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00f6rdert meine Tugend nicht;", "tokens": ["F\u00f6r\u00b7dert", "mei\u00b7ne", "Tu\u00b7gend", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ersch\u00fcttert meine guten,", "tokens": ["Es", "er\u00b7sch\u00fct\u00b7tert", "mei\u00b7ne", "gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frommen Sitten ungemein. \u2013", "tokens": ["From\u00b7men", "Sit\u00b7ten", "un\u00b7ge\u00b7mein", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur des Edlen, nur des Reinen,", "tokens": ["Nur", "des", "Ed\u00b7len", ",", "nur", "des", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe Freunde, seid bestrebt;", "tokens": ["Lie\u00b7be", "Freun\u00b7de", ",", "seid", "be\u00b7strebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkt allein von edlen Weinen,", "tokens": ["Trinkt", "al\u00b7lein", "von", "ed\u00b7len", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinket eure Weine rein! \u2013", "tokens": ["Trin\u00b7ket", "eu\u00b7re", "Wei\u00b7ne", "rein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ausgel\u00f6s't sind alle Kutten,", "tokens": ["Aus\u00b7ge\u00b7l\u00f6s't", "sind", "al\u00b7le", "Kut\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche man um Wein versetzt;", "tokens": ["Wel\u00b7che", "man", "um", "Wein", "ver\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine nur, es ist die meine,", "tokens": ["Ei\u00b7ne", "nur", ",", "es", "ist", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PPER", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber Wirth, sie bleibe dein! \u2013", "tokens": ["Lie\u00b7ber", "Wirth", ",", "sie", "blei\u00b7be", "dein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stirbt Hafis, o nicht begrabt ihn", "tokens": ["Stirbt", "Ha\u00b7fis", ",", "o", "nicht", "be\u00b7grabt", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "FM", "PTKNEG", "VVFIN", "PPER"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In die dumpfe Gruft hinein;", "tokens": ["In", "die", "dump\u00b7fe", "Gruft", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, begrabt ihn in den Keller,", "tokens": ["Nein", ",", "be\u00b7grabt", "ihn", "in", "den", "Kel\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werft ihn in ein Fa\u00df voll Wein!", "tokens": ["Werft", "ihn", "in", "ein", "Fa\u00df", "voll", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}