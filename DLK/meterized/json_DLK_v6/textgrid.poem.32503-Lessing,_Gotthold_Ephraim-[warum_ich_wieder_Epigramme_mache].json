{"textgrid.poem.32503": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "[warum ich wieder Epigramme mache]", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich mit Epigrammen wieder spiele", "tokens": ["Da\u00df", "ich", "mit", "E\u00b7pig\u00b7ram\u00b7men", "wie\u00b7der", "spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich, armer Willebald,", "tokens": ["Ich", ",", "ar\u00b7mer", "Wil\u00b7le\u00b7bald", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das macht, wie ich an mehrerm f\u00fchle,", "tokens": ["Das", "macht", ",", "wie", "ich", "an", "meh\u00b7rerm", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das macht, ich werde alt.", "tokens": ["Das", "macht", ",", "ich", "wer\u00b7de", "alt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}