{"textgrid.poem.43152": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freu' Dich nicht des blauen Himmels,", "tokens": ["Freu'", "Dich", "nicht", "des", "blau\u00b7en", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist Du noch so harmlos, Kind?", "tokens": ["Bist", "Du", "noch", "so", "harm\u00b7los", ",", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlst Du's nicht? durch Erd' und Himmel", "tokens": ["F\u00fchlst", "Du's", "nicht", "?", "durch", "Erd'", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "PTKNEG", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zieht gewitterschw\u00fcler Wind!", "tokens": ["Zieht", "ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7ler", "Wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trau' nur nicht der Himmelsl\u00fcge,", "tokens": ["Trau'", "nur", "nicht", "der", "Him\u00b7mels\u00b7l\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht dem Sonnenl\u00e4cheln trau',", "tokens": ["Nicht", "dem", "Son\u00b7nen\u00b7l\u00e4\u00b7cheln", "trau'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn es regnet, weinet innen \u2013", "tokens": ["Denn", "es", "reg\u00b7net", ",", "wei\u00b7net", "in\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur nach au\u00dfen lacht es blau!", "tokens": ["Nur", "nach", "au\u00b7\u00dfen", "lacht", "es", "blau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}