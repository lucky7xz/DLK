{"dta.poem.7123": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, f\u00fchlen wir uns fr\u00fch nicht munter und erquickt,", "tokens": ["Wie", ",", "f\u00fch\u00b7len", "wir", "uns", "fr\u00fch", "nicht", "mun\u00b7ter", "und", "er\u00b7quickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "PRF", "ADJD", "PTKNEG", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erfrischt, aufs neu belebt, froh, f\u00e4hig und geschickt,", "tokens": ["Er\u00b7frischt", ",", "aufs", "neu", "be\u00b7lebt", ",", "froh", ",", "f\u00e4\u00b7hig", "und", "ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJD", "VVPP", "$,", "ADJD", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, was aus M\u00fcdigkeit wir m\u00fcssen unterlassen,", "tokens": ["Das", ",", "was", "aus", "M\u00fc\u00b7dig\u00b7keit", "wir", "m\u00fcs\u00b7sen", "un\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von neuen wieder anzufassen!", "tokens": ["Von", "neu\u00b7en", "wie\u00b7der", "an\u00b7zu\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es h\u00f6rt, o Wunderwerck! des C\u00f6rpers Handlung auf,", "tokens": ["Es", "h\u00f6rt", ",", "o", "Wun\u00b7der\u00b7werck", "!", "des", "C\u00f6r\u00b7pers", "Hand\u00b7lung", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit der Geister Krafft, die unsre W\u00e4rme n\u00e4hret,", "tokens": ["Da\u00b7mit", "der", "Geis\u00b7ter", "Krafft", ",", "die", "uns\u00b7re", "W\u00e4r\u00b7me", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wovon der Sinnen-Schaar den gr\u00f6sten Theil verzehret,", "tokens": ["Wo\u00b7von", "der", "Sin\u00b7nen\u00b7Schaar", "den", "gr\u00f6s\u00b7ten", "Theil", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich mehr vereinen k\u00f6nn\u2019 und sich verbinden,", "tokens": ["Sich", "mehr", "ver\u00b7ei\u00b7nen", "k\u00f6nn'", "und", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um unser Lebens-Feur von neuen anzuz\u00fcnden:", "tokens": ["Um", "un\u00b7ser", "Le\u00b7bens\u00b7Feur", "von", "neu\u00b7en", "an\u00b7zu\u00b7z\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Daher, wenn gleich bey uns das F\u00fchlen, H\u00f6ren, Sehn,", "tokens": ["Da\u00b7her", ",", "wenn", "gleich", "bey", "uns", "das", "F\u00fch\u00b7len", ",", "H\u00f6\u00b7ren", ",", "Sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ADV", "APPR", "PPER", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bewegen, der Geruch, in Sinnen, stille stehn;", "tokens": ["Be\u00b7we\u00b7gen", ",", "der", "Ge\u00b7ruch", ",", "in", "Sin\u00b7nen", ",", "stil\u00b7le", "stehn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "APPR", "NN", "$,", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So stehen doch in stetigem Gesch\u00e4ffte,", "tokens": ["So", "ste\u00b7hen", "doch", "in", "ste\u00b7ti\u00b7gem", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Blut, Magen, Lung und Hertz, der Eingeweide Kr\u00e4ffte,", "tokens": ["Blut", ",", "Ma\u00b7gen", ",", "Lung", "und", "Hertz", ",", "der", "Ein\u00b7ge\u00b7wei\u00b7de", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nie m\u00fc\u00dfig, nimmer still; um wieder zu ersetzen", "tokens": ["Nie", "m\u00fc\u00b7\u00dfig", ",", "nim\u00b7mer", "still", ";", "um", "wie\u00b7der", "zu", "er\u00b7set\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "PTKVZ", "$.", "APPR", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was abgegangen war. Ach welche S\u00fc\u00dfigkeit!", "tokens": ["Was", "ab\u00b7ge\u00b7gan\u00b7gen", "war", ".", "Ach", "wel\u00b7che", "S\u00fc\u00b7\u00dfig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$.", "ITJ", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Welch \u00e4nsserlich und innerlichs Ergetzen", "tokens": ["Welch", "\u00e4ns\u00b7ser\u00b7lich", "und", "in\u00b7ner\u00b7lichs", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Empfindet man bey stiller Abend-Zeit,", "tokens": ["Emp\u00b7fin\u00b7det", "man", "bey", "stil\u00b7ler", "A\u00b7ben\u00b7dZeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wenn wir den m\u00fcden Leib auf weichen Feder-Decken,", "tokens": ["Wenn", "wir", "den", "m\u00fc\u00b7den", "Leib", "auf", "wei\u00b7chen", "Fe\u00b7der\u00b7De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit einigen Erwegen strecken!", "tokens": ["Mit", "ei\u00b7ni\u00b7gen", "Er\u00b7we\u00b7gen", "stre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wir f\u00fchlen wie die m\u00fcden Sehnen,", "tokens": ["Wir", "f\u00fch\u00b7len", "wie", "die", "m\u00fc\u00b7den", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "In s\u00fcsser Lust, sich aus einander dehnen.", "tokens": ["In", "s\u00fcs\u00b7ser", "Lust", ",", "sich", "aus", "ein\u00b7an\u00b7der", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wodurch, wenn wir recht \u00fcberd\u00e4chten,", "tokens": ["Wo\u00b7durch", ",", "wenn", "wir", "recht", "\u00fc\u00b7berd\u00b7\u00e4ch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wie GOTT den Leib so wunderbar formirt,", "tokens": ["Wie", "GoTT", "den", "Leib", "so", "wun\u00b7der\u00b7bar", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und da\u00df nur Jhm davor Lob, Prei\u00df und Ruhm geb\u00fchrt;", "tokens": ["Und", "da\u00df", "nur", "Jhm", "da\u00b7vor", "Lob", ",", "Prei\u00df", "und", "Ruhm", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "PAV", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir Jhm in unsrer Lust das beste Danck-Lied br\u00e4chten.", "tokens": ["Wir", "Jhm", "in", "uns\u00b7rer", "Lust", "das", "bes\u00b7te", "Dan\u00b7ck\u00b7Lied", "br\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}