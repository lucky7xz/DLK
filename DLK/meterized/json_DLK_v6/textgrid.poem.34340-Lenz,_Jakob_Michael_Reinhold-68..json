{"textgrid.poem.34340": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gehts denn aus dem Weltgen 'raus,", "tokens": ["So", "gehts", "denn", "aus", "dem", "Welt\u00b7gen", "'raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Wollust, zu vergehen!", "tokens": ["O", "Wol\u00b7lust", ",", "zu", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sterbe sonder Furcht und Graus,", "tokens": ["Ich", "ster\u00b7be", "son\u00b7der", "Furcht", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe sie gesehen.", "tokens": ["Ich", "ha\u00b7be", "sie", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Brust und Gedanke voll von ihr:", "tokens": ["Brust", "und", "Ge\u00b7dan\u00b7ke", "voll", "von", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "APPR", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So komm, o Tod! ich geige dir;", "tokens": ["So", "komm", ",", "o", "Tod", "!", "ich", "gei\u00b7ge", "dir", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So komm, o Tod! und tanze mir.", "tokens": ["So", "komm", ",", "o", "Tod", "!", "und", "tan\u00b7ze", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}