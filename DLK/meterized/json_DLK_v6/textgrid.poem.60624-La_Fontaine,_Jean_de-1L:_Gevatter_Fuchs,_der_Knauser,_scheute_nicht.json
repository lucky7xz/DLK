{"textgrid.poem.60624": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gevatter Fuchs, der Knauser, scheute nicht", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gevatter Fuchs, der Knauser, scheute nicht", "tokens": ["Ge\u00b7vat\u00b7ter", "Fuchs", ",", "der", "Knau\u00b7ser", ",", "scheu\u00b7te", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kosten, Nachbar Storch ein Gastmahl zu spendieren.", "tokens": ["Die", "Kos\u00b7ten", ",", "Nach\u00b7bar", "Storch", "ein", "Gast\u00b7mahl", "zu", "spen\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Mahl war karg: als einziges Gericht", "tokens": ["Das", "Mahl", "war", "karg", ":", "als", "ein\u00b7zi\u00b7ges", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KOUS", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lie\u00df klare Br\u00fche unser Schelm servieren \u2013", "tokens": ["Lie\u00df", "kla\u00b7re", "Br\u00fc\u00b7he", "un\u00b7ser", "Schelm", "ser\u00b7vie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und diese gar in einem flachen Teller.", "tokens": ["Und", "die\u00b7se", "gar", "in", "ei\u00b7nem", "fla\u00b7chen", "Tel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Storch mit seinem langen Schnabel sticht", "tokens": ["Der", "Storch", "mit", "sei\u00b7nem", "lan\u00b7gen", "Schna\u00b7bel", "sticht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umsonst hinein, doch schleckte um so schneller", "tokens": ["Um\u00b7sonst", "hin\u00b7ein", ",", "doch", "schleck\u00b7te", "um", "so", "schnel\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Fuchs mit breitem Maul das Ganze auf.", "tokens": ["Der", "Fuchs", "mit", "brei\u00b7tem", "Maul", "das", "Gan\u00b7ze", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Um sich zu revanchieren,", "tokens": ["Um", "sich", "zu", "re\u00b7van\u00b7chie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bat kurze Zeit darauf", "tokens": ["Bat", "kur\u00b7ze", "Zeit", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Storch den Fuchs, bei ihm nun zu soupieren.", "tokens": ["Der", "Storch", "den", "Fuchs", ",", "bei", "ihm", "nun", "zu", "sou\u00b7pie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "$,", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der sprach: \u00bbIch komme gern,", "tokens": ["Der", "sprach", ":", "\u00bb", "Ich", "kom\u00b7me", "gern", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Zu speisen bei so liebem Herrn.\u00ab", "tokens": ["Zu", "spei\u00b7sen", "bei", "so", "lie\u00b7bem", "Herrn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er eilte zur gegebnen Zeit", "tokens": ["Er", "eil\u00b7te", "zur", "ge\u00b7geb\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zur Storchenwohnung, pries die Liebensw\u00fcrdigkeit", "tokens": ["Zur", "Stor\u00b7chen\u00b7woh\u00b7nung", ",", "pries", "die", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Des Freundes, labte sich entz\u00fcckt", "tokens": ["Des", "Freun\u00b7des", ",", "lab\u00b7te", "sich", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Am Duft des Fleisches, das zerst\u00fcckt", "tokens": ["Am", "Duft", "des", "Flei\u00b7sches", ",", "das", "zer\u00b7st\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und fein gekocht \u2013 so ganz, wie er's am liebsten mag,", "tokens": ["Und", "fein", "ge\u00b7kocht", "\u2013", "so", "ganz", ",", "wie", "er's", "am", "liebs\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$(", "ADV", "ADV", "$,", "PWAV", "PIS", "PTKA", "ADJD", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zun\u00e4chst noch abseits lag.", "tokens": ["Zu\u00b7n\u00e4chst", "noch", "ab\u00b7seits", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Er war mit gutem Appetit begl\u00fcckt,", "tokens": ["Er", "war", "mit", "gu\u00b7tem", "Ap\u00b7pe\u00b7tit", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der F\u00fcchsen selten fehlen soll.", "tokens": ["Der", "F\u00fcch\u00b7sen", "sel\u00b7ten", "feh\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch ach, wie war das jammervoll:", "tokens": ["Doch", "ach", ",", "wie", "war", "das", "jam\u00b7mer\u00b7voll", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWAV", "VAFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Man trug das Mahl in einer engen Flasche auf!", "tokens": ["Man", "trug", "das", "Mahl", "in", "ei\u00b7ner", "en\u00b7gen", "Fla\u00b7sche", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die M\u00fcndung war nicht weiter als ein B\u00fcchsenlauf.", "tokens": ["Die", "M\u00fcn\u00b7dung", "war", "nicht", "wei\u00b7ter", "als", "ein", "B\u00fcch\u00b7sen\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Storchenschnabel tauchte leicht hinein ins Glas,", "tokens": ["Der", "Stor\u00b7chen\u00b7schna\u00b7bel", "tauch\u00b7te", "leicht", "hin\u00b7ein", "ins", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Des Gastes Schnauze aber brauchte andres Ma\u00df.", "tokens": ["Des", "Gas\u00b7tes", "Schnau\u00b7ze", "a\u00b7ber", "brauch\u00b7te", "and\u00b7res", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mit leerem Magen zog der Herr nach Haus,", "tokens": ["Mit", "lee\u00b7rem", "Ma\u00b7gen", "zog", "der", "Herr", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Mit eingekniffnem Schwanz und schlappen Ohren;", "tokens": ["Mit", "ein\u00b7ge\u00b7kniff\u00b7nem", "Schwanz", "und", "schlap\u00b7pen", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dem keck ein Huhn den Pelz geschoren.", "tokens": ["Dem", "keck", "ein", "Huhn", "den", "Pelz", "ge\u00b7scho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Merkt's euch, Betr\u00fcger all auf Erden:", "tokens": ["Merkt's", "euch", ",", "Be\u00b7tr\u00fc\u00b7ger", "all", "auf", "Er\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "PIAT", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auch ihr sollt so betrogen werden!", "tokens": ["Auch", "ihr", "sollt", "so", "be\u00b7tro\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}}}}