{"textgrid.poem.66995": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Stille halten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was will das Herz mir spalten?", "tokens": ["Was", "will", "das", "Herz", "mir", "spal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mi\u00dfmut bohrt und s\u00e4gt.", "tokens": ["Der", "Mi\u00df\u00b7mut", "bohrt", "und", "s\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ein w\u00fcstes Schalten,", "tokens": ["Es", "ist", "ein", "w\u00fcs\u00b7tes", "Schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nicht nach Lust und Liebe fr\u00e4gt.", "tokens": ["Das", "nicht", "nach", "Lust", "und", "Lie\u00b7be", "fr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du mu\u00dft fein stille halten,", "tokens": ["Du", "mu\u00dft", "fein", "stil\u00b7le", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie roh es auf dich schl\u00e4gt,", "tokens": ["Wie", "roh", "es", "auf", "dich", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Deine recht gestalten", "tokens": ["Das", "Dei\u00b7ne", "recht", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tragen, was das liebe Herz ertr\u00e4gt.", "tokens": ["Und", "tra\u00b7gen", ",", "was", "das", "lie\u00b7be", "Herz", "er\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}