{"dta.poem.18260": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Donner und Doria!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Man setzt sich ans Schreibpult wie an ein Klavier;", "tokens": ["Man", "setzt", "sich", "ans", "Schreib\u00b7pult", "wie", "an", "ein", "Kla\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor sich drei Bogen gelbes Concept", "tokens": ["Vor", "sich", "drei", "Bo\u00b7gen", "gel\u00b7bes", "Con\u00b7cept"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "CARD", "NN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und kommt sich vor wie ein alter Adept.", "tokens": ["Und", "kommt", "sich", "vor", "wie", "ein", "al\u00b7ter", "A\u00b7dept", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann taucht man ins schwarze Gallelement", "tokens": ["Dann", "taucht", "man", "ins", "schwar\u00b7ze", "Gal\u00b7le\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Selbstber\u00e4ucherungsinstrument,", "tokens": ["Sein", "Selbst\u00b7be\u00b7r\u00e4u\u00b7che\u00b7rungs\u00b7inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tr\u00e4umt sich nach Memphis, Korinth und Walhall", "tokens": ["Tr\u00e4umt", "sich", "nach", "Mem\u00b7phis", ",", "Ko\u00b7rinth", "und", "Wal\u00b7hall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NE", "$,", "NE", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und gebiert einen m\u00e4chtigen Phrasenschwall.", "tokens": ["Und", "ge\u00b7biert", "ei\u00b7nen", "m\u00e4ch\u00b7ti\u00b7gen", "Phra\u00b7sen\u00b7schwall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Daneben spuckt man nach Recht und Pflicht", "tokens": ["Da\u00b7ne\u00b7ben", "spuckt", "man", "nach", "Recht", "und", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der neuen Zeit in ihr Prosagesicht;", "tokens": ["Der", "neu\u00b7en", "Zeit", "in", "ihr", "Pro\u00b7sa\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hat man sich dick mit Gef\u00fchlen beschwert,", "tokens": ["Und", "hat", "man", "sich", "dick", "mit", "Ge\u00b7f\u00fch\u00b7len", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PRF", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wird dr\u00fcber der Thr\u00e4nenk\u00fcbel geleert.", "tokens": ["Wird", "dr\u00fc\u00b7ber", "der", "Thr\u00e4\u00b7nen\u00b7k\u00fc\u00b7bel", "ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann druckt es der Drucker auf fein Velin,", "tokens": ["Dann", "druckt", "es", "der", "Dru\u00b7cker", "auf", "fein", "Ve\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJD", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Buchbinder bindet's in Maroquin", "tokens": ["Der", "Buch\u00b7bin\u00b7der", "bin\u00b7det's", "in", "Ma\u00b7ro\u00b7quin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schlie\u00dflich schimpft's die Kritik: \u201ePoesie\u201c \u2014", "tokens": ["Und", "schlie\u00df\u00b7lich", "schimpft's", "die", "Kri\u00b7tik", ":", "\u201e", "Poe\u00b7sie", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blasphemie!!!", "tokens": ["Blas\u00b7phe\u00b7mie", "!!!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}}}}