{"textgrid.poem.39229": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist ein guter Harfner traun,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein guter Harfner traun,", "tokens": ["Das", "ist", "ein", "gu\u00b7ter", "Harf\u00b7ner", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in des Todes Weh,", "tokens": ["Der", "in", "des", "To\u00b7des", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man die Finger ihm abgehaun,", "tokens": ["Wenn", "man", "die", "Fin\u00b7ger", "ihm", "ab\u00b7ge\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch harft mit seiner Zeh'. \u2013", "tokens": ["Noch", "harft", "mit", "sei\u00b7ner", "Zeh'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr kennt den Tod, den Sigurd litt,", "tokens": ["Ihr", "kennt", "den", "Tod", ",", "den", "Si\u00b7gurd", "litt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn schlug der Schw\u00e4ger List,", "tokens": ["Ihn", "schlug", "der", "Schw\u00e4\u00b7ger", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der den Drachen niederstritt,", "tokens": ["Und", "der", "den", "Dra\u00b7chen", "nie\u00b7der\u00b7stritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er fiel durch Frauenzwist.", "tokens": ["Er", "fiel", "durch", "Frau\u00b7en\u00b7zwist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als vor der T\u00fcr nun kalt und wund", "tokens": ["Als", "vor", "der", "T\u00fcr", "nun", "kalt", "und", "wund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag K\u00f6nig Sigurds Leib,", "tokens": ["Lag", "K\u00f6\u00b7nig", "Si\u00b7gurds", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da freite K\u00f6nig Atlis Mund", "tokens": ["Da", "frei\u00b7te", "K\u00f6\u00b7nig", "At\u00b7lis", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um K\u00f6nig Sigurds Weib.", "tokens": ["Um", "K\u00f6\u00b7nig", "Si\u00b7gurds", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und eh' sie fuhr gen Hunnenland,", "tokens": ["Und", "eh'", "sie", "fuhr", "gen", "Hun\u00b7nen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00f6n'gin Gudrunur,", "tokens": ["Die", "K\u00f6n'\u00b7gin", "Gud\u00b7ru\u00b7nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schwur sie in des Toten Hand", "tokens": ["Da", "schwur", "sie", "in", "des", "To\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen siebenfachen Schwur.", "tokens": ["Ei\u00b7nen", "sie\u00b7ben\u00b7fa\u00b7chen", "Schwur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie schwur bei Sigurds Todesstund'", "tokens": ["Sie", "schwur", "bei", "Si\u00b7gurds", "To\u00b7desstund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den M\u00f6rdern Schmach und Pein:", "tokens": ["Den", "M\u00f6r\u00b7dern", "Schmach", "und", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Bote, reite Du nach Burgund", "tokens": ["\u00bb", "mein", "Bo\u00b7te", ",", "rei\u00b7te", "Du", "nach", "Bur\u00b7gund"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lade die Br\u00fcder mein!\u00ab \u2013", "tokens": ["Und", "la\u00b7de", "die", "Br\u00fc\u00b7der", "mein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Zu den Hunnen \u00fcbers Donaufeld,", "tokens": ["Zu", "den", "Hun\u00b7nen", "\u00fc\u00b7bers", "Do\u00b7nau\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da ritten die Niflungar;", "tokens": ["Da", "rit\u00b7ten", "die", "Nif\u00b7lun\u00b7gar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "K\u00f6nig H\u00f6gni war der eine Held,", "tokens": ["K\u00f6\u00b7nig", "H\u00f6g\u00b7ni", "war", "der", "ei\u00b7ne", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der andre hie\u00df Gunnar.", "tokens": ["Der", "and\u00b7re", "hie\u00df", "Gun\u00b7nar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig H\u00f6gni war ein k\u00fchnes Blut,", "tokens": ["K\u00f6\u00b7nig", "H\u00f6g\u00b7ni", "war", "ein", "k\u00fch\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sein Stahl ward selten kalt.", "tokens": ["Sein", "Stahl", "ward", "sel\u00b7ten", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6nig Gunnar schlug die Harfe gut,", "tokens": ["K\u00f6\u00b7nig", "Gun\u00b7nar", "schlug", "die", "Har\u00b7fe", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nie war ein bess'rer Skald'. \u2013", "tokens": ["Nie", "war", "ein", "bess'\u00b7rer", "Skald'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr wi\u00dft, wie Atlis grimmig' Gemahl", "tokens": ["Ihr", "wi\u00dft", ",", "wie", "At\u00b7lis", "grim\u00b7mig'", "Ge\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die trotzigen Helden fing,", "tokens": ["Die", "trot\u00b7zi\u00b7gen", "Hel\u00b7den", "fing", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr kennt die Schlacht in Etzels Saal,", "tokens": ["Ihr", "kennt", "die", "Schlacht", "in", "Et\u00b7zels", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie sie zu Ende ging.", "tokens": ["Und", "wie", "sie", "zu", "En\u00b7de", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "K\u00f6nig H\u00f6gni vor der Schwester stand,", "tokens": ["K\u00f6\u00b7nig", "H\u00f6g\u00b7ni", "vor", "der", "Schwes\u00b7ter", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr Sinn war grimm und graus,", "tokens": ["Ihr", "Sinn", "war", "grimm", "und", "graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ri\u00df ihm mit der wei\u00dfen Hand", "tokens": ["Sie", "ri\u00df", "ihm", "mit", "der", "wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein rotes Herz heraus.", "tokens": ["Sein", "ro\u00b7tes", "Herz", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "K\u00f6nig Gunnar lie\u00df die Harfe nicht:", "tokens": ["K\u00f6\u00b7nig", "Gun\u00b7nar", "lie\u00df", "die", "Har\u00b7fe", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbdie fahre mit mir ins Grab!\u00ab", "tokens": ["\u00bb", "die", "fah\u00b7re", "mit", "mir", "ins", "Grab", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie hieb ihm an der Harfe dicht", "tokens": ["Sie", "hieb", "ihm", "an", "der", "Har\u00b7fe", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die beiden H\u00e4nde ab:", "tokens": ["Die", "bei\u00b7den", "H\u00e4n\u00b7de", "ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbnun fahre Du samt der Harfe hin", "tokens": ["\u00bb", "nun", "fah\u00b7re", "Du", "samt", "der", "Har\u00b7fe", "hin"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und spiele vor Schlang' und Wurm!\u00ab", "tokens": ["Und", "spie\u00b7le", "vor", "Schlang'", "und", "Wurm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihn werfen lie\u00df die K\u00f6nigin", "tokens": ["Ihn", "wer\u00b7fen", "lie\u00df", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den tiefen Schlangenturm.", "tokens": ["In", "den", "tie\u00b7fen", "Schlan\u00b7gen\u00b7turm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {