{"textgrid.poem.35187": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1849", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch! Klingt das nicht wie ferner Schwerterklang?", "tokens": ["Horch", "!", "Klingt", "das", "nicht", "wie", "fer\u00b7ner", "Schwer\u00b7ter\u00b7klang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Marsch bebt unter dampfenden Schwadronen.", "tokens": ["Die", "Marsch", "bebt", "un\u00b7ter", "damp\u00b7fen\u00b7den", "Schwad\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es jagt der Tod den weiten Plan entlang", "tokens": ["Es", "jagt", "der", "Tod", "den", "wei\u00b7ten", "Plan", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPO"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und erntet unter br\u00fcllenden Kanonen.", "tokens": ["Und", "ern\u00b7tet", "un\u00b7ter", "br\u00fcl\u00b7len\u00b7den", "Ka\u00b7no\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Norden ist ein deutsches Volk erwacht", "tokens": ["Im", "Nor\u00b7den", "ist", "ein", "deut\u00b7sches", "Volk", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft der Freiheit heil'ge Flamme steigen,", "tokens": ["Und", "l\u00e4\u00dft", "der", "Frei\u00b7heit", "heil'\u00b7ge", "Flam\u00b7me", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dem Bedr\u00fccker im Gew\u00fchl der Schlacht", "tokens": ["Um", "dem", "Be\u00b7dr\u00fc\u00b7cker", "im", "Ge\u00b7w\u00fchl", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den deutschen Muth, die deutsche Kraft zu zeigen.", "tokens": ["Den", "deut\u00b7schen", "Muth", ",", "die", "deut\u00b7sche", "Kraft", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun gilts ein Ringen um den h\u00f6chsten Preis,", "tokens": ["Nun", "gilts", "ein", "Rin\u00b7gen", "um", "den", "h\u00f6chs\u00b7ten", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein hei\u00dfes Wogen und ein hei\u00dfes Wagen;", "tokens": ["Ein", "hei\u00b7\u00dfes", "Wo\u00b7gen", "und", "ein", "hei\u00b7\u00dfes", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun schwitzt gar manch ein Herz purpurnen Schwei\u00df", "tokens": ["Nun", "schwitzt", "gar", "manch", "ein", "Herz", "pur\u00b7pur\u00b7nen", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schl\u00e4gt nur, um zum letzten Mal zu schlagen.", "tokens": ["Und", "schl\u00e4gt", "nur", ",", "um", "zum", "letz\u00b7ten", "Mal", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUI", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, mitten unter Leichen bl\u00fcht der Sieg;", "tokens": ["Doch", ",", "mit\u00b7ten", "un\u00b7ter", "Lei\u00b7chen", "bl\u00fcht", "der", "Sieg", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht darf der Lorbeer Thr\u00e4nen uns erpressen.", "tokens": ["Nicht", "darf", "der", "Lor\u00b7beer", "Thr\u00e4\u00b7nen", "uns", "er\u00b7pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer feindlich Bollwerk sterbend noch erstieg.", "tokens": ["Wer", "feind\u00b7lich", "Boll\u00b7werk", "ster\u00b7bend", "noch", "er\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der lebt, und w\u00e4r' er tausendmal vergessen.", "tokens": ["Der", "lebt", ",", "und", "w\u00e4r'", "er", "tau\u00b7send\u00b7mal", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn die Geschichte schreibt mit goldnem Stift", "tokens": ["Denn", "die", "Ge\u00b7schich\u00b7te", "schreibt", "mit", "gold\u00b7nem", "Stift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mi\u00dft Triumphe nicht nach kurzen Jahren.", "tokens": ["Und", "mi\u00dft", "Tri\u00b7um\u00b7phe", "nicht", "nach", "kur\u00b7zen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum gl\u00e4nzt es fort in heller Flammenschrift:", "tokens": ["Drum", "gl\u00e4nzt", "es", "fort", "in", "hel\u00b7ler", "Flam\u00b7men\u00b7schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbder L\u00f6we Sachsens ists mit seinen Schaaren!\u00ab", "tokens": ["\u00bb", "der", "L\u00f6\u00b7we", "Sach\u00b7sens", "ists", "mit", "sei\u00b7nen", "Schaa\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NE", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}