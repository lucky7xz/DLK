{"textgrid.poem.37654": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die dreifache Krone", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Feinde kamen dr\u00e4uend,", "tokens": ["Als", "die", "Fein\u00b7de", "ka\u00b7men", "dr\u00e4u\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Floh der Mensch von Haus und Hof,", "tokens": ["Floh", "der", "Mensch", "von", "Haus", "und", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6gel flohn vom Neste schreiend,", "tokens": ["V\u00f6\u00b7gel", "flohn", "vom", "Nes\u00b7te", "schrei\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der K\u00f6nig blieb am Hof,", "tokens": ["Nur", "der", "K\u00f6\u00b7nig", "blieb", "am", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fand die Eier in dem Neste,", "tokens": ["Fand", "die", "Ei\u00b7er", "in", "dem", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch erwarmet rund und sch\u00f6n,", "tokens": ["Noch", "er\u00b7war\u00b7met", "rund", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Legt sie unter seine Weste,", "tokens": ["Legt", "sie", "un\u00b7ter", "sei\u00b7ne", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dicht an's Herz, sie oft zu sehn.", "tokens": ["Dicht", "an's", "Herz", ",", "sie", "oft", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in diesen Schmerzenstagen,", "tokens": ["Und", "in", "die\u00b7sen", "Schmer\u00b7zens\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Welt in schneller Flucht,", "tokens": ["Wo", "die", "Welt", "in", "schnel\u00b7ler", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freut es ihn, sie sanft zu tragen,", "tokens": ["Freut", "es", "ihn", ",", "sie", "sanft", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Feind sie fernhin sucht.", "tokens": ["Und", "der", "Feind", "sie", "fern\u00b7hin", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sicher h\u00e4tt' er ihn gefangen,", "tokens": ["Si\u00b7cher", "h\u00e4tt'", "er", "ihn", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn er drang mit \u00dcbermuth,", "tokens": ["Denn", "er", "drang", "mit", "\u00dc\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blind vor\u00fcber sind gegangen,", "tokens": ["Blind", "vor\u00b7\u00fc\u00b7ber", "sind", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihn suchten, blind vor Wuth.", "tokens": ["Die", "ihn", "such\u00b7ten", ",", "blind", "vor", "Wuth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfer Trost, die hellen Augen", "tokens": ["S\u00fc\u00b7\u00dfer", "Trost", ",", "die", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehn schon aus der Schale rein,", "tokens": ["Sehn", "schon", "aus", "der", "Scha\u00b7le", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Schn\u00e4blein Thr\u00e4nen saugen,", "tokens": ["Und", "die", "Schn\u00e4b\u00b7lein", "Thr\u00e4\u00b7nen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Pfeifen ihn erfreun;", "tokens": ["Und", "mit", "Pfei\u00b7fen", "ihn", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie ja der Habicht schone,", "tokens": ["Da\u00df", "sie", "ja", "der", "Ha\u00b7bicht", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sperrt er nun die Kleinen ein,", "tokens": ["Sperrt", "er", "nun", "die", "Klei\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter der gew\u00f6lbten Krone,", "tokens": ["Un\u00b7ter", "der", "ge\u00b7w\u00f6lb\u00b7ten", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcttert sie im Schlo\u00df allein.", "tokens": ["F\u00fct\u00b7tert", "sie", "im", "Schlo\u00df", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Feind' und Freunde sich erschlagen,", "tokens": ["Feind'", "und", "Freun\u00b7de", "sich", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helfen kann er nicht dabei,", "tokens": ["Hel\u00b7fen", "kann", "er", "nicht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun wei\u00df er ohne Fragen,", "tokens": ["Und", "nun", "wei\u00df", "er", "oh\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer der letzte Sieger sei;", "tokens": ["Wer", "der", "letz\u00b7te", "Sie\u00b7ger", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Kleinen prophezeihen", "tokens": ["Denn", "die", "Klei\u00b7nen", "pro\u00b7phe\u00b7zei\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm ein friedlich sch\u00f6nes Reich,", "tokens": ["Ihm", "ein", "fried\u00b7lich", "sch\u00f6\u00b7nes", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wen noch etwas kann erfreuen,", "tokens": ["Wen", "noch", "et\u00b7was", "kann", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibt im gr\u00f6\u00dften Ungl\u00fcck reich.", "tokens": ["Bleibt", "im", "gr\u00f6\u00df\u00b7ten", "Un\u00b7gl\u00fcck", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als die Unterthanen kehren", "tokens": ["Als", "die", "Un\u00b7ter\u00b7tha\u00b7nen", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich in's zerst\u00f6rte Haus,", "tokens": ["End\u00b7lich", "in's", "zer\u00b7st\u00f6r\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar kein Kind h\u00e4lt sie in Ehren,", "tokens": ["Gar", "kein", "Kind", "h\u00e4lt", "sie", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Denn sie lie\u00dfen sie im Graus,", "tokens": ["Denn", "sie", "lie\u00b7\u00dfen", "sie", "im", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch den K\u00f6nig sanft umschwebet", "tokens": ["Doch", "den", "K\u00f6\u00b7nig", "sanft", "um\u00b7schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singend seiner Kinder Schaar,", "tokens": ["Sin\u00b7gend", "sei\u00b7ner", "Kin\u00b7der", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil im Ungl\u00fcck er belebet,", "tokens": ["Weil", "im", "Un\u00b7gl\u00fcck", "er", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was dem Tod geweihet war.", "tokens": ["Was", "dem", "Tod", "ge\u00b7wei\u00b7het", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}