{"dta.poem.8604": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Professor .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr den Mann mit st\u00e4ubender Per\u00fccke?", "tokens": ["Seht", "ihr", "den", "Mann", "mit", "st\u00e4u\u00b7ben\u00b7der", "Pe\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sprudelt ihm die hochgelahrte Kehle!", "tokens": ["Wie", "spru\u00b7delt", "ihm", "die", "hoch\u00b7ge\u00b7lahr\u00b7te", "Keh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seht, an der morschen Syllogismen-Kr\u00fccke", "tokens": ["Seht", ",", "an", "der", "mor\u00b7schen", "Syl\u00b7lo\u00b7gis\u00b7men\u00b7Kr\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinkt Gott in seine Welt; die Menschenseele", "tokens": ["Hinkt", "Gott", "in", "sei\u00b7ne", "Welt", ";", "die", "Men\u00b7schen\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist ewig, denn sie ist aus einem St\u00fccke;", "tokens": ["Ist", "e\u00b7wig", ",", "denn", "sie", "ist", "aus", "ei\u00b7nem", "St\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und da\u00df der Argumente keines fehle,", "tokens": ["Und", "da\u00df", "der", "Ar\u00b7gu\u00b7men\u00b7te", "kei\u00b7nes", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Hat er ein weises ", "tokens": ["Hat", "er", "ein", "wei\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Der Mensch ist frei, die Fesseln sind gebrochen!", "tokens": ["Der", "Mensch", "ist", "frei", ",", "die", "Fes\u00b7seln", "sind", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}