{"textgrid.poem.42550": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Ungenannten", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eines Berges Gipfel,", "tokens": ["Auf", "ei\u00b7nes", "Ber\u00b7ges", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da m\u00f6cht ich mit dir stehn,", "tokens": ["Da", "m\u00f6cht", "ich", "mit", "dir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf T\u00e4ler, Waldeswipfel", "tokens": ["Auf", "T\u00e4\u00b7ler", ",", "Wal\u00b7des\u00b7wip\u00b7fel"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit dir herniedersehn;", "tokens": ["Mit", "dir", "her\u00b7nie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da m\u00f6cht ich rings dir zeigen", "tokens": ["Da", "m\u00f6cht", "ich", "rings", "dir", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt im Fr\u00fchlingsschein", "tokens": ["Die", "Welt", "im", "Fr\u00fch\u00b7lings\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sprechen: w\u00e4r's mein eigen,", "tokens": ["Und", "spre\u00b7chen", ":", "w\u00e4r's", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So w\u00e4r es mein und dein.", "tokens": ["So", "w\u00e4r", "es", "mein", "und", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In meiner Seele Tiefen,", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O s\u00e4hst du da hinab,", "tokens": ["O", "s\u00e4hst", "du", "da", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo alle Lieder schliefen,", "tokens": ["Wo", "al\u00b7le", "Lie\u00b7der", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die je ein Gott mir gab!", "tokens": ["Die", "je", "ein", "Gott", "mir", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da w\u00fcrdest du erkennen:", "tokens": ["Da", "w\u00fcr\u00b7dest", "du", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn Echtes ich erstrebt,", "tokens": ["Wenn", "Ech\u00b7tes", "ich", "er\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mag's auch dich nicht nennen,", "tokens": ["Und", "mag's", "auch", "dich", "nicht", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch ist's von dir belebt.", "tokens": ["Doch", "ist's", "von", "dir", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}