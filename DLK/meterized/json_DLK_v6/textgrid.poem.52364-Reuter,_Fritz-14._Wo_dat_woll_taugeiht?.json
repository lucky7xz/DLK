{"textgrid.poem.52364": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "14. Wo dat woll taugeiht?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwat steihst du dor un kikst du dor", "tokens": ["\u00bb", "wat", "steihst", "du", "dor", "un", "kikst", "du", "dor"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "FM", "VVFIN", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un kleist di mit de Hand in't Hor?\u00ab", "tokens": ["Un", "kleist", "di", "mit", "de", "Hand", "in't", "Hor", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fr\u00f6ggt Jochen Burr Jehann Perd\u00f6hlen.", "tokens": ["Fr\u00f6ggt", "Jo\u00b7chen", "Burr", "Je\u00b7hann", "Per\u00b7d\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbje, Brauder!\u00ab seggt Jehann, \u00bbmi fehlen", "tokens": ["\u00bb", "je", ",", "Brau\u00b7der", "!", "\u00ab", "seggt", "Je\u00b7hann", ",", "\u00bb", "mi", "feh\u00b7len"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "$,", "NN", "$.", "$(", "VVFIN", "NE", "$,", "$(", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An den verfluchten Torf an f\u00f6ftig Saden,", "tokens": ["An", "den", "ver\u00b7fluch\u00b7ten", "Torf", "an", "f\u00f6f\u00b7tig", 