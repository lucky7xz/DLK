{"dta.poem.8805": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcrdig kaum, zu dir emporzustreben?", "tokens": ["W\u00fcr\u00b7dig", "kaum", ",", "zu", "dir", "em\u00b7por\u00b7zu\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Blicke jeder ist ein Funken,", "tokens": ["Dei\u00b7ner", "Bli\u00b7cke", "je\u00b7der", "ist", "ein", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der verdunkelt jeden Stern daneben;", "tokens": ["Der", "ver\u00b7dun\u00b7kelt", "je\u00b7den", "Stern", "da\u00b7ne\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PAV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Angefesselt h\u00e4lt mich jede Locke,", "tokens": ["An\u00b7ge\u00b7fes\u00b7selt", "h\u00e4lt", "mich", "je\u00b7de", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und so schleppst du mich dir nach im Leben;", "tokens": ["Und", "so", "schleppst", "du", "mich", "dir", "nach", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bl\u00fchen m\u00f6cht' ich dir um's Haupt, wie Rosen,", "tokens": ["Bl\u00fc\u00b7hen", "m\u00f6cht'", "ich", "dir", "um's", "Haupt", ",", "wie", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADJA", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schlingen mich um deine Knie, wie Reben;", "tokens": ["Schlin\u00b7gen", "mich", "um", "dei\u00b7ne", "Knie", ",", "wie", "Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Selig seyd ihr, liebende Planeten,", "tokens": ["Se\u00b7lig", "seyd", "ihr", ",", "lie\u00b7ben\u00b7de", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Ewig d\u00fcrft ihr um die Sonne schweben!", "tokens": ["E\u00b7wig", "d\u00fcrft", "ihr", "um", "die", "Son\u00b7ne", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Liebe wirft mir in der Seele Wogen,", "tokens": ["Lie\u00b7be", "wirft", "mir", "in", "der", "See\u00b7le", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}}}}}