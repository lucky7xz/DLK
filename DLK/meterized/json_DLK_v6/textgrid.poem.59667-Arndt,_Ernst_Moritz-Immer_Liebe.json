{"textgrid.poem.59667": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Immer Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und klingst du immer Liebe wieder?", "tokens": ["Und", "klingst", "du", "im\u00b7mer", "Lie\u00b7be", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer nur denselben Ton?", "tokens": ["Und", "im\u00b7mer", "nur", "den\u00b7sel\u00b7ben", "Ton", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00dft du keine andern Lieder", "tokens": ["Und", "wei\u00dft", "du", "kei\u00b7ne", "an\u00b7dern", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Gottes Sohn, von Gottes Sohn?", "tokens": ["Als", "Got\u00b7tes", "Sohn", ",", "von", "Got\u00b7tes", "Sohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df er dein Licht, dein Glanz, dein Schein,", "tokens": ["Mu\u00df", "er", "dein", "Licht", ",", "dein", "Glanz", ",", "dein", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df er dein Alles, Alles sein?", "tokens": ["Mu\u00df", "er", "dein", "Al\u00b7les", ",", "Al\u00b7les", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "PIS", "$,", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, er allein: in diesem Namen,", "tokens": ["Ja", ",", "er", "al\u00b7lein", ":", "in", "die\u00b7sem", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "ADV", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem allersch\u00f6nsten Ton,", "tokens": ["In", "die\u00b7sem", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klingt aller Himmel Himmel ", "tokens": ["Klingt", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Cherubim und Seraphim", "tokens": ["Und", "Che\u00b7ru\u00b7bim", "und", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Anbetend knien sie hin vor ihm.", "tokens": ["An\u00b7be\u00b7tend", "kni\u00b7en", "sie", "hin", "vor", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ja, er allein: soweit die Winde", "tokens": ["Ja", ",", "er", "al\u00b7lein", ":", "so\u00b7weit", "die", "Win\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADV", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das gr\u00fcne Erdenrund umwehn,", "tokens": ["Das", "gr\u00fc\u00b7ne", "Er\u00b7den\u00b7rund", "um\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df nun im Klang vom hohen Kinde,", "tokens": ["Mu\u00df", "nun", "im", "Klang", "vom", "ho\u00b7hen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Mensch ward, aller Jubel gehn:", "tokens": ["Das", "Mensch", "ward", ",", "al\u00b7ler", "Ju\u00b7bel", "gehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es klinget kein so s\u00fc\u00dfer Ton", "tokens": ["Es", "klin\u00b7get", "kein", "so", "s\u00fc\u00b7\u00dfer", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als von dem Sohn und aus dem Sohn.", "tokens": ["Als", "von", "dem", "Sohn", "und", "aus", "dem", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein, nimmer lernt es andre Lieder", "tokens": ["Nein", ",", "nim\u00b7mer", "lernt", "es", "and\u00b7re", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das arme, s\u00fcndenkranke Herz,", "tokens": ["Das", "ar\u00b7me", ",", "s\u00fcn\u00b7den\u00b7kran\u00b7ke", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, nimmer klingt es andres wieder", "tokens": ["Nein", ",", "nim\u00b7mer", "klingt", "es", "and\u00b7res", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als jener Sehnsucht s\u00fc\u00dfen Schmerz", "tokens": ["Als", "je\u00b7ner", "Sehn\u00b7sucht", "s\u00fc\u00b7\u00dfen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Menschensohn, vom Gottessohn,", "tokens": ["Vom", "Men\u00b7schen\u00b7sohn", ",", "vom", "Got\u00b7tes\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies bleibt das Lied, der Klang, der Ton.", "tokens": ["Dies", "bleibt", "das", "Lied", ",", "der", "Klang", ",", "der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bleibst das Lied, du liebste Liebe,", "tokens": ["Du", "bleibst", "das", "Lied", ",", "du", "liebs\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bleibst die Sehnsucht, sch\u00f6nstes Bild,", "tokens": ["Du", "bleibst", "die", "Sehn\u00b7sucht", ",", "sch\u00f6ns\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Licht der Lichter, Trieb der Triebe,", "tokens": ["Du", "Licht", "der", "Lich\u00b7ter", ",", "Trieb", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Woraus der Himmel Wonne quillt:", "tokens": ["Wo\u00b7raus", "der", "Him\u00b7mel", "Won\u00b7ne", "quillt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz klingt deine Herrlichkeit", "tokens": ["Mein", "Herz", "klingt", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von nun an bis in Ewigkeit.", "tokens": ["Von", "nun", "an", "bis", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}