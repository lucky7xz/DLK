{"textgrid.poem.24888": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Jaffa", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun lebe wohl, mein morgenl\u00e4ndisch Dach,", "tokens": ["Nun", "le\u00b7be", "wohl", ",", "mein", "mor\u00b7gen\u00b7l\u00e4n\u00b7disch", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Palmen still umfriedet und Cypressen!", "tokens": ["Von", "Pal\u00b7men", "still", "um\u00b7frie\u00b7det", "und", "Cyp\u00b7res\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf dir wie manche N\u00e4chte hab' ich wach,", "tokens": ["Auf", "dir", "wie", "man\u00b7che", "N\u00e4ch\u00b7te", "hab'", "ich", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Sternenhimmel \u00fcberw\u00f6lbt, gesessen!", "tokens": ["Vom", "Ster\u00b7nen\u00b7him\u00b7mel", "\u00fc\u00b7berw\u00b7\u00f6lbt", ",", "ge\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Atemzug der schlummernden Natur", "tokens": ["Der", "A\u00b7tem\u00b7zug", "der", "schlum\u00b7mern\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging durch die Wipfel hin mit sanftem Wehen;", "tokens": ["Ging", "durch", "die", "Wip\u00b7fel", "hin", "mit", "sanf\u00b7tem", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leis durch das tiefe Schweigen rauschten nur", "tokens": ["Leis", "durch", "das", "tie\u00b7fe", "Schwei\u00b7gen", "rauschten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fernher die heil'gen Brunnen der Moscheen.", "tokens": ["Fern\u00b7her", "die", "heil'\u00b7gen", "Brun\u00b7nen", "der", "Mo\u00b7scheen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu H\u00e4upten mir im unermess'nen Raum", "tokens": ["Zu", "H\u00e4up\u00b7ten", "mir", "im", "un\u00b7er\u00b7mess'\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich Myriaden goldner Welten rollen,", "tokens": ["Sah", "ich", "My\u00b7ria\u00b7den", "gold\u00b7ner", "Wel\u00b7ten", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "So flammenhell, als ob seit gestern kaum", "tokens": ["So", "flam\u00b7me\u00b7nhell", ",", "als", "ob", "seit", "ge\u00b7stern", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "ADV", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie aus dem gro\u00dfen Born des Lichts gequollen.", "tokens": ["Sie", "aus", "dem", "gro\u00b7\u00dfen", "Born", "des", "Lichts", "ge\u00b7quol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie, noch unbeth\u00f6rt von Glaubenswahn,", "tokens": ["Und", "wie", ",", "noch", "un\u00b7be\u00b7th\u00f6rt", "von", "Glau\u00b7bens\u00b7wahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ersten Menschen, die nicht Tempel kannten,", "tokens": ["Die", "ers\u00b7ten", "Men\u00b7schen", ",", "die", "nicht", "Tem\u00b7pel", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Andacht auf zu jenen Sternen sahn,", "tokens": ["Mit", "An\u00b7dacht", "auf", "zu", "je\u00b7nen", "Ster\u00b7nen", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die unverg\u00e4nglich dort am Himmel brannten,", "tokens": ["Die", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "dort", "am", "Him\u00b7mel", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Also auch ich; mein Geist schwang sich empor", "tokens": ["Al\u00b7so", "auch", "ich", ";", "mein", "Geist", "schwang", "sich", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sog den Glanz in langen, durst'gen Z\u00fcgen", "tokens": ["Und", "sog", "den", "Glanz", "in", "lan\u00b7gen", ",", "dur\u00b7st'\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und kreiste mit dem hehren Feierchor", "tokens": ["Und", "kreis\u00b7te", "mit", "dem", "heh\u00b7ren", "Fei\u00b7er\u00b7chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonnen, wie sie sanken oder stiegen.", "tokens": ["Der", "Son\u00b7nen", ",", "wie", "sie", "san\u00b7ken", "o\u00b7der", "stie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O wer aus jenem Quell des Lichtes trank,", "tokens": ["O", "wer", "aus", "je\u00b7nem", "Quell", "des", "Lich\u00b7tes", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht dunkel ist's um ihn fortan hienieden;", "tokens": ["Nicht", "dun\u00b7kel", "ist's", "um", "ihn", "for\u00b7tan", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leb wohl, mein Morgenland, und habe Dank!", "tokens": ["Leb", "wohl", ",", "mein", "Mor\u00b7gen\u00b7land", ",", "und", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit mir im Herzen trag' ich deinen Frieden.", "tokens": ["Mit", "mir", "im", "Her\u00b7zen", "trag'", "ich", "dei\u00b7nen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}