{"textgrid.poem.44692": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Noch eins an den Verfasser der falschen Wanderjahre", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Handwerksbursche, wandre!", "tokens": ["Hand\u00b7werks\u00b7bur\u00b7sche", ",", "wand\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fechten gehn auch andre,", "tokens": ["Fech\u00b7ten", "gehn", "auch", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Warum nicht auch du?", "tokens": ["Wa\u00b7rum", "nicht", "auch", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch Gesellenschw\u00e4chen", "tokens": ["Doch", "Ge\u00b7sel\u00b7len\u00b7schw\u00e4\u00b7chen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sollst nur, h\u00e4nselnd, r\u00e4chen;", "tokens": ["Sollst", "nur", ",", "h\u00e4n\u00b7selnd", ",", "r\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "VVPP", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mehr kommt dir nicht zu:", "tokens": ["Mehr", "kommt", "dir", "nicht", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Meister la\u00df in Ruh!", "tokens": ["Meis\u00b7ter", "la\u00df", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}