{"textgrid.poem.52699": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mithridates", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im goldgeschm\u00fcckten Marmelsaal,", "tokens": ["Im", "gold\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Mar\u00b7mel\u00b7saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seiner Pontusstadt,", "tokens": ["Zu", "sei\u00b7ner", "Pon\u00b7tus\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Rosen lag beim pr\u00e4chtigen Mahl", "tokens": ["Auf", "Ro\u00b7sen", "lag", "beim", "pr\u00e4ch\u00b7ti\u00b7gen", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der K\u00f6nig Mithridat.", "tokens": ["Der", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und rings um ihn der Freunde Kreis,", "tokens": ["Und", "rings", "um", "ihn", "der", "Freun\u00b7de", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feldherrn seines Heers;", "tokens": ["Die", "Feld\u00b7herrn", "sei\u00b7nes", "Heers", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tafelten \u2013 so strahlt um Zeus", "tokens": ["Sie", "ta\u00b7fel\u00b7ten", "\u2013", "so", "strahlt", "um", "Zeus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VVFIN", "APPR", "NE"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die G\u00f6tterschaft Homers.", "tokens": ["Die", "G\u00f6t\u00b7ter\u00b7schaft", "Ho\u00b7mers", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Harfe blitzt in sch\u00f6ner Hand,", "tokens": ["Die", "Har\u00b7fe", "blitzt", "in", "sch\u00f6\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesang und Tanz voll Reiz!", "tokens": ["Ge\u00b7sang", "und", "Tanz", "voll", "Reiz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Mann im purpurnen Gewand", "tokens": ["Den", "Mann", "im", "pur\u00b7pur\u00b7nen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht k\u00fcmmert und erfreuts.", "tokens": ["Nicht", "k\u00fcm\u00b7mert", "und", "er\u00b7freuts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es sprudelte des Weines Schaum", "tokens": ["Es", "spru\u00b7del\u00b7te", "des", "Wei\u00b7nes", "Schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kelch, krystallen schwer,", "tokens": ["Im", "Kelch", ",", "krys\u00b7tal\u00b7len", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schmauste nicht, er nippte kaum,", "tokens": ["Er", "schmaus\u00b7te", "nicht", ",", "er", "nipp\u00b7te", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schaute still aufs Meer.", "tokens": ["Und", "schau\u00b7te", "still", "aufs", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr R\u00f6meradler seid zur Hand!", "tokens": ["Ihr", "R\u00f6\u00b7me\u00b7rad\u00b7ler", "seid", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind eure Schwingen matt?", "tokens": ["Sind", "eu\u00b7re", "Schwin\u00b7gen", "matt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch z\u00fcrnt der Herr \u00fcber Meer und Land", "tokens": ["Euch", "z\u00fcrnt", "der", "Herr", "\u00fc\u00b7ber", "Meer", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der K\u00f6nig Mithridat.", "tokens": ["Der", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl unterm freien S\u00e4ulendach", "tokens": ["Wohl", "un\u00b7term", "frei\u00b7en", "S\u00e4u\u00b7len\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trinkt K\u00fchlung jeder Gast,", "tokens": ["Trinkt", "K\u00fch\u00b7lung", "je\u00b7der", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Abendl\u00fcfte werden wach,", "tokens": ["Die", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "wer\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wind ein Segel fa\u00dft.", "tokens": ["Der", "Wind", "ein", "Se\u00b7gel", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein kleiner Nachen treibt ans Land,", "tokens": ["Ein", "klei\u00b7ner", "Na\u00b7chen", "treibt", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das s\u00e4uselnde Geb\u00fcsch", "tokens": ["Das", "s\u00e4u\u00b7seln\u00b7de", "Ge\u00b7b\u00fcsch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Verh\u00fcllt den bl\u00fchnden Gartenstrand", "tokens": ["Ver\u00b7h\u00fcllt", "den", "bl\u00fchn\u00b7den", "Gar\u00b7ten\u00b7strand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fr\u00f6hlichen am Tisch.", "tokens": ["Den", "Fr\u00f6h\u00b7li\u00b7chen", "am", "Tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da klinget auf das ehrne Thor", "tokens": ["Da", "klin\u00b7get", "auf", "das", "ehr\u00b7ne", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am riesigen Portal,", "tokens": ["Am", "rie\u00b7si\u00b7gen", "Por\u00b7tal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es tritt ein Krieger rasch hervor", "tokens": ["Es", "tritt", "ein", "Krie\u00b7ger", "rasch", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit frech entbl\u00f6\u00dftem Stahl.", "tokens": ["Mit", "frech", "ent\u00b7bl\u00f6\u00df\u00b7tem", "Stahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und steigt hinan zum hohen Thron", "tokens": ["Und", "steigt", "hi\u00b7nan", "zum", "ho\u00b7hen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der r\u00f6mische Legat,", "tokens": ["Der", "r\u00f6\u00b7mi\u00b7sche", "Le\u00b7gat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spricht mit stolzem Herrscherton", "tokens": ["Und", "spricht", "mit", "stol\u00b7zem", "Herr\u00b7scher\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum K\u00f6nig Mithridat:", "tokens": ["Zum", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbdas R\u00f6mervolk und sein Senat", "tokens": ["\u00bb", "das", "R\u00f6\u00b7mer\u00b7volk", "und", "sein", "Se\u00b7nat"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt Frieden unbedingt,", "tokens": ["Bringt", "Frie\u00b7den", "un\u00b7be\u00b7dingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihm der K\u00f6nig Mithridat", "tokens": ["Wenn", "ihm", "der", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Reiches H\u00e4lfte bringt.", "tokens": ["Des", "Rei\u00b7ches", "H\u00e4lf\u00b7te", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch wenn der K\u00f6nig solchen Brauch", "tokens": ["Doch", "wenn", "der", "K\u00f6\u00b7nig", "sol\u00b7chen", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt seiner Weisheit fern,", "tokens": ["H\u00e4lt", "sei\u00b7ner", "Weis\u00b7heit", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschlingen die Legionen auch", "tokens": ["Ver\u00b7schlin\u00b7gen", "die", "Le\u00b7gi\u00b7o\u00b7nen", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die andre H\u00e4lfte gern!\u00ab", "tokens": ["Die", "and\u00b7re", "H\u00e4lf\u00b7te", "gern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da ruft der K\u00f6nig Mithridat:", "tokens": ["Da", "ruft", "der", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgeh, sage dem Senat,", "tokens": ["\u00bb", "geh", ",", "sa\u00b7ge", "dem", "Se\u00b7nat", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gern l\u00f6scht den Durst nach seinem Staat", "tokens": ["Gern", "l\u00f6scht", "den", "Durst", "nach", "sei\u00b7nem", "Staat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6nig Mithridat.", "tokens": ["Der", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf! sendet euern Consul nur!", "tokens": ["Auf", "!", "sen\u00b7det", "eu\u00b7ern", "Con\u00b7sul", "nur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die H\u00e4lfte, die ihr wollt \u2013", "tokens": ["Die", "H\u00e4lf\u00b7te", ",", "die", "ihr", "wollt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Poseidon h\u00f6ret meinen Schwur \u2013", "tokens": ["Po\u00b7sei\u00b7don", "h\u00f6\u00b7ret", "mei\u00b7nen", "Schwur", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seis, die ihr schlingen sollt!", "tokens": ["Seis", ",", "die", "ihr", "schlin\u00b7gen", "sollt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Den Consul schick ich selbst und sein", "tokens": ["Den", "Con\u00b7sul", "schick", "ich", "selbst", "und", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KON", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lechzend Legionenheer", "tokens": ["Lech\u00b7zend", "Le\u00b7gi\u00b7o\u00b7nen\u00b7heer"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In diese H\u00e4lfte des Reichs hinein,", "tokens": ["In", "die\u00b7se", "H\u00e4lf\u00b7te", "des", "Reichs", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u2013 Diese H\u00e4lfte ist das Meer.\u00ab", "tokens": ["\u2013", "Die\u00b7se", "H\u00e4lf\u00b7te", "ist", "das", "Meer", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDAT", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Der K\u00f6nig winkt. Da brechen all", "tokens": ["Der", "K\u00f6\u00b7nig", "winkt", ".", "Da", "bre\u00b7chen", "all"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwelger auf vom Schmaus,", "tokens": ["Die", "Schwel\u00b7ger", "auf", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und herrlicher Posaunenschall", "tokens": ["Und", "herr\u00b7li\u00b7cher", "Po\u00b7sau\u00b7nen\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt weit ins Meer hinaus.", "tokens": ["T\u00f6nt", "weit", "ins", "Meer", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Der Westwind wich, der Ostwind streicht", "tokens": ["Der", "We