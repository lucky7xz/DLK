{"textgrid.poem.40065": {"metadata": {"author": {"name": "Hoyers, Anna Ovena", "birth": "N.A.", "death": "N.A."}, "title": "[freut euch im Herren allezeit]", "genre": "verse", "period": "N.A.", "pub_year": 1619, "urn": "N.A.", "language": ["de:0.71", "no:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude Der Frommen/", "tokens": ["Freu\u00b7de", "Der", "From\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mu\u00df von Gott kommen;", "tokens": ["Mu\u00df", "von", "Gott", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wird nicht genommen.", "tokens": ["Wird", "nicht", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Freut euch im Herren allezeit/", "tokens": ["Freut", "euch", "im", "Her\u00b7ren", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasst kunt sey ewer Lindigkeit.", "tokens": ["Lasst", "kunt", "sey", "e\u00b7wer", "Lin\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herr ist nah/ drumb sorget nicht;", "tokens": ["Der", "Herr", "ist", "nah", "/", "drumb", "sor\u00b7get", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PAV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bittet nur so euch was geschicht;", "tokens": ["Bit\u00b7tet", "nur", "so", "euch", "was", "ge\u00b7schicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Legt ab die L\u00fcgen/ redt Warheit;", "tokens": ["Legt", "ab", "die", "L\u00fc\u00b7gen", "/", "redt", "War\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "VVFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit ewerm Nehsten friedlich seyt.", "tokens": ["Mit", "e\u00b7werm", "Nehs\u00b7ten", "fried\u00b7lich", "seyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gebet nicht raum dem L\u00e4sterer;", "tokens": ["Ge\u00b7bet", "nicht", "raum", "dem", "L\u00e4s\u00b7te\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer g'stolen hat der stehl nicht mehr;", "tokens": ["Wer", "g'\u00b7sto\u00b7len", "hat", "der", "stehl", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sondern arbeite mit den h\u00e4nden", "tokens": ["Son\u00b7dern", "ar\u00b7bei\u00b7te", "mit", "den", "h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Zu nutz den d\u00fcrfftig'n und Elenden.", "tokens": ["Zu", "nutz", "den", "d\u00fcrff\u00b7tig'n", "und", "E\u00b7len\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Faul geschw\u00e4tz/ schandbar wort und zorn", "tokens": ["Faul", "ge\u00b7schw\u00e4tz", "/", "schand\u00b7bar", "wort", "und", "zorn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Lasst nicht au\u00df ewerm munde horn;", "tokens": ["Lasst", "nicht", "au\u00df", "e\u00b7werm", "mun\u00b7de", "horn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vnreinigkeit/ geitz/ Hurerey/", "tokens": ["Vnr\u00b7ei\u00b7nig\u00b7keit", "/", "geitz", "/", "Hu\u00b7re\u00b7rey", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vnd l\u00e4sterung fern von euch sey.", "tokens": ["Vnd", "l\u00e4s\u00b7te\u00b7rung", "fern", "von", "euch", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "PPER", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Haltet Freundschafft mit Frommen hertzen/", "tokens": ["Hal\u00b7tet", "Freund\u00b7schafft", "mit", "From\u00b7men", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Seit z\u00fcchtig und Ehrbar im schertzen/", "tokens": ["Seit", "z\u00fcch\u00b7tig", "und", "Ehr\u00b7bar", "im", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "NN", "APPRART", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Vnd meidet allen b\u00f6sen schein.", "tokens": ["Vnd", "mei\u00b7det", "al\u00b7len", "b\u00f6\u00b7sen", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Lasst ewer Red' holdseelig seyn.", "tokens": ["Lasst", "e\u00b7wer", "Red'", "hold\u00b7see\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Meynt es mit allen menschen gut/", "tokens": ["Meynt", "es", "mit", "al\u00b7len", "men\u00b7schen", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Vergebt dem der euch b\u00f6ses thut;", "tokens": ["Ver\u00b7gebt", "dem", "der", "euch", "b\u00f6\u00b7ses", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PRELS", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Gedenckt was Gott euch hat gethan.", "tokens": ["Ge\u00b7denckt", "was", "Gott", "euch", "hat", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Heil'gen notturfft nemt euch an/", "tokens": ["Der", "Heil'\u00b7gen", "not\u00b7turfft", "nemt", "euch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Traget mitleiden mit den Armen;", "tokens": ["Tra\u00b7get", "mit\u00b7lei\u00b7den", "mit", "den", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Vnd ziehet an hertzlichs erbarmen/", "tokens": ["Vnd", "zie\u00b7het", "an", "hertz\u00b7lichs", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Werdet an guten Wercken Reich/", "tokens": ["Wer\u00b7det", "an", "gu\u00b7ten", "Wer\u00b7cken", "Reich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.26": {"text": "Verweist al\u00df Christi Diener euch.", "tokens": ["Ver\u00b7weist", "al\u00df", "Chris\u00b7ti", "Die\u00b7ner", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Redet und thut zu jeder frist", "tokens": ["Re\u00b7det", "und", "thut", "zu", "je\u00b7der", "frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Was nutz und gut zur bessrung ist.", "tokens": ["Was", "nutz", "und", "gut", "zur", "bess\u00b7rung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "In summa seyt erbawlich allen/", "tokens": ["In", "sum\u00b7ma", "seyt", "er\u00b7baw\u00b7lich", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "S\u00fcndiget niemand zu gefallen;", "tokens": ["S\u00fcn\u00b7di\u00b7get", "nie\u00b7mand", "zu", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Seyt in all ewerm thun geflissen", "tokens": ["Seyt", "in", "all", "e\u00b7werm", "thun", "ge\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "NN", "VVINF", "VVPP"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Zu behalten ein guts gewissen.", "tokens": ["Zu", "be\u00b7hal\u00b7ten", "ein", "guts", "ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Betr\u00fcbet nicht den Heil'gen Geist/", "tokens": ["Be\u00b7tr\u00fc\u00b7bet", "nicht", "den", "Heil'\u00b7gen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sondern Gott mit Danck-psalmen preist.", "tokens": ["Son\u00b7dern", "Gott", "mit", "Dan\u00b7ck\u00b7psal\u00b7men", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Lobet den Herrn/ dienet dem Nehsten.", "tokens": ["Lo\u00b7bet", "den", "Herrn", "/", "die\u00b7net", "dem", "Nehs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Vnd steht Euch bey mit h\u00fclff und Rath/", "tokens": ["Vnd", "steht", "Euch", "bey", "mit", "h\u00fclff", "und", "Rath", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sollt ihr sp\u00fchren in der that.", "tokens": ["Das", "sollt", "ihr", "sp\u00fch\u00b7ren", "in", "der", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr lieben Kinder seyt bereit", "tokens": ["Ihr", "lie\u00b7ben", "Kin\u00b7der", "seyt", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu guten wercken allezeit;", "tokens": ["Zu", "gu\u00b7ten", "wer\u00b7cken", "al\u00b7le\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Thut flei\u00df/ da\u00df Gottes Herrligkeit", "tokens": ["Thut", "flei\u00df", "/", "da\u00df", "Got\u00b7tes", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "KOUS", "NN", "NN"],