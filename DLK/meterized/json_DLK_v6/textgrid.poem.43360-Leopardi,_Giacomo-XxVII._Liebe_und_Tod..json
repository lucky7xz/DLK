{"textgrid.poem.43360": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxVII. Liebe und Tod.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Zwillinge des Schicksals Schoo\u00df entsprossen,", "tokens": ["Als", "Zwil\u00b7lin\u00b7ge", "des", "Schick\u00b7sals", "Schoo\u00df", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind Lieb' und Tod Genossen.", "tokens": ["Sind", "Lieb'", "und", "Tod", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nichts Sch\u00f6nres ward hinieden", "tokens": ["Nichts", "Sch\u00f6n\u00b7res", "ward", "hi\u00b7nie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Erde, nichts der Sternenwelt beschieden.", "tokens": ["Der", "Er\u00b7de", ",", "nichts", "der", "Ster\u00b7nen\u00b7welt", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Jener stammt die h\u00f6chste,", "tokens": ["Von", "Je\u00b7ner", "stammt", "die", "h\u00f6chs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die seligste der Freuden,", "tokens": ["Die", "se\u00b7ligs\u00b7te", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Die je uns bl\u00fchen mag im Meer des Seins,", "tokens": ["Die", "je", "uns", "bl\u00fc\u00b7hen", "mag", "im", "Meer", "des", "Seins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und von den schwersten Leiden", "tokens": ["Und", "von", "den", "schwers\u00b7ten", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kann ihr Geno\u00df erl\u00f6sen.", "tokens": ["Kann", "ihr", "Ge\u00b7no\u00df", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das wundersame Wesen,", "tokens": ["Das", "wun\u00b7der\u00b7sa\u00b7me", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Holdselig anzuschauen,", "tokens": ["Hold\u00b7se\u00b7lig", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Nicht wie's der Feigling pflegt sich vorzustellen,", "tokens": ["Nicht", "wie's", "der", "Feig\u00b7ling", "pflegt", "sich", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "NN", "VVFIN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Will gern der jungen Liebe", "tokens": ["Will", "gern", "der", "jun\u00b7gen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sich oftmals zugesellen.", "tokens": ["Sich", "oft\u00b7mals", "zu\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Vereint durchziehn sie dann des Lebens Auen", "tokens": ["Ver\u00b7eint", "durch\u00b7ziehn", "sie", "dann", "des", "Le\u00b7bens", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und sind des Weisen Trost in aller Tr\u00fcbe.", "tokens": ["Und", "sind", "des", "Wei\u00b7sen", "Trost", "in", "al\u00b7ler", "Tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Je mehr voll Liebesglut,", "tokens": ["Je", "mehr", "voll", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Je weiser ist ein Herz, je stolzer achtet's", "tokens": ["Je", "wei\u00b7ser", "ist", "ein", "Herz", ",", "je", "stol\u00b7zer", "ach\u00b7tet's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJA", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Gering des Lebens Wehe.", "tokens": ["Ge\u00b7ring", "des", "Le\u00b7bens", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Kein Machtgebot, o Liebe,", "tokens": ["Kein", "Macht\u00b7ge\u00b7bot", ",", "o", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Befeuert so wie deins zu jedem Wagni\u00df.", "tokens": ["Be\u00b7feu\u00b7ert", "so", "wie", "deins", "zu", "je\u00b7dem", "Wag\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "NE", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Entflammt ja deine N\u00e4he", "tokens": ["Ent\u00b7flammt", "ja", "dei\u00b7ne", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ein jedes Herz mit Muth,", "tokens": ["Ein", "je\u00b7des", "Herz", "mit", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Belebt den sinkenden und pflegt zu Thaten,", "tokens": ["Be\u00b7lebt", "den", "sin\u00b7ken\u00b7den", "und", "pflegt", "zu", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Nicht nur zu m\u00fc\u00df'gem Br\u00fcten, wie sie pflegen,", "tokens": ["Nicht", "nur", "zu", "m\u00fc\u00df'\u00b7gem", "Br\u00fc\u00b7ten", ",", "wie", "sie", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die Geister zu erregen.", "tokens": ["Die", "Geis\u00b7ter", "zu", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn in der Jugend Bl\u00fcte", "tokens": ["Wenn", "in", "der", "Ju\u00b7gend", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich regt in Herzenstiefen", "tokens": ["Sich", "regt", "in", "Her\u00b7zen\u00b7stie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein z\u00e4rtliches Verlangen,", "tokens": ["Ein", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwacht zugleich mit ihm ein m\u00fcdes Bangen,", "tokens": ["Er\u00b7wacht", "zu\u00b7gleich", "mit", "ihm", "ein", "m\u00fc\u00b7des", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein schmachtend Todessehnen im Gem\u00fcthe,", "tokens": ["Ein", "schmach\u00b7tend", "To\u00b7des\u00b7seh\u00b7nen", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht wei\u00df ich, wie; doch Allen,", "tokens": ["Nicht", "wei\u00df", "ich", ",", "wie", ";", "doch", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "$.", "ADV", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die war und hei\u00df geliebt, ist's so ergangen.", "tokens": ["Die", "war", "und", "hei\u00df", "ge\u00b7liebt", ",", "ist's", "so", "er\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KON", "ADJD", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann wohl mit Grau'n betrachtet", "tokens": ["Dann", "wohl", "mit", "Grau'n", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Mensch die Oede rings, und diese Erde", "tokens": ["Der", "Mensch", "die", "O\u00b7e\u00b7de", "rings", ",", "und", "die\u00b7se", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "KON", "PDAT", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "D\u00fcnkt unbewohnbar ihm, wenn seinem Herzen", "tokens": ["D\u00fcnkt", "un\u00b7be\u00b7wohn\u00b7bar", "ihm", ",", "wenn", "sei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der eine Wunsch versagt wird,", "tokens": ["Der", "ei\u00b7ne", "Wunsch", "ver\u00b7sagt", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die neue, grenzenlose", "tokens": ["Die", "neu\u00b7e", ",", "gren\u00b7zen\u00b7lo\u00b7se"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Gl\u00fcckseligkeit, wonach die Seele trachtet.", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ",", "wo\u00b7nach", "die", "See\u00b7le", "trach\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und ahnt er gar den Sturm, der seine Brust", "tokens": ["Und", "ahnt", "er", "gar", "den", "Sturm", ",", "der", "sei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ersch\u00fcttern wird um sie: ersehnt er Ruhe", "tokens": ["Er\u00b7sch\u00fct\u00b7tern", "wird", "um", "sie", ":", "er\u00b7sehnt", "er", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$.", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und m\u00f6cht' im Hafen landen,", "tokens": ["Und", "m\u00f6cht'", "im", "Ha\u00b7fen", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Dem Aufruhr zu entrinnen", "tokens": ["Dem", "Auf\u00b7ruhr", "zu", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der Leidenschaft, die ihm die Welt umnachtet.", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", ",", "die", "ihm", "die", "Welt", "um\u00b7nach\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn Alles dann ringsum", "tokens": ["Wenn", "Al\u00b7les", "dann", "ring\u00b7sum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die wilde Macht verschlungen", "tokens": ["Die", "wil\u00b7de", "Macht", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Gram wie Wetterstrahl im Busen w\u00fcthet,", "tokens": ["Und", "Gram", "wie", "Wet\u00b7ter\u00b7strahl", "im", "Bu\u00b7sen", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie innig tausendmal", "tokens": ["Wie", "in\u00b7nig", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wirst du herangefleht,", "tokens": ["Wirst", "du", "her\u00b7an\u00b7ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O Tod, vom Liebenden in seiner Qual,", "tokens": ["O", "Tod", ",", "vom", "Lie\u00b7ben\u00b7den", "in", "sei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie oft im Abendstrahl,", "tokens": ["Wie", "oft", "im", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie oft, wenn fr\u00fch er sinkt aufs Lager nieder,", "tokens": ["Wie", "oft", ",", "wenn", "fr\u00fch", "er", "sinkt", "aufs", "La\u00b7ger", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Preis't er als h\u00f6chstes Gl\u00fcck, w\u00e4r's ihm verg\u00f6nnt,", "tokens": ["Preis't", "er", "als", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ",", "w\u00e4r's", "ihm", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KOUS", "ADJA", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nie mehr die matten Glieder", "tokens": ["Nie", "mehr", "die", "mat\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu heben, nie die Sonne mehr zu sehen;", "tokens": ["Zu", "he\u00b7ben", ",", "nie", "die", "Son\u00b7ne", "mehr", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und h\u00f6rt er mit des Todtengl\u00f6ckleins Klange", "tokens": ["Und", 