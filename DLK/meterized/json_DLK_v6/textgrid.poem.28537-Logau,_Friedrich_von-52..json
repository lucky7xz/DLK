{"textgrid.poem.28537": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann, Jungfern, eure Fl\u00f6h, die ihr habt zu Haus-innen,", "tokens": ["Wann", ",", "Jung\u00b7fern", ",", "eu\u00b7re", "Fl\u00f6h", ",", "die", "ihr", "habt", "zu", "Haus\u00b7in\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was sie geh\u00f6rt, gesehn, vermelden solten k\u00fcnnen,", "tokens": ["Was", "sie", "ge\u00b7h\u00f6rt", ",", "ge\u00b7sehn", ",", "ver\u00b7mel\u00b7den", "sol\u00b7ten", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie mancher fragte sie, der Lust zu freyen hat,", "tokens": ["Wie", "man\u00b7cher", "frag\u00b7te", "sie", ",", "der", "Lust", "zu", "frey\u00b7en", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh als den besten Freund, um einen treuen Rath.", "tokens": ["Eh", "als", "den", "bes\u00b7ten", "Freund", ",", "um", "ei\u00b7nen", "treu\u00b7en", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,", "KOUI", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}