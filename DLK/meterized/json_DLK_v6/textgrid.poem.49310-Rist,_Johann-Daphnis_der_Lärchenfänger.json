{"textgrid.poem.49310": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Daphnis der L\u00e4rchenf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Daphnis einst spazieren gieng", "tokens": ["Als", "Daph\u00b7nis", "einst", "spa\u00b7zie\u00b7ren", "gieng"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohngef\u00e4hr zwo Lerchen fieng,", "tokens": ["Und", "ohn\u00b7ge\u00b7f\u00e4hr", "zwo", "Ler\u00b7chen", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gedacht er an die Galatheen,", "tokens": ["Ge\u00b7dacht", "er", "an", "die", "Ga\u00b7la\u00b7theen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach: \u00bbAllerliebstes T\u00e4ubelein,", "tokens": ["Sprach", ":", "\u00bb", "Al\u00b7ler\u00b7liebs\u00b7tes", "T\u00e4u\u00b7be\u00b7lein", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, da\u00df du m\u00f6chtest bei mir sein,", "tokens": ["Ach", ",", "da\u00df", "du", "m\u00f6ch\u00b7test", "bei", "mir", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du m\u00f6chtest drei Gefangner sehen.", "tokens": ["Du", "m\u00f6ch\u00b7test", "drei", "Ge\u00b7fang\u00b7ner", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein halbe Stund' ist kaum vorbei,", "tokens": ["Ein", "hal\u00b7be", "Stund'", "ist", "kaum", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da waren diese V\u00f6glein frei,", "tokens": ["Da", "wa\u00b7ren", "die\u00b7se", "V\u00f6\u00b7glein", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die nunmehr in dem Netze hangen;", "tokens": ["Die", "nun\u00b7mehr", "in", "dem", "Net\u00b7ze", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch ist es wahrlich nicht so lang',", "tokens": ["Auch", "ist", "es", "wahr\u00b7lich", "nicht", "so", "lang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als ich noch lebte sonder Zwang", "tokens": ["Als", "ich", "noch", "leb\u00b7te", "son\u00b7der", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und bin doch itzt so stark gefangen.", "tokens": ["Und", "bin", "doch", "itzt", "so", "stark", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was hilft michs denn, da\u00df ich so oft", "tokens": ["Was", "hilft", "michs", "denn", ",", "da\u00df", "ich", "so", "oft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bezwing' ein V\u00f6glein unverhofft", "tokens": ["Be\u00b7zwing'", "ein", "V\u00f6\u00b7glein", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und bin doch selber fast verstricket", "tokens": ["Und", "bin", "doch", "sel\u00b7ber", "fast", "ver\u00b7stri\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Durch Galathe; was n\u00fctzt es mir,", "tokens": ["Durch", "Ga\u00b7la\u00b7the", ";", "was", "n\u00fctzt", "es", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df ich so manches schnelles Thier", "tokens": ["Da\u00df", "ich", "so", "man\u00b7ches", "schnel\u00b7les", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hab' aus den L\u00fcften wegger\u00fccket?", "tokens": ["Hab'", "aus", "den", "L\u00fcf\u00b7ten", "weg\u00b7ge\u00b7r\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sol das noch rechte Freiheit sein,", "tokens": ["Sol", "das", "noch", "rech\u00b7te", "Frei\u00b7heit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Indem so manches V\u00f6gelein", "tokens": ["In\u00b7dem", "so", "man\u00b7ches", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sich selber zum Gefangnen machet", "tokens": ["Sich", "sel\u00b7ber", "zum", "Ge\u00b7fang\u00b7nen", "ma\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und beut sich mir zu dienen an,", "tokens": ["Und", "beut", "sich", "mir", "zu", "die\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPER", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Alsdenn mich die bezwingen kan,", "tokens": ["Als\u00b7denn", "mich", "die", "be\u00b7zwin\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die meiner Schmerzen h\u00f6hnisch lachet.", "tokens": ["Die", "mei\u00b7ner", "Schmer\u00b7zen", "h\u00f6h\u00b7nisch", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ach nein, ihr Lerchen, ob ich zwar", "tokens": ["Ach", "nein", ",", "ihr", "Ler\u00b7chen", ",", "ob", "ich", "zwar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Euch k\u00f6nt' erw\u00fcrgen ganz und gar,", "tokens": ["Euch", "k\u00f6nt'", "er\u00b7w\u00fcr\u00b7gen", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So wil ich doch aus Liebe schonen,", "tokens": ["So", "wil", "ich", "doch", "aus", "Lie\u00b7be", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Ich wil euch nicht mit Ach und Weh,", "tokens": ["Ich", "wil", "euch", "nicht", "mit", "Ach", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Als mir zu thun pflegt Galathe,", "tokens": ["Als", "mir", "zu", "thun", "pflegt", "Ga\u00b7la\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "F\u00fcr unverf\u00e4lschte Treue lohnen.", "tokens": ["F\u00fcr", "un\u00b7ver\u00b7f\u00e4lschte", "Treu\u00b7e", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.31": {"text": "Daf\u00fcr solt ihr bei Tag und Nacht.", "tokens": ["Da\u00b7f\u00fcr", "solt", "ihr", "bei", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn Daphnis h\u00e4lt die Thr\u00e4nenwacht,", "tokens": ["Wenn", "Daph\u00b7nis", "h\u00e4lt", "die", "Thr\u00e4\u00b7nen\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Galatheen Lob ausbreiten", "tokens": ["Der", "Ga\u00b7la\u00b7theen", "Lob", "aus\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Und zeigen allen Hirten an,", "tokens": ["Und", "zei\u00b7gen", "al\u00b7len", "Hir\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Weil Daphnis nicht mehr leben kan,", "tokens": ["Weil", "Daph\u00b7nis", "nicht", "mehr", "le\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sie sollen ihm sein Grab bereiten.", "tokens": ["Sie", "sol\u00b7len", "ihm", "sein", "Grab", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wolan, ihr allerliebsten Thier,", "tokens": ["Wo\u00b7lan", ",", "ihr", "al\u00b7ler\u00b7liebs\u00b7ten", "Thier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ich bin gefangen mehr als ihr", "tokens": ["Ich", "bin", "ge\u00b7fan\u00b7gen", "mehr", "als", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PIAT", "KOKOM", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und kan die Freiheit nicht erwerben;", "tokens": ["Und", "kan", "die", "Frei\u00b7heit", "nicht", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Dennoch so sag' ich in der Stil'", "tokens": ["Den\u00b7noch", "so", "sag'", "ich", "in", "der", "Stil'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und schwer' euch, da\u00df ich redlich wil", "tokens": ["Und", "schwer'", "euch", ",", "da\u00df", "ich", "red\u00b7lich", "wil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der Galatheen Diener sterben.", "tokens": ["Der", "Ga\u00b7la\u00b7theen", "Die\u00b7ner", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}