{"dta.poem.12060": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wei\u00df nicht\u201c hab' ich unbedenklich oft gesagt", "tokens": ["\u201e", "ich", "wei\u00df", "nicht", "\u201c", "hab'", "ich", "un\u00b7be\u00b7denk\u00b7lich", "oft", "ge\u00b7sagt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$(", "VAFIN", "PPER", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Kinde, das mich Unbeantwortlichs gefragt.", "tokens": ["Dem", "Kin\u00b7de", ",", "das", "mich", "Un\u00b7be\u00b7ant\u00b7wort\u00b7lichs", "ge\u00b7fragt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zuletzt hat es gesagt: du wei\u00dft auch gar nichts, Vater!", "tokens": ["Zu\u00b7letzt", "hat", "es", "ge\u00b7sagt", ":", "du", "wei\u00dft", "auch", "gar", "nichts", ",", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "ADV", "ADV", "PIS", "$,", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und zu Besinnung hat mich das gebracht, zu spater.", "tokens": ["Und", "zu", "Be\u00b7sin\u00b7nung", "hat", "mich", "das", "ge\u00b7bracht", ",", "zu", "spa\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "PDS", "VVPP", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201eich wei\u00df nicht\u201c sollst du nie dem Kind auf seine Fragen,", "tokens": ["\u201e", "ich", "wei\u00df", "nicht", "\u201c", "sollst", "du", "nie", "dem", "Kind", "auf", "sei\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$(", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ausweichend ihm vielmehr dies oder jenes sagen.", "tokens": ["Aus\u00b7wei\u00b7chend", "ihm", "viel\u00b7mehr", "dies", "o\u00b7der", "je\u00b7nes", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ADV", "PDS", "KON", "PDS", "VVINF", "$."], "meter": "-+--+----+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201eich denk'? ich glaub'? ich mein'?\u201c ei, Gott beh\u00fcte, nein!", "tokens": ["\u201e", "ich", "denk'", "?", "ich", "glaub'", "?", "ich", "mein'", "?", "\u201c", "ei", ",", "Gott", "be\u00b7h\u00fc\u00b7te", ",", "nein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "$(", "ITJ", "$,", "NN", "VVFIN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das w\u00fcrd' Unwissenheit in andrer Wendung seyn.", "tokens": ["Das", "w\u00fcrd'", "Un\u00b7wis\u00b7sen\u00b7heit", "in", "an\u00b7drer", "Wen\u00b7dung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201enicht sagen will ichs dir, du wirst es schon erfahren,", "tokens": ["\u201e", "nicht", "sa\u00b7gen", "will", "ichs", "dir", ",", "du", "wirst", "es", "schon", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVINF", "VMFIN", "PIS", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwarte nur die Zeit, du kannst dein Fragen sparen.\u201c", "tokens": ["Er\u00b7war\u00b7te", "nur", "die", "Zeit", ",", "du", "kannst", "dein", "Fra\u00b7gen", "spa\u00b7ren", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}