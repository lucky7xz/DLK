{"textgrid.poem.35600": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Wanderer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer lehnt am H\u00fcgel die wei\u00dfe Nacht,", "tokens": ["Im\u00b7mer", "lehnt", "am", "H\u00fc\u00b7gel", "die", "wei\u00b7\u00dfe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wo in Silbert\u00f6nen die Pappel ragt,", "tokens": ["Wo", "in", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen", "die", "Pap\u00b7pel", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stern' und Steine sind.", "tokens": ["Stern'", "und", "Stei\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schlafend w\u00f6lbt sich \u00fcber den Gie\u00dfbach der Steg,", "tokens": ["Schla\u00b7fend", "w\u00f6lbt", "sich", "\u00fc\u00b7ber", "den", "Gie\u00df\u00b7bach", "der", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Folgt dem Knaben ein erstorbenes Antlitz,", "tokens": ["Folgt", "dem", "Kna\u00b7ben", "ein", "ers\u00b7tor\u00b7be\u00b7nes", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sichelmond in rosiger Schlucht", "tokens": ["Si\u00b7chel\u00b7mond", "in", "ro\u00b7si\u00b7ger", "Schlucht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ferne preisenden Hirten. In altem Gestein", "tokens": ["Fer\u00b7ne", "prei\u00b7sen\u00b7den", "Hir\u00b7ten", ".", "In", "al\u00b7tem", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schaut aus kristallenen Augen die Kr\u00f6te,", "tokens": ["Schaut", "aus", "kris\u00b7tal\u00b7le\u00b7nen", "Au\u00b7gen", "die", "Kr\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Erwacht der bl\u00fchende Wind, die Vogelstimme des Totengleichen", "tokens": ["Er\u00b7wacht", "der", "bl\u00fc\u00b7hen\u00b7de", "Wind", ",", "die", "Vo\u00b7gel\u00b7stim\u00b7me", "des", "To\u00b7ten\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und die Schritte ergr\u00fcnen leise im Wald.", "tokens": ["Und", "die", "Schrit\u00b7te", "er\u00b7gr\u00fc\u00b7nen", "lei\u00b7se", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dieses erinnert an Baum und Tier. Langsame Stufen von Moos;", "tokens": ["Die\u00b7ses", "e\u00b7rin\u00b7nert", "an", "Baum", "und", "Tier", ".", "Lang\u00b7sa\u00b7me", "Stu\u00b7fen", "von", "Moos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "KON", "NN", "$.", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+---+-++--+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der Mond,", "tokens": ["Und", "der", "Mond", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "Der gl\u00e4nzend in traurigen Wassern versinkt.", "tokens": ["Der", "gl\u00e4n\u00b7zend", "in", "trau\u00b7ri\u00b7gen", "Was\u00b7sern", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Jener kehrt wieder und wandelt an gr\u00fcnem Gestade,", "tokens": ["Je\u00b7ner", "kehrt", "wie\u00b7der", "und", "wan\u00b7delt", "an", "gr\u00fc\u00b7nem", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schaukelt auf schwarzem Gondelschiffchen durch die verfallene Stadt.", "tokens": ["Schau\u00b7kelt", "auf", "schwar\u00b7zem", "Gon\u00b7del\u00b7schiff\u00b7chen", "durch", "die", "ver\u00b7fal\u00b7le\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+--+", "measure": "iambic.septa.invert"}}}}}