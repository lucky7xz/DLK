{"textgrid.poem.46457": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verstreute Menschen gehen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstreute Menschen gehen", "tokens": ["Ver\u00b7streu\u00b7te", "Men\u00b7schen", "ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im feuchten Regenwehen,", "tokens": ["Im", "feuch\u00b7ten", "Re\u00b7gen\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vorstadtg\u00e4rten rauschen,", "tokens": ["Vor\u00b7stadt\u00b7g\u00e4r\u00b7ten", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wolken sinken, bauschen", "tokens": ["Wol\u00b7ken", "sin\u00b7ken", ",", "bau\u00b7schen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sich. Ein Karren rollt am Zaune hin \u2013", "tokens": ["Sich", ".", "Ein", "Kar\u00b7ren", "rollt", "am", "Zau\u00b7ne", "hin", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ich kann nicht anders: ich mu\u00df sehen, mu\u00df lauschen,", "tokens": ["Ich", "kann", "nicht", "an\u00b7ders", ":", "ich", "mu\u00df", "se\u00b7hen", ",", "mu\u00df", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$.", "PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ich wei\u00df nicht, wie ich bin", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In diesem Allen \u2013", "tokens": ["In", "die\u00b7sem", "Al\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein Gott, du flutest mit dem Wehen in mein Ohr,", "tokens": ["Mein", "Gott", ",", "du", "flu\u00b7test", "mit", "dem", "We\u00b7hen", "in", "mein", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du lachst im Tr\u00e4llern der Kinder da am Gartentor \u2013", "tokens": ["Du", "lachst", "im", "Tr\u00e4l\u00b7lern", "der", "Kin\u00b7der", "da", "am", "Gar\u00b7ten\u00b7tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du willst dies Leben: diese Bilder, dieses Rauschen", "tokens": ["Du", "willst", "dies", "Le\u00b7ben", ":", "die\u00b7se", "Bil\u00b7der", ",", "die\u00b7ses", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "NN", "$.", "PDAT", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In mich f\u00fcr meine Seele tauschen!", "tokens": ["In", "mich", "f\u00fcr", "mei\u00b7ne", "See\u00b7le", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So str\u00f6m' ich mit dem Orgeln dieser Landschaft hin \u2013", "tokens": ["So", "str\u00f6m'", "ich", "mit", "dem", "Or\u00b7geln", "die\u00b7ser", "Land\u00b7schaft", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kann ich nicht anders: ich mu\u00df mich berauschen,", "tokens": ["So", "kann", "ich", "nicht", "an\u00b7ders", ":", "ich", "mu\u00df", "mich", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Da\u00df ich nicht wei\u00df, wie ich bin", "tokens": ["Da\u00df", "ich", "nicht", "wei\u00df", ",", "wie", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In diesem Allen.", "tokens": ["In", "die\u00b7sem", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}