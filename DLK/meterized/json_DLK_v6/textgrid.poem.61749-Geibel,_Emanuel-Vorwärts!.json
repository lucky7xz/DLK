{"textgrid.poem.61749": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Deutschlands Gauen hallt das Wetter aus,", "tokens": ["Durch", "Deutschlands", "Gau\u00b7en", "hallt", "das", "Wet\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Luft wird hell, entschieden ist der Strau\u00df;", "tokens": ["Die", "Luft", "wird", "hell", ",", "ent\u00b7schie\u00b7den", "ist", "der", "Strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VVPP", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zertr\u00fcmmert liegt, das keiner Schmach gewehrt,", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "liegt", ",", "das", "kei\u00b7ner", "Schmach", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Haus am Main, ohnm\u00e4cht'ger Zwietracht Herd,", "tokens": ["Das", "Haus", "am", "Main", ",", "ohn\u00b7m\u00e4cht'\u00b7ger", "Zwiet\u00b7racht", "Herd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcberm Schutt, auf bessern Fels gegr\u00fcndet,", "tokens": ["Und", "\u00fc\u00b7berm", "Schutt", ",", "auf", "bes\u00b7sern", "Fels", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steigt auf der Bau, der schon das Reich verk\u00fcndet.", "tokens": ["Steigt", "auf", "der", "Bau", ",", "der", "schon", "das", "Reich", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einf\u00fcgt sich Stein um Stein. Und f\u00e4llt zersprengt", "tokens": ["Ein\u00b7f\u00fcgt", "sich", "Stein", "um", "Stein", ".", "Und", "f\u00e4llt", "zer\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NN", "APPR", "NN", "$.", "KON", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Manch alter Schmuck, dran unser Herz noch h\u00e4ngt,", "tokens": ["Manch", "al\u00b7ter", "Schmuck", ",", "dran", "un\u00b7ser", "Herz", "noch", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir bringen ihn getrost, wie traut er war,", "tokens": ["Wir", "brin\u00b7gen", "ihn", "ge\u00b7trost", ",", "wie", "traut", "er", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "PWAV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem gro\u00dfen Vaterland zum Opfer dar", "tokens": ["Dem", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7land", "zum", "Op\u00b7fer", "dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und trinken reichres Leben frohgemutet", "tokens": ["Und", "trin\u00b7ken", "reich\u00b7res", "Le\u00b7ben", "froh\u00b7ge\u00b7mu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Strom der Kraft, die aus dem Ganzen flutet.", "tokens": ["Im", "Strom", "der", "Kraft", ",", "die", "aus", "dem", "Gan\u00b7zen", "flu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du aber, kriegerisch Geschlecht, bestellt,", "tokens": ["Du", "a\u00b7ber", ",", "krie\u00b7ge\u00b7risch", "Ge\u00b7schlecht", ",", "be\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hort zu sein der jungen deutschen Welt,", "tokens": ["Ein", "Hort", "zu", "sein", "der", "jun\u00b7gen", "deut\u00b7schen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit deinen Zielen wachse! Was das Schwert", "tokens": ["Mit", "dei\u00b7nen", "Zie\u00b7len", "wach\u00b7se", "!", "Was", "das", "Schwert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PWS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begann, vollend' es, deiner Siege wert!", "tokens": ["Be\u00b7gann", ",", "vollend'", "es", ",", "dei\u00b7ner", "Sie\u00b7ge", "wert", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Haupt umkr\u00e4nzt mit frischem Eichenlaube", "tokens": ["Das", "Haupt", "um\u00b7kr\u00e4nzt", "mit", "fri\u00b7schem", "Ei\u00b7chen\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df, was verwelkt ist, hinter dir im Staube!", "tokens": ["La\u00df", ",", "was", "ver\u00b7welkt", "ist", ",", "hin\u00b7ter", "dir", "im", "Stau\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PRELS", "VVPP", "VAFIN", "$,", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durchbrich in jugendlicher Heldenkraft", "tokens": ["Durch\u00b7brich", "in", "ju\u00b7gend\u00b7li\u00b7cher", "Hel\u00b7den\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der l\u00e4ngst zu eng gewordnen Formel Haft!", "tokens": ["Der", "l\u00e4ngst", "zu", "eng", "ge\u00b7word\u00b7nen", "For\u00b7mel", "Haft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wirf ab den Starrsinn, der, was fr\u00f6hlich bl\u00fcht,", "tokens": ["Wirf", "ab", "den", "Starr\u00b7sinn", ",", "der", ",", "was", "fr\u00f6h\u00b7lich", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gewaltsam nach der Schnur zu ziehn sich m\u00fcht!", "tokens": ["Ge\u00b7walt\u00b7sam", "nach", "der", "Schnur", "zu", "ziehn", "sich", "m\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des jungen Weins lebend'ge Str\u00f6me lassen", "tokens": ["Des", "jun\u00b7gen", "Weins", "le\u00b7ben\u00b7d'\u00b7ge", "Str\u00f6\u00b7me", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sich nimmer in die alten Schl\u00e4uche fassen.", "tokens": ["Sich", "nim\u00b7mer", "in", "die", "al\u00b7ten", "Schl\u00e4u\u00b7che", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du k\u00e4mpftest nicht nach seellos dumpfem Brauch,", "tokens": ["Du", "k\u00e4mpf\u00b7test", "nicht", "nach", "seel\u00b7los", "dum\u00b7pfem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deinen Fahnen wob des Geistes Hauch;", "tokens": ["In", "dei\u00b7nen", "Fah\u00b7nen", "wob", "des", "Geis\u00b7tes", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schuf den Sieg dir, da\u00df im Schlachtgewog'", "tokens": ["Das", "schuf", "den", "Sieg", "dir", ",", "da\u00df", "im", "Schlacht\u00b7ge\u00b7wo\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sein Brausen \u00fcber deinen Fahnen zog;", "tokens": ["Sein", "Brau\u00b7sen", "\u00fc\u00b7ber", "dei\u00b7nen", "Fah\u00b7nen", "zog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit ihm im Bunde vorw\u00e4rts! La\u00df ihn walten", "tokens": ["Mit", "ihm", "im", "Bun\u00b7de", "vor\u00b7w\u00e4rts", "!", "La\u00df", "ihn", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$.", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, die da tot sind, sich an Totes halten!", "tokens": ["Und", ",", "die", "da", "tot", "sind", ",", "sich", "an", "To\u00b7tes", "hal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du f\u00fchrst den Adler, zieh uns denn voran", "tokens": ["Du", "f\u00fchrst", "den", "Ad\u00b7ler", ",", "zieh", "uns", "denn", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Adlersflug auf morgenroter Bahn!", "tokens": ["Mit", "Ad\u00b7lers\u00b7flug", "auf", "mor\u00b7gen\u00b7ro\u00b7ter", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Flieg in der Freiheit Sonne k\u00fchn hinein,", "tokens": ["Flieg", "in", "der", "Frei\u00b7heit", "Son\u00b7ne", "k\u00fchn", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und du wirst deutsch, und dein wird Deutschland sein,", "tokens": ["Und", "du", "wirst", "deutsch", ",", "und", "dein", "wird", "Deutschland", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "VAFIN", "NE", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Schnee der Gletscher bis zum Bernsteinmeere", "tokens": ["Vom", "Schnee", "der", "Glet\u00b7scher", "bis", "zum", "Bern\u00b7stein\u00b7mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Glorreich verj\u00fcngt in Eintracht, Macht und Ehre.", "tokens": ["Glor\u00b7reich", "ver\u00b7j\u00fcngt", "in", "Ein\u00b7tracht", ",", "Macht", "und", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}