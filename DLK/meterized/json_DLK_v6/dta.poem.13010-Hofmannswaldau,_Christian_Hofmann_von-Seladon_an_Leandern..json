{"dta.poem.13010": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seladon an Leandern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leander bindet mich, drum schreib' ich auch gebunden.", "tokens": ["Le\u00b7an\u00b7der", "bin\u00b7det", "mich", ",", "drum", "schreib'", "ich", "auch", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PAV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er fesselt mich mit gunst, o s\u00fcsse dienstbarkeit!", "tokens": ["Er", "fes\u00b7selt", "mich", "mit", "gunst", ",", "o", "s\u00fcs\u00b7se", "dienst\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich k\u00fc\u00df\u2019 in wahrheit noch die angenehmen stunden,", "tokens": ["Ich", "k\u00fc\u00df'", "in", "wahr\u00b7heit", "noch", "die", "an\u00b7ge\u00b7neh\u00b7men", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da dich mein auge sah\u2019, wo aber ist die zeit?", "tokens": ["Da", "dich", "mein", "au\u00b7ge", "sah'", ",", "wo", "a\u00b7ber", "ist", "die", "zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie ist als wie ein flu\u00df gantz unvermerckt entwichen.", "tokens": ["Sie", "ist", "als", "wie", "ein", "flu\u00df", "gantz", "un\u00b7ver\u00b7merckt", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "KOKOM", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch unsre liebe bleibt so fest als helffenbein.", "tokens": ["Doch", "uns\u00b7re", "lie\u00b7be", "bleibt", "so", "fest", "als", "helf\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und hast du gleich mein hertz auf keinen stein gestrichen,", "tokens": ["Und", "hast", "du", "gleich", "mein", "hertz", "auf", "kei\u00b7nen", "stein", "ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird es doch bey der prob\u2019 ein glantz der freundschafft seyn.", "tokens": ["Wird", "es", "doch", "bey", "der", "prob'", "ein", "glantz", "der", "freund\u00b7schafft", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der bunte rosenthal ist itzt mein schreibe-zimmer;", "tokens": ["Der", "bun\u00b7te", "ro\u00b7sen\u00b7thal", "ist", "itzt", "mein", "schrei\u00b7be\u00b7zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die hand mein schreibe-tisch, die dint\u2019 ein bleywei\u00df-stifft.", "tokens": ["Die", "hand", "mein", "schrei\u00b7be\u00b7tisch", ",", "die", "dint'", "ein", "bley\u00b7wei\u00df\u00b7stifft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schreib\u2019 ich nicht, wie ich will, so denck ich gleichwol immer:", "tokens": ["Schreib'", "ich", "nicht", ",", "wie", "ich", "will", ",", "so", "denck", "ich", "gleich\u00b7wol", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VVIMP", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.12": {"text": "Wenn meine feder nur Leanders fehler trifft.", "tokens": ["Wenn", "mei\u00b7ne", "fe\u00b7der", "nur", "Le\u00b7an\u00b7ders", "feh\u00b7ler", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch, was fehler! was? kan auch Leander fehlen?", "tokens": ["Je\u00b7doch", ",", "was", "feh\u00b7ler", "!", "was", "?", "kan", "auch", "Le\u00b7an\u00b7der", "feh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADJD", "$.", "PWS", "$.", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das, was er fehler nennt, ist offtmals ein magnet.", "tokens": ["Das", ",", "was", "er", "feh\u00b7ler", "nennt", ",", "ist", "offt\u00b7mals", "ein", "mag\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VAFIN", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Kein dieb kan so verschmitzt, als er, die hertzen stehlen.", "tokens": ["Kein", "dieb", "kan", "so", "ver\u00b7schmitzt", ",", "als", "er", ",", "die", "hert\u00b7zen", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er weis bald, welcher wind in unsre seegel geht.", "tokens": ["Er", "weis", "bald", ",", "wel\u00b7cher", "wind", "in", "uns\u00b7re", "see\u00b7gel", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch halt! ich schreib\u2019 itzt nicht sein bildni\u00df abzudr\u00fccken,", "tokens": ["Doch", "halt", "!", "ich", "schreib'", "itzt", "nicht", "sein", "bild\u00b7ni\u00df", "ab\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die farben w\u00fcrden mir auch nicht zu dienste stehn.", "tokens": ["Die", "far\u00b7ben", "w\u00fcr\u00b7den", "mir", "auch", "nicht", "zu", "diens\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das, was ich will, kanst du aus dieser zeil\u2019 erblicken.", "tokens": ["Das", ",", "was", "ich", "will", ",", "kanst", "du", "aus", "die\u00b7ser", "zeil'", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mag es Leandern noch vergn\u00fcgt und wohl ergehn?", "tokens": ["Mag", "es", "Le\u00b7an\u00b7dern", "noch", "ver\u00b7gn\u00fcgt", "und", "wohl", "er\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADV", "VVPP", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So viel erlaubt die zeit. So viel ist mein verlangen.", "tokens": ["So", "viel", "er\u00b7laubt", "die", "zeit", ".", "So", "viel", "ist", "mein", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "ADV", "VAFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum schreibe bald, mein Freund! da\u00df dich der himmel liebt.", "tokens": ["Drum", "schrei\u00b7be", "bald", ",", "mein", "Freund", "!", "da\u00df", "dich", "der", "him\u00b7mel", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und hat er dich noch mehr zu lieben angefangen,", "tokens": ["Und", "hat", "er", "dich", "noch", "mehr", "zu", "lie\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So glaub\u2019 auch, da\u00df er mir durch dich vergn\u00fcgung giebt.", "tokens": ["So", "glaub'", "auch", ",", "da\u00df", "er", "mir", "durch", "dich", "ver\u00b7gn\u00fc\u00b7gung", "giebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Noch eins. Die nachtigall l\u00e4st gleich Leandern gr\u00fc\u00dfen,", "tokens": ["Noch", "eins", ".", "Die", "nach\u00b7ti\u00b7gall", "l\u00e4st", "gleich", "Le\u00b7an\u00b7dern", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$.", "ART", "NN", "VVFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und g\u00f6nnet ihm, wie sich, den sitz der sanfften ruh.", "tokens": ["Und", "g\u00f6n\u00b7net", "ihm", ",", "wie", "sich", ",", "den", "sitz", "der", "sanff\u00b7ten", "ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PRF", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jhr beyde, wenn ihr singt, k\u00f6nnt unsern geist vers\u00fc\u00dfen:", "tokens": ["Ihr", "bey\u00b7de", ",", "wenn", "ihr", "singt", ",", "k\u00f6nnt", "un\u00b7sern", "geist", "ver\u00b7s\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dir und der nachtigall h\u00f6r\u2019ich am liebsten zu.", "tokens": ["Dir", "und", "der", "nach\u00b7ti\u00b7gall", "h\u00f6r'\u00b7ich", "am", "liebs\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ADJD", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}