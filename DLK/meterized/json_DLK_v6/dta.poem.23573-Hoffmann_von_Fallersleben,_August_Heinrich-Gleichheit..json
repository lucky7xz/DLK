{"dta.poem.23573": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gleichheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott schuf die Th\u00e4ler, schuf die Berge,", "tokens": ["Gott", "schuf", "die", "Th\u00e4\u00b7ler", ",", "schuf", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott schuf die Riesen, schuf die Zwerge,", "tokens": ["Gott", "schuf", "die", "Rie\u00b7sen", ",", "schuf", "die", "Zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er schuf die Menschen gro\u00df und klein:", "tokens": ["Er", "schuf", "die", "Men\u00b7schen", "gro\u00df", "und", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich soll sich nichts auf Erden sein.", "tokens": ["Gleich", "soll", "sich", "nichts", "auf", "Er\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir wollen Gottes Ordnung halten,", "tokens": ["Wir", "wol\u00b7len", "Got\u00b7tes", "Ord\u00b7nung", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir lassen's also h\u00fcbsch beim Alten;", "tokens": ["Wir", "las\u00b7sen's", "al\u00b7so", "h\u00fcbsch", "beim", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch gleiches Ma\u00df und gleich Gewicht,", "tokens": ["Auch", "glei\u00b7ches", "Ma\u00df", "und", "gleich", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, w\u00e4r's nicht da, ", "tokens": ["Ja", ",", "w\u00e4r's", "nicht", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}}}}}