{"textgrid.poem.24292": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Lied des s\u00e4chsischen Schustergesellen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine liebe Laura", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sagte heit zu mir:", "tokens": ["Sag\u00b7te", "heit", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbkomm doch heite abend", "tokens": ["\u00bb", "komm", "doch", "hei\u00b7te", "a\u00b7bend"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auf \u00e4 Schlickchen Bier;", "tokens": ["Auf", "\u00e4", "Schlick\u00b7chen", "Bier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und \u00e4 Sticke Schwartenmagen", "tokens": ["Und", "\u00e4", "Sti\u00b7cke", "Schwar\u00b7ten\u00b7ma\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirschte auch wohl nich abschlagen.\u00ab", "tokens": ["Wirschte", "auch", "wohl", "nich", "ab\u00b7schla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Meine liebe Laura:", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Das versprech ich dir!", "tokens": ["Das", "ver\u00b7sprech", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meine liebe Laura,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die is scheen gebaut.", "tokens": ["Die", "is", "scheen", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "VVFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mi\u00dft ich nich verreesen,", "tokens": ["Mi\u00dft", "ich", "nich", "ver\u00b7ree\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00e4r se meine Braut.", "tokens": ["W\u00e4r", "se", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jeberallrum is se dicke,", "tokens": ["Je\u00b7be\u00b7ral\u00b7lrum", "is", "se", "di\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "We\u00dfterhole \u00e4 festes Sticke!", "tokens": ["We\u00df\u00b7ter\u00b7ho\u00b7le", "\u00e4", "fes\u00b7tes", "Sti\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Meine liebe Laura,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die is scheen gebaut.", "tokens": ["Die", "is", "scheen", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "VVFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Meine liebe Laura", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Liebt mich mit Gefiehl.", "tokens": ["Liebt", "mich", "mit", "Ge\u00b7fiehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meiner Mutter ihr Junge", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "ihr", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bleibt dann auch nich kiehl.", "tokens": ["Bleibt", "dann", "auch", "nich", "kiehl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Solche M\u00e4\u00e4chen sollen leben,", "tokens": ["Sol\u00b7che", "M\u00e4\u00e4\u00b7chen", "sol\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns was zum besten geben!", "tokens": ["Die", "uns", "was", "zum", "bes\u00b7ten", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRELS", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine liebe Laura", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Liebt mich mit Gefiehl.", "tokens": ["Liebt", "mich", "mit", "Ge\u00b7fiehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Meine liebe Laura,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sei doch nicht so dumm!", "tokens": ["Sei", "doch", "nicht", "so", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dadrauf kannste Gift n\u00e4hm,", "tokens": ["Da\u00b7drauf", "kanns\u00b7te", "Gift", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df ich wiederkumm.", "tokens": ["Da\u00df", "ich", "wie\u00b7der\u00b7kumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schusterpech tut ewig kleben,", "tokens": ["Schus\u00b7ter\u00b7pech", "tut", "e\u00b7wig", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schustertreie gilt fors Leben!", "tokens": ["Schus\u00b7ter\u00b7trei\u00b7e", "gilt", "fors", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine liebe Laura,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Lau\u00b7ra", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sei doch nicht so dumm!", "tokens": ["Sei", "doch", "nicht", "so", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}