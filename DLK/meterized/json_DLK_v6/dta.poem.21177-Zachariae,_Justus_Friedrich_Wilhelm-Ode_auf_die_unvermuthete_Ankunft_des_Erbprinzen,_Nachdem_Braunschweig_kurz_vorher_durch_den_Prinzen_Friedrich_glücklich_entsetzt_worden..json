{"dta.poem.21177": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ode  \n auf die unvermuthete Ankunft  \n des  \n  Erbprinzen,  \n Nachdem  \n  Braunschweig kurz vorher durch den  \n  Prinzen  \n Friedrich  \n gl\u00fccklich entsetzt worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Erbprinz ists! Sein Auge blitzt", "tokens": ["Der", "E\u00b7rbprinz", "ists", "!", "Sein", "Au\u00b7ge", "blitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Heldengeist, der ihn verr\u00e4th.", "tokens": ["Den", "Hel\u00b7den\u00b7geist", ",", "der", "ihn", "ver\u00b7r\u00e4\u00b7th", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00f6rt es, fliegt herzu, und sch\u00fctzt", "tokens": ["Er", "h\u00f6rt", "es", ",", "fliegt", "her\u00b7zu", ",", "und", "sch\u00fctzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Vaterland, das Jhn um H\u00fclfe fleht.", "tokens": ["Sein", "Va\u00b7ter\u00b7land", ",", "das", "Jhn", "um", "H\u00fcl\u00b7fe", "fleht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So eilt der Blitz vom Niedergang", "tokens": ["So", "eilt", "der", "Blitz", "vom", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Aufgang hin, des R\u00e4chers Willen,", "tokens": ["Zum", "Auf\u00b7gang", "hin", ",", "des", "R\u00e4\u00b7chers", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dem der Unschuld Winseln drang,", "tokens": ["Zu", "dem", "der", "Un\u00b7schuld", "Win\u00b7seln", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An den Verbrechern zu erf\u00fcllen.", "tokens": ["An", "den", "Ver\u00b7bre\u00b7chern", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Schon wieherte das stolze Ro\u00df", "tokens": ["Schon", "wie\u00b7her\u00b7te", "das", "stol\u00b7ze", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Des Galliers um uns herum;", "tokens": ["Des", "Gal\u00b7liers", "um", "uns", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Braunschweigs Fluren, \u00f6d und blo\u00df,", "tokens": ["Und", "Braun\u00b7schweigs", "Flu\u00b7ren", ",", "\u00f6d", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeden Hain, vor tiefen Schrecken stumm,", "tokens": ["Und", "je\u00b7den", "Hain", ",", "vor", "tie\u00b7fen", "Schre\u00b7cken", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Umzingelte das freche Heer;", "tokens": ["Um\u00b7zin\u00b7gel\u00b7te", "das", "fre\u00b7che", "Heer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie jauchzten, trunken vor Vergn\u00fcgen,", "tokens": ["Sie", "jauchz\u00b7ten", ",", "trun\u00b7ken", "vor", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sahn im Staub uns schon so sehr,", "tokens": ["Und", "sahn", "im", "Staub", "uns", "schon", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie der Welfen Mauren liegen.", "tokens": ["Als", "wie", "der", "Wel\u00b7fen", "Mau\u00b7ren", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit Feuer, das der Bosheit Hand,", "tokens": ["Mit", "Feu\u00b7er", ",", "das", "der", "Bos\u00b7heit", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Menschen \u00e4hnlich mehr zu seyn,", "tokens": ["Nicht", "Men\u00b7schen", "\u00e4hn\u00b7lich", "mehr", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADJD", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem finstern Tartarus entwandt,", "tokens": ["Dem", "fins\u00b7tern", "Tar\u00b7ta\u00b7rus", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedachten sie, uns unserm Tod zu weihn.", "tokens": ["Ge\u00b7dach\u00b7ten", "sie", ",", "uns", "un\u00b7serm", "Tod", "zu", "weihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schon stand im dunkeln Sturm der Feind", "tokens": ["Schon", "stand", "im", "dun\u00b7keln", "Sturm", "der", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor unsern W\u00e4llen; schon versiegte", "tokens": ["Vor", "un\u00b7sern", "W\u00e4l\u00b7len", ";", "schon", "ver\u00b7sieg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ihm die Fluth; und schnell erscheint,", "tokens": ["Vor", "ihm", "die", "Fluth", ";", "und", "schnell", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da jeder Stral von Hofnung tr\u00fcgte.", "tokens": ["Da", "je\u00b7der", "Stral", "von", "Hof\u00b7nung", "tr\u00fcg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Sieger Friedrich. M\u00e4chtig bricht", "tokens": ["Der", "Sie\u00b7ger", "Fried\u00b7rich", ".", "M\u00e4ch\u00b7tig", "bricht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Phalanx durch; die Schanze trinkt", "tokens": ["Sein", "Pha\u00b7lanx", "durch", ";", "die", "Schan\u00b7ze", "trinkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Feinde Blut; Er k\u00f6mmt, Er ficht;", "tokens": ["Der", "Fein\u00b7de", "Blut", ";", "Er", "k\u00f6mmt", ",", "Er", "ficht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ewge w\u00e4gt; und Frankreichs Schale sinkt.", "tokens": ["Der", "Ew\u00b7ge", "w\u00e4gt", ";", "und", "Fran\u00b7kreichs", "Scha\u00b7le", "sinkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was flieht er so, der stolze Feind,", "tokens": ["Was", "flieht", "er", "so", ",", "der", "stol\u00b7ze", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mit der H\u00f6lle Brand ger\u00fcstet,", "tokens": ["Der", "mit", "der", "H\u00f6l\u00b7le", "Brand", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu unserm Untergang vereint,", "tokens": ["Zu", "un\u00b7serm", "Un\u00b7ter\u00b7gang", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich k\u00fcrzlich noch so hoch gebr\u00fcstet?", "tokens": ["Sich", "k\u00fcrz\u00b7lich", "noch", "so", "hoch", "ge\u00b7br\u00fcs\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er flieht. Vergebens! Jhn ereilt", "tokens": ["Er", "flieht", ".", "Ver\u00b7ge\u00b7bens", "!", "Jhn", "er\u00b7eilt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Carls Erstgebohrner; und sein Schwerdt,", "tokens": ["Carls", "Erst\u00b7ge\u00b7bohr\u00b7ner", ";", "und", "sein", "Schwerdt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nie unth\u00e4tig sich verweilt,", "tokens": ["Das", "nie", "un\u00b7th\u00e4\u00b7tig", "sich", "ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt Rach an ihm, da er den R\u00fccken kehrt.", "tokens": ["Nimmt", "Rach", "an", "ihm", ",", "da", "er", "den", "R\u00fc\u00b7cken", "kehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "O Prinzen, Eure tapfre Hand", "tokens": ["O", "Prin\u00b7zen", ",", "Eu\u00b7re", "tapf\u00b7re", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerbricht die Fesseln! welch Vergn\u00fcgen,", "tokens": ["Zer\u00b7bricht", "die", "Fes\u00b7seln", "!", "welch", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu streiten f\u00fcr das Vaterland,", "tokens": ["Zu", "strei\u00b7ten", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr das Vaterland zu siegen!", "tokens": ["Und", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "zu", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}