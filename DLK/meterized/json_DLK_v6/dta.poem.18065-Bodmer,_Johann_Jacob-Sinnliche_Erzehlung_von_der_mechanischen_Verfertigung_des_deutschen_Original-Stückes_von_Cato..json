{"dta.poem.18065": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Sinnliche Erzehlung  \n von der mechanischen  \n Verfertigung des deutschen  \n Original-St\u00fcckes von Cato.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": ". . . . Kein Mitleid nimmt ihn ein,", "tokens": [".", ".", ".", ".", "Kein", "Mit\u00b7leid", "nimmt", "ihn", "ein", ","], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "$.", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Denn weil er selbst nicht fehlt, so will er nie verzeihn.", "tokens": ["Denn", "weil", "er", "selbst", "nicht", "fehlt", ",", "so", "will", "er", "nie", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}