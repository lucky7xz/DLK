{"textgrid.poem.47654": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit aufgel\u00f6s'ten Locken]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit aufgel\u00f6s'ten Locken", "tokens": ["Mit", "auf\u00b7ge\u00b7l\u00f6s'\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An eure Brust gesunkene,", "tokens": ["An", "eu\u00b7re", "Brust", "ge\u00b7sun\u00b7ke\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit wonnegebrochnen Blicken", "tokens": ["Mit", "won\u00b7ne\u00b7ge\u00b7broch\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Minnerausch ertrunkene,", "tokens": ["Im", "Min\u00b7ne\u00b7rausch", "er\u00b7trun\u00b7ke\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Mit hingeschmolzner Seele", "tokens": ["Mit", "hin\u00b7ge\u00b7schmolz\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ma\u00dflos in euch verlorene", "tokens": ["Ma\u00df\u00b7los", "in", "euch", "ver\u00b7lo\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Holdselige Frauen k\u00fc\u00dft ihr,", "tokens": ["Hold\u00b7se\u00b7li\u00b7ge", "Frau\u00b7en", "k\u00fc\u00dft", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O ihr zum Heil Geborene!", "tokens": ["O", "ihr", "zum", "Heil", "Ge\u00b7bo\u00b7re\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}