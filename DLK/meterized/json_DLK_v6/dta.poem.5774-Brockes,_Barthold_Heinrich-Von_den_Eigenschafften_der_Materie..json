{"dta.poem.5774": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man darauf sein Aug\u2019 aufs Kleine lencket,", "tokens": ["Wenn", "man", "da\u00b7rauf", "sein", "Aug'", "aufs", "Klei\u00b7ne", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf der Kleinheit Gr\u00f6sse dencket;", "tokens": ["Und", "auf", "der", "Klein\u00b7heit", "Gr\u00f6s\u00b7se", "den\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So theilt man sie", "tokens": ["So", "theilt", "man", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auch bis zu ihrem Ende nie.", "tokens": ["Auch", "bis", "zu", "ih\u00b7rem", "En\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist verlieret sich, und alle Krafft vergehet,", "tokens": ["Der", "Geist", "ver\u00b7lie\u00b7ret", "sich", ",", "und", "al\u00b7le", "Krafft", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja der Verstand erstaunt, bewundert, und gestehet:", "tokens": ["Ja", "der", "Ver\u00b7stand", "er\u00b7staunt", ",", "be\u00b7wun\u00b7dert", ",", "und", "ge\u00b7ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVPP", "$,", "VVPP", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Ding\u2019 auf beiden Seiten seyn", "tokens": ["Die", "Ding'", "auf", "bei\u00b7den", "Sei\u00b7ten", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unendlich gro\u00df, unendlich klein.", "tokens": ["Un\u00b7end\u00b7lich", "gro\u00df", ",", "un\u00b7end\u00b7lich", "klein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}