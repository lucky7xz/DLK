{"dta.poem.3988": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Erbrich, o Kind! den Brief, so meine Feder", "tokens": ["Er\u00b7brich", ",", "o", "Kind", "!", "den", "Brief", ",", "so", "mei\u00b7ne", "Fe\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "FM", "NN", "$.", "ART", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schreibet,", "tokens": ["schrei\u00b7bet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die da vor diesesmahl, statt Dinte, Thr\u00e4nen", "tokens": ["Die", "da", "vor", "die\u00b7ses\u00b7mahl", ",", "statt", "Din\u00b7te", ",", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "KOUI", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "f\u00fchrt,", "tokens": ["f\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Weil mich des Schicksals Macht aus diesen Fluh-", "tokens": ["Weil", "mich", "des", "Schick\u00b7sals", "Macht", "aus", "die\u00b7sen", "Fluh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ren treibet,", "tokens": ["ren", "trei\u00b7bet", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So wei\u00df ich allzuwohl, wie viel mein Hertz verliert.", "tokens": ["So", "wei\u00df", "ich", "all\u00b7zu\u00b7wohl", ",", "wie", "viel", "mein", "Hertz", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}