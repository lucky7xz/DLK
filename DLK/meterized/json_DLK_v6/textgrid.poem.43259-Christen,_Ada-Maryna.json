{"textgrid.poem.43259": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Maryna", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit Du gestorben, bin ich recht allein.", "tokens": ["Seit", "Du", "ge\u00b7stor\u00b7ben", ",", "bin", "ich", "recht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich tr\u00e4ume oft, es m\u00fcsse anders sein,", "tokens": ["Ich", "tr\u00e4u\u00b7me", "oft", ",", "es", "m\u00fcs\u00b7se", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann sag' ich mir: Sie ist nur fortgegangen", "tokens": ["Dann", "sag'", "ich", "mir", ":", "Sie", "ist", "nur", "fort\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kehret wieder, denn sie ahnt mein Leid.", "tokens": ["Und", "keh\u00b7ret", "wie\u00b7der", ",", "denn", "sie", "ahnt", "mein", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann kommst Du lachend wie in alter Zeit", "tokens": ["Dann", "kommst", "Du", "la\u00b7chend", "wie", "in", "al\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und streichelst hastig-redend meine Wangen.", "tokens": ["Und", "strei\u00b7chelst", "has\u00b7tig\u00b7re\u00b7dend", "mei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ich erwache ... will Dich wiedersehn,", "tokens": ["Und", "ich", "er\u00b7wa\u00b7che", "...", "will", "Dich", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will Dich in einem Winkel noch ersp\u00e4hn,", "tokens": ["Will", "Dich", "in", "ei\u00b7nem", "Win\u00b7kel", "noch", "er\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich suche wie die Mutter nach dem Kinde!", "tokens": ["Ich", "su\u00b7che", "wie", "die", "Mut\u00b7ter", "nach", "dem", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch pl\u00f6tzlich f\u00e4llt mich der Gedanke an:", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "f\u00e4llt", "mich", "der", "Ge\u00b7dan\u00b7ke", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich die Welt zu Ende laufen kann", "tokens": ["Da\u00df", "ich", "die", "Welt", "zu", "En\u00b7de", "lau\u00b7fen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und nirgend ... nirgend ... nirgend ... ", "tokens": ["Und", "nir\u00b7gend", "...", "nir\u00b7gend", "...", "nir\u00b7gend", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "$(", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}