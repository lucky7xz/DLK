{"textgrid.poem.35042": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Sternschnuppe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fiel ein Stern, habt ihr gedacht,", "tokens": ["Es", "fiel", "ein", "Stern", ",", "habt", "ihr", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus weiten, unbekannten Fernen.", "tokens": ["Aus", "wei\u00b7ten", ",", "un\u00b7be\u00b7kann\u00b7ten", "Fer\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ging unter er in dunkle Nacht?", "tokens": ["Ging", "un\u00b7ter", "er", "in", "dunk\u00b7le", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb er am Himmel bei den Sternen?", "tokens": ["Blieb", "er", "am", "Him\u00b7mel", "bei", "den", "Ster\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ists eine Welt, die im Entstehn", "tokens": ["Ists", "ei\u00b7ne", "Welt", ",", "die", "im", "Ent\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Kraft und Stoff zu holen strebte?", "tokens": ["Sich", "Kraft", "und", "Stoff", "zu", "ho\u00b7len", "streb\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wars eine Welt, die im Vergehn", "tokens": ["Wars", "ei\u00b7ne", "Welt", ",", "die", "im", "Ver\u00b7gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Leuchten sich zu Ende lebte?", "tokens": ["Durchs", "Leuch\u00b7ten", "sich", "zu", "En\u00b7de", "leb\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das werdet ihr vielleicht, vielleicht", "tokens": ["Das", "wer\u00b7det", "ihr", "viel\u00b7leicht", ",", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eure Rohre noch ergr\u00fcnden,", "tokens": ["Durch", "eu\u00b7re", "Roh\u00b7re", "noch", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch wer ihren Weg ihr zeigt,", "tokens": ["Je\u00b7doch", "wer", "ih\u00b7ren", "Weg", "ihr", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann nur der Glaube euch verk\u00fcnden.", "tokens": ["Kann", "nur", "der", "Glau\u00b7be", "euch", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}