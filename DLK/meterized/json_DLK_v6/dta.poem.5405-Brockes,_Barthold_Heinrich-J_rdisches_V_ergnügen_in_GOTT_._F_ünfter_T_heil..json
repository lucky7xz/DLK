{"dta.poem.5405": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen nun, worauf sich roth und wei\u00df", "tokens": ["Die", "Blu\u00b7men", "nun", ",", "wo\u00b7rauf", "sich", "roth", "und", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PRF", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ungemein- und s\u00fcssem Grad vermischen,", "tokens": ["In", "un\u00b7ge\u00b7mein", "und", "s\u00fcs\u00b7sem", "Grad", "ver\u00b7mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stehn an sehr zierlichen und sch\u00f6n gef\u00e4rbten B\u00fcschen.", "tokens": ["Stehn", "an", "sehr", "zier\u00b7li\u00b7chen", "und", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7ten", "B\u00fc\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADJA", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sch\u00f6ne gr\u00fcne Blatt nimmt fast den Preis", "tokens": ["Das", "sch\u00f6\u00b7ne", "gr\u00fc\u00b7ne", "Blatt", "nimmt", "fast", "den", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den audern Bl\u00e4ttern weg, da es so nett formiret,", "tokens": ["Den", "au\u00b7dern", "Bl\u00e4t\u00b7tern", "weg", ",", "da", "es", "so", "nett", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So zierlich eingekerbt", "tokens": ["So", "zier\u00b7lich", "ein\u00b7ge\u00b7kerbt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mit dem sch\u00f6nsten Gr\u00fcn gef\u00e4rbt.", "tokens": ["Und", "mit", "dem", "sch\u00f6ns\u00b7ten", "Gr\u00fcn", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch dieses sch\u00f6ne Gr\u00fcn nun gl\u00e4ntzt der Blumen Pracht", "tokens": ["Durch", "die\u00b7ses", "sch\u00f6\u00b7ne", "Gr\u00fcn", "nun", "gl\u00e4ntzt", "der", "Blu\u00b7men", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einer gr\u00fcnen Schatten-Nacht", "tokens": ["In", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Schat\u00b7ten\u00b7Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Noch desto lieblicher, da wir recht wundersch\u00f6n", "tokens": ["Noch", "des\u00b7to", "lieb\u00b7li\u00b7cher", ",", "da", "wir", "recht", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier wei\u00df, dort roth, durchs Gr\u00fcn erhoben,", "tokens": ["Hier", "wei\u00df", ",", "dort", "roth", ",", "durchs", "Gr\u00fcn", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dort gr\u00fcn, dort roth und wei\u00df, recht als bestrahlet, sehn.", "tokens": ["Dort", "gr\u00fcn", ",", "dort", "roth", "und", "wei\u00df", ",", "recht", "als", "be\u00b7strah\u00b7let", ",", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,", "ADJD", "KOKOM", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach, s\u00e4he man,", "tokens": ["Ach", ",", "s\u00e4\u00b7he", "man", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ohn den, der sie gemacht, zu loben,", "tokens": ["Ohn", "den", ",", "der", "sie", "ge\u00b7macht", ",", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch diese Blumen nimmer an!", "tokens": ["Doch", "die\u00b7se", "Blu\u00b7men", "nim\u00b7mer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}