{"dta.poem.8418": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Traumbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und auf der \u00f6den Wildniss,", "tokens": ["Und", "auf", "der", "\u00f6\u00b7den", "Wild\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Tannenb\u00e4ume D\u00e4mmrung streun,", "tokens": ["Wo", "Tan\u00b7nen\u00b7b\u00e4u\u00b7me", "D\u00e4mm\u00b7rung", "streun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umflattert mich das Bildniss.", "tokens": ["Um\u00b7flat\u00b7tert", "mich", "das", "Bild\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es tanzt aus jedem Busch hervor,", "tokens": ["Es", "tanzt", "aus", "je\u00b7dem", "Busch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Maienl\u00e4mmlein grasen,", "tokens": ["Wo", "Mai\u00b7en\u00b7l\u00e4mm\u00b7lein", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wallt, verh\u00fcllt in leichten Flor,", "tokens": ["Und", "wallt", ",", "ver\u00b7h\u00fcllt", "in", "leich\u00b7ten", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf jedem gr\u00fcnen Rasen.", "tokens": ["Auf", "je\u00b7dem", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann mich, mit meinem Gram vertraut,", "tokens": ["Wann", "mich", ",", "mit", "mei\u00b7nem", "Gram", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Stunde der Gespenster,", "tokens": ["Zur", "Stun\u00b7de", "der", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der liebe helle Mond beschaut,", "tokens": ["Der", "lie\u00b7be", "hel\u00b7le", "Mond", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bebts durch mein Kammerfenster,", "tokens": ["Bebts", "durch", "mein", "Kam\u00b7mer\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und malt sich an die weisse Wand,", "tokens": ["Und", "malt", "sich", "an", "die", "weis\u00b7se", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwebt vor meinen Blicken,", "tokens": ["Und", "schwebt", "vor", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und winkt mir mit der kleinen Hand,", "tokens": ["Und", "winkt", "mir", "mit", "der", "klei\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4chelt mir Entz\u00fccken.", "tokens": ["Und", "l\u00e4\u00b7chelt", "mir", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein guter Engel, sage mir,", "tokens": ["Mein", "gu\u00b7ter", "En\u00b7gel", ",", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Luna sie beflimmert,", "tokens": ["Wo", "Lu\u00b7na", "sie", "be\u00b7flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo, von ihr ber\u00fchrt, von ihr!", "tokens": ["Und", "wo", ",", "von", "ihr", "be\u00b7r\u00fchrt", ",", "von", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "APPR", "PPOSAT", "VVPP", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blume r\u00f6ther schimmert.", "tokens": ["Die", "Blu\u00b7me", "r\u00f6\u00b7ther", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erschaff' ihr Bild aus Morgenlicht,", "tokens": ["Er\u00b7schaff'", "ihr", "Bild", "aus", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Kleid aus Aetherbl\u00e4ue,", "tokens": ["Ihr", "Kleid", "aus", "A\u00b7e\u00b7ther\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und zeig' in jedem Nachtgesicht", "tokens": ["Und", "zeig'", "in", "je\u00b7dem", "Nacht\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir meine Vielgetreue.", "tokens": ["Mir", "mei\u00b7ne", "Viel\u00b7ge\u00b7treu\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo pfl\u00fcckt sie, wann der Lenz beginnt,", "tokens": ["Wo", "pfl\u00fcckt", "sie", ",", "wann", "der", "Lenz", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ersten Maienklocken?", "tokens": ["Die", "ers\u00b7ten", "Mai\u00b7en\u00b7klo\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo spielst du, lieber Abendwind,", "tokens": ["Wo", "spielst", "du", ",", "lie\u00b7ber", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren blonden Locken?", "tokens": ["Mit", "ih\u00b7ren", "blon\u00b7den", "Lo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O eilt, o flattert weg von ihr,", "tokens": ["O", "eilt", ",", "o", "flat\u00b7tert", "weg", "von", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebte Maienwinde,", "tokens": ["Ge\u00b7lieb\u00b7te", "Mai\u00b7en\u00b7win\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sagt es mir, und sagt es mir,", "tokens": ["Und", "sagt", "es", "mir", ",", "und", "sagt", "es", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo ich das M\u00e4dchen finde!", "tokens": ["Wo", "ich", "das", "M\u00e4d\u00b7chen", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}