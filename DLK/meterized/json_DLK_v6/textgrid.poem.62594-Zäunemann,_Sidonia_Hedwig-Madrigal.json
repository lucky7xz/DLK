{"textgrid.poem.62594": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wahrer Christ, der nach dem ewgen strebt,", "tokens": ["Ein", "wah\u00b7rer", "Christ", ",", "der", "nach", "dem", "ew\u00b7gen", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob er gleich auf der Erde lebt;", "tokens": ["Ob", "er", "gleich", "auf", "der", "Er\u00b7de", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ist doch mit seinem Geist im Himmel:", "tokens": ["Ist", "doch", "mit", "sei\u00b7nem", "Geist", "im", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichwie die Kugel, wenn sie l\u00e4uft,", "tokens": ["Gleich\u00b7wie", "die", "Ku\u00b7gel", ",", "wenn", "sie", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am minsten auf der Erde streist.", "tokens": ["Am", "mins\u00b7ten", "auf", "der", "Er\u00b7de", "streist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie man ein lieblichs Saytenspiel im Lerm,", "tokens": ["Wie", "man", "ein", "lieb\u00b7lichs", "Say\u00b7ten\u00b7spiel", "im", "Lerm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Get\u00f6\u00df und Schwarm nicht h\u00f6ret;", "tokens": ["Ge\u00b7t\u00f6\u00df", "und", "Schwarm", "nicht", "h\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ist es mit dem Menschen auch, wenn er das Weltget\u00fcmmel ehret,", "tokens": ["So", "ist", "es", "mit", "dem", "Men\u00b7schen", "auch", ",", "wenn", "er", "das", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und sucht ihm Herz und Ohr zu leihen; so kan er nicht den heilgen Geist", "tokens": ["Und", "sucht", "ihm", "Herz", "und", "Ohr", "zu", "lei\u00b7hen", ";", "so", "kan", "er", "nicht", "den", "heil\u00b7gen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Und seine sanfte Stimme h\u00f6ren, die ihn den Weg nach Salem wei\u00dft.", "tokens": ["Und", "sei\u00b7ne", "sanf\u00b7te", "Stim\u00b7me", "h\u00f6\u00b7ren", ",", "die", "ihn", "den", "Weg", "nach", "Sa\u00b7lem", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "PRELS", "PPER", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}