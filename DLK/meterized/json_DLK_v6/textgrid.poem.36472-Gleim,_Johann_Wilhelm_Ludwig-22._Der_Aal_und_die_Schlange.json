{"textgrid.poem.36472": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "22. Der Aal und die Schlange", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sag' er mir, Herr Bruder, doch einmal,", "tokens": ["So", "sag'", "er", "mir", ",", "Herr", "Bru\u00b7der", ",", "doch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sprach eine Schlange zu dem Aal,", "tokens": ["Sprach", "ei\u00b7ne", "Schlan\u00b7ge", "zu", "dem", "Aal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie seine Schwester ihm gef\u00e4llt?", "tokens": ["Wie", "sei\u00b7ne", "Schwes\u00b7ter", "ihm", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist auf der ganzen weiten Welt", "tokens": ["Ist", "auf", "der", "gan\u00b7zen", "wei\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was sch\u00f6ners? Ist so sch\u00f6n,", "tokens": ["Was", "sch\u00f6\u00b7ners", "?", "Ist", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So glatt, so bunt, noch eine Haut zu sehn?", "tokens": ["So", "glatt", ",", "so", "bunt", ",", "noch", "ei\u00b7ne", "Haut", "zu", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist, antwortete der Aal,", "tokens": ["Sch\u00f6n", "ist", ",", "ant\u00b7wor\u00b7te\u00b7te", "der", "Aal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die deinige, die meinige nur glatt!", "tokens": ["Die", "dei\u00b7ni\u00b7ge", ",", "die", "mei\u00b7ni\u00b7ge", "nur", "glatt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "$,", "PRELS", "PPOSAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie aber kommts, das sag einmal,", "tokens": ["Wie", "a\u00b7ber", "kommts", ",", "das", "sag", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man mich lieber hat", "tokens": ["Da\u00df", "man", "mich", "lie\u00b7ber", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und lieber sieht als dich? Jedweder, der dich sieht,", "tokens": ["Und", "lie\u00b7ber", "sieht", "als", "dich", "?", "Jed\u00b7we\u00b7der", ",", "der", "dich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KOUS", "PPER", "$.", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat Furcht und Schrecken im Gesicht,", "tokens": ["Hat", "Furcht", "und", "Schre\u00b7cken", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ruft Hilf' und flieht!", "tokens": ["Ruft", "Hil\u00b7f'", "und", "flieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Er flieht? warum? das wei\u00df ich nicht!", "tokens": ["Er", "flieht", "?", "wa\u00b7rum", "?", "das", "wei\u00df", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich aber wei\u00df es, spricht der Aal,", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", "es", ",", "spricht", "der", "Aal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch wissen's ja die Menschen alle,", "tokens": ["Auch", "wis\u00b7sen's", "ja", "die", "Men\u00b7schen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die dich im Grase liegen sehn;", "tokens": ["Die", "dich", "im", "Gra\u00b7se", "lie\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von au\u00dfen bist du sch\u00f6n \u2013", "tokens": ["Von", "au\u00b7\u00dfen", "bist", "du", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von innen Gift und Galle!", "tokens": ["Von", "in\u00b7nen", "Gift", "und", "Gal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}