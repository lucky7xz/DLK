{"textgrid.poem.62447": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1. Lied der Maurer", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit drei gewaltigen Schl\u00e4gen", "tokens": ["Mit", "drei", "ge\u00b7wal\u00b7ti\u00b7gen", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rief uns der Meister auf:", "tokens": ["Rief", "uns", "der", "Meis\u00b7ter", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herbei von allen Wegen,", "tokens": ["Her\u00b7bei", "von", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesellen kommt zu Hauf!", "tokens": ["Ge\u00b7sel\u00b7len", "kommt", "zu", "Hauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was hilft uns alles bauen?", "tokens": ["Was", "hilft", "uns", "al\u00b7les", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir bau'n am Sitz der Noth.", "tokens": ["Wir", "bau'n", "am", "Sitz", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Steine zu behauen", "tokens": ["Was", "Stei\u00b7ne", "zu", "be\u00b7hau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Winkelma\u00df und Loth?", "tokens": ["Nach", "Win\u00b7kel\u00b7ma\u00df", "und", "Loth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Roste wird gefressen", "tokens": ["Vom", "Ros\u00b7te", "wird", "ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mauer wie der Muth,", "tokens": ["Die", "Mau\u00b7er", "wie", "der", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von R\u00e4ubern wird besessen", "tokens": ["Von", "R\u00e4u\u00b7bern", "wird", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der B\u00fcrger Haus und Gut.", "tokens": ["Der", "B\u00fcr\u00b7ger", "Haus", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie prangt im K\u00f6nigsgarten", "tokens": ["Wie", "prangt", "im", "K\u00f6\u00b7nigs\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Bau von Meisterhand!", "tokens": ["Der", "Bau", "von", "Meis\u00b7ter\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man konnte kaum erwarten,", "tokens": ["Man", "konn\u00b7te", "kaum", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis er vollendet stand.", "tokens": ["Bis", "er", "voll\u00b7en\u00b7det", "stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun magst du einsam stehen,", "tokens": ["Nun", "magst", "du", "ein\u00b7sam", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O sch\u00f6nes Schauspielhaus \u2013", "tokens": ["O", "sch\u00f6\u00b7nes", "Schau\u00b7spiel\u00b7haus", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Schauspiel, das wir sehen,", "tokens": ["Das", "Schau\u00b7spiel", ",", "das", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treibt alle Lust hinaus.", "tokens": ["Treibt", "al\u00b7le", "Lust", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun aus den Aschenhaufen", "tokens": ["Nun", "aus", "den", "A\u00b7schen\u00b7hau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Vorstadt schnell erstand.", "tokens": ["Die", "Vor\u00b7stadt", "schnell", "er\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spricht man von Feuertaufen,", "tokens": ["Spricht", "man", "von", "Feu\u00b7er\u00b7tau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von Moskau's Wunderbrand.", "tokens": ["Von", "Mos\u00b7kau's", "Wun\u00b7der\u00b7brand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es stehn mit leeren R\u00e4umen", "tokens": ["Es", "stehn", "mit", "lee\u00b7ren", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Speicher unbesucht,", "tokens": ["Die", "Spei\u00b7cher", "un\u00b7be\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die freien Schiffer s\u00e4umen,", "tokens": ["Die", "frei\u00b7en", "Schif\u00b7fer", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Feind verdarb die Frucht.", "tokens": ["Der", "Feind", "ver\u00b7darb", "die", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als an des Schlosses Thoren", "tokens": ["Als", "an", "des", "Schlos\u00b7ses", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir j\u00fcngst den Bau gef\u00fcgt,", "tokens": ["Wir", "j\u00fcngst", "den", "Bau", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da klang in unsern Ohren", "tokens": ["Da", "klang", "in", "un\u00b7sern", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Ton, der schwerlich tr\u00fcgt.", "tokens": ["Ein", "Ton", ",", "der", "schwer\u00b7lich", "tr\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er sprach von ew'gen Rechten,", "tokens": ["Er", "sprach", "von", "ew'\u00b7gen", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom h\u00f6chsten Eigenthum,", "tokens": ["Vom", "h\u00f6chs\u00b7ten", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie man soll verfechten", "tokens": ["Und", "wie", "man", "soll", "ver\u00b7fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00e4ter Schatz und Ruhm.", "tokens": ["Der", "V\u00e4\u00b7ter", "Schatz", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Rittergeister haben", "tokens": ["Die", "Rit\u00b7ter\u00b7geis\u00b7ter", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich herrlich eingeweiht", "tokens": ["Dich", "herr\u00b7lich", "ein\u00b7ge\u00b7weiht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit wunderbaren Gaben,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7ren", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Haus der Gerechtigkeit.", "tokens": ["Haus", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Da sahen wir dich stehen,", "tokens": ["Da", "sa\u00b7hen", "wir", "dich", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du starker kluger Rath,", "tokens": ["Du", "star\u00b7ker", "klu\u00b7ger", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man konnte deutlich sehen,", "tokens": ["Man", "konn\u00b7te", "deut\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sannst auf k\u00fchne That.", "tokens": ["Du", "sannst", "auf", "k\u00fch\u00b7ne", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Willkommen Rath und Held,", "tokens": ["Will\u00b7kom\u00b7men", "Rath", "und", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Waffen sind genommen,", "tokens": ["Die", "Waf\u00b7fen", "sind", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zeuch' mit ins blut'ge Feld.", "tokens": ["Zeuch'", "mit", "ins", "blut'\u00b7ge", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Zeuch' mit zum deutschen Rheine,", "tokens": ["Zeuch'", "mit", "zum", "deut\u00b7schen", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm nach Westphalens Gau'n,", "tokens": ["Komm", "nach", "West\u00b7pha\u00b7lens", "Gau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da sind viel rauhe Steine,", "tokens": ["Da", "sind", "viel", "rau\u00b7he", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel glatte zu behau'n.", "tokens": ["Viel", "glat\u00b7te", "zu", "be\u00b7hau'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da wird aus Blut und Schmerzen", "tokens": ["Da", "wird", "aus", "Blut", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das rechte Heil erst kund;", "tokens": ["Das", "rech\u00b7te", "Heil", "erst", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gefall'ner Helden Herzen", "tokens": ["Ge\u00b7fall'\u00b7ner", "Hel\u00b7den", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind wol ein fester Grund.", "tokens": ["Sind", "wol", "ein", "fes\u00b7ter", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da wollen wir begr\u00fcnden", "tokens": ["Da", "wol\u00b7len", "wir", "be\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das deutsche Freiheitshaus,", "tokens": ["Das", "deut\u00b7sche", "Frei\u00b7heits\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00f6rtel soll es binden,", "tokens": ["Ein", "M\u00f6r\u00b7tel", "soll", "es", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der tausend Jahr' h\u00e4lt aus.", "tokens": ["Der", "tau\u00b7send", "Jahr'", "h\u00e4lt", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Der M\u00f6rtel hei\u00dfet Wille,", "tokens": ["Der", "M\u00f6r\u00b7tel", "hei\u00b7\u00dfet", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hei\u00dft Treu und hei\u00dfet Muth,", "tokens": ["Hei\u00dft", "Treu", "und", "hei\u00b7\u00dfet", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gereift in heil'ger Stille,", "tokens": ["Ge\u00b7reift", "in", "heil'\u00b7ger", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Benetzt mit Heldenblut.", "tokens": ["Be\u00b7netzt", "mit", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Der M\u00f6rtel sind wir Alle,", "tokens": ["Der", "M\u00f6r\u00b7tel", "sind", "wir", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns w\u00e4hlte Gottes Gunst;", "tokens": ["Uns", "w\u00e4hl\u00b7te", "Got\u00b7tes", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gr\u00fc\u00dft im heil'gen Schalle", "tokens": ["So", "gr\u00fc\u00dft", "im", "heil'\u00b7gen", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Maurer freie Kunst!", "tokens": ["Der", "Mau\u00b7rer", "frei\u00b7e", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}