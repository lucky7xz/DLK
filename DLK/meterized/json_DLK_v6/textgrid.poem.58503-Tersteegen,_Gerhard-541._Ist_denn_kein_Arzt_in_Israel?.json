{"textgrid.poem.58503": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "541. Ist denn kein Arzt in Israel?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sorgst und denkst", "tokens": ["Du", "sorgst", "und", "denkst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und dich nur kr\u00e4nkst;", "tokens": ["Und", "dich", "nur", "kr\u00e4nkst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gott ist dir ja", "tokens": ["Gott", "ist", "dir", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So innig nah,", "tokens": ["So", "in\u00b7nig", "nah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mehr liebt er dich", "tokens": ["Mehr", "liebt", "er", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Als du und ich.", "tokens": ["Als", "du", "und", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Kehr kindlich ein,", "tokens": ["Kehr", "kind\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er wartet dein,", "tokens": ["Er", "war\u00b7tet", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ihm heimlich leb", "tokens": ["Ihm", "heim\u00b7lich", "leb"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und ganz dich geb!", "tokens": ["Und", "ganz", "dich", "geb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wenn er dich hat,", "tokens": ["Wenn", "er", "dich", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Wird seine Gnad',", "tokens": ["Wird", "sei\u00b7ne", "Gnad'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Sein zarter Friede dich regieren,", "tokens": ["Sein", "zar\u00b7ter", "Frie\u00b7de", "dich", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Halt ohne Kummer still, er selbst will dich formieren!", "tokens": ["Halt", "oh\u00b7ne", "Kum\u00b7mer", "still", ",", "er", "selbst", "will", "dich", "for\u00b7mie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$,", "PPER", "ADV", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}