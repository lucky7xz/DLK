{"textgrid.poem.63110": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00d6fter hast du mich gescholten,", "tokens": ["\u00d6f\u00b7ter", "hast", "du", "mich", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaubtest meinen Pfad verwunden,", "tokens": ["Glaub\u00b7test", "mei\u00b7nen", "Pfad", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast das Ende nicht gefunden,", "tokens": ["Hast", "das", "En\u00b7de", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem mein wilder Lauf gegolten.", "tokens": ["Dem", "mein", "wil\u00b7der", "Lauf", "ge\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber hoben deine H\u00e4nde", "tokens": ["A\u00b7ber", "ho\u00b7ben", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in meine, quollen Tr\u00e4nen.", "tokens": ["Sich", "in", "mei\u00b7ne", ",", "quol\u00b7len", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00df aus mutterhei\u00dfem Sehnen", "tokens": ["Hei\u00df", "aus", "mut\u00b7ter\u00b7hei\u00b7\u00dfem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchten Rosen ohne Ende.", "tokens": ["Bl\u00fch\u00b7ten", "Ro\u00b7sen", "oh\u00b7ne", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}