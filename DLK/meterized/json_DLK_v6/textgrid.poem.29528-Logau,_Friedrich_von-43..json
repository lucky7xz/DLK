{"textgrid.poem.29528": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer dran, wer Lust zu freyen! Juno hat gleich auffgedeckt", "tokens": ["Im\u00b7mer", "dran", ",", "wer", "Lust", "zu", "frey\u00b7en", "!", "Ju\u00b7no", "hat", "gleich", "auff\u00b7ge\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "$,", "PWS", "NN", "PTKZU", "VVINF", "$.", "NE", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Das so weisse Braut-Gebette, da\u00df sich, wer nur wil, drein streckt.", "tokens": ["Das", "so", "weis\u00b7se", "Braut\u00b7Ge\u00b7bet\u00b7te", ",", "da\u00df", "sich", ",", "wer", "nur", "wil", ",", "drein", "streckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,", "KOUS", "PRF", "$,", "PWS", "ADV", "VMFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Zwar niemanden wil ich globen, da\u00df er werde schlafen wol,", "tokens": ["Zwar", "nie\u00b7man\u00b7den", "wil", "ich", "glo\u00b7ben", ",", "da\u00df", "er", "wer\u00b7de", "schla\u00b7fen", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Doch dem Br\u00e4utigam wils geb\u00fchren, da\u00df er wacker wachen soll.", "tokens": ["Doch", "dem", "Br\u00e4u\u00b7ti\u00b7gam", "wils", "ge\u00b7b\u00fch\u00b7ren", ",", "da\u00df", "er", "wa\u00b7cker", "wa\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}