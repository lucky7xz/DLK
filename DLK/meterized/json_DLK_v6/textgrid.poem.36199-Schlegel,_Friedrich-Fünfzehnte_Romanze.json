{"textgrid.poem.36199": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnfzehnte Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Toten nun bestattet", "tokens": ["Als", "die", "To\u00b7ten", "nun", "be\u00b7stat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nach dem Gebrauch der Christen,", "tokens": ["Sind", "nach", "dem", "Ge\u00b7brauch", "der", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Gruft hinabgesenket,", "tokens": ["In", "die", "Gruft", "hin\u00b7ab\u00b7ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Gebet und sch\u00f6nen Liedern,", "tokens": ["Mit", "Ge\u00b7bet", "und", "sch\u00f6\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu der frommen K\u00e4mpfer Ruhe", "tokens": ["Zu", "der", "from\u00b7men", "K\u00e4mp\u00b7fer", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manches Grab kunstreich gezieret;", "tokens": ["Man\u00b7ches", "Grab", "kuns\u00b7treich", "ge\u00b7zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kaiser Karol mit dem Heere", "tokens": ["Kai\u00b7ser", "Ka\u00b7rol", "mit", "dem", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Heimw\u00e4rts nach Paris hinziehet.", "tokens": ["Heim\u00b7w\u00e4rts", "nach", "Pa\u00b7ris", "hin\u00b7zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Heil'ge M\u00e4nner und Bisch\u00f6fe", "tokens": ["Heil'\u00b7ge", "M\u00e4n\u00b7ner", "und", "Bi\u00b7sch\u00f6\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat er dorten hinbeschieden,", "tokens": ["Hat", "er", "dor\u00b7ten", "hin\u00b7be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nach des Dionysii M\u00fcnster,", "tokens": ["Nach", "des", "Di\u00b7o\u00b7ny\u00b7sii", "M\u00fcns\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kund zu machen seinen Willen.", "tokens": ["Kund", "zu", "ma\u00b7chen", "sei\u00b7nen", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gotte dankend, der ihn schirmte,", "tokens": ["Got\u00b7te", "dan\u00b7kend", ",", "der", "ihn", "schirm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gn\u00e4dig oft ihm half zu siegen,", "tokens": ["Gn\u00e4\u00b7dig", "oft", "ihm", "half", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dann auch betend f\u00fcr die Seelen,", "tokens": ["Dann", "auch", "be\u00b7tend", "f\u00fcr", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die in Roncisvall geblieben,", "tokens": ["Die", "in", "Ron\u00b7cis\u00b7vall", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und der andren M\u00e4rtrer alle", "tokens": ["Und", "der", "an\u00b7dren", "M\u00e4r\u00b7trer", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "In den spanischen Gefilden,", "tokens": ["In", "den", "spa\u00b7ni\u00b7schen", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Gibt und schenkt f\u00fcr ew'ge Zeiten", "tokens": ["Gibt", "und", "schenkt", "f\u00fcr", "ew'\u00b7ge", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Reiche Gaben er der Kirche,", "tokens": ["Rei\u00b7che", "Ga\u00b7ben", "er", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die dem Sankt Denis gestiftet,", "tokens": ["Die", "dem", "Sankt", "De\u00b7nis", "ge\u00b7stif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hohes Gut und Gold und Silber,", "tokens": ["Ho\u00b7hes", "Gut", "und", "Gold", "und", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Land und Leute, viele Rechte,", "tokens": ["Land", "und", "Leu\u00b7te", ",", "vie\u00b7le", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da\u00df der Heil'ge k\u00fcnftig schirmen", "tokens": ["Da\u00df", "der", "Heil'\u00b7ge", "k\u00fcnf\u00b7tig", "schir\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wolle bis auf ew'ge Zeiten,", "tokens": ["Wol\u00b7le", "bis", "auf", "ew'\u00b7ge", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Alle, die dereinstens sitzen", "tokens": ["Al\u00b7le", ",", "die", "de\u00b7reins\u00b7tens", "sit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Werden auf dem goldnen Stuhle", "tokens": ["Wer\u00b7den", "auf", "dem", "gold\u00b7nen", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Dieses Reiches, da\u00df im Kriege", "tokens": ["Die\u00b7ses", "Rei\u00b7ches", ",", "da\u00df", "im", "Krie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Frankreich stets beschirm' und sch\u00fctze", "tokens": ["Fran\u00b7kreich", "stets", "be\u00b7schirm'", "und", "sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "KON", "VVFIN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.30": {"text": "So der Heil'ge, wie im Frieden;", "tokens": ["So", "der", "Heil'\u00b7ge", ",", "wie", "im", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Tritt dann zu Denisens Leiche,", "tokens": ["Tritt", "dann", "zu", "De\u00b7ni\u00b7sens", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Sein Gebet gen Himmel richtend,", "tokens": ["Sein", "Ge\u00b7bet", "gen", "Him\u00b7mel", "rich\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "An dem offnen Sarge kniend,", "tokens": ["An", "dem", "off\u00b7nen", "Sar\u00b7ge", "kni\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.34": {"text": "Da\u00df der Heil'ge wolle bitten", "tokens": ["Da\u00df", "der", "Heil'\u00b7ge", "wol\u00b7le", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "F\u00fcr die teuern Kriegsgenossen,", "tokens": ["F\u00fcr", "die", "teu\u00b7ern", "Kriegs\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Die den M\u00e4rtertod erlitten,", "tokens": ["Die", "den", "M\u00e4r\u00b7ter\u00b7tod", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Da\u00df, der Schulden losgesprochen,", "tokens": ["Da\u00df", ",", "der", "Schul\u00b7den", "los\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ihre Seele ruh' in Frieden.", "tokens": ["Ih\u00b7re", "See\u00b7le", "ruh'", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "In der Nacht nach diesem Tage", "tokens": ["In", "der", "Nacht", "nach", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Ist Sankt Dionys erschienen,", "tokens": ["Ist", "Sankt", "Di\u00b7o\u00b7nys", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Kaiser Karlen angelobend,", "tokens": ["Kai\u00b7ser", "Kar\u00b7len", "an\u00b7ge\u00b7lo\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df auf sein Vorwort und Bitte", "tokens": ["Da\u00df", "auf", "sein", "Vor\u00b7wort", "und", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Aller Schulden sind entledigt", "tokens": ["Al\u00b7ler", "Schul\u00b7den", "sind", "ent\u00b7le\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Jene frommen Glaubensritter,", "tokens": ["Je\u00b7ne", "from\u00b7men", "Glau\u00b7bens\u00b7rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Die f\u00fcr Gottes Ehre streitend,", "tokens": ["Die", "f\u00fcr", "Got\u00b7tes", "Eh\u00b7re", "strei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "In dem Heidenkriege fielen.", "tokens": ["In", "dem", "Hei\u00b7den\u00b7krie\u00b7ge", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Auch f\u00fcr jene, welche willig", "tokens": ["Auch", "f\u00fcr", "je\u00b7ne", ",", "wel\u00b7che", "wil\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PDS", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Fromme Gaben werden stiften,", "tokens": ["From\u00b7me", "Ga\u00b7ben", "wer\u00b7den", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Da\u00df der Bau des sch\u00f6nen M\u00fcnsters", "tokens": ["Da\u00df", "der", "Bau", "des", "sch\u00f6\u00b7nen", "M\u00fcns\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Sei vollendet, will er bitten. \u2013", "tokens": ["Sei", "voll\u00b7en\u00b7det", ",", "will", "er", "bit\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Drauf nach Aachen \u00fcber L\u00fcttich", "tokens": ["Drauf", "nach", "Aa\u00b7chen", "\u00fc\u00b7ber", "L\u00fct\u00b7tich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Karol seinen Weg hinrichtet,", "tokens": ["Ka\u00b7rol", "sei\u00b7nen", "Weg", "hin\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Sich in linder Quelle Fluten,", "tokens": ["Sich", "in", "lin\u00b7der", "Quel\u00b7le", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Nach der Arbeit zu erquicken,", "tokens": ["Nach", "der", "Ar\u00b7beit", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Sankt Marien sch\u00f6nes M\u00fcnster,", "tokens": ["Sankt", "Ma\u00b7ri\u00b7en", "sch\u00f6\u00b7nes", "M\u00fcns\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Das er hatte da gestiftet,", "tokens": ["Das", "er", "hat\u00b7te", "da", "ge\u00b7stif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Hat er reich mit Gold und Silber", "tokens": ["Hat", "er", "reich", "mit", "Gold", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Und mit heil'ger Kunst gezieret,", "tokens": ["Und", "mit", "heil'\u00b7ger", "Kunst", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Lie\u00df mit Flei\u00df da sorglich malen", "tokens": ["Lie\u00df", "mit", "Flei\u00df", "da", "sorg\u00b7lich", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Alle heiligen Geschichten.", "tokens": ["Al\u00b7le", "hei\u00b7li\u00b7gen", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Auch auf gleiche sch\u00f6ne Weise", "tokens": ["Auch", "auf", "glei\u00b7che", "sch\u00f6\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Ward die Kaiserburg gezieret,", "tokens": ["Ward", "die", "Kai\u00b7ser\u00b7burg", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Die er dicht am hohen M\u00fcnster", "tokens": ["Die", "er", "dicht", "am", "ho\u00b7hen", "M\u00fcns\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Sich zur Freude aufgerichtet;", "tokens": ["Sich", "zur", "Freu\u00b7de", "auf\u00b7ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Denn da sah man jene Schlachten", "tokens": ["Denn", "da", "sah", "man", "je\u00b7ne", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Alle wundersam geschildert,", "tokens": ["Al\u00b7le", "wun\u00b7der\u00b7sam", "ge\u00b7schil\u00b7dert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Die in Spanien sind gefochten,", "tokens": ["Die", "in", "Spa\u00b7ni\u00b7en", "sind", "ge\u00b7foch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.68": {"text": "Wo die Heiden sie besiegten;", "tokens": ["Wo", "die", "Hei\u00b7den", "sie", "be\u00b7sieg\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Auch die sieben freien K\u00fcnste,", "tokens": ["Auch", "die", "sie\u00b7ben", "frei\u00b7en", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Die der Weisheit Kreis umschlie\u00dfen,", "tokens": ["Die", "der", "Weis\u00b7heit", "Kreis", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Sah man da nach ihren Zeichen,", "tokens": ["Sah", "man", "da", "nach", "ih\u00b7ren", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Durch der Meister Kunst gebildet.", "tokens": ["Durch", "der", "Meis\u00b7ter", "Kunst", "ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Als das Jahr achthundert vierzehn", "tokens": ["Als", "das", "Jahr", "acht\u00b7hun\u00b7dert", "vier\u00b7zehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "CARD", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Man nun z\u00e4hlte bei den Christen,", "tokens": ["Man", "nun", "z\u00e4hl\u00b7te", "bei", "den", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Sah man wundersame Zeichen,", "tokens": ["Sah", "man", "wun\u00b7der\u00b7sa\u00b7me", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Die auf Karles Tod hinzielten.", "tokens": ["Die", "auf", "Kar\u00b7les", "Tod", "hin\u00b7ziel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Sonn' und Mond hat bei sechs Tagen", "tokens": ["Sonn'", "und", "Mond", "hat", "bei", "sechs", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Schw\u00e4rzlich leuchtend nur geschienen,", "tokens": ["Schw\u00e4rz\u00b7lich", "leuch\u00b7tend", "nur", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "Auch die Worte \u00bbKaiser Karol\u00ab,", "tokens": ["Auch", "die", "Wor\u00b7te", "\u00bb", "Kai\u00b7ser", "Ka\u00b7rol", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$(", "NN", "NE", "$(", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.80": {"text": "Die zur Inschrift dort geschrieben", "tokens": ["Die", "zur", "In\u00b7schrift", "dort", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.81": {"text": "Standen an der Wand der Kirche,", "tokens": ["Stan\u00b7den", "an", "der", "Wand", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Sah man pl\u00f6tzlich einst verschwinden.", "tokens": ["Sah", "man", "pl\u00f6tz\u00b7lich", "einst", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "Einstmals ward es auf der Reise", "tokens": ["Einst\u00b7mals", "ward", "es", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Dunkel um ihn her und finster,", "tokens": ["Dun\u00b7kel", "um", "ihn", "her", "und", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Ganz des Tages Licht verschwunden;", "tokens": ["Ganz", "des", "Ta\u00b7ges", "Licht", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Von der Rechten f\u00e4hrt 