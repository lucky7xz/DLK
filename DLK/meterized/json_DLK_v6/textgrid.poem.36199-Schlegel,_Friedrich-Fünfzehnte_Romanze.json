, "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.93": {"text": "Seine Kriegsgenossen eilten", "tokens": ["Sei\u00b7ne", "Kriegs\u00b7ge\u00b7nos\u00b7sen", "eil\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.94": {"text": "Ihn vom Boden aufzurichten;", "tokens": ["Ihn", "vom", "Bo\u00b7den", "auf\u00b7zu\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.95": {"text": "Ruhig ist er bald entschlafen,", "tokens": ["Ru\u00b7hig", "ist", "er", "bald", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Noch manch' milde Gabe stiftend,", "tokens": ["Noch", "man\u00b7ch'", "mil\u00b7de", "Ga\u00b7be", "stif\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.97": {"text": "F\u00fcr die Armen, f\u00fcr die Kl\u00f6ster,", "tokens": ["F\u00fcr", "die", "Ar\u00b7men", ",", "f\u00fcr", "die", "Kl\u00f6s\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.98": {"text": "Gibt er vieles Gold und Silber,", "tokens": ["Gibt", "er", "vie\u00b7les", "Gold", "und", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.99": {"text": "Da\u00df f\u00fcr seine Kriegsgenossen", "tokens": ["Da\u00df", "f\u00fcr", "sei\u00b7ne", "Kriegs\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "Und sein eigen Heil sie bitten.", "tokens": ["Und", "sein", "ei\u00b7gen", "Heil", "sie", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.101": {"text": "Ruhig in dem Herrn entschlief er,", "tokens": ["Ru\u00b7hig", "in", "dem", "Herrn", "ent\u00b7schlief", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.102": {"text": "Zu empfah'n den Lohn des Himmels;", "tokens": ["Zu", "emp\u00b7fah'n", "den", "Lohn", "des", "Him\u00b7mels", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.103": {"text": "Seine Seele ruh' in Frieden.", "tokens": ["Sei\u00b7ne", "See\u00b7le", "ruh'", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}