{"textgrid.poem.52142": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ort, das Dorff ist hier, das man vom Mertzen nennt,", "tokens": ["Der", "Ort", ",", "das", "Dorff", "ist", "hier", ",", "das", "man", "vom", "Mert\u00b7zen", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drauff ich ins Vierdte Jahr nicht Wirthschafft k\u00f6nnen treiben:", "tokens": ["Drauff", "ich", "ins", "Vierd\u00b7te", "Jahr", "nicht", "Wirth\u00b7schafft", "k\u00f6n\u00b7nen", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "ADJA", "NN", "PTKNEG", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier ist es, da der Held im Felde muste bleiben.", "tokens": ["Hier", "ist", "es", ",", "da", "der", "Held", "im", "Fel\u00b7de", "mus\u00b7te", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus Sachsen Lauenburg: Seht, wie sein Hertze brennt.", "tokens": ["Aus", "Sach\u00b7sen", "Lau\u00b7en\u00b7burg", ":", "Seht", ",", "wie", "sein", "Hert\u00b7ze", "brennt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$.", "VVFIN", "$,", "PWAV", "VAINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die helle Mitternacht, da, als der Tag anbrach,", "tokens": ["Die", "hel\u00b7le", "Mit\u00b7ter\u00b7nacht", ",", "da", ",", "als", "der", "Tag", "an\u00b7brach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht alle Felder schwartz, so weit man konte sehen,", "tokens": ["Macht", "al\u00b7le", "Fel\u00b7der", "schwartz", ",", "so", "weit", "man", "kon\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADJD", "$,", "ADV", "ADJD", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Der Hertzog wuste sich nicht aus der Schaar zu drehen,", "tokens": ["Der", "Hert\u00b7zog", "wus\u00b7te", "sich", "nicht", "aus", "der", "Schaar", "zu", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Torsten Sohn gieng ihm auff hei\u00dfem Fu\u00dfe nach.", "tokens": ["Der", "Tors\u00b7ten", "Sohn", "gieng", "ihm", "auff", "hei\u00b7\u00dfem", "Fu\u00b7\u00dfe", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nu beyde satzten sich dort umb das tr\u00fcbe See,", "tokens": ["Nu", "bey\u00b7de", "satz\u00b7ten", "sich", "dort", "umb", "das", "tr\u00fc\u00b7be", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daraus mein Schw\u00e4her lie\u00df ein Schlam Geb\u00fcrge tragen,", "tokens": ["Da\u00b7raus", "mein", "Schw\u00e4\u00b7her", "lie\u00df", "ein", "Schlam", "Ge\u00b7b\u00fcr\u00b7ge", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und als die V\u00f6lcker theils gelauffen, theils geschlagen,", "tokens": ["Und", "als", "die", "V\u00f6l\u00b7cker", "theils", "ge\u00b7lauf\u00b7fen", ",", "theils", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht ihm ein strenger Schlu\u00df mit zweyen K\u00fcgeln weh.", "tokens": ["Macht", "ihm", "ein", "stren\u00b7ger", "Schlu\u00df", "mit", "zwe\u00b7yen", "K\u00fc\u00b7geln", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "APPR", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Der Zobtenberg, als er den Hertzog so allein", "tokens": ["Der", "Zob\u00b7ten\u00b7berg", ",", "als", "er", "den", "Hert\u00b7zog", "so", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NE", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwundet und zugleich gefangen sahe liegen,", "tokens": ["Ver\u00b7wun\u00b7det", "und", "zu\u00b7gleich", "ge\u00b7fan\u00b7gen", "sa\u00b7he", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kam auff die Wahlstatt hin im Schatten selbst gestiegen,", "tokens": ["Kam", "auff", "die", "Wahl\u00b7statt", "hin", "im", "Schat\u00b7ten", "selbst", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und haubte sich und dann sein andre Br\u00fcder ein.", "tokens": ["Und", "haub\u00b7te", "sich", "und", "dann", "sein", "and\u00b7re", "Br\u00fc\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KON", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nachdem Sie ihn gantz matt auff Kletschkau zugebracht,", "tokens": ["Nach\u00b7dem", "Sie", "ihn", "gantz", "matt", "auff", "Kletschkau", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "APPR", "NE", "VVPP", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da auch mein Meyerhoff die Helden unterhalten,", "tokens": ["Da", "auch", "mein", "Me\u00b7yer\u00b7hoff", "die", "Hel\u00b7den", "un\u00b7ter\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df Bolco seine Statt mit St\u00fccken ward zuspalten:", "tokens": ["Bi\u00df", "Bol\u00b7co", "sei\u00b7ne", "Statt", "mit", "St\u00fc\u00b7cken", "ward", "zus\u00b7pal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gab er der Welt und uns in Schweidnitz gute Nacht.", "tokens": ["Gab", "er", "der", "Welt", "und", "uns", "in", "Schweid\u00b7nitz", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PPER", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie aber, da\u00df das Dorff durch Glut ward abgethan?", "tokens": ["Wie", "a\u00b7ber", ",", "da\u00df", "das", "Dorff", "durch", "Glut", "ward", "ab\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann F\u00fcrsten untergehn, so mu\u00df man Fackeln haben,", "tokens": ["Wann", "F\u00fcrs\u00b7ten", "un\u00b7ter\u00b7gehn", ",", "so", "mu\u00df", "man", "Fa\u00b7ckeln", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADV", "VMFIN", "PIS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil das arme Land man k\u00fcnfftig wird begraben,", "tokens": ["Und", "weil", "das", "ar\u00b7me", "Land", "man", "k\u00fcnff\u00b7tig", "wird", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PIS", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sagt es dem F\u00fcrstenthum auch seine Hinfarth an.", "tokens": ["Sagt", "es", "dem", "F\u00fcrs\u00b7ten\u00b7thum", "auch", "sei\u00b7ne", "Hinf\u00b7arth", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}