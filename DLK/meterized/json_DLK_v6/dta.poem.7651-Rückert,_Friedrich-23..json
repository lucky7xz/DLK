{"dta.poem.7651": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was innig dich ergreift, das la\u00df fein langsam reifen;", "tokens": ["Was", "in\u00b7nig", "dich", "er\u00b7greift", ",", "das", "la\u00df", "fein", "lang\u00b7sam", "rei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was au\u00dfen dich nur streift, mu\u00dft du sogleich ergreifen.", "tokens": ["Was", "au\u00b7\u00dfen", "dich", "nur", "streift", ",", "mu\u00dft", "du", "sog\u00b7leich", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo du's nicht gleich ergreifst, f\u00fcr immer ists verloren;", "tokens": ["Wo", "du's", "nicht", "gleich", "er\u00b7greifst", ",", "f\u00fcr", "im\u00b7mer", "ists", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "ADV", "VVFIN", "$,", "APPR", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch was du in dir reifst, wird schon einmal geboren.", "tokens": ["Doch", "was", "du", "in", "dir", "reifst", ",", "wird", "schon", "ein\u00b7mal", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word",