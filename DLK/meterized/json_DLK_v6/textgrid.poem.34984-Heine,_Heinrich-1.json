{"textgrid.poem.34984": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein K\u00f6nig in Thule, der trinkt", "tokens": ["Es", "ist", "ein", "K\u00f6\u00b7nig", "in", "Thu\u00b7le", ",", "der", "trinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,", "PRELS", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Champagner, es geht ihm nichts dr\u00fcber;", "tokens": ["Cham\u00b7pag\u00b7ner", ",", "es", "geht", "ihm", "nichts", "dr\u00fc\u00b7ber", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PIS", "PAV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wenn er seinen Champagner trinkt,", "tokens": ["Und", "wenn", "er", "sei\u00b7nen", "Cham\u00b7pag\u00b7ner", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dann gehen die Augen ihm \u00fcber.", "tokens": ["Dann", "ge\u00b7hen", "die", "Au\u00b7gen", "ihm", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Ritter sitzen um ihn her,", "tokens": ["Die", "Rit\u00b7ter", "sit\u00b7zen", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Historische Schule;", "tokens": ["Die", "gan\u00b7ze", "His\u00b7to\u00b7ri\u00b7sche", "Schu\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm aber wird die Zunge schwer,", "tokens": ["Ihm", "a\u00b7ber", "wird", "die", "Zun\u00b7ge", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es lallt der K\u00f6nig von Thule:", "tokens": ["Es", "lallt", "der", "K\u00f6\u00b7nig", "von", "Thu\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbals Alexander, der Griechenheld,", "tokens": ["\u00bb", "als", "A\u00b7lex\u00b7an\u00b7der", ",", "der", "Grie\u00b7chen\u00b7held", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit seinem kleinen Haufen", "tokens": ["Mit", "sei\u00b7nem", "klei\u00b7nen", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erobert hatte die ganze Welt,", "tokens": ["Er\u00b7o\u00b7bert", "hat\u00b7te", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da gab er sich ans Saufen.", "tokens": ["Da", "gab", "er", "sich", "ans", "Sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihn hatten so durstig gemacht der Krieg", "tokens": ["Ihn", "hat\u00b7ten", "so", "durs\u00b7tig", "ge\u00b7macht", "der", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Schlachten, die er geschlagen;", "tokens": ["Und", "die", "Schlach\u00b7ten", ",", "die", "er", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er soff sich zu Tode nach dem Sieg,", "tokens": ["Er", "soff", "sich", "zu", "To\u00b7de", "nach", "dem", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er konnte nicht viel vertragen.", "tokens": ["Er", "konn\u00b7te", "nicht", "viel", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich aber bin ein st\u00e4rkerer Mann", "tokens": ["Ich", "a\u00b7ber", "bin", "ein", "st\u00e4r\u00b7ke\u00b7rer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und habe mich kl\u00fcger besonnen:", "tokens": ["Und", "ha\u00b7be", "mich", "kl\u00fc\u00b7ger", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie jener endete, fang ich an,", "tokens": ["Wie", "je\u00b7ner", "en\u00b7de\u00b7te", ",", "fang", "ich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich hab mit dem Trinken begonnen.", "tokens": ["Ich", "hab", "mit", "dem", "Trin\u00b7ken", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Im Rausche wird der Heldenzug", "tokens": ["Im", "Rau\u00b7sche", "wird", "der", "Hel\u00b7den\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir sp\u00e4ter weit besser gelingen;", "tokens": ["Mir", "sp\u00e4\u00b7ter", "weit", "bes\u00b7ser", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Dann werde ich, taumelnd von Krug zu Krug,", "tokens": ["Dann", "wer\u00b7de", "ich", ",", "tau\u00b7melnd", "von", "Krug", "zu", "Krug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die ganze Welt bezwingen.\u00ab", "tokens": ["Die", "gan\u00b7ze", "Welt", "be\u00b7zwin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}