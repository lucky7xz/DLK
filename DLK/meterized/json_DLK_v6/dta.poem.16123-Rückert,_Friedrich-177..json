{"dta.poem.16123": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "177.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verlier, o J\u00fcngling, nur Geduld und Hoffnung nicht;", "tokens": ["Ver\u00b7lier", ",", "o", "J\u00fcng\u00b7ling", ",", "nur", "Ge\u00b7duld", "und", "Hoff\u00b7nung", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ADV", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Richt' auf die Welt Vertraun, auf Gott die Zuversicht,", "tokens": ["Richt'", "auf", "die", "Welt", "Ver\u00b7traun", ",", "auf", "Gott", "die", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An dich die Forderung zu k\u00e4mpfen als ein Mann,", "tokens": ["An", "dich", "die", "For\u00b7de\u00b7rung", "zu", "k\u00e4mp\u00b7fen", "als", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freue dich am Kampf, wenn dir der Sieg entrann.", "tokens": ["Und", "freu\u00b7e", "dich", "am", "Kampf", ",", "wenn", "dir", "der", "Sieg", "ent\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn er dir oft entrann, wird er nicht stets entrinnen;", "tokens": ["Wenn", "er", "dir", "oft", "ent\u00b7rann", ",", "wird", "er", "nicht", "stets", "ent\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur wer noch nichts gewann, hat alles zu gewinnen.", "tokens": ["Nur", "wer", "noch", "nichts", "ge\u00b7wann", ",", "hat", "al\u00b7les", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "PIS", "VVFIN", "$,", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mir selber ist, was mir gelang, gar sp\u00e4t gelungen,", "tokens": ["Mir", "sel\u00b7ber", "ist", ",", "was", "mir", "ge\u00b7lang", ",", "gar", "sp\u00e4t", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mehr nun freut mich, da\u00df ich rang, als was errungen.", "tokens": ["Doch", "mehr", "nun", "freut", "mich", ",", "da\u00df", "ich", "rang", ",", "als", "was", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich w\u00fcnsche nicht, da\u00df sie so gar lang hin dich halten,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nicht", ",", "da\u00df", "sie", "so", "gar", "lang", "hin", "dich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch gut ists, da\u00df sie Zeit dir g\u00f6nnen zum Entfalten.", "tokens": ["Doch", "gut", "ists", ",", "da\u00df", "sie", "Zeit", "dir", "g\u00f6n\u00b7nen", "zum", "Ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "NN", "PPER", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}