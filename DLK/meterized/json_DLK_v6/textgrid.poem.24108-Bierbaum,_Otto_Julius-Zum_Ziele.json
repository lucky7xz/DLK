{"textgrid.poem.24108": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ziele", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00dft uns fahren \u00fcber Land!", "tokens": ["Nun", "la\u00dft", "uns", "fah\u00b7ren", "\u00fc\u00b7ber", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pferde sind schon angespannt", "tokens": ["Die", "Pfer\u00b7de", "sind", "schon", "an\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und scharren mit den Hufen.", "tokens": ["Und", "schar\u00b7ren", "mit", "den", "Hu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6n ist die Welt, und die Welt ist mein,", "tokens": ["Sch\u00f6n", "ist", "die", "Welt", ",", "und", "die", "Welt", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ich h\u00f6re eine Stimme rein", "tokens": ["Ich", "h\u00f6\u00b7re", "ei\u00b7ne", "Stim\u00b7me", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fern meinen Namen rufen.", "tokens": ["Fern", "mei\u00b7nen", "Na\u00b7men", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fahr Kutscher, fahr in den dunklen Tann!", "tokens": ["Fahr", "Kut\u00b7scher", ",", "fahr", "in", "den", "dunk\u00b7len", "Tann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich fahre.", "tokens": ["Ich", "fah\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Fahr Kutscher, fahr mich den Berg hinan!", "tokens": ["Fahr", "Kut\u00b7scher", ",", "fahr", "mich", "den", "Berg", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich fahre.", "tokens": ["Ich", "fah\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und dann hinunter ins Gartenland,", "tokens": ["Und", "dann", "hin\u00b7un\u00b7ter", "ins", "Gar\u00b7ten\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da steht ein Haus: Zum Gl\u00fcck genannt.", "tokens": ["Da", "steht", "ein", "Haus", ":", "Zum", "Gl\u00fcck", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich fahre.", "tokens": ["Ich", "fah\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Es traben die Pferde, es knirscht der Sand,", "tokens": ["Es", "tra\u00b7ben", "die", "Pfer\u00b7de", ",", "es", "knirscht", "der", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es geht durch lachendes, bl\u00fchendes Land.", "tokens": ["Es", "geht", "durch", "la\u00b7chen\u00b7des", ",", "bl\u00fc\u00b7hen\u00b7des", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da steht der Tann im Schweigen.", "tokens": ["Da", "steht", "der", "Tann", "im", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir fahren langsam in ihn ein,", "tokens": ["Wir", "fah\u00b7ren", "lang\u00b7sam", "in", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gr\u00fcn wird der goldene Sonnenschein,", "tokens": ["Gr\u00fcn", "wird", "der", "gol\u00b7de\u00b7ne", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Nun, Rappen, gehts ans Steigen.", "tokens": ["Nun", ",", "Rap\u00b7pen", ",", "gehts", "ans", "Stei\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du die Stimme aus dem Grund?", "tokens": ["H\u00f6rst", "du", "die", "Stim\u00b7me", "aus", "dem", "Grund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ich h\u00f6re.", "tokens": ["Ich", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie wiederhallt von Schlund zu Schlund.", "tokens": ["Sie", "wie\u00b7der\u00b7hallt", "von", "Schlund", "zu", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6re.", "tokens": ["Ich", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es schwebt um uns der leise Schall,", "tokens": ["Es", "schwebt", "um", "uns", "der", "lei\u00b7se", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Stimme ist all\u00fcberall.", "tokens": ["Die", "Stim\u00b7me", "ist", "al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich h\u00f6re.", "tokens": ["Ich", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Der Gipfel da. Die Stimme schweigt.", "tokens": ["Der", "Gip\u00b7fel", "da", ".", "Die", "Stim\u00b7me", "schweigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kutscher in den Abgrund zeigt.", "tokens": ["Der", "Kut\u00b7scher", "in", "den", "Ab\u00b7grund", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blau dehnt sichs ohne Ende.", "tokens": ["Blau", "dehnt", "sichs", "oh\u00b7ne", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort unten ist kein Bl\u00fchen mehr,", "tokens": ["Dort", "un\u00b7ten", "ist", "kein", "Bl\u00fc\u00b7hen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort unten ist es kalt und leer!", "tokens": ["Dort", "un\u00b7ten", "ist", "es", "kalt", "und", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Oh wende die Rappen, wende!", "tokens": ["Oh", "wen\u00b7de", "die", "Rap\u00b7pen", ",", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo hast du mich, Trauriger, hingebracht?", "tokens": ["Wo", "hast", "du", "mich", ",", "Trau\u00b7ri\u00b7ger", ",", "hin\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zum Ziele.", "tokens": ["Zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wohin f\u00e4llt diese schwebende Nacht?", "tokens": ["Wo\u00b7hin", "f\u00e4llt", "die\u00b7se", "schwe\u00b7ben\u00b7de", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zum Ziele.", "tokens": ["Zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich aber, ich will nicht, ich will zur\u00fcck,", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "will", "nicht", ",", "ich", "will", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "$,", "PPER", "VMFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich will zum Hause, genannt Zum Gl\u00fcck!", "tokens": ["Ich", "will", "zum", "Hau\u00b7se", ",", "ge\u00b7nannt", "Zum", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zum Ziele.", "tokens": ["Zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Da wurde mir ruhig und wurde mir klar,", "tokens": ["Da", "wur\u00b7de", "mir", "ru\u00b7hig", "und", "wur\u00b7de", "mir", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da wu\u00dft ich, wohin ich gefahren war,", "tokens": ["Da", "wu\u00dft", "ich", ",", "wo\u00b7hin", "ich", "ge\u00b7fah\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wars zufrieden.", "tokens": ["Und", "wars", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Kutscher fuhr r\u00fcckw\u00e4rts, ich gab ihm die Hand,", "tokens": ["Der", "Kut\u00b7scher", "fuhr", "r\u00fcck\u00b7w\u00e4rts", ",", "ich", "gab", "ihm", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und sprach: oh gr\u00fc\u00df mir das bl\u00fchende Land,", "tokens": ["Und", "sprach", ":", "oh", "gr\u00fc\u00df", "mir", "das", "bl\u00fc\u00b7hen\u00b7de", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus dem ich geschieden.", "tokens": ["Aus", "dem", "ich", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und aus der Leere klangs hell und lind:", "tokens": ["Und", "aus", "der", "Lee\u00b7re", "klangs", "hell", "und", "lind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Komm schnelle.", "tokens": ["Komm", "schnel\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "M\u00fcd ward ich wie im Spielen ein Kind.", "tokens": ["M\u00fcd", "ward", "ich", "wie", "im", "Spie\u00b7len", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KOKOM", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Komm schnelle.", "tokens": ["Komm", "schnel\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich lief in den Abgrund, ins schwebende Meer", "tokens": ["Ich", "lief", "in", "den", "Ab\u00b7grund", ",", "ins", "schwe\u00b7ben\u00b7de", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und f\u00fchlte von mir kein F\u00fchlen mehr.", "tokens": ["Und", "f\u00fchl\u00b7te", "von", "mir", "kein", "F\u00fch\u00b7len", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Komm schnelle.", "tokens": ["Komm", "schnel\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}