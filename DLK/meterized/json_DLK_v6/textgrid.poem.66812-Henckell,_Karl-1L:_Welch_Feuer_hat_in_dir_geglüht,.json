{"textgrid.poem.66812": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch Feuer hat in dir gegl\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Feuer hat in dir gegl\u00fcht,", "tokens": ["Welch", "Feu\u00b7er", "hat", "in", "dir", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schlugst du zu, gewaltiger Mann!", "tokens": ["Wie", "schlugst", "du", "zu", ",", "ge\u00b7wal\u00b7ti\u00b7ger", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Als Deutschland trostlos abgebl\u00fcht,", "tokens": ["Als", "Deutschland", "trost\u00b7los", "ab\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie wipfeltest du himmelan!", "tokens": ["Wie", "wip\u00b7fel\u00b7test", "du", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mutig geredet Blitz und Schwert", "tokens": ["Mu\u00b7tig", "ge\u00b7re\u00b7det", "Blitz", "und", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "In Nacht und Feigheit donnerlaut,", "tokens": ["In", "Nacht", "und", "Feig\u00b7heit", "don\u00b7ner\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Volk die freie Tat gelehrt,", "tokens": ["Dem", "Volk", "die", "frei\u00b7e", "Tat", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Geist die feste Burg gebaut!", "tokens": ["Dem", "Geist", "die", "fes\u00b7te", "Burg", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du Sporn der Jugend, Quell der Kraft,", "tokens": ["Du", "Sporn", "der", "Ju\u00b7gend", ",", "Quell", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Elektrisch Willensbad der Zeit,", "tokens": ["E\u00b7lekt\u00b7risch", "Wil\u00b7lens\u00b7bad", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie brauste deine Leidenschaft", "tokens": ["Wie", "braus\u00b7te", "dei\u00b7ne", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch W\u00fcsten der Gew\u00f6hnlichkeit!", "tokens": ["Durch", "W\u00fcs\u00b7ten", "der", "Ge\u00b7w\u00f6hn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Charakterobelisk im Sand", "tokens": ["Cha\u00b7rak\u00b7te\u00b7ro\u00b7be\u00b7lisk", "im", "Sand"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und Schlamm der Schneckenkriecherei,", "tokens": ["Und", "Schlamm", "der", "Schne\u00b7cken\u00b7krie\u00b7che\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Darauf in heiligen Lettern stand:", "tokens": ["Da\u00b7rauf", "in", "hei\u00b7li\u00b7gen", "Let\u00b7tern", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "\u00bberzieh dich selbst! Sei deutsch: sei frei!\u00ab", "tokens": ["\u00bb", "er\u00b7zieh", "dich", "selbst", "!", "Sei", "deutsch", ":", "sei", "frei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$.", "VAIMP", "ADJD", "$.", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Und jede Letter Lebenshauch", "tokens": ["Und", "je\u00b7de", "Let\u00b7ter", "Le\u00b7bens\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und jedes Wort der Schande Tod \u2013", "tokens": ["Und", "je\u00b7des", "Wort", "der", "Schan\u00b7de", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "In dieser Tage Dunst und Rauch", "tokens": ["In", "die\u00b7ser", "Ta\u00b7ge", "Dunst", "und", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "O Fichte, Fichte tut uns not.", "tokens": ["O", "Fich\u00b7te", ",", "Fich\u00b7te", "tut", "uns", "not", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}