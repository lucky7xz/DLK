{"textgrid.poem.55878": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist die Stunde, da das Reich sich eitel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist die Stunde, da das Reich sich eitel", "tokens": ["Es", "ist", "die", "Stun\u00b7de", ",", "da", "das", "Reich", "sich", "ei\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in seines Glanzes vielen Spiegeln sieht.", "tokens": ["in", "sei\u00b7nes", "Glan\u00b7zes", "vie\u00b7len", "Spie\u00b7geln", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der blasse Zar, des Stammes letztes Glied,", "tokens": ["Der", "blas\u00b7se", "Zar", ",", "des", "Stam\u00b7mes", "letz\u00b7tes", "Glied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "tr\u00e4umt auf dem Thron, davor das Fest geschieht,", "tokens": ["tr\u00e4umt", "auf", "dem", "Thron", ",", "da\u00b7vor", "das", "Fest", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "und leise zittert sein besch\u00e4mter Scheitel", "tokens": ["und", "lei\u00b7se", "zit\u00b7tert", "sein", "be\u00b7sch\u00e4m\u00b7ter", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und seine Hand, die vor den Purpurlehnen", "tokens": ["und", "sei\u00b7ne", "Hand", ",", "die", "vor", "den", "Pur\u00b7pur\u00b7leh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "mit einem unbestimmten Sehnen", "tokens": ["mit", "ei\u00b7nem", "un\u00b7be\u00b7stimm\u00b7ten", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ins wirre Ungewisse flieht.", "tokens": ["ins", "wir\u00b7re", "Un\u00b7ge\u00b7wis\u00b7se", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und um sein Schweigen neigen sich Bojaren", "tokens": ["Und", "um", "sein", "Schwei\u00b7gen", "nei\u00b7gen", "sich", "Bo\u00b7ja\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in blanken Panzern und in Pantherfellen,", "tokens": ["in", "blan\u00b7ken", "Pan\u00b7zern", "und", "in", "Pan\u00b7ther\u00b7fel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie viele fremde f\u00fcrstliche Gefahren,", "tokens": ["wie", "vie\u00b7le", "frem\u00b7de", "f\u00fcrst\u00b7li\u00b7che", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die ihn mit stummer Ungeduld umstellen.", "tokens": ["die", "ihn", "mit", "stum\u00b7mer", "Un\u00b7ge\u00b7duld", "um\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Tief in den Saal schl\u00e4gt ihre Ehrfurcht Wellen.", "tokens": ["Tief", "in", "den", "Saal", "schl\u00e4gt", "ih\u00b7re", "Ehr\u00b7furcht", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und sie gedenken eines andern Zaren,", "tokens": ["Und", "sie", "ge\u00b7den\u00b7ken", "ei\u00b7nes", "an\u00b7dern", "Za\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der oft mit Worten, die aus Wahnsinn waren,", "tokens": ["der", "oft", "mit", "Wor\u00b7ten", ",", "die", "aus", "Wahn\u00b7sinn", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ihnen die Stirnen an die Steine stie\u00df.", "tokens": ["ih\u00b7nen", "die", "Stir\u00b7nen", "an", "die", "Stei\u00b7ne", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und denken also weiter: ", "tokens": ["Und", "den\u00b7ken", "al\u00b7so", "wei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nicht so viel Raum, wenn er zu Throne sa\u00df,", "tokens": ["nicht", "so", "viel", "Raum", ",", "wenn", "er", "zu", "Thro\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "auf dem verwelkten Samt des Kissens leer.", "tokens": ["auf", "dem", "ver\u00b7welk\u00b7ten", "Samt", "des", "Kis\u00b7sens", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er war der Dinge dunkles Ma\u00df,", "tokens": ["Er", "war", "der", "Din\u00b7ge", "dunk\u00b7les", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und die Bojaren wu\u00dften lang nicht mehr,", "tokens": ["und", "die", "Bo\u00b7ja\u00b7ren", "wu\u00df\u00b7ten", "lang", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df rot der Sitz des Sessels sei, so schwer", "tokens": ["da\u00df", "rot", "der", "Sitz", "des", "Ses\u00b7sels", "sei", ",", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "VAFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "lag sein Gewand und wurde golden breit.", "tokens": ["lag", "sein", "Ge\u00b7wand", "und", "wur\u00b7de", "gol\u00b7den", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und weiter denken sie: das Kaiserkleid", "tokens": ["Und", "wei\u00b7ter", "den\u00b7ken", "sie", ":", "das", "Kai\u00b7ser\u00b7kleid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "schl\u00e4ft auf den Schultern dieses Knaben ein.", "tokens": ["schl\u00e4ft", "auf", "den", "Schul\u00b7tern", "die\u00b7ses", "Kna\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Obgleich im ganzen Saal die Fackeln flacken,", "tokens": ["Ob\u00b7gleich", "im", "gan\u00b7zen", "Saal", "die", "Fa\u00b7ckeln", "fla\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sind bleich die Perlen, die in sieben Reihn,", "tokens": ["sind", "bleich", "die", "Per\u00b7len", ",", "die", "in", "sie\u00b7ben", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "wie wei\u00dfe Kinder, knien um seinen Nacken,", "tokens": ["wie", "wei\u00b7\u00dfe", "Kin\u00b7der", ",", "kni\u00b7en", "um", "sei\u00b7nen", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "und die Rubine an den \u00c4rmelzacken,", "tokens": ["und", "die", "Ru\u00b7bi\u00b7ne", "an", "den", "\u00c4r\u00b7mel\u00b7za\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die einst Pokale waren, klar von Wein,", "tokens": ["die", "einst", "Po\u00b7ka\u00b7le", "wa\u00b7ren", ",", "klar", "von", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "sind schwarz wie Schlacken \u2013", "tokens": ["sind", "schwarz", "wie", "Schla\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und ihr Denken schwillt.", "tokens": ["Und", "ihr", "Den\u00b7ken", "schwillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Es dr\u00e4ngt sich heftig an den blassen Kaiser,", "tokens": ["Es", "dr\u00e4ngt", "sich", "hef\u00b7tig", "an", "den", "blas\u00b7sen", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf dessen Haupt die Krone immer leiser", "tokens": ["auf", "des\u00b7sen", "Haupt", "die", "Kro\u00b7ne", "im\u00b7mer", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und dem der Wille immer fremder wird;", "tokens": ["und", "dem", "der", "Wil\u00b7le", "im\u00b7mer", "frem\u00b7der", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "er l\u00e4chelt. Lauter pr\u00fcfen ihn die Preiser,", "tokens": ["er", "l\u00e4\u00b7chelt", ".", "Lau\u00b7ter", "pr\u00fc\u00b7fen", "ihn", "die", "Prei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ihr Neigen n\u00e4hert sich, sie schmeicheln heiser. \u2013", "tokens": ["ihr", "Nei\u00b7gen", "n\u00e4\u00b7hert", "sich", ",", "sie", "schmei\u00b7cheln", "hei\u00b7ser", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "und eine Klinge hat im Traum geklirrt.", "tokens": ["und", "ei\u00b7ne", "Klin\u00b7ge", "hat", "im", "Traum", "ge\u00b7klirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}