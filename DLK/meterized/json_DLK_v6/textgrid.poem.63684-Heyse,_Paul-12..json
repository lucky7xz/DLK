{"textgrid.poem.63684": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, zu tadeln pflegt man ein Gedicht,", "tokens": ["Ich", "wei\u00df", ",", "zu", "ta\u00b7deln", "pflegt", "man", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkennt man, da\u00df es tiefern Sinn entbehre,", "tokens": ["Er\u00b7kennt", "man", ",", "da\u00df", "es", "tie\u00b7fern", "Sinn", "ent\u00b7beh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df f\u00fcr die Welt es wohl kein Schade w\u00e4re,", "tokens": ["Da\u00df", "f\u00fcr", "die", "Welt", "es", "wohl", "kein", "Scha\u00b7de", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADV", "PIAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erblickt' es, totgeboren, nie das Licht.", "tokens": ["Er\u00b7blickt'", "es", ",", "tot\u00b7ge\u00b7bo\u00b7ren", ",", "nie", "das", "Licht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt, da mein guter Doktor mir verspricht,", "tokens": ["Jetzt", ",", "da", "mein", "gu\u00b7ter", "Dok\u00b7tor", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df meinem Kopf die Frische wiederkehre,", "tokens": ["Da\u00df", "mei\u00b7nem", "Kopf", "die", "Fri\u00b7sche", "wie\u00b7der\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn mit Gedanken ich ihn nicht beschwere,", "tokens": ["Wenn", "mit", "Ge\u00b7dan\u00b7ken", "ich", "ihn", "nicht", "be\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird m\u00fc\u00dfig hinzutr\u00e4umen mir zur Pflicht.", "tokens": ["Wird", "m\u00fc\u00b7\u00dfig", "hin\u00b7zu\u00b7tr\u00e4u\u00b7men", "mir", "zur", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wie ein kranker Musiker zuweilen", "tokens": ["Doch", "wie", "ein", "kran\u00b7ker", "Mu\u00b7si\u00b7ker", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Arpeggien spielt und halbe Stunden lang", "tokens": ["Ar\u00b7peg\u00b7gien", "spielt", "und", "hal\u00b7be", "Stun\u00b7den", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Finger auf den Tasten l\u00e4\u00dft verweilen,", "tokens": ["Die", "Fin\u00b7ger", "auf", "den", "Tas\u00b7ten", "l\u00e4\u00dft", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur sich zu laben an der Saiten Klang,", "tokens": ["Nur", "sich", "zu", "la\u00b7ben", "an", "der", "Sai\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So reim' ich spielend manchmal vierzehn Zeilen,", "tokens": ["So", "reim'", "ich", "spie\u00b7lend", "manch\u00b7mal", "vier\u00b7zehn", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was doch erquicklicher als Grillenfang.", "tokens": ["Was", "doch", "er\u00b7quick\u00b7li\u00b7cher", "als", "Gril\u00b7len\u00b7fang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}