{"dta.poem.23733": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Unschuld.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Du, der Unschuld s\u00fc\u00dfe Ruh,", "tokens": ["Du", ",", "der", "Un\u00b7schuld", "s\u00fc\u00b7\u00dfe", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie lieblich schmeichelst du", "tokens": ["O", "wie", "lieb\u00b7lich", "schmei\u00b7chelst", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsern Seelen!", "tokens": ["Un\u00b7sern", "See\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Eitle Wollust fleucht vor dir,", "tokens": ["Eit\u00b7le", "Wol\u00b7lust", "fleucht", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und doch l\u00e4ssest du es mir", "tokens": ["Und", "doch", "l\u00e4s\u00b7sest", "du", "es", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht an Wollust fehlen.", "tokens": ["Nicht", "an", "Wol\u00b7lust", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du streust Rosen und Jesmin", "tokens": ["Du", "streust", "Ro\u00b7sen", "und", "Jes\u00b7min"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Auf die sichern Pfade hin,", "tokens": ["Auf", "die", "si\u00b7chern", "Pfa\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich gehe.", "tokens": ["Die", "ich", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ich bin ganz Zufriedenheit,", "tokens": ["Ich", "bin", "ganz", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich dich voll Heiterkeit", "tokens": ["Wenn", "ich", "dich", "voll", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf mich l\u00e4cheln sehe.", "tokens": ["Auf", "mich", "l\u00e4\u00b7cheln", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ohne Kummer, ohne Reu,", "tokens": ["Oh\u00b7ne", "Kum\u00b7mer", ",", "oh\u00b7ne", "Reu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrst du sie vor mir vorbey", "tokens": ["F\u00fchrst", "du", "sie", "vor", "mir", "vor\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Tage.", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Meine M\u00fch machst du mir leicht,", "tokens": ["Mei\u00b7ne", "M\u00fch", "machst", "du", "mir", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in meine Spiele schleicht", "tokens": ["Und", "in", "mei\u00b7ne", "Spie\u00b7le", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich nicht sp\u00e4te Klage.", "tokens": ["Sich", "nicht", "sp\u00e4\u00b7te", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df mein Herz sich deiner freun,", "tokens": ["La\u00df", "mein", "Herz", "sich", "dei\u00b7ner", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich noch, werd ich \u00e4lter seyn,", "tokens": ["Dich", "noch", ",", "werd", "ich", "\u00e4l\u00b7ter", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundinn nennen.", "tokens": ["Freun\u00b7dinn", "nen\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "In dem Ungl\u00fcck tr\u00f6ste mich,", "tokens": ["In", "dem", "Un\u00b7gl\u00fcck", "tr\u00f6s\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nie la\u00df mich ohne dich", "tokens": ["Und", "nie", "la\u00df", "mich", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Freude kennen.", "tokens": ["Ei\u00b7ne", "Freu\u00b7de", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}