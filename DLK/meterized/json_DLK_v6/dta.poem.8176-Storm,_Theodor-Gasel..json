{"dta.poem.8176": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gasel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "deine Milde lebt,", "tokens": ["dei\u00b7ne", "Mil\u00b7de", "lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Du wei\u00dft es, wie mein ganzes Herz allein in", "tokens": ["Du", "wei\u00dft", "es", ",", "wie", "mein", "gan\u00b7zes", "Herz", "al\u00b7lein", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "ADJA", "NN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "deinem Bilde lebt;", "tokens": ["dei\u00b7nem", "Bil\u00b7de", "lebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Denn wie die Sch\u00f6nheit nimmer sch\u00f6n, die nicht", "tokens": ["Denn", "wie", "die", "Sch\u00f6n\u00b7heit", "nim\u00b7mer", "sch\u00f6n", ",", "die", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "$,", "PRELS", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "der Seele Athem kennt,", "tokens": ["der", "See\u00b7le", "A\u00b7them", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie durch des Lichtes Kraft allein der Zauber der", "tokens": ["Wie", "durch", "des", "Lich\u00b7tes", "Kraft", "al\u00b7lein", "der", "Zau\u00b7ber", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.7": {"text": "Gefilde lebt,", "tokens": ["Ge\u00b7fil\u00b7de", "lebt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So ist das Leben nicht belebt, als durch der Liebe", "tokens": ["So", "ist", "das", "Le\u00b7ben", "nicht", "be\u00b7lebt", ",", "als", "durch", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sakrament;", "tokens": ["Sak\u00b7ra\u00b7ment", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Das f\u00fchlet, wer die Liebe f\u00fchlt, wer unter ihrem", "tokens": ["Das", "f\u00fch\u00b7let", ",", "wer", "die", "Lie\u00b7be", "f\u00fchlt", ",", "wer", "un\u00b7ter", "ih\u00b7rem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$,", "PWS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schilde lebt.", "tokens": ["Schil\u00b7de", "lebt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Ich aber, der die liebste Frau sein unverlierbar", "tokens": ["Ich", "a\u00b7ber", ",", "der", "die", "liebs\u00b7te", "Frau", "sein", "un\u00b7ver\u00b7lier\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "ADJA", "NN", "PPOSAT", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Eigen nennt,", "tokens": ["Ei\u00b7gen", "nennt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ich f\u00fchle, wie die ganze Welt allein in ihrem", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "die", "gan\u00b7ze", "Welt", "al\u00b7lein", "in", "ih\u00b7rem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bilde lebt.", "tokens": ["Bil\u00b7de", "lebt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}