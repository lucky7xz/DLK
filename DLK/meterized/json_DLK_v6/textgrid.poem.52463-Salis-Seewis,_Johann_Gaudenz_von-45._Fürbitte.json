{"textgrid.poem.52463": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "45. F\u00fcrbitte", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige, reine Vernunft, vergieb den Blinden am Wege,", "tokens": ["Hei\u00b7li\u00b7ge", ",", "rei\u00b7ne", "Ver\u00b7nunft", ",", "ver\u00b7gieb", "den", "Blin\u00b7den", "am", "We\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,", "VVIMP", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die dich verfolgen und schm\u00e4hn! \u2013 G\u00f6ttin, sie kannten dich nie!", "tokens": ["Die", "dich", "ver\u00b7fol\u00b7gen", "und", "schm\u00e4hn", "!", "\u2013", "G\u00f6t\u00b7tin", ",", "sie", "kann\u00b7ten", "dich", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVINF", "$.", "$(", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber wehre den ", "tokens": ["A\u00b7ber", "weh\u00b7re", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Vor das vergoldete Kalb, ihren begrenzten Verstand!", "tokens": ["Vor", "das", "ver\u00b7gol\u00b7de\u00b7te", "Kalb", ",", "ih\u00b7ren", "be\u00b7grenz\u00b7ten", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}