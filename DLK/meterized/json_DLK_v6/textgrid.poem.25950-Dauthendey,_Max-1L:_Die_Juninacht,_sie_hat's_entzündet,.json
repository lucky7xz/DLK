{"textgrid.poem.25950": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Juninacht, sie hat's entz\u00fcndet,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Juninacht, sie hat's entz\u00fcndet,", "tokens": ["Die", "Ju\u00b7ni\u00b7nacht", ",", "sie", "hat's", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ein Blick, der dich ergr\u00fcndet,", "tokens": ["Und", "wie", "ein", "Blick", ",", "der", "dich", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So liegt ein kleines Licht im Gras,", "tokens": ["So", "liegt", "ein", "klei\u00b7nes", "Licht", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als flog es dir vom Herzen fort,", "tokens": ["Als", "flog", "es", "dir", "vom", "Her\u00b7zen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Liebeswort, das ungesprochen", "tokens": ["Ein", "Lie\u00b7bes\u00b7wort", ",", "das", "un\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ungebrochen weitergl\u00fcht,", "tokens": ["Und", "un\u00b7ge\u00b7bro\u00b7chen", "wei\u00b7ter\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und lautlos m\u00fcht sich dir zu nennen.", "tokens": ["Und", "laut\u00b7los", "m\u00fcht", "sich", "dir", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch eh' die reife Juninacht", "tokens": ["Doch", "eh'", "die", "rei\u00b7fe", "Ju\u00b7ni\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu End' gedacht,", "tokens": ["Zu", "End'", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Sollst du's erkennen.", "tokens": ["Sollst", "du's", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}