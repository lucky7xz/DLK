{"textgrid.poem.33926": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Adams Opfer", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nsten Fr\u00fcchte, frisch gepfl\u00fcckt,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Fr\u00fcch\u00b7te", ",", "frisch", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gt er zum gr\u00fcnen Festaltar,", "tokens": ["Tr\u00e4gt", "er", "zum", "gr\u00fc\u00b7nen", "Fes\u00b7tal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bringt, mit Blumen reich geschm\u00fcckt,", "tokens": ["Und", "bringt", ",", "mit", "Blu\u00b7men", "reich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fromm als Morgenopfer dar.", "tokens": ["Sie", "fromm", "als", "Mor\u00b7ge\u00b7nop\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst blickt er froh, dann wird er still:", "tokens": ["Erst", "blickt", "er", "froh", ",", "dann", "wird", "er", "still", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Herr, wie arm erschein' ich mir!", "tokens": ["O", "Herr", ",", "wie", "arm", "er\u00b7schein'", "ich", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich den Dank dir bringen will,", "tokens": ["Wenn", "ich", "den", "Dank", "dir", "brin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So borge ich selbst den von dir!", "tokens": ["So", "bor\u00b7ge", "ich", "selbst", "den", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}