{"textgrid.poem.36391": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ignatius bei stiller Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ignatius bei stiller Nacht,", "tokens": ["Ig\u00b7na\u00b7ti\u00b7us", "bei", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann er zum Himmel schauet,", "tokens": ["Wann", "er", "zum", "Him\u00b7mel", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne Stadt bei sich betracht,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Stadt", "bei", "sich", "be\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gott den Seinen bauet.", "tokens": ["Die", "Gott", "den", "Sei\u00b7nen", "bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Angesicht voll Z\u00e4hren flo\u00df,", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "voll", "Z\u00e4h\u00b7ren", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Freud' er ward umfangen,", "tokens": ["Mit", "Freud'", "er", "ward", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Herz von Seufzern \u00fcberflo\u00df,", "tokens": ["Sein", "Herz", "von", "Seuf\u00b7zern", "\u00fc\u00b7ber\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach mit gro\u00df Verlangen:", "tokens": ["Und", "sprach", "mit", "gro\u00df", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jerusalem, du sch\u00f6ne Stadt,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", ",", "du", "sch\u00f6\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann ich zu dir gedenke,", "tokens": ["Wann", "ich", "zu", "dir", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Stund' im hei\u00dfen Z\u00e4hrenbad", "tokens": ["Zur", "Stund'", "im", "hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren\u00b7bad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein' Augen tief versenke.", "tokens": ["Mein'", "Au\u00b7gen", "tief", "ver\u00b7sen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Sonnenlicht, mehr scheine nicht,", "tokens": ["Ach", "Son\u00b7nen\u00b7licht", ",", "mehr", "schei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00f6sch aus den Strahlenschimmer;", "tokens": ["L\u00f6sch", "aus", "den", "Strah\u00b7len\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "All Glanz und Schein, all Flammen dein,", "tokens": ["All", "Glanz", "und", "Schein", ",", "all", "Flam\u00b7men", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "PIAT", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfreuen kann mich's nimmer.", "tokens": ["Er\u00b7freu\u00b7en", "kann", "mich's", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein sch\u00f6nes Licht ist Gott allein,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Licht", "ist", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leucht' im Himmel droben.", "tokens": ["So", "leucht'", "im", "Him\u00b7mel", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sonn', Mond und Stern, mit ihrem Schein,", "tokens": ["Sonn'", ",", "Mond", "und", "Stern", ",", "mit", "ih\u00b7rem", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind da nur ihn zu loben.", "tokens": ["Sind", "da", "nur", "ihn", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sind gegen Gott wie Schatten nur,", "tokens": ["Sind", "ge\u00b7gen", "Gott", "wie", "Schat\u00b7ten", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht m\u00f6gen ihn erreichen;", "tokens": ["Nicht", "m\u00f6\u00b7gen", "ihn", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verdunkelt ganz scheint all ihr Glanz,", "tokens": ["Ver\u00b7dun\u00b7kelt", "ganz", "scheint", "all", "ihr", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und endlich m\u00fcssen weichen.", "tokens": ["Und", "end\u00b7lich", "m\u00fcs\u00b7sen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nichts mag doch sein, als Gott allein,", "tokens": ["Nichts", "mag", "doch", "sein", ",", "als", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VAINF", "$,", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird best\u00e4ndig scheinen;", "tokens": ["Er", "wird", "be\u00b7st\u00e4n\u00b7dig", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All' andre Ding, sie sind gering,", "tokens": ["All'", "and\u00b7re", "Ding", ",", "sie", "sind", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Gott soll ich nicht weinen!", "tokens": ["O", "Gott", "soll", "ich", "nicht", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach wann wird's sein, da\u00df ich erschein',", "tokens": ["Ach", "wann", "wird's", "sein", ",", "da\u00df", "ich", "er\u00b7schein'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "VAINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinen Freuden droben?", "tokens": ["In", "dei\u00b7nen", "Freu\u00b7den", "dro\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach wann werd' ich erschauen dich", "tokens": ["Ach", "wann", "werd'", "ich", "er\u00b7schau\u00b7en", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VAFIN", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deinen Thron erhoben?", "tokens": ["Auf", "dei\u00b7nen", "Thron", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der durst'ge Hirsch nie schneller sprang,", "tokens": ["Der", "dur\u00b7st'\u00b7ge", "Hirsch", "nie", "schnel\u00b7ler", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verschmacht' in hei\u00dfer Sonnen,", "tokens": ["Ver\u00b7schmacht'", "in", "hei\u00b7\u00dfer", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann er vernahm den Wasserklang", "tokens": ["Wann", "er", "ver\u00b7nahm", "den", "Was\u00b7ser\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Fall der k\u00fchlen Bronnen,", "tokens": ["Vom", "Fall", "der", "k\u00fch\u00b7len", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Als ich nach dir tracht' mit Begier,", "tokens": ["Als", "ich", "nach", "dir", "tracht'", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "O Gottes Stadt so schone,", "tokens": ["O", "Got\u00b7tes", "Stadt", "so", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich Tag und Nacht nach dir verschmacht,", "tokens": ["Ich", "Tag", "und", "Nacht", "nach", "dir", "ver\u00b7schmacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dir, o Gottes Wohne.", "tokens": ["Nach", "dir", ",", "o", "Got\u00b7tes", "Woh\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}