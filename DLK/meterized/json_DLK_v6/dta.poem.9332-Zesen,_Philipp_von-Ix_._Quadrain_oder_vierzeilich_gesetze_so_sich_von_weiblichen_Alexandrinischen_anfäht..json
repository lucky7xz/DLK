{"dta.poem.9332": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ix .  \n Quadrain oder vierzeilich gesetze/  \n so sich von weiblichen Alexandrinischen  \n anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein vers ist lobens werth; er wird fein lange tauren/", "tokens": ["Dein", "vers", "ist", "lo\u00b7bens", "werth", ";", "er", "wird", "fein", "lan\u00b7ge", "tau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil sich ein jedes wort hart in einander schraubt/", "tokens": ["weil", "sich", "ein", "je\u00b7des", "wort", "hart", "in", "ein\u00b7an\u00b7der", "schraubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df einer wer jhn liest/ vor schweren worten schnaubt/", "tokens": ["da\u00df", "ei\u00b7ner", "wer", "jhn", "liest", "/", "vor", "schwe\u00b7ren", "wor\u00b7ten", "schnaubt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PWS", "PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie ein Soldate thut/ wenn er ersteigt die mauren.", "tokens": ["wie", "ein", "Sol\u00b7da\u00b7te", "thut", "/", "wenn", "er", "er\u00b7steigt", "die", "mau\u00b7ren", "."], "token_info": ["word", "word", "word