"der", "Kind\u00b7heit", ",", "ka\u00b7men", "za\u00b7gen", "Fal\u00b7les", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und spielten mit den Dingen auf dem Grunde,", "tokens": ["und", "spiel\u00b7ten", "mit", "den", "Din\u00b7gen", "auf", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "bis das Gef\u00e4lle ihr Gef\u00fchl ergriff:", "tokens": ["bis", "das", "Ge\u00b7f\u00e4l\u00b7le", "ihr", "Ge\u00b7f\u00fchl", "er\u00b7griff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann f\u00fcllten sie mit flachem klaren Wasser", "tokens": ["Dann", "f\u00fcll\u00b7ten", "sie", "mit", "fla\u00b7chem", "kla\u00b7ren", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ganze Breite dieses breiten Weges", "tokens": ["die", "gan\u00b7ze", "Brei\u00b7te", "die\u00b7ses", "brei\u00b7ten", "We\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und trieben Wirbel an den tiefen Stellen;", "tokens": ["und", "trie\u00b7ben", "Wir\u00b7bel", "an", "den", "tie\u00b7fen", "Stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "AP