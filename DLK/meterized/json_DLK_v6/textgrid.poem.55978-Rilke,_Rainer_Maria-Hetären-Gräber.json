{"textgrid.poem.55978": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Het\u00e4ren-Gr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihren langen Haaren liegen sie", "tokens": ["In", "ih\u00b7ren", "lan\u00b7gen", "Haa\u00b7ren", "lie\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit braunen, tief in sich gegangenen Gesichtern.", "tokens": ["mit", "brau\u00b7nen", ",", "tief", "in", "sich", "ge\u00b7gan\u00b7ge\u00b7nen", "Ge\u00b7sich\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Augen zu wie vor zu vieler Ferne.", "tokens": ["Die", "Au\u00b7gen", "zu", "wie", "vor", "zu", "vie\u00b7ler", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KOKOM", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Skelette, Munde, Blumen. In den Munden", "tokens": ["Ske\u00b7let\u00b7te", ",", "Mun\u00b7de", ",", "Blu\u00b7men", ".", "In", "den", "Mun\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die glatten Z\u00e4hne wie ein Reise-Schachspiel", "tokens": ["die", "glat\u00b7ten", "Z\u00e4h\u00b7ne", "wie", "ein", "Rei\u00b7se\u00b7Schach\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "aus Elfenbein in Reihen aufgestellt.", "tokens": ["aus", "El\u00b7fen\u00b7bein", "in", "Rei\u00b7hen", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Blumen, gelbe Perlen, schlanke Knochen,", "tokens": ["Und", "Blu\u00b7men", ",", "gel\u00b7be", "Per\u00b7len", ",", "schlan\u00b7ke", "Kno\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "H\u00e4nde und Hemden, welkende Gewebe", "tokens": ["H\u00e4n\u00b7de", "und", "Hem\u00b7den", ",", "wel\u00b7ken\u00b7de", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PWAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "\u00fcber dem eingest\u00fcrzten Herzen. Aber", "tokens": ["\u00fc\u00b7ber", "dem", "ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Her\u00b7zen", ".", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "dort unter jenen Ringen, Talismanen", "tokens": ["dort", "un\u00b7ter", "je\u00b7nen", "Rin\u00b7gen", ",", "Ta\u00b7lis\u00b7ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "und augenblauen Steinen (Lieblings-Angedenken)", "tokens": ["und", "au\u00b7gen\u00b7blau\u00b7en", "Stei\u00b7nen", "(", "Lieb\u00b7lings\u00b7An\u00b7ge\u00b7den\u00b7ken", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "steht noch die stille Krypta des Geschlechtes,", "tokens": ["steht", "noch", "die", "stil\u00b7le", "Kryp\u00b7ta", "des", "Ge\u00b7schlech\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "bis an die W\u00f6lbung voll mit Blumenbl\u00e4ttern.", "tokens": ["bis", "an", "die", "W\u00f6l\u00b7bung", "voll", "mit", "Blu\u00b7men\u00b7bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und wieder gelbe Perlen, weitverrollte, \u2013", "tokens": ["Und", "wie\u00b7der", "gel\u00b7be", "Per\u00b7len", ",", "weit\u00b7ver\u00b7roll\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Schalen gebrannten Tones, deren Bug", "tokens": ["Scha\u00b7len", "ge\u00b7brann\u00b7ten", "To\u00b7nes", ",", "de\u00b7ren", "Bug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "ihr eignes Bild geziert hat, gr\u00fcne Scherben", "tokens": ["ihr", "eig\u00b7nes", "Bild", "ge\u00b7ziert", "hat", ",", "gr\u00fc\u00b7ne", "Scher\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "von Salben-Vasen, die wie Blumen duften,", "tokens": ["von", "Sal\u00b7ben\u00b7Va\u00b7sen", ",", "die", "wie", "Blu\u00b7men", "duf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "und Formen kleiner G\u00f6tter: Hausalt\u00e4re,", "tokens": ["und", "For\u00b7men", "klei\u00b7ner", "G\u00f6t\u00b7ter", ":", "Haus\u00b7al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Het\u00e4renhimmel mit entz\u00fcckten G\u00f6ttern.", "tokens": ["He\u00b7t\u00e4\u00b7ren\u00b7him\u00b7mel", "mit", "ent\u00b7z\u00fcck\u00b7ten", "G\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Gesprengte G\u00fcrtel, flache Skarab\u00e4en,", "tokens": ["Ge\u00b7spreng\u00b7te", "G\u00fcr\u00b7tel", ",", "fla\u00b7che", "Ska\u00b7ra\u00b7b\u00e4\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "kleine Figuren riesigen Geschlechtes,", "tokens": ["klei\u00b7ne", "Fi\u00b7gu\u00b7ren", "rie\u00b7si\u00b7gen", "Ge\u00b7schlech\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "ein Mund der lacht und Tanzende und L\u00e4ufer,", "tokens": ["ein", "Mund", "der", "lacht", "und", "Tan\u00b7zen\u00b7de", "und", "L\u00e4u\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "goldene Fibeln, kleinen Bogen \u00e4hnlich", "tokens": ["gol\u00b7de\u00b7ne", "Fi\u00b7beln", ",", "klei\u00b7nen", "Bo\u00b7gen", "\u00e4hn\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "zur Jagd auf Tier- und Vogelamulette,", "tokens": ["zur", "Jagd", "auf", "Tier", "und", "Vo\u00b7ge\u00b7la\u00b7mu\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "und lange Nadeln, zieres Hausger\u00e4te", "tokens": ["und", "lan\u00b7ge", "Na\u00b7deln", ",", "zie\u00b7res", "Haus\u00b7ge\u00b7r\u00e4\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "und eine runde Scherbe roten Grundes,", "tokens": ["und", "ei\u00b7ne", "run\u00b7de", "Scher\u00b7be", "ro\u00b7ten", "Grun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "darauf, wie eines Eingangs schwarze Aufschrift,", "tokens": ["da\u00b7rauf", ",", "wie", "ei\u00b7nes", "Ein\u00b7gangs", "schwar\u00b7ze", "Auf\u00b7schrift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "die straffen Beine eines Viergespannes.", "tokens": ["die", "straf\u00b7fen", "Bei\u00b7ne", "ei\u00b7nes", "Vier\u00b7ge\u00b7span\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und wieder Blumen, Perlen, die verrollt sind,", "tokens": ["Und", "wie\u00b7der", "Blu\u00b7men", ",", "Per\u00b7len", ",", "die", "ver\u00b7rollt", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "die hellen Lenden einer kleinen Leier,", "tokens": ["die", "hel\u00b7len", "Len\u00b7den", "ei\u00b7ner", "klei\u00b7nen", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "und zwischen Schleiern, die gleich Nebeln fallen,", "tokens": ["und", "zwi\u00b7schen", "Schlei\u00b7ern", ",", "die", "gleich", "Ne\u00b7beln", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "wie ausgekrochen aus des Schuhes Puppe:", "tokens": ["wie", "aus\u00b7ge\u00b7kro\u00b7chen", "aus", "des", "Schu\u00b7hes", "Pup\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "des Fu\u00dfgelenkes leichter Schmetterling.", "tokens": ["des", "Fu\u00df\u00b7ge\u00b7len\u00b7kes", "leich\u00b7ter", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So liegen sie mit Dingen angef\u00fcllt,", "tokens": ["So", "lie\u00b7gen", "sie", "mit", "Din\u00b7gen", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "kostbaren Dingen, Steinen, Spielzeug, Hausrat,", "tokens": ["kost\u00b7ba\u00b7ren", "Din\u00b7gen", ",", "Stei\u00b7nen", ",", "Spiel\u00b7zeug", ",", "Haus\u00b7rat", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "zerschlagnem Tand (was alles in sie abfiel),", "tokens": ["zer\u00b7schlag\u00b7nem", "Tand", "(", "was", "al\u00b7les", "in", "sie", "ab\u00b7fiel", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "PWS", "PIS", "APPR", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und dunkeln wie der Grund von einem Flu\u00df.", "tokens": ["und", "dun\u00b7keln", "wie", "der", "Grund", "von", "ei\u00b7nem", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Flu\u00dfbetten waren sie,", "tokens": ["Flu\u00df\u00b7bet\u00b7ten", "wa\u00b7ren", "sie", ","], "token_info": ["word", "word", "word", 