{"dta.poem.18172": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Maienblumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu der holden Fr\u00fchlings Zeit ging ich", "tokens": ["Zu", "der", "hol\u00b7den", "Fr\u00fch\u00b7lings", "Zeit", "ging", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "in den gr\u00fcnen Wald,", "tokens": ["in", "den", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zu dem Tempel der Natur, stiller", "tokens": ["Zu", "dem", "Tem\u00b7pel", "der", "Na\u00b7tur", ",", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Andacht Aufenthalt,", "tokens": ["An\u00b7dacht", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "In dem Ehrfurchtsvollen Dun-", "tokens": ["In", "dem", "Ehr\u00b7furchts\u00b7vol\u00b7len", "Dun"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "keln mein Gem\u00fcte zu erquikken,", "tokens": ["keln", "mein", "Ge\u00b7m\u00fc\u00b7te", "zu", "er\u00b7quik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und die holden Maienblumen zum", "tokens": ["Und", "die", "hol\u00b7den", "Mai\u00b7en\u00b7blu\u00b7men", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Vergn\u00fcgen abzupfl\u00fckken.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "ab\u00b7zu\u00b7pfl\u00fck\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich fand ihre weisse Pracht, die aus", "tokens": ["Ich", "fand", "ih\u00b7re", "weis\u00b7se", "Pracht", ",", "die", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "dem beschilften Gr\u00fcn", "tokens": ["dem", "be\u00b7schilf\u00b7ten", "Gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Zwar in tiefer Niedrigkeit, doch mir sehr erweklich,", "tokens": ["Zwar", "in", "tie\u00b7fer", "Nied\u00b7rig\u00b7keit", ",", "doch", "mir", "sehr", "er\u00b7wek\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "KON", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "schien.", "tokens": ["schien", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Ich brach sie von ihren Stengeln, mit wollriechenden", "tokens": ["Ich", "brach", "sie", "von", "ih\u00b7ren", "Sten\u00b7geln", ",", "mit", "woll\u00b7rie\u00b7chen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Geb\u00fcschen,", "tokens": ["Ge\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Woraus s\u00fcsse D\u00fcnste steigen, Lung und Herze zu", "tokens": ["Wo\u00b7raus", "s\u00fcs\u00b7se", "D\u00fcns\u00b7te", "stei\u00b7gen", ",", "Lung", "und", "Her\u00b7ze", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "VVINF", "$,", "NN", "KON", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "erfrischen.", "tokens": ["er\u00b7fri\u00b7schen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Als ich einen dikken Straus in ein kleines Band ge-", "tokens": ["Als", "ich", "ei\u00b7nen", "dik\u00b7ken", "Straus", "in", "ein", "klei\u00b7nes", "Band", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "bracht,", "tokens": ["bracht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Setzt ich mich auf einen Stamm und besahe mit", "tokens": ["Setzt", "ich", "mich", "auf", "ei\u00b7nen", "Stamm", "und", "be\u00b7sa\u00b7he", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "APPR"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Bedacht,", "tokens": ["Be\u00b7dacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Dieser Blumen nette Formen, die an zarten Sten-", "tokens": ["Die\u00b7ser", "Blu\u00b7men", "net\u00b7te", "For\u00b7men", ",", "die", "an", "zar\u00b7ten", "Sten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "geln prangen,", "tokens": ["geln", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Und wie lauter kleine Glokken, in gezierter R\u00fcnde", "tokens": ["Und", "wie", "lau\u00b7ter", "klei\u00b7ne", "Glok\u00b7ken", ",", "in", "ge\u00b7zier\u00b7ter", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "hangen.", "tokens": ["han\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Es gefiel mir die Gestalt und gedachte auch dabei,", "tokens": ["Es", "ge\u00b7fiel", "mir", "die", "Ge\u00b7stalt", "und", "ge\u00b7dach\u00b7te", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Da\u00df ein gr\u00fcn Geb\u00fcsch mit Recht, schon vorl\u00e4ngst", "tokens": ["Da\u00df", "ein", "gr\u00fcn", "Ge\u00b7b\u00fcsch", "mit", "Recht", ",", "schon", "vor\u00b7l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "$,", "ADV", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "genennet sei,", "tokens": ["ge\u00b7nen\u00b7net", "sei", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "Der Natur geweihter Tempel; weil hier ebenfals", "tokens": ["Der", "Na\u00b7tur", "ge\u00b7weih\u00b7ter", "Tem\u00b7pel", ";", "weil", "hier", "e\u00b7ben\u00b7fals"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "die Glokken,", "tokens": ["die", "Glok\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Uns den Sch\u00f6pfer zu verehren; zwar durch keinen", "tokens": ["Uns", "den", "Sch\u00f6p\u00b7fer", "zu", "ver\u00b7eh\u00b7ren", ";", "zwar", "durch", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$.", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Schall anlokken,", "tokens": ["Schall", "an\u00b7lok\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.32": {"text": "Doch mit vieler Anmuth lehrn: da\u00df in einen stillen", "tokens": ["Doch", "mit", "vie\u00b7ler", "An\u00b7muth", "lehrn", ":", "da\u00df", "in", "ei\u00b7nen", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$.", "KOUS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Hain", "tokens": ["Hain"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Viele Dinge anzusehn, die uns Andachts Glokken", "tokens": ["Vie\u00b7le", "Din\u00b7ge", "an\u00b7zu\u00b7sehn", ",", "die", "uns", "An\u00b7dachts", "Glok\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVIZU", "$,", "PRELS", "PPER", "PIS", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "sein;", "tokens": ["sein", ";"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "-", "measure": "single.down"}, "line.36": {"text": "Da\u00df nicht nur der B\u00e4ume Wipfel und der Bl\u00e4tter", "tokens": ["Da\u00df", "nicht", "nur", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "und", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.37": {"text": "rauschend Regen,", "tokens": ["rau\u00b7schend", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Sondern auch die kleinsten Kr\u00e4uter, zu des Sch\u00f6p-", "tokens": ["Son\u00b7dern", "auch", "die", "kleins\u00b7ten", "Kr\u00e4u\u00b7ter", ",", "zu", "des", "Sch\u00f6p"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.39": {"text": "fers Preis bewegen.", "tokens": ["fers", "Preis", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Diese weisse Fr\u00fchlings Schaar, die der Sch\u00f6pfer", "tokens": ["Die\u00b7se", "weis\u00b7se", "Fr\u00fch\u00b7lings", "Schaar", ",", "die", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "ausgestreut,", "tokens": ["aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.42": {"text": "Die dem Augen woll gef\u00e4llt, und uns im Geruch", "tokens": ["Die", "dem", "Au\u00b7gen", "woll", "ge\u00b7f\u00e4llt", ",", "und", "uns", "im", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.43": {"text": "erfreut,", "tokens": ["er\u00b7freut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Uberzeugt uns von der G\u00fcte, die der H\u00f6chste f\u00fcr", "tokens": ["Ub\u00b7er\u00b7zeugt", "uns", "von", "der", "G\u00fc\u00b7te", ",", "die", "der", "H\u00f6chs\u00b7te", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.45": {"text": "uns heget,", "tokens": ["uns", "he\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.46": {"text": "Der in diese Kunst-Gew\u00e4chse so viel s\u00fcssen Nuz ge-", "tokens": ["Der", "in", "die\u00b7se", "Kunst\u00b7Ge\u00b7w\u00e4ch\u00b7se", "so", "viel", "s\u00fcs\u00b7sen", "Nuz", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN", "ADV", "PIAT", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.47": {"text": "leget.", "tokens": ["le\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Das erhellt noch mehr wenn man draus ein lieblich", "tokens": ["Das", "er\u00b7hellt", "noch", "mehr", "wenn", "man", "draus", "ein", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "KOUS", "PIS", "PAV", "ART", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.49": {"text": "Wasser brennt,", "tokens": ["Was\u00b7ser", "brennt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.50": {"text": "Das man wegen seiner Kraft, als ein Lebens Was-", "tokens": ["Das", "man", "we\u00b7gen", "sei\u00b7ner", "Kraft", ",", "als", "ein", "Le\u00b7bens", "Was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.51": {"text": "ser kennt,", "tokens": ["ser", "kennt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.52": {"text": "Und die Aerzte \u00f6fters brauchen und zum Gegenmit-", "tokens": ["Und", "die", "A\u00b7erz\u00b7te", "\u00f6f\u00b7ters", "brau\u00b7chen", "und", "zum", "Ge\u00b7gen\u00b7mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "KON", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.53": {"text": "tel w\u00e4hlen,", "tokens": ["tel", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Wenn der Krankheit bange Uebel den verdorbnen", "tokens": ["Wenn", "der", "Krank\u00b7heit", "ban\u00b7ge", "Ue\u00b7bel", "den", "ver\u00b7dorb\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.55": {"text": "K\u00f6rper qu\u00e4len.", "tokens": ["K\u00f6r\u00b7per", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.56": {"text": "Denken wir dem allen nach; so bezeugt die Maien-", "tokens": ["Den\u00b7ken", "wir", "dem", "al\u00b7len", "nach", ";", "so", "be\u00b7zeugt", "die", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "PIS", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.57": {"text": "blum", "tokens": ["blum"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.58": {"text": "Uns des Sch\u00f6pfers weise G\u00fct und treibt uns zu sei-", "tokens": ["Uns", "des", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "G\u00fct", "und", "treibt", "uns", "zu", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "KON", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.59": {"text": "nen Ruhm;", "tokens": ["nen", "Ruhm", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "li