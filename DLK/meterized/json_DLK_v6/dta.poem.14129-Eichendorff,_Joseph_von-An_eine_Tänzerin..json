{"dta.poem.14129": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An eine T\u00e4nzerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.57", "af:0.42"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Warum sind so bla\u00df die Wangen,", "tokens": ["Wa\u00b7rum", "sind", "so", "bla\u00df", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkelfeucht der Augen Glanz,", "tokens": ["Dun\u00b7kel\u00b7feucht", "der", "Au\u00b7gen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein heimliches Verlangen", "tokens": ["Und", "ein", "heim\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmert gl\u00fchend durch den Tanz?", "tokens": ["Schim\u00b7mert", "gl\u00fc\u00b7hend", "durch", "den", "Tanz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schalkhaft lockend schaust Du nieder,", "tokens": ["Schalk\u00b7haft", "lo\u00b7ckend", "schaust", "Du", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebesnacht", "tokens": ["Lie\u00b7bes\u00b7nacht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "S\u00fc\u00dferwacht,", "tokens": ["S\u00fc\u00b7\u00dfer\u00b7wacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Woll\u00fcstig erklingen Lieder \u2014", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "er\u00b7klin\u00b7gen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Schlag nicht so die Augen nieder!", "tokens": ["Schlag", "nicht", "so", "die", "Au\u00b7gen", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}