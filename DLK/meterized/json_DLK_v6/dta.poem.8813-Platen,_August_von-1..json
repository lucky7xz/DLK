{"dta.poem.8813": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chtest du an seiner Stelle kommen mit dem Weine nur!", "tokens": ["M\u00f6ch\u00b7test", "du", "an", "sei\u00b7ner", "Stel\u00b7le", "kom\u00b7men", "mit", "dem", "Wei\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sprich, warum, wenn auf den Stra\u00dfen ich begegne dir,", "tokens": ["Sprich", ",", "wa\u00b7rum", ",", "wenn", "auf", "den", "Stra\u00b7\u00dfen", "ich", "be\u00b7geg\u00b7ne", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Statt in's Auge mir zu blicken, blickst du auf die Steine", "tokens": ["Statt", "in's", "Au\u00b7ge", "mir", "zu", "bli\u00b7cken", ",", "blickst", "du", "auf", "die", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}