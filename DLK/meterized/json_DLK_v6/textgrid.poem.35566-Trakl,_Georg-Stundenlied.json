{"textgrid.poem.35566": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Stundenlied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dunklen Blicken sehen sich die Liebenden an,", "tokens": ["Mit", "dunk\u00b7len", "Bli\u00b7cken", "se\u00b7hen", "sich", "die", "Lie\u00b7ben\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Die Blonden, Strahlenden. In starrender Finsternis", "tokens": ["Die", "Blon\u00b7den", ",", "Strah\u00b7len\u00b7den", ".", "In", "star\u00b7ren\u00b7der", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Umschlingen schm\u00e4chtig sich die sehnenden Arme.", "tokens": ["Um\u00b7schlin\u00b7gen", "schm\u00e4ch\u00b7tig", "sich", "die", "seh\u00b7nen\u00b7den", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Purpurn zerbrach der Gesegneten Mund. Die runden Augen", "tokens": ["Pur\u00b7purn", "zer\u00b7brach", "der", "Ge\u00b7se\u00b7gne\u00b7ten", "Mund", ".", "Die", "run\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Spiegeln das dunkle Gold des Fr\u00fchlingsnachmittags,", "tokens": ["Spie\u00b7geln", "das", "dunk\u00b7le", "Gold", "des", "Fr\u00fch\u00b7lings\u00b7nach\u00b7mit\u00b7tags", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Saum und Schw\u00e4rze des Walds, Abend\u00e4ngste im Gr\u00fcn;", "tokens": ["Saum", "und", "Schw\u00e4r\u00b7ze", "des", "Walds", ",", "A\u00b7ben\u00b7d\u00e4ngs\u00b7te", "im", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Vielleicht uns\u00e4glichen Vogelflug, des Ungeborenen", "tokens": ["Viel\u00b7leicht", "un\u00b7s\u00e4g\u00b7li\u00b7chen", "Vo\u00b7gel\u00b7flug", ",", "des", "Un\u00b7ge\u00b7bo\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Pfad an finsteren D\u00f6rfern, einsamen Sommern hin", "tokens": ["Pfad", "an", "fins\u00b7te\u00b7ren", "D\u00f6r\u00b7fern", ",", "ein\u00b7sa\u00b7men", "Som\u00b7mern", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und aus verfallener Bl\u00e4ue tritt bisweilen ein Abgelebtes.", "tokens": ["Und", "aus", "ver\u00b7fal\u00b7le\u00b7ner", "Bl\u00e4u\u00b7e", "tritt", "bis\u00b7wei\u00b7len", "ein", "Ab\u00b7ge\u00b7leb\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Leise rauscht im Acker das gelbe Korn.", "tokens": ["Lei\u00b7se", "rauscht", "im", "A\u00b7cker", "das", "gel\u00b7be", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Hart ist das Leben und st\u00e4hlern schwingt die Sense der Landmann,", "tokens": ["Hart", "ist", "das", "Le\u00b7ben", "und", "st\u00e4h\u00b7lern", "schwingt", "die", "Sen\u00b7se", "der", "Land\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fcgt gewaltige Balken der Zimmermann.", "tokens": ["F\u00fcgt", "ge\u00b7wal\u00b7ti\u00b7ge", "Bal\u00b7ken", "der", "Zim\u00b7mer\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Purpurn f\u00e4rbt sich das Laub im Herbst; der m\u00f6nchische Geist", "tokens": ["Pur\u00b7purn", "f\u00e4rbt", "sich", "das", "Laub", "im", "Herbst", ";", "der", "m\u00f6n\u00b7chi\u00b7sche", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durchwandelt heitere Tage; reif ist die Traube", "tokens": ["Durch\u00b7wan\u00b7delt", "hei\u00b7te\u00b7re", "Ta\u00b7ge", ";", "reif", "ist", "die", "Trau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und festlich die Luft in ger\u00e4umigen H\u00f6fen.", "tokens": ["Und", "fest\u00b7lich", "die", "Luft", "in", "ge\u00b7r\u00e4u\u00b7mi\u00b7gen", "H\u00f6\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "S\u00fc\u00dfer duften vergilbte Fr\u00fcchte; leise ist das Lachen", "tokens": ["S\u00fc\u00b7\u00dfer", "duf\u00b7ten", "ver\u00b7gilb\u00b7te", "Fr\u00fcch\u00b7te", ";", "lei\u00b7se", "ist", "das", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN", "$.", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Des Frohen, Musik und Tanz in schattigen Kellern;", "tokens": ["Des", "Fro\u00b7hen", ",", "Mu\u00b7sik", "und", "Tanz", "in", "schat\u00b7ti\u00b7gen", "Kel\u00b7lern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Im d\u00e4mmernden Garten Schritt und Stille des verstorbenen Knaben.", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Gar\u00b7ten", "Schritt", "und", "Stil\u00b7le", "des", "ver\u00b7stor\u00b7be\u00b7nen", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}}}}}