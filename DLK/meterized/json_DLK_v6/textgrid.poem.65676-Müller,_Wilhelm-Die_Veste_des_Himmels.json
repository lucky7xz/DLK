{"textgrid.poem.65676": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Veste des Himmels", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Asia hat ausgespieen ihre gelbe Tigerbrut,", "tokens": ["A\u00b7sia", "hat", "aus\u00b7ge\u00b7spi\u00b7een", "ih\u00b7re", "gel\u00b7be", "Ti\u00b7ger\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Da\u00df sie purpurroth sich trinke in der Griechenkinder Blut;", "tokens": ["Da\u00df", "sie", "pur\u00b7pur\u00b7roth", "sich", "trin\u00b7ke", "in", "der", "Grie\u00b7chen\u00b7kin\u00b7der", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Afrika aus ihren W\u00fcsten st\u00fcrmet \u00fcber Hellas Meer", "tokens": ["Af\u00b7ri\u00b7ka", "aus", "ih\u00b7ren", "W\u00fcs\u00b7ten", "st\u00fcr\u00b7met", "\u00fc\u00b7ber", "Hel\u00b7las", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mit des Samums Todeshauche ihre Negerhorden her.", "tokens": ["Mit", "des", "Sa\u00b7mums", "To\u00b7des\u00b7hau\u00b7che", "ih\u00b7re", "Ne\u00b7ger\u00b7hor\u00b7den", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Missolunghi, Stadt der Helden, la\u00df die Kreuzesfahne wehn!", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", ",", "Stadt", "der", "Hel\u00b7den", ",", "la\u00df", "die", "Kreu\u00b7zes\u00b7fah\u00b7ne", "wehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Z\u00e4hle nicht die Ungez\u00e4hlten, die vor deinen Mauern stehn!", "tokens": ["Z\u00e4h\u00b7le", "nicht", "die", "Un\u00b7ge\u00b7z\u00e4hl\u00b7ten", ",", "die", "vor", "dei\u00b7nen", "Mau\u00b7ern", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Z\u00e4hle nicht des Waldes Bl\u00e4tter, z\u00e4hle nicht den Sand am Meer,", "tokens": ["Z\u00e4h\u00b7le", "nicht", "des", "Wal\u00b7des", "Bl\u00e4t\u00b7ter", ",", "z\u00e4h\u00b7le", "nicht", "den", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "In des Himmels Feldern z\u00e4hle deines Gottes Sternenheer.", "tokens": ["In", "des", "Him\u00b7mels", "Fel\u00b7dern", "z\u00e4h\u00b7le", "dei\u00b7nes", "Got\u00b7tes", "Ster\u00b7nen\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ob sich deine Tonnen leeren, deine Scheuern werden licht,", "tokens": ["Ob", "sich", "dei\u00b7ne", "Ton\u00b7nen", "lee\u00b7ren", ",", "dei\u00b7ne", "Scheu\u00b7ern", "wer\u00b7den", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "W\u00e4ge nicht den letzten Brocken, mi\u00df den letzten Tropfen nicht.", "tokens": ["W\u00e4\u00b7ge", "nicht", "den", "letz\u00b7ten", "Bro\u00b7cken", ",", "mi\u00df", "den", "letz\u00b7ten", "Trop\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Hat dein Heiland mit f\u00fcnf Brodten nicht f\u00fcnf Tausende gespeist?", "tokens": ["Hat", "dein", "Hei\u00b7land", "mit", "f\u00fcnf", "Brod\u00b7ten", "nicht", "f\u00fcnf", "Tau\u00b7sen\u00b7de", "ge\u00b7speist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "PTKNEG", "CARD", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Bete, bis vor deinem Rufe sich des Himmels Zelt zerrei\u00dft!", "tokens": ["Be\u00b7te", ",", "bis", "vor", "dei\u00b7nem", "Ru\u00b7fe", "sich", "des", "Him\u00b7mels", "Zelt", "zer\u00b7rei\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Manna regnet's aus den Wolken auf der W\u00fcste d\u00fcrren Sand:", "tokens": ["Man\u00b7na", "reg\u00b7net's", "aus", "den", "Wol\u00b7ken", "auf", "der", "W\u00fcs\u00b7te", "d\u00fcr\u00b7ren", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Gott hat Manna f\u00fcr euch alle \u2013 Streckt nur aus die matte Hand!", "tokens": ["Gott", "hat", "Man\u00b7na", "f\u00fcr", "euch", "al\u00b7le", "\u2013", "Streckt", "nur", "aus", "die", "mat\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "APPR", "PPER", "PIS", "$(", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "Missolunghi, Stadt der Helden, wach' und bete Tag und Nacht!", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", ",", "Stadt", "der", "Hel\u00b7den", ",", "wach'", "und", "be\u00b7te", "Tag", "und", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Sieh, in ihren tiefen Gr\u00fcften sind die Todten auch erwacht.", "tokens": ["Sieh", ",", "in", "ih\u00b7ren", "tie\u00b7fen", "Gr\u00fcf\u00b7ten", "sind", "die", "Tod\u00b7ten", "auch", "er\u00b7wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Sieh, auf deinen W\u00e4llen schreiten ihre Geister hoch daher,", "tokens": ["Sieh", ",", "auf", "dei\u00b7nen", "W\u00e4l\u00b7len", "schrei\u00b7ten", "ih\u00b7re", "Geis\u00b7ter", "hoch", "da\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Flammenschwerter in den H\u00e4nden, doch die Wunden leuchten mehr.", "tokens": ["Flam\u00b7men\u00b7schwer\u00b7ter", "in", "den", "H\u00e4n\u00b7den", ",", "doch", "die", "Wun\u00b7den", "leuch\u00b7ten", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Markos, Suli's K\u00f6nigsadler, sucht der j\u00e4hen Zinne Stand,", "tokens": ["Mar\u00b7kos", ",", "Su\u00b7li's", "K\u00f6\u00b7ni\u00b7gsad\u00b7ler", ",", "sucht", "der", "j\u00e4\u00b7hen", "Zin\u00b7ne", "Stand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und den deutschen Grafen f\u00fchrt er br\u00fcderlich an seiner Hand", "tokens": ["Und", "den", "deut\u00b7schen", "Gra\u00b7fen", "f\u00fchrt", "er", "br\u00fc\u00b7der\u00b7lich", "an", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Aber einsam auch im Tode schleicht der Brittens\u00e4nger hin,", "tokens": ["A\u00b7ber", "ein\u00b7sam", "auch", "im", "To\u00b7de", "schleicht", "der", "Brit\u00b7ten\u00b7s\u00e4n\u00b7ger", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Dem des Lebens R\u00e4thsel schweben dunkel noch vor seinem Sinn.", "tokens": ["Dem", "des", "Le\u00b7bens", "R\u00e4th\u00b7sel", "schwe\u00b7ben", "dun\u00b7kel", "noch", "vor", "sei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Durch die Sterne kreist sein Auge, eine Antwort zu ersp\u00e4hn:", "tokens": ["Durch", "die", "Ster\u00b7ne", "kreist", "sein", "Au\u00b7ge", ",", "ei\u00b7ne", "Ant\u00b7wort", "zu", "er\u00b7sp\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Herrscht der Christen Gott dort oben, und mu\u00df Hellas untergehn?", "tokens": ["Herrscht", "der", "Chris\u00b7ten", "Gott", "dort", "o\u00b7ben", ",", "und", "mu\u00df", "Hel\u00b7las", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ADV", "$,", "KON", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Missolunghi, Stadt der Helden, Hellas Hort und Ehrenstern,", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", ",", "Stadt", "der", "Hel\u00b7den", ",", "Hel\u00b7las", "Hort", "und", "Eh\u00b7rens\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "NE", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.26": {"text": "Schmach der Heiden, Stolz der Christen, Missolunghi, Stadt des Herrn,", "tokens": ["Schmach", "der", "Hei\u00b7den", ",", "Stolz", "der", "Chris\u00b7ten", ",", "Mis\u00b7so\u00b7lung\u00b7hi", ",", "Stadt", "des", "Herrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Deine martyrfesten Mauern werden nimmer untergehn:", "tokens": ["Dei\u00b7ne", "mar\u00b7tyr\u00b7fes\u00b7ten", "Mau\u00b7ern", "wer\u00b7den", "nim\u00b7mer", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Ist die Erde dein nicht w\u00fcrdig, wirst du einst im Himmel stehn,", "tokens": ["Ist", "die", "Er\u00b7de", "dein", "nicht", "w\u00fcr\u00b7dig", ",", "wirst", "du", "einst", "im", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "PTKNEG", "ADJD", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Als die W\u00e4chterin des Thrones, wann des H\u00f6llenf\u00fcrsten Macht", "tokens": ["Als", "die", "W\u00e4ch\u00b7te\u00b7rin", "des", "Thro\u00b7nes", ",", "wann", "des", "H\u00f6l\u00b7len\u00b7f\u00fcrs\u00b7ten", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Wider Gott sich will emp\u00f6ren und die Engel ruft zur Schlacht.", "tokens": ["Wi\u00b7der", "Gott", "sich", "will", "em\u00b7p\u00f6\u00b7ren", "und", "die", "En\u00b7gel", "ruft", "zur", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VMFIN", "VVINF", "KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}