{"textgrid.poem.62340": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Amor, lieber Amor!", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Amor, lieber Amor!", "tokens": ["Ach", "A\u00b7mor", ",", "lie\u00b7ber", "A\u00b7mor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon an dem ersten May", "tokens": ["Schon", "an", "dem", "ers\u00b7ten", "May"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Baut in des Gartens Ecke", "tokens": ["Baut", "in", "des", "Gar\u00b7tens", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich den Altar f\u00fcr dich,", "tokens": ["Ich", "den", "Al\u00b7tar", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und pflanzte Rosenhecken", "tokens": ["Und", "pflanz\u00b7te", "Ro\u00b7sen\u00b7he\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Myrthen dr\u00fcber her:", "tokens": ["Und", "Myr\u00b7then", "dr\u00fc\u00b7ber", "her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und lag nicht jeden Morgen", "tokens": ["Und", "lag", "nicht", "je\u00b7den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Thauvoll ein Blumenkranz", "tokens": ["Thau\u00b7voll", "ein", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Auf deines Altars Mitte?", "tokens": ["Auf", "dei\u00b7nes", "Al\u00b7tars", "Mit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ach alles war umsonst!", "tokens": ["Ach", "al\u00b7les", "war", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Schon streifen Winterwinde", "tokens": ["Schon", "strei\u00b7fen", "Win\u00b7ter\u00b7win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das Laub von Baum und Strauch,", "tokens": ["Das", "Laub", "von", "Baum", "und", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und Phillis ist noch spr\u00f6de,", "tokens": ["Und", "Phil\u00b7lis", "ist", "noch", "spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Spr\u00f6d wie am ersten May.", "tokens": ["Spr\u00f6d", "wie", "am", "ers\u00b7ten", "May", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}