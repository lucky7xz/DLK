{"dta.poem.6935": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch welche auch die kleinsten Z\u00e4serlein", "tokens": ["Durch", "wel\u00b7che", "auch", "die", "kleins\u00b7ten", "Z\u00e4\u00b7ser\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jm allerkleinsten Kraut getrieben seyn.", "tokens": ["Jm", "al\u00b7ler\u00b7kleins\u00b7ten", "Kraut", "ge\u00b7trie\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat er ein eintzigs mal in allen seinen Wercken,", "tokens": ["Hat", "er", "ein", "eint\u00b7zigs", "mal", "in", "al\u00b7len", "sei\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur einen Fehl im Brauch und Endzweck k\u00f6nnen mercken?", "tokens": ["Nur", "ei\u00b7nen", "Fehl", "im", "Brauch", "und", "End\u00b7zweck", "k\u00f6n\u00b7nen", "mer\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur f\u00fcr den Geist ist alles blos gemacht,", "tokens": ["Nur", "f\u00fcr", "den", "Geist", "ist", "al\u00b7les", "blos", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Handlungen, die man als leiblich nur betracht,", "tokens": ["Die", "Hand\u00b7lun\u00b7gen", ",", "die", "man", "als", "leib\u00b7lich", "nur", "be\u00b7tracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "ADJD", "ADV", "NN", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Sind blos, zu Handlungen des Geists, Gelegenheiten.", "tokens": ["Sind", "blos", ",", "zu", "Hand\u00b7lun\u00b7gen", "des", "Geists", ",", "Ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was man an C\u00f6rpern findt f\u00fcr Herrlichkeiten", "tokens": ["Was", "man", "an", "C\u00f6r\u00b7pern", "findt", "f\u00fcr", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und alles, was an ihnen sch\u00f6n,", "tokens": ["Und", "al\u00b7les", ",", "was", "an", "ih\u00b7nen", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "L\u00e4sst uns nichts, als die GOTTHEJT sehn,", "tokens": ["L\u00e4sst", "uns", "nichts", ",", "als", "die", "GoTTHEJT", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und seine Vollenkommenheiten.", "tokens": ["Und", "sei\u00b7ne", "Vol\u00b7len\u00b7kom\u00b7men\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}