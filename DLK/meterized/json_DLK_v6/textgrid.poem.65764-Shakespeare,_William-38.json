{"textgrid.poem.65764": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "38", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6nnt' es meiner Mus' an Stoffgebrechen", "tokens": ["Wie", "k\u00f6nnt'", "es", "mei\u00b7ner", "Mus'", "an", "Stoff\u00b7ge\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Solang du atmest, der du mein Gedicht", "tokens": ["So\u00b7lang", "du", "at\u00b7mest", ",", "der", "du", "mein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchstr\u00f6mst mit deines Wesens holden B\u00e4chen,", "tokens": ["Durch\u00b7str\u00f6mst", "mit", "dei\u00b7nes", "We\u00b7sens", "hol\u00b7den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das jeden niedern Kiel hoch \u00fcberfliegt?", "tokens": ["Das", "je\u00b7den", "nie\u00b7dern", "Kiel", "hoch", "\u00fc\u00b7berf\u00b7liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O danke du dir selbst, wenn lesenswert", "tokens": ["O", "dan\u00b7ke", "du", "dir", "selbst", ",", "wenn", "le\u00b7sens\u00b7wert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In deinen Augen etwas scheint an mir.", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "et\u00b7was", "scheint", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer w\u00e4r' so stumm, den du nicht Schrift gelehrt?", "tokens": ["Wer", "w\u00e4r'", "so", "stumm", ",", "den", "du", "nicht", "Schrift", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "PRELS", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Leiht nicht Erfindung selbst ihr Licht von dir?", "tokens": ["Leiht", "nicht", "Er\u00b7fin\u00b7dung", "selbst", "ihr", "Licht", "von", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sei du die zehnte Muse, zehnmal reicher", "tokens": ["Sei", "du", "die", "zehn\u00b7te", "Mu\u00b7se", ",", "zehn\u00b7mal", "rei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als jene alten neun, zu denen Reimer flehn;", "tokens": ["Als", "je\u00b7ne", "al\u00b7ten", "neun", ",", "zu", "de\u00b7nen", "Rei\u00b7mer", "flehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "CARD", "$,", "APPR", "PRELS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wer dich anruft, ew'ge Lieder zeug' er,", "tokens": ["Und", "wer", "dich", "an\u00b7ruft", ",", "ew'\u00b7ge", "Lie\u00b7der", "zeug'", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die aller Zeit Verw\u00fcstung \u00fcberstehn!", "tokens": ["Die", "al\u00b7ler", "Zeit", "Ver\u00b7w\u00fcs\u00b7tung", "\u00fc\u00b7bers\u00b7tehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Behagt mein leichter Sang der feinen Zeit,", "tokens": ["Be\u00b7hagt", "mein", "leich\u00b7ter", "Sang", "der", "fei\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sei mein die M\u00fch, dein die Zufriedenheit.", "tokens": ["Sei", "mein", "die", "M\u00fch", ",", "dein", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "$,", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}