{"textgrid.poem.54993": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Liebhaber in allen Gestalten", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt, ich w\u00e4r ein Fisch,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "ein", "Fisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So hurtig und frisch;", "tokens": ["So", "hur\u00b7tig", "und", "frisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und k\u00e4mst du zu anglen,", "tokens": ["Und", "k\u00e4mst", "du", "zu", "ang\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich w\u00fcrde nicht manglen.", "tokens": ["Ich", "w\u00fcr\u00b7de", "nicht", "mang\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich wollt, ich w\u00e4r ein Fisch,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "ein", "Fisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So hurtig und frisch.", "tokens": ["So", "hur\u00b7tig", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich wollt, ich w\u00e4r ein Pferd,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "ein", "Pferd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da w\u00e4r ich dir wert.", "tokens": ["Da", "w\u00e4r", "ich", "dir", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O w\u00e4r ich ein Wagen,", "tokens": ["O", "w\u00e4r", "ich", "ein", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bequem dich zu tragen.", "tokens": ["Be\u00b7quem", "dich", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich wollt, ich w\u00e4r ein Pferd,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "ein", "Pferd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da w\u00e4r ich dir wert.", "tokens": ["Da", "w\u00e4r", "ich", "dir", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich wollt, ich w\u00e4re Gold,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4\u00b7re", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dir immer im Sold;", "tokens": ["Dir", "im\u00b7mer", "im", "Sold", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und t\u00e4tst du was kaufen,", "tokens": ["Und", "t\u00e4tst", "du", "was", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "K\u00e4m ich wieder gelaufen.", "tokens": ["K\u00e4m", "ich", "wie\u00b7der", "ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ich wollt, ich w\u00e4re Gold,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4\u00b7re", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dir immer im Sold.", "tokens": ["Dir", "im\u00b7mer", "im", "Sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ich wollt, ich w\u00e4r treu,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein Liebchen stets neu;", "tokens": ["Mein", "Lieb\u00b7chen", "stets", "neu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich wollt mich verhei\u00dfen,", "tokens": ["Ich", "wollt", "mich", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wollt nimmer verreisen.", "tokens": ["Wollt", "nim\u00b7mer", "ver\u00b7rei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich wollt, ich w\u00e4r treu,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mein Liebchen stets neu.", "tokens": ["Mein", "Lieb\u00b7chen", "stets", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ich wollt, ich w\u00e4r alt", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "alt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und runzlig und kalt;", "tokens": ["Und", "runz\u00b7lig", "und", "kalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "T\u00e4tst du mir's versagen,", "tokens": ["T\u00e4tst", "du", "mir's", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da k\u00f6nnt mich's nicht plagen.", "tokens": ["Da", "k\u00f6nnt", "mich's", "nicht", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich wollt, ich w\u00e4r alt", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "alt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und runzlig und kalt.", "tokens": ["Und", "runz\u00b7lig", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "W\u00e4r ich Affe sogleich,", "tokens": ["W\u00e4r", "ich", "Af\u00b7fe", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Voll neckender Streich';", "tokens": ["Voll", "ne\u00b7cken\u00b7der", "Streich'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4tt was dich verdrossen,", "tokens": ["H\u00e4tt", "was", "dich", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So macht ich dir Possen.", "tokens": ["So", "macht", "ich", "dir", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "W\u00e4r ich Affe sogleich,", "tokens": ["W\u00e4r", "ich", "Af\u00b7fe", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Voll neckender Streich'.", "tokens": ["Voll", "ne\u00b7cken\u00b7der", "Streich'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "W\u00e4r ich gut wie ein Schaf,", "tokens": ["W\u00e4r", "ich", "gut", "wie", "ein", "Schaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie der L\u00f6we so brav;", "tokens": ["Wie", "der", "L\u00f6\u00b7we", "so", "brav", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4tt Augen wie's L\u00fcchschen", "tokens": ["H\u00e4tt", "Au\u00b7gen", "wie's", "L\u00fcch\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Listen wie's F\u00fcchschen.", "tokens": ["Und", "Lis\u00b7ten", "wie's", "F\u00fcch\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "W\u00e4r ich gut wie ein Schaf,", "tokens": ["W\u00e4r", "ich", "gut", "wie", "ein", "Schaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie der L\u00f6we so brav.", "tokens": ["Wie", "der", "L\u00f6\u00b7we", "so", "brav", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was alles ich w\u00e4r,", "tokens": ["Was", "al\u00b7les", "ich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Das g\u00f6nnt ich dir sehr;", "tokens": ["Das", "g\u00f6nnt", "ich", "dir", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit f\u00fcrstlichen Gaben,", "tokens": ["Mit", "f\u00fcrst\u00b7li\u00b7chen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du solltest mich haben.", "tokens": ["Du", "soll\u00b7test", "mich", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was alles ich w\u00e4r,", "tokens": ["Was", "al\u00b7les", "ich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das g\u00f6nnt ich dir sehr.", "tokens": ["Das", "g\u00f6nnt", "ich", "dir", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Doch bin ich, wie ich bin,", "tokens": ["Doch", "bin", "ich", ",", "wie", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und nimm mich nur hin!", "tokens": ["Und", "nimm", "mich", "nur", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Willst du be\u00dfre besitzen,", "tokens": ["Willst", "du", "be\u00df\u00b7re", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So la\u00df dir sie schnitzen.", "tokens": ["So", "la\u00df", "dir", "sie", "schnit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich bin nun, wie ich bin;", "tokens": ["Ich", "bin", "nun", ",", "wie", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So nimm mich nur hin!", "tokens": ["So", "nimm", "mich", "nur", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}