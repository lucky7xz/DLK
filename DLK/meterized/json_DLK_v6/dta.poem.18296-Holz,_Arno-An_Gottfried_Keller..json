{"dta.poem.18296": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An Gottfried Keller.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit steht in deiner Schuld.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "steht", "in", "dei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch deine Verse blitzt und rollt", "tokens": ["Durch", "dei\u00b7ne", "Ver\u00b7se", "blitzt", "und", "rollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Goethe'sches Gold!", "tokens": ["Goe\u00b7the'\u00b7sches", "Gold", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte dich bis in den Himmel heben,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "bis", "in", "den", "Him\u00b7mel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach, du glaubst ja nicht an ihn,", "tokens": ["Doch", "ach", ",", "du", "glaubst", "ja", "nicht", "an", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nur die Erde tr\u00e4gt dir Reben,", "tokens": ["Denn", "nur", "die", "Er\u00b7de", "tr\u00e4gt", "dir", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rothe Rosen und wei\u00dfen Jasmin.", "tokens": ["Ro\u00b7the", "Ro\u00b7sen", "und", "wei\u00b7\u00dfen", "Jas\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du bist mir auf hundert von Meilen entr\u00fcckt,", "tokens": ["Du", "bist", "mir", "auf", "hun\u00b7dert", "von", "Mei\u00b7len", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "CARD", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch hab ich dir oft schon die Hand gedr\u00fcckt", "tokens": ["Doch", "hab", "ich", "dir", "oft", "schon", "die", "Hand", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und jauchz dir nun zu durch Nebel und Dunst", "tokens": ["Und", "jauchz", "dir", "nun", "zu", "durch", "Ne\u00b7bel", "und", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKZU", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das alte Spr\u00fcchlein: \u201eGott gr\u00fc\u00df die Kunst!\u201c", "tokens": ["Das", "al\u00b7te", "Spr\u00fcch\u00b7lein", ":", "\u201e", "Gott", "gr\u00fc\u00df", "die", "Kunst", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}