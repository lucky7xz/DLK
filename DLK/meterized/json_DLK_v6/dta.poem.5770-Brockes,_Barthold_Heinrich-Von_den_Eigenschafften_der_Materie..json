{"dta.poem.5770": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin nun werden uns doch die Gedancken f\u00fchren?", "tokens": ["Wo\u00b7hin", "nun", "wer\u00b7den", "uns", "doch", "die", "Ge\u00b7dan\u00b7cken", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welch unendlich klein\u2019", "tokens": ["In", "welch", "un\u00b7end\u00b7lich", "klein'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zarte Theile kan von uns zertheilet seyn", "tokens": ["Und", "zar\u00b7te", "Thei\u00b7le", "kan", "von", "uns", "zer\u00b7thei\u00b7let", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein C\u00f6rpergen, das nicht einmal zu sp\u00fcren!", "tokens": ["Ein", "C\u00f6r\u00b7per\u00b7gen", ",", "das", "nicht", "ein\u00b7mal", "zu", "sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man trifft ein kleines Thier, ein Sonnen-St\u00e4ubgen an,", "tokens": ["Man", "trifft", "ein", "klei\u00b7nes", "Thier", ",", "ein", "Son\u00b7nen\u00b7St\u00e4ub\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das ein Vergr\u00f6\u00dfrungs-Glas kaum sichtbar machen kan,", "tokens": ["Das", "ein", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7Glas", "kaum", "sicht\u00b7bar", "ma\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel tausend tausendmal so klein", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send\u00b7mal", "so", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als wie die kleinsten M\u00fclben seyn;", "tokens": ["Als", "wie", "die", "kleins\u00b7ten", "M\u00fcl\u00b7ben", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dennoch sp\u00fcren wir", "tokens": ["Und", "den\u00b7noch", "sp\u00fc\u00b7ren", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "An ihm, da\u00df es ein w\u00fcrcklich Thier.", "tokens": ["An", "ihm", ",", "da\u00df", "es", "ein", "w\u00fcrck\u00b7lich", "Thier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.11": {"text": "Was hat f\u00fcr eine Kunst die deutliche Figur", "tokens": ["Was", "hat", "f\u00fcr", "ei\u00b7ne", "Kunst", "die", "deut\u00b7li\u00b7che", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Formiret und hervorgebracht?", "tokens": ["For\u00b7mi\u00b7ret", "und", "her\u00b7vor\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch welches Werckzeug, welche Krafft,", "tokens": ["Durch", "wel\u00b7ches", "Wer\u00b7ck\u00b7zeug", ",", "wel\u00b7che", "Krafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wird ihm, zu seinem Nahrungs-Safft,", "tokens": ["Wird", "ihm", ",", "zu", "sei\u00b7nem", "Nah\u00b7rungs\u00b7\u00b7S\u00b7afft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.15": {"text": "Die Nahrung doch geschickt gemacht?", "tokens": ["Die", "Nah\u00b7rung", "doch", "ge\u00b7schickt", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und welche Geisterchen sind es, die stetig eilen,", "tokens": ["Und", "wel\u00b7che", "Geis\u00b7ter\u00b7chen", "sind", "es", ",", "die", "ste\u00b7tig", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.17": {"text": "Und die sich in der Augen Nerv\u2019gen r\u00fchren?", "tokens": ["Und", "die", "sich", "in", "der", "Au\u00b7gen", "Ner\u00b7v'\u00b7gen", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Indessen k\u00f6nnten doch von allen diesen Theilen", "tokens": ["In\u00b7des\u00b7sen", "k\u00f6nn\u00b7ten", "doch", "von", "al\u00b7len", "die\u00b7sen", "Thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "PIAT", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Feuchtigkeit, woraus die Geister sich formiren,", "tokens": ["Der", "Feuch\u00b7tig\u00b7keit", ",", "wo\u00b7raus", "die", "Geis\u00b7ter", "sich", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ob sie gleich noch so klein,", "tokens": ["Ob", "sie", "gleich", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die Theil ohn Unterla\u00df aufs neu verkleinert seyn.", "tokens": ["Die", "Theil", "ohn", "Un\u00b7ter\u00b7la\u00df", "aufs", "neu", "ver\u00b7klei\u00b7nert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "O Wunder der Natur! das wahr, gewi\u00df und fest,", "tokens": ["O", "Wun\u00b7der", "der", "Na\u00b7tur", "!", "das", "wahr", ",", "ge\u00b7wi\u00df", "und", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "ART", "ADJD", "$,", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ob es sich gleich nicht recht begreiffen l\u00e4sst.", "tokens": ["Ob", "es", "sich", "gleich", "nicht", "recht", "be\u00b7greif\u00b7fen", "l\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}