{"dta.poem.21781": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Meinet halben/ fahr immer  \n hin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "So hat dem nu die eine Nacht", "tokens": ["So", "hat", "dem", "nu", "die", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein Tag treu-br\u00fcchig dich gemacht.", "tokens": ["ein", "Tag", "treu\u00b7br\u00fc\u00b7chig", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das hei\u00dft mit falschen Eydes-schw\u00fcren", "tokens": ["das", "hei\u00dft", "mit", "fal\u00b7schen", "Ey\u00b7des\u00b7schw\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein allzu gl\u00e4ubig Kind verf\u00fchren.", "tokens": ["ein", "all\u00b7zu", "gl\u00e4u\u00b7big", "Kind", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich war ia noch in Szyten/", "tokens": ["Ich", "war", "i\u00b7a", "noch", "in", "Szy\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "noch wo ein schwarzes Mohr-gesicht", "tokens": ["noch", "wo", "ein", "schwar\u00b7zes", "Mohr\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in Afriken im Schweisse fliesset/", "tokens": ["in", "Af\u00b7ri\u00b7ken", "im", "Schweis\u00b7se", "flies\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "noch wo der Tyger sich ergiesset.", "tokens": ["noch", "wo", "der", "Ty\u00b7ger", "sich", "er\u00b7gies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja/ wenn mein Schiff im Meere st\u00fcnd", "tokens": ["Ja", "/", "wenn", "mein", "Schiff", "im", "Mee\u00b7re", "st\u00fcnd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und mich ein ungest\u00fcmer Wind", "tokens": ["und", "mich", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wor h\u00e4tt\u2019 in Indien getragen", "tokens": ["wor", "h\u00e4tt'", "in", "In\u00b7di\u00b7en", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}