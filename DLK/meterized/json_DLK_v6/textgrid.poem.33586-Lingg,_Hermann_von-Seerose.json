{"textgrid.poem.33586": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Seerose", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rote Rosen, stolz und pr\u00e4chtig,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "stolz", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchen in der G\u00e4rten Rund,", "tokens": ["Bl\u00fc\u00b7hen", "in", "der", "G\u00e4r\u00b7ten", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine wei\u00dfe wiegt sich n\u00e4chtig,", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "wiegt", "sich", "n\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wurzelnd in der Welle Grund.", "tokens": ["Wur\u00b7zelnd", "in", "der", "Wel\u00b7le", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre zarten bleichen Wangen", "tokens": ["Ih\u00b7re", "zar\u00b7ten", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4rbte nie der Erde Lust,", "tokens": ["F\u00e4rb\u00b7te", "nie", "der", "Er\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein stilles Traumverlangen", "tokens": ["Nur", "ein", "stil\u00b7les", "Traum\u00b7ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb das Sehnen ihrer Brust.", "tokens": ["Blieb", "das", "Seh\u00b7nen", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gerne spr\u00e4ch' sie mit den Sternen,", "tokens": ["Ger\u00b7ne", "spr\u00e4ch'", "sie", "mit", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wenn sie kaum erwacht,", "tokens": ["A\u00b7ber", "wenn", "sie", "kaum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen jene sich entfernen,", "tokens": ["M\u00fcs\u00b7sen", "je\u00b7ne", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgend ihrer Mutter Nacht.", "tokens": ["Fol\u00b7gend", "ih\u00b7rer", "Mut\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Goldne Bl\u00e4tter wirft hernieder", "tokens": ["Gold\u00b7ne", "Bl\u00e4t\u00b7ter", "wirft", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Gestad ein stolzer Baum,", "tokens": ["Vom", "Ge\u00b7stad", "ein", "stol\u00b7zer", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie hascht darnach, und wieder", "tokens": ["Und", "sie", "hascht", "dar\u00b7nach", ",", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War es nichts als nur ein Traum.", "tokens": ["War", "es", "nichts", "als", "nur", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn das Laub, wie Purpur gl\u00fchend,", "tokens": ["Denn", "das", "Laub", ",", "wie", "Pur\u00b7pur", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4rbte nur der Herbst so rot,", "tokens": ["F\u00e4rb\u00b7te", "nur", "der", "Herbst", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und sie selbst sinkt nun verbl\u00fchend", "tokens": ["und", "sie", "selbst", "sinkt", "nun", "ver\u00b7bl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ADV", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit hinunter in den Tod.", "tokens": ["Mit", "hin\u00b7un\u00b7ter", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}