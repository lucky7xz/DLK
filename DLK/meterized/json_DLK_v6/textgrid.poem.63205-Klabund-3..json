{"textgrid.poem.63205": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drausseu singt ein Vogel in der Welt.", "tokens": ["Draus\u00b7seu", "singt", "ein", "Vo\u00b7gel", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Draussen bl\u00fcht ein blaues Fr\u00fchlingsfeld,", "tokens": ["Draus\u00b7sen", "bl\u00fcht", "ein", "blau\u00b7es", "Fr\u00fch\u00b7lings\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Draussen geht ein M\u00e4dchen Arm in Arm", "tokens": ["Draus\u00b7sen", "geht", "ein", "M\u00e4d\u00b7chen", "Arm", "in", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00d6sterlich geputzt mit dem Gendarm.", "tokens": ["\u00d6s\u00b7ter\u00b7lich", "ge\u00b7putzt", "mit", "dem", "Gen\u00b7darm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Draussen sitzen satt im Restaurant", "tokens": ["Draus\u00b7sen", "sit\u00b7zen", "satt", "im", "Res\u00b7tau\u00b7rant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "B\u00fcrger bei Musik und Gabelklang.", "tokens": ["B\u00fcr\u00b7ger", "bei", "Mu\u00b7sik", "und", "Ga\u00b7bel\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Auf der Burg von N\u00fcrnberg spielt ein Kind", "tokens": ["Auf", "der", "Burg", "von", "N\u00fcrn\u00b7berg", "spielt", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Mit den Wolken und dem Himmelswind.", "tokens": ["Mit", "den", "Wol\u00b7ken", "und", "dem", "Him\u00b7mels\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und der Untersuchungsrichter streicht", "tokens": ["Und", "der", "Un\u00b7ter\u00b7su\u00b7chungs\u00b7rich\u00b7ter", "streicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Seiner Frau das blonde Haar vielleicht.", "tokens": ["Sei\u00b7ner", "Frau", "das", "blon\u00b7de", "Haar", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Draussen l\u00e4cheln sie einander an:", "tokens": ["Draus\u00b7sen", "l\u00e4\u00b7cheln", "sie", "ein\u00b7an\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Greis und S\u00e4ugling, M\u00e4dchen oder Mann.", "tokens": ["Greis", "und", "S\u00e4ug\u00b7ling", ",", "M\u00e4d\u00b7chen", "o\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Draussen lieben sie einander sehr:", "tokens": ["Draus\u00b7sen", "lie\u00b7ben", "sie", "ein\u00b7an\u00b7der", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Reh und Wiese, Sonnenschein und Meer.", "tokens": ["Reh", "und", "Wie\u00b7se", ",", "Son\u00b7nen\u00b7schein", "und", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}