{"dta.poem.13244": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun, Tugenden! zum Streit! bezieht das neue Feld!", "tokens": ["\u201e", "nun", ",", "Tu\u00b7gen\u00b7den", "!", "zum", "Streit", "!", "be\u00b7zieht", "das", "neu\u00b7e", "Feld", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$.", "APPRART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ein einer Jeden Herz wohnt ein gepr\u00fcfter Held;", "tokens": ["\u201e", "in", "ei\u00b7ner", "Je\u00b7den", "Herz", "wohnt", "ein", "ge\u00b7pr\u00fcf\u00b7ter", "Held", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "175\u201dNur eure Gegenwart kann jene Flutten hemmen,", "tokens": ["\"", "Nur", "eu\u00b7re", "Ge\u00b7gen\u00b7wart", "kann", "je\u00b7ne", "Flut\u00b7ten", "hem\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VMFIN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie schon so lange Zeit die L\u00e4nder \u00fcberschwemmen.", "tokens": ["\u201e", "die", "schon", "so", "lan\u00b7ge", "Zeit", "die", "L\u00e4n\u00b7der", "\u00fc\u00b7bersc\u00b7hwem\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ADV", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edas Heil des Vaterlands und unser Eigenthum", "tokens": ["\u201e", "das", "Heil", "des", "Va\u00b7ter\u00b7lands", "und", "un\u00b7ser", "Ei\u00b7gen\u00b7thum"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eist eurer Thaten Prei\u00df, Lohn, Ehre, Ziel und Ruhm.", "tokens": ["\u201e", "ist", "eu\u00b7rer", "Tha\u00b7ten", "Prei\u00df", ",", "Lohn", ",", "Eh\u00b7re", ",", "Ziel", "und", "Ruhm", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewer Recht besizt und sicht, kann sich als Sieger preisen,", "tokens": ["\u201e", "wer", "Recht", "be\u00b7sizt", "und", "sicht", ",", "kann", "sich", "als", "Sie\u00b7ger", "prei\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "VVFIN", "KON", "VVFIN", "$,", "VMFIN", "PRF", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "180\u201dDen Feinden unbesorgt die blosse Stirne weisen;", "tokens": ["\"", "Den", "Fein\u00b7den", "un\u00b7be\u00b7sorgt", "die", "blos\u00b7se", "Stir\u00b7ne", "wei\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewogegen, wann die Macht allein dem Heer befiehlt,", "tokens": ["\u201e", "wo\u00b7ge\u00b7gen", ",", "wann", "die", "Macht", "al\u00b7lein", "dem", "Heer", "be\u00b7fiehlt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eman oft am Ende Ruhm, Besiz und Macht verspielt.", "tokens": ["\u201e", "man", "oft", "am", "En\u00b7de", "Ruhm", ",", "Be\u00b7siz", "und", "Macht", "ver\u00b7spielt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "ADV", "APPRART", "NN", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edie Rechte schm\u00fccken zwar sehr oft der Feinde R\u00e4ncke;", "tokens": ["\u201e", "die", "Rech\u00b7te", "schm\u00fc\u00b7cken", "zwar", "sehr", "oft", "der", "Fein\u00b7de", "R\u00e4n\u00b7cke", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edadurch erwei\u00dft man nichts; die That lehrt, was man dencke.", "tokens": ["\u201e", "da\u00b7durch", "er\u00b7wei\u00dft", "man", "nichts", ";", "die", "That", "lehrt", ",", "was", "man", "den\u00b7cke", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PIS", "PIS", "$.", "ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}