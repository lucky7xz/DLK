{"textgrid.poem.48525": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "28. Auf eine Leiche am Neuenjahrstage, 1633", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem wir Iederman, uns Iederman hinwieder", "tokens": ["In", "dem", "wir", "Ie\u00b7der\u00b7man", ",", "uns", "Ie\u00b7der\u00b7man", "hin\u00b7wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NE", "$,", "PPER", "NE", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein gutes Jahr verehrt, so legest du dich nieder,", "tokens": ["ein", "gu\u00b7tes", "Jahr", "ver\u00b7ehrt", ",", "so", "le\u00b7gest", "du", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gelobte Frau, und stirbst. Wie wol doch nimst dues f\u00fcr!", "tokens": ["ge\u00b7lob\u00b7te", "Frau", ",", "und", "stirbst", ".", "Wie", "wol", "doch", "nimst", "du\u00b7es", "f\u00fcr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "$.", "PWAV", "ADV", "ADV", "ADV", "ADJA", "APPR", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Du tust den besten Wundsch, den Niemand w\u00fcndschte dir.", "tokens": ["Du", "tust", "den", "bes\u00b7ten", "Wund\u00b7sch", ",", "den", "Nie\u00b7mand", "w\u00fcnd\u00b7schte", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}