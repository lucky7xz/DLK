{"textgrid.poem.66352": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gute Nacht!", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Unser Taglauf ist vollbracht,", "tokens": ["Un\u00b7ser", "Tag\u00b7lauf", "ist", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldne Sternlein \u00e4ugeln wieder", "tokens": ["Gold\u00b7ne", "Stern\u00b7lein", "\u00e4u\u00b7geln", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Himmels Zinne nieder;", "tokens": ["Von", "des", "Him\u00b7mels", "Zin\u00b7ne", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Mondes Scheibe lacht,", "tokens": ["Und", "des", "Mon\u00b7des", "Schei\u00b7be", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Zum Klavier,", "tokens": ["Zum", "Kla\u00b7vier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Herzensweibchen, eilen wir!", "tokens": ["Her\u00b7zens\u00b7weib\u00b7chen", ",", "ei\u00b7len", "wir", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ins Goldgeweb' zu spielen,", "tokens": ["Um", "ins", "Gold\u00b7ge\u00b7web'", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wir f\u00fcr einander f\u00fchlen;", "tokens": ["Was", "wir", "f\u00fcr", "ein\u00b7an\u00b7der", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mit dir und du mit mir,", "tokens": ["Ich", "mit", "dir", "und", "du", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Am Klavier.", "tokens": ["Am", "Kla\u00b7vier", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Gottes Ruh'", "tokens": ["Got\u00b7tes", "Ruh'"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "S\u00e4uselt uns vom Himmel zu;", "tokens": ["S\u00e4u\u00b7selt", "uns", "vom", "Him\u00b7mel", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt uns der Empfindung F\u00fclle,", "tokens": ["Bringt", "uns", "der", "Emp\u00b7fin\u00b7dung", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlichkeit und Herzensstille,", "tokens": ["Z\u00e4rt\u00b7lich\u00b7keit", "und", "Her\u00b7zens\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach ich f\u00fchle sie wie du,", "tokens": ["Ach", "ich", "f\u00fch\u00b7le", "sie", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PWAV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Ruh'.", "tokens": ["Got\u00b7tes", "Ruh'", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "O gewi\u00df,", "tokens": ["O", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Welt, du bist ein Paradies;", "tokens": ["Welt", ",", "du", "bist", "ein", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir schon im Erdenleben", "tokens": ["Wenn", "wir", "schon", "im", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe nehmen, Liebe geben;", "tokens": ["Lie\u00b7be", "neh\u00b7men", ",", "Lie\u00b7be", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welt, so bist du uns gewi\u00df", "tokens": ["Welt", ",", "so", "bist", "du", "uns", "ge\u00b7wi\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Paradies.", "tokens": ["Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Schimmernd f\u00e4llt", "tokens": ["Schim\u00b7mernd", "f\u00e4llt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Unsre Thr\u00e4n' dem Herrn der Welt.", "tokens": ["Uns\u00b7re", "Thr\u00e4n'", "dem", "Herrn", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! dem Stifter unsrer Ehe", "tokens": ["Ach", "!", "dem", "Stif\u00b7ter", "uns\u00b7rer", "E\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammt der Dank zur fernsten H\u00f6he!", "tokens": ["Flammt", "der", "Dank", "zur", "ferns\u00b7ten", "H\u00f6\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, die Z\u00e4hre, Herr der Welt,", "tokens": ["Sieh", ",", "die", "Z\u00e4h\u00b7re", ",", "Herr", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie f\u00e4llt!", "tokens": ["Wie", "sie", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sieh den Mond in stiller Pracht", "tokens": ["Sieh", "den", "Mond", "in", "stil\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns mit goldnen Strahlen winken,", "tokens": ["Uns", "mit", "gold\u00b7nen", "Strah\u00b7len", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um in deinen Arm zu sinken,", "tokens": ["Um", "in", "dei\u00b7nen", "Arm", "zu", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weib, zur Wonne mir gemacht.", "tokens": ["Weib", ",", "zur", "Won\u00b7ne", "mir", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}