{"textgrid.poem.58288": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "326. Wo Gott sich zeigt, das andre weicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald man hat in sich das h\u00f6chste Gut gefunden,", "tokens": ["So\u00b7bald", "man", "hat", "in", "sich", "das", "h\u00f6chs\u00b7te", "Gut", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald f\u00e4llt Geld und Gut und alle Sorgen hin,", "tokens": ["So\u00b7bald", "f\u00e4llt", "Geld", "und", "Gut", "und", "al\u00b7le", "Sor\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJD", "KON", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein bess'rer Vorwurf h\u00e4lt die Liebe ganz gebunden;", "tokens": ["Ein", "bess'\u00b7rer", "Vor\u00b7wurf", "h\u00e4lt", "die", "Lie\u00b7be", "ganz", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lebt man reich und satt im blinden Glaubenssinn.", "tokens": ["So", "lebt", "man", "reich", "und", "satt", "im", "blin\u00b7den", "Glau\u00b7bens\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}