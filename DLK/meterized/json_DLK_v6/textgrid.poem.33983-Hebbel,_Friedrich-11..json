{"textgrid.poem.33983": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den b\u00e4ngsten Traum begleitet", "tokens": ["Den", "b\u00e4ngs\u00b7ten", "Traum", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein heimliches Gef\u00fchl,", "tokens": ["Ein", "heim\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Alles Nichts bedeutet,", "tokens": ["Da\u00df", "Al\u00b7les", "Nichts", "be\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r' uns noch so schw\u00fcl.", "tokens": ["Und", "w\u00e4r'", "uns", "noch", "so", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da spielt in unser Weinen", "tokens": ["Da", "spielt", "in", "un\u00b7ser", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein L\u00e4cheln hold hinein,", "tokens": ["Ein", "L\u00e4\u00b7cheln", "hold", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber m\u00f6gte meinen,", "tokens": ["Ich", "a\u00b7ber", "m\u00f6g\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So sollt' es immer sein!", "tokens": ["So", "sollt'", "es", "im\u00b7mer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}