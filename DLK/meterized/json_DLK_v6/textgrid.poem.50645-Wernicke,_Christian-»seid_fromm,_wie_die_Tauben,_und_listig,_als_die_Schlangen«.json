{"textgrid.poem.50645": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbseid fromm, wie die Tauben, und listig, als die Schlangen\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob von der Weisheit selbst wir gleich den Spruch empfangen:", "tokens": ["Ob", "von", "der", "Weis\u00b7heit", "selbst", "wir", "gleich", "den", "Spruch", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u00bbseid, wie die Tauben, fromm, und listig, wie die Schlangen\u00ab,", "tokens": ["\u00bb", "seid", ",", "wie", "die", "Tau\u00b7ben", ",", "fromm", ",", "und", "lis\u00b7tig", ",", "wie", "die", "Schlan\u00b7gen", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch folg' ich, weil ich ihn nicht recht verstehe, lieber", "tokens": ["Doch", "fol\u00b7g'", "ich", ",", "weil", "ich", "ihn", "nicht", "recht", "ver\u00b7ste\u00b7he", ",", "lie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Der Einfalt Sittenlehr' in Allem, was ich thu': \u2013", "tokens": ["Der", "Ein\u00b7falt", "Sit\u00b7ten\u00b7lehr'", "in", "Al\u00b7lem", ",", "was", "ich", "thu'", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich eigne mir allein die ", "tokens": ["Ich", "eig\u00b7ne", "mir", "al\u00b7lein", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}