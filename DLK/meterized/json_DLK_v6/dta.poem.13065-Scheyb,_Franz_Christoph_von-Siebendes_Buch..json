{"dta.poem.13065": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst aber wer erz\u00e4hlt, was die Matronen dachten?", "tokens": ["Sonst", "a\u00b7ber", "wer", "er\u00b7z\u00e4hlt", ",", "was", "die", "Mat\u00b7ro\u00b7nen", "dach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was vor Erwegungen sie bey dem Umstand machten?", "tokens": ["Was", "vor", "Er\u00b7we\u00b7gun\u00b7gen", "sie", "bey", "dem", "Um\u00b7stand", "mach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Jezt sahe man den Rath, und jezt einander an;", "tokens": ["Jezt", "sa\u00b7he", "man", "den", "Rath", ",", "und", "jezt", "ein\u00b7an\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "KON", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die redte; jene schwieg; da sich der Grei\u00df besann;", "tokens": ["Die", "red\u00b7te", ";", "je\u00b7ne", "schwieg", ";", "da", "sich", "der", "Grei\u00df", "be\u00b7sann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PDS", "VVFIN", "$.", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "595Als st\u00fcnde sein Gem\u00fcth in zweifelnden Gedancken;", "tokens": ["st\u00fcn\u00b7de", "sein", "Ge\u00b7m\u00fcth", "in", "zwei\u00b7feln\u00b7den", "Ge\u00b7dan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Als fieng er wieder an in seinem Schlu\u00df zu wancken.", "tokens": ["Als", "fi\u00b7eng", "er", "wie\u00b7der", "an", "in", "sei\u00b7nem", "Schlu\u00df", "zu", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPR", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}