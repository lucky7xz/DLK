{"textgrid.poem.34775": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Golfe von Biskaya", "tokens": ["Am", "Gol\u00b7fe", "von", "Bis\u00b7ka\u00b7ya"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat sie den Tag erblickt;", "tokens": ["Hat", "sie", "den", "Tag", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat schon in der Wiege", "tokens": ["Sie", "hat", "schon", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwei junge Katzen erdr\u00fcckt.", "tokens": ["Zwei", "jun\u00b7ge", "Kat\u00b7zen", "er\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie lief mit blo\u00dfen F\u00fc\u00dfen", "tokens": ["Sie", "lief", "mit", "blo\u00b7\u00dfen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00fcber die Pyren\u00e4'n;", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Py\u00b7ren\u00e4'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf lie\u00df sie als junge Riesin", "tokens": ["Drauf", "lie\u00df", "sie", "als", "jun\u00b7ge", "Rie\u00b7sin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "In Perpignan sich sehn.", "tokens": ["In", "Per\u00b7pig\u00b7nan", "sich", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt ist sie die gr\u00f6\u00dfte Dame", "tokens": ["Jetzt", "ist", "sie", "die", "gr\u00f6\u00df\u00b7te", "Da\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Faubourg Saint-Denis;", "tokens": ["Im", "Faub\u00b7ourg", "Saint\u00b7De\u00b7nis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kostet dem kleinen Sir William", "tokens": ["Sie", "kos\u00b7tet", "dem", "klei\u00b7nen", "Sir", "Wil\u00b7li\u00b7am"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schon dreizehntausend Louis.", "tokens": ["Schon", "drei\u00b7zehn\u00b7tau\u00b7send", "Lou\u00b7is", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}