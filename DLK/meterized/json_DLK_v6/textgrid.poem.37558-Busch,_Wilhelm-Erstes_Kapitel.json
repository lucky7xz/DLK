n", "kannst", "!!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJA", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Oh, wie begl\u00fcckt ist doch ein Mann,", "tokens": ["Oh", ",", "wie", "be\u00b7gl\u00fcckt", "ist", "doch", "ein", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er Gedichte machen kann!", "tokens": ["Wenn", "er", "Ge\u00b7dich\u00b7te", "ma\u00b7chen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}