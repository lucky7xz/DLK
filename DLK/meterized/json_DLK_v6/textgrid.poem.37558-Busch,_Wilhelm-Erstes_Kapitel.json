{"textgrid.poem.37558": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Kapitel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wohl ist dem, der dann und wann", "tokens": ["Wie", "wohl", "ist", "dem", ",", "der", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "$,", "PRELS", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich etwas Sch\u00f6nes dichten kann!", "tokens": ["Sich", "et\u00b7was", "Sch\u00f6\u00b7nes", "dich\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mensch, durchtrieben und gescheit,", "tokens": ["Der", "Mensch", ",", "durch\u00b7trie\u00b7ben", "und", "ge\u00b7scheit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bemerkte schon seit alter Zeit,", "tokens": ["Be\u00b7merk\u00b7te", "schon", "seit", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihm hienieden allerlei", "tokens": ["Da\u00df", "ihm", "hien\u00b7ie\u00b7den", "al\u00b7ler\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdrie\u00dflich und zuwider sei.", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "und", "zu\u00b7wi\u00b7der", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Freude flieht auf allen Wegen;", "tokens": ["Die", "Freu\u00b7de", "flieht", "auf", "al\u00b7len", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der \u00c4rger kommt uns gern entgegen.", "tokens": ["Der", "\u00c4r\u00b7ger", "kommt", "uns", "gern", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gar mancher schleicht betr\u00fcbt umher;", "tokens": ["Gar", "man\u00b7cher", "schleicht", "be\u00b7tr\u00fcbt", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Knopfloch ist so \u00f6d und leer.", "tokens": ["Sein", "Kno\u00b7pfloch", "ist", "so", "\u00f6d", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr manchen hat ein M\u00e4dchen Reiz,", "tokens": ["F\u00fcr", "man\u00b7chen", "hat", "ein", "M\u00e4d\u00b7chen", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur bleibt die Liebe seinerseits.", "tokens": ["Nur", "bleibt", "die", "Lie\u00b7be", "sei\u00b7ner\u00b7seits", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch gibt's noch mehr Verdrie\u00dflichkeiten.", "tokens": ["Doch", "gibt's", "noch", "mehr", "Ver\u00b7drie\u00df\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zum Beispiel l\u00e4\u00dft sich nicht bestreiten:", "tokens": ["Zum", "Bei\u00b7spiel", "l\u00e4\u00dft", "sich", "nicht", "be\u00b7strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Sorge, wie man Nahrung findet,", "tokens": ["Die", "Sor\u00b7ge", ",", "wie", "man", "Nah\u00b7rung", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ist h\u00e4ufig nicht so unbegr\u00fcndet.", "tokens": ["Ist", "h\u00e4u\u00b7fig", "nicht", "so", "un\u00b7be\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Kommt einer dann und fragt: Wie geht's?", "tokens": ["Kommt", "ei\u00b7ner", "dann", "und", "fragt", ":", "Wie", "geht's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Steht man gew\u00f6hnlich oder stets", "tokens": ["Steht", "man", "ge\u00b7w\u00f6hn\u00b7lich", "o\u00b7der", "stets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Gewisserma\u00dfen peinlich da,", "tokens": ["Ge\u00b7wis\u00b7ser\u00b7ma\u00b7\u00dfen", "pein\u00b7lich", "da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Indem man spricht: Nun, so lala!", "tokens": ["In\u00b7dem", "man", "spricht", ":", "Nun", ",", "so", "la\u00b7la", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$.", "ADV", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und nur der Heuchler lacht vergn\u00fcglich", "tokens": ["Und", "nur", "der", "Heuch\u00b7ler", "lacht", "ver\u00b7gn\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und gibt zur Antwort: Ei, vorz\u00fcglich!", "tokens": ["Und", "gibt", "zur", "Ant\u00b7wort", ":", "Ei", ",", "vor\u00b7z\u00fcg\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Im Durchschnitt ist man kummervoll", "tokens": ["Im", "Durch\u00b7schnitt", "ist", "man", "kum\u00b7mer\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und wei\u00df nicht, was man machen soll. \u2013", "tokens": ["Und", "wei\u00df", "nicht", ",", "was", "man", "ma\u00b7chen", "soll", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht so der Dichter. Kaum mi\u00dff\u00e4llt", "tokens": ["Nicht", "so", "der", "Dich\u00b7ter.", "Kaum", "mi\u00df\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "abbreviation", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihm diese altgebackne Welt,", "tokens": ["Ihm", "die\u00b7se", "alt\u00b7ge\u00b7back\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So knetet er aus weicher Kleie", "tokens": ["So", "kne\u00b7tet", "er", "aus", "wei\u00b7cher", "Klei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr sich privatim eine neue", "tokens": ["F\u00fcr", "sich", "pri\u00b7va\u00b7tim", "ei\u00b7ne", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "ART", "ADJA"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und zieht als freier Musensohn", "tokens": ["Und", "zieht", "als", "frei\u00b7er", "Mu\u00b7sen\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In die Poetendimension,", "tokens": ["In", "die", "Poe\u00b7ten\u00b7di\u00b7men\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Die f\u00fcnfte, da die vierte jetzt", "tokens": ["Die", "f\u00fcnf\u00b7te", ",", "da", "die", "vier\u00b7te", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KOUS", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Geistern ohnehin besetzt.", "tokens": ["Von", "Geis\u00b7tern", "oh\u00b7ne\u00b7hin", "be\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier ist es luftig, duftig, sch\u00f6n,", "tokens": ["Hier", "ist", "es", "luf\u00b7tig", ",", "duf\u00b7tig", ",", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hier hat er nichts mehr auszustehn,", "tokens": ["Hier", "hat", "er", "nichts", "mehr", "aus\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hier aus dem m\u00fctterlichen Busen", "tokens": ["Hier", "aus", "dem", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der ewig wohlgen\u00e4hrten Musen", "tokens": ["Der", "e\u00b7wig", "wohl\u00b7ge\u00b7n\u00e4hr\u00b7ten", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Rinnt ihm der Stoff best\u00e4ndig neu", "tokens": ["Rinnt", "ihm", "der", "Stoff", "be\u00b7st\u00e4n\u00b7dig", "neu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In seine saubre Molkerei.", "tokens": ["In", "sei\u00b7ne", "saub\u00b7re", "Mol\u00b7ke\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gleichwie die brave Bauernmutter.", "tokens": ["Gleich\u00b7wie", "die", "bra\u00b7ve", "Bau\u00b7ern\u00b7mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Tagt\u00e4glich macht sie frische Butter.", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "macht", "sie", "fri\u00b7sche", "But\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Des Abends sp\u00e4t, des Morgens fr\u00fche", "tokens": ["Des", "A\u00b7bends", "sp\u00e4t", ",", "des", "Mor\u00b7gens", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Zupft sie am Hinterleib der K\u00fche", "tokens": ["Zupft", "sie", "am", "Hin\u00b7ter\u00b7leib", "der", "K\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mit kunstge\u00fcbten Handgelenken", "tokens": ["Mit", "kunst\u00b7ge\u00b7\u00fcb\u00b7ten", "Hand\u00b7ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und tr\u00e4gt, was kommt, zu k\u00fchlen Schr\u00e4nken,", "tokens": ["Und", "tr\u00e4gt", ",", "was", "kommt", ",", "zu", "k\u00fch\u00b7len", "Schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wo bald ihr Finger, leicht gekr\u00fcmmt,", "tokens": ["Wo", "bald", "ihr", "Fin\u00b7ger", ",", "leicht", "ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Den fetten Rahm, der oben schwimmt,", "tokens": ["Den", "fet\u00b7ten", "Rahm", ",", "der", "o\u00b7ben", "schwimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Beiseite sch\u00f6pft und so in Masse", "tokens": ["Bei\u00b7sei\u00b7te", "sch\u00f6pft", "und", "so", "in", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Vereint im hohen Butterfasse.", "tokens": ["Ver\u00b7eint", "im", "ho\u00b7hen", "But\u00b7ter\u00b7fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Jetzt mit durchl\u00f6chertem Pistille", "tokens": ["Jetzt", "mit", "durch\u00b7l\u00f6\u00b7cher\u00b7tem", "Pis\u00b7til\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Bedr\u00e4ngt sie die geschmeid'ge F\u00fclle.", "tokens": ["Be\u00b7dr\u00e4ngt", "sie", "die", "ge\u00b7schmei\u00b7d'\u00b7ge", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Es kullert, bullert, quitscht und quatscht,", "tokens": ["Es", "kul\u00b7lert", ",", "bul\u00b7lert", ",", "quitscht", "und", "quatscht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wird auf und nieder durchgematscht,", "tokens": ["Wird", "auf", "und", "nie\u00b7der", "durch\u00b7ge\u00b7matscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Bis das geplagte Element", "tokens": ["Bis", "das", "ge\u00b7plag\u00b7te", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Vor Angst in Dick und D\u00fcnn sich trennt.", "tokens": ["Vor", "Angst", "in", "Dick", "und", "D\u00fcnn", "sich", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Dies ist der Augenblick der Wonne.", "tokens": ["Dies", "ist", "der", "Au\u00b7gen\u00b7blick", "der", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Sie hebt das Dicke aus der Tonne,", "tokens": ["Sie", "hebt", "das", "Di\u00b7cke", "aus", "der", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Legt's in die Mulde, flach von Holz,", "tokens": ["Legt's", "in", "die", "Mul\u00b7de", ",", "flach", "von", "Holz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Durchknetet es und dr\u00fcckt und rollt's,", "tokens": ["Durch\u00b7kne\u00b7tet", "es", "und", "dr\u00fcckt", "und", "rollt's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und sieh, in frohen H\u00e4nden h\u00e4lt se", "tokens": ["Und", "sieh", ",", "in", "fro\u00b7hen", "H\u00e4n\u00b7den", "h\u00e4lt", "se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die wohlgeratne Butterw\u00e4lze.", "tokens": ["Die", "wohl\u00b7ge\u00b7rat\u00b7ne", "But\u00b7ter\u00b7w\u00e4l\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So auch der Dichter. \u2013 Stillbegl\u00fcckt", "tokens": ["So", "auch", "der", "Dich\u00b7ter.", "\u2013", "Still\u00b7be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "abbreviation", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat er sich was zurechtgedr\u00fcckt", "tokens": ["Hat", "er", "sich", "was", "zu\u00b7recht\u00b7ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchlt sich nun in jeder Richtung", "tokens": ["Und", "f\u00fchlt", "sich", "nun", "in", "je\u00b7der", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Befriedigt durch die eigne Dichtung.", "tokens": ["Be\u00b7frie\u00b7digt", "durch", "die", "eig\u00b7ne", "Dich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch guter Menschen Hauptbestreben", "tokens": ["Doch", "gu\u00b7ter", "Men\u00b7schen", "Haupt\u00b7be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist, andern auch was abzugeben.", "tokens": ["Ist", ",", "an\u00b7dern", "auch", "was", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PIS", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Dichter, dem sein Fabrikat", "tokens": ["Der", "Dich\u00b7ter", ",", "dem", "sein", "Fab\u00b7ri\u00b7kat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So viel Genu\u00df bereitet hat,", "tokens": ["So", "viel", "Ge\u00b7nu\u00df", "be\u00b7rei\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er sehnt sich sehr, er kann nicht ruhn,", "tokens": ["Er", "sehnt", "sich", "sehr", ",", "er", "kann", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Audi andern damit wohlzutun;", "tokens": ["Au\u00b7di", "an\u00b7dern", "da\u00b7mit", "wohl\u00b7zu\u00b7tun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und mu\u00df er sich auch recht bem\u00fchn,", "tokens": ["Und", "mu\u00df", "er", "sich", "auch", "recht", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er sucht sich wen und findet ihn;", "tokens": ["Er", "sucht", "sich", "wen", "und", "fin\u00b7det", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und str\u00e4ubt sich der vor solchen Freuden,", "tokens": ["Und", "str\u00e4ubt", "sich", "der", "vor", "sol\u00b7chen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Er kann sein Gl\u00fcck mal nicht vermeiden.", "tokens": ["Er", "kann", "sein", "Gl\u00fcck", "mal", "nicht", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Am Mittelknopfe seiner Weste", "tokens": ["Am", "Mit\u00b7tel\u00b7knop\u00b7fe", "sei\u00b7ner", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "H\u00e4lt ihn der Dichter dringend feste,", "tokens": ["H\u00e4lt", "ihn", "der", "Dich\u00b7ter", "drin\u00b7gend", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "F\u00fchrt ihn beiseit zum guten Zwecke", "tokens": ["F\u00fchrt", "ihn", "bei\u00b7seit", "zum", "gu\u00b7ten", "Zwe\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "In eine lauschig stille Ecke,", "tokens": ["In", "ei\u00b7ne", "lau\u00b7schig", "stil\u00b7le", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und schon erfolgt der Griff, der rasche,", "tokens": ["Und", "schon", "er\u00b7folgt", "der", "Griff", ",", "der", "ra\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Links in die warme Busentasche,", "tokens": ["Links", "in", "die", "war\u00b7me", "Bu\u00b7sen\u00b7ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und rauschend \u00f6ffnen sich die Spalten", "tokens": ["Und", "rau\u00b7schend", "\u00f6ff\u00b7nen", "sich", "die", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Des Manuskripts, die viel enthalten.", "tokens": ["Des", "Ma\u00b7nus\u00b7kripts", ",", "die", "viel", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Lippe spr\u00fcht, das Auge leuchtet,", "tokens": ["Die", "Lip\u00b7pe", "spr\u00fcht", ",", "das", "Au\u00b7ge", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Des Lauschers Bart wird angefeuchtet,", "tokens": ["Des", "Lau\u00b7schers", "Bart", "wird", "an\u00b7ge\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Denn nah und warm, wie sanftes Fl\u00f6ten,", "tokens": ["Denn", "nah", "und", "warm", ",", "wie", "sanf\u00b7tes", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ert\u00f6nt die Stimme des Poeten. \u2013", "tokens": ["Er\u00b7t\u00f6nt", "die", "Stim\u00b7me", "des", "Po\u00b7et\u00b7en", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Vortrefflich! ruft des Dichters Freund;", "tokens": ["Vor\u00b7treff\u00b7lich", "!", "ruft", "des", "Dich\u00b7ters", "Freund", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dasselbe, was der Dichter meint;", "tokens": ["Das\u00b7sel\u00b7be", ",", "was", "der", "Dich\u00b7ter", "meint", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und, was er sicher wei\u00df, zu glauben,", "tokens": ["Und", ",", "was", "er", "si\u00b7cher", "wei\u00df", ",", "zu", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Darf sich doch jeder wohl erlauben.", "tokens": ["Darf", "sich", "doch", "je\u00b7der", "wohl", "er\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sch\u00f6n, wenn dann, was er erdacht,", "tokens": ["Wie", "sch\u00f6n", ",", "wenn", "dann", ",", "was", "er", "er\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfunden und zurechtgemacht,", "tokens": ["Emp\u00b7fun\u00b7den", "und", "zu\u00b7recht\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn seines Geistes Kunstprodukt,", "tokens": ["Wenn", "sei\u00b7nes", "Geis\u00b7tes", "Kunst\u00b7pro\u00b7dukt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Morgenbl\u00e4ttchen abgedruckt,", "tokens": ["Im", "Mor\u00b7gen\u00b7bl\u00e4tt\u00b7chen", "ab\u00b7ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom treuen Kolporteur geleitet,", "tokens": ["Vom", "treu\u00b7en", "Kol\u00b7por\u00b7teur", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich durch die ganze Stadt verbreitet.", "tokens": ["Sich", "durch", "die", "gan\u00b7ze", "Stadt", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Wasser kocht. \u2013 In jedem Hause,", "tokens": ["Das", "Was\u00b7ser", "kocht", ".", "\u2013", "In", "je\u00b7dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hervor aus stiller Schlummerklause,", "tokens": ["Her\u00b7vor", "aus", "stil\u00b7ler", "Schlum\u00b7mer\u00b7klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Eilt neugest\u00e4rkt und neugereinigt,", "tokens": ["Eilt", "neu\u00b7ge\u00b7st\u00e4rkt", "und", "neu\u00b7ge\u00b7rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Froh gr\u00fc\u00dfend, weil aufs neu vereinigt,", "tokens": ["Froh", "gr\u00fc\u00b7\u00dfend", ",", "weil", "aufs", "neu", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "APPRART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hausvater, Mutter, J\u00fcngling, M\u00e4dchen", "tokens": ["Haus\u00b7va\u00b7ter", ",", "Mut\u00b7ter", ",", "J\u00fcng\u00b7ling", ",", "M\u00e4d\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zum Fr\u00fchkaffee mit frischen Br\u00f6tchen.", "tokens": ["Zum", "Fr\u00fch\u00b7kaf\u00b7fee", "mit", "fri\u00b7schen", "Br\u00f6t\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie alle bitten nach der Reihe", "tokens": ["Sie", "al\u00b7le", "bit\u00b7ten", "nach", "der", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das Morgenblatt sich aus, das neue,", "tokens": ["Das", "Mor\u00b7gen\u00b7blatt", "sich", "aus", ",", "das", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und jeder stutzt und jeder spricht:", "tokens": ["Und", "je\u00b7der", "stutzt", "und", "je\u00b7der", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Was f\u00fcr ein reizendes Gedicht!", "tokens": ["Was", "f\u00fcr", "ein", "rei\u00b7zen\u00b7des", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Durch die Lorgnetten, durch die Brillen,", "tokens": ["Durch", "die", "Lorg\u00b7net\u00b7ten", ",", "durch", "die", "Bril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Durch weit ge\u00f6ffnete Pupillen,", "tokens": ["Durch", "weit", "ge\u00b7\u00f6ff\u00b7ne\u00b7te", "Pu\u00b7pil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Erst in den Kopf, dann in das Herz,", "tokens": ["Erst", "in", "den", "Kopf", ",", "dann", "in", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dann kreuz und quer und niederw\u00e4rts", "tokens": ["Dann", "kreuz", "und", "quer", "und", "nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Flie\u00dft's und durchweicht das ganze Wesen", "tokens": ["Flie\u00dft's", "und", "durch\u00b7weicht", "das", "gan\u00b7ze", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Von allen denen, die es lesen.", "tokens": ["Von", "al\u00b7len", "de\u00b7nen", ",", "die", "es", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nun lebt in Leib und Seel der Leute,", "tokens": ["Nun", "lebt", "in", "Leib", "und", "Seel", "der", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Umschlossen vom Bezirk der H\u00e4ute", "tokens": ["Um\u00b7schlos\u00b7sen", "vom", "Be\u00b7zirk", "der", "H\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und andern warmen Kleidungsst\u00fccken,", "tokens": ["Und", "an\u00b7dern", "war\u00b7men", "Klei\u00b7dungs\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Dichter fort, um zu begl\u00fccken,", "tokens": ["Der", "Dich\u00b7ter", "fort", ",", "um", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bis da\u00df er schlie\u00dflich abgen\u00fctzt,", "tokens": ["Bis", "da\u00df", "er", "schlie\u00df\u00b7lich", "ab\u00b7ge\u00b7n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Verklungen oder ausgeschwitzt.", "tokens": ["Ver\u00b7klun\u00b7gen", "o\u00b7der", "aus\u00b7ge\u00b7schwitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein sch\u00f6nes Los! Indessen doch", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Los", "!", "In\u00b7des\u00b7sen", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das allersch\u00f6nste bl\u00fcht ihm noch.", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "bl\u00fcht", "ihm", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Laura, seine s\u00fc\u00dfe Qual,", "tokens": ["Denn", "Lau\u00b7ra", ",", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Himmelstraum, sein Ideal,", "tokens": ["Sein", "Him\u00b7mel\u00b