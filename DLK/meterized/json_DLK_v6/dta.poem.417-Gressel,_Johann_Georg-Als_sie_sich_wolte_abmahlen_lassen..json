{"dta.poem.417": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich wolte abmahlen lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Kunst sucht Bissula ihr Antlitz nicht zu schmincken/", "tokens": ["Durch", "Kunst", "sucht", "Bis\u00b7su\u00b7la", "ihr", "Ant\u00b7litz", "nicht", "zu", "schmin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die angebohrne Pracht wil ohne H\u00fclffe blincken/", "tokens": ["Die", "an\u00b7ge\u00b7bohr\u00b7ne", "Pracht", "wil", "oh\u00b7ne", "H\u00fclf\u00b7fe", "blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die andern m\u00f6gen Schmuck von Purpur Schnecken leihn/", "tokens": ["Die", "an\u00b7dern", "m\u00f6\u00b7gen", "Schmuck", "von", "Pur\u00b7pur", "Schne\u00b7cken", "leihn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auf die gelbe Haut ein weisses Pulver streun.", "tokens": ["Und", "auf", "die", "gel\u00b7be", "Haut", "ein", "weis\u00b7ses", "Pul\u00b7ver", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum wei\u00df ihr Angesicht auch niemand abzumahlen/", "tokens": ["Drum", "wei\u00df", "ihr", "An\u00b7ge\u00b7sicht", "auch", "nie\u00b7mand", "ab\u00b7zu\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ADV", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch Mahler auf! vermisch der Ros-und Liljen Prahlen/", "tokens": ["Doch", "Mah\u00b7ler", "auf", "!", "ver\u00b7misch", "der", "Ros\u00b7\u00b7und", "Lil\u00b7jen", "Prah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$.", "ADJD", "ART", "NN", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn die ", "tokens": ["Wenn", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "So wisse/ diese mu\u00df des Mundes Farbe seyn.", "tokens": ["So", "wis\u00b7se", "/", "die\u00b7se", "mu\u00df", "des", "Mun\u00b7des", "Far\u00b7be", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PDS", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}