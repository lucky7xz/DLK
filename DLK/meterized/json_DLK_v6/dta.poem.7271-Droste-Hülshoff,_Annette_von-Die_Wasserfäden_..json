{"dta.poem.7271": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wasserf\u00e4den .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.57", "af:0.42"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Neid' uns! neid' uns! lass' die Zweige hangen,", "tokens": ["Neid'", "uns", "!", "neid'", "uns", "!", "lass'", "die", "Zwei\u00b7ge", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "$.", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht weil fl\u00fcssigen Kristall wir trinken,", "tokens": ["Nicht", "weil", "fl\u00fcs\u00b7si\u00b7gen", "Kris\u00b7tall", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Neben uns des Himmels Sterne blinken,", "tokens": ["Ne\u00b7ben", "uns", "des", "Him\u00b7mels", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sonne sich in unserm Netz gefangen \u2014", "tokens": ["Son\u00b7ne", "sich", "in", "un\u00b7serm", "Netz", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein, des Teiches Blutsverwandte, fest", "tokens": ["Nein", ",", "des", "Tei\u00b7ches", "Bluts\u00b7ver\u00b7wand\u00b7te", ",", "fest"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00e4lt er all uns an die Brust gepre\u00dft,", "tokens": ["H\u00e4lt", "er", "all", "uns", "an", "die", "Brust", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und wir bohren uns're feinen Ranken", "tokens": ["Und", "wir", "boh\u00b7ren", "un\u00b7s'\u00b7re", "fei\u00b7nen", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "In das Herz ihm, wie ein liebend Weib,", "tokens": ["In", "das", "Herz", "ihm", ",", "wie", "ein", "lie\u00b7bend", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$,", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Dringen Adern gleich durch seinen Leib,", "tokens": ["Drin\u00b7gen", "A\u00b7dern", "gleich", "durch", "sei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "D\u00e4mmern auf wie seines Traums Gedanken;", "tokens": ["D\u00e4m\u00b7mern", "auf", "wie", "sei\u00b7nes", "Traums", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Wer uns kennt, der nennt uns lieb und treu,", "tokens": ["Wer", "uns", "kennt", ",", "der", "nennt", "uns", "lieb", "und", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Und die Schmerle birgt in uns'rer Hut", "tokens": ["Und", "die", "Schmer\u00b7le", "birgt", "in", "un\u00b7s'\u00b7rer", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Und die Karpfenmutter ihre Brut;", "tokens": ["Und", "die", "Kar\u00b7pfen\u00b7mut\u00b7ter", "ih\u00b7re", "Brut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Welle mag in unserm Schleier kosen;", "tokens": ["Wel\u00b7le", "mag", "in", "un\u00b7serm", "Schlei\u00b7er", "ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Uns nur traut die holde Wasserfey,", "tokens": ["Uns", "nur", "traut", "die", "hol\u00b7de", "Was\u00b7ser\u00b7fey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Sie, die Sch\u00f6ne, lieblicher als Rosen.", "tokens": ["Sie", ",", "die", "Sch\u00f6\u00b7ne", ",", "lieb\u00b7li\u00b7cher", "als", "Ro\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Schleu\u00df, Trifolium, ", "tokens": ["Schleu\u00df", ",", "Tri\u00b7fo\u00b7li\u00b7um", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Kurz dein Tag, doch k\u00f6niglich sein Lauf!", "tokens": ["Kurz", "dein", "Tag", ",", "doch", "k\u00f6\u00b7nig\u00b7lich", "sein", "Lauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}