{"textgrid.poem.53334": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir klagen \u00fcberall", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir klagen \u00fcberall", "tokens": ["Wir", "kla\u00b7gen", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Noht und Todes-Fall", "tokens": ["Das", "Noht", "und", "To\u00b7des\u00b7Fall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vns manches Leid erreget,", "tokens": ["Vns", "man\u00b7ches", "Leid", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd nehmen nicht in acht", "tokens": ["Vnd", "neh\u00b7men", "nicht", "in", "acht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Gottes Eiffers Macht", "tokens": ["Da\u00df", "Got\u00b7tes", "Eif\u00b7fers", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns also schl\u00e4get.", "tokens": ["Vns", "al\u00b7so", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er, dessen Auge sieht", "tokens": ["Er", ",", "des\u00b7sen", "Au\u00b7ge", "sieht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was in der Welt geschieht,", "tokens": ["Was", "in", "der", "Welt", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja selbs die Sonne blendet,", "tokens": ["Ja", "selbs", "die", "Son\u00b7ne", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt mehr als flei\u00dfig war", "tokens": ["Nimmt", "mehr", "als", "flei\u00b7\u00dfig", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Bo\u00dfheit hier und dar", "tokens": ["Was", "Bo\u00df\u00b7heit", "hier", "und", "dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sein Vrtheil sch\u00e4ndet.", "tokens": ["Sein", "Vrtheil", "sch\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Mein Auffstehn, meine Rhu", "tokens": ["Mein", "Auffs\u00b7tehn", ",", "mei\u00b7ne", "Rhu"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd alles was ich thue", "tokens": ["Vnd", "al\u00b7les", "was", "ich", "thue"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwebt stets Ihm vor Gesichte,", "tokens": ["Schwebt", "stets", "Ihm", "vor", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Trotz insonderheit", "tokens": ["Mein", "Trotz", "in\u00b7son\u00b7der\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Steht bei Ihm iederzeit", "tokens": ["Steht", "bei", "Ihm", "ie\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie vor Gerichte.", "tokens": ["Wie", "vor", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Auch was ich noch nicht merck'", "tokens": ["Auch", "was", "ich", "noch", "nicht", "merck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es sey ein S\u00fcnden-Werck,", "tokens": ["Es", "sey", "ein", "S\u00fcn\u00b7den\u00b7\u00b7Werck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist vor Ihm dargestellet", "tokens": ["Ist", "vor", "Ihm", "dar\u00b7ge\u00b7stel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd warttet, was doch jhm", "tokens": ["Vnd", "wart\u00b7tet", ",", "was", "doch", "jhm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Zorn f\u00fcr Vngest\u00fcm", "tokens": ["Sein", "Zorn", "f\u00fcr", "Vn\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zum Vrtheil f\u00e4llet.", "tokens": ["Zum", "Vrtheil", "f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nun klag des Lebens Frist", "tokens": ["Nun", "klag", "des", "Le\u00b7bens", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie so fl\u00fcchtig ist,", "tokens": ["Da\u00df", "sie", "so", "fl\u00fcch\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wir so sparsam alten:", "tokens": ["Vnd", "wir", "so", "spar\u00b7sam", "al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der S\u00fcnden Vngemach", "tokens": ["Der", "S\u00fcn\u00b7den", "Vn\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd hierauff Gottes Rach'", "tokens": ["Vnd", "hier\u00b7auff", "Got\u00b7tes", "Rach'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Heisst uns erkalten.", "tokens": ["Heisst", "uns", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Drumb unsre Tage sind", "tokens": ["Drumb", "uns\u00b7re", "Ta\u00b7ge", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So schnell als kaum der Wind,", "tokens": ["So", "schnell", "als", "kaum", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd unsre Jahre fliehen", "tokens": ["Vnd", "uns\u00b7re", "Jah\u00b7re", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd wir mit jhnen auch", "tokens": ["Vnd", "wir", "mit", "jh\u00b7nen", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gleich wie sich sonst ein Rauch", "tokens": ["Gleich", "wie", "sich", "sonst", "ein", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Pflegt zu verziehen.", "tokens": ["Pflegt", "zu", "ver\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "O Herr, lehr in der Zeit", "tokens": ["O", "Herr", ",", "lehr", "in", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vns unsre Sterblicheit", "tokens": ["Vns", "uns\u00b7re", "Sterb\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wol zu Gem\u00fchte fassen,", "tokens": ["Wol", "zu", "Ge\u00b7m\u00fch\u00b7te", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vnd mach uns hiedurch klug,", "tokens": ["Vnd", "mach", "uns", "hie\u00b7durch", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wir des Satans Trug", "tokens": ["Da\u00df", "wir", "des", "Sa\u00b7tans", "Trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die S\u00fcnde lassen.", "tokens": ["Die", "S\u00fcn\u00b7de", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Kehr Dich doch wieder her,", "tokens": ["Kehr", "Dich", "doch", "wie\u00b7der", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "End unser Angstbeschwer,", "tokens": ["End", "un\u00b7ser", "Angst\u00b7be\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd sollen wir dann reisen,", "tokens": ["Vnd", "sol\u00b7len", "wir", "dann", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So nimm uns auff zu Dir,", "tokens": ["So", "nimm", "uns", "auff", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wir Dich zeitlich hier,", "tokens": ["Da\u00df", "wir", "Dich", "zeit\u00b7lich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dort ewig, preisen!", "tokens": ["Dort", "e\u00b7wig", ",", "prei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}