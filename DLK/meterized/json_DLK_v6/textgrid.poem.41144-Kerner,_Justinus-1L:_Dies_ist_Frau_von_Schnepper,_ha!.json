{"textgrid.poem.41144": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies ist Frau von Schnepper, ha!", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist Frau von Schnepper, ha!", "tokens": ["Dies", "ist", "Frau", "von", "Schnep\u00b7per", ",", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hocherstaunt nach ihrer Leiche,", "tokens": ["Ho\u00b7cher\u00b7staunt", "nach", "ih\u00b7rer", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sie sich im Mittelreiche,", "tokens": ["Als", "sie", "sich", "im", "Mit\u00b7tel\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Himmelreiche sah.", "tokens": ["Nicht", "im", "Him\u00b7mel\u00b7rei\u00b7che", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbeinen Schnepper an meinem Kleid!\u00ab", "tokens": ["\u00bb", "ei\u00b7nen", "Schnep\u00b7per", "an", "mei\u00b7nem", "Kleid", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sprach sie sterbend noch zum Schneider;", "tokens": ["Sprach", "sie", "ster\u00b7bend", "noch", "zum", "Schnei\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPRART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einzig wegen sch\u00f6ner Kleider", "tokens": ["Ein\u00b7zig", "we\u00b7gen", "sch\u00f6\u00b7ner", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat der Sonntag sie erfreut.", "tokens": ["Hat", "der", "Sonn\u00b7tag", "sie", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Jetzt doch rief sie: \u00bbHu der Nacht!", "tokens": ["Jetzt", "doch", "rief", "sie", ":", "\u00bb", "Hu", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "$(", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Als mein Leib mir wurde starrer,", "tokens": ["Als", "mein", "Leib", "mir", "wur\u00b7de", "star\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sprach doch zu mir der Herr Pfarrer:", "tokens": ["Sprach", "doch", "zu", "mir", "der", "Herr", "Pfar\u00b7rer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "\u203abald Sie schaun des Himmels Pracht,", "tokens": ["\u203a", "bald", "Sie", "schaun", "des", "Him\u00b7mels", "Pracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Schon ein Engel steht bereit,", "tokens": ["Schon", "ein", "En\u00b7gel", "steht", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sie zu f\u00fchrn in Gottes Arme,\u2039", "tokens": ["Sie", "zu", "f\u00fchrn", "in", "Got\u00b7tes", "Ar\u00b7me", ",", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "NN", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und nun, da\u00df sich Gott erbarme!", "tokens": ["Und", "nun", ",", "da\u00df", "sich", "Gott", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PRF", "NN", "VVFIN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.16": {"text": "Und nun welche Einsamkeit!", "tokens": ["Und", "nun", "wel\u00b7che", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wo ist nun des Himmels Pracht,", "tokens": ["Wo", "ist", "nun", "des", "Him\u00b7mels", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.18": {"text": "Ist die Sonne, sind die Sterne?", "tokens": ["Ist", "die", "Son\u00b7ne", ",", "sind", "die", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nur mein Kleid (noch seh' ich's gerne)", "tokens": ["Nur", "mein", "Kleid", "(", "noch", "seh'", "ich's", "ger\u00b7ne", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PIS", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Blieb mir in des Hades Nacht \u2013", "tokens": ["Blieb", "mir", "in", "des", "Ha\u00b7des", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Da ruft's fernher: \u203aIn dich geh!", "tokens": ["Da", "ruft's", "fern\u00b7her", ":", "\u203a", "In", "dich", "geh", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "$(", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Nieder zog dich Erdenschwere,", "tokens": ["Nie\u00b7der", "zog", "dich", "Er\u00b7den\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Deine tr\u00fcbe Seele kl\u00e4re,", "tokens": ["Dei\u00b7ne", "tr\u00fc\u00b7be", "See\u00b7le", "kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Dann erst schwebet sie zur H\u00f6h'.", "tokens": ["Dann", "erst", "schwe\u00b7bet", "sie", "zur", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Hier in Nacht dir Licht erring,", "tokens": ["Hier", "in", "Nacht", "dir", "Licht", "er\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Bis dir f\u00e4llt vom Aug' die Schuppe,", "tokens": ["Bis", "dir", "f\u00e4llt", "vom", "Aug'", "die", "Schup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wiss'! erst in der Nacht der Puppe", "tokens": ["Wiss'", "!", "erst", "in", "der", "Nacht", "der", "Pup\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wird die Raup' zum Schmetterling.\u2039\u00ab", "tokens": ["Wird", "die", "Raup'", "zum", "Schmet\u00b7ter\u00b7ling", ".", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}