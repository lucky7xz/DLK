{"dta.poem.6270": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! warum wird doch dies Metall", "tokens": ["Ach", "!", "wa\u00b7rum", "wird", "doch", "dies", "Me\u00b7tall"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu etwas anders angewandt,", "tokens": ["Zu", "et\u00b7was", "an\u00b7ders", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als zu der Kunst, das fette Land", "tokens": ["Als", "zu", "der", "Kunst", ",", "das", "fet\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer Pflugschaar \u00fcberall,", "tokens": ["Mit", "ei\u00b7ner", "Pflug\u00b7schaar", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(als eine Zier und Ehr vom Ackerban) zu pfl\u00fcgen?", "tokens": ["(", "als", "ei\u00b7ne", "Zier", "und", "Ehr", "vom", "A\u00b7cker\u00b7ban", ")", "zu", "pfl\u00fc\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "KON", "NN", "APPRART", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein ein grimmiger, verfluchter Geist erfand", "tokens": ["Al\u00b7lein", "ein", "grim\u00b7mi\u00b7ger", ",", "ver\u00b7fluch\u00b7ter", "Geist", "er\u00b7fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So Lantz-als Degen, die wir tr\u00e4ncken,", "tokens": ["So", "Lant\u00b7zals", "De\u00b7gen", ",", "die", "wir", "tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch eine blinde Wuth,", "tokens": ["Durch", "ei\u00b7ne", "blin\u00b7de", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mit Menschen-Blut.", "tokens": ["Mit", "Men\u00b7schen\u00b7Blut", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Armseel\u2019ge Sterbliche! was m\u00f6gt ihr doch gedencken?", "tokens": ["Arm\u00b7seel'\u00b7ge", "Sterb\u00b7li\u00b7che", "!", "was", "m\u00f6gt", "ihr", "doch", "ge\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Das Eisen, welches ihr zu Klingen", "tokens": ["Das", "Ei\u00b7sen", ",", "wel\u00b7ches", "ihr", "zu", "Klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verfertigt, um euch umzubringen,", "tokens": ["Ver\u00b7fer\u00b7tigt", ",", "um", "euch", "um\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Hat euch der Himmel wollen schencken,", "tokens": ["Hat", "euch", "der", "Him\u00b7mel", "wol\u00b7len", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nur blos, auf da\u00df dadurch m\u00f6g\u2019 aus der Erden", "tokens": ["Nur", "blos", ",", "auf", "da\u00df", "da\u00b7durch", "m\u00f6g'", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "KOUS", "PAV", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Die Frucht, die euch ern\u00e4hrt, hervor gezogen werden.", "tokens": ["Die", "Frucht", ",", "die", "euch", "er\u00b7n\u00e4hrt", ",", "her\u00b7vor", "ge\u00b7zo\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}