{"textgrid.poem.57743": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Helljagd", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneelicht die Heide, hell und klar,", "tokens": ["Schnee\u00b7licht", "die", "Hei\u00b7de", ",", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu Ende geht das alte Jahr;", "tokens": ["Zu", "En\u00b7de", "geht", "das", "al\u00b7te", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sucht sich sein Grab im wei\u00dfen Schnee", "tokens": ["Sucht", "sich", "sein", "Grab", "im", "wei\u00b7\u00dfen", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Samt seiner Wonne, seinem Weh.", "tokens": ["Samt", "sei\u00b7ner", "Won\u00b7ne", ",", "sei\u00b7nem", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich starre in die Nacht hinein.", "tokens": ["Ich", "star\u00b7re", "in", "die", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel ist voll blankem Schein;", "tokens": ["Der", "Him\u00b7mel", "ist", "voll", "blan\u00b7kem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sternlein bei dem andern steht,", "tokens": ["Ein", "Stern\u00b7lein", "bei", "dem", "an\u00b7dern", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Silberblumen ausges\u00e4t.", "tokens": ["Wie", "Sil\u00b7ber\u00b7blu\u00b7men", "aus\u00b7ge\u00b7s\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin nicht traurig, bin nicht froh,", "tokens": ["Ich", "bin", "nicht", "trau\u00b7rig", ",", "bin", "nicht", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz, das ist in Nirgendwo;", "tokens": ["Mein", "Herz", ",", "das", "ist", "in", "Nir\u00b7gend\u00b7wo", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist nicht da, es ist nicht dort,", "tokens": ["Es", "ist", "nicht", "da", ",", "es", "ist", "nicht", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist an einem andern Ort.", "tokens": ["Es", "ist", "an", "ei\u00b7nem", "an\u00b7dern", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da kommt mein Blick zu mir zur\u00fcck,", "tokens": ["Da", "kommt", "mein", "Blick", "zu", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da heb' ich das gesenkte Genick,", "tokens": ["Da", "heb'", "ich", "das", "ge\u00b7senk\u00b7te", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da horch' ich in die Nacht hinein,", "tokens": ["Da", "horch'", "ich", "in", "die", "Nacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da h\u00f6r' ich eine Eule schrei'n.", "tokens": ["Da", "h\u00f6r'", "ich", "ei\u00b7ne", "Eu\u00b7le", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Eule Schrei, der war es nicht,", "tokens": ["Der", "Eu\u00b7le", "Schrei", ",", "der", "war", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich senke wieder das Gesicht;", "tokens": ["Ich", "sen\u00b7ke", "wie\u00b7der", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk' nicht an dies, denk' nicht an das,", "tokens": ["Denk'", "nicht", "an", "dies", ",", "denk'", "nicht", "an", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "PDS", "$,", "VVFIN", "PTKNEG", "APPR", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich wei\u00df nicht wie, ich wei\u00df nicht was.", "tokens": ["Ich", "wei\u00df", "nicht", "wie", ",", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Blick geht wieder gradeaus,", "tokens": ["Mein", "Blick", "geht", "wie\u00b7der", "gra\u00b7de\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fuchs steht auf dem Seelenhaus,", "tokens": ["Der", "Fuchs", "steht", "auf", "dem", "See\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steht schwarz auf dem verschneiten Stein,", "tokens": ["Steht", "schwarz", "auf", "dem", "ver\u00b7schnei\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bellt heiser in die Nacht hinein.", "tokens": ["Bellt", "hei\u00b7ser", "in", "die", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Fuchses Bellen war es nicht,", "tokens": ["Des", "Fuch\u00b7ses", "Bel\u00b7len", "war", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich senke wieder das Gesicht;", "tokens": ["Ich", "sen\u00b7ke", "wie\u00b7der", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kommt der Ruf von Nimmermehr", "tokens": ["Da", "kommt", "der", "Ruf", "von", "Nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nirgendwo noch einmal her.", "tokens": ["Und", "Nir\u00b7gend\u00b7wo", "noch", "ein\u00b7mal", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Hohl kommt er her vom Seelenland,", "tokens": ["Hohl", "kommt", "er", "her", "vom", "See\u00b7len\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich nehm' die B\u00fcchse in die Hand;", "tokens": ["Ich", "nehm'", "die", "B\u00fcch\u00b7se", "in", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im wilden Walde geht der Wind,", "tokens": ["Im", "wil\u00b7den", "Wal\u00b7de", "geht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lied zu summen er beginnt.", "tokens": ["Sein", "Lied", "zu", "sum\u00b7men", "er", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Wind im Walde war es nicht,", "tokens": ["Der", "Wind", "im", "Wal\u00b7de", "war", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich senke wieder das Gesicht;", "tokens": ["Ich", "sen\u00b7ke", "wie\u00b7der", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis ich vergesse, wo ich bin,", "tokens": ["Bis", "ich", "ver\u00b7ges\u00b7se", ",", "wo", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hallt es nochmals nach mir hin.", "tokens": ["Da", "hallt", "es", "noch\u00b7mals", "nach", "mir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Uhr im fernen Dorfe schl\u00e4gt,", "tokens": ["Die", "Uhr", "im", "fer\u00b7nen", "Dor\u00b7fe", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zw\u00f6lf Schl\u00e4ge es her\u00fcber tr\u00e4gt;", "tokens": ["Zw\u00f6lf", "Schl\u00e4\u00b7ge", "es", "her\u00b7\u00fc\u00b7ber", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lausche ich mit off'nem Mund,", "tokens": ["Da", "lau\u00b7sche", "ich", "mit", "off'\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6re meinen toten Hund.", "tokens": ["Ich", "h\u00f6\u00b7re", "mei\u00b7nen", "to\u00b7ten", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Viel' Stimmen kommen querfeldein,", "tokens": ["Viel'", "Stim\u00b7men", "kom\u00b7men", "quer\u00b7fel\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ho R\u00fcd ho und Hussaschrei'n;", "tokens": ["Mit", "Ho", "R\u00fcd", "ho", "und", "Hus\u00b7saschrei'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "KON", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weit weg sind sie und wieder nah,", "tokens": ["Weit", "weg", "sind", "sie", "und", "wie\u00b7der", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind hier nicht und sind auch nicht da.", "tokens": ["Sind", "hier", "nicht", "und", "sind", "auch", "nicht", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "KON", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Sterne springen hin und her,", "tokens": ["Die", "Ster\u00b7ne", "sprin\u00b7gen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie springen in die Kreuz und Quer;", "tokens": ["Sie", "sprin\u00b7gen", "in", "die", "Kreuz", "und", "Quer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie treten rechts und links zur Seit'", "tokens": ["Sie", "tre\u00b7ten", "rechts", "und", "links", "zur", "Seit'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer hellen Herrlichkeit.", "tokens": ["In", "ih\u00b7rer", "hel\u00b7len", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Es \u00f6ffnet sich das Himmelstor,", "tokens": ["Es", "\u00f6ff\u00b7net", "sich", "das", "Him\u00b7mels\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wei\u00dfe Keiler bricht hervor;", "tokens": ["Der", "wei\u00b7\u00dfe", "Kei\u00b7ler", "bricht", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seiner F\u00e4hrte braust die Jagd", "tokens": ["In", "sei\u00b7ner", "F\u00e4hr\u00b7te", "braust", "die", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hernieder durch die Weihenacht.", "tokens": ["Her\u00b7nie\u00b7der", "durch", "die", "Wei\u00b7he\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ich halt' das Hifthorn an den Mund:", "tokens": ["Ich", "halt'", "das", "Hif\u00b7thorn", "an", "den", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daher, daher, mein lieber Hund;", "tokens": ["Da\u00b7her", ",", "da\u00b7her", ",", "mein", "lie\u00b7ber", "Hund", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iam