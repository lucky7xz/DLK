{"dta.poem.9545": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-reime an den Herrn Marschalck von  \n Carnitz/ den tag nach seiner hochzeit mit der  \n von Sch\u00f6ningin.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nheit deiner braut hat dich verliebt gemacht/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "dei\u00b7ner", "braut", "hat", "dich", "ver\u00b7liebt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wir die Sch\u00f6ningin/ nach ihrer sch\u00f6nheit/ nennen;", "tokens": ["Die", "wir", "die", "Sch\u00f6\u00b7nin\u00b7gin", "/", "nach", "ih\u00b7rer", "sch\u00f6n\u00b7heit", "/", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und gleichwohl sagt man uns/ da\u00df erst seit dieser nacht/", "tokens": ["Und", "gleich\u00b7wohl", "sagt", "man", "uns", "/", "da\u00df", "erst", "seit", "die\u00b7ser", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "$(", "KOUS", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du ihrer sch\u00f6nheit prei\u00df beginnest recht zu kennen.", "tokens": ["Du", "ih\u00b7rer", "sch\u00f6n\u00b7heit", "prei\u00df", "be\u00b7gin\u00b7nest", "recht", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr reitzendes gesicht/ mit munterkeit verm\u00e4hlt/", "tokens": ["Ihr", "reit\u00b7zen\u00b7des", "ge\u00b7sicht", "/", "mit", "mun\u00b7ter\u00b7keit", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVPP", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwie\u00df dir zwar genug der sch\u00f6nheit ihre st\u00e4rcke.", "tokens": ["Er\u00b7wie\u00df", "dir", "zwar", "ge\u00b7nug", "der", "sch\u00f6n\u00b7heit", "ih\u00b7re", "st\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch hat sie vor dir/ bi\u00df diese nacht/ verhehlt", "tokens": ["Je\u00b7doch", "hat", "sie", "vor", "dir", "/", "bi\u00df", "die\u00b7se", "nacht", "/", "ver\u00b7hehlt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$(", "APPR", "PDAT", "NN", "$(", "VVFIN"], "meter": "-+-------+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die allers\u00fcssesten und sch\u00f6nsten wunderwercke.", "tokens": ["Die", "al\u00b7ler\u00b7s\u00fcs\u00b7ses\u00b7ten", "und", "sch\u00f6ns\u00b7ten", "wun\u00b7der\u00b7wer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Heut aber da du ihr den g\u00fcrtel hast gel\u00f6\u00dft/", "tokens": ["Heut", "a\u00b7ber", "da", "du", "ihr", "den", "g\u00fcr\u00b7tel", "hast", "ge\u00b7l\u00f6\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du den gantzen Schatz vor deinen augen liegen:", "tokens": ["Siehst", "du", "den", "gant\u00b7zen", "Schatz", "vor", "dei\u00b7nen", "au\u00b7gen", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn eh die Venus nicht die schlancken lenden bl\u00f6st/", "tokens": ["Denn", "eh", "die", "Ve\u00b7nus", "nicht", "die", "schlan\u00b7cken", "len\u00b7den", "bl\u00f6st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan ihre sch\u00f6nheit nicht der Paris heissen siegen.", "tokens": ["Kan", "ih\u00b7re", "sch\u00f6n\u00b7heit", "nicht", "der", "Pa\u00b7ris", "heis\u00b7sen", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O gl\u00fccke dieser nacht/ die dich an sie getraut!", "tokens": ["O", "gl\u00fc\u00b7cke", "die\u00b7ser", "nacht", "/", "die", "dich", "an", "sie", "ge\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "$(", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Du freyhest mehr mit ihr/ als sie von sich gepriesen.", "tokens": ["Du", "frey\u00b7hest", "mehr", "mit", "ihr", "/", "als", "sie", "von", "sich", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "$(", "KOUS", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sch\u00f6nheit deiner frau ist gr\u00f6sser denn der braut/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "dei\u00b7ner", "frau", "ist", "gr\u00f6s\u00b7ser", "denn", "der", "braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dir die hochzeit erst ihr Paradie\u00df gewiesen.", "tokens": ["Weil", "dir", "die", "hoch\u00b7zeit", "erst", "ihr", "Pa\u00b7ra\u00b7die\u00df", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}