{"textgrid.poem.48178": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun la\u00df den Sturm aus Norden wehn", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00df den Sturm aus Norden wehn", "tokens": ["Nun", "la\u00df", "den", "Sturm", "aus", "Nor\u00b7den", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und herbstlich sich die Fluren f\u00e4rben \u2013", "tokens": ["und", "herbst\u00b7lich", "sich", "die", "Flu\u00b7ren", "f\u00e4r\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wir glauben nicht an Sterben,", "tokens": ["wir", "glau\u00b7ben", "nicht", "an", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "an Sterben und Vergehn!", "tokens": ["an", "Ster\u00b7ben", "und", "Ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns wirft der fr\u00fcchtereife Baum", "tokens": ["Uns", "wirft", "der", "fr\u00fcch\u00b7ter\u00b7ei\u00b7fe", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die roten Aepfel vor die F\u00fc\u00dfe,", "tokens": ["die", "ro\u00b7ten", "A\u00b7e\u00b7pfel", "vor", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "wir kosten ihre S\u00fc\u00dfe", "tokens": ["wir", "kos\u00b7ten", "ih\u00b7re", "S\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "und schl\u00fcrfen ihren Schaum.", "tokens": ["und", "schl\u00fcr\u00b7fen", "ih\u00b7ren", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Feuermohn im Gartenbeet,", "tokens": ["Der", "Feu\u00b7er\u00b7mohn", "im", "Gar\u00b7ten\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ob seiner Flammen Pracht verlodert,", "tokens": ["ob", "sei\u00b7ner", "Flam\u00b7men", "Pracht", "ver\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Knisterseide modert: \u2013", "tokens": ["die", "Knis\u00b7ter\u00b7sei\u00b7de", "mo\u00b7dert", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hochragend seine Kapsel steht.", "tokens": ["hoch\u00b7ra\u00b7gend", "sei\u00b7ne", "Kap\u00b7sel", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Von meinem Drucke k\u00f6rnerschwer", "tokens": ["Von", "mei\u00b7nem", "Dru\u00b7cke", "k\u00f6r\u00b7ner\u00b7schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zerspringt die reife H\u00fclle, \u2013", "tokens": ["zer\u00b7springt", "die", "rei\u00b7fe", "H\u00fcl\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "keimkr\u00e4ftigen Samens F\u00fclle", "tokens": ["keim\u00b7kr\u00e4f\u00b7ti\u00b7gen", "Sa\u00b7mens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "streut rings der Wind umher.", "tokens": ["streut", "rings", "der", "Wind", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist das ewige Gebot", "tokens": ["Das", "ist", "das", "e\u00b7wi\u00b7ge", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des ungest\u00f6rten Weiterwebens:", "tokens": ["des", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Wei\u00b7ter\u00b7we\u00b7bens", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "eine neue Form des Lebens", "tokens": ["ei\u00b7ne", "neu\u00b7e", "Form", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist jeder Tod.", "tokens": ["ist", "je\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dem Fr\u00fchling drum im Herbst ein Glas!", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "drum", "im", "Herbst", "ein", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir trinken aus dem Vollen \u2013", "tokens": ["Wir", "trin\u00b7ken", "aus", "dem", "Vol\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "die goldenen Tropfen rollen,", "tokens": ["die", "gol\u00b7de\u00b7nen", "Trop\u00b7fen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Dankopfer, ins gelbe Gras....", "tokens": ["Dan\u00b7kop\u00b7fer", ",", "ins", "gel\u00b7be", "Gras", "...."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}