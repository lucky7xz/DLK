{"textgrid.poem.55522": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ziblis", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, setzt euch zu mir nieder,", "tokens": ["M\u00e4d\u00b7chen", ",", "setzt", "euch", "zu", "mir", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand st\u00f6rt hier unsre Ruh,", "tokens": ["Nie\u00b7mand", "st\u00f6rt", "hier", "uns\u00b7re", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, es kommt der Fr\u00fchling wieder,", "tokens": ["Seht", ",", "es", "kommt", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weckt die Blumen und die Lieder,", "tokens": ["Weckt", "die", "Blu\u00b7men", "und", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn zu ehren, h\u00f6rt mir zu.", "tokens": ["Ihn", "zu", "eh\u00b7ren", ",", "h\u00f6rt", "mir", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weise, strenge M\u00fctter lehren:", "tokens": ["Wei\u00b7se", ",", "stren\u00b7ge", "M\u00fct\u00b7ter", "leh\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbm\u00e4dchen, flieht der M\u00e4nner List!\u00ab", "tokens": ["\u00bb", "m\u00e4d\u00b7chen", ",", "flieht", "der", "M\u00e4n\u00b7ner", "List", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "VVFIN", "ART", "NN", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch la\u00dft ihr euch bet\u00f6ren!", "tokens": ["Und", "doch", "la\u00dft", "ihr", "euch", "be\u00b7t\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt, ihr sollt ein Beispiel h\u00f6ren,", "tokens": ["H\u00f6rt", ",", "ihr", "sollt", "ein", "Bei\u00b7spiel", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer am meisten furchtbar ist.", "tokens": ["Wer", "am", "meis\u00b7ten", "furcht\u00b7bar", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "PIS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ziblis, jung und sch\u00f6n, zur Liebe,", "tokens": ["Zib\u00b7lis", ",", "jung", "und", "sch\u00f6n", ",", "zur", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Z\u00e4rtlichkeit gemacht,", "tokens": ["Zu", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Floh aus rauhem, wilden Triebe,", "tokens": ["Floh", "aus", "rau\u00b7hem", ",", "wil\u00b7den", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht aus Tugend alle Liebe,", "tokens": ["Nicht", "aus", "Tu\u00b7gend", "al\u00b7le", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Freude war die Jagd.", "tokens": ["Ih\u00b7re", "Freu\u00b7de", "war", "die", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als sie einst tief im Gestr\u00e4uche", "tokens": ["Als", "sie", "einst", "tief", "im", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sorglos froh ein Liedchen sang,", "tokens": ["Sorg\u00b7los", "froh", "ein", "Lied\u00b7chen", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward sie bla\u00df wie eine Leiche,", "tokens": ["Ward", "sie", "bla\u00df", "wie", "ei\u00b7ne", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da aus einer alten Eiche", "tokens": ["Da", "aus", "ei\u00b7ner", "al\u00b7ten", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein geh\u00f6rnter Waldgott sprang.", "tokens": ["Ein", "ge\u00b7h\u00f6rn\u00b7ter", "Wald\u00b7gott", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Z\u00e4rtlich lacht das Ungeheuer,", "tokens": ["Z\u00e4rt\u00b7lich", "lacht", "das", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziblis wendet ihr Gesicht,", "tokens": ["Zib\u00b7lis", "wen\u00b7det", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4uft, doch der geh\u00f6rnte Freier", "tokens": ["L\u00e4uft", ",", "doch", "der", "ge\u00b7h\u00f6rn\u00b7te", "Frei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Springt ihr wie ein h\u00fcpfend Feuer", "tokens": ["Springt", "ihr", "wie", "ein", "h\u00fcp\u00b7fend", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach und ruft: \u00bbO flieh mich nicht!\u00ab", "tokens": ["Nach", "und", "ruft", ":", "\u00bb", "O", "flieh", "mich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KON", "VVFIN", "$.", "$(", "NE", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schrei'n kann niemals \u00fcberwinden.", "tokens": ["Schrei'n", "kann", "nie\u00b7mals", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie lief schneller, er ihr nach.", "tokens": ["Sie", "lief", "schnel\u00b7ler", ",", "er", "ihr", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kam sie zu den Gr\u00fcnden,", "tokens": ["End\u00b7lich", "kam", "sie", "zu", "den", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da, wo unter jungen Linden", "tokens": ["Da", ",", "wo", "un\u00b7ter", "jun\u00b7gen", "Lin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Emiren am Wasser lag.", "tokens": ["E\u00b7mi\u00b7ren", "am", "Was\u00b7ser", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbhilf mir!\u00ab rief sie. Er, voll Freude,", "tokens": ["\u00bb", "hilf", "mir", "!", "\u00ab", "rief", "sie", ".", "Er", ",", "voll", "Freu\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$.", "$(", "VVFIN", "PPER", "$.", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er so die Nymphe sah,", "tokens": ["Da\u00df", "er", "so", "die", "Nym\u00b7phe", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stand bewaffnet zu dem Streite", "tokens": ["Stand", "be\u00b7waff\u00b7net", "zu", "dem", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Ast der n\u00e4chsten Weide,", "tokens": ["Mit", "dem", "Ast", "der", "n\u00e4chs\u00b7ten", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der Waldgott kam, schon da.", "tokens": ["Als", "der", "Wald\u00b7gott", "kam", ",", "schon", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der trat n\u00e4her, ihn zu h\u00f6hnen,", "tokens": ["Der", "trat", "n\u00e4\u00b7her", ",", "ihn", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ging schnell den Zweikampf ein.", "tokens": ["Und", "ging", "schnell", "den", "Zwei\u00b7kampf", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie erbebt f\u00fcr Emirenen.", "tokens": ["Sie", "er\u00b7bebt", "f\u00fcr", "E\u00b7mi\u00b7re\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer wird das Herz der Sch\u00f6nen", "tokens": ["Im\u00b7mer", "wird", "das", "Herz", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf des Sch\u00f6nen Seite sein.", "tokens": ["Auf", "des", "Sch\u00f6\u00b7nen", "Sei\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Seinen Feind im Sand zu h\u00f6hnen,", "tokens": ["Sei\u00b7nen", "Feind", "im", "Sand", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regt sich Fu\u00df und Arm und Hand,", "tokens": ["Regt", "sich", "Fu\u00df", "und", "Arm", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald mit Sto\u00dfen, bald mit Dehnen.", "tokens": ["Bald", "mit", "Sto\u00b7\u00dfen", ",", "bald", "mit", "Deh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe st\u00e4rkt die Kraft der Sehnen,", "tokens": ["Lie\u00b7be", "st\u00e4rkt", "die", "Kraft", "der", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Beide waren gleich entbrannt.", "tokens": ["Bei\u00b7de", "wa\u00b7ren", "gleich", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Endlich sinkt der Faun zur Erden,", "tokens": ["End\u00b7lich", "sinkt", "der", "Faun", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ihn traf ein harter Streich.", "tokens": ["Denn", "ihn", "traf", "ein", "har\u00b7ter", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00e4\u00dflich zerrt er die Geb\u00e4rden;", "tokens": ["Gr\u00e4\u00df\u00b7lich", "zerrt", "er", "die", "Ge\u00b7b\u00e4r\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Emiren, ihn loszuwerden,", "tokens": ["E\u00b7mi\u00b7ren", ",", "ihn", "los\u00b7zu\u00b7wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVIZU", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wirft ihn in den n\u00e4chsten Teich.", "tokens": ["Wirft", "ihn", "in", "den", "n\u00e4chs\u00b7ten", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ziblis lag mit matten Blicken,", "tokens": ["Zib\u00b7lis", "lag", "mit", "mat\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Sieger kam, im Gras.", "tokens": ["Da", "der", "Sie\u00b7ger", "kam", ",", "im", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wird's ihm, ihr zu helfen, gl\u00fccken?", "tokens": ["Wird's", "ihm", ",", "ihr", "zu", "hel\u00b7fen", ",", "gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leicht sind M\u00e4dchen zu erquicken,", "tokens": ["Leicht", "sind", "M\u00e4d\u00b7chen", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft ist ihre Krankheit Spa\u00df.", "tokens": ["Oft", "ist", "ih\u00b7re", "Krank\u00b7heit", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sie erhebt sich. Neues Leben", "tokens": ["Sie", "er\u00b7hebt", "sich", ".", "Neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gibt ein hei\u00dfer Ku\u00df ihr gleich.", "tokens": ["Gibt", "ein", "hei\u00b7\u00dfer", "Ku\u00df", "ihr", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, der einen schon gegeben,", "tokens": ["Doch", ",", "der", "ei\u00b7nen", "schon", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollte nicht nach mehrern streben?", "tokens": ["Soll\u00b7te", "nicht", "nach", "meh\u00b7rern", "stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das sieht einem M\u00e4rchen gleich.", "tokens": ["Das", "sieht", "ei\u00b7nem", "M\u00e4r\u00b7chen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Wartet nur. Es folgten K\u00fcsse", "tokens": ["War\u00b7tet", "nur", ".", "Es", "folg\u00b7ten", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$.", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hundertweis; sie schmeckten ihr.", "tokens": ["Hun\u00b7dert\u00b7weis", ";", "sie", "schmeck\u00b7ten", "ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die M\u00e4ulchen schmecken s\u00fc\u00dfe.", "tokens": ["Ja", ",", "die", "M\u00e4ul\u00b7chen", "schme\u00b7cken", "s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bei Ziblis waren diese", "tokens": ["Und", "bei", "Zib\u00b7lis", "wa\u00b7ren", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VAFIN", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gar die ersten. Glaubt es mir.", "tokens": ["Gar", "die", "ers\u00b7ten", ".", "Glaubt", "es", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Darum sog mit langen Z\u00fcgen", "tokens": ["Da\u00b7rum", "sog", "mit", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie begierig immer mehr.", "tokens": ["Sie", "be\u00b7gie\u00b7rig", "im\u00b7mer", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich trunken von Vergn\u00fcgen,", "tokens": ["End\u00b7lich", "trun\u00b7ken", "von", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward dem Emiren das Siegen,", "tokens": ["Ward", "dem", "E\u00b7mi\u00b7ren", "das", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Wie ihr denken k\u00f6nnt, nicht schwer.", "tokens": ["Wie", "ihr", "den\u00b7ken", "k\u00f6nnt", ",", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VVFIN", "$,", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "M\u00e4dchen, f\u00fcrchtet rauher Leute", "tokens": ["M\u00e4d\u00b7chen", ",", "f\u00fcrch\u00b7tet", "rau\u00b7her", "Leu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Buhlerische Wollust nie.", "tokens": ["Buh\u00b7le\u00b7ri\u00b7sche", "Wol\u00b7lust", "nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im ehrfurchtsvollen Kleide", "tokens": ["Die", "im", "ehr\u00b7furchts\u00b7vol\u00b7len", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viel von unschuldsvoller Freude", "tokens": ["Viel", "von", "un\u00b7schulds\u00b7vol\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reden, M\u00e4dchen, f\u00fcrchtet die.", "tokens": ["Re\u00b7den", ",", "M\u00e4d\u00b7chen", ",", "f\u00fcrch\u00b7tet", "die", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ART", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wacht, denn da ist nichts zu scherzen.", "tokens": ["Wacht", ",", "denn", "da", "ist", "nichts", "zu", "scher\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid viel lieber klug als kalt.", "tokens": ["Seid", "viel", "lie\u00b7ber", "klug", "als", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zittert stets f\u00fcr eure Herzen.", "tokens": ["Zit\u00b7tert", "stets", "f\u00fcr", "eu\u00b7re", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat man einmal diese Herzen,", "tokens": ["Hat", "man", "ein\u00b7mal", "die\u00b7se", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha, das andre hat man bald!", "tokens": ["Ha", ",", "das", "and\u00b7re", "hat", "man", "bald", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PIS", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}