{"textgrid.poem.64295": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Quelle, du bist mir werter, denn des lauten,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Quelle, du bist mir werter, denn des lauten,", "tokens": ["Quel\u00b7le", ",", "du", "bist", "mir", "wer\u00b7ter", ",", "denn", "des", "lau\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Felsenst\u00fcrzenden Stroms erz\u00fcrnte Woge!", "tokens": ["Fel\u00b7sen\u00b7st\u00fcr\u00b7zen\u00b7den", "Stroms", "er\u00b7z\u00fcrn\u00b7te", "Wo\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deinem leisen Lispel entschl\u00fcpfen s\u00fc\u00dfe", "tokens": ["Dei\u00b7nem", "lei\u00b7sen", "Lis\u00b7pel", "ent\u00b7schl\u00fcp\u00b7fen", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Freuden der Seele!", "tokens": ["Freu\u00b7den", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Freuden der Seele fliehn der Welt Get\u00f6se,", "tokens": ["Freu\u00b7den", "der", "See\u00b7le", "fliehn", "der", "Welt", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sind der Ruhe Gespielen! lieben deine", "tokens": ["Sind", "der", "Ru\u00b7he", "Ge\u00b7spie\u00b7len", "!", "lie\u00b7ben", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "$.", "VVFIN", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Blumenthale, lieben, wie du, die K\u00fchle", "tokens": ["Blu\u00b7ment\u00b7ha\u00b7le", ",", "lie\u00b7ben", ",", "wie", "du", ",", "die", "K\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Duftender Erlen!", "tokens": ["Duf\u00b7ten\u00b7der", "Er\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}