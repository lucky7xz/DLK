{"textgrid.poem.66762": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Diadem", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer je erw\u00e4hlt den innern Thron der Dinge,", "tokens": ["Wer", "je", "er\u00b7w\u00e4hlt", "den", "in\u00b7nern", "Thron", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Menschenherzens majest\u00e4tisch Gut,", "tokens": ["Des", "Men\u00b7schen\u00b7her\u00b7zens", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wahrte klar den Geist vor \u00dcbermut,", "tokens": ["Und", "wahr\u00b7te", "klar", "den", "Geist", "vor", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er in falschem Wahn sich nicht verfinge \u2013", "tokens": ["Da\u00df", "er", "in", "fal\u00b7schem", "Wahn", "sich", "nicht", "ver\u00b7fin\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er wird getragen frei auf sichrer Schwinge,", "tokens": ["Er", "wird", "ge\u00b7tra\u00b7gen", "frei", "auf", "sich\u00b7rer", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die er sich selber schuf, ob Wind und Flut,", "tokens": ["Die", "er", "sich", "sel\u00b7ber", "schuf", ",", "ob", "Wind", "und", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geborgen schwebt er in der h\u00f6chsten Hut,", "tokens": ["Ge\u00b7bor\u00b7gen", "schwebt", "er", "in", "der", "h\u00f6chs\u00b7ten", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleichwie gehalten von dem Ring der Ringe.", "tokens": ["Gleich\u00b7wie", "ge\u00b7hal\u00b7ten", "von", "dem", "Ring", "der", "Rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "In dieses Daseins rohen Stofflichkeiten,", "tokens": ["In", "die\u00b7ses", "Da\u00b7seins", "ro\u00b7hen", "Stoff\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo oft als edel das Gemeine gilt,", "tokens": ["Wo", "oft", "als", "e\u00b7del", "das", "Ge\u00b7mei\u00b7ne", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der hohle Tropf von D\u00fcnkel \u00fcberquillt", "tokens": ["Der", "hoh\u00b7le", "Tropf", "von", "D\u00fcn\u00b7kel", "\u00fc\u00b7berq\u00b7uillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Larven keck in Hermelinen schreiten,", "tokens": ["Und", "Lar\u00b7ven", "keck", "in", "Her\u00b7me\u00b7li\u00b7nen", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward ihm, zu \u00fcberwinden Schmach und Leben,", "tokens": ["Ward", "ihm", ",", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", "Schmach", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Diadem der seltnen Kraft gegeben.", "tokens": ["Das", "Di\u00b7a\u00b7dem", "der", "selt\u00b7nen", "Kraft", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}