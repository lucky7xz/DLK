{"textgrid.poem.44889": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Literatur", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas Ausland sch\u00e4tzt und lobt uns allgemach,", "tokens": ["\u00bb", "das", "Aus\u00b7land", "sch\u00e4tzt", "und", "lobt", "uns", "all\u00b7ge\u00b7mach", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur ihre Kenntnis unsrer mu\u00df ich d\u00fcrftig nennen.\u00ab", "tokens": ["Nur", "ih\u00b7re", "Kennt\u00b7nis", "uns\u00b7rer", "mu\u00df", "ich", "d\u00fcrf\u00b7tig", "nen\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPOSAT", "VMFIN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Freund, der Nachteil zieht den Vorteil nach,", "tokens": ["Mein", "Freund", ",", "der", "Nach\u00b7teil", "zieht", "den", "Vor\u00b7teil", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die loben minder wohl, wenn sie uns besser kennen.", "tokens": ["Die", "lo\u00b7ben", "min\u00b7der", "wohl", ",", "wenn", "sie", "uns", "bes\u00b7ser", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}