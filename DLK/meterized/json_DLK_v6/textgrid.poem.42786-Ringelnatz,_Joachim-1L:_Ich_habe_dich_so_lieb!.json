{"textgrid.poem.42786": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe dich so lieb!", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe dich so lieb!", "tokens": ["Ich", "ha\u00b7be", "dich", "so", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich w\u00fcrde dir ohne Bedenken", "tokens": ["Ich", "w\u00fcr\u00b7de", "dir", "oh\u00b7ne", "Be\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eine Kachel aus meinem Ofen", "tokens": ["Ei\u00b7ne", "Ka\u00b7chel", "aus", "mei\u00b7nem", "O\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schenken.", "tokens": ["Schen\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ich habe dir nichts getan.", "tokens": ["Ich", "ha\u00b7be", "dir", "nichts", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nun ist mir traurig zu Mut.", "tokens": ["Nun", "ist", "mir", "trau\u00b7rig", "zu", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "An den H\u00e4ngen der Eisenbahn", "tokens": ["An", "den", "H\u00e4n\u00b7gen", "der", "Ei\u00b7sen\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Leuchtet der Ginster so gut.", "tokens": ["Leuch\u00b7tet", "der", "Gins\u00b7ter", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Vorbei \u2013 verj\u00e4hrt \u2013", "tokens": ["Vor\u00b7bei", "\u2013", "ver\u00b7j\u00e4hrt", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$(", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Doch nimmer vergessen.", "tokens": ["Doch", "nim\u00b7mer", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich reise.", "tokens": ["Ich", "rei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Alles, was lange w\u00e4hrt,", "tokens": ["Al\u00b7les", ",", "was", "lan\u00b7ge", "w\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ist leise.", "tokens": ["Ist", "lei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Die Zeit entstellt", "tokens": ["Die", "Zeit", "ent\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Alle Lebewesen.", "tokens": ["Al\u00b7le", "Le\u00b7be\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Hund bellt.", "tokens": ["Ein", "Hund", "bellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Er kann nicht lesen.", "tokens": ["Er", "kann", "nicht", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er kann nicht schreiben.", "tokens": ["Er", "kann", "nicht", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wir k\u00f6nnen nicht bleiben.", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich lache.", "tokens": ["Ich", "la\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die L\u00f6cher sind die Hauptsache", "tokens": ["Die", "L\u00f6\u00b7cher", "sind", "die", "Haupt\u00b7sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "An einem Sieb.", "tokens": ["An", "ei\u00b7nem", "Sieb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich habe dich so lieb.", "tokens": ["Ich", "ha\u00b7be", "dich", "so", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}