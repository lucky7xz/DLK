{"textgrid.poem.53080": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Laxat sibi frena juventus", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein, lasst mir doch den Willen!", "tokens": ["Mein", ",", "lasst", "mir", "doch", "den", "Wil\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mag nicht trawrig seyn,", "tokens": ["Ich", "mag", "nicht", "traw\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe mich der Grillen,", "tokens": ["Ich", "ha\u00b7be", "mich", "der", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Kummers vnd der Pein", "tokens": ["Des", "Kum\u00b7mers", "vnd", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt kaum entladen k\u00f6nnen,", "tokens": ["Jetzt", "kaum", "ent\u00b7la\u00b7den", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr machet wieder Streit,", "tokens": ["Ihr", "ma\u00b7chet", "wie\u00b7der", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd wolt mir gantz nicht g\u00f6nnen", "tokens": ["Vnd", "wolt", "mir", "gantz", "nicht", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die kurtze Fr\u00f6ligkeit.", "tokens": ["Die", "kurt\u00b7ze", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df mich wol zu halten,", "tokens": ["Ich", "wei\u00df", "mich", "wol", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ja nicht mehr ein Kind;", "tokens": ["Bin", "ja", "nicht", "mehr", "ein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trawrt jhr mit ewren Alten", "tokens": ["Trawrt", "jhr", "mit", "ew\u00b7ren", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schon vnt\u00fcchtig sind,", "tokens": ["Die", "schon", "vn\u00b7t\u00fcch\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wehrt Ihr in meiner Bl\u00fcte,", "tokens": ["Wehrt", "Ihr", "in", "mei\u00b7ner", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr w\u00fcrdet so nicht stehn,", "tokens": ["Ihr", "w\u00fcr\u00b7det", "so", "nicht", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Z\u00fcgel dem Gem\u00fcthe", "tokens": ["Den", "Z\u00fc\u00b7gel", "dem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was weiter lassen gehn.", "tokens": ["Was", "wei\u00b7ter", "las\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie offt habt jhr gesaget,", "tokens": ["Wie", "offt", "habt", "jhr", "ge\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn wo ein junges Blut", "tokens": ["Wenn", "wo", "ein", "jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kalb hat au\u00dfgejaget:", "tokens": ["Das", "Kalb", "hat", "au\u00df\u00b7ge\u00b7ja\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pfui! Da\u00df es nie so gut", "tokens": ["Pfui", "!", "Da\u00df", "es", "nie", "so", "gut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit mir vorhin geschehen,", "tokens": ["Mit", "mir", "vor\u00b7hin", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach hett' ich noch zur\u00fcck", "tokens": ["Ach", "hett'", "ich", "noch", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Jahrchen oder Zehen,", "tokens": ["Ein", "Jahr\u00b7chen", "o\u00b7der", "Ze\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich wolt' erst sein ein Strick!", "tokens": ["Ich", "wolt'", "erst", "sein", "ein", "Strick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Offt habt jhr angefangen", "tokens": ["Offt", "habt", "jhr", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu sagen, wie ihr wol", "tokens": ["Zu", "sa\u00b7gen", ",", "wie", "ihr", "wol"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es vormals angegangen,", "tokens": ["Es", "vor\u00b7mals", "an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie jhr des Stockens voll", "tokens": ["Wie", "jhr", "des", "Sto\u00b7ckens", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gespielet vnd gesungen,", "tokens": ["Ge\u00b7spie\u00b7let", "vnd", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie jhr zu Fusse frisch", "tokens": ["Wie", "jhr", "zu", "Fus\u00b7se", "frisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Getantzet vnd gesprungen", "tokens": ["Ge\u00b7tant\u00b7zet", "vnd", "ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Offt vber St\u00fcel vnd Tisch.", "tokens": ["Offt", "vber", "St\u00fcel", "vnd", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Auch noch, wann vnterweilen", "tokens": ["Auch", "noch", ",", "wann", "vn\u00b7ter\u00b7wei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch eine Lust ber\u00fchrt,", "tokens": ["Euch", "ei\u00b7ne", "Lust", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie pflegt jhr dann zu eilen", "tokens": ["Wie", "pflegt", "jhr", "dann", "zu", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da, wo man T\u00e4ntze f\u00fchrt!", "tokens": ["Da", ",", "wo", "man", "T\u00e4nt\u00b7ze", "f\u00fchrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab' Euch h\u00f6ren singen", "tokens": ["Ich", "hab'", "Euch", "h\u00f6\u00b7ren", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Manch altes Buhlen-Lied,", "tokens": ["Manch", "al\u00b7tes", "Buh\u00b7len\u00b7Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das zeiget von den Dingen", "tokens": ["Das", "zei\u00b7get", "von", "den", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Ewrer Jugend Bl\u00fcet.", "tokens": ["In", "Ew\u00b7rer", "Ju\u00b7gend", "Bl\u00fcet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drumb lasst auch mich geniessen", "tokens": ["Drumb", "lasst", "auch", "mich", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir das Gl\u00fcck vnd Gott", "tokens": ["Was", "mir", "das", "Gl\u00fcck", "vnd", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ertheilet wollen wissen!", "tokens": ["Er\u00b7thei\u00b7let", "wol\u00b7len", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sorgen bleiche Rott'", "tokens": ["Der", "Sor\u00b7gen", "blei\u00b7che", "Rott'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist doch in stetem wachen,", "tokens": ["Ist", "doch", "in", "ste\u00b7tem", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd schawet, wie sie sich", "tokens": ["Vnd", "scha\u00b7wet", ",", "wie", "sie", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur an vns m\u00f6ge machen", "tokens": ["Nur", "an", "vns", "m\u00f6\u00b7ge", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch jhren Mord vnd Stich.", "tokens": ["Durch", "jhren", "Mord", "vnd", "Stich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die Zeit, so vntern H\u00e4nden", "tokens": ["Die", "Zeit", ",", "so", "vn\u00b7tern", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht ruhend k\u00f6mpt vnd fleucht,", "tokens": ["Nicht", "ru\u00b7hend", "k\u00f6mpt", "vnd", "fleucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sol man zur Lust anwenden,", "tokens": ["Sol", "man", "zur", "Lust", "an\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eh als sie von vns weicht;", "tokens": ["Eh", "als", "sie", "von", "vns", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Augenblick, die Fahrte,", "tokens": ["Das", "Au\u00b7gen\u00b7blick", ",", "die", "Fahr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In der ich sage, Nu!", "tokens": ["In", "der", "ich", "sa\u00b7ge", ",", "Nu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist mein; Auff die ich warte,", "tokens": ["Ist", "mein", ";", "Auff", "die", "ich", "war\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$.", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "K\u00f6mpt tausent F\u00e4llen zu.", "tokens": ["K\u00f6mpt", "tau\u00b7sent", "F\u00e4l\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}