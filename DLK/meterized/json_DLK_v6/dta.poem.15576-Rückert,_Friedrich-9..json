{"dta.poem.15576": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung halte fest: Gott wird dich nicht verlassen;", "tokens": ["Die", "Hoff\u00b7nung", "hal\u00b7te", "fest", ":", "Gott", "wird", "dich", "nicht", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Aergste das dir droht, er wird es dir erlassen.", "tokens": ["Das", "A\u00b7ergs\u00b7te", "das", "dir", "droht", ",", "er", "wird", "es", "dir", "er\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Und traf das Aergste dich, so bleib' in Zuversicht:", "tokens": ["Und", "traf", "das", "A\u00b7ergs\u00b7te", "dich", ",", "so", "bleib'", "in", "Zu\u00b7ver\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Hoffnung schlug dir fehl, doch Gott verlie\u00df dich nicht.", "tokens": ["Die", "Hoff\u00b7nung", "schlug", "dir", "fehl", ",", "doch", "Gott", "ver\u00b7lie\u00df", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja, da\u00df dich Gott nicht hat verlassen, mu\u00dft du sagen,", "tokens": ["Ja", ",", "da\u00df", "dich", "Gott", "nicht", "hat", "ver\u00b7las\u00b7sen", ",", "mu\u00dft", "du", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NN", "PTKNEG", "VAFIN", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da er die Kraft dir gibt das Aergste zu ertragen.", "tokens": ["Da", "er", "die", "Kraft", "dir", "gibt", "das", "A\u00b7ergs\u00b7te", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}