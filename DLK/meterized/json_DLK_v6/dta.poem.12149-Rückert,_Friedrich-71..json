{"dta.poem.12149": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Glaube pr\u00e4gt in seinem Thun sich aus,", "tokens": ["Des", "Men\u00b7schen", "Glau\u00b7be", "pr\u00e4gt", "in", "sei\u00b7nem", "Thun", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Formt seine Z\u00fcg' und blickt ihm zu dem Aug' heraus.", "tokens": ["Formt", "sei\u00b7ne", "Z\u00fcg'", "und", "blickt", "ihm", "zu", "dem", "Aug'", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sein Glaub' ist es der ihn aufrichtet oder b\u00fcckt,", "tokens": ["Sein", "Glaub'", "ist", "es", "der", "ihn", "auf\u00b7rich\u00b7tet", "o\u00b7der", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zum Himmel ihn erhebt, zum Boden niederdr\u00fcckt.", "tokens": ["Zum", "Him\u00b7mel", "ihn", "er\u00b7hebt", ",", "zum", "Bo\u00b7den", "nie\u00b7der\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,", "APPRART", "NN", "V