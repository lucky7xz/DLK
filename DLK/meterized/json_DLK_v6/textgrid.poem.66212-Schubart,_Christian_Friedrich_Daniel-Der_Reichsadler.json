{"textgrid.poem.66212": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Reichsadler", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Forscher in der Wappenkunde,", "tokens": ["Ihr", "For\u00b7scher", "in", "der", "Wap\u00b7pen\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was fragt ihr \u00e4ngstlich nach dem Grunde:", "tokens": ["Was", "fragt", "ihr", "\u00e4ngst\u00b7lich", "nach", "dem", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warum in jeder Schilderei", "tokens": ["Wa\u00b7rum", "in", "je\u00b7der", "Schil\u00b7de\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutsche Adler doppelk\u00f6pfig sei?", "tokens": ["Der", "deut\u00b7sche", "Ad\u00b7ler", "dop\u00b7pel\u00b7k\u00f6p\u00b7fig", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbzwei K\u00f6pfe,\u00ab sprecht ihr oft im Feuer,", "tokens": ["\u00bb", "zwei", "K\u00f6p\u00b7fe", ",", "\u00ab", "sprecht", "ihr", "oft", "im", "Feu\u00b7er", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "NN", "$,", "$(", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbsind ja ein wahres Ungeheuer,", "tokens": ["\u00bb", "sind", "ja", "ein", "wah\u00b7res", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und K\u00f6pfe noch dazu, wie die,", "tokens": ["Und", "K\u00f6p\u00b7fe", "noch", "da\u00b7zu", ",", "wie", "die", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PAV", "$,", "PWAV", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Voll bissiger Antipathie.\u00ab", "tokens": ["Voll", "bis\u00b7si\u00b7ger", "An\u00b7ti\u00b7pa\u00b7thie", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "O la\u00dft doch einmal nach, mit Forschen euch zu plagen!", "tokens": ["O", "la\u00dft", "doch", "ein\u00b7mal", "nach", ",", "mit", "For\u00b7schen", "euch", "zu", "pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Novellist sogar kann euch die Wahrheit sagen.", "tokens": ["Ein", "No\u00b7vel\u00b7list", "so\u00b7gar", "kann", "euch", "die", "Wahr\u00b7heit", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der eine Kopf, der ", "tokens": ["Der", "ei\u00b7ne", "Kopf", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Sanft scheint und desto sch\u00e4rfer pickt,", "tokens": ["Sanft", "scheint", "und", "des\u00b7to", "sch\u00e4r\u00b7fer", "pickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist Kaiser ", "tokens": ["Ist", "Kai\u00b7ser"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Der andre Kopf, der ", "tokens": ["Der", "and\u00b7re", "Kopf", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Scharf sieht und mit dem Schnabel haut,", "tokens": ["Scharf", "sieht", "und", "mit", "dem", "Schna\u00b7bel", "haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Warum sie aber uneins sind,", "tokens": ["Wa\u00b7rum", "sie", "a\u00b7ber", "un\u00b7eins", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Begreift beinah ein kleines Kind;", "tokens": ["Be\u00b7greift", "bei\u00b7nah", "ein", "klei\u00b7nes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie sind entzweit in dem gemeinen Falle:", "tokens": ["Sie", "sind", "ent\u00b7zweit", "in", "dem", "ge\u00b7mei\u00b7nen", "Fal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Was eine Kralle packt, packt auch die andre Kralle!", "tokens": ["Was", "ei\u00b7ne", "Kral\u00b7le", "packt", ",", "packt", "auch", "die", "and\u00b7re", "Kral\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum zerren sie so j\u00e4mmerlich \u2013", "tokens": ["Drum", "zer\u00b7ren", "sie", "so", "j\u00e4m\u00b7mer\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Vaterland, wie daurst du mich!", "tokens": ["Va\u00b7ter\u00b7land", ",", "wie", "daurst", "du", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}