{"dta.poem.12698": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen leidtragenden   chymicum.  \n D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die pulver, die er macht, die kranckheit zu besiegen,", "tokens": ["Die", "pul\u00b7ver", ",", "die", "er", "macht", ",", "die", "kran\u00b7ck\u00b7heit", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erblickte j\u00fcngst der tod. Was, sagt er, soll das seyn?", "tokens": ["Er\u00b7blick\u00b7te", "j\u00fcngst", "der", "tod", ".", "Was", ",", "sagt", "er", ",", "soll", "das", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "PWS", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ART", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und blie\u00df mit aller macht sein sterbe-pulver drein;", "tokens": ["Und", "blie\u00df", "mit", "al\u00b7ler", "macht", "sein", "ster\u00b7be\u00b7pul\u00b7ver", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sieht man nun sein kind in staub und pulver liegen.", "tokens": ["Da", "sieht", "man", "nun", "sein", "kind", "in", "staub", "und", "pul\u00b7ver", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "APPR", "ADJD", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}