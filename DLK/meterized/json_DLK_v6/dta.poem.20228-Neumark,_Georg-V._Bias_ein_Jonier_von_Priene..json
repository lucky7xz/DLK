{"dta.poem.20228": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Bias ein Jonier von  \n Priene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Einer ist des andern Wolff/ einer ist des andern", "tokens": ["Ei\u00b7ner", "ist", "des", "an\u00b7dern", "Wolff", "/", "ei\u00b7ner", "ist", "des", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NE", "$(", "PIS", "VAFIN", "ART", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Also sind die Menschen selbst ihr Verderben ohne", "tokens": ["Al\u00b7so", "sind", "die", "Men\u00b7schen", "selbst", "ihr", "Ver\u00b7der\u00b7ben", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}