{"textgrid.poem.57745": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Heide riecht nach Menschenblut", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Heide riecht nach Menschenblut", "tokens": ["Die", "Hei\u00b7de", "riecht", "nach", "Men\u00b7schen\u00b7blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und riecht nach Todesschwei\u00df,", "tokens": ["Und", "riecht", "nach", "To\u00b7des\u00b7schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blutig ist des Baches Flut,", "tokens": ["Und", "blu\u00b7tig", "ist", "des", "Ba\u00b7ches", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht so tr\u00e4g' und leis';", "tokens": ["Und", "geht", "so", "tr\u00e4g'", "und", "leis'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ging am Morgen flink und laut", "tokens": ["Und", "ging", "am", "Mor\u00b7gen", "flink", "und", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ging so hell und klar,", "tokens": ["Und", "ging", "so", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel guter M\u00e4nner rotes Blut", "tokens": ["Viel", "gu\u00b7ter", "M\u00e4n\u00b7ner", "ro\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hinein geronnen war.", "tokens": ["Hin\u00b7ein", "ge\u00b7ron\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Kaiser Karl sitzt stumm und still,", "tokens": ["Und", "Kai\u00b7ser", "Karl", "sitzt", "stumm", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Angesicht ist bla\u00df,", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "ist", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Blutdunst nicht vergehen will,", "tokens": ["Der", "Blut\u00b7dunst", "nicht", "ver\u00b7ge\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es qualmt das R\u00e4ucherfa\u00df;", "tokens": ["Es", "qualmt", "das", "R\u00e4u\u00b7cher\u00b7fa\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach Todesangstschwei\u00df riecht die Luft,", "tokens": ["Nach", "To\u00b7de\u00b7sangstsc\u00b7hwei\u00df", "riecht", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wind weht Blutgeruch,", "tokens": ["Der", "Wind", "weht", "Blut\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er weht zum Kaiserzelt hinein", "tokens": ["Er", "weht", "zum", "Kai\u00b7ser\u00b7zelt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Eines ganzen Volkes Fluch.", "tokens": ["Ei\u00b7nes", "gan\u00b7zen", "Vol\u00b7kes", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es wagt kein witzig Wort der Narr,", "tokens": ["Es", "wagt", "kein", "wit\u00b7zig", "Wort", "der", "Narr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Wort der Kardinal;", "tokens": ["Kein", "Wort", "der", "Kar\u00b7di\u00b7nal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Kaisers Augen blicken starr,", "tokens": ["Des", "Kai\u00b7sers", "Au\u00b7gen", "bli\u00b7cken", "starr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Mund ist eng und schmal;", "tokens": ["Sein", "Mund", "ist", "eng", "und", "schmal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Kaisers Lieblingslustmagd schaut", "tokens": ["Des", "Kai\u00b7sers", "Lieb\u00b7lings\u00b7lust\u00b7magd", "schaut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Furcht in ihren Scho\u00df,", "tokens": ["Voll", "Furcht", "in", "ih\u00b7ren", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein L\u00e4cheln gab ihr heut' der Herr,", "tokens": ["Kein", "L\u00e4\u00b7cheln", "gab", "ihr", "heut'", "der", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Zorn ist allzu gro\u00df.", "tokens": ["Sein", "Zorn", "ist", "all\u00b7zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Lager weht der Wind heran", "tokens": ["Vom", "La\u00b7ger", "weht", "der", "Wind", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gel\u00e4chter und Gesang,", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein blonder Sachsenfiedelmann", "tokens": ["Ein", "blon\u00b7der", "Sach\u00b7sen\u00b7fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erg\u00f6tzt mit Geigenklang", "tokens": ["Er\u00b7g\u00f6tzt", "mit", "Gei\u00b7gen\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Schelmenlied das Frankenvolk;", "tokens": ["Und", "Schel\u00b7men\u00b7lied", "das", "Fran\u00b7ken\u00b7volk", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der blasse Kaiser winkt,", "tokens": ["Der", "blas\u00b7se", "Kai\u00b7ser", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Wache eilt, zum Kaiserzelt", "tokens": ["Die", "Wa\u00b7che", "eilt", ",", "zum", "Kai\u00b7ser\u00b7zelt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den fremden Mann sie bringt.", "tokens": ["Den", "frem\u00b7den", "Mann", "sie", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der steht und starrt auf all die Pracht", "tokens": ["Der", "steht", "und", "starrt", "auf", "all", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blicket bl\u00f6d' und dumm,", "tokens": ["Und", "bli\u00b7cket", "bl\u00f6d'", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der W\u00fcrzwein hat ihn blind gemacht,", "tokens": ["Der", "W\u00fcrz\u00b7wein", "hat", "ihn", "blind", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein L\u00e4cheln geht rundum;", "tokens": ["Ein", "L\u00e4\u00b7cheln", "geht", "run\u00b7dum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Kaiser winkt, der Fiedler stellt", "tokens": ["Der", "Kai\u00b7ser", "winkt", ",", "der", "Fied\u00b7ler", "stellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich nach Gewohnheit hin,", "tokens": ["Sich", "nach", "Ge\u00b7wohn\u00b7heit", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bein \u00fcber Bein, den Kopf geneigt,", "tokens": ["Bein", "\u00fc\u00b7ber", "Bein", ",", "den", "Kopf", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fiedel an dem Kinn.", "tokens": ["Die", "Fie\u00b7del", "an", "dem", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Fiedel singt, die Fiedel klingt,", "tokens": ["Die", "Fie\u00b7del", "singt", ",", "die", "Fie\u00b7del", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn im gr\u00fcnen Hag", "tokens": ["Als", "wenn", "im", "gr\u00fc\u00b7nen", "Hag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus allen Zweigen lustig springt", "tokens": ["Aus", "al\u00b7len", "Zwei\u00b7gen", "lus\u00b7tig", "springt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bunten Finken Schlag;", "tokens": ["Der", "bun\u00b7ten", "Fin\u00b7ken", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Todesschwei\u00dfgeruch zerfliegt,", "tokens": ["Der", "To\u00b7des\u00b7schwei\u00df\u00b7ge\u00b7ruch", "zer\u00b7fliegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Blutdunst ist zerweht,", "tokens": ["Der", "Blut\u00b7dunst", "ist", "zer\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um Kaiser Karels dunkle Stirn", "tokens": ["Um", "Kai\u00b7ser", "Ka\u00b7rels", "dunk\u00b7le", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein heller Schimmer geht.", "tokens": ["Ein", "hel\u00b7ler", "Schim\u00b7mer", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Fiedel singt, die Fiedel klingt,", "tokens": ["Die", "Fie\u00b7del", "singt", ",", "die", "Fie\u00b7del", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lacht des Spielmanns Mund,", "tokens": ["Es", "lacht", "des", "Spiel\u00b7manns", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liebessehnsuchtslied er singt,", "tokens": ["Ein", "Lie\u00b7bes\u00b7sehn\u00b7suchts\u00b7lied", "er", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das klingt so weh und wund;", "tokens": ["Das", "klingt", "so", "weh", "und", "wund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Kaisers Augen werden mild,", "tokens": ["Des", "Kai\u00b7sers", "Au\u00b7gen", "wer\u00b7den", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er winkt, der Schenk gie\u00dft ein", "tokens": ["Er", "winkt", ",", "der", "Schenk", "gie\u00dft", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und reicht dem blonden Fiedelmann", "tokens": ["Und", "reicht", "dem", "blon\u00b7den", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kelch mit rotem Wein.", "tokens": ["Den", "Kelch", "mit", "ro\u00b7tem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der dankt und trinkt, die Neige rinnt", "tokens": ["Der", "dankt", "und", "trinkt", ",", "die", "Nei\u00b7ge", "rinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blutrot ihm auf die Hand,", "tokens": ["Blut\u00b7rot", "ihm", "auf", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er starrt drauf hin und sinnt und sinnt,", "tokens": ["Er", "starrt", "drauf", "hin", "und", "sinnt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachtwind singt im Land;", "tokens": ["Der", "Nacht\u00b7wind", "singt", "im", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Spielmann wirft den Kopf zur\u00fcck,", "tokens": ["Der", "Spiel\u00b7mann", "wirf