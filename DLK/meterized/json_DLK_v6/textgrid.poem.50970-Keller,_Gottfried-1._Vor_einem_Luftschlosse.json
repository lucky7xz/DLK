{"textgrid.poem.50970": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Vor einem Luftschlosse", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne B\u00fcrgerin, sieh, der Mai", "tokens": ["Sch\u00f6\u00b7ne", "B\u00fcr\u00b7ge\u00b7rin", ",", "sieh", ",", "der", "Mai"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Flutet um deine Fenster!", "tokens": ["Flu\u00b7tet", "um", "dei\u00b7ne", "Fens\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Alle Seelen sind nun frei,", "tokens": ["Al\u00b7le", "See\u00b7len", "sind", "nun", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es zerflie\u00dfen der Tyrannei", "tokens": ["Und", "es", "zer\u00b7flie\u00b7\u00dfen", "der", "Ty\u00b7ran\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gr\u00e4mliche Gespenster!", "tokens": ["Gr\u00e4m\u00b7li\u00b7che", "Ge\u00b7spens\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In die Tiefe tauche k\u00fchn,", "tokens": ["In", "die", "Tie\u00b7fe", "tau\u00b7che", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewige Jugend zu werben,", "tokens": ["E\u00b7wi\u00b7ge", "Ju\u00b7gend", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wo die B\u00e4ume des Lebens bl\u00fchn", "tokens": ["Wo", "die", "B\u00e4u\u00b7me", "des", "Le\u00b7bens", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Augen wie Sterne gl\u00fchn;", "tokens": ["Und", "die", "Au\u00b7gen", "wie", "Ster\u00b7ne", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Droben bei dir ist Sterben!", "tokens": ["Dro\u00b7ben", "bei", "dir", "ist", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "L\u00f6se der Krone g\u00fcldenen Glanz", "tokens": ["L\u00f6\u00b7se", "der", "Kro\u00b7ne", "g\u00fcl\u00b7de\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus den Lockenringen!", "tokens": ["Aus", "den", "Lo\u00b7cken\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirf sie herab! im klingenden Tanz", "tokens": ["Wirf", "sie", "her\u00b7ab", "!", "im", "klin\u00b7gen\u00b7den", "Tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einen duftigen Rosenkranz", "tokens": ["Ei\u00b7nen", "duf\u00b7ti\u00b7gen", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wollen wir froh dir schlingen!", "tokens": ["Wol\u00b7len", "wir", "froh", "dir", "schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "F\u00fchle, du Engel, dies heilige Wehn,", "tokens": ["F\u00fch\u00b7le", ",", "du", "En\u00b7gel", ",", "dies", "hei\u00b7li\u00b7ge", "Wehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NE", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Das allm\u00e4chtige Treiben!", "tokens": ["Das", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ja, dein Himmel wird untergehn", "tokens": ["Ja", ",", "dein", "Him\u00b7mel", "wird", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und ein sch\u00f6nerer auferstehn \u2013", "tokens": ["Und", "ein", "sch\u00f6\u00b7ne\u00b7rer", "auf\u00b7er\u00b7stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVINF", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Willst du ein Engel bleiben?", "tokens": ["Willst", "du", "ein", "En\u00b7gel", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht wie Luna in schweigender Nacht", "tokens": ["Nicht", "wie", "Lu\u00b7na", "in", "schwei\u00b7gen\u00b7der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "K\u00fc\u00dfte den tr\u00e4umenden Schl\u00e4fer;", "tokens": ["K\u00fc\u00df\u00b7te", "den", "tr\u00e4u\u00b7men\u00b7den", "Schl\u00e4\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Komm, wenn der sonnige Tag uns lacht,", "tokens": ["Komm", ",", "wenn", "der", "son\u00b7ni\u00b7ge", "Tag", "uns", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da\u00df das alte Lied erwacht:", "tokens": ["Da\u00df", "das", "al\u00b7te", "Lied", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nigstochter und Sch\u00e4fer!", "tokens": ["K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "und", "Sch\u00e4\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}