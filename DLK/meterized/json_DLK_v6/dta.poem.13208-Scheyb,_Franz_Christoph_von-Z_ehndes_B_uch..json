{"dta.poem.13208": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nigliche Mund begunnte kaum zu schweigen;", "tokens": ["Der", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Mund", "be\u00b7gunn\u00b7te", "kaum", "zu", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gieng der gute Rath, sich vor dem Thron zu neigen;", "tokens": ["So", "gieng", "der", "gu\u00b7te", "Rath", ",", "sich", "vor", "dem", "Thron", "zu", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem sich stille Lust im Saal versp\u00fchren lie\u00df,", "tokens": ["In\u00b7dem", "sich", "stil\u00b7le", "Lust", "im", "Saal", "ver\u00b7sp\u00fch\u00b7ren", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die mit gebrochnem Thon sich von den Lippen stie\u00df,", "tokens": ["Die", "mit", "ge\u00b7broch\u00b7nem", "Thon", "sich", "von", "den", "Lip\u00b7pen", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "315Und tausend Regungen mit froher Ehrfurcht h\u00e4gte;", "tokens": ["tau\u00b7send", "Re\u00b7gun\u00b7gen", "mit", "fro\u00b7her", "Ehr\u00b7furcht", "h\u00e4g\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ein murmelndes Ger\u00e4usch sich durch die Reihen regte.", "tokens": ["Ein", "mur\u00b7meln\u00b7des", "Ge\u00b7r\u00e4usch", "sich", "durch", "die", "Rei\u00b7hen", "reg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Grei\u00df schien ausser sich; sein Auge war benezt;", "tokens": ["Der", "Grei\u00df", "schien", "aus\u00b7ser", "sich", ";", "sein", "Au\u00b7ge", "war", "be\u00b7nezt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Mund der T\u00fcchtigkeit zu reden fast entsezt.", "tokens": ["Der", "Mund", "der", "T\u00fcch\u00b7tig\u00b7keit", "zu", "re\u00b7den", "fast", "ent\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was Freude, Lieb und Trost, was Eifer und Entz\u00fccken", "tokens": ["Was", "Freu\u00b7de", ",", "Lieb", "und", "Trost", ",", "was", "Ei\u00b7fer", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "NN", "KON", "NN", "$,", "PWS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "320In einer Seele wirckt, sah man in seinen Blicken.", "tokens": ["ei\u00b7ner", "See\u00b7le", "wirckt", ",", "sah", "man", "in", "sei\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Inzwischen drang man sich bi\u00df zu dem Kinder Chor;", "tokens": ["I\u00b7nzwi\u00b7schen", "drang", "man", "sich", "bi\u00df", "zu", "dem", "Kin\u00b7der", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.12": {"text": "So trug er endlich doch beherzt die Worte vor:", "tokens": ["So", "trug", "er", "end\u00b7lich", "doch", "be\u00b7herzt", "die", "Wor\u00b7te", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}