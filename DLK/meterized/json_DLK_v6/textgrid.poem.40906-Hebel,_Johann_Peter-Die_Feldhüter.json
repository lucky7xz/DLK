{"textgrid.poem.40906": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Feldh\u00fcter", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinte Wald und Berg bis an die duftige Wulke,", "tokens": ["Hin\u00b7te", "Wald", "und", "Berg", "bis", "an", "die", "duf\u00b7ti\u00b7ge", "Wul\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "vorne Matte voll Chlee, und Saat und goldene Lewat,", "tokens": ["vor\u00b7ne", "Mat\u00b7te", "voll", "Chlee", ",", "und", "Saat", "und", "gol\u00b7de\u00b7ne", "Le\u00b7wat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "NN", "$,", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "stoht e H\u00fctten im Feld und in der einsame Mittnacht.", "tokens": ["stoht", "e", "H\u00fct\u00b7ten", "im", "Feld", "und", "in", "der", "ein\u00b7sa\u00b7me", "Mitt\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "APPRART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Numme d'Sterne wachen, und numme no d'Feldberger Wiese,", "tokens": ["Num\u00b7me", "d'\u00b7S\u00b7ter\u00b7ne", "wa\u00b7chen", ",", "und", "num\u00b7me", "no", "d'\u00b7Feld\u00b7ber\u00b7ger", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KON", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "und der Schuhu im Wald und \u00f6bbe Geister und Hirze.", "tokens": ["und", "der", "Schu\u00b7hu", "im", "Wald", "und", "\u00f6b\u00b7be", "Geis\u00b7ter", "und", "Hir\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Aber im H\u00fcttli sitzen und h\u00fcte die buschige Felder", "tokens": ["A\u00b7ber", "im", "H\u00fctt\u00b7li", "sit\u00b7zen", "und", "h\u00fc\u00b7te", "die", "bu\u00b7schi\u00b7ge", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVINF", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.7": {"text": "'s Meiers muntere Fritz und 's M\u00fcllers lockige Heiner", "tokens": ["'s", "Mei\u00b7ers", "mun\u00b7te\u00b7re", "Fritz", "und", "'s", "M\u00fcl\u00b7lers", "lo\u00b7cki\u00b7ge", "Hei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NE", "KON", "PPER", "NE", "ADJA", "NN"], "meter": "---+--+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "\u00bbheinerli\u00ab, seit der Fritz, \u00bbder Schlof goht lisli um d'H\u00fctte.", "tokens": ["\u00bb", "hei\u00b7ner\u00b7li", "\u00ab", ",", "seit", "der", "Fritz", ",", "\u00bb", "der", "Schlof", "goht", "lis\u00b7li", "um", "d'\u00b7H\u00fct\u00b7te", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$(", "$,", "APPR", "ART", "NE", "$,", "$(", "ART", "NN", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.9": {"text": "Lueg, jez chuunt er is inen, und lueg doch weger, er het di!", "tokens": ["Lu\u00b7eg", ",", "jez", "chu\u00b7unt", "er", "is", "i\u00b7nen", ",", "und", "lu\u00b7eg", "doch", "we\u00b7ger", ",", "er", "het", "di", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+---+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.10": {"text": "Weidli, chumm ins Gr\u00fcn! Mer wenn im lieblige Wechsel", "tokens": ["Weid\u00b7li", ",", "chumm", "ins", "Gr\u00fcn", "!", "Mer", "wenn", "im", "lieb\u00b7li\u00b7ge", "Wech\u00b7sel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "APPRART", "NN", "$.", "PIAT", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "mitenander singen. Es weiht e lustige Nachtluft,", "tokens": ["mi\u00b7ten\u00b7an\u00b7der", "sin\u00b7gen", ".", "Es", "weiht", "e", "lus\u00b7ti\u00b7ge", "Nacht\u00b7luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "PPER", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--++", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "gv\u00e4tterlet mittem Laub und exerzirt mit de Halme:", "tokens": ["gv\u00e4t\u00b7ter\u00b7let", "mit\u00b7tem", "Laub", "und", "ex\u00b7er\u00b7zirt", "mit", "de", "Hal\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "\u00bbrechts um kehrt euch! Links her stellt euch! Nonemol rechts um!\u00ab", "tokens": ["\u00bb", "rechts", "um", "kehrt", "euch", "!", "Links", "her", "stellt", "euch", "!", "No\u00b7ne\u00b7mol", "rechts", "um", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "VVFIN", "PPER", "$.", "NE", "APZR", "VVFIN", "PPER", "$.", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "Aber 's M\u00fcllers Heiner mit siner lockige Stirne", "tokens": ["A\u00b7ber", "'s", "M\u00fcl\u00b7lers", "Hei\u00b7ner", "mit", "si\u00b7ner", "lo\u00b7cki\u00b7ge", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "NE", "APPR", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "streckt si und stoht uf, und sucht si gl\u00e4serni R\u00f6hre.", "tokens": ["streckt", "si", "und", "stoht", "uf", ",", "und", "sucht", "si", "gl\u00e4\u00b7ser\u00b7ni", "R\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "FM", "FM", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "\u00bbfritzli, sto\u00df mi nit!\u00ab Jez stehn sie gegen enander,", "tokens": ["\u00bb", "fritz\u00b7li", ",", "sto\u00df", "mi", "nit", "!", "\u00ab", "Jez", "stehn", "sie", "ge\u00b7gen", "en\u00b7an\u00b7der", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "VVFIN", "NE", "PTKNEG", "$.", "$(", "NE", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "der am Chriesibaum, der an der duftige Linde,", "tokens": ["der", "am", "Chrie\u00b7si\u00b7baum", ",", "der", "an", "der", "duf\u00b7ti\u00b7ge", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "und probire d'T\u00f6n in ihrer H\u00f6chi und Tiefi,", "tokens": ["und", "pro\u00b7bi\u00b7re", "d'\u00b7T\u00f6n", "in", "ih\u00b7rer", "H\u00f6\u00b7chi", "und", "Tie\u00b7fi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "KON", "NE", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "setzen ab, und setzen a. \u00bbSing, Heinerli, du z'erst!\u00ab", "tokens": ["set\u00b7zen", "ab", ",", "und", "set\u00b7zen", "a.", "\u00bb", "Sing", ",", "Hei\u00b7ner\u00b7li", ",", "du", "z'\u00b7erst", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PIS", "$(", "NE", "$,", "NE", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+--+---+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "seit der Fritz, \u00bbde hesch doch, traui, n\u00e4ume ne Sch\u00e4tzli.\u00ab", "tokens": ["seit", "der", "Fritz", ",", "\u00bb", "de", "hesch", "doch", ",", "trau\u00b7i", ",", "n\u00e4u\u00b7me", "ne", "Sch\u00e4tz\u00b7li", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NE", "$,", "$(", "NE", "ADJD", "ADV", "$,", "VVFIN", "$,", "VVFIN", "NE", "NE", "$.", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}}}}