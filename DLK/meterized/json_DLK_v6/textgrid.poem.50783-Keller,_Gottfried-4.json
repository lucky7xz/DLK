{"textgrid.poem.50783": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben mir, als sie der Tod belogen,", "tokens": ["Sie", "ha\u00b7ben", "mir", ",", "als", "sie", "der", "Tod", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie's scheint, die Sonntagsweste angezogen:", "tokens": ["Wie's", "scheint", ",", "die", "Sonn\u00b7tags\u00b7wes\u00b7te", "an\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In ihren Taschen fand ich einen alten", "tokens": ["In", "ih\u00b7ren", "Ta\u00b7schen", "fand", "ich", "ei\u00b7nen", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zahnstocher und ein Bleistift aufbehalten.", "tokens": ["Zahn\u00b7sto\u00b7cher", "und", "ein", "Blei\u00b7stift", "auf\u00b7be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst gab es Tage, wo man zum Geleite", "tokens": ["Einst", "gab", "es", "Ta\u00b7ge", ",", "wo", "man", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PWAV", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Toten Schwert und Pfeile legt' zur Seite: \u2013", "tokens": ["Den", "To\u00b7ten", "Schwert", "und", "Pfei\u00b7le", "legt'", "zur", "Sei\u00b7te", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schm\u00e4hlich Jahrhundert du, das seinen Leichen", "tokens": ["Schm\u00e4h\u00b7lich", "Jahr\u00b7hun\u00b7dert", "du", ",", "das", "sei\u00b7nen", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zahnstocher nur und Bleistift wei\u00df zu reichen!", "tokens": ["Zahn\u00b7sto\u00b7cher", "nur", "und", "Blei\u00b7stift", "wei\u00df", "zu", "rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}