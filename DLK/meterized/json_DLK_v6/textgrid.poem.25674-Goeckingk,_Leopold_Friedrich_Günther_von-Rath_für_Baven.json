{"textgrid.poem.25674": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Rath f\u00fcr Baven", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr schlechte Malereyen, die", "tokens": ["Sehr", "schlech\u00b7te", "Ma\u00b7le\u00b7re\u00b7yen", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus Herkulanums Schutt gegraben,", "tokens": ["Aus", "Her\u00b7ku\u00b7la\u00b7nums", "Schutt", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts wen'ger sich vermuthet haben,", "tokens": ["Nichts", "wen'\u00b7ger", "sich", "ver\u00b7mu\u00b7thet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "PRF", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind itzt die Seltenheit von mancher Gallerie.", "tokens": ["Sind", "itzt", "die", "Sel\u00b7ten\u00b7heit", "von", "man\u00b7cher", "Gal\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ha Bav! da doch auf dieser Erde", "tokens": ["Ha", "Bav", "!", "da", "doch", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Mittel ist, da\u00df deine Poesie", "tokens": ["Kein", "Mit\u00b7tel", "ist", ",", "da\u00df", "dei\u00b7ne", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Zeit gesch\u00e4tzt, gelesen werde,", "tokens": ["Zur", "Zeit", "ge\u00b7sch\u00e4tzt", ",", "ge\u00b7le\u00b7sen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So rath' ich dir: Vergrabe sie.", "tokens": ["So", "ra\u00b7th'", "ich", "dir", ":", "Ver\u00b7gra\u00b7be", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}