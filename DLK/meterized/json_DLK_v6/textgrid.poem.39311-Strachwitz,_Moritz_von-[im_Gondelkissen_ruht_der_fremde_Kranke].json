{"textgrid.poem.39311": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[im Gondelkissen ruht der fremde Kranke]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Gondelkissen ruht der fremde Kranke,", "tokens": ["Im", "Gon\u00b7del\u00b7kis\u00b7sen", "ruht", "der", "frem\u00b7de", "Kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich sonnend in der wundervollen Kl\u00e4re,", "tokens": ["Sich", "son\u00b7nend", "in", "der", "wun\u00b7der\u00b7vol\u00b7len", "Kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da st\u00fcrmt ihn auf ein t\u00f6dlicher Gedanke:", "tokens": ["Da", "st\u00fcrmt", "ihn", "auf", "ein", "t\u00f6d\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn ich wirklich nun gestorben w\u00e4re", "tokens": ["Und", "wenn", "ich", "wirk\u00b7lich", "nun", "ge\u00b7stor\u00b7ben", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meines Dichtens ungl\u00fccksel'ger Schatte", "tokens": ["Und", "mei\u00b7nes", "Dich\u00b7tens", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "Schat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dahingeweht im Hauche dieser Meere,", "tokens": ["Da\u00b7hin\u00b7ge\u00b7weht", "im", "Hau\u00b7che", "die\u00b7ser", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann k\u00e4mest Du, die ich verloren hatte,", "tokens": ["Dann", "k\u00e4\u00b7mest", "Du", ",", "die", "ich", "ver\u00b7lo\u00b7ren", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht hierher, Venedigs Glanz zu schauen,", "tokens": ["Viel\u00b7leicht", "hier\u00b7her", ",", "Ve\u00b7ne\u00b7digs", "Glanz", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und mit Dir k\u00e4me ein geliebter Gatte!", "tokens": ["Und", "mit", "Dir", "k\u00e4\u00b7me", "ein", "ge\u00b7lieb\u00b7ter", "Gat\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sehe schon in Deinem Auge tauen", "tokens": ["Ich", "se\u00b7he", "schon", "in", "Dei\u00b7nem", "Au\u00b7ge", "tau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der reinsten Seele keuschestes Entz\u00fccken,", "tokens": ["Der", "reins\u00b7ten", "See\u00b7le", "keu\u00b7sches\u00b7tes", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie der Vollmond schwimmt im Dunkelblauen.", "tokens": ["So", "wie", "der", "Voll\u00b7mond", "schwimmt", "im", "Dun\u00b7kel\u00b7blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich seh' Dich gehn mit wundersel'gen Blicken", "tokens": ["Ich", "seh'", "Dich", "gehn", "mit", "wun\u00b7der\u00b7sel'\u00b7gen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch diese duftgewobenen Arkaden,", "tokens": ["Durch", "die\u00b7se", "duft\u00b7ge\u00b7wo\u00b7be\u00b7nen", "Ar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die feenhaft sich ineinander stricken!", "tokens": ["Die", "fe\u00b7en\u00b7haft", "sich", "in\u00b7ein\u00b7an\u00b7der", "stri\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich seh' Dich trunken dann im Glanze baden,", "tokens": ["Ich", "seh'", "Dich", "trun\u00b7ken", "dann", "im", "Glan\u00b7ze", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn der Piazza Feiern\u00e4chte blinken", "tokens": ["Wenn", "der", "Piaz\u00b7za", "Fei\u00b7er\u00b7n\u00e4ch\u00b7te", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ganz Venedig schwimmt in Serenaden.", "tokens": ["Und", "ganz", "Ve\u00b7ne\u00b7dig", "schwimmt", "in", "Se\u00b7re\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "APPR", "NE", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "O stolzes Gl\u00fcck, Dein selig ", "tokens": ["O", "stol\u00b7zes", "Gl\u00fcck", ",", "Dein", "se\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Hand in Hand auf einem Meer zu schweben,", "tokens": ["Und", "Hand", "in", "Hand", "auf", "ei\u00b7nem", "Meer", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin Mond und Sterne jubelnd untersinken.", "tokens": ["Drin", "Mond", "und", "Ster\u00b7ne", "ju\u00b7belnd", "un\u00b7ter\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch mitten in dem farbenreichsten Leben", "tokens": ["Doch", "mit\u00b7ten", "in", "dem", "far\u00b7ben\u00b7reichs\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll sich vor Dir ein l\u00e4ngst vergess'ner Name,", "tokens": ["Soll", "sich", "vor", "Dir", "ein", "l\u00e4ngst", "ver\u00b7ge\u00b7ss'\u00b7ner", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Des armen Tr\u00e4umers bleich Gespenst erheben!", "tokens": ["Des", "ar\u00b7men", "Tr\u00e4u\u00b7mers", "bleich", "Ge\u00b7spenst", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und sprechen soll es mit melod'schem Grame,", "tokens": ["Und", "spre\u00b7chen", "soll", "es", "mit", "me\u00b7lo\u00b7d'\u00b7schem", "Gra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie ferner Wellen klagendes Getose:", "tokens": ["Wie", "fer\u00b7ner", "Wel\u00b7len", "kla\u00b7gen\u00b7des", "Ge\u00b7to\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbla\u00df mich Dein F\u00fchrer sein, o sch\u00f6ne Dame!", "tokens": ["\u00bb", "la\u00df", "mich", "Dein", "F\u00fch\u00b7rer", "sein", ",", "o", "sch\u00f6\u00b7ne", "Da\u00b7me", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "VAINF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Bis zu des Abgrunds muschelreichem Moose", "tokens": ["Bis", "zu", "des", "Ab\u00b7grunds", "mu\u00b7schel\u00b7rei\u00b7chem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kenn' ich Venedig und ich will erschlie\u00dfen", "tokens": ["Kenn'", "ich", "Ve\u00b7ne\u00b7dig", "und", "ich", "will", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den tiefsten Kelch Dir dieser Meeresrose.", "tokens": ["Den", "tiefs\u00b7ten", "Kelch", "Dir", "die\u00b7ser", "Mee\u00b7res\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Als man mich damals ri\u00df von Deinen F\u00fc\u00dfen,", "tokens": ["Als", "man", "mich", "da\u00b7mals", "ri\u00df", "von", "Dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sucht' ich noch dies Ufer zu erreichen,", "tokens": ["Da", "sucht'", "ich", "noch", "dies", "U\u00b7fer", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um meinen Schmerz erhaben zu genie\u00dfen!", "tokens": ["Um", "mei\u00b7nen", "Schmerz", "er\u00b7ha\u00b7ben", "zu", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Hier prangt der Tod mit tausend Siegeszeichen,", "tokens": ["Hier", "prangt", "der", "Tod", "mit", "tau\u00b7send", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ew'ge Sch\u00f6nheit strahlt von seiner Hippe,", "tokens": ["Doch", "ew'\u00b7ge", "Sch\u00f6n\u00b7heit", "strahlt", "von", "sei\u00b7ner", "Hip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00f6lbt sich glorreich \u00fcber stolzen Leichen!", "tokens": ["Und", "w\u00f6lbt", "sich", "glor\u00b7reich", "\u00fc\u00b7ber", "stol\u00b7zen", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Es wirft die Kunst um bleichende Gerippe", "tokens": ["Es", "wirft", "die", "Kunst", "um", "blei\u00b7chen\u00b7de", "Ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Purpurkleid im reinsten Farbentone,", "tokens": ["Ihr", "Pur\u00b7pur\u00b7kleid", "im", "reins\u00b7ten", "Far\u00b7ben\u00b7to\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis da\u00df sie l\u00e4cheln, s\u00fc\u00df wie Deine Lippe! \u2013", "tokens": ["Bis", "da\u00df", "sie", "l\u00e4\u00b7cheln", ",", "s\u00fc\u00df", "wie", "Dei\u00b7ne", "Lip\u00b7pe", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Sprich! bin ich nicht ein guter Cicerone?", "tokens": ["Sprich", "!", "bin", "ich", "nicht", "ein", "gu\u00b7ter", "Ci\u00b7ce\u00b7ro\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie bl\u00fcht Venedig! Doch der Hauch der Gr\u00fcfte", "tokens": ["Wie", "bl\u00fcht", "Ve\u00b7ne\u00b7dig", "!", "Doch", "der", "Hauch", "der", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NE", "$.", "KON", "ART", "NN", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wogt fast berauschend aus der Blumenkrone.", "tokens": ["Wogt", "fast", "be\u00b7rau\u00b7schend", "aus", "der", "Blu\u00b7men\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Durch all' den Jubel goldgestirnter L\u00fcfte.\u00ab", "tokens": ["Durch", "all'", "den", "Ju\u00b7bel", "gold\u00b7ges\u00b7tirn\u00b7ter", "L\u00fcf\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}