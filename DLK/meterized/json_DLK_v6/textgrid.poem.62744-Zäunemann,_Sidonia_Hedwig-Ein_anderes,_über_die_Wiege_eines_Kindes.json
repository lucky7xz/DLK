{"textgrid.poem.62744": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein anderes, \u00fcber die Wiege eines Kindes", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00e4fst in Ruh, und bildest dir nicht ein,", "tokens": ["Du", "schl\u00e4fst", "in", "Ruh", ",", "und", "bil\u00b7dest", "dir", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kleine Wiege werde", "tokens": ["Die", "klei\u00b7ne", "Wie\u00b7ge", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf dieser schn\u00f6den Erde,", "tokens": ["Auf", "die\u00b7ser", "schn\u00f6\u00b7den", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Vorbild deines gr\u00f6\u00dfren Schicksals seyn.", "tokens": ["Das", "Vor\u00b7bild", "dei\u00b7nes", "gr\u00f6\u00df\u00b7ren", "Schick\u00b7sals", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wiege wirft dich hin und her:", "tokens": ["Die", "Wie\u00b7ge", "wirft", "dich", "hin", "und", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wirst Du auch nach mehren Jahren", "tokens": ["So", "wirst", "Du", "auch", "nach", "meh\u00b7ren", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Schicksals Spielwerk wohl erfahren.", "tokens": ["Des", "Schick\u00b7sals", "Spiel\u00b7werk", "wohl", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird sich stets bem\u00fchn,", "tokens": ["Es", "wird", "sich", "stets", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dich \u00f6fters hin und her zu ziehn,", "tokens": ["Dich", "\u00f6f\u00b7ters", "hin", "und", "her", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}