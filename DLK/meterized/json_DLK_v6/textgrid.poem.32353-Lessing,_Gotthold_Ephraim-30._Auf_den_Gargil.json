{"textgrid.poem.32353": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "30. Auf den Gargil", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit richtrisch scharfem Kiel durchackert seine Lieder", "tokens": ["Mit", "rich\u00b7trisch", "schar\u00b7fem", "Kiel", "durch\u00b7a\u00b7ckert", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gargil. Ins neunte Jahr schreibt, l\u00f6scht und schreibt er wieder.", "tokens": ["Gar\u00b7gil", ".", "Ins", "neun\u00b7te", "Jahr", "schreibt", ",", "l\u00f6scht", "und", "schreibt", "er", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Lied ist Lieb' und Wein. Kann man es ihm verdenken,", "tokens": ["Sein", "Lied", "ist", "Lieb'", "und", "Wein", ".", "Kann", "man", "es", "ihm", "ver\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$.", "VMFIN", "PIS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er der Nachwelt will vollkommne Possen schenken?", "tokens": ["Da\u00df", "er", "der", "Nach\u00b7welt", "will", "voll\u00b7komm\u00b7ne", "Pos\u00b7sen", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}