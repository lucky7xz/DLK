{"textgrid.poem.24709": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging hernieder weite Bergesstiegen", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging hernieder weite Bergesstiegen", "tokens": ["Ich", "ging", "her\u00b7nie\u00b7der", "wei\u00b7te", "Ber\u00b7ges\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fchlt im wundervollen Netz mich liegen,", "tokens": ["Und", "f\u00fchlt", "im", "wun\u00b7der\u00b7vol\u00b7len", "Netz", "mich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Gottes Netz, im Lebenstraum gefangen.", "tokens": ["In", "Got\u00b7tes", "Netz", ",", "im", "Le\u00b7ben\u00b7straum", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Winde liefen und die V\u00f6gel sangen.", "tokens": ["Die", "Win\u00b7de", "lie\u00b7fen", "und", "die", "V\u00f6\u00b7gel", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie trug, wie trug das Tal den Wasserspiegel!", "tokens": ["Wie", "trug", ",", "wie", "trug", "das", "Tal", "den", "Was\u00b7ser\u00b7spie\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie rauschend stand der Wald, wie schwoll der H\u00fcgel!", "tokens": ["Wie", "rau\u00b7schend", "stand", "der", "Wald", ",", "wie", "schwoll", "der", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch flog ein Falk, still leuchtete der Raum:", "tokens": ["Hoch", "flog", "ein", "Falk", ",", "still", "leuch\u00b7te\u00b7te", "der", "Raum", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Im Leben lag mein Herz, in Tod und Traum.", "tokens": ["Im", "Le\u00b7ben", "lag", "mein", "Herz", ",", "in", "Tod", "und", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}