{"textgrid.poem.58055": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "93. Gebeugt unter alle Menschen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seel', leg dich stets im Demutssinn", "tokens": ["Seel'", ",", "leg", "dich", "stets", "im", "De\u00b7muts\u00b7sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu aller Menschen F\u00fc\u00dfen hin!", "tokens": ["Zu", "al\u00b7ler", "Men\u00b7schen", "F\u00fc\u00b7\u00dfen", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird dir 'was B\u00f6ses angetan,", "tokens": ["Wird", "dir", "'was", "B\u00f6\u00b7ses", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So nimm es ohne Murren an!", "tokens": ["So", "nimm", "es", "oh\u00b7ne", "Mur\u00b7ren", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sich in sich noch etwas d\u00fcnket,", "tokens": ["Wer", "sich", "in", "sich", "noch", "et\u00b7was", "d\u00fcn\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PRF", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Gott, o Elend! wahrlich stinket;", "tokens": ["Vor", "Gott", ",", "o", "E\u00b7lend", "!", "wahr\u00b7lich", "stin\u00b7ket", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "FM", "NN", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist sonst 'was Gut's an deinem Leben,", "tokens": ["Ist", "sonst", "'was", "Gut's", "an", "dei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So bleibt es Gottes, der's gegeben.", "tokens": ["So", "bleibt", "es", "Got\u00b7tes", ",", "der's", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}