{"textgrid.poem.56680": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Okkulta", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch ihren Leib die Sterne scheinen,", "tokens": ["Durch", "ih\u00b7ren", "Leib", "die", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch ihren K\u00f6rper bl\u00e4st der Wind,", "tokens": ["durch", "ih\u00b7ren", "K\u00f6r\u00b7per", "bl\u00e4st", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "f\u00fcr diese ewig \u00dcberreinen", "tokens": ["f\u00fcr", "die\u00b7se", "e\u00b7wig", "\u00dc\u00b7berr\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sind unsere Sinne taub und blind.", "tokens": ["sind", "un\u00b7se\u00b7re", "Sin\u00b7ne", "taub", "und", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie lieben nicht wie Menschenkinder", "tokens": ["Sie", "lie\u00b7ben", "nicht", "wie", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und gr\u00fcbeln nicht dem Dasein nach", "tokens": ["und", "gr\u00fc\u00b7beln", "nicht", "dem", "Da\u00b7sein", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "t\u00f6richt und blind und immer blinder", "tokens": ["t\u00f6\u00b7richt", "und", "blind", "und", "im\u00b7mer", "blin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr Wirklichkeit und Licht und Tag.", "tokens": ["f\u00fcr", "Wirk\u00b7lich\u00b7keit", "und", "Licht", "und", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hungern nicht, sie dursten nicht,", "tokens": ["Sie", "hun\u00b7gern", "nicht", ",", "sie", "durs\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie schweben wie der Staub im Strahl", "tokens": ["sie", "schwe\u00b7ben", "wie", "der", "Staub", "im", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in einem erdenfernen Licht", "tokens": ["in", "ei\u00b7nem", "er\u00b7den\u00b7fer\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und spotten ewig unserer Qual.", "tokens": ["und", "spot\u00b7ten", "e\u00b7wig", "un\u00b7se\u00b7rer", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}