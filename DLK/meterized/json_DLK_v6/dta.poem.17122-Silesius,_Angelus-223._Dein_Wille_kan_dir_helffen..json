{"dta.poem.17122": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "223.  Dein Wille kan dir helffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Verzage nicht mein Kind/ hastu nur gutten Willen/", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", "mein", "Kind", "/", "has\u00b7tu", "nur", "gut\u00b7ten", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$(", "VAFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird sich endlich wol dein Ungewitter stillen.", "tokens": ["So", "wird", "sich", "end\u00b7lich", "wol", "dein", "Un\u00b7ge\u00b7wit\u00b7ter", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}