{"dta.poem.6722": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bemercken wir denn nicht, da\u00df Wasser, welches rein,", "tokens": ["Be\u00b7mer\u00b7cken", "wir", "denn", "nicht", ",", "da\u00df", "Was\u00b7ser", ",", "wel\u00b7ches", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch da\u00df ein St\u00fcck Crystall, so man zum Drey-Eck macht,", "tokens": ["Auch", "da\u00df", "ein", "St\u00fcck", "Crys\u00b7tall", ",", "so", "man", "zum", "Drey\u00b7Eck", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "$,", "ADV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In welchen keine Farben seyn,", "tokens": ["In", "wel\u00b7chen", "kei\u00b7ne", "Far\u00b7ben", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns doch das Aug\u2019 erfreut, durch sch\u00f6ner Farben Pracht?", "tokens": ["Uns", "doch", "das", "Aug'", "er\u00b7freut", ",", "durch", "sch\u00f6\u00b7ner", "Far\u00b7ben", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sey demnach bem\u00fcht in allen Sachen,", "tokens": ["Man", "sey", "dem\u00b7nach", "be\u00b7m\u00fcht", "in", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch zwischen dem Gef\u00fchl, und dem, was es erregt,", "tokens": ["Doch", "zwi\u00b7schen", "dem", "Ge\u00b7f\u00fchl", ",", "und", "dem", ",", "was", "es", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KON", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Stets einen Unterscheid zu machen!", "tokens": ["Stets", "ei\u00b7nen", "Un\u00b7ter\u00b7scheid", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man mu\u00df nicht, wie man meistens pflegt,", "tokens": ["Man", "mu\u00df", "nicht", ",", "wie", "man", "meis\u00b7tens", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In feuchte Lufft den bunten Bogen setzen,", "tokens": ["In", "feuch\u00b7te", "Lufft", "den", "bun\u00b7ten", "Bo\u00b7gen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Woran die Augen sich ergetzen.", "tokens": ["Wo\u00b7ran", "die", "Au\u00b7gen", "sich", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}