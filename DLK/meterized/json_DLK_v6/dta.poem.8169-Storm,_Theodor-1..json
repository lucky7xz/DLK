{"dta.poem.8169": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Verj\u00e4hrtes Leid lie\u00df nimmer dich genesen;", "tokens": ["Ver\u00b7j\u00e4hr\u00b7tes", "Leid", "lie\u00df", "nim\u00b7mer", "dich", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Mutterfreude war f\u00fcr dich zu schwer,", "tokens": ["Die", "Mut\u00b7ter\u00b7freu\u00b7de", "war", "f\u00fcr", "dich", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben war dir gar zu hart gewesen. \u2014", "tokens": ["Das", "Le\u00b7ben", "war", "dir", "gar", "zu", "hart", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er sa\u00df bei dir in letzter Liebespflicht;", "tokens": ["Er", "sa\u00df", "bei", "dir", "in", "letz\u00b7ter", "Lie\u00b7bes\u00b7pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch eine Nacht, noch eine war gegeben!", "tokens": ["Noch", "ei\u00b7ne", "Nacht", ",", "noch", "ei\u00b7ne", "war", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch die verrann; dann kam das Morgenlicht.", "tokens": ["Auch", "die", "ver\u00b7rann", ";", "dann", "kam", "das", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein guter Mann, wie gerne wollt' ich leben!", "tokens": ["Mein", "gu\u00b7ter", "Mann", ",", "wie", "ger\u00b7ne", "wollt'", "ich", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er h\u00f6rte still die sanften Worte an,", "tokens": ["Er", "h\u00f6r\u00b7te", "still", "die", "sanf\u00b7ten", "Wor\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie sein Ohr in bangen Pausen trafen:", "tokens": ["Wie", "sie", "sein", "Ohr", "in", "ban\u00b7gen", "Pau\u00b7sen", "tra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sorg' f\u00fcr das Kind \u2014 ich sterbe, s\u00fc\u00dfer Mann.", "tokens": ["Sor\u00b7g'", "f\u00fcr", "das", "Kind", "ich", "ster\u00b7be", ",", "s\u00fc\u00b7\u00dfer", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dann halbverst\u00e4ndlich noch: Nun will ich schlafen.", "tokens": ["Dann", "halb\u00b7ver\u00b7st\u00e4nd\u00b7lich", "noch", ":", "Nun", "will", "ich", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dann nichts mehr; \u2014 du wurdest nimmer wach,", "tokens": ["Und", "dann", "nichts", "mehr", ";", "du", "wur\u00b7dest", "nim\u00b7mer", "wach", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "$.", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dein Auge brach, die Welt ward immer tr\u00fcber;", "tokens": ["Dein", "Au\u00b7ge", "brach", ",", "die", "Welt", "ward", "im\u00b7mer", "tr\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Athem Gottes wehte durch's Gemach,", "tokens": ["Der", "A\u00b7them", "Got\u00b7tes", "weh\u00b7te", "durch's", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Kind schrie auf, und dann warst du hin\u00fcber.", "tokens": ["Dein", "Kind", "schrie", "auf", ",", "und", "dann", "warst", "du", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}