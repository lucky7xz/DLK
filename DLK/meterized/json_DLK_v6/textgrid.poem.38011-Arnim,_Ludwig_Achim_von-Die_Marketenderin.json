{"textgrid.poem.38011": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Marketenderin", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat sich ein M\u00e4dchen in'n F\u00e4hndrich verliebt,", "tokens": ["Es", "hat", "sich", "ein", "M\u00e4d\u00b7chen", "in'n", "F\u00e4hn\u00b7drich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er spricht ihr von Ehre und heirath sie nicht,", "tokens": ["Er", "spricht", "ihr", "von", "Eh\u00b7re", "und", "hei\u00b7rath", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wenn der F\u00e4hndrich die Fahne thut r\u00fchren,", "tokens": ["Wenn", "der", "F\u00e4hn\u00b7drich", "die", "Fah\u00b7ne", "thut", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Thut sich ihr Herzchen vor Freuden floriren.", "tokens": ["Thut", "sich", "ihr", "Herz\u00b7chen", "vor", "Freu\u00b7den", "flo\u00b7ri\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Tambur die Trummel im Wirbel schon r\u00fchrt,", "tokens": ["Der", "Tam\u00b7bur", "die", "Trum\u00b7mel", "im", "Wir\u00b7bel", "schon", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O wundersch\u00f6n M\u00e4dchen must leiden gro\u00df Noth,", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6n", "M\u00e4d\u00b7chen", "must", "lei\u00b7den", "gro\u00df", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "VMFIN", "VVINF", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da hei\u00dft es, Soldaten in's Feld m\u00fc\u00dft marschieren,", "tokens": ["Da", "hei\u00dft", "es", ",", "Sol\u00b7da\u00b7ten", "in's", "Feld", "m\u00fc\u00dft", "mar\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bald haben wir kein Geld, bald haben wir kein Brod.", "tokens": ["Bald", "ha\u00b7ben", "wir", "kein", "Geld", ",", "bald", "ha\u00b7ben", "wir", "kein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald haben wir kein Brod. bald haben wir kein Geld,", "tokens": ["Bald", "ha\u00b7ben", "wir", "kein", "Brod", ".", "bald", "ha\u00b7ben", "wir", "kein", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O du wundersch\u00f6n M\u00e4del! so geht es im Feld,", "tokens": ["O", "du", "wun\u00b7der\u00b7sch\u00f6n", "M\u00e4\u00b7del", "!", "so", "geht", "es", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und wenn der Feind kommt und bringet uns um,", "tokens": ["Und", "wenn", "der", "Feind", "kommt", "und", "brin\u00b7get", "uns", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bleib bei der Armee und halt dich fein frumm.", "tokens": ["Bleib", "bei", "der", "Ar\u00b7mee", "und", "halt", "dich", "fein", "frumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}