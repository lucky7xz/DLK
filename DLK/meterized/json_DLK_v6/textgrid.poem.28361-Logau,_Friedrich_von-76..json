{"textgrid.poem.28361": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Liebe dann wol blind? Wann ich sie recht seh an,", "tokens": ["Ist", "Lie\u00b7be", "dann", "wol", "blind", "?", "Wann", "ich", "sie", "recht", "seh", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "ADJD", "$.", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So siht sie offtmals mehr, als iemand sehen kan,", "tokens": ["So", "siht", "sie", "offt\u00b7mals", "mehr", ",", "als", "ie\u00b7mand", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und f\u00fchrt, was nirgend da, noch dennoch auff die Bahn.", "tokens": ["Und", "f\u00fchrt", ",", "was", "nir\u00b7gend", "da", ",", "noch", "den\u00b7noch", "auff", "die", "Bahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}