{"textgrid.poem.41335": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Montagne", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Montagne, G\u00fcnstling der Natur,", "tokens": ["Mon\u00b7tag\u00b7ne", ",", "G\u00fcnst\u00b7ling", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es sollte dich nur der, den Witz und Freiheit adeln,", "tokens": ["Es", "soll\u00b7te", "dich", "nur", "der", ",", "den", "Witz", "und", "Frei\u00b7heit", "a\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil er dir r\u00fchmlich gleicht, erheben oder tadeln;", "tokens": ["Weil", "er", "dir", "r\u00fchm\u00b7lich", "gleicht", ",", "er\u00b7he\u00b7ben", "o\u00b7der", "ta\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem sei ein Socrates; wo nicht, ein Epicur!", "tokens": ["Dem", "sei", "ein", "So\u00b7cra\u00b7tes", ";", "wo", "nicht", ",", "ein", "E\u00b7pi\u00b7cur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$.", "PWAV", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du bist, zu aller Lust, in dem, was du geschrieben,", "tokens": ["Du", "bist", ",", "zu", "al\u00b7ler", "Lust", ",", "in", "dem", ",", "was", "du", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachl\u00e4ssig sch\u00f6n, und lehrreich zweifelhaft,", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "sch\u00f6n", ",", "und", "lehr\u00b7reich", "zwei\u00b7fel\u00b7haft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Unwissend voller Wissenschaft:", "tokens": ["Un\u00b7wis\u00b7send", "vol\u00b7ler", "Wis\u00b7sen\u00b7schaft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch der dich meistert, mu\u00df dich lieben.", "tokens": ["Auch", "der", "dich", "meis\u00b7tert", ",", "mu\u00df", "dich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPER", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und hei\u00dft wol der mit Recht gelehrt,", "tokens": ["Und", "hei\u00dft", "wol", "der", "mit", "Recht", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem nicht dein Buch Geschmack und Kenntni\u00df mehrt?", "tokens": ["Dem", "nicht", "dein", "Buch", "Ge\u00b7schmack", "und", "Kennt\u00b7ni\u00df", "mehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}