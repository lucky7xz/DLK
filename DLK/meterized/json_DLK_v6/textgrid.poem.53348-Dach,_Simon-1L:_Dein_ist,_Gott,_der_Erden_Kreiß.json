{"textgrid.poem.53348": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein ist, Gott, der Erden Krei\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein ist, Gott, der Erden Krei\u00df", "tokens": ["Dein", "ist", ",", "Gott", ",", "der", "Er\u00b7den", "Krei\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "$,", "NN", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd der Mensch darinnen,", "tokens": ["Vnd", "der", "Mensch", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Niemand k\u00f6mpt von hinnen,", "tokens": ["Nie\u00b7mand", "k\u00f6mpt", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Niemand her ohn dein Gehei\u00df,", "tokens": ["Nie\u00b7mand", "her", "ohn", "dein", "Ge\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df ist sein Belieben,", "tokens": ["Di\u00df", "ist", "sein", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Eh wir sehn der SonnenStral", "tokens": ["Eh", "wir", "sehn", "der", "Son\u00b7nen", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steht schon vnsrer Tage Zahl", "tokens": ["Steht", "schon", "vns\u00b7rer", "Ta\u00b7ge", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In sein Buch geschrieben.", "tokens": ["In", "sein", "Buch", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dieser eilt ein Kind von hier,", "tokens": ["Die\u00b7ser", "eilt", "ein", "Kind", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in grawen Haaren,", "tokens": ["Der", "in", "gra\u00b7wen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der in solchen Jahren,", "tokens": ["Der", "in", "sol\u00b7chen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die des Lebens beste Zier,", "tokens": ["Die", "des", "Le\u00b7bens", "bes\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da er solte N\u00fctzen", "tokens": ["Da", "er", "sol\u00b7te", "N\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Seiner lieben Vater-Stad,", "tokens": ["Sei\u00b7ner", "lie\u00b7ben", "Va\u00b7ter\u00b7Stad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seyn der Eltern Trost vnd Raht", "tokens": ["Seyn", "der", "El\u00b7tern", "Trost", "vnd", "Raht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd jhr Alter st\u00fctzen.", "tokens": ["Vnd", "jhr", "Al\u00b7ter", "st\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Diesen rei\u00dft die Darre fort,", "tokens": ["Die\u00b7sen", "rei\u00dft", "die", "Dar\u00b7re", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den die Pestilentze,", "tokens": ["Den", "die", "Pes\u00b7ti\u00b7lent\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Den des Vaters Grentze,", "tokens": ["Den", "des", "Va\u00b7ters", "Grent\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jenen wo ein frembder Ort:", "tokens": ["Je\u00b7nen", "wo", "ein", "fremb\u00b7der", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir gleich den Blinden", "tokens": ["Da\u00df", "wir", "gleich", "den", "Blin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hierin tappen nach dem Licht,", "tokens": ["Hie\u00b7rin", "tap\u00b7pen", "nach", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd durchaus in dein Gericht", "tokens": ["Vnd", "durc\u00b7haus", "in", "dein", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Vnd nicht k\u00f6nnen finden.", "tokens": ["Vnd", "nicht", "k\u00f6n\u00b7nen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber du, Herr, bist allein", "tokens": ["A\u00b7ber", "du", ",", "Herr", ",", "bist", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfheit, Glantz vnd St\u00e4rcke,", "tokens": ["Wei\u00df\u00b7heit", ",", "Glantz", "vnd", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wilst in deinem Wercke", "tokens": ["Wilst", "in", "dei\u00b7nem", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Von vns Vngemeistert seyn,", "tokens": ["Von", "vns", "Vn\u00b7ge\u00b7meis\u00b7tert", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hilff es vns erkennen,", "tokens": ["Hilff", "es", "vns", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hemm du vnser Angst-geschrey,", "tokens": ["Hemm", "du", "vn\u00b7ser", "Angst\u00b7ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df vns in Gedult vnd Rew", "tokens": ["La\u00df", "vns", "in", "Ge\u00b7dult", "vnd", "Rew"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Stets dich Vater nennen.", "tokens": ["Stets", "dich", "Va\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Du allein kennst vnsre Zeit,", "tokens": ["Du", "al\u00b7lein", "kennst", "vns\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Niemand den du liebest,", "tokens": ["Nie\u00b7mand", "den", "du", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd sein End' jhm giebest,", "tokens": ["Vnd", "sein", "End'", "jhm", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wird zur Vnzeit abgemeyt;", "tokens": ["Wird", "zur", "Vn\u00b7zeit", "ab\u00b7ge\u00b7meyt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird er hin genommen,", "tokens": ["Wird", "er", "hin", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ob er jung ist, oder alt", "tokens": ["Ob", "er", "jung", "ist", ",", "o\u00b7der", "alt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ey, gefiel dir, also bald", "tokens": ["Ey", ",", "ge\u00b7fiel", "dir", ",", "al\u00b7so", "bald"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird er auch Vollkommen.", "tokens": ["Wird", "er", "auch", "Voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}