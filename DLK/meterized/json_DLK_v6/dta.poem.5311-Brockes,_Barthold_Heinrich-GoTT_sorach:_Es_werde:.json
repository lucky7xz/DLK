{"dta.poem.5311": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "GoTT  sorach: Es werde:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschen Wort ist Wind, der GOttheit Wort", "tokens": ["Der", "Men\u00b7schen", "Wort", "ist", "Wind", ",", "der", "Got\u00b7theit", "Wort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sind Wercke;", "tokens": ["sind", "Wer\u00b7cke", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "GoTT sprach: Es werde Licht!", "tokens": ["GoTT", "sprach", ":", "Es", "wer\u00b7de", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Licht ward alsobald. Er f\u00e4hret fort: ", "tokens": ["Das", "Licht", "ward", "al\u00b7so\u00b7bald", ".", "Er", "f\u00e4h\u00b7ret", "fort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lust, Himmel, Erd' und Meer! Luft, Himmel, Meer", "tokens": ["Lust", ",", "Him\u00b7mel", ",", "Erd'", "und", "Meer", "!", "Luft", ",", "Him\u00b7mel", ",", "Meer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$.", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und Erde", "tokens": ["und", "Er\u00b7de"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ward augenblicks. So spricht die GOttheit, wenn sie", "tokens": ["Ward", "au\u00b7gen\u00b7blicks", ".", "So", "spricht", "die", "Got\u00b7theit", ",", "wenn", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "spricht!", "tokens": ["spricht", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und weil, da seine Werck stets w\u00e4hren, nicht veralten,", "tokens": ["Und", "weil", ",", "da", "sei\u00b7ne", "Werck", "stets", "w\u00e4h\u00b7ren", ",", "nicht", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht wiederum vergehn; derselbigen Erhalten", "tokens": ["Nicht", "wie\u00b7de\u00b7rum", "ver\u00b7gehn", ";", "der\u00b7sel\u00b7bi\u00b7gen", "Er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein stetes Schaffen ist; so kan man GOTT zu Ehren,", "tokens": ["Ein", "ste\u00b7tes", "Schaf\u00b7fen", "ist", ";", "so", "kan", "man", "GoTT", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "ADV", "VMFIN", "PIS", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein unaufh\u00f6rlich Wort: ", "tokens": ["Sein", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "Wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wir k\u00f6nnen \u00fcberall, in aller Dinge Wesen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "\u00fc\u00b7be\u00b7rall", ",", "in", "al\u00b7ler", "Din\u00b7ge", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der GOttheit grosses ", "tokens": ["Der", "Got\u00b7theit", "gros\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wer Ohren hat zu h\u00f6ren, h\u00f6re dann,", "tokens": ["Wer", "Oh\u00b7ren", "hat", "zu", "h\u00f6\u00b7ren", ",", "h\u00f6\u00b7re", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mit Andacht und mit Lust, die Rede GOTTES an!", "tokens": ["Mit", "An\u00b7dacht", "und", "mit", "Lust", ",", "die", "Re\u00b7de", "GoT\u00b7TES", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer Augen hat zu sehen, seh\u2019 und lerne", "tokens": ["Wer", "Au\u00b7gen", "hat", "zu", "se\u00b7hen", ",", "seh'", "und", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die Lettern dieser Welt, das A. B. C. der Sterne,", "tokens": ["Die", "Let\u00b7tern", "die\u00b7ser", "Welt", ",", "das", "A.", "B.", "C.", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation", "abbreviation", "abbreviation", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Worin von seiner Macht, von seinem ew\u2019gen Lieben,", "tokens": ["Wo\u00b7rin", "von", "sei\u00b7ner", "Macht", ",", "von", "sei\u00b7nem", "ew'\u00b7gen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und seiner Wei\u00dfheit Licht Geheimnisse geschrieben.", "tokens": ["Und", "sei\u00b7ner", "Wei\u00df\u00b7heit", "Licht", "Ge\u00b7heim\u00b7nis\u00b7se", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein Geist, der sich bem\u00fcht, nur erst zu buchstabiren", "tokens": ["Ein", "Geist", ",", "der", "sich", "be\u00b7m\u00fcht", ",", "nur", "erst", "zu", "buch\u00b7sta\u00b7bi\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "$,", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In diesem Buch der Weisheit, das so sch\u00f6n,", "tokens": ["In", "die\u00b7sem", "Buch", "der", "Weis\u00b7heit", ",", "das", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wird, mit stets neuer Lust, den Innhalt bald verstehn.", "tokens": ["Wird", ",", "mit", "stets", "neu\u00b7er", "Lust", ",", "den", "Inn\u00b7halt", "bald", "ver\u00b7stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf allen Bl\u00e4ttern steht die ewig wahre Lehre:", "tokens": ["Auf", "al\u00b7len", "Bl\u00e4t\u00b7tern", "steht", "die", "e\u00b7wig", "wah\u00b7re", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "GoTT ist das h\u00f6chste Gut! und: GOTT allein\ndie Ehre!", "tokens": ["GoTT", "ist", "das", "h\u00f6chs\u00b7te", "Gut", "!", "und", ":", "GoTT", "al\u00b7lein", "die", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "KON", "$.", "NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}