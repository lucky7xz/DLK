{"dta.poem.10061": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgte Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war die laue Fr\u00fchlings-Zeit", "tokens": ["Es", "war", "die", "lau\u00b7e", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren h\u00f6chsten Flor gekommen;", "tokens": ["In", "ih\u00b7ren", "h\u00f6chs\u00b7ten", "Flor", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Monat Junius hatt\u2019 allbereit,", "tokens": ["Der", "Mo\u00b7nat", "Ju\u00b7nius", "hatt'", "all\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Rosen ausgeschm\u00fcckt, den Anfang j\u00fcngst genommen;", "tokens": ["Mit", "Ro\u00b7sen", "aus\u00b7ge\u00b7schm\u00fcckt", ",", "den", "An\u00b7fang", "j\u00fcngst", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Gottlieb, da der Tag sich etwas abgek\u00fchlt,", "tokens": ["Als", "Gott\u00b7lieb", ",", "da", "der", "Tag", "sich", "et\u00b7was", "ab\u00b7ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und man bereits ein Abend-L\u00fcfftchen f\u00fchlt\u2019,", "tokens": ["Und", "man", "be\u00b7reits", "ein", "A\u00b7ben\u00b7dL\u00fcfft\u00b7chen", "f\u00fchlt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An einem klaren Bach spatzierte,", "tokens": ["An", "ei\u00b7nem", "kla\u00b7ren", "Bach", "spat\u00b7zier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Au dessen Ufer rechter Seits", "tokens": ["Au", "des\u00b7sen", "U\u00b7fer", "rech\u00b7ter", "Seits"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRELAT", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Ein gr\u00fcnes W\u00e4ldchen stand,", "tokens": ["Ein", "gr\u00fc\u00b7nes", "W\u00e4ld\u00b7chen", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das seiner Fluth Crystall mit einem Bilde zierte,", "tokens": ["Das", "sei\u00b7ner", "Fluth", "Crys\u00b7tall", "mit", "ei\u00b7nem", "Bil\u00b7de", "zier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das selbst ein W\u00e4ldchen schien. Es gl\u00e4ntzt\u2019 in seiner Hand", "tokens": ["Das", "selbst", "ein", "W\u00e4ld\u00b7chen", "schien", ".", "Es", "gl\u00e4ntzt'", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein sch\u00f6ner Rosen-Strau\u00df, der rings \u00fcmher die Lufft", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Ro\u00b7sen\u00b7Strau\u00df", ",", "der", "rings", "\u00fcm\u00b7her", "die", "Lufft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erf\u00fcllete mit einem Balsam-Dufft\u2019,", "tokens": ["Er\u00b7f\u00fcl\u00b7le\u00b7te", "mit", "ei\u00b7nem", "Bal\u00b7sam\u00b7Dufft'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und welcher den Geruch erquickt.", "tokens": ["Und", "wel\u00b7cher", "den", "Ge\u00b7ruch", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Apfel, den uns China schickt,", "tokens": ["Ein", "Ap\u00b7fel", ",", "den", "uns", "Chi\u00b7na", "schickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den er von ungefehr in seiner Tasche fand,", "tokens": ["Den", "er", "von", "un\u00b7ge\u00b7fehr", "in", "sei\u00b7ner", "Ta\u00b7sche", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erfrischte seinen Gaum. Der Silber-reine Schall", "tokens": ["Er\u00b7frischte", "sei\u00b7nen", "Gaum", ".", "Der", "Sil\u00b7ber\u00b7rei\u00b7ne", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Von einer in bebl\u00fchmten Hecken", "tokens": ["Von", "ei\u00b7ner", "in", "be\u00b7bl\u00fchm\u00b7ten", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hell musicirenden verliebten Nachtigall", "tokens": ["Hell", "mu\u00b7si\u00b7ci\u00b7ren\u00b7den", "ver\u00b7lieb\u00b7ten", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Drang ihm durchs Ohr ans Hertz. Mit einem s\u00fcssen", "tokens": ["Drang", "ihm", "durchs", "Ohr", "ans", "Hertz", ".", "Mit", "ei\u00b7nem", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "APPRART", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Mit halb entz\u00fccktem Blick, ward er zugleich, wie klar,", "tokens": ["Mit", "halb", "ent\u00b7z\u00fcck\u00b7tem", "Blick", ",", "ward", "er", "zu\u00b7gleich", ",", "wie", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie g\u00fclden, wie voll Glantz, wie Wunder-Wunder-sch\u00f6n", "tokens": ["Wie", "g\u00fcl\u00b7den", ",", "wie", "voll", "Glantz", ",", "wie", "Wun\u00b7der\u00b7Wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Licht- und Lebens-Quell, die Sonn\u2019, im untergehn,", "tokens": ["Die", "Licht", "und", "Le\u00b7bens\u00b7Quell", ",", "die", "Sonn'", ",", "im", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,", "ART", "NN", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In unvergleichlicher Vollkommenheit,", "tokens": ["In", "un\u00b7ver\u00b7gleich\u00b7li\u00b7cher", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Durch das noch zarte gr\u00fcn, gewahr.", "tokens": ["Durch", "das", "noch", "zar\u00b7te", "gr\u00fcn", ",", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Er stutzt\u2019, als er die Mannichfaltigkeit", "tokens": ["Er", "stutzt'", ",", "als", "er", "die", "Man\u00b7nich\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der Lieblichkeiten auf der Welt,", "tokens": ["Der", "Lieb\u00b7lich\u00b7kei\u00b7ten", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sich vor sein Seelen-Auge stellt\u2019:", "tokens": ["Sich", "vor", "sein", "See\u00b7len\u00b7Au\u00b7ge", "stellt'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie Erde, Lufft und Fluth an Pracht und Lust so reich,", "tokens": ["Wie", "Er\u00b7de", ",", "Lufft", "und", "Fluth", "an", "Pracht", "und", "Lust", "so", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df dennoch, da er es \u00fcberlegte,", "tokens": ["Und", "da\u00df", "den\u00b7noch", ",", "da", "er", "es", "\u00fc\u00b7ber\u00b7leg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wie alle Sinnen ihm von ungefehr zugleich", "tokens": ["Wie", "al\u00b7le", "Sin\u00b7nen", "ihm", "von", "un\u00b7ge\u00b7fehr", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER", "APPR", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vergn\u00fcget w\u00fcrden es ihn doch nicht mehr bewegte,", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "w\u00fcr\u00b7den", "es", "ihn", "doch", "nicht", "mehr", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Und er es kaum bemerckt. Mein GOtt! rieff er woher", "tokens": ["Und", "er", "es", "kaum", "be\u00b7merckt", ".", "Mein", "Gott", "!", "rieff", "er", "wo\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "ADV", "VVPP", "$.", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Entsteht die\u00df Ungl\u00fcck doch, da\u00df wir so \u00f6ffters blind,", "tokens": ["Ent\u00b7steht", "die\u00df", "Un\u00b7gl\u00fcck", "doch", ",", "da\u00df", "wir", "so", "\u00f6ff\u00b7ters", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Geschmack-Geruch-Geh\u00f6r- und F\u00fchl-los sind?", "tokens": ["Ge\u00b7schmack\u00b7Ge\u00b7ruch\u00b7Ge\u00b7h\u00f6r", "und", "F\u00fchl\u00b7los", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Da\u00df wir, f\u00fcr das Gesch\u00f6pf, im Schlummer gleichsam lie-", "tokens": ["Da\u00df", "wir", ",", "f\u00fcr", "das", "Ge\u00b7sch\u00f6pf", ",", "im", "Schlum\u00b7mer", "gleich\u00b7sam", "lie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie f\u00e4llt es uns, ach leider! doch so schwer,", "tokens": ["Wie", "f\u00e4llt", "es", "uns", ",", "ach", "lei\u00b7der", "!", "doch", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "ITJ", "ADV", "$.", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Uns im Vergn\u00fcgen zu vergn\u00fcgen!", "tokens": ["Uns", "im", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er fand darauf so viel, wie er recht in sich gieng,", "tokens": ["Er", "fand", "da\u00b7rauf", "so", "viel", ",", "wie", "er", "recht", "in", "sich", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df unser Geist f\u00fcr mehr als einerley", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "f\u00fcr", "mehr", "als", "ei\u00b7ner\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIAT", "KOKOM", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf einmahl nicht geschaffen sey.", "tokens": ["Auf", "ein\u00b7mahl", "nicht", "ge\u00b7schaf\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weshalben er, in seinem dencken,", "tokens": ["We\u00b7shal\u00b7ben", "er", ",", "in", "sei\u00b7nem", "den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein\u2019 Ordnung an zu halten fieng,", "tokens": ["Ein'", "Ord\u00b7nung", "an", "zu", "hal\u00b7ten", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und einem ieden Sinn ein\u2019 eigne Zeit zu schencken,", "tokens": ["Und", "ei\u00b7nem", "ie\u00b7den", "Sinn", "ein'", "eig\u00b7ne", "Zeit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auf ihn absonderlich der Seelen Krafft zu lencken,", "tokens": ["Auf", "ihn", "ab\u00b7son\u00b7der\u00b7lich", "der", "See\u00b7len", "Krafft", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit allem Recht beschlo\u00df. Worauf er also dachte,", "tokens": ["Mit", "al\u00b7lem", "Recht", "be\u00b7schlo\u00df", ".", "Wo\u00b7rauf", "er", "al\u00b7so", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$.", "PAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und seinem Sch\u00f6pfer seine Lust,", "tokens": ["Und", "sei\