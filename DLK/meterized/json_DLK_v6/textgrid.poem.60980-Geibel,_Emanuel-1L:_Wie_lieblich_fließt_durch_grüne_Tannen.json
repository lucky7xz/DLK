{"textgrid.poem.60980": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieblich flie\u00dft durch gr\u00fcne Tannen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich flie\u00dft durch gr\u00fcne Tannen", "tokens": ["Wie", "lieb\u00b7lich", "flie\u00dft", "durch", "gr\u00fc\u00b7ne", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf B\u00f6hmens H\u00f6hn der Sonne Strahl!", "tokens": ["Auf", "B\u00f6h\u00b7mens", "H\u00f6hn", "der", "Son\u00b7ne", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs Dickicht rauscht das Reh von dannen,", "tokens": ["Durchs", "Di\u00b7ckicht", "rauscht", "das", "Reh", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Felsen blinkt der Quell ins Tal,", "tokens": ["Durch", "Fel\u00b7sen", "blinkt", "der", "Quell", "ins", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fern zu blauen Bergeswarten", "tokens": ["Und", "fern", "zu", "blau\u00b7en", "Ber\u00b7ges\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKZU", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verliert sich tr\u00e4umend Aug' und Sinn,", "tokens": ["Ver\u00b7liert", "sich", "tr\u00e4u\u00b7mend", "Aug'", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du aber wandelst durch den Garten", "tokens": ["Du", "a\u00b7ber", "wan\u00b7delst", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In stiller Anmut l\u00e4chelnd hin.", "tokens": ["In", "stil\u00b7ler", "An\u00b7mut", "l\u00e4\u00b7chelnd", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie dein Blick mit leiser Frage", "tokens": ["Und", "wie", "dein", "Blick", "mit", "lei\u00b7ser", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich freundlich zu dem meinen neigt,", "tokens": ["Sich", "freund\u00b7lich", "zu", "dem", "mei\u00b7nen", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da mu\u00df ich denken jener Tage,", "tokens": ["Da", "mu\u00df", "ich", "den\u00b7ken", "je\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir zuerst dein Herz gezeigt;", "tokens": ["Die", "mir", "zu\u00b7erst", "dein", "Herz", "ge\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich, ein ungest\u00fcmer Knabe,", "tokens": ["Da", "ich", ",", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von dunklem Jugenddrang bewegt", "tokens": ["Von", "dunk\u00b7lem", "Ju\u00b7gend\u00b7drang", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der ersten Lieder fr\u00fche Gabe", "tokens": ["Der", "ers\u00b7ten", "Lie\u00b7der", "fr\u00fc\u00b7he", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schamrot in deine Hand gelegt.", "tokens": ["Scham\u00b7rot", "in", "dei\u00b7ne", "Hand", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, damals klang's mir leise wider,", "tokens": ["Ach", ",", "da\u00b7mals", "klang's", "mir", "lei\u00b7se", "wi\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich voll Sehnsucht vorgef\u00fchlt,", "tokens": ["Was", "ich", "voll", "Sehn\u00b7sucht", "vor\u00b7ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flatternd irrten meine Lieder,", "tokens": ["Und", "flat\u00b7ternd", "irr\u00b7ten", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenn der Wind in Saiten w\u00fchlt.", "tokens": ["Wie", "wenn", "der", "Wind", "in", "Sai\u00b7ten", "w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch schwankte vor dem jungen Herzen", "tokens": ["Noch", "schwank\u00b7te", "vor", "dem", "jun\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt mir wie ein goldner Traum;", "tokens": ["Die", "Welt", "mir", "wie", "ein", "gold\u00b7ner", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein den Abgrund aller Schmerzen,", "tokens": ["Al\u00b7lein", "den", "Ab\u00b7grund", "al\u00b7ler", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freuden Gipfel ahnt' ich kaum.", "tokens": ["Der", "Freu\u00b7den", "Gip\u00b7fel", "ahnt'", "ich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch anders ward es. Leid und Wonne,", "tokens": ["Doch", "an\u00b7ders", "ward", "es", ".", "Leid", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun hab' ich sie zum Grund erprobt;", "tokens": ["Nun", "hab'", "ich", "sie", "zum", "Grund", "er\u00b7probt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich hat versengt des S\u00fcdens Sonne,", "tokens": ["Mich", "hat", "ver\u00b7sengt", "des", "S\u00fc\u00b7dens", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich hat des Nordens Sturm umtobt.", "tokens": ["Mich", "hat", "des", "Nor\u00b7dens", "Sturm", "um\u00b7tobt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich trank der Liebe vollsten Sprudel,", "tokens": ["Ich", "trank", "der", "Lie\u00b7be", "volls\u00b7ten", "Spru\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich weint' um die verlorne Lust;", "tokens": ["Ich", "weint'", "um", "die", "ver\u00b7lor\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch in des Lebens wildem Strudel", "tokens": ["Doch", "in", "des", "Le\u00b7bens", "wil\u00b7dem", "Stru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ward ich des Zieles mir bewu\u00dft.", "tokens": ["Ward", "ich", "des", "Zie\u00b7les", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn drau\u00dfen der verworrne Reigen", "tokens": ["Wenn", "drau\u00b7\u00dfen", "der", "ver\u00b7worr\u00b7ne", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Tages laut und lauter scholl,", "tokens": ["Des", "Ta\u00b7ges", "laut", "und", "lau\u00b7ter", "scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lernt' ich zum Born hinabzusteigen,", "tokens": ["Lernt'", "ich", "zum", "Born", "hin\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem mir ew'ge Klarheit quoll.", "tokens": ["Aus", "dem", "mir", "ew'\u00b7ge", "Klar\u00b7heit", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir spielte wie mit k\u00fchler Schwinge", "tokens": ["Mir", "spiel\u00b7te", "wie", "mit", "k\u00fch\u00b7ler", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ums Haupt der Odem der Natur,", "tokens": ["Ums", "Haupt", "der", "O\u00b7dem", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und einsam den Gesang der Dinge", "tokens": ["Und", "ein\u00b7sam", "den", "Ge\u00b7sang", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vernahm mein Ohr aus Wald und Flur.", "tokens": ["Ver\u00b7nahm", "mein", "Ohr", "aus", "Wald", "und", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da ward es hell mir im Gem\u00fcte,", "tokens": ["Da", "ward", "es", "hell", "mir", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah durch ", "tokens": ["Ich", "sah", "durch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Zeiten Schritt, der Blumen Bl\u00fcte", "tokens": ["Der", "Zei\u00b7ten", "Schritt", ",", "der", "Blu\u00b7men", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In heil'ger Ordnung wechselnd gehn;", "tokens": ["In", "heil'\u00b7ger", "Ord\u00b7nung", "wech\u00b7selnd", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah den Tod das Sein geb\u00e4ren,", "tokens": ["Ich", "sah", "den", "Tod", "das", "Sein", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Einklang h\u00f6rt' ich durch im Zwist,", "tokens": ["Den", "Ein\u00b7klang", "h\u00f6rt'", "ich", "durch", "im", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ahnend lernt' ich tief verehren", "tokens": ["Und", "ah\u00b7nend", "lernt'", "ich", "tief", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Wunder dessen, was da ist.", "tokens": ["Das", "Wun\u00b7der", "des\u00b7sen", ",", "was", "da", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was so im Busen ich getragen,", "tokens": ["Was", "so", "im", "Bu\u00b7sen", "ich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich gek\u00e4mpft, verfehlt, ersiegt,", "tokens": ["Was", "ich", "ge\u00b7k\u00e4mpft", ",", "ver\u00b7fehlt", ",", "er\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das la\u00df dir nun dies B\u00fcchlein sagen,", "tokens": ["Das", "la\u00df", "dir", "nun", "dies", "B\u00fcch\u00b7lein", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drin meine Seele vor dir liegt.", "tokens": ["Drin", "mei\u00b7ne", "See\u00b7le", "vor", "dir", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So nimm es hin! Und wuchert munter", "tokens": ["So", "nimm", "es", "hin", "!", "Und", "wu\u00b7chert", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$.", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Manch buntes Unkraut auch noch heut:", "tokens": ["Manch", "bun\u00b7tes", "Un\u00b7kraut", "auch", "noch", "heut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon sind die Erstlingshalme drunter", "tokens": ["Schon", "sind", "die", "Erst\u00b7lings\u00b7hal\u00b7me", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ernte, die mein Leben beut.", "tokens": ["Der", "Ern\u00b7te", ",", "die", "mein", "Le\u00b7ben", "beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}