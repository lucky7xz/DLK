{"textgrid.poem.56084": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Ball", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Runder, der das Warme aus zwei H\u00e4nden", "tokens": ["Du", "Run\u00b7der", ",", "der", "das", "War\u00b7me", "aus", "zwei", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Fliegen, oben, fortgiebt, sorglos wie", "tokens": ["im", "Flie\u00b7gen", ",", "o\u00b7ben", ",", "fort\u00b7giebt", ",", "sorg\u00b7los", "wie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "$,", "VVFIN", "$,", "ADJD", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sein Eigenes; was in den Gegenst\u00e4nden", "tokens": ["sein", "Ei\u00b7ge\u00b7nes", ";", "was", "in", "den", "Ge\u00b7gen\u00b7st\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$.", "PWS", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "nicht bleiben kann, zu unbeschwert f\u00fcr sie,", "tokens": ["nicht", "blei\u00b7ben", "kann", ",", "zu", "un\u00b7be\u00b7schwert", "f\u00fcr", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "PTKA", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "zu wenig Ding und doch noch Ding genug,", "tokens": ["zu", "we\u00b7nig", "Ding", "und", "doch", "noch", "Ding", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADV", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "um nicht aus allem drau\u00dfen Aufgereihten", "tokens": ["um", "nicht", "aus", "al\u00b7lem", "drau\u00b7\u00dfen", "Auf\u00b7ge\u00b7reih\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKNEG", "APPR", "PIS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "unsichtbar pl\u00f6tzlich in uns einzugleiten:", "tokens": ["un\u00b7sicht\u00b7bar", "pl\u00f6tz\u00b7lich", "in", "uns", "ein\u00b7zu\u00b7glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "das glitt in dich, du zwischen Fall und Flug", "tokens": ["das", "glitt", "in", "dich", ",", "du", "zwi\u00b7schen", "Fall", "und", "Flug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "$,", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "noch Unentschlossener: der, wenn er steigt,", "tokens": ["noch", "Un\u00b7ent\u00b7schlos\u00b7se\u00b7ner", ":", "der", ",", "wenn", "er", "steigt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "ART", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "als h\u00e4tte er ihn mit hinaufgehoben,", "tokens": ["als", "h\u00e4t\u00b7te", "er", "ihn", "mit", "hin\u00b7auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den Wurf entf\u00fchrt und freil\u00e4\u00dft \u2013, und sich neigt", "tokens": ["den", "Wurf", "ent\u00b7f\u00fchrt", "und", "frei\u00b7l\u00e4\u00dft", "\u2013", ",", "und", "sich", "neigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "$(", "$,", "KON", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und einh\u00e4lt und den Spielenden von oben", "tokens": ["und", "ein\u00b7h\u00e4lt", "und", "den", "Spie\u00b7len\u00b7den", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ART", "NN", "APPR", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "auf einmal eine neue Stelle zeigt,", "tokens": ["auf", "ein\u00b7mal", "ei\u00b7ne", "neu\u00b7e", "Stel\u00b7le", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "sie ordnend wie zu einer Tanzfigur,", "tokens": ["sie", "ord\u00b7nend", "wie", "zu", "ei\u00b7ner", "Tanz\u00b7fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "um dann, erwartet und erw\u00fcnscht von allen,", "tokens": ["um", "dann", ",", "er\u00b7war\u00b7tet", "und", "er\u00b7w\u00fcnscht", "von", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "VVPP", "KON", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "rasch, einfach, kunstlos, ganz Natur,", "tokens": ["rasch", ",", "ein\u00b7fach", ",", "kunst\u00b7los", ",", "ganz", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "PTKVZ", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dem Becher hoher H\u00e4nde zuzufallen.", "tokens": ["dem", "Be\u00b7cher", "ho\u00b7her", "H\u00e4n\u00b7de", "zu\u00b7zu\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}