{"dta.poem.6685": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wird f\u00fcr Lust (wenn es die Ohren trifft,)", "tokens": ["Was", "wird", "f\u00fcr", "Lust", "(", "wenn", "es", "die", "Oh\u00b7ren", "trifft", ",", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch abgemessenen Zusammenklang,", "tokens": ["Durch", "ab\u00b7ge\u00b7mes\u00b7se\u00b7nen", "Zu\u00b7sam\u00b7men\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch Abstand und Zusammenhang", "tokens": ["Durch", "Ab\u00b7stand", "und", "Zu\u00b7sam\u00b7men\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In Seelen, die harmonisch, nicht gestifft?", "tokens": ["In", "See\u00b7len", ",", "die", "har\u00b7mo\u00b7nisch", ",", "nicht", "ge\u00b7stifft", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sonderlich ein ", "tokens": ["Wenn", "son\u00b7der\u00b7lich", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und seine T\u00f6ne moderiret,", "tokens": ["Und", "sei\u00b7ne", "T\u00f6\u00b7ne", "mo\u00b7de\u00b7ri\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In lieblichen Ges\u00e4ngen, die man h\u00f6rt,", "tokens": ["In", "lieb\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7gen", ",", "die", "man", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja immer mehr die Wunder noch vermehrt,", "tokens": ["Ja", "im\u00b7mer", "mehr", "die", "Wun\u00b7der", "noch", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und uns fast glauben macht,", "tokens": ["Und", "uns", "fast", "glau\u00b7ben", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ob nicht ein holder Geist den Klang vom Himmel bracht.", "tokens": ["Ob", "nicht", "ein", "hol\u00b7der", "Geist", "den", "Klang", "vom", "Him\u00b7mel", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}