{"textgrid.poem.55270": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Lug oder Trug?", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf man das Volk betriegen?", "tokens": ["Darf", "man", "das", "Volk", "be\u00b7trie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sage: nein!", "tokens": ["Ich", "sa\u00b7ge", ":", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch willst du sie bel\u00fcgen,", "tokens": ["Doch", "willst", "du", "sie", "be\u00b7l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mach es nur nicht fein.", "tokens": ["So", "mach", "es", "nur", "nicht", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}