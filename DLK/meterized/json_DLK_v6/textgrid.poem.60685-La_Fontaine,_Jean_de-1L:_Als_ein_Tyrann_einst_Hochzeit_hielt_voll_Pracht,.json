{"textgrid.poem.60685": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ein Tyrann einst Hochzeit hielt voll Pracht,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ein Tyrann einst Hochzeit hielt voll Pracht,", "tokens": ["Als", "ein", "Ty\u00b7rann", "einst", "Hoch\u00b7zeit", "hielt", "voll", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ertr\u00e4nkte froh das Volk sein Leid im Wein.", "tokens": ["Er\u00b7tr\u00e4nk\u00b7te", "froh", "das", "Volk", "sein", "Leid", "im", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00c4sop nur hielt's f\u00fcr dumm und unbedacht,", "tokens": ["\u00c4\u00b7sop", "nur", "hielt's", "f\u00fcr", "dumm", "und", "un\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei solchem Feste so voll Lust zu sein.", "tokens": ["Bei", "sol\u00b7chem", "Fes\u00b7te", "so", "voll", "Lust", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbdie Sonne\u00ab, sprach er, \u00bbzog einst in Betracht,", "tokens": ["\u00bb", "die", "Son\u00b7ne", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "zog", "einst", "in", "Be\u00b7tracht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu treten in den Ehestand hinein;", "tokens": ["Zu", "tre\u00b7ten", "in", "den", "E\u00b7hes\u00b7tand", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Einstimmig h\u00f6rt man da die Fr\u00f6sche schrein", "tokens": ["Ein\u00b7stim\u00b7mig", "h\u00f6rt", "man", "da", "die", "Fr\u00f6\u00b7sche", "schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und angstvoll bei dem Schicksal sich beklagen:", "tokens": ["Und", "angst\u00b7voll", "bei", "dem", "Schick\u00b7sal", "sich", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "\u203aschon eine Sonne l\u00e4\u00dft sich kaum ertragen,", "tokens": ["\u203a", "schon", "ei\u00b7ne", "Son\u00b7ne", "l\u00e4\u00dft", "sich", "kaum", "er\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was tun wir, kommt nun gar der Kindersegen!", "tokens": ["Was", "tun", "wir", ",", "kommt", "nun", "gar", "der", "Kin\u00b7der\u00b7se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Scheint erst ein halbes Dutzend Sonnen her,", "tokens": ["Scheint", "erst", "ein", "hal\u00b7bes", "Dut\u00b7zend", "Son\u00b7nen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So werden sie das gro\u00dfe Weltenmeer", "tokens": ["So", "wer\u00b7den", "sie", "das", "gro\u00b7\u00dfe", "Wel\u00b7ten\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mit all seinen Bewohnern trockenlegen.", "tokens": ["Mit", "all", "sei\u00b7nen", "Be\u00b7woh\u00b7nern", "tro\u00b7cken\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Ade dann, Sumpf, du St\u00e4tte unsres Gl\u00fccks!", "tokens": ["A\u00b7de", "dann", ",", "Sumpf", ",", "du", "St\u00e4t\u00b7te", "uns\u00b7res", "Gl\u00fccks", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "F\u00fcr uns bleibt nur die schwarze Flut des Styx.\u2039 \u2013", "tokens": ["F\u00fcr", "uns", "bleibt", "nur", "die", "schwar\u00b7ze", "Flut", "des", "Styx", ".", "\u2039", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das war, so scheint es mir,", "tokens": ["Das", "war", ",", "so", "scheint", "es", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Nicht schlecht gesprochen f\u00fcr ein armes Tier.\u00ab", "tokens": ["Nicht", "schlecht", "ge\u00b7spro\u00b7chen", "f\u00fcr", "ein", "ar\u00b7mes", "Tier", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}