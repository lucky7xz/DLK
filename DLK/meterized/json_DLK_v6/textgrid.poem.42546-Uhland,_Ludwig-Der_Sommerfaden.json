{"textgrid.poem.42546": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sommerfaden", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da fliegt, als wir im Felde gehen,", "tokens": ["Da", "fliegt", ",", "als", "wir", "im", "Fel\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sommerfaden \u00fcber Land,", "tokens": ["Ein", "Som\u00b7mer\u00b7fa\u00b7den", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein leicht und licht Gespinst der Feen,", "tokens": ["Ein", "leicht", "und", "licht", "Ge\u00b7spinst", "der", "Feen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kn\u00fcpft von mir zu ihr ein Band.", "tokens": ["Und", "kn\u00fcpft", "von", "mir", "zu", "ihr", "ein", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nehm ihn f\u00fcr ein g\u00fcnstig Zeichen,", "tokens": ["Ich", "nehm", "ihn", "f\u00fcr", "ein", "g\u00fcns\u00b7tig", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zeichen, wie die Lieb es braucht.", "tokens": ["Ein", "Zei\u00b7chen", ",", "wie", "die", "Lieb", "es", "braucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Hoffnungen der Hoffnungsreichen,", "tokens": ["O", "Hoff\u00b7nun\u00b7gen", "der", "Hoff\u00b7nungs\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus Duft gewebt, von Luft zerhaucht!", "tokens": ["Aus", "Duft", "ge\u00b7webt", ",", "von", "Luft", "zer\u00b7haucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}