{"textgrid.poem.48884": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, liebster Damon! Ich bin \u00fcberwunden,", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, liebster Damon! Ich bin \u00fcberwunden,", "tokens": ["Ja", ",", "liebs\u00b7ter", "Da\u00b7mon", "!", "Ich", "bin", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist empfindet, was er nie empfunden;", "tokens": ["Mein", "Geist", "emp\u00b7fin\u00b7det", ",", "was", "er", "nie", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchl die von mir sonst verlachten Schmerzen", "tokens": ["Ich", "f\u00fchl", "die", "von", "mir", "sonst", "ver\u00b7lach\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jezt in dem Herzen.", "tokens": ["Jezt", "in", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich die Hand j\u00fcngst, die dein Auge deckte,", "tokens": ["Als", "ich", "die", "Hand", "j\u00fcngst", ",", "die", "dein", "Au\u00b7ge", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorwitzig fortri\u00df; Himmel! was erweckte", "tokens": ["Vor\u00b7wit\u00b7zig", "for\u00b7tri\u00df", ";", "Him\u00b7mel", "!", "was", "er\u00b7weck\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$.", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein sch\u00f6nes Auge, voller treuen Thr\u00e4nen,", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", ",", "vol\u00b7ler", "treu\u00b7en", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir nicht f\u00fcr Sehnen!", "tokens": ["Mir", "nicht", "f\u00fcr", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich floh und weinte. Wie ward mir zu Muthe!", "tokens": ["Ich", "floh", "und", "wein\u00b7te", ".", "Wie", "ward", "mir", "zu", "Mu\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein heftig Feuer wallte mir im Blute.", "tokens": ["Ein", "hef\u00b7tig", "Feu\u00b7er", "wall\u00b7te", "mir", "im", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Flammen werden unaufh\u00f6rlich w\u00e4hren,", "tokens": ["Die", "Flam\u00b7men", "wer\u00b7den", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mich verzehren.", "tokens": ["Die", "mich", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Komm treuster Damon! den ich mir erw\u00e4hle,", "tokens": ["Komm", "treus\u00b7ter", "Da\u00b7mon", "!", "den", "ich", "mir", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf meinen Lippen schwebt mir schon die Seele,", "tokens": ["Auf", "mei\u00b7nen", "Lip\u00b7pen", "schwebt", "mir", "schon", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um durch die deinen, unter Scherz und k\u00fcssen,", "tokens": ["Um", "durch", "die", "dei\u00b7nen", ",", "un\u00b7ter", "Scherz", "und", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "PPOSAT", "$,", "APPR", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dich zu fliessen.", "tokens": ["In", "dich", "zu", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}