{"textgrid.poem.52425": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "6. Landlied f\u00fcr M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, Gespielen, seht, die Flur", "tokens": ["Seht", ",", "Ge\u00b7spie\u00b7len", ",", "seht", ",", "die", "Flur"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchet nur,", "tokens": ["Bl\u00fc\u00b7het", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Um der Unschuld zu gefallen.", "tokens": ["Um", "der", "Un\u00b7schuld", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft uns froh am Blumenrain", "tokens": ["La\u00dft", "uns", "froh", "am", "Blu\u00b7men\u00b7rain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im Hain", "tokens": ["Und", "im", "Hain"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Unter jungen Schatten wallen.", "tokens": ["Un\u00b7ter", "jun\u00b7gen", "Schat\u00b7ten", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch der Wiese zartes Gr\u00fcn", "tokens": ["Durch", "der", "Wie\u00b7se", "zar\u00b7tes", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringsum bl\u00fchn", "tokens": ["Ring\u00b7sum", "bl\u00fchn"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Tausend Blumenkelch' und Dolden,", "tokens": ["Tau\u00b7send", "Blu\u00b7men\u00b7kelch'", "und", "Dol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell von Sonnenschein und Tau,", "tokens": ["Hell", "von", "Son\u00b7nen\u00b7schein", "und", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelblau,", "tokens": ["Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Rot und violett und golden.", "tokens": ["Rot", "und", "vi\u00b7o\u00b7lett", "und", "gol\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4hlt die d\u00fcftevollen aus,", "tokens": ["W\u00e4hlt", "die", "d\u00fcf\u00b7te\u00b7vol\u00b7len", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch zum Strau\u00df,", "tokens": ["Euch", "zum", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df er prang' am weichen Mieder.", "tokens": ["Da\u00df", "er", "prang'", "am", "wei\u00b7chen", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strebt der Busen aus dem Flor", "tokens": ["Strebt", "der", "Bu\u00b7sen", "aus", "dem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Halb hervor,", "tokens": ["Halb", "her\u00b7vor", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wall' er bergend auf ihn nieder.", "tokens": ["Wall'", "er", "ber\u00b7gend", "auf", "ihn", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ohn' ein starres Staatsgewand", "tokens": ["Ohn'", "ein", "star\u00b7res", "Staats\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt aufs Land,", "tokens": ["Eilt", "aufs", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ohne Perlen und Geschmeide;", "tokens": ["Oh\u00b7ne", "Per\u00b7len", "und", "Ge\u00b7schmei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freier hebt, voll Fr\u00fchlingslust,", "tokens": ["Frei\u00b7er", "hebt", ",", "voll", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich die Brust", "tokens": ["Sich", "die", "Brust"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Unter leichtem Sch\u00e4ferkleide.", "tokens": ["Un\u00b7ter", "leich\u00b7tem", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unentstellt von Ziererei,", "tokens": ["Un\u00b7ent\u00b7stellt", "von", "Zie\u00b7re\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Los und frei", "tokens": ["Los", "und", "frei"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "La\u00dft die langen Flechten hangen;", "tokens": ["La\u00dft", "die", "lan\u00b7gen", "Flech\u00b7ten", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zerstreuter Locken Spiel", "tokens": ["Und", "zer\u00b7streu\u00b7ter", "Lo\u00b7cken", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4us'le k\u00fchl", "tokens": ["S\u00e4us'\u00b7le", "k\u00fchl"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Um die warmen Rosenwangen.", "tokens": ["Um", "die", "war\u00b7men", "Ro\u00b7sen\u00b7wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcrzt euch leicht zum Reihentanz;", "tokens": ["Sch\u00fcrzt", "euch", "leicht", "zum", "Rei\u00b7hen\u00b7tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Biegt zum Kranz", "tokens": ["Biegt", "zum", "Kranz"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Rosmarin voll blauer Bl\u00fcte,", "tokens": ["Ros\u00b7ma\u00b7rin", "voll", "blau\u00b7er", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein weit umschlungnes Band", "tokens": ["Und", "ein", "weit", "um\u00b7schlung\u00b7nes", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieg' am Rand", "tokens": ["Flieg'", "am", "Rand"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Eurer gelben Halmenh\u00fcte.", "tokens": ["Eu\u00b7rer", "gel\u00b7ben", "Hal\u00b7men\u00b7h\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf des Waldes Farrenkraut", "tokens": ["Auf", "des", "Wal\u00b7des", "Far\u00b7ren\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt vertraut", "tokens": ["Setzt", "ver\u00b7traut"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Euch zusammen; kos't und singet,", "tokens": ["Euch", "zu\u00b7sam\u00b7men", ";", "kos't", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis des Abends falber Schein", "tokens": ["Bis", "des", "A\u00b7bends", "fal\u00b7ber", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Hain", "tokens": ["In", "den", "Hain"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Durch die Espenwipfel dringet.", "tokens": ["Durch", "die", "Es\u00b7pen\u00b7wip\u00b7fel", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}