{"textgrid.poem.40454": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Aggstein", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun die ersten Lerchen stiegen", "tokens": ["Nun", "die", "ers\u00b7ten", "Ler\u00b7chen", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Himmel freundlich lacht,", "tokens": ["Und", "der", "Him\u00b7mel", "freund\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' auch ich zu neuem Fliegen", "tokens": ["Hab'", "auch", "ich", "zu", "neu\u00b7em", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wanderfroh mich aufgemacht.", "tokens": ["Wan\u00b7der\u00b7froh", "mich", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir gilt's heut, Kuenringer Feste,", "tokens": ["Dir", "gilt's", "heut", ",", "Ku\u00b7en\u00b7rin\u00b7ger", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aggstein, wetterbraun und rot,", "tokens": ["A\u00b7ggstein", ",", "wet\u00b7ter\u00b7braun", "und", "rot", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVINF", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der gleich einem Geierneste", "tokens": ["Der", "gleich", "ei\u00b7nem", "Gei\u00b7er\u00b7nes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf die Wachau niederdroht.", "tokens": ["Auf", "die", "Wac\u00b7hau", "nie\u00b7der\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leicht ist Einla\u00df zu gewinnen,", "tokens": ["Leicht", "ist", "Ein\u00b7la\u00df", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Kein Gewaffen sperrt den Pfad", "tokens": ["Kein", "Ge\u00b7waf\u00b7fen", "sperrt", "den", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Hornsto\u00df von den Zinnen", "tokens": ["Und", "kein", "Horn\u00b7sto\u00df", "von", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meldet, da\u00df ein Wandrer naht.", "tokens": ["Mel\u00b7det", ",", "da\u00df", "ein", "Wand\u00b7rer", "naht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Linder Fr\u00fchlingsluft erschlossen", "tokens": ["Lin\u00b7der", "Fr\u00fch\u00b7lings\u00b7luft", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehn des Burgstalls Tr\u00fcmmerreih'n,", "tokens": ["Stehn", "des", "Burg\u00b7stalls", "Tr\u00fcm\u00b7mer\u00b7reih'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Jerg Schreckenwalds Genossen", "tokens": ["Und", "Jerg", "Schre\u00b7cken\u00b7walds", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reiten nicht mehr aus und ein.", "tokens": ["Rei\u00b7ten", "nicht", "mehr", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoch im Innern schl\u00fcpft ein Pf\u00f6rtlein", "tokens": ["Hoch", "im", "In\u00b7nern", "schl\u00fcpft", "ein", "Pf\u00f6rt\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den freien Fels hinaus", "tokens": ["Auf", "den", "frei\u00b7en", "Fels", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein schaurig schmales \u00d6rtlein", "tokens": ["Und", "ein", "schau\u00b7rig", "schma\u00b7les", "\u00d6rt\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberrascht mit starrem Graus.", "tokens": ["\u00dc\u00b7berr\u00b7ascht", "mit", "star\u00b7rem", "Graus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosengarten ist's gehei\u00dfen,", "tokens": ["Ro\u00b7sen\u00b7gar\u00b7ten", "ist's", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch vieldeutig klingt das Wort,", "tokens": ["Doch", "viel\u00b7deu\u00b7tig", "klingt", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die dornig wilden wei\u00dfen", "tokens": ["Nur", "die", "dor\u00b7nig", "wil\u00b7den", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Todesrosen bl\u00fchen dort.", "tokens": ["To\u00b7des\u00b7ro\u00b7sen", "bl\u00fc\u00b7hen", "dort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mancher stand hinausgesto\u00dfen", "tokens": ["Man\u00b7cher", "stand", "hin\u00b7aus\u00b7ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Kuppe steilem Rand,", "tokens": ["Auf", "der", "Kup\u00b7pe", "stei\u00b7lem", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ihn Sturm und Wettertosen", "tokens": ["Bis", "ihn", "Sturm", "und", "Wet\u00b7ter\u00b7to\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hunger \u00fcbermannt;", "tokens": ["Und", "der", "Hun\u00b7ger", "\u00fc\u00b7berm\u00b7annt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mancher, seine Qual zu k\u00fcrzen,", "tokens": ["Man\u00b7cher", ",", "sei\u00b7ne", "Qual", "zu", "k\u00fcr\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zog den Sprung zur Tiefe vor,", "tokens": ["Zog", "den", "Sprung", "zur", "Tie\u00b7fe", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo zerschellt in j\u00e4hem St\u00fcrzen", "tokens": ["Wo", "zer\u00b7schellt", "in", "j\u00e4\u00b7hem", "St\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald sich sein Gebein verlor.", "tokens": ["Bald", "sich", "sein", "Ge\u00b7bein", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "... Schwer emp\u00f6rt schau' ich das wilde", "tokens": ["...", "Schwer", "em\u00b7p\u00f6rt", "schau'", "ich", "das", "wil\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denkmal wilder Menschenart ...", "tokens": ["Denk\u00b7mal", "wil\u00b7der", "Men\u00b7schen\u00b7art", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh \u2013 da winkt vers\u00f6hnlich milde", "tokens": ["Sieh", "\u2013", "da", "winkt", "ver\u00b7s\u00f6hn\u00b7lich", "mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADV", "VVFIN", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch ein Gru\u00df der Gegenwart:", "tokens": ["Auch", "ein", "Gru\u00df", "der", "Ge\u00b7gen\u00b7wart", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwindlig ob des Abgrunds Schauer", "tokens": ["Schwind\u00b7lig", "ob", "des", "Ab\u00b7grunds", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOUS", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ragt des h\u00f6chsten Giebels Zack,", "tokens": ["Ragt", "des", "h\u00f6chs\u00b7ten", "Gie\u00b7bels", "Zack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und am h\u00f6chsten Saum der Mauer", "tokens": ["Und", "am", "h\u00f6chs\u00b7ten", "Saum", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Prangt der Name \u2013 ", "tokens": ["Prangt", "der", "Na\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+---", "measure": "dactylic.init"}}}}}