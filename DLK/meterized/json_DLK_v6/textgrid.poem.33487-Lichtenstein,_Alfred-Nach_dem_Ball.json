{"textgrid.poem.33487": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Ball", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht kriecht in die Keller, muffig matt.", "tokens": ["Die", "Nacht", "kriecht", "in", "die", "Kel\u00b7ler", ",", "muf\u00b7fig", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Glanzkleider torkeln durch der Stra\u00dfen Schutt.", "tokens": ["Glanz\u00b7klei\u00b7der", "tor\u00b7keln", "durch", "der", "Stra\u00b7\u00dfen", "Schutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gesichter sind verschimmelt und kaputt.", "tokens": ["Ge\u00b7sich\u00b7ter", "sind", "ver\u00b7schim\u00b7melt", "und", "ka\u00b7putt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "K\u00fchl brennt der blaue Morgen auf der Stadt.", "tokens": ["K\u00fchl", "brennt", "der", "blau\u00b7e", "Mor\u00b7gen", "auf", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie bald Musik und Tanz und Gier zerrann ...", "tokens": ["Wie", "bald", "Mu\u00b7sik", "und", "Tanz", "und", "Gier", "zer\u00b7rann", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es riecht nach Sonne. Und der Tag beginnt", "tokens": ["Es", "riecht", "nach", "Son\u00b7ne", ".", "Und", "der", "Tag", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Schienenwagen, Pferden, Schrei und Wind.", "tokens": ["Mit", "Schie\u00b7nen\u00b7wa\u00b7gen", ",", "Pfer\u00b7den", ",", "Schrei", "und", "Wind", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Mann streicht einen Herrenrumpf grau an.", "tokens": ["Ein", "Mann", "streicht", "ei\u00b7nen", "Her\u00b7ren\u00b7rumpf", "grau", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Alltag und Arbeit staubt die Menschen ein.", "tokens": ["All\u00b7tag", "und", "Ar\u00b7beit", "staubt", "die", "Men\u00b7schen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Familien fressen stumm ihr Mittagsmahl.", "tokens": ["Fa\u00b7mi\u00b7li\u00b7en", "fres\u00b7sen", "stumm", "ihr", "Mit\u00b7tags\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Durch einen Sch\u00e4del schwingt noch oft ein Saal,", "tokens": ["Durch", "ei\u00b7nen", "Sch\u00e4\u00b7del", "schwingt", "noch", "oft", "ein", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Viel dumpfe Sehnsucht und ein Seidenbein.", "tokens": ["Viel", "dump\u00b7fe", "Sehn\u00b7sucht", "und", "ein", "Sei\u00b7den\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}