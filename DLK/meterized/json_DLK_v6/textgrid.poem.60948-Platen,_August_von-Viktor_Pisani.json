{"textgrid.poem.60948": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Viktor Pisani", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als vom Kerker heraus, den ihm die Verleumder bereitet,", "tokens": ["Als", "vom", "Ker\u00b7ker", "he\u00b7raus", ",", "den", "ihm", "die", "Ver\u00b7leum\u00b7der", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Viktor trat, aufs neu F\u00fchrer der Flotte zu sein,", "tokens": ["Vik\u00b7tor", "trat", ",", "aufs", "neu", "F\u00fch\u00b7rer", "der", "Flot\u00b7te", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "----++--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dr\u00e4ngte das Volk sich um ihn, und sie riefen: Es lebe Pisani!", "tokens": ["Dr\u00e4ng\u00b7te", "das", "Volk", "sich", "um", "ihn", ",", "und", "sie", "rie\u00b7fen", ":", "Es", "le\u00b7be", "Pi\u00b7sa\u00b7ni", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Aber er wandte sich streng gegen den P\u00f6bel und sprach:", "tokens": ["A\u00b7ber", "er", "wand\u00b7te", "sich", "streng", "ge\u00b7gen", "den", "P\u00f6\u00b7bel", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "B\u00fcrgern geziemt es, zu rufen: Es lebe der heilige Markus!", "tokens": ["B\u00fcr\u00b7gern", "ge\u00b7ziemt", "es", ",", "zu", "ru\u00b7fen", ":", "Es", "le\u00b7be", "der", "hei\u00b7li\u00b7ge", "Mar\u00b7kus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wann doch duldete je knechtische Rufe der Staat?", "tokens": ["Wann", "doch", "dul\u00b7de\u00b7te", "je", "knech\u00b7ti\u00b7sche", "Ru\u00b7fe", "der", "Staat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}