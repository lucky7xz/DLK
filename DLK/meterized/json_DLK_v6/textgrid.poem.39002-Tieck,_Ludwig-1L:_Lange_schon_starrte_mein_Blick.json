{"textgrid.poem.39002": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lange schon starrte mein Blick", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange schon starrte mein Blick", "tokens": ["Lan\u00b7ge", "schon", "starr\u00b7te", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Hinaus in Flur und H\u00fcgel,", "tokens": ["Hin\u00b7aus", "in", "Flur", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer nicht erschien der Wunsch,", "tokens": ["Und", "im\u00b7mer", "nicht", "er\u00b7schien", "der", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sehns\u00fcchtigen Seele.", "tokens": ["Der", "sehn\u00b7s\u00fcch\u00b7ti\u00b7gen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Stille Tr\u00e4umerei umh\u00fcllte den Geist,", "tokens": ["Stil\u00b7le", "Tr\u00e4u\u00b7me\u00b7rei", "um\u00b7h\u00fcll\u00b7te", "den", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Da wendet sich pl\u00f6tzlich der Weg,", "tokens": ["Da", "wen\u00b7det", "sich", "pl\u00f6tz\u00b7lich", "der", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und rechts erscheint der hohe Petrus-Dom,", "tokens": ["Und", "rechts", "er\u00b7scheint", "der", "ho\u00b7he", "Pe\u00b7tru\u00b7sDom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Vatikans Pallast,", "tokens": ["Des", "Va\u00b7ti\u00b7kans", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Und fern umher gestreut wie H\u00fctten,", "tokens": ["Und", "fern", "um\u00b7her", "ge\u00b7streut", "wie", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die weltber\u00fchmte Stadt.", "tokens": ["Die", "welt\u00b7be\u00b7r\u00fchm\u00b7te", "Stadt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So ist der weite Weg nun \u00fcberwunden,", "tokens": ["So", "ist", "der", "wei\u00b7te", "Weg", "nun", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und endlich, endlich ist das erw\u00fcnschte Ziel erschienen?", "tokens": ["Und", "end\u00b7lich", ",", "end\u00b7lich", "ist", "das", "er\u00b7w\u00fcnschte", "Ziel", "er\u00b7schie\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie ich mich sammle,", "tokens": ["Und", "wie", "ich", "mich", "samm\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mich und die Gr\u00f6\u00dfe des Momentes zu f\u00fchlen,", "tokens": ["Mich", "und", "die", "Gr\u00f6\u00b7\u00dfe", "des", "Mo\u00b7men\u00b7tes", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Zerrinnt in Schmerz", "tokens": ["Zer\u00b7rinnt", "in", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das kaum gehaschte Bild,", "tokens": ["Das", "kaum", "ge\u00b7haschte", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und alle die alten edlen Erinnrungen", "tokens": ["Und", "al\u00b7le", "die", "al\u00b7ten", "ed\u00b7len", "E\u00b7rinn\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Entfliehn vor der dr\u00fcckenden, engen Gegenwart.", "tokens": ["Ent\u00b7fliehn", "vor", "der", "dr\u00fc\u00b7cken\u00b7den", ",", "en\u00b7gen", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wie klein ist der Mensch,", "tokens": ["Wie", "klein", "ist", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wie arm im Schein des Reichthums!", "tokens": ["Wie", "arm", "im", "Schein", "des", "Reicht\u00b7hums", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon treten die Geb\u00e4ude n\u00e4her,", "tokens": ["Schon", "tre\u00b7ten", "die", "Ge\u00b7b\u00e4u\u00b7de", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon heimathlicher wird Berg und Flur,", "tokens": ["Schon", "hei\u00b7math\u00b7li\u00b7cher", "wird", "Berg", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von alten Gem\u00e4lden", "tokens": ["Von", "al\u00b7ten", "Ge\u00b7m\u00e4l\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Erwacht in frischern Farben das Angedenken;", "tokens": ["Er\u00b7wacht", "in", "fri\u00b7schern", "Far\u00b7ben", "das", "An\u00b7ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hier schon die Br\u00fccke,", "tokens": ["Hier", "schon", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Stra\u00dfe der Vorstadt,", "tokens": ["Die", "Stra\u00b7\u00dfe", "der", "Vor\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und rascheren Trabes", "tokens": ["Und", "ra\u00b7sche\u00b7ren", "Tra\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "N\u00e4hern wir uns dem Pappelthor.", "tokens": ["N\u00e4\u00b7hern", "wir", "uns", "dem", "Pap\u00b7pel\u00b7thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ART", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Wir treten ein,", "tokens": ["Wir", "tre\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Vor mir der Platz und Obelisk,", "tokens": ["Vor", "mir", "der", "Platz", "und", "O\u00b7be\u00b7lisk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die drei Stra\u00dfen mit offnen Armen,", "tokens": ["Die", "drei", "Stra\u00b7\u00dfen", "mit", "off\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ein n\u00fcchternes Licht", "tokens": ["Ein", "n\u00fcch\u00b7ter\u00b7nes", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Erhellt unerfreulich", "tokens": ["Er\u00b7hellt", "un\u00b7er\u00b7freu\u00b7lich"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Tempel und Pallast.", "tokens": ["Tem\u00b7pel", "und", "Pal\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ich kann mich nur tr\u00f6sten.", "tokens": ["Ich", "kann", "mich", "nur", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Nun schnell in den Armen", "tokens": ["Nun", "schnell", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Geliebter Freunde", "tokens": ["Ge\u00b7lieb\u00b7ter", "Freun\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Der Klage Laut ert\u00f6nen zu lassen.", "tokens": ["Der", "Kla\u00b7ge", "Laut", "er\u00b7t\u00f6\u00b7nen", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}