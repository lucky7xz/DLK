{"textgrid.poem.67212": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn neu das Licht der Erde sich gezeiget,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn neu das Licht der Erde sich gezeiget,", "tokens": ["Wenn", "neu", "das", "Licht", "der", "Er\u00b7de", "sich", "ge\u00b7zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Fr\u00fchlingsregen gl\u00e4nzt das gr\u00fcne Tal und munter", "tokens": ["Von", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", "gl\u00e4nzt", "das", "gr\u00fc\u00b7ne", "Tal", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Bl\u00fcten Wei\u00df am hellen Strom hinunter,", "tokens": ["Der", "Bl\u00fc\u00b7ten", "Wei\u00df", "am", "hel\u00b7len", "Strom", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachdem ein heitrer Tag zu Menschen sich geneiget.", "tokens": ["Nach\u00b7dem", "ein", "hei\u00b7trer", "Tag", "zu", "Men\u00b7schen", "sich", "ge\u00b7nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sichtbarkeit gewinnt von hellen Unterschieden,", "tokens": ["Die", "Sicht\u00b7bar\u00b7keit", "ge\u00b7winnt", "von", "hel\u00b7len", "Un\u00b7ter\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fr\u00fchlingshimmel weilt mit seinem Frieden,", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", "weilt", "mit", "sei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ungest\u00f6rt der Mensch des Jahres Reiz betrachtet,", "tokens": ["Da\u00df", "un\u00b7ge\u00b7st\u00f6rt", "der", "Mensch", "des", "Jah\u00b7res", "Reiz", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auf Vollkommenheit des Lebens achtet.", "tokens": ["Und", "auf", "Voll\u00b7kom\u00b7men\u00b7heit", "des", "Le\u00b7bens", "ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}