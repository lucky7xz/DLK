eter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und nun flattern wilde Blitze,", "tokens": ["Und", "nun", "flat\u00b7tern", "wil\u00b7de", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm rast um den Felsenriff,", "tokens": ["Sturm", "rast", "um", "den", "Fel\u00b7sen\u00b7riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von grimmer Wogen Spitze", "tokens": ["Und", "von", "grim\u00b7mer", "Wo\u00b7gen", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt geborsten sich das Schiff.", "tokens": ["St\u00fcrzt", "ge\u00b7bors\u00b7ten", "sich", "das", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwankend auf des Mastes Splitter,", "tokens": ["Schwan\u00b7kend", "auf", "des", "Mas\u00b7tes", "Split\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlingt die Braut sich um den Ritter.", "tokens": ["Schlingt", "die", "Braut", "sich", "um", "den", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und die M\u00fcde in den Armen,", "tokens": ["Und", "die", "M\u00fc\u00b7de", "in", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Springt er abw\u00e4rts, sinkt und ringt,", "tokens": ["Springt", "er", "ab\u00b7w\u00e4rts", ",", "sinkt", "und", "ringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt den Leib, den bl\u00fchend warmen,", "tokens": ["H\u00e4lt", "den", "Leib", ",", "den", "bl\u00fc\u00b7hend", "war\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis er alle Wogen zwingt,", "tokens": ["Bis", "er", "al\u00b7le", "Wo\u00b7gen", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und am Blumenstrand gerettet,", "tokens": ["Und", "am", "Blu\u00b7men\u00b7strand", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf das Gras sein Liebstes bettet.", "tokens": ["Auf", "das", "Gras", "sein", "Liebs\u00b7tes", "bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbwache auf, wach auf, du Sch\u00f6ne!", "tokens": ["\u00bb", "wa\u00b7che", "auf", ",", "wach", "auf", ",", "du", "Sch\u00f6\u