{"textgrid.poem.59440": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch des Meeresschlosses Hallen", "tokens": ["Durch", "des", "Mee\u00b7res\u00b7schlos\u00b7ses", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf besp\u00fcltem Felsenhang,", "tokens": ["Auf", "be\u00b7sp\u00fcl\u00b7tem", "Fel\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weht der H\u00f6rner festlich Schallen;", "tokens": ["Weht", "der", "H\u00f6r\u00b7ner", "fest\u00b7lich", "Schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froher Hochzeitg\u00e4ste Drang,", "tokens": ["Fro\u00b7her", "Hoch\u00b7zeit\u00b7g\u00e4s\u00b7te", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei der Kerzen Zauberglanze,", "tokens": ["Bei", "der", "Ker\u00b7zen", "Zau\u00b7ber\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wogt im buntverschlungnen Tanze.", "tokens": ["Wogt", "im", "bunt\u00b7ver\u00b7schlung\u00b7nen", "Tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber an des Fensters Bogen,", "tokens": ["A\u00b7ber", "an", "des", "Fens\u00b7ters", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne von der lauten Pracht,", "tokens": ["Fer\u00b7ne", "von", "der", "lau\u00b7ten", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut der Br\u00e4ut'gam in die Wogen", "tokens": ["Schaut", "der", "Br\u00e4ut'\u00b7gam", "in", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drau\u00dfen in der finstern Nacht,", "tokens": ["Drau\u00b7\u00dfen", "in", "der", "fins\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die trunknen Blicke schreiten", "tokens": ["Und", "die", "trunk\u00b7nen", "Bli\u00b7cke", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Furchtlos durch die \u00f6den Weiten.", "tokens": ["Furcht\u00b7los", "durch", "die", "\u00f6\u00b7den", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bblieblich\u00ab, sprach der wilde Ritter", "tokens": ["\u00bb", "lieb\u00b7lich", "\u00ab", ",", "sprach", "der", "wil\u00b7de", "Rit\u00b7ter"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der zarten, sch\u00f6nen Braut,", "tokens": ["Zu", "der", "zar\u00b7ten", ",", "sch\u00f6\u00b7nen", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bblieblich girrt die sanfte Zither \u2013", "tokens": ["\u00bb", "lieb\u00b7lich", "girrt", "die", "sanf\u00b7te", "Zi\u00b7ther", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wogen dumpfes Brausen", "tokens": ["Und", "der", "Wo\u00b7gen", "dum\u00b7pfes", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Hebt das Herz in k\u00fchnem Grausen.", "tokens": ["Hebt", "das", "Herz", "in", "k\u00fch\u00b7nem", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich kann hier nicht m\u00fc\u00dfig lauern,", "tokens": ["Ich", "kann", "hier", "nicht", "m\u00fc\u00b7\u00dfig", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Treiben auf dem flachen Sand,", "tokens": ["Trei\u00b7ben", "auf", "dem", "fla\u00b7chen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Kreis von Felsenmauern", "tokens": ["Die\u00b7ser", "Kreis", "von", "Fel\u00b7sen\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt mein Leben nicht umspannt;", "tokens": ["H\u00e4lt", "mein", "Le\u00b7ben", "nicht", "um\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6nre L\u00e4nder bl\u00fchen ferne,", "tokens": ["Sch\u00f6n\u00b7re", "L\u00e4n\u00b7der", "bl\u00fc\u00b7hen", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das verk\u00fcnden mir die Sterne.", "tokens": ["Das", "ver\u00b7k\u00fcn\u00b7den", "mir", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du mu\u00dft glauben, du mu\u00dft wagen,", "tokens": ["Du", "mu\u00dft", "glau\u00b7ben", ",", "du", "mu\u00dft", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, den Argonauten gleich,", "tokens": ["Und", ",", "den", "Ar\u00b7go\u00b7nau\u00b7ten", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die Woge fromm dich tragen", "tokens": ["Wird", "die", "Wo\u00b7ge", "fromm", "dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das wunderbare Reich;", "tokens": ["In", "das", "wun\u00b7der\u00b7ba\u00b7re", "Reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutig streitend mit den Winden,", "tokens": ["Mu\u00b7tig", "strei\u00b7tend", "mit", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df ich meine Heimat finden!", "tokens": ["Mu\u00df", "ich", "mei\u00b7ne", "Hei\u00b7mat", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Siehst du, hei\u00dfer Sehnsucht Fl\u00fcgel,", "tokens": ["Siehst", "du", ",", "hei\u00b7\u00dfer", "Sehn\u00b7sucht", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Segel dort gespannt?", "tokens": ["Wei\u00b7\u00dfe", "Se\u00b7gel", "dort", "ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst du tief die feuchten H\u00fcgel", "tokens": ["H\u00f6rst", "du", "tief", "die", "feuch\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlagen an die Felsenwand?", "tokens": ["Schla\u00b7gen", "an", "die", "Fel\u00b7sen\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ist Sang zum Hochzeitsreigen \u2013", "tokens": ["Das", "ist", "Sang", "zum", "Hoch\u00b7zeits\u00b7rei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Willst du mit mir niedersteigen?", "tokens": ["Willst", "du", "mit", "mir", "nie\u00b7ders\u00b7tei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kannst du rechte Liebe fassen,", "tokens": ["Kannst", "du", "rech\u00b7te", "Lie\u00b7be", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so frage, zaudre nicht!", "tokens": ["Nun", "so", "fra\u00b7ge", ",", "zaud\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlo\u00df und Garten mu\u00dft du lassen", "tokens": ["Schlo\u00df", "und", "Gar\u00b7ten", "mu\u00dft", "du", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Eltern Angesicht \u2013", "tokens": ["Und", "der", "El\u00b7tern", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf der Flut mit mir alleine,", "tokens": ["Auf", "der", "Flut", "mit", "mir", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da erst, Liebchen, bist du meine!\u00ab", "tokens": ["Da", "erst", ",", "Lieb\u00b7chen", ",", "bist", "du", "mei\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schweigend sieht ihn an die milde", "tokens": ["Schwei\u00b7gend", "sieht", "ihn", "an", "die", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Braut mit schauerlicher Lust,", "tokens": ["Braut", "mit", "schau\u00b7er\u00b7li\u00b7cher", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinkt dem k\u00fchnen Ritterbilde", "tokens": ["Sinkt", "dem", "k\u00fch\u00b7nen", "Rit\u00b7ter\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trunken an die stolze Brust:", "tokens": ["Trun\u00b7ken", "an", "die", "stol\u00b7ze", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbdir hab ich mein Los ergeben,", "tokens": ["\u00bb", "dir", "hab", "ich", "mein", "Los", "er\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schalte nun mit meinem Leben.\u00ab", "tokens": ["Schal\u00b7te", "nun", "mit", "mei\u00b7nem", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und er tr\u00e4gt die s\u00fc\u00dfe Beute", "tokens": ["Und", "er", "tr\u00e4gt", "die", "s\u00fc\u00b7\u00dfe", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubelnd aus dem Schlo\u00df aufs Schiff,", "tokens": ["Ju\u00b7belnd", "aus", "dem", "Schlo\u00df", "aufs", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drunten harren seine Leute,", "tokens": ["Drun\u00b7ten", "har\u00b7ren", "sei\u00b7ne", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sto\u00dfen froh vom Felsenriff;", "tokens": ["Sto\u00b7\u00dfen", "froh", "vom", "Fel\u00b7sen\u00b7riff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die H\u00f6rner leis verhallen,", "tokens": ["Und", "die", "H\u00f6r\u00b7ner", "leis", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einsam rings die Wogen schallen.", "tokens": ["Ein\u00b7sam", "rings", "die", "Wo\u00b7gen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie die Sterne matter blinken", "tokens": ["Wie", "die", "Ster\u00b7ne", "mat\u00b7ter", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die morgenrote Flut,", "tokens": ["In", "die", "mor\u00b7gen\u00b7ro\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht sie fern die Berge sinken,", "tokens": ["Sieht", "sie", "fern", "die", "Ber\u00b7ge", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammend steigt die hehre Glut,", "tokens": ["Flam\u00b7mend", "steigt", "die", "heh\u00b7re", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcberm Spiegel trunkner Wellen", "tokens": ["\u00dc\u00b7berm", "Spie\u00b7gel", "trunk\u00b7ner", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rauschender die Segel schwellen.", "tokens": ["Rau\u00b7schen\u00b7der", "die", "Se\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Monde steigen und sich neigen,", "tokens": ["Mon\u00b7de", "stei\u00b7gen", "und", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich weht schon fremde Luft,", "tokens": ["Lieb\u00b7lich", "weht", "schon", "frem\u00b7de", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sehn sie ein Eiland steigen", "tokens": ["Da", "sehn", "sie", "ein", "Ei\u00b7land", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Feenhaft aus blauem Duft,", "tokens": ["Fe\u00b7en\u00b7haft", "aus", "blau\u00b7em", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein farb'ger Blumenstreifen \u2013", "tokens": ["Wie", "ein", "fa\u00b7rb'\u00b7ger", "Blu\u00b7men\u00b7strei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Meerw\u00e4rts fremde V\u00f6gel schweifen.", "tokens": ["Meer\u