{"textgrid.poem.62760": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein b\u00f6ses Weib, das keinem Drachen wich,", "genre": "verse", "period": "N.A.", "pub_year": 1754, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein b\u00f6ses Weib, das keinem Drachen wich,", "tokens": ["Ein", "b\u00f6\u00b7ses", "Weib", ",", "das", "kei\u00b7nem", "Dra\u00b7chen", "wich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schrecklichste von allen Ruthen", "tokens": ["Die", "schreck\u00b7lichs\u00b7te", "von", "al\u00b7len", "Ru\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des strafenden Geschicks, ers\u00e4ufte sich", "tokens": ["Des", "stra\u00b7fen\u00b7den", "Ge\u00b7schicks", ",", "er\u00b7s\u00e4uf\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ward ein Spiel der Fluthen.", "tokens": ["Und", "ward", "ein", "Spiel", "der", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Mann sucht den entseelten Leib,", "tokens": ["Ihr", "Mann", "sucht", "den", "ent\u00b7seel\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den er mit Sang und Klang begraben wollte,", "tokens": ["Den", "er", "mit", "Sang", "und", "Klang", "be\u00b7gra\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit als Poltergeist auch nach dem Tod sein Weib", "tokens": ["Da\u00b7mit", "als", "Pol\u00b7ter\u00b7geist", "auch", "nach", "dem", "Tod", "sein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "NN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihn ja nicht plagen sollte.", "tokens": ["Ihn", "ja", "nicht", "pla\u00b7gen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er fuhr in einem Kahn mit bangem Flei\u00df", "tokens": ["Er", "fuhr", "in", "ei\u00b7nem", "Kahn", "mit", "ban\u00b7gem", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Flu\u00df hinab: er w\u00fchlt in Moor und Schl\u00fcnden,", "tokens": ["Den", "Flu\u00df", "hin\u00b7ab", ":", "er", "w\u00fchlt", "in", "Moor", "und", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fand ihren Modehut und ihren Modestei\u00df;", "tokens": ["Fand", "ih\u00b7ren", "Mo\u00b7de\u00b7hut", "und", "ih\u00b7ren", "Mo\u00b7des\u00b7tei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie selbst war nicht zu finden.", "tokens": ["Sie", "selbst", "war", "nicht", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft uns die Gondel drehn, rief endlich Nachbar Veit,", "tokens": ["La\u00dft", "uns", "die", "Gon\u00b7del", "drehn", ",", "rief", "end\u00b7lich", "Nach\u00b7bar", "Veit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "ADV", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Bootsmann, aus: ist sie sich gleich geblieben,", "tokens": ["Sein", "Boots\u00b7mann", ",", "aus", ":", "ist", "sie", "sich", "gleich", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$.", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So hat sie wohl der Geist der Widerspenstigkeit", "tokens": ["So", "hat", "sie", "wohl", "der", "Geist", "der", "Wi\u00b7der\u00b7spens\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Strom hinaufgetrieben.", "tokens": ["Den", "Strom", "hin\u00b7auf\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}