{"textgrid.poem.47421": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mich in ein arm Gestiebe]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich in ein arm Gestiebe", "tokens": ["Mich", "in", "ein", "arm", "Ge\u00b7stie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwandelt hat die Liebe,", "tokens": ["Ver\u00b7wan\u00b7delt", "hat", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und also deine Sch\u00f6ne", "tokens": ["Und", "al\u00b7so", "dei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umwirbel' ich und h\u00f6hne,", "tokens": ["Um\u00b7wir\u00b7bel'", "ich", "und", "h\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "So leicht und zart beschaffen,", "tokens": ["So", "leicht", "und", "zart", "be\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Winde Sturmgewalt.", "tokens": ["Der", "Win\u00b7de", "Sturm\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn mich hinwegzuraffen", "tokens": ["Denn", "mich", "hin\u00b7weg\u00b7zu\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus deiner Reize Schlingen,", "tokens": ["Aus", "dei\u00b7ner", "Rei\u00b7ze", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nie wird es ihr gelingen,", "tokens": ["Nie", "wird", "es", "ihr", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So schrecklich ihre Woge wallt.", "tokens": ["So", "schreck\u00b7lich", "ih\u00b7re", "Wo\u00b7ge", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}