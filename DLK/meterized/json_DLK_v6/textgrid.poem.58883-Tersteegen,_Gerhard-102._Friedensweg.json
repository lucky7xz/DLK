{"textgrid.poem.58883": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "102. Friedensweg", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gar nichts will als Gott allein", "tokens": ["Wer", "gar", "nichts", "will", "als", "Gott", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIS", "VMFIN", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinen liebsten Willen,", "tokens": ["Und", "sei\u00b7nen", "liebs\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mag in D\u00fcrre, Furcht und Pein", "tokens": ["Der", "mag", "in", "D\u00fcr\u00b7re", ",", "Furcht", "und", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich leicht und sicher stillen.", "tokens": ["Sich", "leicht", "und", "si\u00b7cher", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}