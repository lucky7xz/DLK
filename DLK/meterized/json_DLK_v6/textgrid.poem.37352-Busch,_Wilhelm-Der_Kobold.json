{"textgrid.poem.37352": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Kobold", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem H\u00e4uschen, sozusagen \u2013", "tokens": ["In", "ei\u00b7nem", "H\u00e4usc\u00b7hen", ",", "so\u00b7zu\u00b7sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(den ersten Stock bewohnt der Magen)", "tokens": ["(", "den", "ers\u00b7ten", "Stock", "be\u00b7wohnt", "der", "Ma\u00b7gen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In einem H\u00e4uschen war's nicht richtig.", "tokens": ["In", "ei\u00b7nem", "H\u00e4usc\u00b7hen", "wa\u00b7r's", "nicht", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darinnen spukt und tobte t\u00fcchtig", "tokens": ["Da\u00b7rin\u00b7nen", "spukt", "und", "tob\u00b7te", "t\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Kobold wie ein wildes B\u00fcbchen", "tokens": ["Ein", "Ko\u00b7bold", "wie", "ein", "wil\u00b7des", "B\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Keller bis zum Oberst\u00fcbchen.", "tokens": ["Vom", "Kel\u00b7ler", "bis", "zum", "O\u00b7bers\u00b7t\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcrwahr, es war ein b\u00f6s Get\u00f6s.", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "war", "ein", "b\u00f6s", "Ge\u00b7t\u00f6s", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hausherr wird zuletzt nerv\u00f6s,", "tokens": ["Der", "Haus\u00b7herr", "wird", "zu\u00b7letzt", "ner\u00b7v\u00f6s", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und als ein desperater Mann", "tokens": ["Und", "als", "ein", "des\u00b7pe\u00b7ra\u00b7ter", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Steckt er kurzweg sein H\u00e4uschen an", "tokens": ["Steckt", "er", "kurz\u00b7weg", "sein", "H\u00e4usc\u00b7hen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und baut ein Haus sich anderswo", "tokens": ["Und", "baut", "ein", "Haus", "sich", "an\u00b7ders\u00b7wo"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und meint, da ging es ihm nicht so.", "tokens": ["Und", "meint", ",", "da", "ging", "es", "ihm", "nicht", "so", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Allein, da sieht er sich betrogen.", "tokens": ["Al\u00b7lein", ",", "da", "sieht", "er", "sich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Kobold ist mit umgezogen", "tokens": ["Der", "Ko\u00b7bold", "ist", "mit", "um\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und macht Spektakel und Rumor", "tokens": ["Und", "macht", "Spek\u00b7ta\u00b7kel", "und", "Ru\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Viel \u00e4rger noch, als wie zuvor.", "tokens": ["Viel", "\u00e4r\u00b7ger", "noch", ",", "als", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOUS", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ha, rief der Mann, wer bist du, sprich!", "tokens": ["Ha", ",", "rief", "der", "Mann", ",", "wer", "bist", "du", ",", "sprich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Kobold lacht: Ich bin dein Ich.", "tokens": ["Der", "Ko\u00b7bold", "lacht", ":", "Ich", "bin", "dein", "Ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPOSAT", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}