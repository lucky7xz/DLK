{"dta.poem.1061": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die erste Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wan jhr/ g\u00f6tliche Liebelein/", "tokens": ["Wan", "jhr", "/", "g\u00f6t\u00b7li\u00b7che", "Lie\u00b7be\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mir gantz gn\u00e4diglich zugesaget/", "tokens": ["Mir", "gantz", "gn\u00e4\u00b7di\u00b7glich", "zu\u00b7ge\u00b7sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Da ich bey ewerm br\u00fcnnelein", "tokens": ["Da", "ich", "bey", "e\u00b7werm", "br\u00fcn\u00b7nel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch zu begr\u00fcssen mich gewaget/", "tokens": ["Euch", "zu", "be\u00b7gr\u00fcs\u00b7sen", "mich", "ge\u00b7wa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch meiner Oden s\u00fcssigkeit", "tokens": ["Durch", "mei\u00b7ner", "O\u00b7den", "s\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jener thorheit kundbar zumachen/", "tokens": ["Je\u00b7ner", "thor\u00b7heit", "kund\u00b7bar", "zu\u00b7ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Welche au\u00df aigensinnigkeit", "tokens": ["Wel\u00b7che", "au\u00df", "ai\u00b7gen\u00b7sin\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Die Teutsche Poesy verlachen:", "tokens": ["Die", "Teut\u00b7sche", "Poe\u00b7sy", "ver\u00b7la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "So machet nu/ O s\u00fcsse schar", "tokens": ["So", "ma\u00b7chet", "nu", "/", "O", "s\u00fcs\u00b7se", "schar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$(", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jetzmahls ewere Zusag wahr;", "tokens": ["Jetz\u00b7mahls", "e\u00b7we\u00b7re", "Zu\u00b7sag", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Vnd helfet mir meine wort setzen/", "tokens": ["Vnd", "hel\u00b7fet", "mir", "mei\u00b7ne", "wort", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Das sie die G\u00f6tter selbs erg\u00f6tzen.", "tokens": ["Das", "sie", "die", "G\u00f6t\u00b7ter", "selbs", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}