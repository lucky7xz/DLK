{"textgrid.poem.46491": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fenster schlie\u00dfen, glimmern stille,", "genre": "verse", "period": "N.A.", "pub_year": 1918, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fenster schlie\u00dfen, glimmern stille,", "tokens": ["Fens\u00b7ter", "schlie\u00b7\u00dfen", ",", "glim\u00b7mern", "stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uslein r\u00fccken Dach an Dach", "tokens": ["H\u00e4us\u00b7lein", "r\u00fc\u00b7cken", "Dach", "an", "Dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel stehen feiernd stille,", "tokens": ["Him\u00b7mel", "ste\u00b7hen", "fei\u00b7ernd", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mond wird Silberfrucht und wach.", "tokens": ["Mond", "wird", "Sil\u00b7ber\u00b7frucht", "und", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcder Leib schl\u00e4ft in der Stille,", "tokens": ["M\u00fc\u00b7der", "Leib", "schl\u00e4ft", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz schl\u00e4gt alle Stunden nach,", "tokens": ["Herz", "schl\u00e4gt", "al\u00b7le", "Stun\u00b7den", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lebt f\u00fcr sich durch Schlaf und Stille \u2013", "tokens": ["Lebt", "f\u00fcr", "sich", "durch", "Schlaf", "und", "Stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohin denn? wozu? aus wessen Wille?", "tokens": ["Wo\u00b7hin", "denn", "?", "wo\u00b7zu", "?", "aus", "wes\u00b7sen", "Wil\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "PWAV", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Lautlos, langsam fallen Wand und Dach.", "tokens": ["Laut\u00b7los", ",", "lang\u00b7sam", "fal\u00b7len", "Wand", "und", "Dach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}