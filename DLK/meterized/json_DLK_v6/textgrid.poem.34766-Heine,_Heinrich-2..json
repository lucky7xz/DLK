{"textgrid.poem.34766": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie rasch du auch vor\u00fcberschrittest,", "tokens": ["Wie", "rasch", "du", "auch", "vor\u00b7\u00fc\u00b7bersc\u00b7hrit\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal schautest du zur\u00fcck,", "tokens": ["Noch", "ein\u00b7mal", "schau\u00b7test", "du", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mund, wie fragend, k\u00fchnge\u00f6ffnet,", "tokens": ["Der", "Mund", ",", "wie", "fra\u00b7gend", ",", "k\u00fchn\u00b7ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrmischer Hochmut in dem Blick.", "tokens": ["St\u00fcr\u00b7mi\u00b7scher", "Hoch\u00b7mut", "in", "dem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Oh, da\u00df ich nie zu fassen suchte", "tokens": ["Oh", ",", "da\u00df", "ich", "nie", "zu", "fas\u00b7sen", "such\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wei\u00dfe fl\u00fcchtige Gewand!", "tokens": ["Das", "wei\u00b7\u00dfe", "fl\u00fcch\u00b7ti\u00b7ge", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die holde Spur der kleinen F\u00fc\u00dfe,", "tokens": ["Die", "hol\u00b7de", "Spur", "der", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oh, da\u00df ich nie sie wiederfand!", "tokens": ["Oh", ",", "da\u00df", "ich", "nie", "sie", "wie\u00b7der\u00b7fand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschwunden ist ja deine Wildheit,", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "ja", "dei\u00b7ne", "Wild\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bist wie die andern zahm und klar,", "tokens": ["Bist", "wie", "die", "an\u00b7dern", "zahm", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sanft und unertr\u00e4glich g\u00fctig,", "tokens": ["Und", "sanft", "und", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ach! nun liebst du mich sogar!", "tokens": ["Und", "ach", "!", "nun", "liebst", "du", "mich", "so\u00b7gar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}