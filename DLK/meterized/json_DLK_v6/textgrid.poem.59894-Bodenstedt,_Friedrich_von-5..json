{"textgrid.poem.59894": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Mirza-Jussuf, dein Schmollen jetzt!", "tokens": ["La\u00df", ",", "Mir\u00b7za\u00b7Jus\u00b7suf", ",", "dein", "Schmol\u00b7len", "jetzt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich bin zu munter, um dir zu grollen jetzt!", "tokens": ["Ich", "bin", "zu", "mun\u00b7ter", ",", "um", "dir", "zu", "grol\u00b7len", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUI", "PPER", "APPR", "ADJA", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Statt Ha\u00df auszus\u00e4en, wie du es tust,", "tokens": ["Statt", "Ha\u00df", "aus\u00b7zu\u00b7s\u00e4\u00b7en", ",", "wie", "du", "es", "tust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schl\u00fcrf' ich ein meinen Becher, den vollen, jetzt.", "tokens": ["Schl\u00fcr\u00b7f'", "ich", "ein", "mei\u00b7nen", "Be\u00b7cher", ",", "den", "vol\u00b7len", ",", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Schon genug bist du bestraft in der Welt hier!", "tokens": ["Schon", "ge\u00b7nug", "bist", "du", "be\u00b7straft", "in", "der", "Welt", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da\u00df nichts dir behagt, nichts gef\u00e4llt hier \u2013", "tokens": ["Da\u00df", "nichts", "dir", "be\u00b7hagt", ",", "nichts", "ge\u00b7f\u00e4llt", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und ist doch f\u00fcr jeden, der zu genie\u00dfen wei\u00df,", "tokens": ["Und", "ist", "doch", "f\u00fcr", "je\u00b7den", ",", "der", "zu", "ge\u00b7nie\u00b7\u00dfen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PIAT", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Alles so herrlich gemacht und bestellt hier!", "tokens": ["Al\u00b7les", "so", "herr\u00b7lich", "ge\u00b7macht", "und", "be\u00b7stellt", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "VVPP", "KON", "VVFIN", "ADV", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}