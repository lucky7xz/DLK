{"textgrid.poem.61255": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Stern will gr\u00fc\u00dfend funkeln,", "tokens": ["Kein", "Stern", "will", "gr\u00fc\u00b7\u00dfend", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Wolken h\u00e4ngt die Nacht;", "tokens": ["In", "Wol\u00b7ken", "h\u00e4ngt", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch geht durchs Tal im Dunkeln", "tokens": ["Doch", "geht", "durchs", "Tal", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein S\u00e4useln lau und sacht.", "tokens": ["Ein", "S\u00e4u\u00b7seln", "lau", "und", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geheimnisvolles Wallen", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Wal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt von den Wipfeln her,", "tokens": ["Kommt", "von", "den", "Wip\u00b7feln", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einzelne Tropfen fallen", "tokens": ["Ein\u00b7zel\u00b7ne", "Trop\u00b7fen", "fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie Tr\u00e4nen hei\u00df und schwer.", "tokens": ["Wie", "Tr\u00e4\u00b7nen", "hei\u00df", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist, als k\u00f6nnt' ich sp\u00fcren", "tokens": ["Mir", "ist", ",", "als", "k\u00f6nnt'", "ich", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wind, im Dufte der Flur,", "tokens": ["Im", "Wind", ",", "im", "Duf\u00b7te", "der", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie sich die Kr\u00e4fte r\u00fchren", "tokens": ["Wie", "sich", "die", "Kr\u00e4f\u00b7te", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schaffenden Natur.", "tokens": ["Der", "schaf\u00b7fen\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, mir im Busen ringt es", "tokens": ["Ach", ",", "mir", "im", "Bu\u00b7sen", "ringt", "es"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So dunkelm\u00e4chtig auch,", "tokens": ["So", "dun\u00b7kel\u00b7m\u00e4ch\u00b7tig", "auch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da br\u00fctet's, und da klingt es", "tokens": ["Da", "br\u00fc\u00b7tet's", ",", "und", "da", "klingt", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewegt vom Fr\u00fchlingshauch.", "tokens": ["Be\u00b7wegt", "vom", "Fr\u00fch\u00b7lings\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es r\u00fchrt der Saft sich wieder", "tokens": ["Es", "r\u00fchrt", "der", "Saft", "sich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meines Lebens Baum.", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist's Liebe? Sind es Lieder?", "tokens": ["Ist's", "Lie\u00b7be", "?", "Sind", "es", "Lie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch unterscheid' ich's kaum.", "tokens": ["Noch", "un\u00b7ter\u00b7scheid'", "ich's", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}