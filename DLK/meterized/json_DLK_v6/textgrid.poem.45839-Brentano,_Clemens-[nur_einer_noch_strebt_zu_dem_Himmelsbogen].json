{"textgrid.poem.45839": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[nur einer noch strebt zu dem Himmelsbogen]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur einer noch strebt zu dem Himmelsbogen", "tokens": ["Nur", "ei\u00b7ner", "noch", "strebt", "zu", "dem", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der letzte ist's, die gr\u00fcne Hoffnung spielt", "tokens": ["Der", "letz\u00b7te", "ist's", ",", "die", "gr\u00fc\u00b7ne", "Hoff\u00b7nung", "spielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So bang um ihn, der ewig hingezogen", "tokens": ["So", "bang", "um", "ihn", ",", "der", "e\u00b7wig", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Himmelblau, die letzte Bl\u00fcte k\u00fchlt.", "tokens": ["Im", "Him\u00b7mel\u00b7blau", ",", "die", "letz\u00b7te", "Bl\u00fc\u00b7te", "k\u00fchlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Bl\u00fcte harrt, will da\u00df aus jenen Wogen,", "tokens": ["Die", "Bl\u00fc\u00b7te", "harrt", ",", "will", "da\u00df", "aus", "je\u00b7nen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VMFIN", "KOUS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Sternlein ihr in Busen fall', doch w\u00fchlt", "tokens": ["Ein", "Stern\u00b7lein", "ihr", "in", "Bu\u00b7sen", "fall'", ",", "doch", "w\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "PTKVZ", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein schwarzer Sturm, in sanften Himmelsmeeren", "tokens": ["Ein", "schwar\u00b7zer", "Sturm", ",", "in", "sanf\u00b7ten", "Him\u00b7mels\u00b7mee\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ohn' den Stern, kann lang die Bl\u00fct' nicht w\u00e4hren.", "tokens": ["Und", "ohn'", "den", "Stern", ",", "kann", "lang", "die", "Bl\u00fct'", "nicht", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VMFIN", "ADJD", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und endlich irrt, von unerstiegnen H\u00f6hen", "tokens": ["Und", "end\u00b7lich", "irrt", ",", "von", "un\u00b7er\u00b7sti\u00b7eg\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die es verlie\u00df, und nimmermehr erringt", "tokens": ["Die", "es", "ver\u00b7lie\u00df", ",", "und", "nim\u00b7mer\u00b7mehr", "er\u00b7ringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wesen her; das mir mit s\u00fc\u00dfen Wehen", "tokens": ["Ein", "We\u00b7sen", "her", ";", "das", "mir", "mit", "s\u00fc\u00b7\u00dfen", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Busen l\u00f6st. Zur offnen Wunde dringt", "tokens": ["Den", "Bu\u00b7sen", "l\u00f6st", ".", "Zur", "off\u00b7nen", "Wun\u00b7de", "dringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein h\u00f6hres Leben. Nie wird mir's vergehen", "tokens": ["Sein", "h\u00f6h\u00b7res", "Le\u00b7ben", ".", "Nie", "wird", "mir's", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VAFIN", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil mein Gebet sich um die Sch\u00f6nheit schlingt.", "tokens": ["Weil", "mein", "Ge\u00b7bet", "sich", "um", "die", "Sch\u00f6n\u00b7heit", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch kann sie nur sich sch\u00f6n herniederneigen", "tokens": ["Doch", "kann", "sie", "nur", "sich", "sch\u00f6n", "her\u00b7nie\u00b7der\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ohne sie ich nie die H\u00f6h' ersteigen.", "tokens": ["Und", "oh\u00b7ne", "sie", "ich", "nie", "die", "H\u00f6h'", "er\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So fesselt mich die sch\u00f6nste Freiheit wieder", "tokens": ["So", "fes\u00b7selt", "mich", "die", "sch\u00f6ns\u00b7te", "Frei\u00b7heit", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ew'ger Sehnsucht an die Erde hin.", "tokens": ["Mit", "ew'\u00b7ger", "Sehn\u00b7sucht", "an", "die", "Er\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn sie verlor die G\u00f6ttlichkeit der Lieder", "tokens": ["Denn", "sie", "ver\u00b7lor", "die", "G\u00f6tt\u00b7lich\u00b7keit", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sie elegisch singt. Der hohe Sinn", "tokens": ["Die", "sie", "e\u00b7le\u00b7gisch", "singt", ".", "Der", "ho\u00b7he", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Blickt traurig zu der harten Erde nieder", "tokens": ["Blickt", "trau\u00b7rig", "zu", "der", "har\u00b7ten", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sucht der Freude sp\u00e4rlichen Gewinn.", "tokens": ["Und", "sucht", "der", "Freu\u00b7de", "sp\u00e4r\u00b7li\u00b7chen", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So bete ich zum Ewigen das nimmer", "tokens": ["So", "be\u00b7te", "ich", "zum", "E\u00b7wi\u00b7gen", "das", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In ihr verlischt, und weine um die Tr\u00fcmmer.", "tokens": ["In", "ihr", "ver\u00b7lischt", ",", "und", "wei\u00b7ne", "um", "die", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein reines Wesen hat mich an der Stelle", "tokens": ["Ein", "rei\u00b7nes", "We\u00b7sen", "hat", "mich", "an", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo es mich liebend stille anger\u00fchrt", "tokens": ["Wo", "es", "mich", "lie\u00b7bend", "stil\u00b7le", "an\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "ADJA", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Heiligkeit erf\u00fcllt und zarter Helle.", "tokens": ["Mit", "Hei\u00b7lig\u00b7keit", "er\u00b7f\u00fcllt", "und", "zar\u00b7ter", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles, was das Leben zu mir f\u00fchrt", "tokens": ["Und", "al\u00b7les", ",", "was", "das", "Le\u00b7ben", "zu", "mir", "f\u00fchrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird wilde Woge, in der sanften Quelle", "tokens": ["Wird", "wil\u00b7de", "Wo\u00b7ge", ",", "in", "der", "sanf\u00b7ten", "Quel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sich wie \u00c4hnlichkeit in mir verliert.", "tokens": ["Die", "sich", "wie", "\u00c4hn\u00b7lich\u00b7keit", "in", "mir", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOKOM", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich seh' im Quell die Sterne spiegelnd beben,", "tokens": ["Ich", "seh'", "im", "Quell", "die", "Ster\u00b7ne", "spie\u00b7gelnd", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Spiegel aber wellenabw\u00e4rts streben.", "tokens": ["Den", "Spie\u00b7gel", "a\u00b7ber", "wel\u00b7len\u00b7ab\u00b7w\u00e4rts", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es spricht die kalte Sch\u00f6nheit auch aus Dir", "tokens": ["Es", "spricht", "die", "kal\u00b7te", "Sch\u00f6n\u00b7heit", "auch", "aus", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die nichts erzeugt, als ihren eignen Willen", "tokens": ["Die", "nichts", "er\u00b7zeugt", ",", "als", "ih\u00b7ren", "eig\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVPP", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sch\u00f6n zu sein, und jeder beuget ihr", "tokens": ["So", "sch\u00f6n", "zu", "sein", ",", "und", "je\u00b7der", "beu\u00b7get", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$,", "KON", "PIS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den eignen Sinn, ihn mit ihr anzuf\u00fcllen.", "tokens": ["Den", "eig\u00b7nen", "Sinn", ",", "ihn", "mit", "ihr", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie wandelt ewig sich nur schaffend hier.", "tokens": ["Sie", "wan\u00b7delt", "e\u00b7wig", "sich", "nur", "schaf\u00b7fend", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nie kann sie die fremde Sehnsucht stillen.", "tokens": ["Und", "nie", "kann", "sie", "die", "frem\u00b7de", "Sehn\u00b7sucht", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie blickt in sich sich selbst so sch\u00f6n erbauet,", "tokens": ["Sie", "blickt", "in", "sich", "sich", "selbst", "so", "sch\u00f6n", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "PRF", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn sie erlischt wenn sie ins Leben schauet.", "tokens": ["Denn", "sie", "er\u00b7lischt", "wenn", "sie", "ins", "Le\u00b7ben", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}