{"dta.poem.10334": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Warte, warte, wilder Schiffmann,", "tokens": ["War\u00b7te", ",", "war\u00b7te", ",", "wil\u00b7der", "Schiff\u00b7mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich folg' ich zum Hafen dir;", "tokens": ["Gleich", "fol\u00b7g'", "ich", "zum", "Ha\u00b7fen", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von zwei Jungfraun nehm' ich Abschied,", "tokens": ["Von", "zwei", "Jung\u00b7fraun", "nehm'", "ich", "Ab\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Europa und von Ihr.", "tokens": ["Von", "Eu\u00b7ro\u00b7pa", "und", "von", "Ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blutquell, rinn' aus meinen Augen,", "tokens": ["Blut\u00b7quell", ",", "rinn'", "aus", "mei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blutquell, brich aus meinem Leib,", "tokens": ["Blut\u00b7quell", ",", "brich", "aus", "mei\u00b7nem", "Leib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich mit dem hei\u00dfen Blute", "tokens": ["Da\u00df", "ich", "mit", "dem", "hei\u00b7\u00dfen", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Meine Schmerzen niederschreib'.", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen", "nie\u00b7der\u00b7schreib'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ei, mein Lieb, warum just heute", "tokens": ["Ei", ",", "mein", "Lieb", ",", "wa\u00b7rum", "just", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauderst du, mein Blut zu sehn?", "tokens": ["Schau\u00b7derst", "du", ",", "mein", "Blut", "zu", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahst mich bleich und herzeblutend", "tokens": ["Sahst", "mich", "bleich", "und", "her\u00b7ze\u00b7blu\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange Jahre vor dir stehn!", "tokens": ["Lan\u00b7ge", "Jah\u00b7re", "vor", "dir", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kennst du noch das alte Liedchen", "tokens": ["Kennst", "du", "noch", "das", "al\u00b7te", "Lied\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Schlang im Paradies,", "tokens": ["Von", "der", "Schlang", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die durch schlimme Apfelgabe", "tokens": ["Die", "durch", "schlim\u00b7me", "Ap\u00b7fel\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern Ahn in's Elend stie\u00df?", "tokens": ["Un\u00b7sern", "Ahn", "in's", "E\u00b7lend", "stie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alles Unheil brachten Aepfel!", "tokens": ["Al\u00b7les", "Un\u00b7heil", "brach\u00b7ten", "A\u00b7e\u00b7pfel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Eva bracht' damit den Tod,", "tokens": ["E\u00b7va", "bracht'", "da\u00b7mit", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eris brachte Trojas Flammen,", "tokens": ["E\u00b7ris", "brach\u00b7te", "Tro\u00b7jas", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bracht'st beides, Flamm' und Tod.", "tokens": ["Du", "bracht'st", "bei\u00b7des", ",", "Flamm'", "und", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "NN", "KON", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}