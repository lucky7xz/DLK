{"dta.poem.13473": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zigeunerin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Am Kreuzweg, da lausche ich, wenn die Stern'", "tokens": ["Am", "Kreuz\u00b7weg", ",", "da", "lau\u00b7sche", "ich", ",", "wenn", "die", "Stern'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Feuer im Walde verglommen,", "tokens": ["Und", "die", "Feu\u00b7er", "im", "Wal\u00b7de", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und wo der erste Hund bellt von fern,", "tokens": ["Und", "wo", "der", "ers\u00b7te", "Hund", "bellt", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da wird mein Br\u00e4ut'gam herkommen.", "tokens": ["Da", "wird", "mein", "Br\u00e4ut'\u00b7gam", "her\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eund als der Tag graut' durch das Geh\u00f6lz,", "tokens": ["\u201e", "und", "als", "der", "Tag", "graut'", "durch", "das", "Ge\u00b7h\u00f6lz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sah ich eine Katze sich schlingen,", "tokens": ["Sah", "ich", "ei\u00b7ne", "Kat\u00b7ze", "sich", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ich scho\u00df ihr auf den nu\u00dfbraunen Pelz,", "tokens": ["Ich", "scho\u00df", "ihr", "auf", "den", "nu\u00df\u00b7brau\u00b7nen", "Pelz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die macht' einmal weite Spr\u00fcnge!\u201c \u2014", "tokens": ["Die", "macht'", "ein\u00b7mal", "wei\u00b7te", "Spr\u00fcn\u00b7ge", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "'s ist Schad' nur um's Pelzlein, du kriegst mich nit!", "tokens": ["'", "s", "ist", "Schad", "'", "nur", "um's", "Pelz\u00b7lein", ",", "du", "kriegst", "mich", "nit", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NE", "$(", "ADV", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mein Schatz mu\u00df seyn wie die andern:", "tokens": ["Mein", "Schatz", "mu\u00df", "seyn", "wie", "die", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VAINF", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Braun und ein Stutzbart auf ungrischen Schnitt", "tokens": ["Braun", "und", "ein", "Stutz\u00b7bart", "auf", "un\u00b7gri\u00b7schen", "Schnitt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und ein fr\u00f6hliches Herze zum Wandern.", "tokens": ["Und", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Her\u00b7ze", "zum", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}