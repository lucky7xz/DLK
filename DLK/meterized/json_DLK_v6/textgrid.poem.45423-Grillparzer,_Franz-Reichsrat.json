{"textgrid.poem.45423": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Reichsrat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu k\u00fcnftgen Ministern Erlesene", "tokens": ["Zu", "k\u00fcnft\u00b7gen", "Mi\u00b7nis\u00b7tern", "Er\u00b7le\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Minister bereits Gewesene", "tokens": ["Und", "Mi\u00b7nis\u00b7ter", "be\u00b7reits", "Ge\u00b7we\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "NN"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Finden sich hier im Quantum;", "tokens": ["Fin\u00b7den", "sich", "hier", "im", "Quan\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ein Ort, der halb Elysium", "tokens": ["Ein", "Ort", ",", "der", "halb", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und halb auch limbus infantum.", "tokens": ["Und", "halb", "auch", "lim\u00b7bus", "in\u00b7fan\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "FM", "FM", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}