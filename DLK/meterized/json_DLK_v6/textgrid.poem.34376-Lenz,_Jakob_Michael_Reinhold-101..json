{"textgrid.poem.34376": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Hochzeit zweier T\u00e4ubgen,", "tokens": ["Zur", "Hoch\u00b7zeit", "zwei\u00b7er", "T\u00e4ub\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von jeher Mann und Weibgen,", "tokens": ["Von", "je\u00b7her", "Mann", "und", "Weib\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die nicht sich auserkohren,", "tokens": ["Die", "nicht", "sich", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nur sich nicht verloren,", "tokens": ["Die", "nur", "sich", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll Euer Liebden Gnaden", "tokens": ["Soll", "Eu\u00b7er", "Lieb\u00b7den", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In aller Ehrfurcht laden,", "tokens": ["In", "al\u00b7ler", "Ehr\u00b7furcht", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In ihrem Namen zwar", "tokens": ["In", "ih\u00b7rem", "Na\u00b7men", "zwar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Hochzeitsbitter Paar.", "tokens": ["Der", "Hoch\u00b7zeits\u00b7bit\u00b7ter", "Paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wer Herz hat froh zu sein", "tokens": ["Wer", "Herz", "hat", "froh", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wo treue Liebe thronet", "tokens": ["Wo", "treu\u00b7e", "Lie\u00b7be", "thro\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In vollem Sonnenschein,", "tokens": ["In", "vol\u00b7lem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wens st\u00e4rket, wens belohnet,", "tokens": ["Wens", "st\u00e4r\u00b7ket", ",", "wens", "be\u00b7loh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der trete froh herein;", "tokens": ["Der", "tre\u00b7te", "froh", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Versuch es, mit zu schw\u00e4rmen", "tokens": ["Ver\u00b7such", "es", ",", "mit", "zu", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "APPR", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und f\u00fchlt er eignen Schmerz,", "tokens": ["Und", "f\u00fchlt", "er", "eig\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "An ihrem Gl\u00fcck zu w\u00e4rmen", "tokens": ["An", "ih\u00b7rem", "Gl\u00fcck", "zu", "w\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Sein Schweitzerliches Herz.", "tokens": ["Sein", "Schweit\u00b7zer\u00b7li\u00b7ches", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Exempel nur genommen,", "tokens": ["Ex\u00b7em\u00b7pel", "nur", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Es wird an ihn auch kommen,", "tokens": ["Es", "wird", "an", "ihn", "auch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Welt ist rund und weit,", "tokens": ["Die", "Welt", "ist", "rund", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Hat jeder seine Zeit.", "tokens": ["Hat", "je\u00b7der", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Es kann durch langes Trauren", "tokens": ["Es", "kann", "durch", "lan\u00b7ges", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Leicht unser Herz versauren,", "tokens": ["Leicht", "un\u00b7ser", "Herz", "ver\u00b7sau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und wenn wir uns zerstreun,", "tokens": ["Und", "wenn", "wir", "uns", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Ist doch die Lust nicht rein.", "tokens": ["Ist", "doch", "die", "Lust", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "O! der ist proskribiret", "tokens": ["O", "!", "der", "ist", "pros\u00b7kri\u00b7bi\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "VAFIN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.27": {"text": "Wen fremdes Gl\u00fcck nicht r\u00fchret,", "tokens": ["Wen", "frem\u00b7des", "Gl\u00fcck", "nicht", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Der kann es selbst nie sein,", "tokens": ["Der", "kann", "es", "selbst", "nie", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Kein Thier freut sich allein.", "tokens": ["Kein", "Thier", "freut", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Es m\u00fc\u00dft denn sein von Thieren \u2013", "tokens": ["Es", "m\u00fc\u00dft", "denn", "sein", "von", "Thie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Doch wo komm' ich hinein", "tokens": ["Doch", "wo", "komm'", "ich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Mit meinem Pourparliren?", "tokens": ["Mit", "mei\u00b7nem", "Pour\u00b7par\u00b7li\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Wollt Euch nur demonstriren,", "tokens": ["Wollt", "Euch", "nur", "de\u00b7monst\u00b7ri\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.34": {"text": "Da\u00df wenn sich zwey geniren", "tokens": ["Da\u00df", "wenn", "sich", "zwey", "ge\u00b7ni\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PRF", "CARD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und jeden invitiren", "tokens": ["Und", "je\u00b7den", "in\u00b7vi\u00b7ti\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.36": {"text": "Vor ihnen zu scharmiren,", "tokens": ["Vor", "ih\u00b7nen", "zu", "schar\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.37": {"text": "Ihr nicht d\u00fcrft sagen: Nein!", "tokens": ["Ihr", "nicht", "d\u00fcrft", "sa\u00b7gen", ":", "Nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "VMFIN", "VVINF", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}