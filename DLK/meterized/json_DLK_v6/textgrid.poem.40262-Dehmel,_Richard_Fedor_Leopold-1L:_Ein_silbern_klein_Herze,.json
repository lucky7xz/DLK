{"textgrid.poem.40262": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein silbern klein Herze,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein silbern klein Herze,", "tokens": ["Ein", "sil\u00b7bern", "klein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "von Golde ein'n Ring,", "tokens": ["von", "Gol\u00b7de", "ein'n", "Ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "die gab sie mir, als ich", "tokens": ["die", "gab", "sie", "mir", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wandern ging,", "tokens": ["wan\u00b7dern", "ging", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "und that in das Herze", "tokens": ["und", "that", "in", "das", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ihr Bild hinein;", "tokens": ["ihr", "Bild", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so einsam der Morgen,", "tokens": ["so", "ein\u00b7sam", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "bin doch nicht allein ...", "tokens": ["bin", "doch", "nicht", "al\u00b7lein", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Arme Padde im Gleise,", "tokens": ["Ar\u00b7me", "Pad\u00b7de", "im", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "zerquetscht liegst du.", "tokens": ["zer\u00b7quetscht", "liegst", "du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wandre meine Stra\u00dfe", "tokens": ["Ich", "wand\u00b7re", "mei\u00b7ne", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und wandre immer zu.", "tokens": ["und", "wand\u00b7re", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon teilt sich der Nebel", "tokens": ["Schon", "teilt", "sich", "der", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "und schimmert die Welt,", "tokens": ["und", "schim\u00b7mert", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "im Sonnenschein glitzert", "tokens": ["im", "Son\u00b7nen\u00b7schein", "glit\u00b7zert"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "das Aehrenfeld;", "tokens": ["das", "A\u00b7eh\u00b7ren\u00b7feld", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "die Hummeln summen,", "tokens": ["die", "Hum\u00b7meln", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Lerchen klingen;", "tokens": ["die", "Ler\u00b7chen", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "die Birken wehen,", "tokens": ["die", "Bir\u00b7ken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "die Zweige schwingen;", "tokens": ["die", "Zwei\u00b7ge", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "die Pappeln, die sch\u00fctteln", "tokens": ["die", "Pap\u00b7peln", ",", "die", "sch\u00fct\u00b7teln"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die Bl\u00e4tter im Wind;", "tokens": ["die", "Bl\u00e4t\u00b7ter", "im", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "sie fl\u00fcstern, sie singen", "tokens": ["sie", "fl\u00fcs\u00b7tern", ",", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "von meinem fernen Kind.", "tokens": ["von", "mei\u00b7nem", "fer\u00b7nen", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Herzelein nehm'ich", "tokens": ["Das", "Her\u00b7ze\u00b7lein", "nehm'\u00b7ich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "vom seidenen Band", "tokens": ["vom", "sei\u00b7de\u00b7nen", "Band"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "und leg's in das Ringlein", "tokens": ["und", "leg's", "in", "das", "Rin\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in meiner Hand,", "tokens": ["in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "so streit ich und schau", "tokens": ["so", "streit", "ich", "und", "schau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "als ein Zeichen mir's an:", "tokens": ["als", "ein", "Zei\u00b7chen", "mir's", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "so halt ich in Treuen", "tokens": ["so", "halt", "ich", "in", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "ohn Ende Dich umfahn ...", "tokens": ["ohn", "En\u00b7de", "Dich", "um\u00b7fahn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Was rennst, Meister Lampe?", "tokens": ["Was", "rennst", ",", "Meis\u00b7ter", "Lam\u00b7pe", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "heut jag'ich nicht.", "tokens": ["heut", "jag'\u00b7ich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wandre, ich schreite;", "tokens": ["Ich", "wand\u00b7re", ",", "ich", "schrei\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Sonne sticht.", "tokens": ["die", "Son\u00b7ne", "sticht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "In Dorfes Mitten", "tokens": ["In", "Dor\u00b7fes", "Mit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der Friedhof sich hebt;", "tokens": ["der", "Fried\u00b7hof", "sich", "hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "wie wird's gar k\u00fchl sich ruhen,", "tokens": ["wie", "wird's", "gar", "k\u00fchl", "sich", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wenn man mich einst begr\u00e4bt!", "tokens": ["wenn", "man", "mich", "einst", "be\u00b7gr\u00e4bt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "zwei wei\u00dfe Rosen biegen", "tokens": ["zwei", "wei\u00b7\u00dfe", "Ro\u00b7sen", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ums Grabkreuz die Aest,", "tokens": ["ums", "Grab\u00b7kreuz", "die", "A\u00b7est", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "drauf steht mein Nam geschrieben;", "tokens": ["drauf", "steht", "mein", "Nam", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "bis der Regen ihn l\u00f6scht ...", "tokens": ["bis", "der", "Re\u00b7gen", "ihn", "l\u00f6scht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Hinterm Kirchlein die Schenke", "tokens": ["Hin\u00b7term", "Kirch\u00b7lein", "die", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "hei\u00dft \u00bbZu den 3 Linden\u00ab;", "tokens": ["hei\u00dft", "\u00bb", "Zu", "den", "3", "Lin\u00b7den", "\u00ab", ";"], "token_info": ["word", "punct", "word", "word", "number", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "APPR", "ART", "CARD", "NE", "$(", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "da wird ein Ruhepl\u00e4tzchen", "tokens": ["da", "wird", "ein", "Ru\u00b7he\u00b7pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sich auch wol noch finden!", "tokens": ["sich", "auch", "wol", "noch", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Ei Tausend, mein Sch\u00e4tzchen:", "tokens": ["Ei", "Tau\u00b7send", ",", "mein", "Sch\u00e4tz\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "so schmuck, und allein?", "tokens": ["so", "schmuck", ",", "und", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ei komm doch, r\u00fcck n\u00e4her;", "tokens": ["Ei", "komm", "doch", ",", "r\u00fcck", "n\u00e4\u00b7her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "trink aus, schenk ein! \u2013", "tokens": ["trink", "aus", ",", "schenk", "ein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Na Sch\u00e4tzel, was weinst denn?", "tokens": ["Na", "Sch\u00e4t\u00b7zel", ",", "was", "weinst", "denn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ja, die Welt ist hohl,", "tokens": ["Ja", ",", "die", "Welt", "ist", "hohl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "die Welt ist ne Flasche:", "tokens": ["die", "Welt", "ist", "ne", "Fla\u00b7sche", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "trink aus! leb wohl! \u2013", "tokens": ["trink", "aus", "!", "leb", "wohl", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Was wackelt der Pfahl da?", "tokens": ["Was", "wa\u00b7ckelt", "der", "Pfahl", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "der ist wol betrunken!", "tokens": ["der", "ist", "wol", "be\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich wandre, ich schreite,", "tokens": ["Ich", "wand\u00b7re", ",", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in Sinnen versunken.", "tokens": ["in", "Sin\u00b7nen", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Wir war'n ja so alleine;", "tokens": ["Wir", "wa\u00b7r'n", "ja", "so", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "und sie, sie so weit!", "tokens": ["und", "sie", ",", "sie", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "ich will ihr Alles sagen,", "tokens": ["ich", "will", "ihr", "Al\u00b7les", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "bis sie mir verzeiht ...", "tokens": ["bis", "sie", "mir", "ver\u00b7zeiht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Und am End meiner Reise", "tokens": ["Und", "am", "End", "mei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "steht mein elterlich Haus,", "tokens": ["steht", "mein", "el\u00b7ter\u00b7lich", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "da schaut mein lieb M\u00fctterchen", "tokens": ["da", "schaut", "mein", "lieb", "M\u