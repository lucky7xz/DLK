rden;", "tokens": ["ob", "man", "gl\u00fcck\u00b7lich", "kann", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "der Himmel ist hoch,", "tokens": ["der", "Him\u00b7mel", "ist", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "und wir leben auf Erden \u2013", "tokens": ["und", "wir", "le\u00b7ben", "auf", "Er\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "schrumm!", "tokens": ["schrumm", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}}}}}