{"textgrid.poem.34467": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie wandelten zum Feste gen Jerusalem", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wandelten zum Feste gen Jerusalem", "tokens": ["Sie", "wan\u00b7del\u00b7ten", "zum", "Fes\u00b7te", "gen", "Je\u00b7ru\u00b7sa\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NE"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und kamen in ein Dorf, das heisst Bethania.", "tokens": ["und", "ka\u00b7men", "in", "ein", "Dorf", ",", "das", "heisst", "Be\u00b7tha\u00b7nia", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da war ein Weib mit Namen Martha, dieses nahm", "tokens": ["Da", "war", "ein", "Weib", "mit", "Na\u00b7men", "Mar\u00b7tha", ",", "die\u00b7ses", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "NE", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Herren auf und pflegte vielgesch\u00e4ftig sein.", "tokens": ["den", "Her\u00b7ren", "auf", "und", "pfleg\u00b7te", "viel\u00b7ge\u00b7sch\u00e4f\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und eine junge Schwester hatte sie, die hiess", "tokens": ["Und", "ei\u00b7ne", "jun\u00b7ge", "Schwes\u00b7ter", "hat\u00b7te", "sie", ",", "die", "hiess"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Maria. \u2013 Da der Herr auf seinem Lager nun", "tokens": ["Ma\u00b7ria", ".", "\u2013", "Da", "der", "Herr", "auf", "sei\u00b7nem", "La\u00b7ger", "nun"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "$(", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "am Tische ruhte, setzte sich Maria still", "tokens": ["am", "Ti\u00b7sche", "ruh\u00b7te", ",", "setz\u00b7te", "sich", "Ma\u00b7ria", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "NE", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "zu seinen F\u00fcssen nieder: voller Innigkeit", "tokens": ["zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "nie\u00b7der", ":", "vol\u00b7ler", "In\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und Andacht lauschte seinem tiefen Worte sie.", "tokens": ["und", "An\u00b7dacht", "lauschte", "sei\u00b7nem", "tie\u00b7fen", "Wor\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wie sie also dasass, sich vergass und nur", "tokens": ["Und", "wie", "sie", "al\u00b7so", "da\u00b7sass", ",", "sich", "ver\u00b7gass", "und", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "PDS", "$,", "PRF", "VVFIN", "KON", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "an seinen Lippen hangend ihm ins Auge sah,", "tokens": ["an", "sei\u00b7nen", "Lip\u00b7pen", "han\u00b7gend", "ihm", "ins", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "trat ihre Schwester, die dazwischen immerfort,", "tokens": ["trat", "ih\u00b7re", "Schwes\u00b7ter", ",", "die", "da\u00b7zwi\u00b7schen", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "f\u00fcr ihren Herrn besorgt, sich hatte abgem\u00fcht,", "tokens": ["f\u00fcr", "ih\u00b7ren", "Herrn", "be\u00b7sorgt", ",", "sich", "hat\u00b7te", "ab\u00b7ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PRF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "sie trat hinzu und sprach: Herr, fragst du nicht danach,", "tokens": ["sie", "trat", "hin\u00b7zu", "und", "sprach", ":", "Herr", ",", "fragst", "du", "nicht", "da\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dass meine Schwester mich allein dir dienen l\u00e4sst?", "tokens": ["dass", "mei\u00b7ne", "Schwes\u00b7ter", "mich", "al\u00b7lein", "dir", "die\u00b7nen", "l\u00e4sst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist es der Weiber Amt und eigne Sorge doch,", "tokens": ["Ist", "es", "der", "Wei\u00b7ber", "Amt", "und", "eig\u00b7ne", "Sor\u00b7ge", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu schaffen emsiglich und treu des Hauses Werk:", "tokens": ["zu", "schaf\u00b7fen", "em\u00b7sig\u00b7lich", "und", "treu", "des", "Hau\u00b7ses", "Werk", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und jene h\u00f6rt auf deine Worte wie ein Mann", "tokens": ["und", "je\u00b7ne", "h\u00f6rt", "auf", "dei\u00b7ne", "Wor\u00b7te", "wie", "ein", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und denket nicht, was ihr allein geziemen mag.", "tokens": ["und", "den\u00b7ket", "nicht", ",", "was", "ihr", "al\u00b7lein", "ge\u00b7zie\u00b7men", "mag."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VVPP", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So sage denn, dass sie mit mir angreife nun", "tokens": ["So", "sa\u00b7ge", "denn", ",", "dass", "sie", "mit", "mir", "an\u00b7grei\u00b7fe", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und, gleich wie ich, sich m\u00fche treu in deinem Dienst! \u2013", "tokens": ["und", ",", "gleich", "wie", "ich", ",", "sich", "m\u00fc\u00b7he", "treu", "in", "dei\u00b7nem", "Dienst", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "ADV", "KOKOM", "PPER", "$,", "PRF", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch Jesus sprach zu jener: Martha, Martha! ", "tokens": ["Doch", "Je\u00b7sus", "sprach", "zu", "je\u00b7ner", ":", "Mar\u00b7tha", ",", "Mar\u00b7tha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "PDAT", "$.", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "hast viele Sorg und M\u00fche \u2013 aber ", "tokens": ["hast", "vie\u00b7le", "Sorg", "und", "M\u00fc\u00b7he", "\u2013", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "$(", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Maria hat das gute Theil erw\u00e4hlt, das soll", "tokens": ["Ma\u00b7ria", "hat", "das", "gu\u00b7te", "Theil", "er\u00b7w\u00e4hlt", ",", "das", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,", "PDS", "VMFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "ihr nicht genommen werden. Martha, glaube mir:", "tokens": ["ihr", "nicht", "ge\u00b7nom\u00b7men", "wer\u00b7den", ".", "Mar\u00b7tha", ",", "glau\u00b7be", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAINF", "$.", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "auch dir und deinesgleichen soll es werden einst.", "tokens": ["auch", "dir", "und", "dei\u00b7nes\u00b7glei\u00b7chen", "soll", "es", "wer\u00b7den", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "VVINF", "VMFIN", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}