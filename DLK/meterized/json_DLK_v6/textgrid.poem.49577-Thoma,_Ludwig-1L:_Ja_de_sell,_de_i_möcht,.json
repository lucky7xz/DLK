{"textgrid.poem.49577": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja de sell, de i m\u00f6cht,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja de sell, de i m\u00f6cht,", "tokens": ["Ja", "de", "sell", ",", "de", "i", "m\u00f6cht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "ADJD", "$,", "NE", "NE", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Waar ma heut no ganz g'recht,", "tokens": ["Waar", "ma", "heut", "no", "ganz", "g'\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "NE", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aba d\u00f6s sell is a Frag,", "tokens": ["A\u00b7ba", "d\u00f6s", "sell", "is", "a", "Frag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob i 's moring, ja moring no mag.", "tokens": ["Ob", "i", "'s", "mo\u00b7ring", ",", "ja", "mo\u00b7ring", "no", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$,", "ADV", "NE", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mua\u00dft a 's flei\u00dfi oschaug'n,", "tokens": ["Mu\u00b7a\u00dft", "a", "'s", "flei\u00b7\u00dfi", "o\u00b7schaug'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Kunnt da leicht was net taug'n,", "tokens": ["Kunnt", "da", "leicht", "was", "net", "taug'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "FM", "FM", "FM", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kunnt di leicht was schiniern \u2013 \u2013 \u2013", "tokens": ["Kunnt", "di", "leicht", "was", "schi\u00b7ni\u00b7ern", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "NE", "ADJD", "PIS", "VVINF", "$(", "$(", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Drum mua\u00df ma 's Diandl, ja 's Diandl probiern.", "tokens": ["Drum", "mu\u00b7a\u00df", "ma", "'s", "Di\u00b7andl", ",", "ja", "'s", "Di\u00b7andl", "pro\u00b7biern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "PPER", "NE", "$,", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja a so mua\u00df ma lebn!", "tokens": ["Ja", "a", "so", "mu\u00b7a\u00df", "ma", "lebn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "An jed'n Deckel aufhebn,", "tokens": ["An", "je\u00b7d'n", "De\u00b7ckel", "auf\u00b7hebn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "A jede Sch\u00fcssel neigspecht,", "tokens": ["A", "je\u00b7de", "Sch\u00fcs\u00b7sel", "neig\u00b7specht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja wann ma 's allabest-allabest m\u00f6cht.", "tokens": ["Ja", "wann", "ma", "'s", "all\u00b7abest\u00b7all\u00b7abest", "m\u00f6cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "NE", "PPER", "ADV", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}