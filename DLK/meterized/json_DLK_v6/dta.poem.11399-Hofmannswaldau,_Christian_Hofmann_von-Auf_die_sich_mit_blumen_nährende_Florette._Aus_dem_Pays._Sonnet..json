{"dta.poem.11399": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die sich mit blumen n\u00e4hrende Florette.  \n Aus dem  Pays.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florette! dein geschmack ist ziemlich ungemein:", "tokens": ["Flo\u00b7ret\u00b7te", "!", "dein", "ge\u00b7schmack", "ist", "ziem\u00b7lich", "un\u00b7ge\u00b7mein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wilst, man solle dir sonst nichts als blumen geben,", "tokens": ["Du", "wilst", ",", "man", "sol\u00b7le", "dir", "sonst", "nichts", "als", "blu\u00b7men", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PIS", "VMFIN", "PPER", "ADV", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht die lilien offt an den perlen kleben,", "tokens": ["Man", "sieht", "die", "li\u00b7li\u00b7en", "offt", "an", "den", "per\u00b7len", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo deine z\u00e4hne nicht noch mehr als perlen seyn.", "tokens": ["Wo", "dei\u00b7ne", "z\u00e4h\u00b7ne", "nicht", "noch", "mehr", "als", "per\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "PTKNEG", "ADV", "PIAT", "KOKOM", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der rosen-lichte mund nimmt lauter rosen ein:", "tokens": ["Der", "ro\u00b7sen\u00b7lich\u00b7te", "mund", "nimmt", "lau\u00b7ter", "ro\u00b7sen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sch\u00f6nsten nelcke tod ist deiner zunge leben:", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "nel\u00b7cke", "tod", "ist", "dei\u00b7ner", "zun\u00b7ge", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die veilge kan sich kaum aus ihrer wurtzel heben,", "tokens": ["Die", "veil\u00b7ge", "kan", "sich", "kaum", "aus", "ih\u00b7rer", "wurt\u00b7zel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So legt dein hunger ihr schon einen leichen-stein.", "tokens": ["So", "legt", "dein", "hun\u00b7ger", "ihr", "schon", "ei\u00b7nen", "lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein bedenckst du auch, was dir der winter dr\u00e4uet,", "tokens": ["Al\u00b7lein", "be\u00b7denckst", "du", "auch", ",", "was", "dir", "der", "win\u00b7ter", "dr\u00e4u\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der aller g\u00e4rten pracht in seinem grimm zerstreuet,", "tokens": ["Der", "al\u00b7ler", "g\u00e4r\u00b7ten", "pracht", "in", "sei\u00b7nem", "grimm", "zer\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bi\u00df auf den blumen-schmuck, den deine sch\u00f6nheit weist?", "tokens": ["Bi\u00df", "auf", "den", "blu\u00b7men\u00b7schmuck", ",", "den", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "weist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Florett\u2019! entwehne dich dergleichen lecker-bissen;", "tokens": ["Flo\u00b7rett'", "!", "ent\u00b7weh\u00b7ne", "dich", "derg\u00b7lei\u00b7chen", "le\u00b7cke\u00b7rbis\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sonst wirst du, wenn die zeit dir diese kost entreist,", "tokens": ["Sonst", "wirst", "du", ",", "wenn", "die", "zeit", "dir", "die\u00b7se", "kost", "en\u00b7treist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus harter hungers-noth dich selbst verzehren m\u00fcssen.", "tokens": ["Aus", "har\u00b7ter", "hun\u00b7ger\u00b7snoth", "dich", "selbst", "ver\u00b7zeh\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}