{"dta.poem.13122": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier fieng ein Lispeln an die Reihen durch zu schleichen,", "tokens": ["Hier", "fi\u00b7eng", "ein", "Lis\u00b7peln", "an", "die", "Rei\u00b7hen", "durch", "zu", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie wann man Bienen sieht durch einen Garten streichen", "tokens": ["Wie", "wann", "man", "Bie\u00b7nen", "sieht", "durch", "ei\u00b7nen", "Gar\u00b7ten", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PWAV", "PIS", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und h\u00f6ret ihr Gesaus, ihr summendes Gemurr;", "tokens": ["Und", "h\u00f6\u00b7ret", "ihr", "Ge\u00b7saus", ",", "ihr", "sum\u00b7men\u00b7des", "Ge\u00b7murr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die\u00df war was man darauf, sonst weiter nichts, erfuhr.", "tokens": ["Die\u00df", "war", "was", "man", "da\u00b7rauf", ",", "sonst", "wei\u00b7ter", "nichts", ",", "er\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PWS", "PIS", "PAV", "$,", "ADV", "ADV", "PIS", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In diesen Regungen, in dem Gespr\u00e4ch und Zischen", "tokens": ["In", "die\u00b7sen", "Re\u00b7gun\u00b7gen", ",", "in", "dem", "Ge\u00b7spr\u00e4ch", "und", "Zi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "590Stund wieder jemand auf, und redte laut dazwischen:", "tokens": ["wie\u00b7der", "je\u00b7mand", "auf", ",", "und", "red\u00b7te", "laut", "da\u00b7zwi\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}