{"textgrid.poem.45348": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Reaktion", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcr ein gr\u00e4ulich schlechtes Wetter,", "tokens": ["Was", "f\u00fcr", "ein", "gr\u00e4u\u00b7lich", "schlech\u00b7tes", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sumpf die Stra\u00dfe bis ans Haus!", "tokens": ["Ein", "Sumpf", "die", "Stra\u00b7\u00dfe", "bis", "ans", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ben\u00fctzen wir die Gelegenheit, Herr Vetter,", "tokens": ["Be\u00b7n\u00fct\u00b7zen", "wir", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "Herr", "Vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und gie\u00dfen auch unsern Nachttopf aus.", "tokens": ["Und", "gie\u00b7\u00dfen", "auch", "un\u00b7sern", "Nacht\u00b7topf", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}