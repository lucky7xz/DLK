{"textgrid.poem.58642": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du mich verworfen, Herr? Soll ich denn im Schmerz vergehen?", "tokens": ["Hast", "du", "mich", "ver\u00b7wor\u00b7fen", ",", "Herr", "?", "Soll", "ich", "denn", "im", "Schmerz", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,", "NN", "$.", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bin ich dir zum Ekel nun? Willst du mich nicht mehr ansehen?", "tokens": ["Bin", "ich", "dir", "zum", "E\u00b7kel", "nun", "?", "Willst", "du", "mich", "nicht", "mehr", "an\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach, mein Freund, schl\u00e4gst du mich selbst, du, mein Heiland, heil mich dann!", "tokens": ["Ach", ",", "mein", "Freund", ",", "schl\u00e4gst", "du", "mich", "selbst", ",", "du", ",", "mein", "Hei\u00b7land", ",", "heil", "mich", "dann", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "D\u00fcrres Zion, merke doch, Gott verwirft nur deine S\u00fcnden,", "tokens": ["D\u00fcr\u00b7res", "Zi\u00b7on", ",", "mer\u00b7ke", "doch", ",", "Gott", "ver\u00b7wirft", "nur", "dei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "$,", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Er ist feind der Eigenheit, die in deinem Grund zu finden;", "tokens": ["Er", "ist", "feind", "der", "Ei\u00b7gen\u00b7heit", ",", "die", "in", "dei\u00b7nem", "Grund", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Hoffe hin und hoffe her, glaub's, dein Geist nicht Frieden find't,", "tokens": ["Hof\u00b7fe", "hin", "und", "hof\u00b7fe", "her", ",", "glaub's", ",", "dein", "Geist", "nicht", "Frie\u00b7den", "find't", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,", "NE", "$,", "PPOSAT", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Bis du f\u00fchlst und frei bekennst, da\u00df du seist ein b\u00f6ses Kind!", "tokens": ["Bis", "du", "f\u00fchlst", "und", "frei", "be\u00b7kennst", ",", "da\u00df", "du", "seist", "ein", "b\u00f6\u00b7ses", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}