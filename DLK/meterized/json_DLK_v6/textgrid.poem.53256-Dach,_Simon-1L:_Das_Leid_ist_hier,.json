{"textgrid.poem.53256": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Leid ist hier,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leid ist hier,", "tokens": ["Das", "Leid", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da sehen wir,", "tokens": ["Da", "se\u00b7hen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O grosser Gott,", "tokens": ["O", "gros\u00b7ser", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wenn dein Gebot", "tokens": ["Wenn", "dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nicht wird vollbracht,", "tokens": ["Nicht", "wird", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Was Krieg vnd Schlacht", "tokens": ["Was", "Krieg", "vnd", "Schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vns denn f\u00fcr grossen Jammer macht.", "tokens": ["Vns", "denn", "f\u00fcr", "gros\u00b7sen", "Jam\u00b7mer", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Feinde Heer", "tokens": ["Der", "Fein\u00b7de", "Heer"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Fleucht durch das Meer,", "tokens": ["Fleucht", "durch", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Setzt ohne Ruh'", "tokens": ["Setzt", "oh\u00b7ne", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auff vns nur zu:", "tokens": ["Auff", "vns", "nur", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie steht Ihr Muth", "tokens": ["Wie", "steht", "Ihr", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nach vnserm Gut!", "tokens": ["Nach", "vn\u00b7serm", "Gut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie d\u00fcrstet sie nach Mord vnd Blut!", "tokens": ["Wie", "d\u00fcrs\u00b7tet", "sie", "nach", "Mord", "vnd", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo sol man doch", "tokens": ["Wo", "sol", "man", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In diesem Joch", "tokens": ["In", "die\u00b7sem", "Joch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vnd Creutz' hinziehn?", "tokens": ["Vnd", "Creutz'", "hin\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wir wollen fliehn", "tokens": ["Wir", "wol\u00b7len", "fliehn"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zu Dir, O Gott;", "tokens": ["Zu", "Dir", ",", "O", "Gott", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der grossen Noht", "tokens": ["Der", "gros\u00b7sen", "Noht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Entheb' vns doch durch Christi Todt!", "tokens": ["Ent\u00b7heb'", "vns", "doch", "durch", "Chris\u00b7ti", "Todt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zeuch Du, O Held,", "tokens": ["Zeuch", "Du", ",", "O", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit vns ins Feld!", "tokens": ["Mit", "vns", "ins", "Feld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir sind zu schwach,", "tokens": ["Wir", "sind", "zu", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "F\u00fchr Du die Sach',", "tokens": ["F\u00fchr", "Du", "die", "Sach'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vnd sch\u00fctz hinfort", "tokens": ["Vnd", "sch\u00fctz", "hin\u00b7fort"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bey vns dein Wort,", "tokens": ["Bey", "vns", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sey ewig vnser Fels vnd Hort!", "tokens": ["Sey", "e\u00b7wig", "vn\u00b7ser", "Fels", "vnd", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}