{"dta.poem.5977": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein klares Glas kan unsern Augen", "tokens": ["Ein", "kla\u00b7res", "Glas", "kan", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es deutlich gnug zu zeigen taugen:", "tokens": ["Es", "deut\u00b7lich", "gnug", "zu", "zei\u00b7gen", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Wasser drinnen rund gedreht wird und gewendt,", "tokens": ["Wenn", "Was\u00b7ser", "drin\u00b7nen", "rund", "ge\u00b7dreht", "wird", "und", "ge\u00b7wendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und man von Siegel-Wachs ein Pulver darein reibt,", "tokens": ["Und", "man", "von", "Sie\u00b7gel\u00b7Wachs", "ein", "Pul\u00b7ver", "da\u00b7rein", "reibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NE", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das seine eigne Last zu Grunde treibt;", "tokens": ["Das", "sei\u00b7ne", "eig\u00b7ne", "Last", "zu", "Grun\u00b7de", "treibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wird durchs allererste Drehen,", "tokens": ["So", "wird", "durchs", "al\u00b7le\u00b7rers\u00b7te", "Dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Pulver nach dem Umkreis gehen:", "tokens": ["Das", "Pul\u00b7ver", "nach", "dem", "Um\u00b7kreis", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein dreht man dies Gla\u00df nur lange Zeit,", "tokens": ["Al\u00b7lein", "dreht", "man", "dies", "Gla\u00df", "nur", "lan\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PDS", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und stimmt des Wassers Fl\u00fc\u00dfigkeit", "tokens": ["Und", "stimmt", "des", "Was\u00b7sers", "Fl\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In diesen Circkelchen, die sich best\u00e4ndig regen,", "tokens": ["In", "die\u00b7sen", "Cir\u00b7ckel\u00b7chen", ",", "die", "sich", "be\u00b7st\u00e4n\u00b7dig", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit der Bewegung, die dann allgemein,", "tokens": ["Mit", "der", "Be\u00b7we\u00b7gung", ",", "die", "dann", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Jemehr und mehr allm\u00e4hlich \u00fcberein;", "tokens": ["Je\u00b7mehr", "und", "mehr", "all\u00b7m\u00e4h\u00b7lich", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "So setze man", "tokens": ["So", "set\u00b7ze", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Das Glas so dann", "tokens": ["Das", "Glas", "so", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Nur stille nieder:", "tokens": ["Nur", "stil\u00b7le", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "So wird das Wasser, welches sich", "tokens": ["So", "wird", "das", "Was\u00b7ser", ",", "wel\u00b7ches", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Viel fl\u00fcssiger und hefftiglich", "tokens": ["Viel", "fl\u00fcs\u00b7si\u00b7ger", "und", "heff\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Durchs Pulver jaget hin und wieder,", "tokens": ["Durchs", "Pul\u00b7ver", "ja\u00b7get", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sich immer in der Fluht bequemen", "tokens": ["Sich", "im\u00b7mer", "in", "der", "Fluht", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die erste Stelle einzunehmen;", "tokens": ["Die", "ers\u00b7te", "Stel\u00b7le", "ein\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und wird vom Mittel-Punct weit abgef\u00fchrt:", "tokens": ["Und", "wird", "vom", "Mit\u00b7tel\u00b7Punct", "weit", "ab\u00b7ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das rohte Siegel-Wachs hingegen", "tokens": ["Das", "roh\u00b7te", "Sie\u00b7gel\u00b7Wachs", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wird sich in Mittel-Punct gejagt zusammen legen;", "tokens": ["Wird", "sich", "in", "Mit\u00b7tel\u00b7Punct", "ge\u00b7jagt", "zu\u00b7sam\u00b7men", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "VVPP", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da aus den Theilchen denn, die an einander kleben,", "tokens": ["Da", "aus", "den", "Theil\u00b7chen", "denn", ",", "die", "an", "ein\u00b7an\u00b7der", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein rundes Wesen sich formirt,", "tokens": ["Ein", "run\u00b7des", "We\u00b7sen", "sich", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das eine Kugel wird, mit Wasser gantz umgeben.", "tokens": ["Das", "ei\u00b7ne", "Ku\u00b7gel", "wird", ",", "mit", "Was\u00b7ser", "gantz", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}