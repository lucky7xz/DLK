{"textgrid.poem.56729": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singen will ich den Hochgesang,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singen will ich den Hochgesang,", "tokens": ["Sin\u00b7gen", "will", "ich", "den", "Hoch\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "den mit Sterngoldlettern", "tokens": ["den", "mit", "Stern\u00b7gold\u00b7let\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "der heilige Geist der Erkenntnis", "tokens": ["der", "hei\u00b7li\u00b7ge", "Geist", "der", "Er\u00b7kennt\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "in den schwarzen Riesenschiefer", "tokens": ["in", "den", "schwar\u00b7zen", "Rie\u00b7sen\u00b7schie\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "n\u00e4chtigen Firmaments", "tokens": ["n\u00e4ch\u00b7ti\u00b7gen", "Fir\u00b7ma\u00b7ments"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "leuchtend gegraben,", "tokens": ["leuch\u00b7tend", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "den jauchzenden Hochgesang,", "tokens": ["den", "jauch\u00b7zen\u00b7den", "Hoch\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "des Kehrreim von zahllosen Ch\u00f6ren", "tokens": ["des", "Kehr\u00b7reim", "von", "zahl\u00b7lo\u00b7sen", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "von Weltengeschlechtern das All durcht\u00f6nt:", "tokens": ["von", "Wel\u00b7ten\u00b7ge\u00b7schlech\u00b7tern", "das", "All", "durch\u00b7t\u00f6nt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Auf allen Sternen ist Liebe!", "tokens": ["Auf", "al\u00b7len", "Ster\u00b7nen", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Siehe, ich ma\u00df auf dem Feuerfittich", "tokens": ["Sie\u00b7he", ",", "ich", "ma\u00df", "auf", "dem", "Feu\u00b7er\u00b7fit\u00b7tich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "rascher Kometen die Bahnen der Ewigkeit,", "tokens": ["ra\u00b7scher", "Ko\u00b7me\u00b7ten", "die", "Bah\u00b7nen", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "durch tausend Planetenreigen", "tokens": ["durch", "tau\u00b7send", "Pla\u00b7ne\u00b7ten\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "flog ich zitternden Geistes,", "tokens": ["flog", "ich", "zit\u00b7tern\u00b7den", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "sp\u00e4hte und lauschte hinab", "tokens": ["sp\u00e4h\u00b7te", "und", "lauschte", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "auf die kreisenden B\u00e4lle", "tokens": ["auf", "die", "krei\u00b7sen\u00b7den", "B\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "mit \u00fcberirdischen Sehnsuchtsinnen.", "tokens": ["mit", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7schen", "Sehn\u00b7such\u00b7tsin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und entgegen schwoll mir allewig", "tokens": ["Und", "ent\u00b7ge\u00b7gen", "schwoll", "mir", "al\u00b7le\u00b7wig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "ADJD"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "aus unz\u00e4hlbarer Lebenden Br\u00fcsten:", "tokens": ["aus", "un\u00b7z\u00e4hl\u00b7ba\u00b7rer", "Le\u00b7ben\u00b7den", "Br\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Auf allen Sternen ist Liebe!", "tokens": ["Auf", "al\u00b7len", "Ster\u00b7nen", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sahst du je ein liebendes Paar", "tokens": ["Sahst", "du", "je", "ein", "lie\u00b7ben\u00b7des", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "sich vereinen zu seligem Ku\u00df,", "tokens": ["sich", "ver\u00b7ei\u00b7nen", "zu", "se\u00b7li\u00b7gem", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "PTKZU", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "sahst du je der Mutterlippe", "tokens": ["sahst", "du", "je", "der", "Mut\u00b7ter\u00b7lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "stummes Segengebet des Kindes", "tokens": ["stum\u00b7mes", "Se\u00b7gen\u00b7ge\u00b7bet", "des", "Kin\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "reinen Scheitel inbr\u00fcnstig weihen,", "tokens": ["rei\u00b7nen", "Schei\u00b7tel", "in\u00b7br\u00fcns\u00b7tig", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "sahst du je die stille Flamme", "tokens": ["sahst", "du", "je", "die", "stil\u00b7le", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "heiliger Freundschaft im Kusse brennen \u2013", "tokens": ["hei\u00b7li\u00b7ger", "Freund\u00b7schaft", "im", "Kus\u00b7se", "bren\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "oh dann sang auch deine Seele,", "tokens": ["oh", "dann", "sang", "auch", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "stammelte schauernd die s\u00fc\u00dfe Gewi\u00dfheit:", "tokens": ["stam\u00b7mel\u00b7te", "schau\u00b7ernd", "die", "s\u00fc\u00b7\u00dfe", "Ge\u00b7wi\u00df\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Auf allen Sternen ist Liebe!", "tokens": ["Auf", "al\u00b7len", "Ster\u00b7nen", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Trunken bin ich von diesem Liede,", "tokens": ["Trun\u00b7ken", "bin", "ich", "von", "die\u00b7sem", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "das aus der Harfe der Ewigkeit hallt.", "tokens": ["das", "aus", "der", "Har\u00b7fe", "der", "E\u00b7wig\u00b7keit", "hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oh meine Br\u00fcder auf wandelnden Welten,", "tokens": ["Oh", "mei\u00b7ne", "Br\u00fc\u00b7der", "auf", "wan\u00b7deln\u00b7den", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "deren Sonnen purpurne Kr\u00e4nze", "tokens": ["de\u00b7ren", "Son\u00b7nen", "pur\u00b7pur\u00b7ne", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PRELAT", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "um die Muttersonne des Alls", "tokens": ["um", "die", "Mut\u00b7ter\u00b7son\u00b7ne", "des", "Alls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "ewigen Rhythmus' Sturmschwung rei\u00dft,", "tokens": ["e\u00b7wi\u00b7gen", "Rhyth\u00b7mus'", "Sturm\u00b7schwung", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "gr\u00fc\u00dfen la\u00dft euch durch \u00c4onen!", "tokens": ["gr\u00fc\u00b7\u00dfen", "la\u00dft", "euch", "durch", "\u00c4\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tausendgestaltiger Sterblicher Hymnen", "tokens": ["Tau\u00b7send\u00b7ge\u00b7stal\u00b7ti\u00b7ger", "Sterb\u00b7li\u00b7cher", "Hym\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Ein' ich des Menschengeschlechts Dithyrambe.", "tokens": ["Ein'", "ich", "des", "Men\u00b7schen\u00b7ge\u00b7schlechts", "Dit\u00b7hy\u00b7ram\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf allen Sternen ist Liebe!", "tokens": ["Auf", "al\u00b7len", "Ster\u00b7nen", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Liebe! Liebe! durch die Unendlichkeit", "tokens": ["Lie\u00b7be", "!", "Lie\u00b7be", "!", "durch", "die", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ausgegossen, ein Strom erl\u00f6senden Lichts,", "tokens": ["aus\u00b7ge\u00b7gos\u00b7sen", ",", "ein", "Strom", "er\u00b7l\u00f6\u00b7sen\u00b7den", "Lichts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "in das Nichts, die Nacht der Herzen", "tokens": ["in", "das", "Nichts", ",", "die", "Nacht", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deine gl\u00fchenden Wogen schlagend \u2013", "tokens": ["dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7den", "Wo\u00b7gen", "schla\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "hebend aus dem Dumpfen das Heilige \u2013", "tokens": ["he\u00b7bend", "aus", "dem", "Dum\u00b7pfen", "das", "Hei\u00b7li\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "aus dem Chaos rettend und schaffend den Gott \u2013", "tokens": ["aus", "dem", "Chaos", "ret\u00b7tend", "und", "schaf\u00b7fend", "den", "Gott", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "ADJD", "ART", "NN", "$("], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Gottheit auf die Stirn dem Menschen", "tokens": ["Got\u00b7theit", "auf", "die", "Stirn", "dem", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "pr\u00e4gend und ins schimmernde Aug ihm", "tokens": ["pr\u00e4\u00b7gend", "und", "ins", "schim\u00b7mern\u00b7de", "Aug", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPRART", "ADJA", "NN", "PPER"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Gottheit senkend \u2013 Liebe! Liebe!", "tokens": ["Got\u00b7theit", "sen\u00b7kend", "\u2013", "Lie\u00b7be", "!", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$(", "NN", "$.", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Auf allen Sternen ist Liebe!", "tokens": ["Auf", "al\u00b7len", "Ster\u00b7nen", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Liebe! Liebe! bist du die Mutter auch", "tokens": ["Lie\u00b7be", "!", "Lie\u00b7be", "!", "bist", "du", "die", "Mut\u00b7ter", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "aller Schmerzen, aller der Lebensqual,", "tokens": ["al\u00b7ler", "Schmer\u00b7zen", ",", "al\u00b7ler", "der", "Le\u00b7bens\u00b7qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "wer ertr\u00e4gt um dich nicht alles,", "tokens": ["wer", "er\u00b7tr\u00e4gt", "um", "dich", "nicht", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "PTKNEG", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "stolzen Mutes, ein Held, ein Ringer!", "tokens": ["stol\u00b7zen", "Mu\u00b7tes", ",", "ein", "Held", ",", "ein", "Rin\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Heilig sprechen wir Ha\u00df und Leid und Schuld,", "tokens": ["Hei\u00b7lig", "spre\u00b7chen", "wir", "Ha\u00df", "und", "Leid", "und", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "denn wir lassen von dir nicht, oh Liebe!", "tokens": ["denn", "wir", "las\u00b7sen", "von", "dir", "nicht", ",", "oh", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "