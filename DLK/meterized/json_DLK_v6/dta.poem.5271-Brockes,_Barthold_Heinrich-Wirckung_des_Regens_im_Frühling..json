{"dta.poem.5271": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wirckung des Regens im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach einem lang-und fcharffen Osten-Winde,", "tokens": ["Nach", "ei\u00b7nem", "lang\u00b7\u00b7und", "fcharf\u00b7fen", "Os\u00b7ten\u00b7Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der im April und M\u00e4rtz geherrschet, und das Land", "tokens": ["Der", "im", "Ap\u00b7ril", "und", "M\u00e4rtz", "ge\u00b7herr\u00b7schet", ",", "und", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "KON", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast g\u00e4ntzlich ausgedorrt, fiel auf einmahl gelinde,", "tokens": ["Fast", "g\u00e4ntz\u00b7lich", "aus\u00b7ge\u00b7dorrt", ",", "fiel", "auf", "ein\u00b7mahl", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ich von ungefehr im Garten mich befand,", "tokens": ["Wie", "ich", "von", "un\u00b7ge\u00b7fehr", "im", "Gar\u00b7ten", "mich", "be\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus S\u00fcden, ein schon l\u00e4ngst erseufzter Regen.", "tokens": ["Aus", "S\u00fc\u00b7den", ",", "ein", "schon", "l\u00e4ngst", "er\u00b7seufz\u00b7ter", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein GOtt, welch ein erw\u00fcnscht Bewegen,", "tokens": ["Mein", "Gott", ",", "welch", "ein", "er\u00b7w\u00fcnscht", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "ART", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das \u00fcberall der Erden Fl\u00e4che ziert,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", "der", "Er\u00b7den", "Fl\u00e4\u00b7che", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ward dadurch \u00fcberall versp\u00fcrt!", "tokens": ["Ward", "da\u00b7durch", "\u00fc\u00b7be\u00b7rall", "ver\u00b7sp\u00fcrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich dachte nach, wie er so n\u00fctzlich sey,", "tokens": ["Ich", "dach\u00b7te", "nach", ",", "wie", "er", "so", "n\u00fctz\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und fiel mir die\u00df dar\u00fcber bey:", "tokens": ["Und", "fiel", "mir", "die\u00df", "da\u00b7r\u00fc\u00b7ber", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ach liebster GOtt! was trieffen nicht in diesem l\u00e4ngst-ge-", "tokens": ["Ach", "liebs\u00b7ter", "Gott", "!", "was", "trief\u00b7fen", "nicht", "in", "die\u00b7sem", "l\u00e4ngs\u00b7tge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "PWS", "VVFIN", "PTKNEG", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "w\u00fcnschten Regen", "tokens": ["w\u00fcnschten", "Re\u00b7gen"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "In das fast ausgedorrte Land f\u00fcr Anmuth, Fruchtbar-", "tokens": ["In", "das", "fast", "aus\u00b7ge\u00b7dorr\u00b7te", "Land", "f\u00fcr", "An\u00b7muth", ",", "Frucht\u00b7ba\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "APPR", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "keit und Segen!", "tokens": ["keit", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Es sagt mit Recht die Welt f\u00fcr diesen Segens-Tranck,", "tokens": ["Es", "sagt", "mit", "Recht", "die", "Welt", "f\u00fcr", "die\u00b7sen", "Se\u00b7gens\u00b7Tranck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ich absonderlich, dir, Vater, Lob und Danck.", "tokens": ["Und", "ich", "ab\u00b7son\u00b7der\u00b7lich", ",", "dir", ",", "Va\u00b7ter", ",", "Lob", "und", "Danck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$,", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die, durch die kalte Luft, zur\u00fcck gehaltnen S\u00e4fte", "tokens": ["Die", ",", "durch", "die", "kal\u00b7te", "Luft", ",", "zu\u00b7r\u00fcck", "ge\u00b7halt\u00b7nen", "S\u00e4f\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vermehrten die bi\u00dfher gehemmten Kr\u00e4fte,", "tokens": ["Ver\u00b7mehr\u00b7ten", "die", "bi\u00df\u00b7her", "ge\u00b7hemm\u00b7ten", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Und drengten recht mit Macht durch St\u00e4mm- und Zwei-", "tokens": ["Und", "dreng\u00b7ten", "recht", "mit", "Macht", "durch", "St\u00e4m\u00b7m", "und", "Zwei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "APPR", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "ge sich.", "tokens": ["ge", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.21": {"text": "Die Knospen spalteten fast sichtbarlich.", "tokens": ["Die", "Knos\u00b7pen", "spal\u00b7te\u00b7ten", "fast", "sicht\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Es kamen junger Bl\u00fcht\u2019 und zarter Bl\u00e4tter Sprossen", "tokens": ["Es", "ka\u00b7men", "jun\u00b7ger", "Bl\u00fcht'", "und", "zar\u00b7ter", "Bl\u00e4t\u00b7ter", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Aus ihrem Auffenthalt hervorgeschossen.", "tokens": ["Aus", "ih\u00b7rem", "Auf\u00b7fent\u00b7halt", "her\u00b7vor\u00b7ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Es lie\u00df, ob wolten sie durchaus nicht l\u00e4nger s\u00e4umen.", "tokens": ["Es", "lie\u00df", ",", "ob", "wol\u00b7ten", "sie", "durc\u00b7haus", "nicht", "l\u00e4n\u00b7ger", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In der Allee, auf Stauden, B\u00e4umen,", "tokens": ["In", "der", "Al\u00b7lee", ",", "auf", "Stau\u00b7den", ",", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Auf Hecken, \u00fcberall, erschien", "tokens": ["Auf", "He\u00b7cken", ",", "\u00fc\u00b7be\u00b7rall", ",", "er\u00b7schien"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ein gr\u00fcnlich Braun, ein br\u00e4unlich Gr\u00fcn;", "tokens": ["Ein", "gr\u00fcn\u00b7lich", "Braun", ",", "ein", "br\u00e4un\u00b7lich", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Doch schien all\u2019 Augenblick das Braun sich zu vermindern.", "tokens": ["Doch", "schien", "all'", "Au\u00b7gen\u00b7blick", "das", "Braun", "sich", "zu", "ver\u00b7min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}