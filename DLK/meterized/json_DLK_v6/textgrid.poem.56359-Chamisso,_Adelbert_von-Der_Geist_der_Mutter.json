"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.73": {"text": "Hier ward er seines Teuersten beraubt,", "tokens": ["Hier", "ward", "er", "sei\u00b7nes", "Teu\u00b7ers\u00b7ten", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.74": {"text": "Hier hat der Ernst des Lebens ihn erfa\u00dft", "tokens": ["Hier", "hat", "der", "Ernst", "des", "Le\u00b7bens", "ihn", "er\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.75": {"text": "Und seiner Kindheit \u00fcpp'ges Reis entlaubt.", "tokens": ["Und", "sei\u00b7ner", "Kind\u00b7heit", "\u00fcpp'\u00b7ges", "Reis", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.76": {"text": "Und jetzt! \u2013 So steht er eine lange Rast,", "tokens": ["Und", "jetzt", "!", "\u2013", "So", "steht", "er", "ei\u00b7ne", "lan\u00b7ge", "Rast", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.77": {"text": "Von Garnen der Erinnerung umstellt,", "tokens": ["Von", "Gar\u00b7nen", "der", "E\u00b7rin\u00b7ne\u00b7rung", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.78": {"text": "Das Herz zermalmt von namenloser Last.", "tokens": ["Das", "Herz", "zer\u00b7malmt", "von", "na\u00b7men\u00b7lo\u00b7ser", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.79": {"text": "Und endlich nieder auf das Lager f\u00e4llt", "tokens": ["Und", "end\u00b7lich", "nie\u00b7der", "auf", "das", "La\u00b7ger", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.80": {"text": "Er weinend, schluchzend, schmerzen\u00fcberwunden,", "tokens": ["Er", "wei\u00b7nend", ",", "schluch\u00b7zend", ",", "schmer\u00b7zen\u00b7\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.81": {"text": "Den Schlaf nicht suchend, der sich ferne h\u00e4lt.", "tokens": ["Den", "Schlaf", "nicht", "su\u00b7chend", ",", "der", "sich", "fer\u00b7ne", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.82": {"text": "Der Schlo\u00dfuhr ehrne Zunge z\u00e4hlt die Stunden,", "tokens": ["Der", "Schlo\u00b7\u00dfuhr", "ehr\u00b7ne", "Zun\u00b7ge", "z\u00e4hlt", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.83": {"text": "Es schlie\u00dft die Nacht sich zu, das Licht verglimmt,", "tokens": ["Es", "schlie\u00dft", "die", "Nacht", "sich", "zu", ",", "das", "Licht", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.84": {"text": "In grauser Stille bluten seine Wunden.", "tokens": ["In", "grau\u00b7ser", "Stil\u00b7le", "blu\u00b7ten", "sei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.85": {"text": "Da mahnt ihn ein Ger\u00e4usch, das er vernimmt,", "tokens": ["Da", "mahnt", "ihn", "ein", "Ge\u00b7r\u00e4usch", ",", "das", "er", "ver\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.86": {"text": "Da\u00df dr\u00fcben bei dem Vater er gelassen", "tokens": ["Da\u00df", "dr\u00fc\u00b7ben", "bei", "dem", "Va\u00b7ter", "er", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.87": {"text": "Die Waffen, die zu seinem Schutz bestimmt.", "tokens": ["Die", "Waf\u00b7fen", ",", "die", "zu", "sei\u00b7nem", "Schutz", "be\u00b7stimmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.88": {"text": "Und ringsher sp\u00e4hend sieht er einen blassen", "tokens": ["Und", "rings\u00b7her", "sp\u00e4\u00b7hend", "sieht", "er", "ei\u00b7nen", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.89": {"text": "Unsichern Schimmer durch das Zimmer wehen;", "tokens": ["Un\u00b7si\u00b7chern", "Schim\u00b7mer", "durch", "das", "Zim\u00b7mer", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.90": {"text": "Es reizt ihn, den ins Auge scharf zu fassen.", "tokens": ["Es", "reizt", "ihn", ",", "den", "ins", "Au\u00b7ge", "scharf", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.91": {"text": "Er h\u00f6ret drau\u00dfen leisen Schrittes gehen;", "tokens": ["Er", "h\u00f6\u00b7ret", "drau\u00b7\u00dfen", "lei\u00b7sen", "Schrit\u00b7tes", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.92": {"text": "Er siehet jenen Schimmer sich gestalten,", "tokens": ["Er", "sie\u00b7het", "je\u00b7nen", "Schim\u00b7mer", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.93": {"text": "Und siehet seine Mutter vor ihm stehen.", "tokens": ["Und", "sie\u00b7het", "sei\u00b7ne", "Mut\u00b7ter", "vor", "ihm", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.94": {"text": "Sie winkt ihm, regungslos sich zu verhalten,", "tokens": ["Sie", "winkt", "ihm", ",", "re\u00b7gungs\u00b7los", "sich", "zu", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.95": {"text": "Sie hebt die Augen schmerzenreich empor,", "tokens": ["Sie", "hebt", "die", "Au\u00b7gen", "schmer\u00b7zen\u00b7reich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.96": {"text": "Sie scheinet \u00fcber ihn die Wacht zu halten.", "tokens": ["Sie", "schei\u00b7net", "\u00fc\u00b7ber", "ihn", "die", "Wacht", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.97": {"text": "Es rauscht, die T\u00fcr geht auf, \u2013 sie tritt davor, \u2013", "tokens": ["Es", "rauscht", ",", "die", "T\u00fcr", "geht", "auf", ",", "\u2013", "sie", "tritt", "da\u00b7vor", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,", "$(", "PPER", "VVFIN", "PAV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.98": {"text": "Ein lauter, angsterpre\u00dfter Schrei erschallt,", "tokens": ["Ein", "lau\u00b7ter", ",", "angs\u00b7ter\u00b7pre\u00df\u00b7ter", "Schrei", "er\u00b7schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.99": {"text": "Die Stimme seines Vaters traf sein Ohr;", "tokens": ["Die", "Stim\u00b7me", "sei\u00b7nes", "Va\u00b7ters", "traf", "sein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.100": {"text": "Da wirft man Schweres klirrend hin, es hallt", "tokens": ["Da", "wirft", "man", "Schwe\u00b7res", "klir\u00b7rend", "hin", ",", "es", "hallt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.101": {"text": "Der Gang von fl\u00fccht'gen Schritten, es verklingt, \u2013", "tokens": ["Der", "Gang", "von", "fl\u00fccht'\u00b7gen", "Schrit\u00b7ten", ",", "es", "ver\u00b7klingt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.102": {"text": "Zerflossen ist in Nebel die Gestalt.", "tokens": ["Zer\u00b7flos\u00b7sen", "ist", "in", "Ne\u00b7bel", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.103": {"text": "Er aber dort auf seinem Lager ringt", "tokens": ["Er", "a\u00b7ber", "dort", "auf", "sei\u00b7nem", "La\u00b7ger", "ringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.104": {"text": "Mit dem Entsetzen, bis mit hellem Scheine", "tokens": ["Mit", "dem", "Ent\u00b7set\u00b7zen", ",", "bis", "mit", "hel\u00b7lem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.105": {"text": "Der junge Tag in seine Augen dringt.", "tokens": ["Der", "jun\u00b7ge", "Tag", "in", "sei\u00b7ne", "Au\u00b7gen", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.106": {"text": "Er schaut umher; die T\u00fcr ist auf, und seine", "tokens": ["Er", "schaut", "um\u00b7her", ";", "die", "T\u00fcr", "ist", "auf", ",", "und", "sei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.107": {"text": "Pistolen liegen auf der Schwelle dort;", "tokens": ["Pis\u00b7to\u00b7len", "lie\u00b7gen", "auf", "der", "Schwel\u00b7le", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.108": {"text": "Er fragt sich nicht, was er dar\u00fcber meine.", "tokens": ["Er", "fragt", "sich", "nicht", ",", "was", "er", "da\u00b7r\u00fc\u00b7ber", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "PWS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.109": {"text": "Er schleicht hinaus sich leise, spricht kein Wort,", "tokens": ["Er", "schleicht", "hin\u00b7aus", "sich", "lei\u00b7se", ",", "spricht", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APZR", "PRF", "ADJD", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.110": {"text": "Er sattelt, steigt zu Ro\u00df und dr\u00fcckt die Sporen;", "tokens": ["Er", "sat\u00b7telt", ",", "steigt", "zu", "Ro\u00df", "und", "dr\u00fcckt", "die", "Spo\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.111": {"text": "Erst ihrem Grabe zu, dann weiter fort. \u2013", "tokens": ["Erst", "ih\u00b7rem", "Gra\u00b7be", "zu", ",", "dann", "wei\u00b7ter", "fort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.112": {"text": "Es hat sich jede Spur von ihm verloren.", "tokens": ["Es", "hat", "sich", "je\u00b7de", "Spur", "von", "ihm", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}