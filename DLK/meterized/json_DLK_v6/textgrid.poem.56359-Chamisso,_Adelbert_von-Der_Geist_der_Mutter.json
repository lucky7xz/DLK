{"textgrid.poem.56359": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist der Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Muse f\u00fchrt euch in das Schlo\u00df des Grafen;", "tokens": ["Die", "Mu\u00b7se", "f\u00fchrt", "euch", "in", "das", "Schlo\u00df", "des", "Gra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hat den alten Wappenschild am Tor", "tokens": ["Sie", "hat", "den", "al\u00b7ten", "Wap\u00b7pen\u00b7schild", "am", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verhangen, und es soll sein Name schlafen.", "tokens": ["Ver\u00b7han\u00b7gen", ",", "und", "es", "soll", "sein", "Na\u00b7me", "schla\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seht dort ihn selbst, der bleich und hager vor", "tokens": ["Seht", "dort", "ihn", "selbst", ",", "der", "bleich", "und", "ha\u00b7ger", "vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Pergamente z\u00e4hneknirschend lacht,", "tokens": ["Dem", "Per\u00b7ga\u00b7men\u00b7te", "z\u00e4h\u00b7nek\u00b7nir\u00b7schend", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zitternd, wie es rauschet, f\u00e4hrt empor.", "tokens": ["Und", "zit\u00b7ternd", ",", "wie", "es", "rau\u00b7schet", ",", "f\u00e4hrt", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schaut nicht hinab in seines Busens Nacht,", "tokens": ["Schaut", "nicht", "hin\u00b7ab", "in", "sei\u00b7nes", "Bu\u00b7sens", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Fragt nicht nach seinem Unmut, seinem Groll,", "tokens": ["Fragt", "nicht", "nach", "sei\u00b7nem", "Un\u00b7mut", ",", "sei\u00b7nem", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und nicht, was vor ihm selbst ihn schaudern macht.", "tokens": ["Und", "nicht", ",", "was", "vor", "ihm", "selbst", "ihn", "schau\u00b7dern", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PRELS", "APPR", "PPER", "ADV", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Blickt ab von ihm; seht schweigsam, ahndungsvoll", "tokens": ["Blickt", "ab", "von", "ihm", ";", "seht", "schweig\u00b7sam", ",", "ahn\u00b7dungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPER", "$.", "VVFIN", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Dienerschaft den einz'gen Sohn erwarten,", "tokens": ["Die", "Die\u00b7ner\u00b7schaft", "den", "einz'\u00b7gen", "Sohn", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dem jetzt der Mutter Erbe werden soll.", "tokens": ["Dem", "jetzt", "der", "Mut\u00b7ter", "Er\u00b7be", "wer\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er ward in Schul und Welt und Krieg vom harten", "tokens": ["Er", "ward", "in", "Schul", "und", "Welt", "und", "Krieg", "vom", "har\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Geschick versto\u00dfen, seit die Augen schlo\u00df,", "tokens": ["Ge\u00b7schick", "ver\u00b7sto\u00b7\u00dfen", ",", "seit", "die", "Au\u00b7gen", "schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die liebend pflegte seiner Kindheit Garten.", "tokens": ["Die", "lie\u00b7bend", "pfleg\u00b7te", "sei\u00b7ner", "Kind\u00b7heit", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nun kehrt er heim in seines Vaters Schlo\u00df;", "tokens": ["Nun", "kehrt", "er", "heim", "in", "sei\u00b7nes", "Va\u00b7ters", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Er wieget sich in zaubervollen Tr\u00e4umen,", "tokens": ["Er", "wie\u00b7get", "sich", "in", "zau\u00b7ber\u00b7vol\u00b7len", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und spornt vor Ungeduld sein feurig Ro\u00df.", "tokens": ["Und", "spornt", "vor", "Un\u00b7ge\u00b7duld", "sein", "feu\u00b7rig", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und dort beginnt inmitten gr\u00fcnen R\u00e4umen", "tokens": ["Und", "dort", "be\u00b7ginnt", "in\u00b7mit\u00b7ten", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Das Dorf mit roten D\u00e4chern zu erscheinen;", "tokens": ["Das", "Dorf", "mit", "ro\u00b7ten", "D\u00e4\u00b7chern", "zu", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Kirche dort, und unter jenen B\u00e4umen...!", "tokens": ["Die", "Kir\u00b7che", "dort", ",", "und", "un\u00b7ter", "je\u00b7nen", "B\u00e4u\u00b7men", "...", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "APPR", "PDAT", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Er hat den Baum gepflanzt, der jetzt mit seinen", "tokens": ["Er", "hat", "den", "Baum", "ge\u00b7pflanzt", ",", "der", "jetzt", "mit", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PRELS", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Weitausgespannten \u00c4sten schirmt das Grab", "tokens": ["Weit\u00b7aus\u00b7ge\u00b7spann\u00b7ten", "\u00c4s\u00b7ten", "schirmt", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Mutter, wo er beten mu\u00df und weinen:", "tokens": ["Der", "Mut\u00b7ter", ",", "wo", "er", "be\u00b7ten", "mu\u00df", "und", "wei\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u00bbvernimm du mich, die mir das Leben gab,", "tokens": ["\u00bb", "ver\u00b7nimm", "du", "mich", ",", "die", "mir", "das", "Le\u00b7ben", "gab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PRF", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Du, deren Bild ich stets in mir getragen;", "tokens": ["Du", ",", "de\u00b7ren", "Bild", "ich", "stets", "in", "mir", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Nicht wende jetzt die Augen von mir ab.", "tokens": ["Nicht", "wen\u00b7de", "jetzt", "die", "Au\u00b7gen", "von", "mir", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der fremdgewordnen Heimat werd ich klagen,", "tokens": ["Der", "fremd\u00b7ge\u00b7word\u00b7nen", "Hei\u00b7mat", "werd", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Da\u00df meine Tr\u00e4ume noch nur Tr\u00e4ume sind;", "tokens": ["Da\u00df", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "noch", "nur", "Tr\u00e4u\u00b7me", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Du sollst um mich die Geisterarme schlagen.\u00ab \u2013", "tokens": ["Du", "sollst", "um", "mich", "die", "Geis\u00b7ter\u00b7ar\u00b7me", "schla\u00b7gen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PRF", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und nun zu Ro\u00df! zum Schlo\u00df hinan geschwind!", "tokens": ["Und", "nun", "zu", "Ro\u00df", "!", "zum", "Schlo\u00df", "hi\u00b7nan", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$.", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Bach, \u2013 die Felsenwand, \u2013 die alten F\u00f6hren,", "tokens": ["Der", "Bach", ",", "\u2013", "die", "Fel\u00b7sen\u00b7wand", ",", "\u2013", "die", "al\u00b7ten", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "ART", "NN", "$,", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ihr dunkles Haupt bewegt der Abendwind;", "tokens": ["Ihr", "dunk\u00b7les", "Haupt", "be\u00b7wegt", "der", "Ab\u00b7end\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Sie scheinen seines Herzens Gru\u00df zu h\u00f6ren", "tokens": ["Sie", "schei\u00b7nen", "sei\u00b7nes", "Her\u00b7zens", "Gru\u00df", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und zu erwidern; Fremde sind allein", "tokens": ["Und", "zu", "er\u00b7wi\u00b7dern", ";", "Frem\u00b7de", "sind", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "$.", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Die Menschen, die die T\u00e4uschung ihm zerst\u00f6ren.", "tokens": ["Die", "Men\u00b7schen", ",", "die", "die", "T\u00e4u\u00b7schung", "ihm", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und hier, um diesen Felsen mu\u00df es sein, \u2013", "tokens": ["Und", "hier", ",", "um", "die\u00b7sen", "Fel\u00b7sen", "mu\u00df", "es", "sein", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "PDAT", "NN", "VMFIN", "PPER", "VAINF", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Es wendet sich der Weg, und vor ihm prangen", "tokens": ["Es", "wen\u00b7det", "sich", "der", "Weg", ",", "und", "vor", "ihm", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Des Schlosses Zinnen rot im Abendschein;", "tokens": ["Des", "Schlos\u00b7ses", "Zin\u00b7nen", "rot", "im", "A\u00b7bend\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Da rollen Tr\u00e4nen \u00fcber seine Wangen;", "tokens": ["Da", "rol\u00b7len", "Tr\u00e4\u00b7nen", "\u00fc\u00b7ber", "sei\u00b7ne", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Er st\u00fcrmt den Hof hinan, und Diener kommen", "tokens": ["Er", "st\u00fcrmt", "den", "Hof", "hi\u00b7nan", ",", "und", "Die\u00b7ner", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Neugierig fremd herbei ihn zu empfangen.", "tokens": ["Neu\u00b7gie\u00b7rig", "fremd", "her\u00b7bei", "ihn", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Nach seinem Vater fragt er, sucht ihn frommen", "tokens": ["Nach", "sei\u00b7nem", "Va\u00b7ter", "fragt", "er", ",", "sucht", "ihn", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Und liebedurst'gen Blickes: hat er, ach!", "tokens": ["Und", "lie\u00b7be\u00b7dur\u00b7st'\u00b7gen", "Bli\u00b7ckes", ":", "hat", "er", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "VAFIN", "PPER", "$,", "ITJ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Von seines Sohnes Heimkehr nichts vernommen?", "tokens": ["Von", "sei\u00b7nes", "Soh\u00b7nes", "Heim\u00b7kehr", "nichts", "ver\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Dem J\u00e4ger folgt er durch die Halle nach,", "tokens": ["Dem", "J\u00e4\u00b7ger", "folgt", "er", "durch", "die", "Hal\u00b7le", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "met