{"dta.poem.1160": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "46. Die sch\u00f6nen grauen Haare.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest dich/ warum? weil dir so fr\u00fch/ Melinde/ ", "tokens": ["Du", "kla\u00b7gest", "dich", "/", "wa\u00b7rum", "?", "weil", "dir", "so", "fr\u00fch", "/", "Me\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "$.", "KOUS", "PPER", "ADV", "ADJD", "$(", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deiner Haare Gold manch Silber-Faden sticht:", "tokens": ["Durch", "dei\u00b7ner", "Haa\u00b7re", "Gold", "manch", "Sil\u00b7ber\u00b7Fa\u00b7den", "sticht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sich der kalte Schnee um deine Scheitel flicht/", "tokens": ["Weil", "sich", "der", "kal\u00b7te", "Schnee", "um", "dei\u00b7ne", "Schei\u00b7tel", "flicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da noch des Sommers Brand erhitzet deine Gr\u00fcnde:", "tokens": ["Da", "noch", "des", "Som\u00b7mers", "Brand", "er\u00b7hit\u00b7zet", "dei\u00b7ne", "Gr\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich nicht: indem ich noch die alte Glutt empfinde/", "tokens": ["Ich", "nicht", ":", "in\u00b7dem", "ich", "noch", "die", "al\u00b7te", "Glutt", "emp\u00b7fin\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob schon der k\u00fchle Reiff aus deinen Schl\u00e4ffen bricht/", "tokens": ["Ob", "schon", "der", "k\u00fch\u00b7le", "Reiff", "aus", "dei\u00b7nen", "Schl\u00e4f\u00b7fen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein hei\u00dfes Athem-ziehn und Seufftzen k\u00fchlt sich nicht/", "tokens": ["Mein", "hei\u00b7\u00dfes", "A\u00b7them\u00b7ziehn", "und", "Seufft\u00b7zen", "k\u00fchlt", "sich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PRF", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weist dein Gebirge gleich das Zeichen kalter Winde.", "tokens": ["Weist", "dein", "Ge\u00b7bir\u00b7ge", "gleich", "das", "Zei\u00b7chen", "kal\u00b7ter", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wei\u00df Haar zeigt wei\u00dfen Sinn/ und kr\u00f6nt ein kluges Haubt.", "tokens": ["Wei\u00df", "Haar", "zeigt", "wei\u00b7\u00dfen", "Sinn", "/", "und", "kr\u00f6nt", "ein", "klu\u00b7ges", "Haubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit der Augen Sonn\u2019 ein Himmel sey erlaubt/", "tokens": ["Da\u00b7mit", "der", "Au\u00b7gen", "Sonn'", "ein", "Him\u00b7mel", "sey", "er\u00b7laubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieht man ein graues Haar gleich Wolcken ob ihr prangen.", "tokens": ["Sieht", "man", "ein", "grau\u00b7es", "Haar", "gleich", "Wol\u00b7cken", "ob", "ihr", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "ADV", "NN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum sag ich offtermahls: Ihr angenehmen Wangen/", "tokens": ["Drum", "sag", "ich", "off\u00b7ter\u00b7mahls", ":", "Ihr", "an\u00b7ge\u00b7neh\u00b7men", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(verwundert \u00fcbers Haar/ das keinem Schnee giebt nach/)", "tokens": ["(", "ver\u00b7wun\u00b7dert", "\u00fc\u00b7bers", "Haar", "/", "das", "kei\u00b7nem", "Schnee", "giebt", "nach", "/", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPRART", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "APPR", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier deckt den Rosen-Stock ein weisses Liljen-Dach.", "tokens": ["Hier", "deckt", "den", "Ro\u00b7sen\u00b7Stock", "ein", "weis\u00b7ses", "Lil\u00b7jen\u00b7Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}