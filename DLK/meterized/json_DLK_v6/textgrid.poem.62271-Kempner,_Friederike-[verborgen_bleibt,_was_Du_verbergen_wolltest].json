{"textgrid.poem.62271": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[verborgen bleibt, was Du verbergen wolltest]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verborgen bleibt, was Du verbergen wolltest \u2013", "tokens": ["Ver\u00b7bor\u00b7gen", "bleibt", ",", "was", "Du", "ver\u00b7ber\u00b7gen", "woll\u00b7test", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Millionen Sonnen Du vor uns aufrolltest \u2013", "tokens": ["Als", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", "Du", "vor", "uns", "auf\u00b7roll\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da wolltest Du uns Macht und Sch\u00f6nheit zeigen,", "tokens": ["Da", "woll\u00b7test", "Du", "uns", "Macht", "und", "Sch\u00f6n\u00b7heit", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor solcher Gr\u00f6\u00dfe mu\u00df der Mensch sich neigen.", "tokens": ["Vor", "sol\u00b7cher", "Gr\u00f6\u00b7\u00dfe", "mu\u00df", "der", "Mensch", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}