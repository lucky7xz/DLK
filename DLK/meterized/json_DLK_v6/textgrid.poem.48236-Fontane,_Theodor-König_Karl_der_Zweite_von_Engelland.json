{"textgrid.poem.48236": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Karl der Zweite von Engelland", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Karl der Zweite von Engelland", "tokens": ["K\u00f6\u00b7nig", "Karl", "der", "Zwei\u00b7te", "von", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ART", "NN", "APPR", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bei Mit- und Nachwelt in Ungunst stand;", "tokens": ["Bei", "Mit", "und", "Nach\u00b7welt", "in", "Un\u00b7gunst", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Geschichtsbuch ist zu lesen,", "tokens": ["In", "je\u00b7dem", "Ge\u00b7schichts\u00b7buch", "ist", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er sei durchaus vom \u00dcbel gewesen", "tokens": ["Er", "sei", "durc\u00b7haus", "vom", "\u00dc\u00b7bel", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VAPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und habe das denkbar Schlimmste verbrochen:", "tokens": ["Und", "ha\u00b7be", "das", "denk\u00b7bar", "Schlimms\u00b7te", "ver\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nie was Kluges getan, nie was Dummes gesprochen.", "tokens": ["Nie", "was", "Klu\u00b7ges", "ge\u00b7tan", ",", "nie", "was", "Dum\u00b7mes", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "VVPP", "$,", "ADV", "PWS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Ach K\u00f6nig Karl von Engelland,", "tokens": ["Ach", "K\u00f6\u00b7nig", "Karl", "von", "En\u00b7gel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einen kenn' ich, der hebt f\u00fcr dich die Hand,", "tokens": ["Ei\u00b7nen", "kenn'", "ich", ",", "der", "hebt", "f\u00fcr", "dich", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Einen kenn' ich, der sich zu sagen erdreistet,", "tokens": ["Ei\u00b7nen", "kenn'", "ich", ",", "der", "sich", "zu", "sa\u00b7gen", "er\u00b7dreis\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "$,", "PRELS", "PRF", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Du hast das denkbar Gr\u00f6\u00dfte geleistet.", "tokens": ["Du", "hast", "das", "denk\u00b7bar", "Gr\u00f6\u00df\u00b7te", "ge\u00b7leis\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn immer zu tun, was klug und weise,", "tokens": ["Denn", "im\u00b7mer", "zu", "tun", ",", "was", "klug", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie sehr ich diese Kunst auch preise,", "tokens": ["Wie", "sehr", "ich", "die\u00b7se", "Kunst", "auch", "prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie mu\u00df ihr Auge doch niederschlagen", "tokens": ["Sie", "mu\u00df", "ihr", "Au\u00b7ge", "doch", "nie\u00b7der\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Vor der h\u00f6heren Kunst, nie was Dummes zu sagen.", "tokens": ["Vor", "der", "h\u00f6\u00b7he\u00b7ren", "Kunst", ",", "nie", "was", "Dum\u00b7mes", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "PWS", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}}}}