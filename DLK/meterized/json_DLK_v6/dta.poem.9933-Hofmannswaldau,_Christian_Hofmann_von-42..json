{"dta.poem.9933": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Keine fr\u00fcchte trag ich nicht/ unter sich stehn meine zweige:", "tokens": ["Kei\u00b7ne", "fr\u00fcch\u00b7te", "trag", "ich", "nicht", "/", "un\u00b7ter", "sich", "stehn", "mei\u00b7ne", "zwei\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "$(", "APPR", "PRF", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Aeste hab ich ohne blatt: meine reinligkeit ich zeige", "tokens": ["A\u00b7es\u00b7te", "hab", "ich", "oh\u00b7ne", "blatt", ":", "mei\u00b7ne", "rein\u00b7lig\u00b7keit", "ich", "zei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PTKVZ", "$.", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "---+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aller orten wo ich bin. Nehme weg/ was bringet schaden/", "tokens": ["Al\u00b7ler", "or\u00b7ten", "wo", "ich", "bin", ".", "Neh\u00b7me", "weg", "/", "was", "brin\u00b7get", "scha\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PWAV", "PPER", "VAFIN", "$.", "NN", "PTKVZ", "$(", "PWS", "VVFIN", "VVINF", "$("], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bey den alten steh ich mehr/ als bey kindern/ in genaden.", "tokens": ["Bey", "den", "al\u00b7ten", "steh", "ich", "mehr", "/", "als", "bey", "kin\u00b7dern", "/", "in", "ge\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "PPER", "ADV", "$(", "KOUS", "APPR", "NN", "$(", "APPR", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}