{"textgrid.poem.52185": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Alle", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprach der Geist: Sieh auf! Es war im Traume.", "tokens": ["Es", "sprach", "der", "Geist", ":", "Sieh", "auf", "!", "Es", "war", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "PTKVZ", "$.", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hob den Blick. In lichtem Wolkenraume", "tokens": ["Ich", "hob", "den", "Blick", ".", "In", "lich\u00b7tem", "Wol\u00b7ken\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah ich den Herrn das Brot den Zw\u00f6lfen brechen", "tokens": ["Sah", "ich", "den", "Herrn", "das", "Brot", "den", "Zw\u00f6l\u00b7fen", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ahnungsvolle Liebesworte sprechen.", "tokens": ["Und", "ah\u00b7nungs\u00b7vol\u00b7le", "Lie\u00b7bes\u00b7wor\u00b7te", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weit \u00fcber ihre H\u00e4upter lud die Erde", "tokens": ["Weit", "\u00fc\u00b7ber", "ih\u00b7re", "H\u00e4up\u00b7ter", "lud", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ein mit allumarmender Geb\u00e4rde.", "tokens": ["Er", "ein", "mit", "al\u00b7lu\u00b7mar\u00b7men\u00b7der", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es sprach der Geist: Sieh auf! Ein Linnen schweben", "tokens": ["Es", "sprach", "der", "Geist", ":", "Sieh", "auf", "!", "Ein", "Lin\u00b7nen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "PTKVZ", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich und vielen schon das Mahl gegeben,", "tokens": ["Sah", "ich", "und", "vie\u00b7len", "schon", "das", "Mahl", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PIS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da breiteten sich unter tausend H\u00e4nden", "tokens": ["Da", "brei\u00b7te\u00b7ten", "sich", "un\u00b7ter", "tau\u00b7send", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tische, doch verd\u00e4mmerten die Enden", "tokens": ["Die", "Ti\u00b7sche", ",", "doch", "ver\u00b7d\u00e4m\u00b7mer\u00b7ten", "die", "En\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In grauen Nebel, drin auf bleichen Stufen", "tokens": ["In", "grau\u00b7en", "Ne\u00b7bel", ",", "drin", "auf", "blei\u00b7chen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kummergestalten sa\u00dfen ungerufen.", "tokens": ["Kum\u00b7mer\u00b7ge\u00b7stal\u00b7ten", "sa\u00b7\u00dfen", "un\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Es sprach der Geist: Sieh auf! Die Luft umblaute", "tokens": ["Es", "sprach", "der", "Geist", ":", "Sieh", "auf", "!", "Die", "Luft", "um\u00b7blau\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "PTKVZ", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein unerme\u00dflich Mahl, so weit ich schaute,", "tokens": ["Ein", "un\u00b7er\u00b7me\u00df\u00b7lich", "Mahl", ",", "so", "weit", "ich", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sprangen reich die Brunnen auf des Lebens,", "tokens": ["Da", "spran\u00b7gen", "reich", "die", "Brun\u00b7nen", "auf", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da streckte keine Schale sich vergebens,", "tokens": ["Da", "streck\u00b7te", "kei\u00b7ne", "Scha\u00b7le", "sich", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da lag das ganze Volk auf vollen Garben,", "tokens": ["Da", "lag", "das", "gan\u00b7ze", "Volk", "auf", "vol\u00b7len", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Platz war leer und keiner durfte darben.", "tokens": ["Kein", "Platz", "war", "leer", "und", "kei\u00b7ner", "durf\u00b7te", "dar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "KON", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}