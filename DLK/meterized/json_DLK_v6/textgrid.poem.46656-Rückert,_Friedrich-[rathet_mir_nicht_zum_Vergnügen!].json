{"textgrid.poem.46656": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[rathet mir nicht zum Vergn\u00fcgen!]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rathet mir nicht zum Vergn\u00fcgen!", "tokens": ["Ra\u00b7thet", "mir", "nicht", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Es kann nicht mein Herz betr\u00fcgen,", "tokens": ["Es", "kann", "nicht", "mein", "Herz", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Kann nur Leid zum Leide f\u00fcgen.", "tokens": ["Kann", "nur", "Leid", "zum", "Lei\u00b7de", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft mich bei dem Gram im Zimmer!", "tokens": ["La\u00dft", "mich", "bei", "dem", "Gram", "im", "Zim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Blick bin ich gewohnt.", "tokens": ["Sei\u00b7nen", "Blick", "bin", "ich", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt mir nicht, da\u00df Fr\u00fchlingsschimmer", "tokens": ["Sagt", "mir", "nicht", ",", "da\u00df", "Fr\u00fch\u00b7lings\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drau\u00dfen auf den Auen thront!", "tokens": ["Drau\u00b7\u00dfen", "auf", "den", "Au\u00b7en", "thront", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mein Auge sei verschont", "tokens": ["Da\u00df", "mein", "Au\u00b7ge", "sei", "ver\u00b7schont"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Freud' entw\u00f6hnten Z\u00fcgen!", "tokens": ["Mit", "der", "Freud'", "ent\u00b7w\u00f6hn\u00b7ten", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rathet mir nicht zum Vergn\u00fcgen!", "tokens": ["Ra\u00b7thet", "mir", "nicht", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Ladet mich nicht ein zum Garten,", "tokens": ["La\u00b7det", "mich", "nicht", "ein", "zum", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Blumen halten Spiel!", "tokens": ["Wo", "die", "Blu\u00b7men", "hal\u00b7ten", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was kann ich nun dort erwarten,", "tokens": ["Was", "kann", "ich", "nun", "dort", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo es sonst mir wohl gefiel?", "tokens": ["Wo", "es", "sonst", "mir", "wohl", "ge\u00b7fiel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jede Blum' auf welkem Stiel", "tokens": ["Je\u00b7de", "Blum'", "auf", "wel\u00b7kem", "Stiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will umsonst mir Freude l\u00fcgen,", "tokens": ["Will", "um\u00b7sonst", "mir", "Freu\u00b7de", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Es kann nicht mein Herz betr\u00fcgen.", "tokens": ["Es", "kann", "nicht", "mein", "Herz", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wie sich in die Krankheitss\u00e4fte", "tokens": ["Wie", "sich", "in", "die", "Krank\u00b7heits\u00b7s\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt gleich des Kranken Kost,", "tokens": ["Wan\u00b7delt", "gleich", "des", "Kran\u00b7ken", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also sauget neue Kr\u00e4fte", "tokens": ["Al\u00b7so", "sau\u00b7get", "neu\u00b7e", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mein Kummer aus dem Trost.", "tokens": ["Nur", "mein", "Kum\u00b7mer", "aus", "dem", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Strahl aus hellem Ost", "tokens": ["Je\u00b7der", "Strahl", "aus", "hel\u00b7lem", "Ost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will der Seele Tr\u00fcbheit r\u00fcgen,", "tokens": ["Will", "der", "See\u00b7le", "Tr\u00fcb\u00b7heit", "r\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann nur Leid zum Leide f\u00fcgen.", "tokens": ["Kann", "nur", "Leid", "zum", "Lei\u00b7de", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}