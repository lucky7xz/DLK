{"dta.poem.8693": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Himmelstrauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Am Himmelsantlitz wandelt ein Gedanke,", "tokens": ["Am", "Him\u00b7mel\u00b7sant\u00b7litz", "wan\u00b7delt", "ein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die d\u00fcstre Wolke dort, so bang, so schwer;", "tokens": ["Die", "d\u00fcst\u00b7re", "Wol\u00b7ke", "dort", ",", "so", "bang", ",", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie auf dem Lager sich der Seelenkranke,", "tokens": ["Wie", "auf", "dem", "La\u00b7ger", "sich", "der", "See\u00b7len\u00b7kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wirft sich der Strauch im Winde hin und her.", "tokens": ["Wirft", "sich", "der", "Strauch", "im", "Win\u00b7de", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Himmel t\u00f6nt ein schwermuthmattes Grollen,", "tokens": ["Vom", "Him\u00b7mel", "t\u00f6nt", "ein", "schwer\u00b7muth\u00b7mat\u00b7tes", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dunkle Wimper blinzet manches Mal,", "tokens": ["Die", "dunk\u00b7le", "Wim\u00b7per", "blin\u00b7zet", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2014 So blinzen Augen, wenn sie weinen wollen, \u2014", "tokens": ["So", "blin\u00b7zen", "Au\u00b7gen", ",", "wenn", "sie", "wei\u00b7nen", "wol\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und aus der Wimper zuckt ein schwacher Strahl. \u2014", "tokens": ["Und", "aus", "der", "Wim\u00b7per", "zuckt", "ein", "schwa\u00b7cher", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun schleichen aus dem Moore k\u00fchle Schauer,", "tokens": ["Nun", "schlei\u00b7chen", "aus", "dem", "Moo\u00b7re", "k\u00fch\u00b7le", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leise Nebel \u00fcber's Heideland;", "tokens": ["Und", "lei\u00b7se", "Ne\u00b7bel", "\u00fc\u00b7ber's", "Hei\u00b7de\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel lie\u00df, nachsinnend seiner Trauer,", "tokens": ["Der", "Him\u00b7mel", "lie\u00df", ",", "nach\u00b7sin\u00b7nend", "sei\u00b7ner", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne l\u00e4\u00dfig fallen aus der Hand.", "tokens": ["Die", "Son\u00b7ne", "l\u00e4\u00b7\u00dfig", "fal\u00b7len", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}