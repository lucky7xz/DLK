{"dta.poem.13998": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schalk .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und nun wehen Lerchenlieder", "tokens": ["Und", "nun", "we\u00b7hen", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es schl\u00e4gt die Nachtigall,", "tokens": ["Und", "es", "schl\u00e4gt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauschend von den Bergen nieder", "tokens": ["Rau\u00b7schend", "von", "den", "Ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt der k\u00fchle Wasserfall,", "tokens": ["Kommt", "der", "k\u00fch\u00b7le", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rings im Walde bunt Gefieder: \u2014", "tokens": ["Rings", "im", "Wal\u00b7de", "bunt", "Ge\u00b7fie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchling, Fr\u00fchling ist es wieder", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "ist", "es", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Jauchzen \u00fcberall.", "tokens": ["Und", "ein", "Jauch\u00b7zen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}