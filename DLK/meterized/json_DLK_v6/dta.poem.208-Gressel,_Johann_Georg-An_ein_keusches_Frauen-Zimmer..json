{"dta.poem.208": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ein keusches Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll mein schwacher Kiel hier vor ein Opffer bringen/", "tokens": ["Was", "soll", "mein", "schwa\u00b7cher", "Kiel", "hier", "vor", "ein", "Opf\u00b7fer", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allwo die Schmeichelung nicht darf zu finden seyn?", "tokens": ["All\u00b7wo", "die", "Schmei\u00b7che\u00b7lung", "nicht", "darf", "zu", "fin\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VMFIN", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nicht die Lustbarkeit darf aus der Feder dringen/", "tokens": ["Wo", "nicht", "die", "Lust\u00b7bar\u00b7keit", "darf", "aus", "der", "Fe\u00b7der", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch nicht die matte Hand darf ", "tokens": ["Auch", "nicht", "die", "mat\u00b7te", "Hand", "darf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich mu\u00df ein reines Blatt vor ihre F\u00fcsse legen/", "tokens": ["Ich", "mu\u00df", "ein", "rei\u00b7nes", "Blatt", "vor", "ih\u00b7re", "F\u00fcs\u00b7se", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das kein ", "tokens": ["Das", "kein"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die Zeilen d\u00fcrffen nicht verdeckte Gallen hegen/", "tokens": ["Die", "Zei\u00b7len", "d\u00fcrf\u00b7fen", "nicht", "ver\u00b7deck\u00b7te", "Gal\u00b7len", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als die von ferne flieht ihr tugendhaffter Geist.", "tokens": ["Als", "die", "von", "fer\u00b7ne", "flieht", "ihr", "tu\u00b7gend\u00b7haff\u00b7ter", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Derhalben lieffer ich nur diese schlechte Riegen/", "tokens": ["Der\u00b7hal\u00b7ben", "lief\u00b7fer", "ich", "nur", "die\u00b7se", "schlech\u00b7te", "Rie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und zahle meine Schuld mit blossen Worten ab/", "tokens": ["Und", "zah\u00b7le", "mei\u00b7ne", "Schuld", "mit", "blos\u00b7sen", "Wor\u00b7ten", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Unverm\u00f6gen l\u00e4\u00dft von mir nichts bessers kriegen/", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "l\u00e4\u00dft", "von", "mir", "nichts", "bes\u00b7sers", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil ich den ", "tokens": ["Weil", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ich bin auf dem ", "tokens": ["Ich", "bin", "auf", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.14": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "So hat auch ", "tokens": ["So", "hat", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Durch angebohrnen Witz und Zierde nicht ge\u00e4tzt.", "tokens": ["Durch", "an\u00b7ge\u00b7bohr\u00b7nen", "Witz", "und", "Zier\u00b7de", "nicht", "ge\u00b7\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich bringe meine Pflicht nicht in geschminckten Worten/", "tokens": ["Ich", "brin\u00b7ge", "mei\u00b7ne", "Pflicht", "nicht", "in", "ge\u00b7schminck\u00b7ten", "Wor\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nicht unter Prahlerey der schn\u00f6den Eitelkeit/", "tokens": ["Nicht", "un\u00b7ter", "Prah\u00b7le\u00b7rey", "der", "schn\u00f6\u00b7den", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich tiesche schwartzes Brodt nicht angemachte Torten", "tokens": ["Ich", "tie\u00b7sche", "schwart\u00b7zes", "Brodt", "nicht", "an\u00b7ge\u00b7mach\u00b7te", "Tor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von Wind und Wasser auf/ die wol ein ander weyht.", "tokens": ["Von", "Wind", "und", "Was\u00b7ser", "auf", "/", "die", "wol", "ein", "an\u00b7der", "weyht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "$(", "ART", "ADV", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein ander prange sehr mit grossen ", "tokens": ["Ein", "an\u00b7der", "pran\u00b7ge", "sehr", "mit", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und mit get\u00fcnchter Pracht der falschen Glei\u00dfnerey/", "tokens": ["Und", "mit", "ge\u00b7t\u00fcnch\u00b7ter", "Pracht", "der", "fal\u00b7schen", "Glei\u00df\u00b7ne\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er mache sich ber\u00fchmt mit seines Maules-Renten", "tokens": ["Er", "ma\u00b7che", "sich", "be\u00b7r\u00fchmt", "mit", "sei\u00b7nes", "Mau\u00b7les\u00b7Ren\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So zeigt sich doch zuletzt/ da\u00df er ein L\u00fcgner sey.", "tokens": ["So", "zeigt", "sich", "doch", "zu\u00b7letzt", "/", "da\u00df", "er", "ein", "L\u00fcg\u00b7ner", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Kan ich nicht Adlern gleich bis an die Wolcken tragen", "tokens": ["Kan", "ich", "nicht", "Ad\u00b7lern", "gleich", "bis", "an", "die", "Wol\u00b7cken", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr zugeh\u00f6rigs Lob/ und mu\u00df ich niedrig gehn/", "tokens": ["Ihr", "zu\u00b7ge\u00b7h\u00f6\u00b7rigs", "Lob", "/", "und", "mu\u00df", "ich", "nied\u00b7rig", "gehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So will ich Schwalben gleich es durch die Gassen jagen", "tokens": ["So", "will", "ich", "Schwal\u00b7ben", "gleich", "es", "durch", "die", "Gas\u00b7sen", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn wird die ", "tokens": ["Denn", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Fehlt hier ", "tokens": ["Fehlt", "hier"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Wird nicht ein ", "tokens": ["Wird", "nicht", "ein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Der seines Nahmens Ruhm bis an die Wolcken schwunge", "tokens": ["Der", "sei\u00b7nes", "Nah\u00b7mens", "Ruhm", "bis", "an", "die", "Wol\u00b7cken", "schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "F\u00fchrt mich ein ", "tokens": ["F\u00fchrt", "mich", "ein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Singt nicht ein kluger Schwaan hier seine Wunder-Lieder/", "tokens": ["Singt", "nicht", "ein", "klu\u00b7ger", "Schwaan", "hier", "sei\u00b7ne", "Wun\u00b7der\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So wird ihr doch die Gan\u00df nicht gar entgegen seyn.", "tokens": ["So", "wird", "ihr", "doch", "die", "Gan\u00df", "nicht", "gar", "ent\u00b7ge\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es trotzt ein Stammler auch bey ihren Gunst-Gefieder/", "tokens": ["Es", "trotzt", "ein", "Stamm\u00b7ler", "auch", "bey", "ih\u00b7ren", "Gunst\u00b7Ge\u00b7fie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bey ihr mu\u00df ", "tokens": ["Bey", "ihr", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Doch endlich auch einmahl beym End Zweck anzulangen/", "tokens": ["Doch", "end\u00b7lich", "auch", "ein\u00b7mahl", "beym", "End", "Zweck", "an\u00b7zu\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPRART", "NN", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So ziert den sch\u00f6nen Leib auch ein beliebter Geist/", "tokens": ["So", "ziert", "den", "sch\u00f6\u00b7nen", "Leib", "auch", "ein", "be\u00b7lieb\u00b7ter", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.41": {"text": "Die Augen blitzen Gluht/ die Wangen zeugen Rosen/", "tokens": ["Die", "Au\u00b7gen", "blit\u00b7zen", "Gluht", "/", "die", "Wan\u00b7gen", "zeu\u00b7gen", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Darinnen zarte Milch nett untermischet ist/", "tokens": ["Da\u00b7rin\u00b7nen", "zar\u00b7te", "Milch", "nett", "un\u00b7ter\u00b7mi\u00b7schet", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die Adern quillen auf und gleichen den Turckosen/", "tokens": ["Die", "A\u00b7dern", "quil\u00b7len", "auf", "und", "glei\u00b7chen", "den", "Tur\u00b7cko\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJA", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die ihre Glieder-Schnee in h\u00f6chster Anmuth k\u00fc\u00dft.", "tokens": ["Die", "ih\u00b7re", "Glie\u00b7der\u00b7Schnee", "in", "h\u00f6chs\u00b7ter", "An\u00b7muth", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dis hat ein treuer Knecht in Eile aufgesetzet/", "tokens": ["Dis", "hat", "ein", "treu\u00b7er", "Knecht", "in", "Ei\u00b7le", "auf\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und mit gezwungner Macht den stumpffen Kiel beseelt/", "tokens": ["Und", "mit", "ge\u00b7zwung\u00b7ner", "Macht", "den", "stumpf\u00b7fen", "Kiel", "be\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Da\u00df er sich/ doch mit M\u00fch/ in ", "tokens": ["Da\u00df", "er", "sich", "/", "doch", "mit", "M\u00fch", "/", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "$(", "ADV", "APPR", "NN", "$(", "APPR"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.48": {"text": "Und diese schlechte Schrifft mit dem Papier verm\u00e4hlt.", "tokens": ["Und", "die\u00b7se", "schlech\u00b7te", "Schrifft", "mit", "dem", "Pa\u00b7pier", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}