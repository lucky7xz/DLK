{"textgrid.poem.28163": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Pest, die Ehrensucht sind beyde strenges Gifft,", "tokens": ["Die", "Pest", ",", "die", "Eh\u00b7ren\u00b7sucht", "sind", "bey\u00b7de", "stren\u00b7ges", "Gifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die nur meistens hoch, und jene nieder trifft.", "tokens": ["Da\u00df", "die", "nur", "meis\u00b7tens", "hoch", ",", "und", "je\u00b7ne", "nie\u00b7der", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "ADJD", "$,", "KON", "PDS", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Ehre henckt man nach; die Pest fleucht iederman,", "tokens": ["Der", "Eh\u00b7re", "henckt", "man", "nach", ";", "die", "Pest", "fleucht", "ie\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob die der Welt gleich nicht, wie jene schaden kan.", "tokens": ["Ob", "die", "der", "Welt", "gleich", "nicht", ",", "wie", "je\u00b7ne", "scha\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "PTKNEG", "$,", "PWAV", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}