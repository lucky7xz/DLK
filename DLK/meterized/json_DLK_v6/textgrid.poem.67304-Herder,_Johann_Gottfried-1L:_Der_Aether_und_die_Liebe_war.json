{"textgrid.poem.67304": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Aether und die Liebe war", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Aether und die Liebe war", "tokens": ["Der", "A\u00b7e\u00b7ther", "und", "die", "Lie\u00b7be", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Das \u00e4ltste hohe G\u00f6tterpaar;", "tokens": ["Das", "\u00e4lts\u00b7te", "ho\u00b7he", "G\u00f6t\u00b7ter\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zeugten die Unsterblichen,", "tokens": ["Sie", "zeug\u00b7ten", "die", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Den Himmel und die Seligen.", "tokens": ["Den", "Him\u00b7mel", "und", "die", "Se\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und tiefer in der Wolken Reich", "tokens": ["Und", "tie\u00b7fer", "in", "der", "Wol\u00b7ken", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ihr Geschlecht der Wolke gleich;", "tokens": ["Ward", "ihr", "Ge\u00b7schlecht", "der", "Wol\u00b7ke", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie, ewig sch\u00f6n und ewig jung,", "tokens": ["Sie", ",", "e\u00b7wig", "sch\u00f6n", "und", "e\u00b7wig", "jung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erzeugten uns die D\u00e4mmerung.", "tokens": ["Er\u00b7zeug\u00b7ten", "uns", "die", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Aus Licht und Schatten webten sie", "tokens": ["Aus", "Licht", "und", "Schat\u00b7ten", "web\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschen t\u00e4uschend Dasein hie;", "tokens": ["Der", "Men\u00b7schen", "t\u00e4u\u00b7schend", "Da\u00b7sein", "hie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur D\u00e4mmerung ist unser Blick,", "tokens": ["Nur", "D\u00e4m\u00b7me\u00b7rung", "ist", "un\u00b7ser", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur D\u00e4mmerung ist unser Gl\u00fcck.", "tokens": ["Nur", "D\u00e4m\u00b7me\u00b7rung", "ist", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Jugend holdes Morgenroth", "tokens": ["Der", "Ju\u00b7gend", "hol\u00b7des", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbirget, was der Tag uns droht;", "tokens": ["Ver\u00b7bir\u00b7get", ",", "was", "der", "Tag", "uns", "droht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Blume schw\u00fclen Mittag k\u00fchlt", "tokens": ["Der", "Blu\u00b7me", "schw\u00fc\u00b7len", "Mit\u00b7tag", "k\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Zephyr, der am Abend spielt.", "tokens": ["Ein", "Ze\u00b7phyr", ",", "der", "am", "A\u00b7bend", "spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Ohr und Auge t\u00e4uscht sich gern;", "tokens": ["Und", "Ohr", "und", "Au\u00b7ge", "t\u00e4uscht", "sich", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz, es pochet in die Fern',", "tokens": ["Das", "Herz", ",", "es", "po\u00b7chet", "in", "die", "Fern'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnscht und hat, und glaubet's kaum;", "tokens": ["Und", "w\u00fcnscht", "und", "hat", ",", "und", "glau\u00b7bet's", "kaum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VAFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn auch sein sch\u00f6nstes Gl\u00fcck ist Traum.", "tokens": ["Denn", "auch", "sein", "sch\u00f6ns\u00b7tes", "Gl\u00fcck", "ist", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Hoffnung, ewig sch\u00f6n und jung,", "tokens": ["Die", "Hoff\u00b7nung", ",", "e\u00b7wig", "sch\u00f6n", "und", "jung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist uns ein Kind der D\u00e4mmerung;", "tokens": ["Ist", "uns", "ein", "Kind", "der", "D\u00e4m\u00b7me\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auch ihre Schwester, Sehnsucht, liebt", "tokens": ["Auch", "ih\u00b7re", "Schwes\u00b7ter", ",", "Sehn\u00b7sucht", ",", "liebt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schleier, der die Lieb' umgiebt.", "tokens": ["Den", "Schlei\u00b7er", ",", "der", "die", "Lieb'", "um\u00b7giebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich dank' Euch, die Ihr um mich schwebt,", "tokens": ["Ich", "dank'", "Euch", ",", "die", "Ihr", "um", "mich", "schwebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Ihr die H\u00fclle mir gewebt;", "tokens": ["Da\u00df", "Ihr", "die", "H\u00fcl\u00b7le", "mir", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Lieb' und Aether, leiht, o leiht", "tokens": ["Doch", "Lieb'", "und", "A\u00b7e\u00b7ther", ",", "leiht", ",", "o", "leiht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "VVFIN", "$,", "FM", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir einst ein heller Pilgerkleid!", "tokens": ["Mir", "einst", "ein", "hel\u00b7ler", "Pil\u00b7ger\u00b7kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}