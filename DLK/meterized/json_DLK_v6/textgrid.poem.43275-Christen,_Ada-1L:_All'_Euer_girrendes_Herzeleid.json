{"textgrid.poem.43275": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: All' Euer girrendes Herzeleid", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "All' Euer girrendes Herzeleid", "tokens": ["All'", "Eu\u00b7er", "gir\u00b7ren\u00b7des", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Thut lange nicht so weh,", "tokens": ["Thut", "lan\u00b7ge", "nicht", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Winterk\u00e4lte im d\u00fcnnen Kleid,", "tokens": ["Wie", "Win\u00b7ter\u00b7k\u00e4l\u00b7te", "im", "d\u00fcn\u00b7nen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die blo\u00dfen F\u00fc\u00dfe im Schnee.", "tokens": ["Die", "blo\u00b7\u00dfen", "F\u00fc\u00b7\u00dfe", "im", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "All' Eure romantische Seelennoth", "tokens": ["All'", "Eu\u00b7re", "ro\u00b7man\u00b7ti\u00b7sche", "See\u00b7len\u00b7noth"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Schafft nicht so herbe Pein,", "tokens": ["Schafft", "nicht", "so", "her\u00b7be", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ohne Dach und ohne Brod", "tokens": ["Wie", "oh\u00b7ne", "Dach", "und", "oh\u00b7ne", "Brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich betten auf einen Stein.", "tokens": ["Sich", "bet\u00b7ten", "auf", "ei\u00b7nen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}