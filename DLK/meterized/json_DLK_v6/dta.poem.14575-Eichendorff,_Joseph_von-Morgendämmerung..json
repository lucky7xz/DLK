{"dta.poem.14575": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgend\u00e4mmerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O fromme V\u00f6glein, ihr und ich, wir warten", "tokens": ["O", "from\u00b7me", "V\u00f6\u00b7glein", ",", "ihr", "und", "ich", ",", "wir", "war\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "KON", "PPER", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf's frohe Licht, da ist uns vor Verlangen", "tokens": ["Auf's", "fro\u00b7he", "Licht", ",", "da", "ist", "uns", "vor", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei stiller Nacht erwacht so sehnend Singen.", "tokens": ["Bei", "stil\u00b7ler", "Nacht", "er\u00b7wacht", "so", "seh\u00b7nend", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}