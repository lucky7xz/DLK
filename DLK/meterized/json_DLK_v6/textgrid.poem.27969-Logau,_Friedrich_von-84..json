{"textgrid.poem.27969": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott, wo nem ich Danck, der ich so viel genummen", "tokens": ["O", "Gott", ",", "wo", "nem", "ich", "Danck", ",", "der", "ich", "so", "viel", "ge\u00b7num\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "PIS", "PPER", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Von Wolthat, die mir ist zu Hause h\u00e4uffig kummen", "tokens": ["Von", "Wolt\u00b7hat", ",", "die", "mir", "ist", "zu", "Hau\u00b7se", "h\u00e4uf\u00b7fig", "kum\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VAFIN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch deine G\u00fctigkeit? Thust du nicht mehr hier wol,", "tokens": ["Durch", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", "?", "Thust", "du", "nicht", "mehr", "hier", "wol", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So wei\u00df ich keinen Rath, wie recht ich dancken sol!", "tokens": ["So", "wei\u00df", "ich", "kei\u00b7nen", "Rath", ",", "wie", "recht", "ich", "dan\u00b7cken", "sol", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}