{"textgrid.poem.41586": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am letzten Tage des Jahres", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Jahr geht um,", "tokens": ["Das", "Jahr", "geht", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Faden rollt sich sausend ab.", "tokens": ["Der", "Fa\u00b7den", "rollt", "sich", "sau\u00b7send", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein St\u00fcndchen noch, das letzte heut,", "tokens": ["Ein", "St\u00fcnd\u00b7chen", "noch", ",", "das", "letz\u00b7te", "heut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00e4ubend rieselt in sein Grab", "tokens": ["Und", "st\u00e4u\u00b7bend", "rie\u00b7selt", "in", "sein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was einstens war lebend'ge Zeit.", "tokens": ["Was", "eins\u00b7tens", "war", "le\u00b7ben\u00b7d'\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich harre stumm.", "tokens": ["Ich", "har\u00b7re", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "'s ist tiefe Nacht!", "tokens": ["'s", "ist", "tie\u00b7fe", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ob wohl ein Auge offen noch?", "tokens": ["Ob", "wohl", "ein", "Au\u00b7ge", "of\u00b7fen", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesen Mauern r\u00fcttelt dein", "tokens": ["In", "die\u00b7sen", "Mau\u00b7ern", "r\u00fct\u00b7telt", "dein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verrinnen, Zeit! Mir schaudert, doch", "tokens": ["Ver\u00b7rin\u00b7nen", ",", "Zeit", "!", "Mir", "schau\u00b7dert", ",", "doch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es will die letzte Stunde sein", "tokens": ["Es", "will", "die", "letz\u00b7te", "Stun\u00b7de", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einsam durchwacht.", "tokens": ["Ein\u00b7sam", "durch\u00b7wacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Gesehen all,", "tokens": ["Ge\u00b7se\u00b7hen", "all", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PIAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was ich begangen und gedacht,", "tokens": ["Was", "ich", "be\u00b7gan\u00b7gen", "und", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mir aus Haupt und Herzen stieg:", "tokens": ["Was", "mir", "aus", "Haupt", "und", "Her\u00b7zen", "stieg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das steht nun eine ernste Wacht", "tokens": ["Das", "steht", "nun", "ei\u00b7ne", "erns\u00b7te", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Himmelstor. O halber Sieg,", "tokens": ["Am", "Him\u00b7mels\u00b7tor", ".", "O", "hal\u00b7ber", "Sieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O schwerer Fall!", "tokens": ["O", "schwe\u00b7rer", "Fall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie rei\u00dft der Wind", "tokens": ["Wie", "rei\u00dft", "der", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Am Fensterkreuze, ja es will", "tokens": ["Am", "Fens\u00b7ter\u00b7kreu\u00b7ze", ",", "ja", "es", "will"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Sturmesfittigen das Jahr", "tokens": ["Auf", "Stur\u00b7mes\u00b7fit\u00b7ti\u00b7gen", "das", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerst\u00e4uben, nicht ein Schatten still", "tokens": ["Zer\u00b7st\u00e4u\u00b7ben", ",", "nicht", "ein", "Schat\u00b7ten", "still"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PTKNEG", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verhauchen unterm Sternenklar.", "tokens": ["Ver\u00b7hau\u00b7chen", "un\u00b7term", "Ster\u00b7nen\u00b7klar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du S\u00fcndenkind!", "tokens": ["Du", "S\u00fcn\u00b7den\u00b7kind", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "War nicht ein hohl", "tokens": ["War", "nicht", "ein", "hohl"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und heimlich Sausen jeder Tag", "tokens": ["Und", "heim\u00b7lich", "Sau\u00b7sen", "je\u00b7der", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deiner w\u00fcsten Brust Verlies,", "tokens": ["In", "dei\u00b7ner", "w\u00fcs\u00b7ten", "Brust", "Ver\u00b7lies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo langsam Stein an Stein zerbrach,", "tokens": ["Wo", "lang\u00b7sam", "Stein", "an", "Stein", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es den kalten Odem stie\u00df", "tokens": ["Wenn", "es", "den", "kal\u00b7ten", "O\u00b7dem", "stie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom starren Pol?", "tokens": ["Vom", "star\u00b7ren", "Pol", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mein L\u00e4mpchen will", "tokens": ["Mein", "L\u00e4mp\u00b7chen", "will"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Verl\u00f6schen, und begierig saugt", "tokens": ["Ver\u00b7l\u00f6\u00b7schen", ",", "und", "be\u00b7gie\u00b7rig", "saugt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Docht den letzten Tropfen \u00d6l.", "tokens": ["Der", "Docht", "den", "letz\u00b7ten", "Trop\u00b7fen", "\u00d6l", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist so mein Leben auch verraucht,", "tokens": ["Ist", "so", "mein", "Le\u00b7ben", "auch", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er\u00f6ffnet sich des Grabes H\u00f6hl'", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", "des", "Gra\u00b7bes", "H\u00f6hl'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir schwarz und still?", "tokens": ["Mir", "schwarz", "und", "still", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wohl in dem Kreis,", "tokens": ["Wohl", "in", "dem", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den dieses Jahres Lauf umzieht,", "tokens": ["Den", "die\u00b7ses", "Jah\u00b7res", "Lauf", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Leben bricht: Ich wu\u00dft' es lang!", "tokens": ["Mein", "Le\u00b7ben", "bricht", ":", "Ich", "wu\u00dft'", "es", "lang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch hat dies Herz gegl\u00fcht", "tokens": ["Und", "den\u00b7noch", "hat", "dies", "Herz", "ge\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PDS", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In eitler Leidenschaften Drang.", "tokens": ["In", "eit\u00b7ler", "Lei\u00b7den\u00b7schaf\u00b7ten", "Drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir br\u00fcht der Schwei\u00df", "tokens": ["Mir", "br\u00fcht", "der", "Schwei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Der tiefsten Angst", "tokens": ["Der", "tiefs\u00b7ten", "Angst"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf Stirn und Hand! \u2013 Wie, d\u00e4mmert feucht", "tokens": ["Auf", "Stirn", "und", "Hand", "!", "\u2013", "Wie", ",", "d\u00e4m\u00b7mert", "feucht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$(", "PWAV", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Stern dort durch die Wolken nicht?", "tokens": ["Ein", "Stern", "dort", "durch", "die", "Wol\u00b7ken", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r' es der Liebe Stern vielleicht,", "tokens": ["W\u00e4r'", "es", "der", "Lie\u00b7be", "Stern", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ADV", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Dir z\u00fcrnend mit dem tr\u00fcben Licht,", "tokens": ["Dir", "z\u00fcr\u00b7nend", "mit", "dem", "tr\u00fc\u00b7ben", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du so bangst?", "tokens": ["Da\u00df", "du", "so", "bangst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Horch, welch Gesumm?", "tokens": ["Horch", ",", "welch", "Ge\u00b7summ", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und wieder? Sterbemelodie!", "tokens": ["Und", "wie\u00b7der", "?", "Ster\u00b7be\u00b7me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Glocke regt den ehrnen Mund.", "tokens": ["Die", "Glo\u00b7cke", "regt", "den", "ehr\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Herr! ich falle auf das Knie:", "tokens": ["O", "Herr", "!", "ich", "fal\u00b7le", "auf", "das", "Knie", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sei gn\u00e4dig meiner letzten Stund!", "tokens": ["Sei", "gn\u00e4\u00b7dig", "mei\u00b7ner", "letz\u00b7ten", "Stund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Jahr ist um!", "tokens": ["Das", "Jahr", "ist", "um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}