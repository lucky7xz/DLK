{"textgrid.poem.38294": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Maien im Maien ists lieblich und sch\u00f6n,", "tokens": ["Im", "Mai\u00b7en", "im", "Mai\u00b7en", "ists", "lieb\u00b7lich", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da finden sich viel Kurzweil und Wonn';", "tokens": ["Da", "fin\u00b7den", "sich", "viel", "Kurz\u00b7weil", "und", "Wonn'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Frau Nachtigall singet,", "tokens": ["Frau", "Nach\u00b7ti\u00b7gall", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Lerche sich schwinget", "tokens": ["Die", "Ler\u00b7che", "sich", "schwin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ueber Berg und \u00fcber Thal.", "tokens": ["Ue\u00b7ber", "Berg", "und", "\u00fc\u00b7ber", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Pforten der Erde, die schlie\u00dfen sich auf,", "tokens": ["Die", "Pfor\u00b7ten", "der", "Er\u00b7de", ",", "die", "schlie\u00b7\u00dfen", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und lassen so manches Bl\u00fcmlein herauf,", "tokens": ["Und", "las\u00b7sen", "so", "man\u00b7ches", "Bl\u00fcm\u00b7lein", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als Lilien und Rosen,", "tokens": ["Als", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Violen, Zeitlosen,", "tokens": ["Vi\u00b7o\u00b7len", ",", "Zeit\u00b7lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Cypressen und auch N\u00e4gelein.", "tokens": ["Cyp\u00b7res\u00b7sen", "und", "auch", "N\u00e4\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "In solchen wohlriechenden Bl\u00fcmlein zart,", "tokens": ["In", "sol\u00b7chen", "wohl\u00b7rie\u00b7chen\u00b7den", "Bl\u00fcm\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Spazieret eine Jungfrau von edeler Art;", "tokens": ["Spa\u00b7zie\u00b7ret", "ei\u00b7ne", "Jung\u00b7frau", "von", "e\u00b7de\u00b7ler", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie windet und bindet,", "tokens": ["Sie", "win\u00b7det", "und", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gar zierlich und fein,", "tokens": ["Gar", "zier\u00b7lich", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ihrem Herzallerliebsten ein Kr\u00e4nzelein.", "tokens": ["Ih\u00b7rem", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ten", "ein", "Kr\u00e4n\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Da herzt man, da scherzt man, da freuet man sich,", "tokens": ["Da", "herzt", "man", ",", "da", "scherzt", "man", ",", "da", "freu\u00b7et", "man", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PIS", "PRF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da singt man, da springt man, da ist man fr\u00f6hlich;", "tokens": ["Da", "singt", "man", ",", "da", "springt", "man", ",", "da", "ist", "man", "fr\u00f6h\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PIS", "$,", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da klaget ein Liebchen", "tokens": ["Da", "kla\u00b7get", "ein", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dem andern sein' Noth,", "tokens": ["Dem", "an\u00b7dern", "sein'", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da k\u00fc\u00dft man so manches M\u00fcndlein roth.", "tokens": ["Da", "k\u00fc\u00dft", "man", "so", "man\u00b7ches", "M\u00fcnd\u00b7lein", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach Scheiden, ach Scheiden, du schneidendes Schwerdt,", "tokens": ["Ach", "Schei\u00b7den", ",", "ach", "Schei\u00b7den", ",", "du", "schnei\u00b7den\u00b7des", "Schwerdt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du hast mir mein junges frisch Herzlein verkehrt.", "tokens": ["Du", "hast", "mir", "mein", "jun\u00b7ges", "frisch", "Herz\u00b7lein", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wiederkommen macht,", "tokens": ["Wie\u00b7der\u00b7kom\u00b7men", "macht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df man Scheiden nicht acht't;", "tokens": ["Da\u00df", "man", "Schei\u00b7den", "nicht", "acht't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ade, zu tausend guter Nacht.", "tokens": ["A\u00b7de", ",", "zu", "tau\u00b7send", "gu\u00b7ter", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Im Maien, im Maien, da freuet man sich,", "tokens": ["Im", "Mai\u00b7en", ",", "im", "Mai\u00b7en", ",", "da", "freu\u00b7et", "man", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "ADV", "VVFIN", "PIS", "PRF", "$,"], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da singt man, da springt man, da ist man fr\u00f6hlich,", "tokens": ["Da", "singt", "man", ",", "da", "springt", "man", ",", "da", "ist", "man", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PIS", "$,", "ADV", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da kommet so manches", "tokens": ["Da", "kom\u00b7met", "so", "man\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Liebchen zusammen;", "tokens": ["Lieb\u00b7chen", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ade, in tausend Gottes Namen.", "tokens": ["A\u00b7de", ",", "in", "tau\u00b7send", "Got\u00b7tes", "Na\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "CARD", "NN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}