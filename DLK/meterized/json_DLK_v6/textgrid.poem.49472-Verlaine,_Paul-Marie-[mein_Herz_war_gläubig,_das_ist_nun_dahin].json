{"textgrid.poem.49472": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[mein Herz war gl\u00e4ubig, das ist nun dahin]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz war gl\u00e4ubig, das ist nun dahin,", "tokens": ["Mein", "Herz", "war", "gl\u00e4u\u00b7big", ",", "das", "ist", "nun", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PDS", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Und wiederum erf\u00fcllt das Weib mich ganz,", "tokens": ["Und", "wie\u00b7de\u00b7rum", "er\u00b7f\u00fcllt", "das", "Weib", "mich", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich auch noch voll tiefer Ehrfurcht bin", "tokens": ["Wenn", "ich", "auch", "noch", "voll", "tie\u00b7fer", "Ehr\u00b7furcht", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr des verlor'nen Bildes Zauberglanz,", "tokens": ["F\u00fcr", "des", "ver\u00b7lor'\u00b7nen", "Bil\u00b7des", "Zau\u00b7ber\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jedoch, das Weib erf\u00fcllt mich wieder ganz.", "tokens": ["Je\u00b7doch", ",", "das", "Weib", "er\u00b7f\u00fcllt", "mich", "wie\u00b7der", "ganz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich betete zum Gott der Kinderzeiten,", "tokens": ["Ich", "be\u00b7te\u00b7te", "zum", "Gott", "der", "Kin\u00b7der\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch heute kniee ich vor dir allein;", "tokens": ["Doch", "heu\u00b7te", "kni\u00b7ee", "ich", "vor", "dir", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach gl\u00e4ub'ges Mitleid, lichte Hoffnung weihten", "tokens": ["Ach", "gl\u00e4ub'\u00b7ges", "Mit\u00b7leid", ",", "lich\u00b7te", "Hoff\u00b7nung", "weih\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir die ergl\u00fch'nde Seele zart und rein.", "tokens": ["Mir", "die", "er\u00b7gl\u00fch'n\u00b7de", "See\u00b7le", "zart", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch heute kniee ich vor dir allein.", "tokens": ["Doch", "heu\u00b7te", "kni\u00b7ee", "ich", "vor", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von neuem wird durch dich das Weib zum Herren,", "tokens": ["Von", "neu\u00b7em", "wird", "durch", "dich", "das", "Weib", "zum", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mir allm\u00e4chtig jede Freiheit raubte,", "tokens": ["Der", "mir", "all\u00b7m\u00e4ch\u00b7tig", "je\u00b7de", "Frei\u00b7heit", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der t\u00fcckisch ins Verderben mich zu zerren", "tokens": ["Der", "t\u00fc\u00b7ckisch", "ins", "Ver\u00b7der\u00b7ben", "mich", "zu", "zer\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Hinterlist mir jeden Wunsch erlaubte ...", "tokens": ["Voll", "Hin\u00b7ter\u00b7list", "mir", "je\u00b7den", "Wunsch", "er\u00b7laub\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O sel'ge Zeiten, da mein Herz noch glaubte!", "tokens": ["O", "sel'\u00b7ge", "Zei\u00b7ten", ",", "da", "mein", "Herz", "noch", "glaub\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}