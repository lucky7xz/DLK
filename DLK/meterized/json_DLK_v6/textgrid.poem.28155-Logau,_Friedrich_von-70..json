{"textgrid.poem.28155": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veit schonet seinen Leib, schont aber nicht sein Geld,", "tokens": ["Veit", "scho\u00b7net", "sei\u00b7nen", "Leib", ",", "schont", "a\u00b7ber", "nicht", "sein", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da doch sein liebes Weib von beyden wenig h\u00e4lt.", "tokens": ["Da", "doch", "sein", "lie\u00b7bes", "Weib", "von", "bey\u00b7den", "we\u00b7nig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er soll seyn sparsam hier; dort soll er offt und viel", "tokens": ["Er", "soll", "seyn", "spar\u00b7sam", "hier", ";", "dort", "soll", "er", "offt", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebrauchen seinen Leib zu ihrem, wan sie wil.", "tokens": ["Ge\u00b7brau\u00b7chen", "sei\u00b7nen", "Leib", "zu", "ih\u00b7rem", ",", "wan", "sie", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}