{"dta.poem.6644": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es unterscheiden sich die Seelen,", "tokens": ["Es", "un\u00b7ter\u00b7schei\u00b7den", "sich", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erniedrigt durch des C\u00f6rpers Druck-Gewicht,", "tokens": ["Er\u00b7nied\u00b7rigt", "durch", "des", "C\u00f6r\u00b7pers", "Druck\u00b7Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ihrer Sinnen Werckzeug nicht,", "tokens": ["Von", "ih\u00b7rer", "Sin\u00b7nen", "Wer\u00b7ck\u00b7zeug", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn wir, voll Unvernunft, uns falsche Schl\u00fcsse wehlen:", "tokens": ["Wenn", "wir", ",", "voll", "Un\u00b7ver\u00b7nunft", ",", "uns", "fal\u00b7sche", "Schl\u00fcs\u00b7se", "weh\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kan, spricht man, GOTT der HERR und SCHOEPF-", "tokens": ["Kan", ",", "spricht", "man", ",", "GoTT", "der", "HeRR", "und", "ScHO\u00b7EPF"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "$,", "VVFIN", "PIS", "$,", "NE", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "FeR aller Sachen", "tokens": ["FeR", "al\u00b7ler", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nicht C\u00f6rper, welche dencken, machen?", "tokens": ["Nicht", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "den\u00b7cken", ",", "ma\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PRELS", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja, GOTT der HERR formirt nach Seinem Willen", "tokens": ["Ja", ",", "GoTT", "der", "HeRR", "for\u00b7mirt", "nach", "Sei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sowol die C\u00f6rper, als Gedancken:", "tokens": ["So\u00b7wol", "die", "C\u00f6r\u00b7per", ",", "als", "Ge\u00b7dan\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch beyde bleiben stets, Sein Wollen zu erf\u00fcllen,", "tokens": ["Doch", "bey\u00b7de", "blei\u00b7ben", "stets", ",", "Sein", "Wol\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In ihren angewiesnen Schrancken.", "tokens": ["In", "ih\u00b7ren", "an\u00b7ge\u00b7wi\u00b7es\u00b7nen", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Von denen von Natur verschiednen Eigenschafften", "tokens": ["Von", "de\u00b7nen", "von", "Na\u00b7tur", "ver\u00b7schied\u00b7nen", "Ei\u00b7gen\u00b7schaff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und Kr\u00e4fften wird man nimmer sehn", "tokens": ["Und", "Kr\u00e4ff\u00b7ten", "wird", "man", "nim\u00b7mer", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die eine Art zur andern \u00fcbergehn.", "tokens": ["Die", "ei\u00b7ne", "Art", "zur", "an\u00b7dern", "\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ob die Gedancken gleich sich an die C\u00f6rper hangen;", "tokens": ["Ob", "die", "Ge\u00b7dan\u00b7cken", "gleich", "sich", "an", "die", "C\u00f6r\u00b7per", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So k\u00f6nnen C\u00f6rper doch zum Dencken nie gelangen.", "tokens": ["So", "k\u00f6n\u00b7nen", "C\u00f6r\u00b7per", "doch", "zum", "Den\u00b7cken", "nie", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die GOTTHEJT will Sich Selbst nicht widersprechen", "tokens": ["Die", "GoTTHEJT", "will", "Sich", "Selbst", "nicht", "wi\u00b7der\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "In Jhren Ordnungen den Schlu\u00df zu unterbrechen.", "tokens": ["In", "Ih\u00b7ren", "Ord\u00b7nun\u00b7gen", "den", "Schlu\u00df", "zu", "un\u00b7ter\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In beyden bleibet Jhr zugleich", "tokens": ["In", "bey\u00b7den", "blei\u00b7bet", "Ihr", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Jhr unver\u00e4nderliches Reich.", "tokens": ["Ihr", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7ches", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es bleibet c\u00f6rperlich, was GOTT zum C\u00f6rper macht,", "tokens": ["Es", "blei\u00b7bet", "c\u00f6r\u00b7per\u00b7lich", ",", "was", "GoTT", "zum", "C\u00f6r\u00b7per", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PRELS", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und geistig, so er nicht aus Stoff hervorgebracht.", "tokens": ["Und", "geis\u00b7tig", ",", "so", "er", "nicht", "aus", "Stoff", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}