{"dta.poem.19101": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIII.   Auf seiner Gemahlin 29. Ge-  \n burts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Herr JEsu! hier ist eine Schaar versammlet,", "tokens": ["Herr", "Je\u00b7su", "!", "hier", "ist", "ei\u00b7ne", "Schaar", "ver\u00b7samm\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die beten, und zugleich gebieten kan;", "tokens": ["Die", "be\u00b7ten", ",", "und", "zu\u00b7gleich", "ge\u00b7bie\u00b7ten", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn also ist die Kraft, gleich wie der Mann:", "tokens": ["Denn", "al\u00b7so", "ist", "die", "Kraft", ",", "gleich", "wie", "der", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ob dabey die \u00e4u\u00dfre Zunge stammlet,", "tokens": ["Und", "ob", "da\u00b7bey", "die", "\u00e4u\u00df\u00b7re", "Zun\u00b7ge", "stamm\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ist der innre Mensch ein k\u00fchner Held,", "tokens": ["So", "ist", "der", "inn\u00b7re", "Mensch", "ein", "k\u00fch\u00b7ner", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der sich getrost dem HErrn vors Hertze stellt.", "tokens": ["Der", "sich", "ge\u00b7trost", "dem", "Herrn", "vors", "Hert\u00b7ze", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ART", "NN", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Nahme wird hier munter angeschrien,", "tokens": ["Dein", "Nah\u00b7me", "wird", "hier", "mun\u00b7ter", "an\u00b7ge\u00b7schri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Hertze wird zu uns hinab ger\u00fcckt,", "tokens": ["Dein", "Hert\u00b7ze", "wird", "zu", "uns", "hin\u00b7ab", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Geist wird gegen unsern Geist verz\u00fcckt,", "tokens": ["Dein", "Geist", "wird", "ge\u00b7gen", "un\u00b7sern", "Geist", "ver\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe l\u00e4st sich gerne niederziehen,", "tokens": ["Die", "Lie\u00b7be", "l\u00e4st", "sich", "ger\u00b7ne", "nie\u00b7der\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man ist es so an ihrer Art gewohnt,", "tokens": ["Man", "ist", "es", "so", "an", "ih\u00b7rer", "Art", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Seitdem sie mitten unter uns gethront.", "tokens": ["Seit\u00b7dem", "sie", "mit\u00b7ten", "un\u00b7ter", "uns", "ge\u00b7thront", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Komm, Lieber! komm, und giesse deine Sch\u00e4tze", "tokens": ["Komm", ",", "Lie\u00b7ber", "!", "komm", ",", "und", "gies\u00b7se", "dei\u00b7ne", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "$.", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gieb ihr, da\u00df sie aus Kraft in Kr\u00e4fte geh,", "tokens": ["Gieb", "ihr", ",", "da\u00df", "sie", "aus", "Kraft", "in", "Kr\u00e4f\u00b7te", "geh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sich durch dich bald \u00fcber alles setze;", "tokens": ["Und", "sich", "durch", "dich", "bald", "\u00fc\u00b7ber", "al\u00b7les", "set\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gieb ihr, da\u00df sie sich deiner freuen mag,", "tokens": ["Gieb", "ihr", ",", "da\u00df", "sie", "sich", "dei\u00b7ner", "freu\u00b7en", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mache ihr den Tag zum Ruhe-Tag!", "tokens": ["Und", "ma\u00b7che", "ihr", "den", "Tag", "zum", "Ru\u00b7he\u00b7Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist uns Ernst um dieser Seele willen:", "tokens": ["Es", "ist", "uns", "Ernst", "um", "die\u00b7ser", "See\u00b7le", "wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir lieben sie und ihre H\u00fctte auch.", "tokens": ["Wir", "lie\u00b7ben", "sie", "und", "ih\u00b7re", "H\u00fct\u00b7te", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey unserm Ernst ist deiner seits der Brauch,", "tokens": ["Bey", "un\u00b7serm", "Ernst", "ist", "dei\u00b7ner", "seits", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du ihn pflegst in Gnaden zu erf\u00fcllen.", "tokens": ["Da\u00df", "du", "ihn", "pflegst", "in", "Gna\u00b7den", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hie hast du sie nach Geiste, Seel und Leib,", "tokens": ["Hie", "hast", "du", "sie", "nach", "Geis\u00b7te", ",", "Seel", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir woll\u2019n, da\u00df alles noch beysammen bleib\u2019!", "tokens": ["Wir", "woll'n", ",", "da\u00df", "al\u00b7les", "noch", "bey\u00b7sam\u00b7men", "bleib'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PIS", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du hast ihr erst ein Pfand der Treu genommen.", "tokens": ["Du", "hast", "ihr", "erst", "ein", "Pfand", "der", "Treu", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie k\u00fcssete mit Thr\u00e4nen diesen Ruf;", "tokens": ["Sie", "k\u00fcs\u00b7se\u00b7te", "mit", "Thr\u00e4\u00b7nen", "die\u00b7sen", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie wuste wol, da\u00df, der die Seele schuf,", "tokens": ["Sie", "wus\u00b7te", "wol", ",", "da\u00df", ",", "der", "die", "See\u00b7le", "schuf", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch Macht hat, da\u00df er sie l\u00e4\u00dft wiederkommen,", "tokens": ["Auch", "Macht", "hat", ",", "da\u00df", "er", "sie", "l\u00e4\u00dft", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie h\u00e4tte dirs auch mit der Zeit verg\u00f6nnt,", "tokens": ["Sie", "h\u00e4t\u00b7te", "dirs", "auch", "mit", "der", "Zeit", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie es erst zu Wucher angewendt.", "tokens": ["Wenn", "sie", "es", "erst", "zu", "Wu\u00b7cher", "an\u00b7ge\u00b7wendt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da hast du es: die Zeit war nicht vorhanden,", "tokens": ["Da", "hast", "du", "es", ":", "die", "Zeit", "war", "nicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man es h\u00e4tt zum Wechsler ausgethan;", "tokens": ["Da\u00df", "man", "es", "h\u00e4tt", "zum", "Wechs\u00b7ler", "aus\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nimms immerhin zu allen Gnaden an,", "tokens": ["Nimms", "im\u00b7mer\u00b7hin", "zu", "al\u00b7len", "Gna\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So eilig kan mans nicht in unsern Landen.", "tokens": ["So", "ei\u00b7lig", "kan", "mans", "nicht", "in", "un\u00b7sern", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weist aber du, was mit zu machen sey;", "tokens": ["Weist", "a\u00b7ber", "du", ",", "was", "mit", "zu", "ma\u00b7chen", "sey", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wuchre selbst damit, du hast es frey.", "tokens": ["So", "wuch\u00b7re", "selbst", "da\u00b7mit", ",", "du", "hast", "es", "frey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nur schencke uns die zwey geliebten Kinder,", "tokens": ["Nur", "schen\u00b7cke", "uns", "die", "zwey", "ge\u00b7lieb\u00b7ten", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die du uns schon auf Tag und Jahre g\u00f6nnst,", "tokens": ["Die", "du", "uns", "schon", "auf", "Tag", "und", "Jah\u00b7re", "g\u00f6nnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du sie eh nicht von der H\u00fctte trennst,", "tokens": ["Da\u00df", "du", "sie", "eh", "nicht", "von", "der", "H\u00fct\u00b7te", "trennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "KOUS", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00df da\u00df sie erst des Fleisches Uberwinder,", "tokens": ["Bi\u00df", "da\u00df", "sie", "erst", "des", "Flei\u00b7sches", "U\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und in der That, sie seyn gleich Gro\u00df und Klein,", "tokens": ["Und", "in", "der", "That", ",", "sie", "seyn", "gleich", "Gro\u00df", "und", "Klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPER", "PPOSAT", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Erst-Geburt im Geist theilhaftig seyn.", "tokens": ["Der", "Er\u00b7stGe\u00b7burt", "im", "Geist", "theil\u00b7haf\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Gr\u00e4fin Mann, der unsere Gemeine", "tokens": ["Der", "Gr\u00e4\u00b7fin", "Mann", ",", "der", "un\u00b7se\u00b7re", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gantz br\u00fcderlich im Geist gefasset hat,", "tokens": ["Gantz", "br\u00fc\u00b7der\u00b7lich", "im", "Geist", "ge\u00b7fas\u00b7set", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem gieb in deinem Hertzen eine Statt,", "tokens": ["Dem", "gieb", "in", "dei\u00b7nem", "Hert\u00b7zen", "ei\u00b7ne", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit sein Licht recht brenne und recht scheine;", "tokens": ["Da\u00b7mit", "sein", "Licht", "recht", "bren\u00b7ne", "und", "recht", "schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wilst du, da\u00df sich dein der S\u00fcnder r\u00fchm,", "tokens": ["Und", "wilst", "du", ",", "da\u00df", "sich", "dein"