{"textgrid.poem.60288": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging an deiner Seite", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging an deiner Seite", "tokens": ["Ich", "ging", "an", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einem Buchenhaine;", "tokens": ["In", "ei\u00b7nem", "Bu\u00b7chen\u00b7hai\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein st\u00f6rendes Geleite", "tokens": ["Ein", "st\u00f6\u00b7ren\u00b7des", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lie\u00df nimmer uns alleine.", "tokens": ["Lie\u00df", "nim\u00b7mer", "uns", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und mu\u00dften wir zur\u00fccke", "tokens": ["Und", "mu\u00df\u00b7ten", "wir", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Herz die Worte pressen,", "tokens": ["Ins", "Herz", "die", "Wor\u00b7te", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns sagten unsre Blicke,", "tokens": ["Uns", "sag\u00b7ten", "uns\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df wir uns nicht vergessen.", "tokens": ["Da\u00df", "wir", "uns", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sehn wir uns nicht wieder", "tokens": ["Und", "sehn", "wir", "uns", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem Erdenleben,", "tokens": ["In", "die\u00b7sem", "Er\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich werden meine Lieder", "tokens": ["Dich", "wer\u00b7den", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verherrlichend umschweben.", "tokens": ["Ver\u00b7herr\u00b7li\u00b7chend", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das B\u00e4chlein trieb hinunter", "tokens": ["Das", "B\u00e4ch\u00b7lein", "trieb", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wellen rasche T\u00e4nze,", "tokens": ["Der", "Wel\u00b7len", "ra\u00b7sche", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und rauschend flocht und bunter", "tokens": ["Und", "rau\u00b7schend", "flocht", "und", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herbst der Wehmut Kr\u00e4nze.", "tokens": ["Der", "Herbst", "der", "Weh\u00b7mut", "Kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch aus des Walds Verd\u00fcstern,", "tokens": ["Doch", "aus", "des", "Walds", "Ver\u00b7d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Stimmen des Vergehens,", "tokens": ["Den", "Stim\u00b7men", "des", "Ver\u00b7ge\u00b7hens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt ich die Hoffnung fl\u00fcstern", "tokens": ["H\u00f6rt", "ich", "die", "Hoff\u00b7nung", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des ewgen Wiedersehens.", "tokens": ["Des", "ew\u00b7gen", "Wie\u00b7der\u00b7se\u00b7hens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}