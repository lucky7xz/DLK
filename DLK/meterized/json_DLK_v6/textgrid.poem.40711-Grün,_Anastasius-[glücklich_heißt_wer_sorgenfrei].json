{"textgrid.poem.40711": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[gl\u00fccklich hei\u00dft wer sorgenfrei]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich hei\u00dft wer sorgenfrei,", "tokens": ["Gl\u00fcck\u00b7lich", "hei\u00dft", "wer", "sor\u00b7gen\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PWS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklicher doch, mein ich, sei", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "doch", ",", "mein", "ich", ",", "sei"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "PPOSAT", "PPER", "$,", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer voll Sorgen, wenn's die rechten:", "tokens": ["Wer", "voll", "Sor\u00b7gen", ",", "wenn's", "die", "rech\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "$,", "KOUS", "ART", "ADJA", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sorgen, Andrer Leid zu mindern,", "tokens": ["Sor\u00b7gen", ",", "A\u00b7ndrer", "Leid", "zu", "min\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorgen, Unrecht zu verhindern,", "tokens": ["Sor\u00b7gen", ",", "Un\u00b7recht", "zu", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremdem Werth den Kranz zu flechten;", "tokens": ["Frem\u00b7dem", "Werth", "den", "Kranz", "zu", "flech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sorgen, in den schwersten Tagen", "tokens": ["Sor\u00b7gen", ",", "in", "den", "schwers\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fremde Sorgen selbst zu tragen.", "tokens": ["Frem\u00b7de", "Sor\u00b7gen", "selbst", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}