{"textgrid.poem.34978": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen politischen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du singst, wie einst Tyrt\u00e4us sang,", "tokens": ["Du", "singst", ",", "wie", "einst", "Tyr\u00b7t\u00e4us", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "NE", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Heldenmut beseelet,", "tokens": ["Von", "Hel\u00b7den\u00b7mut", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hast du schlecht dein Publikum", "tokens": ["Doch", "hast", "du", "schlecht", "dein", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Zeit gew\u00e4hlet.", "tokens": ["Und", "dei\u00b7ne", "Zeit", "ge\u00b7w\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Beif\u00e4llig horchen sie dir zwar,", "tokens": ["Bei\u00b7f\u00e4l\u00b7lig", "hor\u00b7chen", "sie", "dir", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und loben, schier begeistert:", "tokens": ["Und", "lo\u00b7ben", ",", "schier", "be\u00b7geis\u00b7tert", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie edel dein Gedankenflug,", "tokens": ["Wie", "e\u00b7del", "dein", "Ge\u00b7dan\u00b7ken\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du die Form bemeistert.", "tokens": ["Wie", "du", "die", "Form", "be\u00b7meis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie pflegen auch beim Glase Wein", "tokens": ["Sie", "pfle\u00b7gen", "auch", "beim", "Gla\u00b7se", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vivat dir zu bringen", "tokens": ["Ein", "Vi\u00b7vat", "dir", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manchen Schlachtgesang von dir", "tokens": ["Und", "man\u00b7chen", "Schlacht\u00b7ge\u00b7sang", "von", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lautbr\u00fcllend nachzusingen.", "tokens": ["Laut\u00b7br\u00fcl\u00b7lend", "nach\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Knecht singt gern ein Freiheitslied", "tokens": ["Der", "Knecht", "singt", "gern", "ein", "Frei\u00b7heits\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends in der Schenke:", "tokens": ["Des", "A\u00b7bends", "in", "der", "Schen\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das f\u00f6rdert die Verdauungskraft,", "tokens": ["Das", "f\u00f6r\u00b7dert", "die", "Ver\u00b7dau\u00b7ungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcrzet die Getr\u00e4nke.", "tokens": ["Und", "w\u00fcr\u00b7zet", "die", "Ge\u00b7tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}