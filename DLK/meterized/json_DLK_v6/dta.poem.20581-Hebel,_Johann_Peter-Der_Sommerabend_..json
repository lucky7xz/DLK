{"dta.poem.20581": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Sommerabend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.85", "pl:0.14"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "O, lueg doch, wie isch d' Sunn so m\u00fced,               ", "tokens": ["O", ",", "lu\u00b7eg", "doch", ",", "wie", "isch", "d'", "Sunn", "so", "m\u00fced", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "NE", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lueg, wie sie d\u2019 Heimeth abezieht!", "tokens": ["lu\u00b7eg", ",", "wie", "sie", "d'", "Hei\u00b7meth", "a\u00b7be\u00b7zieht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O lueg, wie Stral um Stral verglimmt,", "tokens": ["O", "lu\u00b7eg", ",", "wie", "Stral", "um", "Stral", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und wie sie \u2019s Fazenetli nimmt,", "tokens": ["und", "wie", "sie", "'s", "Fa\u00b7ze\u00b7net\u00b7li", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "e W\u00fclkli, blau mit roth verm\u00fcscht,", "tokens": ["e", "W\u00fclk\u00b7li", ",", "blau", "mit", "roth", "ver\u00b7m\u00fcscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und wie sie an der Stirne w\u00fcscht.", "tokens": ["und", "wie", "sie", "an", "der", "Stir\u00b7ne", "w\u00fcscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u2019s isch wohr, sie het au \u00fcbel Zit,", "tokens": ["'s", "isch", "wohr", ",", "sie", "het", "au", "\u00fc\u00b7bel", "Zit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "PPER", "VAFIN", "NE", "ADJD", "NN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "im Summer gar, der Weg isch wit,", "tokens": ["im", "Sum\u00b7mer", "gar", ",", "der", "Weg", "isch", "wit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und z\u2019 schaffe findt sie \u00fcberal", "tokens": ["und", "z'", "schaf\u00b7fe", "findt", "sie", "\u00fc\u00b7be\u00b7ral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in Hus und Feld, in Berg und Thal;", "tokens": ["in", "Hus", "und", "Feld", ",", "in", "Berg", "und", "Thal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2019s will alles Liecht und W\u00e4rmi ha,", "tokens": ["'s", "will", "al\u00b7les", "Liecht", "und", "W\u00e4r\u00b7mi", "ha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "KON", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "und spricht sie um e Segen a.", "tokens": ["und", "spricht", "sie", "um", "e", "Se\u00b7gen", "a."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Meng Bl\u00fcmli het sie usstaffirt,", "tokens": ["Meng", "Bl\u00fcm\u00b7li", "het", "sie", "us\u00b7staf\u00b7firt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und mit scharmante Farbe ziert,", "tokens": ["und", "mit", "schar\u00b7man\u00b7te", "Far\u00b7be", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "und mengem Immli z\u2019trinke ge,", "tokens": ["und", "men\u00b7gem", "I\u00b7mmli", "z'\u00b7trin\u00b7ke", "ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "NE", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "und gfrogt: Hesch gnug und witt no meh?", "tokens": ["und", "gfrogt", ":", "Hesch", "gnug", "und", "witt", "no", "meh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "ADV", "KON", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und \u2019s Ch\u00e4ferli het hinte no", "tokens": ["und", "'s", "Ch\u00e4\u00b7fer\u00b7li", "het", "hin\u00b7te", "no"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "NE", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "doch au si Tr\u00f6pfli \u00fcbercho.", "tokens": ["doch", "au", "si", "Tr\u00f6\u00b7pf\u00b7li", "\u00fc\u00b7berc\u00b7ho", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Meng Some-Ch\u00f6pfli het sie gsprengt,", "tokens": ["Meng", "So\u00b7me\u00b7Ch\u00f6pf\u00b7li", "het", "sie", "gs\u00b7prengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "und \u2019s zitig S\u00f6mli use g\u2019lengt.", "tokens": ["und", "'s", "zi\u00b7tig", "S\u00f6m\u00b7li", "u\u00b7se", "g'\u00b7lengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NE", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hen d\u2019 V\u00f6gel nit bis z\u2019 allerlezt", "tokens": ["Hen", "d'", "V\u00f6\u00b7gel", "nit", "bis", "z'", "al\u00b7ler\u00b7lezt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "PTKNEG", "APPR", "NE", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "e Bettles gha, und d\u2019 Schn\u00e4bel g\u2019wezt?", "tokens": ["e", "Bett\u00b7les", "gha", ",", "und", "d'", "Schn\u00e4\u00b7bel", "g'\u00b7wezt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "KON", "NE", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und kein goht hungerig ins Bett,", "tokens": ["Und", "kein", "goht", "hun\u00b7ge\u00b7rig", "ins", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "wo nit si Theil im Chr\u00f6pfli het.", "tokens": ["wo", "nit", "si", "Theil", "im", "Chr\u00f6pf\u00b7li", "het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "CARD", "NN", "APPRART", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wo am Baum e Chriesi lacht,", "tokens": ["Und", "wo", "am", "Baum", "e", "Chrie\u00b7si", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "se het sie\u2019m rothi B\u00e4ckli gmacht;", "tokens": ["se", "het", "sie'm", "ro\u00b7thi", "B\u00e4ck\u00b7li", "gmacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und wo im Feld en Aehri schwankt,", "tokens": ["und", "wo", "im", "Feld", "en", "A\u00b7e\u00b7hri", "schwankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und wo am Pfohl e Rebe rankt,", "tokens": ["und", "wo", "am", "Pfohl", "e", "Re\u00b7be", "rankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "se het sie eben abe glengt,", "tokens": ["se", "het", "sie", "e\u00b7ben", "a\u00b7be", "glengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "und het\u2019s mit Laub und Bluest umhengt.", "tokens": ["und", "het's", "mit", "Laub", "und", "Blu\u00b7est", "um\u00b7hengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Und uf der Bleichi het sie gschaft", "tokens": ["Und", "uf", "der", "Blei\u00b7chi", "het", "sie", "gschaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00fctie und ie us aller Chraft;", "tokens": ["h\u00fc\u00b7tie", "und", "ie", "us", "al\u00b7ler", "Chraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Bleicher het si selber gfreut,", "tokens": ["der", "Blei\u00b7cher", "het", "si", "sel\u00b7ber", "gfreut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "doch h\u00e4tt\u2019 er nit: Vergelts Gott! gseit;", "tokens": ["doch", "h\u00e4tt'", "er", "nit", ":", "Ver\u00b7gelts", "Gott", "!", "gseit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "$.", "NN", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und het e Frau ne W\u00f6schli gha,", "tokens": ["und", "het", "e", "Frau", "ne", "W\u00f6schli", "gha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "NE", "NE", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "se het sie trochnet druf und dra.", "tokens": ["se", "het", "sie", "troch\u00b7net", "druf", "und", "dra", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PAV", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u2019s isch weger wohr, und \u00fcberal", "tokens": ["'s", "isch", "we\u00b7ger", "wohr", ",", "und", "\u00fc\u00b7be\u00b7ral"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wo d\u2019 S\u00e4gesen im ganze Thal", "tokens": ["wo", "d'", "S\u00e4\u00b7ge\u00b7sen", "im", "gan\u00b7ze", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "dur Gras und Halme gangen isch,", "tokens": ["dur", "Gras", "und", "Hal\u00b7me", "gan\u00b7gen", "isch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "se het sie g\u2019heuet froh und frisch.", "tokens": ["se", "het", "sie", "g'\u00b7heu\u00b7et", "froh", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Es isch e Sach, by miner Treu,", "tokens": ["Es", "isch", "e", "Sach", ",", "by", "mi\u00b7ner", "Treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "am Morge Gras und z\u2019obe Heu!", "tokens": ["am", "Mor\u00b7ge", "Gras", "und", "z'\u00b7o\u00b7be", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Drum isch sie iez so s\u00f6lli m\u00fced,", "tokens": ["Drum", "isch", "sie", "iez", "so", "s\u00f6l\u00b7li", "m\u00fced", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und brucht zum Schlof kei Obe-Lied;", "tokens": ["und", "brucht", "zum", "Schlof", "kei", "O\u00b7be\u00b7Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "kei Wunder, wenn sie schnuft und schwizt,", "tokens": ["kei", "Wun\u00b7der", ",", "wenn", "sie", "schnuft", "und", "schwizt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "lueg wie sie d\u00f6rt uf \u2019s Bergli sizt!", "tokens": ["lu\u00b7eg", "wie", "sie", "d\u00f6rt", "uf", "'s",