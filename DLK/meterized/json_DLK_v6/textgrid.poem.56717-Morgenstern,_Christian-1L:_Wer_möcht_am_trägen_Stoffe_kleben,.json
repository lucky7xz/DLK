{"textgrid.poem.56717": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer m\u00f6cht am tr\u00e4gen Stoffe kleben,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer m\u00f6cht am tr\u00e4gen Stoffe kleben,", "tokens": ["Wer", "m\u00f6cht", "am", "tr\u00e4\u00b7gen", "Stof\u00b7fe", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem Fittich ward zu Weltenflug!", "tokens": ["dem", "Fit\u00b7tich", "ward", "zu", "Wel\u00b7ten\u00b7flug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lobe mir den s\u00fc\u00dfen Trug,", "tokens": ["Ich", "lo\u00b7be", "mir", "den", "s\u00fc\u00b7\u00dfen", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das heitre Spiel mit Welt und Leben.", "tokens": ["das", "heit\u00b7re", "Spiel", "mit", "Welt", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In tausend Buntgewande steck ich,", "tokens": ["In", "tau\u00b7send", "Bunt\u00b7ge\u00b7wan\u00b7de", "steck", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "was geistig, leiblich mich umschwebt;", "tokens": ["was", "geis\u00b7tig", ",", "leib\u00b7lich", "mich", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "in jedem Ding mich selbst entdeck ich:", "tokens": ["in", "je\u00b7dem", "Ding", "mich", "selbst", "ent\u00b7deck", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "nur der lebt Sich, der also lebt.", "tokens": ["nur", "der", "lebt", "Sich", ",", "der", "al\u00b7so", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PRF", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mir ist, ich sei emporgest\u00fcrmt", "tokens": ["Mir", "ist", ",", "ich", "sei", "em\u00b7por\u00b7ge\u00b7st\u00fcrmt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00fcber st\u00fcrzende Wasserf\u00e4lle.", "tokens": ["\u00fc\u00b7ber", "st\u00fcr\u00b7zen\u00b7de", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mir engt's die Brust, um mich get\u00fcrmt", "tokens": ["Mir", "engt's", "die", "Brust", ",", "um", "mich", "ge\u00b7t\u00fcrmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "$,", "KOUI", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ahn ich sch\u00fctzende Nebelw\u00e4lle.", "tokens": ["ahn", "ich", "sch\u00fct\u00b7zen\u00b7de", "Ne\u00b7bel\u00b7w\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Aus dumpfen Regionen,", "tokens": ["Aus", "dum\u00b7pfen", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "aus Welten von Zwergen,", "tokens": ["aus", "Wel\u00b7ten", "von", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "trieb's mich fort,", "tokens": ["trie\u00b7b's", "mich", "fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "ob auf ragenden Bergen", "tokens": ["ob", "auf", "ra\u00b7gen\u00b7den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "ein besserer Ort", "tokens": ["ein", "bes\u00b7se\u00b7rer", "Ort"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "dem Freien, zu wohnen.", "tokens": ["dem", "Frei\u00b7en", ",", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Es weht mir um die Stirne", "tokens": ["Es", "weht", "mir", "um", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ein Hauch wie von Frauengewand ...", "tokens": ["ein", "Hauch", "wie", "von", "Frau\u00b7en\u00b7ge\u00b7wand", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Folgte zum steilen Firne", "tokens": ["Folg\u00b7te", "zum", "stei\u00b7len", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "mir wer aus dem Unterland?", "tokens": ["mir", "wer", "aus", "dem", "Un\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PWS", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es beugt sich zu mir nieder", "tokens": ["Es", "beugt", "sich", "zu", "mir", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ein liebes, sch\u00f6nes Gesicht ...", "tokens": ["ein", "lie\u00b7bes", ",", "sch\u00f6\u00b7nes", "Ge\u00b7sicht", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Glaubst Du, ich kenne Dich nicht,", "tokens": ["Glaubst", "Du", ",", "ich", "ken\u00b7ne", "Dich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "S\u00e4ngerin meiner Lieder?", "tokens": ["S\u00e4n\u00b7ge\u00b7rin", "mei\u00b7ner", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Du bist ja, wo ich bin,", "tokens": ["Du", "bist", "ja", ",", "wo", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "mein bester Kamerade!", "tokens": ["mein", "bes\u00b7ter", "Ka\u00b7me\u00b7ra\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bei Dir trifft mich kein Schade,", "tokens": ["Bei", "Dir", "trifft", "mich", "kein", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "meine Herzensk\u00f6nigin!", "tokens": ["mei\u00b7ne", "Her\u00b7zens\u00b7k\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdu flohest aus Finsternissen,", "tokens": ["\u00bb", "du", "flo\u00b7hest", "aus", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "m\u00fchsamen Mutes,", "tokens": ["m\u00fch\u00b7sa\u00b7men", "Mu\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ich wei\u00df es.", "tokens": ["ich", "wei\u00df", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du hast zerrissen", "tokens": ["Du", "hast", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dein Herz, Dein hei\u00dfes,", "tokens": ["Dein", "Herz", ",", "Dein", "hei\u00b7\u00dfes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "und bei dem Leuchten Deines Blutes", "tokens": ["und", "bei", "dem", "Leuch\u00b7ten", "Dei\u00b7nes", "Blu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "bist Du den dunklen Pfad", "tokens": ["bist", "Du", "den", "dunk\u00b7len", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "weiter getreten,", "tokens": ["wei\u00b7ter", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "bis Du mich fandest", "tokens": ["bis", "Du", "mich", "fan\u00b7dest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "und mit tiefen Gebeten", "tokens": ["und", "mit", "tie\u00b7fen", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "mich an Dich bandest,", "tokens": ["mich", "an", "Dich", "ban\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "da\u00df ich Dich liebgewann,", "tokens": ["da\u00df", "ich", "Dich", "lieb\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "dem ringenden Mann", "tokens": ["dem", "rin\u00b7gen\u00b7den", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "ein treuer Kamerad.", "tokens": ["ein", "treu\u00b7er", "Ka\u00b7me\u00b7rad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du brachst uralte Ketten", "tokens": ["Du", "brachst", "ur\u00b7al\u00b7te", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und kamst heute Nacht", "tokens": ["und", "kamst", "heu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "in mein Reich.", "tokens": ["in", "mein", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ich will Dich betten", "tokens": ["Ich", "will", "Dich", "bet\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "an meiner Brust", "tokens": ["an", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "warm und weich,", "tokens": ["warm", "und", "weich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "in Tr\u00e4umepracht", "tokens": ["in", "Tr\u00e4u\u00b7me\u00b7pracht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Deine Seele verz\u00fccken:", "tokens": ["Dei\u00b7ne", "See\u00b7le", "ver\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "der ganzen Welt", "tokens": ["der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Au\u00dfen und Innen", "tokens": ["Au\u00b7\u00dfen", "und", "In\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "sei Deinem Sinnen", "tokens": ["sei", "Dei\u00b7nem", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "preisgestellt.", "tokens": ["preis\u00b7ge\u00b7stellt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Magst sie schm\u00fccken", "tokens": ["Magst", "sie", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "mit lachender Lust,", "tokens": ["mit", "la\u00b7chen\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "magst sie tausendfach", "tokens": ["magst", "sie", "tau\u00b7send\u00b7fach"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "deuten und taufen,", "tokens": ["deu\u00b7ten", "und", "tau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "mit Berg und Wald,", "tokens": ["mit", "Berg", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "mit Wiese und Bach,", "tokens": ["mit", "Wie\u00b7se", "und", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "mit Wolken und Winden,", "tokens": ["mit", "Wol\u00b7ken", "und", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "mit Sternenhaufen", "tokens": ["mit", "Ster\u00b7nen\u00b7hau\u00b7fen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Dein Spiel treiben,", "tokens": ["Dein", "Spiel", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Deinen Spa\u00df finden;", "tokens": ["Dei\u00b7nen", "Spa\u00df", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "brauchst nicht zu bleiben", "tokens": ["brauchst", "nicht", "zu", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "an einem Ort;", "tokens": ["an", "ei\u00b7nem", "O