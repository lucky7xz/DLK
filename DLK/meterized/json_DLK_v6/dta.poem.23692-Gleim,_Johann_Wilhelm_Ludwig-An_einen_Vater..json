{"dta.poem.23692": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An einen Vater.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warte, Lenchen soll dich k\u00fcssen!", "tokens": ["War\u00b7te", ",", "Len\u00b7chen", "soll", "dich", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann wirst du die Jugend loben;", "tokens": ["Dann", "wirst", "du", "die", "Ju\u00b7gend", "lo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann wirst du dich schnell veri\u00fcngen;", "tokens": ["Dann", "wirst", "du", "dich", "schnell", "ve\u00b7ri\u00fcn\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann wirst du nicht murrisch sagen:", "tokens": ["Dann", "wirst", "du", "nicht", "mur\u00b7risch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kinder, seid doch nicht so lustig!", "tokens": ["Kin\u00b7der", ",", "seid", "doch", "nicht", "so", "lus\u00b7tig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}