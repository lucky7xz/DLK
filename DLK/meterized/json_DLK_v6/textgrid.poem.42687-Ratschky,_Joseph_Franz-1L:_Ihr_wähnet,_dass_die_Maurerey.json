{"textgrid.poem.42687": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr w\u00e4hnet, dass die Maurerey", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr w\u00e4hnet, dass die Maurerey", "tokens": ["Ihr", "w\u00e4h\u00b7net", ",", "dass", "die", "Mau\u00b7re\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Werk f\u00fcr Schwesternk\u00f6pfe sey:", "tokens": ["Kein", "Werk", "f\u00fcr", "Schwes\u00b7tern\u00b7k\u00f6p\u00b7fe", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Br\u00fcder, h\u00f6ret auf zu l\u00e4stern!", "tokens": ["O", "Br\u00fc\u00b7der", ",", "h\u00f6\u00b7ret", "auf", "zu", "l\u00e4s\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn was ihr k\u00f6nnet, alles das", "tokens": ["Denn", "was", "ihr", "k\u00f6n\u00b7net", ",", "al\u00b7les", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "PIS", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gelinget ohne Winkelmass", "tokens": ["Ge\u00b7lin\u00b7get", "oh\u00b7ne", "Win\u00b7kel\u00b7mass"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Zirkel mancher unsrer Schwestern.", "tokens": ["Und", "Zir\u00b7kel", "man\u00b7cher", "uns\u00b7rer", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr bringt's durch Theophrasts Arkan", "tokens": ["Ihr", "bringt's", "durch", "Theo\u00b7ph\u00b7rasts", "Ar\u00b7kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun schon so weit, dass dann und wann", "tokens": ["Nun", "schon", "so", "weit", ",", "dass", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tiegel sammt dem Golde scheitern:", "tokens": ["Die", "Tie\u00b7gel", "sammt", "dem", "Gol\u00b7de", "schei\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwestern k\u00f6nnen, Gott weiss wie,", "tokens": ["Die", "Schwes\u00b7tern", "k\u00f6n\u00b7nen", ",", "Gott", "weiss", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "NN", "VVFIN", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sicher sonder Alchymie,", "tokens": ["Doch", "si\u00b7cher", "son\u00b7der", "Al\u00b7chy\u00b7mie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch euer Gold weit besser l\u00e4utern.", "tokens": ["Euch", "eu\u00b7er", "Gold", "weit", "bes\u00b7ser", "l\u00e4u\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch Schr\u00f6pfers magische Gewalt", "tokens": ["Durch", "Schr\u00f6p\u00b7fers", "ma\u00b7gi\u00b7sche", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bez\u00e4hmt, muss manche Luftgestalt", "tokens": ["Be\u00b7z\u00e4hmt", ",", "muss", "man\u00b7che", "Luft\u00b7ge\u00b7stalt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu allem, was ihr wollt, euch taugen:", "tokens": ["Zu", "al\u00b7lem", ",", "was", "ihr", "wollt", ",", "euch", "tau\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwestern fesseln einen Tross", "tokens": ["Die", "Schwes\u00b7tern", "fes\u00b7seln", "ei\u00b7nen", "Tross"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dienstbarer Geister manchmal bloss", "tokens": ["Dienst\u00b7ba\u00b7rer", "Geis\u00b7ter", "manch\u00b7mal", "bloss"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "ADJA"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Durch die Magie verliebter Augen.", "tokens": ["Durch", "die", "Ma\u00b7gie", "ver\u00b7lieb\u00b7ter", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "O Br\u00fcder! Edelmuth verkennt", "tokens": ["O", "Br\u00fc\u00b7der", "!", "E\u00b7del\u00b7muth", "ver\u00b7kennt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie neidisch fremden Werth, und g\u00f6nnt", "tokens": ["Nie", "nei\u00b7disch", "frem\u00b7den", "Werth", ",", "und", "g\u00f6nnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gern jedermann sein Bisschen Ehre.", "tokens": ["Gern", "je\u00b7der\u00b7mann", "sein", "Bis\u00b7schen", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum g\u00f6nnt sie unsern Schwestern auch!", "tokens": ["Drum", "g\u00f6nnt", "sie", "un\u00b7sern", "Schwes\u00b7tern", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es t\u00f6n' ihr Lob nach Maurerbrauch", "tokens": ["Es", "t\u00f6n'", "ihr", "Lob", "nach", "Mau\u00b7rer\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus unserm blinkenden Gewehre!", "tokens": ["Aus", "un\u00b7serm", "blin\u00b7ken\u00b7den", "Ge\u00b7weh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}