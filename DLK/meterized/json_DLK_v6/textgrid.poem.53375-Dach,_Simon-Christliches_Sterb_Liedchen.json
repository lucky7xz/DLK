{"textgrid.poem.53375": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Sterb Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit mir machst du es leicht ein Ende", "tokens": ["Mit", "mir", "machst", "du", "es", "leicht", "ein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Denn was bin ich, der Schatten Kind,", "tokens": ["Denn", "was", "bin", "ich", ",", "der", "Schat\u00b7ten", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Vatter, gegen deine H\u00e4nde", "tokens": ["O", "Vat\u00b7ter", ",", "ge\u00b7gen", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aller Dinge Meister sind?", "tokens": ["Die", "al\u00b7ler", "Din\u00b7ge", "Meis\u00b7ter", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gestalt und Leben", "tokens": ["Die", "Ge\u00b7stalt", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd Erhaltung geben", "tokens": ["Vnd", "Er\u00b7hal\u00b7tung", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Allem nah vnd fern,", "tokens": ["Al\u00b7lem", "nah", "vnd", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Die die Himmels-Scheiben", "tokens": ["Die", "die", "Him\u00b7mels\u00b7Schei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Vnauffh\u00f6rlich treiben", "tokens": ["Vnauff\u00b7h\u00f6r\u00b7lich", "trei\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Vmb den Angel-Stern?", "tokens": ["Vmb", "den", "An\u00b7gel\u00b7S\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die ungehe\u00fcer-grosse Festen", "tokens": ["Die", "un\u00b7ge\u00b7he\u00b7\u00fcer\u00b7gros\u00b7se", "Fes\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umbspannest du mit einer Hand,", "tokens": ["U\u00b7mbspan\u00b7nest", "du", "mit", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du hast den Ostraum von dem Westen", "tokens": ["Du", "hast", "den", "O\u00b7straum", "von", "dem", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit fernen Grentzen abgetrannt,", "tokens": ["Mit", "fer\u00b7nen", "Grent\u00b7zen", "ab\u00b7ge\u00b7trannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4lst die Wind' im Z\u00fcgel,", "tokens": ["H\u00e4lst", "die", "Wind'", "im", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd gibst jhnen Fl\u00fcgel", "tokens": ["Vnd", "gibst", "jh\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wenn es dir gef\u00e4lt,", "tokens": ["Wenn", "es", "dir", "ge\u00b7f\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wolcken, Flut und Flammen", "tokens": ["Wol\u00b7cken", ",", "Flut", "und", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Tretten stracks zusammen", "tokens": ["Tret\u00b7ten", "stracks", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Rings vmb dein Gezelt.", "tokens": ["Rings", "vmb", "dein", "Ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vnd suchst mit mir es an zu nehmen?", "tokens": ["Vnd", "suchst", "mit", "mir", "es", "an", "zu", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schaw wie ich zugerichtet bin,", "tokens": ["Schaw", "wie", "ich", "zu\u00b7ge\u00b7rich\u00b7tet", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gleiche kaum nur einem Sch\u00e4men,", "tokens": ["Ich", "glei\u00b7che", "kaum", "nur", "ei\u00b7nem", "Sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lebens-Kr\u00e4ffte sind dahin,", "tokens": ["Die", "Le\u00b7bens\u00b7Kr\u00e4ff\u00b7te", "sind", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin hin und wieder", "tokens": ["Ich", "bin", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Da\u00df mich meine Glieder", "tokens": ["Da\u00df", "mich", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "An zu sehen grawt,", "tokens": ["An", "zu", "se\u00b7hen", "grawt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Bin von dir zubrochen,", "tokens": ["Bin", "von", "dir", "zu\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Hange mit den Knochen", "tokens": ["Han\u00b7ge", "mit", "den", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Kaum in meiner Haut.", "tokens": ["Kaum", "in", "mei\u00b7ner", "Haut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ich rede schwer mit meiner Zungen,", "tokens": ["Ich", "re\u00b7de", "schwer", "mit", "mei\u00b7ner", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Augen Fenster brechen mir,", "tokens": ["Der", "Au\u00b7gen", "Fens\u00b7ter", "bre\u00b7chen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Athem ist in meiner Lungen,", "tokens": ["Kein", "A\u00b7them", "ist", "in", "mei\u00b7ner", "Lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als der rechte Tod ist hier,", "tokens": ["Nichts", "als", "der", "rech\u00b7te", "Tod", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey gegr\u00fcsst, o Erde,", "tokens": ["Sey", "ge\u00b7gr\u00fcs\u00b7st", ",", "o", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der ich jetzund werde,", "tokens": ["Der", "ich", "je\u00b7tzund", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sonne, gutte Nacht,", "tokens": ["Son\u00b7ne", ",", "gut\u00b7te", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Gutte Nacht, jhr Meinen,", "tokens": ["Gut\u00b7te", "Nacht", ",", "jhr", "Mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Keiner ist durch weinen", "tokens": ["Kei\u00b7ner", "ist", "durch", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Noch zur\u00fcck gebracht!", "tokens": ["Noch", "zu\u00b7r\u00fcck", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wirst du mich aber, Gott, verlassen", "tokens": ["Wirst", "du", "mich", "a\u00b7ber", ",", "Gott", ",", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "$,", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser meiner letzten Noht?", "tokens": ["In", "die\u00b7ser", "mei\u00b7ner", "letz\u00b7ten", "Noht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer weis sich mein doch an zu massen?", "tokens": ["Wer", "weis", "sich", "mein", "doch", "an", "zu", "mas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "PRF", "PPOSAT", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Menschen H\u00fclff' ist l\u00e4ngst hie tod.", "tokens": ["Denn", "Men\u00b7schen", "H\u00fclff'", "ist", "l\u00e4ngst", "hie", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ist nun mein Flehen,", "tokens": ["Wo", "ist", "nun", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn ich her gesehen", "tokens": ["Wenn", "ich", "her", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APZR", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Auff den letzten Streit,", "tokens": ["Auff", "den", "letz\u00b7ten", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df du mich wolttst retten", "tokens": ["Da\u00df", "du", "mich", "wolttst", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Aus des Todes-Ketten?", "tokens": ["Aus", "des", "To\u00b7des\u00b7Ket\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Komm, nun ist es Zeit.", "tokens": ["Komm", ",", "nun", "ist", "es", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sind meine S\u00fcnden dir im Wege", "tokens": ["Sind", "mei\u00b7ne", "S\u00fcn\u00b7den", "dir", "im", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich durch Busse, so mich kr\u00e4nckt,", "tokens": ["Die", "ich", "durch", "Bus\u00b7se", ",", "so", "mich", "kr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff meines Jesu Schultern lege", "tokens": ["Auff", "mei\u00b7nes", "Je\u00b7su", "Schul\u00b7tern", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der jetzt vor mir am Cre\u00fctze", "tokens": ["Der", "jetzt", "vor", "mir", "am", "Cre\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heilt sein Blutvergiessen", "tokens": ["Heilt", "sein", "Blut\u00b7ver\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mir nicht das gewissen?", "tokens": ["Mir", "nicht", "das", "ge\u00b7wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PDS", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sein Verdienst ist mein,", "tokens": ["Sein", "Ver\u00b7dienst", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Kanst du also schweigen?", "tokens": ["Kanst", "du", "al\u00b7so", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Mir nicht H\u00fclff erzeigen?", "tokens": ["Mir", "nicht", "H\u00fclff", "er\u00b7zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Mich verseumen? nein!", "tokens": ["Mich", "ver\u00b7seu\u00b7men", "?", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nur komm mit einer sanfften Stunden,", "tokens": ["Nur", "komm", "mit", "ei\u00b7ner", "sanff\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich warte sehnlich auff dein Heil", "tokens": ["Ich", "war\u00b7te", "sehn\u00b7lich", "auff", "dein", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich der Schmertzen seyn entbunden,", "tokens": ["La\u00df", "mich", "der", "Schmert\u00b7zen", "seyn", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd schenck mir deines Reiches Theil,", "tokens": ["Vnd", "schenck", "mir", "dei\u00b7nes", "Rei\u00b7ches", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich dich wil preisen", "tokens": ["Da", "ich", "dich", "wil", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit der Engel Weisen,", "tokens": ["Mit", "der", "En\u00b7gel", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wil in dir erfrewt", "tokens": ["Wil", "in", "dir", "er\u00b7frewt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Frey gestehn, da\u00df Ehre", "tokens": ["Frey", "ge\u00b7stehn", ",", "da\u00df", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Danck vnd Ruhm geh\u00f6re", "tokens": ["Danck", "vnd", "Ruhm", "ge\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Dir in Ewigheit.", "tokens": ["Dir", "in", "E\u00b7wig\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}