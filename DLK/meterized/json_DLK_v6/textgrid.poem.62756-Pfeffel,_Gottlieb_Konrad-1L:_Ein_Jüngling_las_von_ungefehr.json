{"textgrid.poem.62756": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein J\u00fcngling las von ungefehr", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00fcngling las von ungefehr", "tokens": ["Ein", "J\u00fcng\u00b7ling", "las", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer Harmonie der Sph\u00e4ren", "tokens": ["Von", "ei\u00b7ner", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Plato. Ha! die mu\u00df ich h\u00f6ren,", "tokens": ["Im", "Pla\u00b7to", ".", "Ha", "!", "die", "mu\u00df", "ich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$.", "ITJ", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rief er, und bat den Jupiter,", "tokens": ["Rief", "er", ",", "und", "bat", "den", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Ihm sein Verlangen zu gew\u00e4hren.", "tokens": ["Ihm", "sein", "Ver\u00b7lan\u00b7gen", "zu", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umsonst sprach dieser: junger Thor!", "tokens": ["Um\u00b7sonst", "sprach", "die\u00b7ser", ":", "jun\u00b7ger", "Thor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das g\u00f6ttliche Concert der Sph\u00e4ren", "tokens": ["Das", "g\u00f6tt\u00b7li\u00b7che", "Con\u00b7cert", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nicht f\u00fcr eines Menschen Ohr!", "tokens": ["Ist", "nicht", "f\u00fcr", "ei\u00b7nes", "Men\u00b7schen", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er lie\u00df nicht ab, ihn zu beschw\u00f6ren,", "tokens": ["Er", "lie\u00df", "nicht", "ab", ",", "ihn", "zu", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bis Zevs einst die Geduld verlor,", "tokens": ["Bis", "Zevs", "einst", "die", "Ge\u00b7duld", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sich entschlo\u00df, ihn zu erh\u00f6ren.", "tokens": ["Und", "sich", "ent\u00b7schlo\u00df", ",", "ihn", "zu", "er\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er r\u00fchret seinen Scheitel an;", "tokens": ["Er", "r\u00fch\u00b7ret", "sei\u00b7nen", "Schei\u00b7tel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der J\u00fcngling h\u00f6rt durch alle Himmel,", "tokens": ["Der", "J\u00fcng\u00b7ling", "h\u00f6rt", "durch", "al\u00b7le", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und was? ... Ein gr\u00e4\u00dfliches Get\u00fcmmel.", "tokens": ["Und", "was", "?", "...", "Ein", "gr\u00e4\u00df\u00b7li\u00b7ches", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein tausendstimmiger Orkan,", "tokens": ["Ein", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7ger", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bewehrt mit Graus und Untergange,", "tokens": ["Be\u00b7wehrt", "mit", "Graus", "und", "Un\u00b7ter\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und alle Donner durch die Hand", "tokens": ["Und", "al\u00b7le", "Don\u00b7ner", "durch", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des R\u00e4chers auf die Welt gesandt,", "tokens": ["Des", "R\u00e4\u00b7chers", "auf", "die", "Welt", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sind gegen diesem Rundgesange,", "tokens": ["Sind", "ge\u00b7gen", "die\u00b7sem", "Rund\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dem Summen einer Biene gleich.", "tokens": ["Dem", "Sum\u00b7men", "ei\u00b7ner", "Bie\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "O Zevs! was l\u00e4ssest du mich h\u00f6ren?", "tokens": ["O", "Zevs", "!", "was", "l\u00e4s\u00b7sest", "du", "mich", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWS", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So rief der J\u00fcngling starr und bleich:", "tokens": ["So", "rief", "der", "J\u00fcng\u00b7ling", "starr", "und", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ist das die Harmonie der Sph\u00e4ren?", "tokens": ["Ist", "das", "die", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "So br\u00fcllt die H\u00f6lle nach dem Raub:", "tokens": ["So", "br\u00fcllt", "die", "H\u00f6l\u00b7le", "nach", "dem", "Raub", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ha, mache mich viel lieber taub,", "tokens": ["Ha", ",", "ma\u00b7che", "mich", "viel", "lie\u00b7ber", "taub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Du f\u00fcrchterlicher Gott der G\u00f6tter!", "tokens": ["Du", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Gott", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Itzt rufet Zevs aus einem Wetter:", "tokens": ["Itzt", "ru\u00b7fet", "Zevs", "aus", "ei\u00b7nem", "Wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Erkenne, bl\u00f6des Erdenkind,", "tokens": ["Er\u00b7ken\u00b7ne", ",", "bl\u00f6\u00b7des", "Er\u00b7den\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df Menschen keine G\u00f6tter sind.", "tokens": ["Da\u00df", "Men\u00b7schen", "kei\u00b7ne", "G\u00f6t\u00b7ter", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Du h\u00f6rst ein schreckendes Get\u00fcmmel,", "tokens": ["Du", "h\u00f6rst", "ein", "schre\u00b7cken\u00b7des", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und ich \u2013 die Harmonie der Himmel.", "tokens": ["Und", "ich", "\u2013", "die", "Har\u00b7mo\u00b7nie", "der", "Him\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}