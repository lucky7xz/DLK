{"textgrid.poem.50715": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, da diese alten Herrn", "tokens": ["Nun", ",", "da", "die\u00b7se", "al\u00b7ten", "Herrn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Tief im Rausche sanken,", "tokens": ["Tief", "im", "Rau\u00b7sche", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oben auch von Stern zu Stern", "tokens": ["O\u00b7ben", "auch", "von", "Stern", "zu", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgennebel schwanken:", "tokens": ["Mor\u00b7gen\u00b7ne\u00b7bel", "schwan\u00b7ken", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "R\u00fccken wir zusammen", "tokens": ["R\u00fc\u00b7cken", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unterm Gartentor.", "tokens": ["Un\u00b7term", "Gar\u00b7ten\u00b7tor", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jetzt in neue Flammen", "tokens": ["Jetzt", "in", "neu\u00b7e", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schl\u00e4gt die Lust empor!", "tokens": ["Schl\u00e4gt", "die", "Lust", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df der junge Sonnenball,", "tokens": ["Da\u00df", "der", "jun\u00b7ge", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt er auf den H\u00fcgeln,", "tokens": ["Rollt", "er", "auf", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich im funkelnden Kristall", "tokens": ["Sich", "im", "fun\u00b7keln\u00b7den", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Fr\u00f6hlich mag bespiegeln:", "tokens": ["Fr\u00f6h\u00b7lich", "mag", "be\u00b7spie\u00b7geln", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Halten wir entgegen", "tokens": ["Hal\u00b7ten", "wir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Becher ihm und Glas!", "tokens": ["Be\u00b7cher", "ihm", "und", "Glas", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Flie\u00dfe, goldner Regen,", "tokens": ["Flie\u00b7\u00dfe", ",", "gold\u00b7ner", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Gl\u00fche, dunkles Na\u00df!", "tokens": ["Gl\u00fc\u00b7he", ",", "dunk\u00b7les", "Na\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jungfrau! geh und sieh mir nach", "tokens": ["Jung\u00b7frau", "!", "geh", "und", "sieh", "mir", "nach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "KON", "VVIMP", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings in allen G\u00e4rten,", "tokens": ["Rings", "in", "al\u00b7len", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob schon viele Rosen wach:", "tokens": ["Ob", "schon", "vie\u00b7le", "Ro\u00b7sen", "wach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring die tauverkl\u00e4rten!", "tokens": ["Bring", "die", "tau\u00b7ver\u00b7kl\u00e4r\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rosen, Rosen bringe!", "tokens": ["Ro\u00b7sen", ",", "Ro\u00b7sen", "brin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rosenduft soll wehn!", "tokens": ["Ro\u00b7sen\u00b7duft", "soll", "wehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn ich trink und singe,", "tokens": ["Wenn", "ich", "trink", "und", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Will ich Blumen sehn!", "tokens": ["Will", "ich", "Blu\u00b7men", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Horcht! der erste Amselschlag", "tokens": ["Horcht", "!", "der", "ers\u00b7te", "Am\u00b7sel\u00b7schlag"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schallet aus den Gr\u00fcnden!", "tokens": ["Schal\u00b7let", "aus", "den", "Gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Treue W\u00e4chter soll der Tag", "tokens": ["Treu\u00b7e", "W\u00e4ch\u00b7ter", "soll", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig in uns finden!", "tokens": ["Freu\u00b7dig", "in", "uns", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wer wird denn vermissen", "tokens": ["Wer", "wird", "denn", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wenn sie sangbeflissen,", "tokens": ["Wenn", "sie", "sang\u00b7be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Heiter durchgewacht?", "tokens": ["Hei\u00b7ter", "durch\u00b7ge\u00b7wacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Tief ist meiner Freude Born,", "tokens": ["Tief", "ist", "mei\u00b7ner", "Freu\u00b7de", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer als das Leiden,", "tokens": ["Tie\u00b7fer", "als", "das", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch es wacht der rote Zorn", "tokens": ["Doch", "es", "wacht", "der", "ro\u00b7te", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell in ihnen beiden!", "tokens": ["Hell", "in", "ih\u00b7nen", "bei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darum lasset rinnen", "tokens": ["Da\u00b7rum", "las\u00b7set", "rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Letztes Glas und Lied:", "tokens": ["Letz\u00b7tes", "Glas", "und", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Zornig uns von hinnen", "tokens": ["Zor\u00b7nig", "uns", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nun die Freude zieht!", "tokens": ["Nun", "die", "Freu\u00b7de", "zieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}