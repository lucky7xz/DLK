{"textgrid.poem.61052": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O komm zu mir, wenn durch die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O komm zu mir, wenn durch die Nacht", "tokens": ["O", "komm", "zu", "mir", ",", "wenn", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wandelt das Sternenheer!", "tokens": ["Wan\u00b7delt", "das", "Ster\u00b7nen\u00b7heer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann schwebt mit uns in Mondespracht", "tokens": ["Dann", "schwebt", "mit", "uns", "in", "Mon\u00b7de\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gondel \u00fcbers Meer.", "tokens": ["Die", "Gon\u00b7del", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Luft ist weich wie Liebesscherz,", "tokens": ["Die", "Luft", "ist", "weich", "wie", "Lie\u00b7bes\u00b7scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sanft spielt der goldne Schein,", "tokens": ["Sanft", "spielt", "der", "gold\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Zither klingt und zieht dein Herz", "tokens": ["Die", "Zi\u00b7ther", "klingt", "und", "zieht", "dein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit in die Lust hinein.", "tokens": ["Mit", "in", "die", "Lust", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O komm zu mir, wenn durch die Nacht", "tokens": ["O", "komm", "zu", "mir", ",", "wenn", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wandelt das Sternenheer!", "tokens": ["Wan\u00b7delt", "das", "Ster\u00b7nen\u00b7heer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Dann schwebt mit uns in Mondespracht", "tokens": ["Dann", "schwebt", "mit", "uns", "in", "Mon\u00b7de\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Gondel \u00fcbers Meer.", "tokens": ["Die", "Gon\u00b7del", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist f\u00fcr Liebende die Stund',", "tokens": ["Das", "ist", "f\u00fcr", "Lie\u00b7ben\u00b7de", "die", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liebchen, wie ich und du;", "tokens": ["Lieb\u00b7chen", ",", "wie", "ich", "und", "du", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So friedlich blaut des Himmels Rund,", "tokens": ["So", "fried\u00b7lich", "blaut", "des", "Him\u00b7mels", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schl\u00e4ft das Meer in Ruh'.", "tokens": ["Es", "schl\u00e4ft", "das", "Meer", "in", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie es schl\u00e4ft, da sagt der Blick,", "tokens": ["Und", "wie", "es", "schl\u00e4ft", ",", "da", "sagt", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was keine Zunge spricht,", "tokens": ["Was", "kei\u00b7ne", "Zun\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Lippe zieht sich nicht zur\u00fcck", "tokens": ["Die", "Lip\u00b7pe", "zieht", "sich", "nicht", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wehrt dem Kusse nicht.", "tokens": ["Und", "wehrt", "dem", "Kus\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O komm zu mir, wenn durch die Nacht", "tokens": ["O", "komm", "zu", "mir", ",", "wenn", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wandelt das Sternenheer!", "tokens": ["Wan\u00b7delt", "das", "Ster\u00b7nen\u00b7heer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Dann schwebt mit uns in Mondespracht", "tokens": ["Dann", "schwebt", "mit", "uns", "in", "Mon\u00b7de\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Gondel \u00fcbers Meer.", "tokens": ["Die", "Gon\u00b7del", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}