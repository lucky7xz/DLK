{"textgrid.poem.65904": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Fl\u00f6te", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geweihter Hades, deiner Urkraft Flamme", "tokens": ["Ge\u00b7weih\u00b7ter", "Ha\u00b7des", ",", "dei\u00b7ner", "Ur\u00b7kraft", "Flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umf\u00e4chert uns mit Feuerfingern f\u00fcrchterlich;", "tokens": ["Um\u00b7f\u00e4\u00b7chert", "uns", "mit", "Feu\u00b7er\u00b7fin\u00b7gern", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch bangt mir kaum vor Stachels nahem Stich:", "tokens": ["Doch", "bangt", "mir", "kaum", "vor", "Sta\u00b7chels", "na\u00b7hem", "Stich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Blicken rings auf D\u00fcsterung zum Stamme,", "tokens": ["Mein", "Bli\u00b7cken", "rings", "auf", "D\u00fcs\u00b7te\u00b7rung", "zum", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Damit er sich mit Rinde klar beklammre,", "tokens": ["Da\u00b7mit", "er", "sich", "mit", "Rin\u00b7de", "klar", "be\u00b7klamm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befahl dem Glast: Zerfahl' geschwisterlich!", "tokens": ["Be\u00b7fahl", "dem", "Glast", ":", "Zer\u00b7fahl'", "ge\u00b7schwis\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im alten Walde sammelt sich das Ich,", "tokens": ["Im", "al\u00b7ten", "Wal\u00b7de", "sam\u00b7melt", "sich", "das", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch hilft mir Demeter, der Seele Amme.", "tokens": ["Auch", "hilft", "mir", "De\u00b7me\u00b7ter", ",", "der", "See\u00b7le", "Am\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "So traut daheim, bei Tau und Morgenr\u00f6te,", "tokens": ["So", "traut", "da\u00b7heim", ",", "bei", "Tau", "und", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verdankt das Auge Mandelzweigen Rast,", "tokens": ["Ver\u00b7dankt", "das", "Au\u00b7ge", "Man\u00b7del\u00b7zwei\u00b7gen", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn himmlisch bl\u00fchn sie auf ins Tal der N\u00f6te.", "tokens": ["Denn", "himm\u00b7lisch", "bl\u00fchn", "sie", "auf", "ins", "Tal", "der", "N\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun Amsel meines Liebens, frei vom Ast,", "tokens": ["Nun", "Am\u00b7sel", "mei\u00b7nes", "Lie\u00b7bens", ",", "frei", "vom", "Ast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beschwing dein Lied zum Lenz, mit Spiel der Fl\u00f6te,", "tokens": ["Be\u00b7schwing", "dein", "Lied", "zum", "Lenz", ",", "mit", "Spiel", "der", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie du's von Hirten sanft vernommen hast!", "tokens": ["Wie", "du's", "von", "Hir\u00b7ten", "sanft", "ver\u00b7nom\u00b7men", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}