{"textgrid.poem.63833": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Chi bella non \u00e8 ...", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah im Olivenwalde", "tokens": ["Ich", "sah", "im", "O\u00b7li\u00b7ven\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein M\u00e4gdlein wandeln durchs Gras,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "wan\u00b7deln", "durchs", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Beeren, zerstreut auf der Halde,", "tokens": ["Das", "Bee\u00b7ren", ",", "zer\u00b7streut", "auf", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geb\u00fcckt in ihr Sch\u00fcrzchen las", "tokens": ["Ge\u00b7b\u00fcckt", "in", "ihr", "Sch\u00fcrz\u00b7chen", "las"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sang, als ob ihr gro\u00df Leid geschah:", "tokens": ["Und", "sang", ",", "als", "ob", "ihr", "gro\u00df", "Leid", "ge\u00b7schah", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es klang so traurig und tr\u00fcbe", "tokens": ["Es", "klang", "so", "trau\u00b7rig", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von einsamer Todesstund',", "tokens": ["Von", "ein\u00b7sa\u00b7mer", "To\u00b7desstund'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als klagt um verlorene Liebe", "tokens": ["Als", "klagt", "um", "ver\u00b7lo\u00b7re\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein nimmergek\u00fc\u00dfter Mund:", "tokens": ["Ein", "nim\u00b7mer\u00b7ge\u00b7k\u00fc\u00df\u00b7ter", "Mund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die H\u00e4\u00dflichen sterben allein, ach ja!", "tokens": ["Die", "H\u00e4\u00df\u00b7li\u00b7chen", "ster\u00b7ben", "al\u00b7lein", ",", "ach", "ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ITJ", "ITJ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Da blickte sie auf, und mit Staunen", "tokens": ["Da", "blick\u00b7te", "sie", "auf", ",", "und", "mit", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gewahrt' ich ein reizend Gesicht.", "tokens": ["Ge\u00b7wahrt'", "ich", "ein", "rei\u00b7zend", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es lacht aus den Augen, den braunen,", "tokens": ["Es", "lacht", "aus", "den", "Au\u00b7gen", ",", "den", "brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein schalkhaft blitzendes Licht.", "tokens": ["Ein", "schalk\u00b7haft", "blit\u00b7zen\u00b7des", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mit solchen Augen, wer klagte da:", "tokens": ["Mit", "sol\u00b7chen", "Au\u00b7gen", ",", "wer", "klag\u00b7te", "da", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWS", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Schelmin sah mit Err\u00f6ten,", "tokens": ["Die", "Schel\u00b7min", "sah", "mit", "Er\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie sehr sie den Fremdling behext,", "tokens": ["Wie", "sehr", "sie", "den", "Fremd\u00b7ling", "be\u00b7hext", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Fand gleichwohl nicht vonn\u00f6ten,", "tokens": ["Fand", "gleich\u00b7wohl", "nicht", "von\u00b7n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu \u00e4ndern den seufzenden Text,", "tokens": ["Zu", "\u00e4n\u00b7dern", "den", "seuf\u00b7zen\u00b7den", "Text", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und sang mit Lachen, so lang sie mich sah:", "tokens": ["Und", "sang", "mit", "La\u00b7chen", ",", "so", "lang", "sie", "mich", "sah", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}