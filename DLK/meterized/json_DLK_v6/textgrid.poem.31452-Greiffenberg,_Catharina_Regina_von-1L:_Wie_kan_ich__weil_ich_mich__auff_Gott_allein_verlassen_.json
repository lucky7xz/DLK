{"textgrid.poem.31452": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie kan ich / weil ich mich / auff Gott allein verlassen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kan ich / weil ich mich / auff Gott allein verlassen /", "tokens": ["Wie", "kan", "ich", "/", "weil", "ich", "mich", "/", "auff", "Gott", "al\u00b7lein", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "$(", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "doch unerrettet seyn? thm alles m\u00fcglich ist.", "tokens": ["doch", "un\u00b7er\u00b7ret\u00b7tet", "seyn", "?", "thm", "al\u00b7les", "m\u00fcg\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$.", "VVFIN", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur das nicht / da\u00df er de\u00df / der ihm vertraut / vergist,", "tokens": ["Nur", "das", "nicht", "/", "da\u00df", "er", "de\u00df", "/", "der", "ihm", "ver\u00b7traut", "/", "ver\u00b7gist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDS", "PTKNEG", "$(", "KOUS", "PPER", "ART", "$(", "ART", "PPER", "ADJD", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sein seyn pflegt alle ding / nur dieses nicht / zu fassen.", "tokens": ["sein", "seyn", "pflegt", "al\u00b7le", "ding", "/", "nur", "die\u00b7ses", "nicht", "/", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAINF", "VVFIN", "PIAT", "NN", "$(", "ADV", "PDS", "PTKNEG", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sein verheissen f\u00e4hl / unm\u00fcglich ist dermassen /", "tokens": ["Da\u00df", "sein", "ver\u00b7heis\u00b7sen", "f\u00e4hl", "/", "un\u00b7m\u00fcg\u00b7lich", "ist", "der\u00b7mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "ADJD", "VAFIN", "PDS", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df / eh als sie / die Erd' und Himmel brechen m\u00fcst.", "tokens": ["da\u00df", "/", "eh", "als", "sie", "/", "die", "Erd'", "und", "Him\u00b7mel", "bre\u00b7chen", "m\u00fcst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "KOUS", "PPER", "$(", "ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie ist gewisser / als du selbst im Wesen bist.", "tokens": ["Sie", "ist", "ge\u00b7wis\u00b7ser", "/", "als", "du", "selbst", "im", "We\u00b7sen", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch ihr' Erf\u00fcllung reist die unerh\u00f6rtste strassen.", "tokens": ["Doch", "ih\u00b7r'", "Er\u00b7f\u00fcl\u00b7lung", "reist", "die", "un\u00b7er\u00b7h\u00f6rts\u00b7te", "stras\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Sie schwindet in der Hand / wann man sie sicher h\u00e4lt.", "tokens": ["Sie", "schwin\u00b7det", "in", "der", "Hand", "/", "wann", "man", "sie", "si\u00b7cher", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PWAV", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd wann sie \u00fcber Meer / nach unserm Sinn / entfernet /", "tokens": ["Vnd", "wann", "sie", "\u00fc\u00b7ber", "Meer", "/", "nach", "un\u00b7serm", "Sinn", "/", "ent\u00b7fer\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "sie / als ein wunder Geist / uns in die H\u00e4nde f\u00e4llt;", "tokens": ["sie", "/", "als", "ein", "wun\u00b7der", "Geist", "/", "uns", "in", "die", "H\u00e4n\u00b7de", "f\u00e4llt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ART", "ADJA", "NN", "$(", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "im Augenblick verlischt / bald wider neu besternet.", "tokens": ["im", "Au\u00b7gen\u00b7blick", "ver\u00b7lischt", "/", "bald", "wi\u00b7der", "neu", "bes\u00b7ter\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$(", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df treibt sie / bi\u00df man sich in alle wendung schickt;", "tokens": ["Di\u00df", "treibt", "sie", "/", "bi\u00df", "man", "sich", "in", "al\u00b7le", "wen\u00b7dung", "schickt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PIS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dann klar und offenbar best\u00e4ndig sie erquickt.", "tokens": ["dann", "klar", "und", "of\u00b7fen\u00b7bar", "be\u00b7st\u00e4n\u00b7dig", "sie", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}