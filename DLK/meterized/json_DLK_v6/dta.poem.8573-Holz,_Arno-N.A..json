{"dta.poem.8573": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf seiner lustigen Hallelujawiese", "tokens": ["Auf", "sei\u00b7ner", "lus\u00b7ti\u00b7gen", "Hal\u00b7le\u00b7lu\u00b7ja\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "duldet mein fr\u00f6hliches Herz keine Schatten.", "tokens": ["dul\u00b7det", "mein", "fr\u00f6h\u00b7li\u00b7ches", "Herz", "kei\u00b7ne", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Rote, lachende Rubensheilige", "tokens": ["Ro\u00b7te", ",", "la\u00b7chen\u00b7de", "Ru\u00b7bens\u00b7hei\u00b7li\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "tanzen mit nackten Wiener W\u00e4schermadeln Cancan.", "tokens": ["tan\u00b7zen", "mit", "nack\u00b7ten", "Wie\u00b7ner", "W\u00e4\u00b7scher\u00b7ma\u00b7deln", "Can\u00b7can", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN", "NE", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Unter fast brechenden Leberwurstb\u00e4umen", "tokens": ["Un\u00b7ter", "fast", "bre\u00b7chen\u00b7den", "Le\u00b7ber\u00b7wurst\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "k\u00fcsst Corregio die Jo.", "tokens": ["k\u00fcsst", "Cor\u00b7re\u00b7gio", "die", "Jo", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Niemand geniert sich.", "tokens": ["Nie\u00b7mand", "ge\u00b7niert", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Goethe, der Hundsfott, langt sich quer \u00fcber den Schooss die dicke", "tokens": ["Goe\u00b7the", ",", "der", "Hunds\u00b7fott", ",", "langt", "sich", "quer", "\u00fc\u00b7ber", "den", "Scho\u00b7oss", "die", "di\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+--+-+--+--++-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Vulpius.", "tokens": ["Vul\u00b7pius", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Kleine, gefl\u00fcgelte L\u00fcmmels rufen Prost,", "tokens": ["Klei\u00b7ne", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "L\u00fcm\u00b7mels", "ru\u00b7fen", "Prost", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jobst Sackmann, mein Liebling, setzt n l\u00fcttn K\u00fcmmel Aquavit drup!", "tokens": ["Jobst", "Sack\u00b7mann", ",", "mein", "Lieb\u00b7ling", ",", "setzt", "n", "l\u00fcttn", "K\u00fcm\u00b7mel", "A\u00b7qua\u00b7vit", "drup", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}}}}