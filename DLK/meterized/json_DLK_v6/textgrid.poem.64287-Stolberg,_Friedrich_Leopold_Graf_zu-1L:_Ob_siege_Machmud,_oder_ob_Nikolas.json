{"textgrid.poem.64287": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob siege Machmud, oder ob Nikolas", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob siege Machmud, oder ob Nikolas", "tokens": ["Ob", "sie\u00b7ge", "Mach\u00b7mud", ",", "o\u00b7der", "ob", "Ni\u00b7ko\u00b7las"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "$,", "KON", "KOUS", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Popen h\u00f6re; ob sich der Bischof Roms", "tokens": ["Den", "Po\u00b7pen", "h\u00f6\u00b7re", ";", "ob", "sich", "der", "Bi\u00b7schof", "Roms"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "PRF", "ART", "NN", "NE"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Despotisch aufbl\u00e4h', oder knechtisch", "tokens": ["Des\u00b7po\u00b7tisch", "auf\u00b7bl\u00e4h'", ",", "o\u00b7der", "knech\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lecke die Ferse den Burboniden;", "tokens": ["Le\u00b7cke", "die", "Fer\u00b7se", "den", "Bur\u00b7bo\u00b7ni\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ob dort in schlauer Caesar Oktavius", "tokens": ["Ob", "dort", "in", "schlau\u00b7er", "Cae\u00b7sar", "Ok\u00b7ta\u00b7vius"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Volk bejoche, welchem noch Freiheit galt;", "tokens": ["Ein", "Volk", "be\u00b7jo\u00b7che", ",", "wel\u00b7chem", "noch", "Frei\u00b7heit", "galt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob hier, nach sp\u00e4tgefundnen Rechten,", "tokens": ["Ob", "hier", ",", "nach", "sp\u00e4t\u00b7ge\u00b7fund\u00b7nen", "Rech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nige Habe des andern teilen:", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "Ha\u00b7be", "des", "an\u00b7dern", "tei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Soll mich nicht k\u00fcmmern! Eine der Menschlichkeit", "tokens": ["Soll", "mich", "nicht", "k\u00fcm\u00b7mern", "!", "Ei\u00b7ne", "der", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "ART", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geweinte Thr\u00e4ne flo\u00df, da der Korse j\u00fcngst", "tokens": ["Ge\u00b7wein\u00b7te", "Thr\u00e4\u00b7ne", "flo\u00df", ",", "da", "der", "Kor\u00b7se", "j\u00fcngst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den edlen Nacken bog, als seine", "tokens": ["Den", "ed\u00b7len", "Na\u00b7cken", "bog", ",", "als", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "R\u00e4uber ihm sandte der Vielgeliebte.", "tokens": ["R\u00e4u\u00b7ber", "ihm", "sand\u00b7te", "der", "Viel\u00b7ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Seitdem entsagt' ich aller Mitwissenschaft", "tokens": ["Seit\u00b7dem", "ent\u00b7sagt'", "ich", "al\u00b7ler", "Mit\u00b7wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Um ferne Schlachten, und den erzwungenen", "tokens": ["Um", "fer\u00b7ne", "Schlach\u00b7ten", ",", "und", "den", "er\u00b7zwun\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "$,", "KON", "ART", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vertrag, der oft mit feuchtem \u00d6lzweig", "tokens": ["Ver\u00b7trag", ",", "der", "oft", "mit", "feuch\u00b7tem", "\u00d6l\u00b7zweig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummernde Gluten verbarg, nicht l\u00f6schte.", "tokens": ["Schlum\u00b7mern\u00b7de", "Glu\u00b7ten", "ver\u00b7barg", ",", "nicht", "l\u00f6schte", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Komm, s\u00fc\u00dfe Ruhe! s\u00fc\u00dfe Gespielin, komm,", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfe", "Ru\u00b7he", "!", "s\u00fc\u00b7\u00dfe", "Ge\u00b7spie\u00b7lin", ",", "komm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der frohen Unschuld! f\u00fchre mit deiner Hand", "tokens": ["Der", "fro\u00b7hen", "Un\u00b7schuld", "!", "f\u00fch\u00b7re", "mit", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den J\u00fcngling, der sein ganzes Leben", "tokens": ["Den", "J\u00fcng\u00b7ling", ",", "der", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, und der l\u00e4chelnden Weisheit heiligt!", "tokens": ["Dir", ",", "und", "der", "l\u00e4\u00b7cheln\u00b7den", "Weis\u00b7heit", "hei\u00b7ligt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Und fr\u00fchen Weihrauch deinen Alt\u00e4ren streut,", "tokens": ["Und", "fr\u00fc\u00b7hen", "Weih\u00b7rauch", "dei\u00b7nen", "Al\u00b7t\u00e4\u00b7ren", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Hafen segnend, weil ihm der Ocean", "tokens": ["Den", "Ha\u00b7fen", "seg\u00b7nend", ",", "weil", "ihm", "der", "O\u00b7cean"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch l\u00e4chelt, eh die schwarze Woge", "tokens": ["Noch", "l\u00e4\u00b7chelt", ",", "eh", "die", "schwar\u00b7ze", "Wo\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Prediget Rettung zugleich und Weisheit!", "tokens": ["Pre\u00b7di\u00b7get", "Ret\u00b7tung", "zu\u00b7gleich", "und", "Weis\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dem sp\u00e4ten Opfrer \u00f6ffnet ihr Heiligtum", "tokens": ["Dem", "sp\u00e4\u00b7ten", "Opf\u00b7rer", "\u00f6ff\u00b7net", "ihr", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Ruhe selten. Ekel und Schlummer t\u00e4uscht", "tokens": ["Die", "Ru\u00b7he", "sel\u00b7ten", ".", "E\u00b7kel", "und", "Schlum\u00b7mer", "t\u00e4uscht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den m\u00fcden Weltmann, stets von neuen", "tokens": ["Den", "m\u00fc\u00b7den", "Welt\u00b7mann", ",", "stets", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcnschen und gei\u00dfelnder Furcht gest\u00f6ret.", "tokens": ["W\u00fcn\u00b7schen", "und", "gei\u00b7\u00df\u00b7eln\u00b7der", "Furcht", "ge\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "In stille Th\u00e4ler wird sie mich f\u00fchren, wann", "tokens": ["In", "stil\u00b7le", "Th\u00e4\u00b7ler", "wird", "sie", "mich", "f\u00fch\u00b7ren", ",", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$,", "PWAV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sturmwind raset, mir, wann der Mittag z\u00fcrnt,", "tokens": ["Der", "Sturm\u00b7wind", "ra\u00b7set", ",", "mir", ",", "wann", "der", "Mit\u00b7tag", "z\u00fcrnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Am Schattenufer k\u00fchler Quellen,", "tokens": ["Am", "Schat\u00b7te\u00b7nu\u00b7fer", "k\u00fch\u00b7ler", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sitze bereiten im Duft der Rosen.", "tokens": ["Sit\u00b7ze", "be\u00b7rei\u00b7ten", "im", "Duft", "der", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "In heitrer Mondnacht wird sie Ges\u00e4nge mich", "tokens": ["In", "hei\u00b7trer", "Mond\u00b7nacht", "wird", "sie", "Ge\u00b7s\u00e4n\u00b7ge", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Voll Einfalt lehren, voller Empfindungen,", "tokens": ["Voll", "Ein\u00b7falt", "leh\u00b7ren", ",", "vol\u00b7ler", "Emp\u00b7fin\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis Philomel', aus schwanken \u00c4sten,", "tokens": ["Bis", "Phi\u00b7lo\u00b7mel'", ",", "aus", "schwan\u00b7ken", "\u00c4s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Festliches Schweigen umher verbreitet.", "tokens": ["Fest\u00b7li\u00b7ches", "Schwei\u00b7gen", "um\u00b7her", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Des Baches Silber, welches, vom sanften Hang", "tokens": ["Des", "Ba\u00b7ches", "Sil\u00b7ber", ",", "wel\u00b7ches", ",", "vom", "sanf\u00b7ten", "Hang"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des H\u00fcgels, murmelnd, zwischen Violen rinnt,", "tokens": ["Des", "H\u00fc\u00b7gels", ",", "mur\u00b7melnd", ",", "zwi\u00b7schen", "Vi\u00b7o\u00b7len", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Gleicht dann mein Leben; eine Welle", "tokens": ["Gleicht", "dann", "mein", "Le\u00b7ben", ";", "ei\u00b7ne", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Folget der andern, ein Tag dem andern!", "tokens": ["Fol\u00b7get", "der", "an\u00b7dern", ",", "ein", "Tag", "dem", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "NN", "ART", "ADJA", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Voll Freuden jeder! Jeder dem d\u00fcstern Pfuhl", "tokens": ["Voll", "Freu\u00b7den", "je\u00b7der", "!", "Je\u00b7der", "dem", "d\u00fcs\u00b7tern", "Pfuhl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "PIAT", "$.", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zwar n\u00e4her; aber, sieh! es entstr\u00f6mt dem Pfuhl", "tokens": ["Zwar", "n\u00e4\u00b7her", ";", "a\u00b7ber", ",", "sieh", "!", "es", "ent\u00b7str\u00f6mt", "dem", "Pfuhl"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "ADV", "$,", "VVIMP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein hellerer Krystall, als jener,", "tokens": ["Ein", "hel\u00b7le\u00b7rer", "Krys\u00b7tall", ",", "als", "je\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher die Blume der Wiese tr\u00e4nkte.", "tokens": ["Wel\u00b7cher", "die", "Blu\u00b7me", "der", "Wie\u00b7se", "tr\u00e4nk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}