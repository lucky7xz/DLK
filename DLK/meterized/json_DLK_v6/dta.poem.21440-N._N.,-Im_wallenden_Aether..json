{"dta.poem.21440": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Im wallenden Aether.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Umflie\u00dft mich Duftwogen", "tokens": ["Um\u00b7flie\u00dft", "mich", "Duft\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Des wallenden Aethers,", "tokens": ["Des", "wal\u00b7len\u00b7den", "A\u00b7e\u00b7thers", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gie\u00dft g\u00f6ttlichen Odem\u2019s", "tokens": ["Gie\u00dft", "g\u00f6tt\u00b7li\u00b7chen", "O\u00b7dem's"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Anhauch und Leben", "tokens": ["An\u00b7hauch", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In\u2019s innerste Mark mir!", "tokens": ["In's", "in\u00b7ners\u00b7te", "Mark", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Durchschau\u2019rt jeden Nerv", "tokens": ["Durch\u00b7schau'rt", "je\u00b7den", "Nerv"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit sonntrunkener Andacht!", "tokens": ["Mit", "sonn\u00b7trun\u00b7ke\u00b7ner", "An\u00b7dacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "La\u00dft hinab mich tauchen", "tokens": ["La\u00dft", "hin\u00b7ab", "mich", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "In himmlische Lichtsph\u00e4r\u2019!", "tokens": ["In", "himm\u00b7li\u00b7sche", "Licht\u00b7sph\u00e4r'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Umarme mich br\u00fcnstig", "tokens": ["Um\u00b7ar\u00b7me", "mich", "br\u00fcns\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Du seliges Schweigen", "tokens": ["Du", "se\u00b7li\u00b7ges", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Unendlicher Liebe! . .", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "Lie\u00b7be", "!", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Hinstirbt die Sehnsucht,", "tokens": ["Hins\u00b7tirbt", "die", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Die ewige Sehnsucht", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Der erdkranken Seele.", "tokens": ["Der", "erd\u00b7kran\u00b7ken", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Gesprengt sind die Bande", "tokens": ["Ge\u00b7sprengt", "sind", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Der sterblichen H\u00fclle,", "tokens": ["Der", "sterb\u00b7li\u00b7chen", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Ert\u00f6dtet die wilden", "tokens": ["Er\u00b7t\u00f6d\u00b7tet", "die", "wil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "D\u00e4monen des Fleisches.", "tokens": ["D\u00e4\u00b7mo\u00b7nen", "des", "Flei\u00b7sches", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Dann werd\u2019 ich dich schauen,", "tokens": ["Dann", "werd'", "ich", "dich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Ganz schauen und f\u00fchlen,", "tokens": ["Ganz", "schau\u00b7en", "und", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Erl\u00f6ser und Allgott,", "tokens": ["Er\u00b7l\u00f6\u00b7ser", "und", "All\u00b7gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Mit Sonnen und Sternen", "tokens": ["Mit", "Son\u00b7nen", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Im Schoo\u00dfe dir liegen", "tokens": ["Im", "Schoo\u00b7\u00dfe", "dir", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Und tr\u00e4umen, was du tr\u00e4umst.", "tokens": ["Und", "tr\u00e4u\u00b7men", ",", "was", "du", "tr\u00e4umst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Dann stillst du die brennenden", "tokens": ["Dann", "stillst", "du", "die", "bren\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.27": {"text": "Schmerzen des M\u00fcden,", "tokens": ["Schmer\u00b7zen", "des", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Dann stillst du den Durst,", "tokens": ["Dann", "stillst", "du", "den", "Durst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Den uners\u00e4ttlichen Durst", "tokens": ["Den", "un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7chen", "Durst"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Nach ewiger Tr\u00f6stung,", "tokens": ["Nach", "e\u00b7wi\u00b7ger", "Tr\u00f6s\u00b7tung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Dann labt mich dein Auge,", "tokens": ["Dann", "labt", "mich", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Dein lichtspendend Auge,", "tokens": ["Dein", "lichts\u00b7pen\u00b7dend", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Du Urquell der Gnade,", "tokens": ["Du", "Ur\u00b7quell", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Dann zerflie\u00df\u2019 ich im Weihku\u00df", "tokens": ["Dann", "zer\u00b7flie\u00df'", "ich", "im", "Weih\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Deiner Seelenumarmung,", "tokens": ["Dei\u00b7ner", "See\u00b7le\u00b7num\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.36": {"text": "Du allm\u00e4chtige Selbstkraft!", "tokens": ["Du", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Selbst\u00b7kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}