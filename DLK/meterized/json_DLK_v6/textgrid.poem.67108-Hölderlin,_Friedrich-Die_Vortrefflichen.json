{"textgrid.poem.67108": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Vortrefflichen", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieben Br\u00fcder! versucht es nur nicht, vortrefflich zu werden,", "tokens": ["Lie\u00b7ben", "Br\u00fc\u00b7der", "!", "ver\u00b7sucht", "es", "nur", "nicht", ",", "vor\u00b7treff\u00b7lich", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ehrt das Schicksal und tragts, St\u00fcmper auf Erden zu sein;", "tokens": ["Ehrt", "das", "Schick\u00b7sal", "und", "tragts", ",", "St\u00fcm\u00b7per", "auf", "Er\u00b7den", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$,", "NN", "APPR", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn ist Einmal der Kopf voran, so folget der Schweif auch", "tokens": ["Denn", "ist", "Ein\u00b7mal", "der", "Kopf", "vo\u00b7ran", ",", "so", "fol\u00b7get", "der", "Schweif", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und die klassische Zeit deutscher Po\u00ebten ist aus.", "tokens": ["Und", "die", "klas\u00b7si\u00b7sche", "Zeit", "deut\u00b7scher", "Po\u00ebten", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}