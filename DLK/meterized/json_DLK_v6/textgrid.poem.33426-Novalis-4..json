{"textgrid.poem.33426": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hin\u00fcber wall ich,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "wall", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und jede Pein", "tokens": ["Und", "je\u00b7de", "Pein"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wird einst ein Stachel", "tokens": ["Wird", "einst", "ein", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Wollust seyn.", "tokens": ["Der", "Wol\u00b7lust", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Noch wenig Zeiten,", "tokens": ["Noch", "we\u00b7nig", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "So bin ich los,", "tokens": ["So", "bin", "ich", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und liege trunken", "tokens": ["Und", "lie\u00b7ge", "trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der Lieb' im Schoo\u00df.", "tokens": ["Der", "Lieb'", "im", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Unendliches Leben", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wogt m\u00e4chtig in mir", "tokens": ["Wogt", "m\u00e4ch\u00b7tig", "in", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ich schaue von oben", "tokens": ["Ich", "schau\u00b7e", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Herunter nach dir.", "tokens": ["Her\u00b7un\u00b7ter", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "An jenem H\u00fcgel", "tokens": ["An", "je\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Verlischt dein Glanz \u2013", "tokens": ["Ver\u00b7lischt", "dein", "Glanz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein Schatten bringet", "tokens": ["Ein", "Schat\u00b7ten", "brin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den k\u00fchlenden Kranz.", "tokens": ["Den", "k\u00fch\u00b7len\u00b7den", "Kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O! sauge, Geliebter,", "tokens": ["O", "!", "sau\u00b7ge", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$,", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gewaltig mich an,", "tokens": ["Ge\u00b7wal\u00b7tig", "mich", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da\u00df ich entschlummern", "tokens": ["Da\u00df", "ich", "ent\u00b7schlum\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und lieben kann.", "tokens": ["Und", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich f\u00fchle des Todes", "tokens": ["Ich", "f\u00fch\u00b7le", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Verj\u00fcngende Flut,", "tokens": ["Ver\u00b7j\u00fcn\u00b7gen\u00b7de", "Flut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Zu Balsam und Aether", "tokens": ["Zu", "Bal\u00b7sam", "und", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.10": {"text": "Verwandelt mein Blut \u2013", "tokens": ["Ver\u00b7wan\u00b7delt", "mein", "Blut", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ich lebe bey Tage", "tokens": ["Ich", "le\u00b7be", "bey", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Voll Glauben und Muth", "tokens": ["Voll", "Glau\u00b7ben", "und", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Und sterbe die N\u00e4chte", "tokens": ["Und", "ster\u00b7be", "die", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "In heiliger Glut.", "tokens": ["In", "hei\u00b7li\u00b7ger", "Glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}