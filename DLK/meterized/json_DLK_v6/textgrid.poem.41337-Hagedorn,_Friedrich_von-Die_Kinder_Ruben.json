{"textgrid.poem.41337": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kinder Ruben", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Israel straft jeden Stamm sein Fluch", "tokens": ["In", "Is\u00b7rael", "straft", "je\u00b7den", "Stamm", "sein", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf diesen Tag. Dies lehrt ein kleines Buch", "tokens": ["Auf", "die\u00b7sen", "Tag", ".", "Dies", "lehrt", "ein", "klei\u00b7nes", "Buch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von einem ungl\u00fccksvollen Schw\u00e4tzer.", "tokens": ["Von", "ei\u00b7nem", "un\u00b7gl\u00fccks\u00b7vol\u00b7len", "Schw\u00e4t\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kinder Ruben Fluch wird schrecklich angef\u00fchrt:", "tokens": ["Der", "Kin\u00b7der", "Ru\u00b7ben", "Fluch", "wird", "schreck\u00b7lich", "an\u00b7ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was gr\u00fcn ist, das verdorrt, sobald sie es ber\u00fchrt:", "tokens": ["Was", "gr\u00fcn", "ist", ",", "das", "ver\u00b7dorrt", ",", "so\u00b7bald", "sie", "es", "be\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Vorbild vieler Uebersetzer.", "tokens": ["Ein", "Vor\u00b7bild", "vie\u00b7ler", "Ue\u00b7ber\u00b7set\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}