{"dta.poem.8846": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und stets und wiederum auf falscher Spur zu seyn?", "tokens": ["Und", "stets", "und", "wie\u00b7de\u00b7rum", "auf", "fal\u00b7scher", "Spur", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward nicht dieselbe Kraft, die dort im Sterne flammt,", "tokens": ["Ward", "nicht", "die\u00b7sel\u00b7be", "Kraft", ",", "die", "dort", "im", "Ster\u00b7ne", "flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDAT", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestimmt als Rose hier die Zier der Flur zu seyn?", "tokens": ["Be\u00b7stimmt", "als", "Ro\u00b7se", "hier", "die", "Zier", "der", "Flur", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was seufzt ihr euch zur\u00fcck in's sonst'ge Paradies,", "tokens": ["Was", "seufzt", "ihr", "euch", "zu\u00b7r\u00fcck", "in's", "sonst'\u00b7ge", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um wie das Sonnenlicht verkl\u00e4rt und pur zu seyn?", "tokens": ["Um", "wie", "das", "Son\u00b7nen\u00b7licht", "ver\u00b7kl\u00e4rt", "und", "pur", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOKOM", "ART", "NN", "VVPP", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was w\u00fcnscht ihr schmerzbewegt euch bald im Erden-", "tokens": ["Was", "w\u00fcnscht", "ihr", "schmerz\u00b7be\u00b7wegt", "euch", "bald", "im", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und \u00fcber Wolken bald und im Azur zu seyn?", "tokens": ["Und", "\u00fc\u00b7ber", "Wol\u00b7ken", "bald", "und", "im", "A\u00b7zur", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "KON", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Was forscht ihr fr\u00fch und spat dem Quell des Uebels", "tokens": ["Was", "forscht", "ihr", "fr\u00fch", "und", "spat", "dem", "Quell", "des", "Ue\u00b7bels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das doch kein andres ist, als Kreatur zu seyn?", "tokens": ["Das", "doch", "kein", "and\u00b7res", "ist", ",", "als", "Kre\u00b7a\u00b7tur", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "PIS", "VAFIN", "$,", "KOUS", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sich selbst zu schau'n, erschuf der Ewige das All,", "tokens": ["Sich", "selbst", "zu", "schau'n", ",", "er\u00b7schuf", "der", "E\u00b7wi\u00b7ge", "das", "All", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ist der Schmerz des All's, ein Spiegel nur zu", "tokens": ["Das", "ist", "der", "Schmerz", "des", "All's", ",", "ein", "Spie\u00b7gel", "nur", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}