{"textgrid.poem.57840": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Stromer", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier auf der Heide,", "tokens": ["Hier", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da fuhr ich auf und auf und ab;", "tokens": ["Da", "fuhr", "ich", "auf", "und", "auf", "und", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schritt lief ich im Sommer,", "tokens": ["Schritt", "lief", "ich", "im", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wintertags Trab.", "tokens": ["Win\u00b7ter\u00b7tags", "Trab", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Hei\u00df ist der Sommer,", "tokens": ["Hei\u00df", "ist", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Kalt ist der Winterwinterwind,", "tokens": ["Kalt", "ist", "der", "Win\u00b7ter\u00b7win\u00b7ter\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warm ist das Lieben", "tokens": ["Warm", "ist", "das", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Beim sch\u00f6nsten Kind.", "tokens": ["Beim", "sch\u00f6ns\u00b7ten", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hinter der Hecke,", "tokens": ["Hin\u00b7ter", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da ist ein guter guter Platz;", "tokens": ["Da", "ist", "ein", "gu\u00b7ter", "gu\u00b7ter", "Platz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da will ich tr\u00e4umen", "tokens": ["Da", "will", "ich", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Von meinem Schatz.", "tokens": ["Von", "mei\u00b7nem", "Schatz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die, die ich meine,", "tokens": ["Die", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die lebt wohl l\u00e4ngst, ja l\u00e4ngst nicht mehr;", "tokens": ["Die", "lebt", "wohl", "l\u00e4ngst", ",", "ja", "l\u00e4ngst", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosenb\u00fcsche", "tokens": ["Die", "Ro\u00b7sen\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sind kahl und leer.", "tokens": ["Sind", "kahl", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Hinter der Hecke,", "tokens": ["Hin\u00b7ter", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da ist der Schnee, der Schnee so weich;", "tokens": ["Da", "ist", "der", "Schnee", ",", "der", "Schnee", "so", "weich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib' ich da liegen,", "tokens": ["Bleib'", "ich", "da", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mir ist es gleich.", "tokens": ["Mir", "ist", "es", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Tragen zwei Engel", "tokens": ["Tra\u00b7gen", "zwei", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "CARD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mich auf zur Himmelshimmelsh\u00f6h';", "tokens": ["Mich", "auf", "zur", "Him\u00b7mels\u00b7him\u00b7mels\u00b7h\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein feines Liebchen", "tokens": ["Mein", "fei\u00b7nes", "Lieb\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich wieder seh'.", "tokens": ["Ich", "wie\u00b7der", "seh'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein Liljenstengel", "tokens": ["Ein", "Lil\u00b7jen\u00b7sten\u00b7gel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4gt sie in ihrer, ihrer Hand;", "tokens": ["Tr\u00e4gt", "sie", "in", "ih\u00b7rer", ",", "ih\u00b7rer", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Liljenstengel", "tokens": ["Ein", "Lil\u00b7jen\u00b7sten\u00b7gel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit goldnem Band.", "tokens": ["Mit", "gold\u00b7nem", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}