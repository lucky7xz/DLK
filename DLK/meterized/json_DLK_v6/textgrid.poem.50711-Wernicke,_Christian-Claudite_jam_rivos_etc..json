{"textgrid.poem.50711": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Claudite jam rivos etc.", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00dft eure klaren B\u00e4ch', ihr Musen! es ist Zeit;", "tokens": ["Schlie\u00dft", "eu\u00b7re", "kla\u00b7ren", "B\u00e4ch'", ",", "ihr", "Mu\u00b7sen", "!", "es", "ist", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Deutschland find' ich euch von keiner Nutzbarkeit.", "tokens": ["In", "Deutschland", "find'", "ich", "euch", "von", "kei\u00b7ner", "Nutz\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "H\u00e4tt' ich gelernt, wie man im Felde sich l\u00e4\u00dft schlagen,", "tokens": ["H\u00e4tt'", "ich", "ge\u00b7lernt", ",", "wie", "man", "im", "Fel\u00b7de", "sich", "l\u00e4\u00dft", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "PWAV", "PIS", "APPRART", "NN", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So h\u00e4tt' ich schon vielleicht zwei Wachen vor der Th\u00fcr.", "tokens": ["So", "h\u00e4tt'", "ich", "schon", "viel\u00b7leicht", "zwei", "Wa\u00b7chen", "vor", "der", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und h\u00e4tt' ich bunte Schn\u00fcr' auf meinem Rock getragen,", "tokens": ["Und", "h\u00e4tt'", "ich", "bun\u00b7te", "Schn\u00fcr'", "auf", "mei\u00b7nem", "Rock", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ging mir auch vielleicht anitzt kein Staatsrath f\u00fcr.", "tokens": ["So", "ging", "mir", "auch", "viel\u00b7leicht", "a\u00b7nitzt", "kein", "Staats\u00b7rath", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00e4tt' ich durch Schatzungen gelernt, das Volk zu dr\u00fccken,", "tokens": ["H\u00e4tt'", "ich", "durch", "Schat\u00b7zun\u00b7gen", "ge\u00b7lernt", ",", "das", "Volk", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So tr\u00fcg' ich auch vielleicht bereits ein", "tokens": ["So", "tr\u00fcg'", "ich", "auch", "viel\u00b7leicht", "be\u00b7reits", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und w\u00fc\u00dft' ich leckerhaft die Tafel anzuschicken,", "tokens": ["Und", "w\u00fc\u00dft'", "ich", "le\u00b7cker\u00b7haft", "die", "Ta\u00b7fel", "an\u00b7zu\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So h\u00e4tt' ich manchen Sitz", "tokens": ["So", "h\u00e4tt'", "ich", "man\u00b7chen", "Sitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es mu\u00df, wer Etwas hier gedenket zu erwischen,", "tokens": ["Es", "mu\u00df", ",", "wer", "Et\u00b7was", "hier", "ge\u00b7den\u00b7ket", "zu", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "ADV", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Statt eures klaren Bachs in tr\u00fcben Wassern fischen.", "tokens": ["Statt", "eu\u00b7res", "kla\u00b7ren", "Bachs", "in", "tr\u00fc\u00b7ben", "Was\u00b7sern", "fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}