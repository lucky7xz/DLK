{"textgrid.poem.62964": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst fiel der Leu, der auf der Jagd", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst fiel der Leu, der auf der Jagd", "tokens": ["Einst", "fiel", "der", "Leu", ",", "der", "auf", "der", "Jagd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu tief sich in das Holz gewagt,", "tokens": ["Zu", "tief", "sich", "in", "das", "Holz", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zween Tiegern in die Pranken.", "tokens": ["Zween", "Tie\u00b7gern", "in", "die", "Pran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewaltig war sein Widerstand;", "tokens": ["Ge\u00b7wal\u00b7tig", "war", "sein", "Wi\u00b7der\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein ersch\u00f6pft und \u00fcbermannt", "tokens": ["Al\u00b7lein", "er\u00b7sch\u00f6pft", "und", "\u00fc\u00b7berm\u00b7annt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fieng er itzt an zu wanken.", "tokens": ["Fi\u00b7eng", "er", "itzt", "an", "zu", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sprang der Dogge schnell heran", "tokens": ["Da", "sprang", "der", "Dog\u00b7ge", "schnell", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rettete dem armen Chan", "tokens": ["Und", "ret\u00b7te\u00b7te", "dem", "ar\u00b7men", "Chan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch seinen Tod das Leben:", "tokens": ["Durch", "sei\u00b7nen", "Tod", "das", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn kaum entfloh die M\u00f6rderbrut,", "tokens": ["Denn", "kaum", "ent\u00b7floh", "die", "M\u00f6r\u00b7der\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sah er ihn mit stillem Muth", "tokens": ["So", "sah", "er", "ihn", "mit", "stil\u00b7lem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Geist den G\u00f6ttern geben.", "tokens": ["Den", "Geist", "den", "G\u00f6t\u00b7tern", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Itzt kam der ganze Hof herbey:", "tokens": ["Itzt", "kam", "der", "gan\u00b7ze", "Hof", "her\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmir eckelt hier, sprach K\u00f6nig Leu", "tokens": ["\u00bb", "mir", "ec\u00b7kelt", "hier", ",", "sprach", "K\u00f6\u00b7nig", "Leu"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Fuchse, seinem Sklaven:", "tokens": ["Zum", "Fuch\u00b7se", ",", "sei\u00b7nem", "Skla\u00b7ven", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weg mit dem Aas, es braucht kein Grab;", "tokens": ["Weg", "mit", "dem", "Aas", ",", "es", "braucht", "kein", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nur zieh mir ja die Haut ihm ab,", "tokens": ["Nur", "zieh", "mir", "ja", "die", "Haut", "ihm", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es l\u00e4\u00dft sich gut drauf schlafen.\u00ab", "tokens": ["Es", "l\u00e4\u00dft", "sich", "gut", "drauf", "schla\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist dieses, rief mit bitterm Hohn", "tokens": ["Ist", "die\u00b7ses", ",", "rief", "mit", "bit\u00b7term", "Hohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00e4r zum Wolf, des Helden Lohn,", "tokens": ["Der", "B\u00e4r", "zum", "Wolf", ",", "des", "Hel\u00b7den", "Lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach dem wir alle d\u00fcrsten?", "tokens": ["Nach", "dem", "wir", "al\u00b7le", "d\u00fcrs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stirb f\u00fcr dein Weib, f\u00fcr deinen Freund,", "tokens": ["Stirb", "f\u00fcr", "dein", "Weib", ",", "f\u00fcr", "dei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcrs Vaterland, f\u00fcr deinen Feind;", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", ",", "f\u00fcr", "dei\u00b7nen", "Feind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur stirb f\u00fcr keinen F\u00fcrsten.", "tokens": ["Nur", "stirb", "f\u00fcr", "kei\u00b7nen", "F\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}