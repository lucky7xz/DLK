{"textgrid.poem.35544": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Erf\u00fcllung", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verhaltenes Geigengeriesel", "tokens": ["Ver\u00b7hal\u00b7te\u00b7nes", "Gei\u00b7gen\u00b7ge\u00b7rie\u00b7sel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zittert in mein Gemach \u2013", "tokens": ["Zit\u00b7tert", "in", "mein", "Ge\u00b7mach", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich horche auf ... und denke", "tokens": ["Ich", "hor\u00b7che", "auf", "...", "und", "den\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den stillen T\u00f6nen nach ...", "tokens": ["Den", "stil\u00b7len", "T\u00f6\u00b7nen", "nach", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie betasten meine Seele", "tokens": ["Sie", "be\u00b7tas\u00b7ten", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebkosend, scheu und mild \u2013", "tokens": ["Lieb\u00b7ko\u00b7send", ",", "scheu", "und", "mild", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt in werbender Sch\u00f6ne", "tokens": ["Es", "kommt", "in", "wer\u00b7ben\u00b7der", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu mir dein liebes Bild ...", "tokens": ["Zu", "mir", "dein", "lie\u00b7bes", "Bild", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist eine alte Geschichte \u2013", "tokens": ["Das", "ist", "ei\u00b7ne", "al\u00b7te", "Ge\u00b7schich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Man sieht's auf den ersten Blick:", "tokens": ["Man", "sieht's", "auf", "den", "ers\u00b7ten", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein lyrischer Dichter wird immer", "tokens": ["Ein", "ly\u00b7ri\u00b7scher", "Dich\u00b7ter", "wird", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Opfer diskreter Musik ...", "tokens": ["Das", "Op\u00b7fer", "dis\u00b7kre\u00b7ter", "Mu\u00b7sik", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sie flockt so krauses Get\u00e4ndel,", "tokens": ["Sie", "flockt", "so", "krau\u00b7ses", "Ge\u00b7t\u00e4n\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie plaudert entz\u00fcckendes Zeug \u2013", "tokens": ["Sie", "plau\u00b7dert", "ent\u00b7z\u00fc\u00b7cken\u00b7des", "Zeug", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie st\u00f6bert aus Seelengr\u00fcnden", "tokens": ["Sie", "st\u00f6\u00b7bert", "aus", "See\u00b7len\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verge\u00dfner Gef\u00fchle Gestr\u00e4uch!", "tokens": ["Ver\u00b7ge\u00df\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", "Ge\u00b7str\u00e4uch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Auch mich hat sie ergriffen ...", "tokens": ["Auch", "mich", "hat", "sie", "er\u00b7grif\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tiefinnerstes aufgew\u00fchlt \u2013", "tokens": ["Tie\u00b7fin\u00b7ners\u00b7tes", "auf\u00b7ge\u00b7w\u00fchlt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sehr ich dich doch liebe:", "tokens": ["Wie", "sehr", "ich", "dich", "doch", "lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das habe ich da erst gef\u00fchlt!", "tokens": ["Das", "ha\u00b7be", "ich", "da", "erst", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun schweigen die stillen T\u00f6ne,", "tokens": ["Nun", "schwei\u00b7gen", "die", "stil\u00b7len", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und alles hat sich erf\u00fcllt \u2013", "tokens": ["Und", "al\u00b7les", "hat", "sich", "er\u00b7f\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PRF", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und in unendlicher Sch\u00f6ne", "tokens": ["Und", "in", "un\u00b7end\u00b7li\u00b7cher", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schau' ich dein liebes Bild ...", "tokens": ["Schau'", "ich", "dein", "lie\u00b7bes", "Bild", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}