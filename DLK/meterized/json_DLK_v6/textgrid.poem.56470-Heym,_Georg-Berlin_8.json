{"textgrid.poem.56470": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Berlin 8", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schornsteine stehn in gro\u00dfem Zwischenraum", "tokens": ["Schorn\u00b7stei\u00b7ne", "stehn", "in", "gro\u00b7\u00dfem", "Zwi\u00b7schen\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Wintertag, und tragen seine Last,", "tokens": ["Im", "Win\u00b7ter\u00b7tag", ",", "und", "tra\u00b7gen", "sei\u00b7ne", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des schwarzen Himmels dunkelnden Palast.", "tokens": ["Des", "schwar\u00b7zen", "Him\u00b7mels", "dun\u00b7keln\u00b7den", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie goldne Stufe brennt sein niedrer Saum.", "tokens": ["Wie", "gold\u00b7ne", "Stu\u00b7fe", "brennt", "sein", "nie\u00b7drer", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Fern zwischen kahlen B\u00e4umen, manchem Haus,", "tokens": ["Fern", "zwi\u00b7schen", "kah\u00b7len", "B\u00e4u\u00b7men", ",", "man\u00b7chem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Z\u00e4unen und Schuppen, wo die Weltstadt ebbt,", "tokens": ["Z\u00e4u\u00b7nen", "und", "Schup\u00b7pen", ",", "wo", "die", "Welt\u00b7stadt", "ebbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und auf vereisten Schienen m\u00fchsam schleppt", "tokens": ["Und", "auf", "ver\u00b7eis\u00b7ten", "Schie\u00b7nen", "m\u00fch\u00b7sam", "schleppt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein langer G\u00fcterzug sich schwer hinaus.", "tokens": ["Ein", "lan\u00b7ger", "G\u00fc\u00b7ter\u00b7zug", "sich", "schwer", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Armenkirchhof ragt, schwarz, Stein an Stein,", "tokens": ["Ein", "Ar\u00b7men\u00b7kirch\u00b7hof", "ragt", ",", "schwarz", ",", "Stein", "an", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Toten schaun den roten Untergang", "tokens": ["Die", "To\u00b7ten", "schaun", "den", "ro\u00b7ten", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihrem Loch. Er schmeckt wie starker Wein.", "tokens": ["Aus", "ih\u00b7rem", "Loch", ".", "Er", "schmeckt", "wie", "star\u00b7ker", "Wein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sitzen strickend an der Wand entlang,", "tokens": ["Sie", "sit\u00b7zen", "stri\u00b7ckend", "an", "der", "Wand", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "APPO", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fctzen aus Ru\u00df dem nackten Schl\u00e4fenbein,", "tokens": ["M\u00fct\u00b7zen", "aus", "Ru\u00df", "dem", "nack\u00b7ten", "Schl\u00e4\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zur Marseillaise, dem alten Sturmgesang.", "tokens": ["Zur", "Mar\u00b7seil\u00b7laise", ",", "dem", "al\u00b7ten", "Sturm\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}