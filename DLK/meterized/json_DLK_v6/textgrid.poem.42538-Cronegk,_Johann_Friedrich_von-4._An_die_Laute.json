{"textgrid.poem.42538": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4. An die Laute", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du singst, o Nachtigall! allein", "tokens": ["Du", "singst", ",", "o", "Nach\u00b7ti\u00b7gall", "!", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei schauervoller Nacht:", "tokens": ["Bei", "schau\u00b7er\u00b7vol\u00b7ler", "Nacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Lied ert\u00f6nt im dunkeln Hain,", "tokens": ["Dein", "Lied", "er\u00b7t\u00f6nt", "im", "dun\u00b7keln", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo nur die Schwermut wacht.", "tokens": ["Wo", "nur", "die", "Schwer\u00b7mut", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Lied erfrischt des Wandrers Herz,", "tokens": ["Dein", "Lied", "er\u00b7frischt", "des", "Wand\u00b7rers", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der tief im Wald verirrt,", "tokens": ["Der", "tief", "im", "Wald", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von mancher Furcht, von manchem Schmerz", "tokens": ["Von", "man\u00b7cher", "Furcht", ",", "von", "man\u00b7chem", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Best\u00fcrmt und trostlos wird.", "tokens": ["Be\u00b7st\u00fcrmt", "und", "trost\u00b7los", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er h\u00f6rt den kl\u00e4glich s\u00fc\u00dfen Ton", "tokens": ["Er", "h\u00f6rt", "den", "kl\u00e4g\u00b7lich", "s\u00fc\u00b7\u00dfen", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ehrfurchtvoller Lust:", "tokens": ["Mit", "ehr\u00b7furcht\u00b7vol\u00b7ler", "Lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Hoffnung, die schon fast entflohn,", "tokens": ["Die", "Hoff\u00b7nung", ",", "die", "schon", "fast", "ent\u00b7flohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwacht in seiner Brust.", "tokens": ["Er\u00b7wacht", "in", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun geht er durch die dunkle Bahn", "tokens": ["Nun", "geht", "er", "durch", "die", "dunk\u00b7le", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit sichern Schritten hin:", "tokens": ["Mit", "si\u00b7chern", "Schrit\u00b7ten", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sein Schutzgeist gehet still voran;", "tokens": ["Sein", "Schutz\u00b7geist", "ge\u00b7het", "still", "vo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der N\u00e4chte Schrecken fliehn.", "tokens": ["Der", "N\u00e4ch\u00b7te", "Schre\u00b7cken", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn auf des Lebens dunkelm Pfad", "tokens": ["Wenn", "auf", "des", "Le\u00b7bens", "dun\u00b7kelm", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele trostlos irrt,", "tokens": ["Die", "See\u00b7le", "trost\u00b7los", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ohne Schutz und ohne Rat", "tokens": ["Und", "oh\u00b7ne", "Schutz", "und", "oh\u00b7ne", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schwermut Beute wird.", "tokens": ["Der", "Schwer\u00b7mut", "Beu\u00b7te", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Oh, sanfte Laute! t\u00f6ne du", "tokens": ["Oh", ",", "sanf\u00b7te", "Lau\u00b7te", "!", "t\u00f6\u00b7ne", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei stiller Mitternacht", "tokens": ["Bei", "stil\u00b7ler", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir Hoffnung, Trost und Ruhe zu,", "tokens": ["Mir", "Hoff\u00b7nung", ",", "Trost", "und", "Ru\u00b7he", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hirten gl\u00fccklich macht!", "tokens": ["Die", "Hir\u00b7ten", "gl\u00fcck\u00b7lich", "macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Entfernt von pr\u00e4cht'ger Toren Hohn,", "tokens": ["Ent\u00b7fernt", "von", "pr\u00e4cht'\u00b7ger", "To\u00b7ren", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lehrst du mich ruhig sein.", "tokens": ["Lehrst", "du", "mich", "ru\u00b7hig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Leben sei, so wie dein Ton,", "tokens": ["Mein", "Le\u00b7ben", "sei", ",", "so", "wie", "dein", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still, anmutsvoll und rein.", "tokens": ["Still", ",", "an\u00b7muts\u00b7voll", "und", "rein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der pr\u00e4chtigen Trompeten Klang", "tokens": ["Der", "pr\u00e4ch\u00b7ti\u00b7gen", "Trom\u00b7pe\u00b7ten", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist sch\u00f6n, doch f\u00fcrchterlich:", "tokens": ["Ist", "sch\u00f6n", ",", "doch", "f\u00fcrch\u00b7ter\u00b7lich", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ganz leise t\u00f6net dein Gesang,", "tokens": ["Ganz", "lei\u00b7se", "t\u00f6\u00b7net", "dein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reizend nur f\u00fcr mich.", "tokens": ["Und", "rei\u00b7zend", "nur", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So sei mein Leben stillbegl\u00fcckt,", "tokens": ["So", "sei", "mein", "Le\u00b7ben", "still\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sanft, aber unbekannt,", "tokens": ["Sanft", ",", "a\u00b7ber", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit stillen Tugenden geschm\u00fcckt,", "tokens": ["Mit", "stil\u00b7len", "Tu\u00b7gen\u00b7den", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im sichern Mittelstand.", "tokens": ["Im", "si\u00b7chern", "Mit\u00b7tel\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Ein schimmernd Gl\u00fcck begehr' ich nie:", "tokens": ["Ein", "schim\u00b7mernd", "Gl\u00fcck", "be\u00b7gehr'", "ich", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oh, w\u00e4r' die Weisheit mein!", "tokens": ["Oh", ",", "w\u00e4r'", "die", "Weis\u00b7heit", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhabne Vorsicht, gib mir sie,", "tokens": ["Er\u00b7hab\u00b7ne", "Vor\u00b7sicht", ",", "gib", "mir", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werd' ich gl\u00fccklich sein!", "tokens": ["So", "werd'", "ich", "gl\u00fcck\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Lorbeer bleibt best\u00e4ndig gr\u00fcn,", "tokens": ["Der", "Lor\u00b7beer", "bleibt", "be\u00b7st\u00e4n\u00b7dig", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den uns die Muse reicht,", "tokens": ["Den", "uns", "die", "Mu\u00b7se", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn auch die Zeiten schnell entfliehn,", "tokens": ["Wenn", "auch", "die", "Zei\u00b7ten", "schnell", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Jugend Scherz entweicht.", "tokens": ["Der", "Ju\u00b7gend", "Scherz", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mein Alter sei nicht freudenleer,", "tokens": ["Mein", "Al\u00b7ter", "sei", "nicht", "freu\u00b7den\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ohne Scherz und Lied!", "tokens": ["Nicht", "oh\u00b7ne", "Scherz", "und", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod ist nur dem Toren schwer,", "tokens": ["Der", "Tod", "ist", "nur", "dem", "To\u00b7ren", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sterbend alles flieht.", "tokens": ["Dem", "ster\u00b7bend", "al\u00b7les", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}