{"textgrid.poem.56276": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Verratene Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da nachts wir uns k\u00fc\u00dften, o M\u00e4dchen,", "tokens": ["Da", "nachts", "wir", "uns", "k\u00fc\u00df\u00b7ten", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hat keiner uns zugeschaut;", "tokens": ["Hat", "kei\u00b7ner", "uns", "zu\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Sterne, die standen am Himmel,", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "stan\u00b7den", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir haben den Sternen getraut.", "tokens": ["Wir", "ha\u00b7ben", "den", "Ster\u00b7nen", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Es ist ein Stern gefallen,", "tokens": ["Es", "ist", "ein", "Stern", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hat dem Meer uns verklagt,", "tokens": ["Der", "hat", "dem", "Meer", "uns", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da hat das Meer es dem Ruder,", "tokens": ["Da", "hat", "das", "Meer", "es", "dem", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Ruder dem Schiffer gesagt.", "tokens": ["Das", "Ru\u00b7der", "dem", "Schif\u00b7fer", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Da sang derselbe Schiffer", "tokens": ["Da", "sang", "der\u00b7sel\u00b7be", "Schif\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es seiner Liebsten vor,", "tokens": ["Es", "sei\u00b7ner", "Liebs\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun singen's auf Stra\u00dfen und M\u00e4rkten", "tokens": ["Nun", "sin\u00b7gen's", "auf", "Stra\u00b7\u00dfen", "und", "M\u00e4rk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die M\u00e4dchen und Knaben im Chor.", "tokens": ["Die", "M\u00e4d\u00b7chen", "und", "Kna\u00b7ben", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}