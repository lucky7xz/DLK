{"textgrid.poem.24837": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kalter, grauer Nebel hing", "tokens": ["Ein", "kal\u00b7ter", ",", "grau\u00b7er", "Ne\u00b7bel", "hing"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Falten nieder auf das Thal,", "tokens": ["In", "Fal\u00b7ten", "nie\u00b7der", "auf", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wieder ich zum erstenmal", "tokens": ["Als", "wie\u00b7der", "ich", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg zur Waldkapelle ging.", "tokens": ["Den", "Weg", "zur", "Wald\u00b7ka\u00b7pel\u00b7le", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich suchte den bekannten Pfad,", "tokens": ["Ich", "such\u00b7te", "den", "be\u00b7kann\u00b7ten", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den, wenn die Glocke feiervoll", "tokens": ["Den", ",", "wenn", "die", "Glo\u00b7cke", "fei\u00b7er\u00b7voll"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Fr\u00fchgebete rufend scholl,", "tokens": ["Zum", "Fr\u00fch\u00b7ge\u00b7be\u00b7te", "ru\u00b7fend", "scholl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Knabe Tag f\u00fcr Tag betrat.", "tokens": ["Der", "Kna\u00b7be", "Tag", "f\u00fcr", "Tag", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nun war seine Spur verwischt,", "tokens": ["Doch", "nun", "war", "sei\u00b7ne", "Spur", "ver\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Nesseln ward mein Fu\u00df gehemmt;", "tokens": ["Von", "Nes\u00b7seln", "ward", "mein", "Fu\u00df", "ge\u00b7hemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde selber schien mir fremd,", "tokens": ["Die", "Er\u00b7de", "sel\u00b7ber", "schien", "mir", "fremd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit vieler Herbste Laub gemischt.", "tokens": ["Mit", "vie\u00b7ler", "Herbs\u00b7te", "Laub", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Wandrer gleich, der unbekannt", "tokens": ["Dem", "Wand\u00b7rer", "gleich", ",", "der", "un\u00b7be\u00b7kannt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An unwirtbaren K\u00fcsten irrt,", "tokens": ["An", "un\u00b7wirt\u00b7ba\u00b7ren", "K\u00fcs\u00b7ten", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So stand ich zweifelnd und verwirrt,", "tokens": ["So", "stand", "ich", "zwei\u00b7felnd", "und", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fremdling in dem eignen Land.", "tokens": ["Ein", "Fremd\u00b7ling", "in", "dem", "eig\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Stets matter glomm das Tageslicht,", "tokens": ["Stets", "mat\u00b7ter", "glomm", "das", "Ta\u00b7ges\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verloren scholl ein Glockenklang;", "tokens": ["Ver\u00b7lo\u00b7ren", "scholl", "ein", "Glo\u00b7cken\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich irrte viel, ich suchte lang,", "tokens": ["Ich", "irr\u00b7te", "viel", ",", "ich", "such\u00b7te", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die Kapelle fand ich nicht.", "tokens": ["Doch", "die", "Ka\u00b7pel\u00b7le", "fand", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}