{"textgrid.poem.26794": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlauf, Kameraden, aufs Pferd, aufs Pferd!", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf, Kameraden, aufs Pferd, aufs Pferd!", "tokens": ["Wohl\u00b7auf", ",", "Ka\u00b7me\u00b7ra\u00b7den", ",", "aufs", "Pferd", ",", "aufs", "Pferd", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ins Feld, in die Freiheit gezogen!", "tokens": ["Ins", "Feld", ",", "in", "die", "Frei\u00b7heit", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Felde, da ist der Mann noch was wert,", "tokens": ["Im", "Fel\u00b7de", ",", "da", "ist", "der", "Mann", "noch", "was", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "PWS", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wird das Herz noch gewogen.", "tokens": ["Da", "wird", "das", "Herz", "noch", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da tritt kein anderer f\u00fcr ihn ein,", "tokens": ["Da", "tritt", "kein", "an\u00b7de\u00b7rer", "f\u00fcr", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf sich selber steht er da ganz allein.", "tokens": ["Auf", "sich", "sel\u00b7ber", "steht", "er", "da", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus der Welt die Freiheit verschwunden ist,", "tokens": ["Aus", "der", "Welt", "die", "Frei\u00b7heit", "ver\u00b7schwun\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man sieht nur Herren und Knechte,", "tokens": ["Man", "sieht", "nur", "Her\u00b7ren", "und", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Falschheit herrschet, die Hinterlist", "tokens": ["Die", "Falschheit", "herr\u00b7schet", ",", "die", "Hin\u00b7ter\u00b7list"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei dem feigen Menschengeschlechte.", "tokens": ["Bei", "dem", "fei\u00b7gen", "Men\u00b7schen\u00b7ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der dem Tod ins Angesicht schauen kann,", "tokens": ["Der", "dem", "Tod", "ins", "An\u00b7ge\u00b7sicht", "schau\u00b7en", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Soldat allein ist der freie Mann.", "tokens": ["Der", "Sol\u00b7dat", "al\u00b7lein", "ist", "der", "frei\u00b7e", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Lebens \u00c4ngsten, er wirft sie weg,", "tokens": ["Des", "Le\u00b7bens", "\u00c4ngs\u00b7ten", ",", "er", "wirft", "sie", "weg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat nicht mehr zu f\u00fcrchten, zu sorgen,", "tokens": ["Hat", "nicht", "mehr", "zu", "f\u00fcrch\u00b7ten", ",", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er reitet dem Schicksal entgegen keck,", "tokens": ["Er", "rei\u00b7tet", "dem", "Schick\u00b7sal", "ent\u00b7ge\u00b7gen", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Triffts heute nicht, trifft es doch morgen,", "tokens": ["Triffts", "heu\u00b7te", "nicht", ",", "trifft", "es", "doch", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und trifft es morgen, so lasset uns heut", "tokens": ["Und", "trifft", "es", "mor\u00b7gen", ",", "so", "las\u00b7set", "uns", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Noch schl\u00fcrfen die Neige der k\u00f6stlichen Zeit.", "tokens": ["Noch", "schl\u00fcr\u00b7fen", "die", "Nei\u00b7ge", "der", "k\u00f6st\u00b7li\u00b7chen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Von dem Himmel f\u00e4llt ihm sein lustig Los,", "tokens": ["Von", "dem", "Him\u00b7mel", "f\u00e4llt", "ihm", "sein", "lus\u00b7tig", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJD", "NE", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Brauchts nicht mit M\u00fch zu erstreben,", "tokens": ["Brauchts", "nicht", "mit", "M\u00fch", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der Fr\u00f6ner, der sucht in der Erde Scho\u00df,", "tokens": ["Der", "Fr\u00f6\u00b7ner", ",", "der", "sucht", "in", "der", "Er\u00b7de", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da meint er den Schatz zu erheben.", "tokens": ["Da", "meint", "er", "den", "Schatz", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er gr\u00e4bt und schaufelt, solang er lebt,", "tokens": ["Er", "gr\u00e4bt", "und", "schau\u00b7felt", ",", "so\u00b7lang", "er", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und gr\u00e4bt, bis er endlich sein Grab sich gr\u00e4bt.", "tokens": ["Und", "gr\u00e4bt", ",", "bis", "er", "end\u00b7lich", "sein", "Grab", "sich", "gr\u00e4bt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Der Reiter und sein geschwindes Ro\u00df,", "tokens": ["Der", "Rei\u00b7ter", "und", "sein", "ge\u00b7schwin\u00b7des", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sind gef\u00fcrchtete G\u00e4ste;", "tokens": ["Sie", "sind", "ge\u00b7f\u00fcrch\u00b7te\u00b7te", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es flimmern die Lampen im Hochzeitschlo\u00df,", "tokens": ["Es", "flim\u00b7mern", "die", "Lam\u00b7pen", "im", "Hoch\u00b7zeit\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ungeladen kommt er zum Feste.", "tokens": ["Un\u00b7ge\u00b7la\u00b7den", "kommt", "er", "zum", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Er wirbt nicht lange, er zeiget nicht Gold,", "tokens": ["Er", "wirbt", "nicht", "lan\u00b7ge", ",", "er", "zei\u00b7get", "nicht", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im Sturm erringt er den Minnesold.", "tokens": ["Im", "Sturm", "er\u00b7ringt", "er", "den", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Warum weint die Dirn und zergr\u00e4met sich schier?", "tokens": ["Wa\u00b7rum", "weint", "die", "Dirn", "und", "zer\u00b7gr\u00e4\u00b7met", "sich", "schier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df fahren dahin, la\u00df fahren!", "tokens": ["La\u00df", "fah\u00b7ren", "da\u00b7hin", ",", "la\u00df", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PAV", "$,", "VVIMP", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er hat auf Erden kein bleibend Quartier,", "tokens": ["Er", "hat", "auf", "Er\u00b7den", "kein", "blei\u00b7bend", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kann treue Lieb nicht bewahren.", "tokens": ["Kann", "treu\u00b7e", "Lieb", "nicht", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das rasche Schicksal, es treibt ihn fort,", "tokens": ["Das", "ra\u00b7sche", "Schick\u00b7sal", ",", "es", "treibt", "ihn", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Seine Ruh l\u00e4\u00dft er an keinem Ort.", "tokens": ["Sei\u00b7ne", "Ruh", "l\u00e4\u00dft", "er", "an", "kei\u00b7nem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Drum frisch, Kameraden, den Rappen gez\u00e4umt,", "tokens": ["Drum", "frisch", ",", "Ka\u00b7me\u00b7ra\u00b7den", ",", "den", "Rap\u00b7pen", "ge\u00b7z\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Brust im Gefechte gel\u00fcftet!", "tokens": ["Die", "Brust", "im", "Ge\u00b7fech\u00b7te", "ge\u00b7l\u00fcf\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Jugend brauset, das Leben sch\u00e4umt,", "tokens": ["Die", "Ju\u00b7gend", "brau\u00b7set", ",", "das", "Le\u00b7ben", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Frisch auf! eh der Geist noch verd\u00fcftet.", "tokens": ["Frisch", "auf", "!", "eh", "der", "Geist", "noch", "ver\u00b7d\u00fcf\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "KOUS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und setzet ihr nicht das Leben ein,", "tokens": ["Und", "set\u00b7zet", "ihr", "nicht", "das", "Le\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nie wird euch das Leben gewonnen sein.", "tokens": ["Nie", "wird", "euch", "das", "Le\u00b7ben", "ge\u00b7won\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}