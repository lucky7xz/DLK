{"textgrid.poem.62045": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Rose unter Dornen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Rose unter Dornen", "tokens": ["Wie", "die", "Ro\u00b7se", "un\u00b7ter", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht das Ideelle jetzt,", "tokens": ["Steht", "das", "I\u00b7deel\u00b7le", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur das scheu\u00dflich Materielle", "tokens": ["Nur", "das", "scheu\u00df\u00b7lich", "Ma\u00b7te\u00b7ri\u00b7el\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kommt zuerst und kommt zuletzt!", "tokens": ["Kommt", "zu\u00b7erst", "und", "kommt", "zu\u00b7letzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wird gepredigt aller Orten,", "tokens": ["Wird", "ge\u00b7pre\u00b7digt", "al\u00b7ler", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Vernunft, Gebot der Zeit,", "tokens": ["Als", "Ver\u00b7nunft", ",", "Ge\u00b7bot", "der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Beispiel und mit Worten", "tokens": ["Und", "mit", "Bei\u00b7spiel", "und", "mit", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht es \u00fcberall sich breit.", "tokens": ["Macht", "es", "\u00fc\u00b7be\u00b7rall", "sich", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber, wie die R\u00f6slein bl\u00fchen,", "tokens": ["A\u00b7ber", ",", "wie", "die", "R\u00f6s\u00b7lein", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungetr\u00fcbt und ewig rein,", "tokens": ["Un\u00b7ge\u00b7tr\u00fcbt", "und", "e\u00b7wig", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz der D\u00fcnste, die da ziehen, \u2013", "tokens": ["Trotz", "der", "D\u00fcns\u00b7te", ",", "die", "da", "zie\u00b7hen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibet alle Sch\u00f6nheit sein.", "tokens": ["Blei\u00b7bet", "al\u00b7le", "Sch\u00f6n\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}