{"textgrid.poem.66107": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein klapperd\u00fcrrer Fiedelmann", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein klapperd\u00fcrrer Fiedelmann", "tokens": ["Ein", "klap\u00b7per\u00b7d\u00fcr\u00b7rer", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand unter einem Baume", "tokens": ["Stand", "un\u00b7ter", "ei\u00b7nem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte seine Geige an", "tokens": ["Und", "setz\u00b7te", "sei\u00b7ne", "Gei\u00b7ge", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geigte wie im Traume", "tokens": ["Und", "geig\u00b7te", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sang ein leises Zwitscherlied,", "tokens": ["Und", "sang", "ein", "lei\u00b7ses", "Zwit\u00b7scher\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das r\u00fchrte an die \u00c4ste,", "tokens": ["Das", "r\u00fchr\u00b7te", "an", "die", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und als der letzte Ton verschied,", "tokens": ["Und", "als", "der", "letz\u00b7te", "Ton", "ver\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da starb ein Spatz im Neste.", "tokens": ["Da", "starb", "ein", "Spatz", "im", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der klapperd\u00fcrre Fiedelmann", "tokens": ["Der", "klap\u00b7per\u00b7d\u00fcr\u00b7re", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand unter trocknem Kranze", "tokens": ["Stand", "un\u00b7ter", "trock\u00b7nem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte seine Geige an", "tokens": ["Und", "setz\u00b7te", "sei\u00b7ne", "Gei\u00b7ge", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geigte flott zum Tanze", "tokens": ["Und", "geig\u00b7te", "flott", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und geigte flott zum Erntebier,", "tokens": ["Und", "geig\u00b7te", "flott", "zum", "Ern\u00b7te\u00b7bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Rock und Sch\u00fcrze fliegen,", "tokens": ["Wo", "Rock", "und", "Sch\u00fcr\u00b7ze", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein letzter Triller, zart und zier,", "tokens": ["Ein", "letz\u00b7ter", "Tril\u00b7ler", ",", "zart", "und", "zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da mu\u00df die Gro\u00dfmagd liegen.", "tokens": ["Da", "mu\u00df", "die", "Gro\u00df\u00b7magd", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wieder stand der Fiedelmann", "tokens": ["Und", "wie\u00b7der", "stand", "der", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stocksteif vorm Pastorate", "tokens": ["Stock\u00b7steif", "vorm", "Pas\u00b7to\u00b7ra\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte seine Geige an", "tokens": ["Und", "setz\u00b7te", "sei\u00b7ne", "Gei\u00b7ge", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur geistlichen Sonate.", "tokens": ["Zur", "geist\u00b7li\u00b7chen", "So\u00b7na\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein r\u00fchrend Religioso sang", "tokens": ["Ein", "r\u00fch\u00b7rend", "Re\u00b7li\u00b7gi\u00b7o\u00b7so", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Von allen Himmelsschauern,", "tokens": ["Von", "al\u00b7len", "Him\u00b7mels\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein schluchzender Morendogang \u2013", "tokens": ["Ein", "schluch\u00b7zen\u00b7der", "Mo\u00b7ren\u00b7do\u00b7gang", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Wer predigt nun den Bauern?", "tokens": ["Wer", "pre\u00b7digt", "nun", "den", "Bau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann stand der flei\u00dfige Fiedelmann", "tokens": ["Dann", "stand", "der", "flei\u00b7\u00dfi\u00b7ge", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl auf der Herrendiele", "tokens": ["Wohl", "auf", "der", "Her\u00b7ren\u00b7die\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte seine Geige an", "tokens": ["Und", "setz\u00b7te", "sei\u00b7ne", "Gei\u00b7ge", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu raschem, scharfem Spiele.", "tokens": ["Zu", "ra\u00b7schem", ",", "schar\u00b7fem", "Spie\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das klang halb wie ein Trinklied froh,", "tokens": ["Das", "klang", "halb", "wie", "ein", "Trin\u00b7klied", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Halb wie ein Sturm auf Schanzen,", "tokens": ["Halb", "wie", "ein", "Sturm", "auf", "Schan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein kurzes, keckes Tremolo,", "tokens": ["Ein", "kur\u00b7zes", ",", "ke\u00b7ckes", "Tre\u00b7mo\u00b7lo", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da mu\u00df der Schlo\u00dfherr tanzen.", "tokens": ["Da", "mu\u00df", "der", "Schlo\u00df\u00b7herr", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und neulich stand der Fiedelmann", "tokens": ["Und", "neu\u00b7lich", "stand", "der", "Fie\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch vor des Schulzen Kammer", "tokens": ["Auch", "vor", "des", "Schul\u00b7zen", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte seine Geige an", "tokens": ["Und", "setz\u00b7te", "sei\u00b7ne", "Gei\u00b7ge", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang wie eine Ammer", "tokens": ["Und", "sang", "wie", "ei\u00b7ne", "Am\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sang und sang den ganzen Tag", "tokens": ["Und", "sang", "und", "sang", "den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sang vor tauben Ohren,", "tokens": ["Und", "sang", "vor", "tau\u00b7ben", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An dem, der da im Fieber lag,", "tokens": ["An", "dem", ",", "der", "da", "im", "Fie\u00b7ber", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schien jede Kunst verloren.", "tokens": ["Schien", "je\u00b7de", "Kunst", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da trat er dicht ans Bettgestell,", "tokens": ["Da", "trat", "er", "dicht", "ans", "Bett\u00b7ge\u00b7stell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hub w\u00fctend an zu kratzen,", "tokens": ["Hub", "w\u00fc\u00b7tend", "an", "zu", "krat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch statt des Kranken Trommelfell", "tokens": ["Doch", "statt", "des", "Kran\u00b7ken", "Trom\u00b7mel\u00b7fell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dft ihm die Quinte platzen.", "tokens": ["Mu\u00dft", "ihm", "die", "Quin\u00b7te", "plat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erbost schlug er sein Saitenspiel", "tokens": ["Er\u00b7bost", "schlug", "er", "sein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Aufs Haupt dem z\u00e4hen Recken,", "tokens": ["Aufs", "Haupt", "dem", "z\u00e4\u00b7hen", "Re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Geige in zwei St\u00fccke fiel,", "tokens": ["Die", "Gei\u00b7ge", "in", "zwei", "St\u00fc\u00b7cke", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schulze starb vor Schrecken.", "tokens": ["Der", "Schul\u00b7ze", "starb", "vor", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der klapperd\u00fcrre Fiedelmann,", "tokens": ["Der", "klap\u00b7per\u00b7d\u00fcr\u00b7re", "Fie\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hockt er nun am Rande", "tokens": ["Da", "hockt", "er", "nun", "am", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und leimt sein Zeug, so gut er kann,", "tokens": ["Und", "leimt", "sein", "Zeug", ",", "so", "gut", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flickt Saiten, Steg und Bande", "tokens": ["Flickt", "Sai\u00b7ten", ",", "Steg", "und", "Ban\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und brummt, das hat man nun davon,", "tokens": ["Und", "brummt", ",", "das", "hat", "man", "nun", "da\u00b7von", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "VAFIN", "PIS", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem spielt ich zu manierlich,", "tokens": ["Dem", "spielt", "ich", "zu", "ma\u00b7nier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Jetzt lern ich Ba\u00df und Bombardon,", "tokens": ["Jetzt", "lern", "ich", "Ba\u00df", "und", "Bom\u00b7bar\u00b7don", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Geige ist zu zierlich.", "tokens": ["Die", "Gei\u00b7ge", "ist", "zu", "zier\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}