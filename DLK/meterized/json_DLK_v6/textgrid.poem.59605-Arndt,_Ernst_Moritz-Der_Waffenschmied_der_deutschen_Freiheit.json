{"textgrid.poem.59605": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der Waffenschmied der deutschen Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem geb\u00fchrt der h\u00f6chste Preis?", "tokens": ["Wem", "ge\u00b7b\u00fchrt", "der", "h\u00f6chs\u00b7te", "Preis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur dem Mann, der still erschafft,", "tokens": ["Nur", "dem", "Mann", ",", "der", "still", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in M\u00fchen schwer und hei\u00df", "tokens": ["Der", "in", "M\u00fc\u00b7hen", "schwer", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie verzagt und nie erschlafft,", "tokens": ["Nie", "ver\u00b7zagt", "und", "nie", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der im Drange von Gefahren", "tokens": ["Der", "im", "Dran\u00b7ge", "von", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt, was seine V\u00e4ter waren;", "tokens": ["F\u00fchlt", ",", "was", "sei\u00b7ne", "V\u00e4\u00b7ter", "wa\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der selbst mit dem Schelm und Knecht,", "tokens": ["Der", "selbst", "mit", "dem", "Schelm", "und", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dem Buben glatt und blank", "tokens": ["Mit", "dem", "Bu\u00b7ben", "glatt", "und", "blank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer wahr und grad' und recht", "tokens": ["Im\u00b7mer", "wahr", "und", "grad'", "und", "recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht der Ehre festen Gang,", "tokens": ["Geht", "der", "Eh\u00b7re", "fes\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der dem\u00fctig und bescheiden", "tokens": ["Der", "de\u00b7m\u00fc\u00b7tig", "und", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles kann um Tugend leiden;", "tokens": ["Al\u00b7les", "kann", "um", "Tu\u00b7gend", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Den der Ehre Gaukelspiel", "tokens": ["Den", "der", "Eh\u00b7re", "Gau\u00b7kel\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Gold nicht lockt noch h\u00e4lt,", "tokens": ["Und", "das", "Gold", "nicht", "lockt", "noch", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf ein Gef\u00fchl, ein Ziel", "tokens": ["Der", "auf", "ein", "Ge\u00b7f\u00fchl", ",", "ein", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Alle Kr\u00e4fte m\u00e4chtig stellt,", "tokens": ["Al\u00b7le", "Kr\u00e4f\u00b7te", "m\u00e4ch\u00b7tig", "stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schandeketten zu zerbrechen", "tokens": ["Schan\u00b7de\u00b7ket\u00b7ten", "zu", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den welschen Trug zu r\u00e4chen;", "tokens": ["Und", "den", "wel\u00b7schen", "Trug", "zu", "r\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der, wenn Memmen matt und feig", "tokens": ["Der", ",", "wenn", "Mem\u00b7men", "matt", "und", "feig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "NN", "PTKVZ", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dingen um der Knechtschaft Lohn,", "tokens": ["Din\u00b7gen", "um", "der", "Knecht\u00b7schaft", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer m\u00fcd und nimmer bleich", "tokens": ["Nim\u00b7mer", "m\u00fcd", "und", "nim\u00b7mer", "bleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trotzt der Sp\u00f6tter schn\u00f6dem Hohn,", "tokens": ["Trotzt", "der", "Sp\u00f6t\u00b7ter", "schn\u00f6\u00b7dem", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, wenn alle Welt auch teufelt,", "tokens": ["Der", ",", "wenn", "al\u00b7le", "Welt", "auch", "teu\u00b7felt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie am Vaterland verzweifelt.", "tokens": ["Nie", "am", "Va\u00b7ter\u00b7land", "ver\u00b7zwei\u00b7felt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nenne ihn! Wie hei\u00dft der Mann?", "tokens": ["Nen\u00b7ne", "ihn", "!", "Wie", "hei\u00dft", "der", "Mann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Freiheit Waffenschmied?", "tokens": ["Deut\u00b7scher", "Frei\u00b7heit", "Waf\u00b7fen\u00b7schmied", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der nie wankend ab und an", "tokens": ["Der", "nie", "wan\u00b7kend", "ab", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "PTKVZ", "KON", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ging den festen Heldenschritt?", "tokens": ["Ging", "den", "fes\u00b7ten", "Hel\u00b7den\u00b7schritt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der im stillen hat geschaffen", "tokens": ["Der", "im", "stil\u00b7len", "hat", "ge\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ross' und M\u00e4nner, Krieg und Waffen?", "tokens": ["Ross'", "und", "M\u00e4n\u00b7ner", ",", "Krieg", "und", "Waf\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Scharnhorst hei\u00dft der edle Mann,", "tokens": ["Scharn\u00b7horst", "hei\u00dft", "der", "ed\u00b7le", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Freiheit Waffenschmied,", "tokens": ["Deut\u00b7scher", "Frei\u00b7heit", "Waf\u00b7fen\u00b7schmied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf Rettung rastlos sann,", "tokens": ["Der", "auf", "Ret\u00b7tung", "rast\u00b7los", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieles tat und vieles litt,", "tokens": ["Vie\u00b7les", "tat", "und", "vie\u00b7les", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er k\u00f6nnte deutsche Ehren", "tokens": ["Da\u00df", "er", "k\u00f6nn\u00b7te", "deut\u00b7sche", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr den heil'gen Krieg bewehren.", "tokens": ["F\u00fcr", "den", "heil'\u00b7gen", "Krieg", "be\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schon hat er den gro\u00dfen Streit,", "tokens": ["Schon", "hat", "er", "den", "gro\u00b7\u00dfen", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der uns steht um h\u00f6chstes Gut,", "tokens": ["Der", "uns", "steht", "um", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrlich hat er ihn geweiht", "tokens": ["Herr\u00b7lich", "hat", "er", "ihn", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem teuren Heldenblut:", "tokens": ["Mit", "dem", "teu\u00b7ren", "Hel\u00b7den\u00b7blut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Allen Tapfern rann's zum Pfande,", "tokens": ["Al\u00b7len", "Tap\u00b7fern", "rann's", "zum", "Pfan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df erliegen wird die Schande.", "tokens": ["Da\u00df", "er\u00b7lie\u00b7gen", "wird", "die", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Darum, Klang, der Freiheit klingt,", "tokens": ["Da\u00b7rum", ",", "Klang", ",", "der", "Frei\u00b7heit", "klingt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Kling ihn hell wie Orgelton,", "tokens": ["Kling", "ihn", "hell", "wie", "Or\u00b7gel\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum, Lied, das Freiheit singt,", "tokens": ["Da\u00b7rum", ",", "Lied", ",", "das", "Frei\u00b7heit", "singt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe Deutschlands tapfern Sohn,", "tokens": ["Sin\u00b7ge", "Deutschlands", "tap\u00b7fern", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Zeig' ihn allen Biederleuten", "tokens": ["Zeig'", "ihn", "al\u00b7len", "Bie\u00b7der\u00b7leu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Zeichen be\u00dfrer Zeiten.", "tokens": ["Als", "ein", "Zei\u00b7chen", "be\u00df\u00b7rer", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Treuer, biedrer deutscher Held,", "tokens": ["Treu\u00b7er", ",", "bie\u00b7drer", "deut\u00b7scher", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott mit uns und Gott mit dir!", "tokens": ["Gott", "mit", "uns", "und", "Gott", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die Ehre oben h\u00e4lt,", "tokens": ["Der", "die", "Eh\u00b7re", "o\u00b7ben", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehe bei dir f\u00fcr und f\u00fcr!", "tokens": ["Ste\u00b7he", "bei", "dir", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm mit Vaterlandesrettern", "tokens": ["Nimm", "mit", "Va\u00b7ter\u00b7lan\u00b7des\u00b7ret\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm den Kranz von Eichenbl\u00e4ttern.", "tokens": ["Nimm", "den", "Kranz", "von", "Ei\u00b7chen\u00b7bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}