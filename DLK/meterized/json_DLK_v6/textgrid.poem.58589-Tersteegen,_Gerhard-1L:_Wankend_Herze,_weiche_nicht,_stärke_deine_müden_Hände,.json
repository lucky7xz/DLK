{"textgrid.poem.58589": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wankend Herze, weiche nicht, st\u00e4rke deine m\u00fcden H\u00e4nde,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wankend Herze, weiche nicht, st\u00e4rke deine m\u00fcden H\u00e4nde,", "tokens": ["Wan\u00b7kend", "Her\u00b7ze", ",", "wei\u00b7che", "nicht", ",", "st\u00e4r\u00b7ke", "dei\u00b7ne", "m\u00fc\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJA", "PTKNEG", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wandle fort und sinke nicht gar hinunter im Elende,", "tokens": ["Wand\u00b7le", "fort", "und", "sin\u00b7ke", "nicht", "gar", "hin\u00b7un\u00b7ter", "im", "E\u00b7len\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ei, Verzagter, sei getrost, f\u00fcrchte nicht und stehe fest,", "tokens": ["Ei", ",", "Ver\u00b7zag\u00b7ter", ",", "sei", "ge\u00b7trost", ",", "f\u00fcrch\u00b7te", "nicht", "und", "ste\u00b7he", "fest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "VVPP", "$,", "VVFIN", "PTKNEG", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Traue Gott, er ist so treu, da\u00df er Schwache nie verl\u00e4\u00dft!", "tokens": ["Trau\u00b7e", "Gott", ",", "er", "ist", "so", "treu", ",", "da\u00df", "er", "Schwa\u00b7che", "nie", "ver\u00b7l\u00e4\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Balde kommt der frohe Tag, da Gott wird in dich einkehren,", "tokens": ["Bal\u00b7de", "kommt", "der", "fro\u00b7he", "Tag", ",", "da", "Gott", "wird", "in", "dich", "ein\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "NN", "VAFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Dann wirst du ihm danken noch, wenn du ihn wirst selber h\u00f6ren,", "tokens": ["Dann", "wirst", "du", "ihm", "dan\u00b7ken", "noch", ",", "wenn", "du", "ihn", "wirst", "sel\u00b7ber", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Dann wirst du ihn schauen nah, der dir jetzt so ferne scheint;", "tokens": ["Dann", "wirst", "du", "ihn", "schau\u00b7en", "nah", ",", "der", "dir", "jetzt", "so", "fer\u00b7ne", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wache nur und schaue zu, Gott vertilget deine Feind'!", "tokens": ["Wa\u00b7che", "nur", "und", "schau\u00b7e", "zu", ",", "Gott", "ver\u00b7til\u00b7get", "dei\u00b7ne", "Feind'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "PTKVZ", "$,", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}