{"textgrid.poem.39247": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Spielmann aus Welschland kam,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Spielmann aus Welschland kam,", "tokens": ["Ein", "Spiel\u00b7mann", "aus", "Wel\u00b7schland", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der blies das Horn so s\u00fc\u00df,", "tokens": ["Der", "blies", "das", "Horn", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er 'nem jeden, der's vernahm,", "tokens": ["Da\u00df", "er", "'nem", "je\u00b7den", ",", "der's", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz aus dem Leibe blies.", "tokens": ["Das", "Herz", "aus", "dem", "Lei\u00b7be", "blies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vor Kaiser Karl und seinem Gesind',", "tokens": ["Vor", "Kai\u00b7ser", "Karl", "und", "sei\u00b7nem", "Ge\u00b7sind'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da lie\u00df er sein Horn erschallen,", "tokens": ["Da", "lie\u00df", "er", "sein", "Horn", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Er blies so laut, er blies so lind,", "tokens": ["Er", "blies", "so", "laut", ",", "er", "blies", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das t\u00e4t dem Kaiser gefallen:", "tokens": ["Das", "t\u00e4t", "dem", "Kai\u00b7ser", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Spielemann, mein Spielemann,", "tokens": ["\u00bb", "mein", "Spie\u00b7le\u00b7mann", ",", "mein", "Spie\u00b7le\u00b7mann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Horn hat hellen Ton,", "tokens": ["Dein", "Horn", "hat", "hel\u00b7len", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was das Horn erreichen kann,", "tokens": ["Und", "was", "das", "Horn", "er\u00b7rei\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sei des Hornes Lohn.", "tokens": ["Das", "sei", "des", "Hor\u00b7nes", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf hohem Berg, in weiter Au',", "tokens": ["Auf", "ho\u00b7hem", "Berg", ",", "in", "wei\u00b7ter", "Au'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sollst Du's blasen am Rheine,", "tokens": ["Da", "sollst", "Du's", "bla\u00b7sen", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Soweit man's h\u00f6rt im ganzen Gau,", "tokens": ["So\u00b7weit", "man's", "h\u00f6rt", "im", "gan\u00b7zen", "Gau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei alles Land das Deine!\u00ab", "tokens": ["Sei", "al\u00b7les", "Land", "das", "Dei\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "PPOSAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Spielmann auf dem Berge stand,", "tokens": ["Der", "Spiel\u00b7mann", "auf", "dem", "Ber\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ringsum viel Rebenh\u00fcgel,", "tokens": ["Ring\u00b7sum", "viel", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und blaues Gebirg' und gr\u00fcnes Land", "tokens": ["Und", "blau\u00b7es", "Ge\u00b7bir\u00b7g'", "und", "gr\u00fc\u00b7nes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und blitzender Str\u00f6me Spiegel.", "tokens": ["Und", "blit\u00b7zen\u00b7der", "Str\u00f6\u00b7me", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er setzte das Horn wohl an den Mund,", "tokens": ["Er", "setz\u00b7te", "das", "Horn", "wohl", "an", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sich selber auf den Rasen,", "tokens": ["Sich", "sel\u00b7ber", "auf", "den", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weit in die Rund', aus Herzensgrund,", "tokens": ["Weit", "in", "die", "Rund'", ",", "aus", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da t\u00e4t er blasen und blasen.", "tokens": ["Da", "t\u00e4t", "er", "bla\u00b7sen", "und", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es war zuerst ein schwimmender Hall,", "tokens": ["Es", "war", "zu\u00b7erst", "ein", "schwim\u00b7men\u00b7der", "Hall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und dann ein hallend Geschmetter,", "tokens": ["Und", "dann", "ein", "hal\u00b7lend", "Ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Westwind schwieg und der Wasserfall,", "tokens": ["Der", "West\u00b7wind", "schwieg", "und", "der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schwieg das Rauschen der Bl\u00e4tter.", "tokens": ["Es", "schwieg", "das", "Rau\u00b7schen", "der", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Bergeskuppen, die Schl\u00f6sser drauf,", "tokens": ["Die", "Ber\u00b7ges\u00b7kup\u00b7pen", ",", "die", "Schl\u00f6s\u00b7ser", "drauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die neigten sich horchend hin\u00fcber,", "tokens": ["Die", "neig\u00b7ten", "sich", "hor\u00b7chend", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Den Flug, den hielten die Adler auf,", "tokens": ["Den", "Flug", ",", "den", "hiel\u00b7ten", "die", "Ad\u00b7ler", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schwammen lautlos dar\u00fcber.", "tokens": ["Und", "schwam\u00b7men", "laut\u00b7los", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und lustiger blies der Spielemann,", "tokens": ["Und", "lus\u00b7ti\u00b7ger", "blies", "der", "Spie\u00b7le\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er blies zum wirbelnden Tanze,", "tokens": ["Er", "blies", "zum", "wir\u00b7beln\u00b7den", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Eichen fa\u00dften einander an", "tokens": ["Die", "Ei\u00b7chen", "fa\u00df\u00b7ten", "ein\u00b7an\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und walzten am Bergeskranze.", "tokens": ["Und", "walz\u00b7ten", "am", "Ber\u00b7ges\u00b7kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Schnitter warfen die Sensen fort,", "tokens": ["Die", "Schnit\u00b7ter", "war\u00b7fen", "die", "Sen\u00b7sen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Dirnen mu\u00dften sie schwingen;", "tokens": ["Die", "Dir\u00b7nen", "mu\u00df\u00b7ten", "sie", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der alte Rhein im felsigen Bord", "tokens": ["Der", "al\u00b7te", "Rhein", "im", "fel\u00b7si\u00b7gen", "Bord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie ein Kn\u00e4blein wollt' er springen.", "tokens": ["Wie", "ein", "Kn\u00e4\u00b7blein", "wollt'", "er", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Spielmann nahm das Horn vom Mund,", "tokens": ["Der", "Spiel\u00b7mann", "nahm", "das", "Horn", "vom", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War freudig aus der Ma\u00dfen,", "tokens": ["War", "freu\u00b7dig", "aus", "der", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Dorf und Weiler in der Rund',", "tokens": ["Durch", "Dorf", "und", "Wei\u00b7ler", "in", "der", "Rund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schritt er seine Stra\u00dfen.", "tokens": ["Da", "schritt", "er", "sei\u00b7ne", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbhast Du das Horn geh\u00f6rt?\u00ab fragt' er,", "tokens": ["\u00bb", "hast", "Du", "das", "Horn", "ge\u00b7h\u00f6rt", "?", "\u00ab", "fragt'", "er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "T\u00e4t sich ein Bauer zeigen,", "tokens": ["T\u00e4t", "sich", "ein", "Bau\u00b7er", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und scholl ein \u00bbJa\u00ab zur Antwort her,", "tokens": ["Und", "scholl", "ein", "\u00bb", "Ja", "\u00ab", "zur", "Ant\u00b7wort", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "$(", "PTKANT", "$(", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Rief er: \u00bbDu bist mein eigen!\u00ab", "tokens": ["Rief", "er", ":", "\u00bb", "Du", "bist", "mein", "ei\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich wollt', ich w\u00e4r' ein Spielemann", "tokens": ["Ich", "wollt'", ",", "ich", "w\u00e4r'", "ein", "Spie\u00b7le\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit solcher Klanggewalt,", "tokens": ["Mit", "sol\u00b7cher", "Klang\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alles k\u00e4m' in meinen Bann,", "tokens": ["Da\u00df", "al\u00b7les", "k\u00e4m'", "in", "mei\u00b7nen", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit mein Lied erschallt.", "tokens": ["So", "weit", "mein", "Lied", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Land und Leut', nicht Burg und Wald,", "tokens": ["Nicht", "Land", "und", "Leut'", ",", "nicht", "Burg", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sollten vor mir sich neigen;", "tokens": ["Die", "soll\u00b7ten", "vor", "mir", "sich", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich wollte nur, wo es widerhallt,", "tokens": ["Ich", "woll\u00b7te", "nur", ",", "wo", "es", "wi\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "W\u00e4r' jedes Herz mein eigen.", "tokens": ["W\u00e4r'", "je\u00b7des", "Herz", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}