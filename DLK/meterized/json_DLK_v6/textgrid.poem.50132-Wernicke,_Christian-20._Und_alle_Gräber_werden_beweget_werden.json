{"textgrid.poem.50132": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "20. Und alle Gr\u00e4ber werden beweget werden", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird die ", "tokens": ["Es", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In das man unsers Leibs ", "tokens": ["In", "das", "man", "un\u00b7sers", "Leibs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn der Lebens-F\u00fcrst erscheinen wird, und wenn", "tokens": ["Doch", "wenn", "der", "Le\u00b7bens\u00b7F\u00fcrst", "er\u00b7schei\u00b7nen", "wird", ",", "und", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "VAFIN", "$,", "KON", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die donnernde Posaun erschallen wird auf Erden,", "tokens": ["Die", "don\u00b7nern\u00b7de", "Po\u00b7saun", "er\u00b7schal\u00b7len", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wird das ", "tokens": ["Denn", "wird", "das"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}