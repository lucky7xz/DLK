{"textgrid.poem.52827": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00fchl des Lesers von Schillers Gedichten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleiner Mensch, der du im Weltgew\u00fchle,", "tokens": ["Klei\u00b7ner", "Mensch", ",", "der", "du", "im", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Tropfen in dem Meer vergehst,", "tokens": ["Wie", "ein", "Trop\u00b7fen", "in", "dem", "Meer", "ver\u00b7gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und im Nichts durchbohrenden Gef\u00fchle", "tokens": ["Und", "im", "Nichts", "durch\u00b7boh\u00b7ren\u00b7den", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor dem eignen Werk erschrocken stehst,", "tokens": ["Vor", "dem", "eig\u00b7nen", "Werk", "er\u00b7schro\u00b7cken", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fl\u00f6tetest du gleich mit Engelzungen", "tokens": ["Fl\u00f6\u00b7te\u00b7test", "du", "gleich", "mit", "En\u00b7gel\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Blieben deine Worte ja zu grob,", "tokens": ["Blie\u00b7ben", "dei\u00b7ne", "Wor\u00b7te", "ja", "zu", "grob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch, von edler Dankbarkeit durchdrungen,", "tokens": ["Doch", ",", "von", "ed\u00b7ler", "Dank\u00b7bar\u00b7keit", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Stammelst du ein unbescheidnes Lob!", "tokens": ["Stam\u00b7melst", "du", "ein", "un\u00b7be\u00b7scheid\u00b7nes", "Lob", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}