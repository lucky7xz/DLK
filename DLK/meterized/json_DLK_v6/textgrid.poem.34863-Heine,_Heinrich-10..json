{"textgrid.poem.34863": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freilich, ein ungl\u00e4ub'ger Thomas,", "tokens": ["Frei\u00b7lich", ",", "ein", "un\u00b7gl\u00e4ub'\u00b7ger", "Tho\u00b7mas", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Glaub ich an den Himmel nicht,", "tokens": ["Glaub", "ich", "an", "den", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die Kirchenlehre Romas", "tokens": ["Den", "die", "Kir\u00b7chen\u00b7leh\u00b7re", "Ro\u00b7mas"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Jerusalems verspricht.", "tokens": ["Und", "Je\u00b7ru\u00b7sa\u00b7lems", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Doch die Existenz der Engel,", "tokens": ["Doch", "die", "E\u00b7xis\u00b7tenz", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die bezweifelte ich nie;", "tokens": ["Die", "be\u00b7zwei\u00b7fel\u00b7te", "ich", "nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lichtgesch\u00f6pfe sonder M\u00e4ngel,", "tokens": ["Licht\u00b7ge\u00b7sch\u00f6p\u00b7fe", "son\u00b7der", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier auf Erden wandeln sie.", "tokens": ["Hier", "auf", "Er\u00b7den", "wan\u00b7deln", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur, gen\u00e4d'ge Frau, die Fl\u00fcgel", "tokens": ["Nur", ",", "ge\u00b7n\u00e4d'\u00b7ge", "Frau", ",", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprech ich jenen Wesen ab;", "tokens": ["Sprech", "ich", "je\u00b7nen", "We\u00b7sen", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel gibt es ohne Fl\u00fcgel,", "tokens": ["En\u00b7gel", "gibt", "es", "oh\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich selbst gesehen hab.", "tokens": ["Wie", "ich", "selbst", "ge\u00b7se\u00b7hen", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieblich mit den wei\u00dfen H\u00e4nden,", "tokens": ["Lieb\u00b7lich", "mit", "den", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich mit dem sch\u00f6nen Blick", "tokens": ["Lieb\u00b7lich", "mit", "dem", "sch\u00f6\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctzen sie den Menschen, wenden", "tokens": ["Sch\u00fct\u00b7zen", "sie", "den", "Men\u00b7schen", ",", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ART", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von ihm ab das Mi\u00dfgeschick.", "tokens": ["Von", "ihm", "ab", "das", "Mi\u00df\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihre Huld und ihre Gnaden", "tokens": ["Ih\u00b7re", "Huld", "und", "ih\u00b7re", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6sten jeden, doch zumeist", "tokens": ["Tr\u00f6s\u00b7ten", "je\u00b7den", ",", "doch", "zu\u00b7meist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PIAT", "$,", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn, der doppelt qualbeladen,", "tokens": ["Ihn", ",", "der", "dop\u00b7pelt", "qual\u00b7be\u00b7la\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn, den man den Dichter hei\u00dft.", "tokens": ["Ihn", ",", "den", "man", "den", "Dich\u00b7ter", "hei\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}}}}}