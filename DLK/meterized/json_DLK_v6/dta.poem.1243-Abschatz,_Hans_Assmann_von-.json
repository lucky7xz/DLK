er": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Welt die g\u00f6ldnen Blicke/", "tokens": ["Uns\u00b7rer", "Welt", "die", "g\u00f6ld\u00b7nen", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4sset alles/ was man sieht", "tokens": ["L\u00e4s\u00b7set", "al\u00b7les", "/", "was", "man", "sieht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$(", "PWS", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter sich betr\u00fcbt zur\u00fccke;", "tokens": ["Hin\u00b7ter", "sich", "be\u00b7tr\u00fcbt", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht/ wie so in Trauren steht", "tokens": ["Seht", "/", "wie", "so", "in", "Trau\u00b7ren", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOKOM", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn Lisillens Sonn entgeht.", "tokens": ["Wenn", "Li\u00b7sil\u00b7lens", "Sonn", "ent\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H-enckers H\u00e4nde k\u00f6nnen nicht", "tokens": ["H\u00b7\u00b7en\u00b7ckers", "H\u00e4n\u00b7de", "k\u00f6n\u00b7nen", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uber wenig Tage qu\u00e4len:", "tokens": ["U\u00b7ber", "we\u00b7nig", "Ta\u00b7ge", "qu\u00e4\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer Lisillens sich entbricht/", "tokens": ["Wer", "Li\u00b7sil\u00b7lens", "sich", "ent\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PRF", "VVPP", "$("], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Kan der Pein kein Ende z\u00e4hlen:", "tokens": ["Kan", "der", "Pein", "kein", "En\u00b7de", "z\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Qual und Sorge frist ihn ab/", "tokens": ["Qual", "und", "Sor\u00b7ge", "frist", "ihn", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben ist sein t\u00e4glich Grab.", "tokens": ["Le\u00b7ben", "ist", "sein", "t\u00e4g\u00b7lich", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "I-st gleich in der Todten-Zunfft", "tokens": ["I\u00b7st", "gleich", "in", "der", "Tod\u00b7ten\u00b7Zunfft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der erbla\u00dfte C\u00f6rper kommen/", "tokens": ["Der", "er\u00b7bla\u00df\u00b7te", "C\u00f6r\u00b7per", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt ihm doch die Wiederk