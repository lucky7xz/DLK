"-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Uns noch Gl\u00fcck und Heyl steht offen?", "tokens": ["Uns", "noch", "Gl\u00fcck", "und", "Heyl", "steht", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nne mir drauff einen Ku\u00df", "tokens": ["G\u00f6n\u00b7ne", "mir", "drauff", "ei\u00b7nen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh ich dich verlassen mu\u00df.", "tokens": ["Eh", "ich", "dich", "ver\u00b7las\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}