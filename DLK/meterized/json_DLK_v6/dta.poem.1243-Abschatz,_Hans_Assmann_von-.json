["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VAPP", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B-itter ist der Galle Safft/", "tokens": ["B\u00b7\u00b7it\u00b7ter", "ist", "der", "Gal\u00b7le", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bitter was aus Wermut quillet/", "tokens": ["Bit\u00b7ter", "was", "aus", "Wer\u00b7mut", "quil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der schwartzen Pillen Krafft", "tokens": ["Was", "der", "schwart\u00b7zen", "Pil\u00b7len", "Krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Myrrh und Aloe verh\u00fcllet;", "tokens": ["Myrrh", "und", "A\u00b7loe", "ver\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Doch dein Scheiden bildt mir ein/", "tokens": ["Doch", "dein", "Schei\u00b7den", "bildt", "mir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jenes m\u00fcsse Zucker seyn.", "tokens": ["Je\u00b7nes", "m\u00fcs\u00b7se", "Zu\u00b7cker", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S-chwer ists/ wenn der m\u00fcde Geist", "toke