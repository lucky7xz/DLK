eter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Schreines, der des Daseins holdste Gaben", "tokens": ["Schrei\u00b7nes", ",", "der", "des", "Da\u00b7seins", "holds\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Alle in sich birgt. Willst du sie nehmen?", "tokens": ["Al\u00b7le", "in", "sich", "birgt", ".", "Willst", "du", "sie", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PRF", "VVFIN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Bebend griff ich nach dem goldnen K\u00e4stchen,", "tokens": ["Be\u00b7bend", "griff", "ich", "nach", "dem", "gold\u00b7nen", "K\u00e4st\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das mir nun die Wunderbare reichte,", "tokens": ["Das", "mir", "nun", "die", "Wun\u00b7der\u00b7ba\u00b7re", "reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und ich sprach, mit Schleiern vor den Blicken:", "tokens": ["Und", "ich", "sprach", ",", "mit", "Schlei\u00b7ern", "vor", "den", "Bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gib! Ich wei\u00df, die Gaben der Pandora", "tokens": ["Gib", "!", "Ich", "wei\u00df", ",", "die", "Ga\u00b7ben", "der", "Pan\u00b7do\u00b7ra"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Hei\u00dfen \u00dcbel, und die Weisen fliehn sie,", "tokens": ["Hei\u00b7\u00dfen", "\u00dc\u00b7bel", ",", "und", "die", "Wei\u00b7sen", "fliehn", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Murmelnd, da\u00df sie Gift und Wahnsinn seien,", "tokens": ["Mur\u00b7melnd", ",", "da\u00df", "sie", "Gift", "und", "Wahn\u00b7sinn", "sei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nebelg\u00fcter, die das Licht der Wahrheit,", "tokens": ["Ne\u00b7bel\u00b7g\u00fc\u00b7ter", ",", "die", "das", "Licht", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Scheinbar hold, doch tr\u00fcgerisch verh\u00e4ngen.", "tokens": ["Schein\u00b7bar", "hold", ",", "doch", "tr\u00fc\u00b7ge\u00b7risch", "ver\u00b7h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ach, die Weisen mit den blinden Augen!", "tokens": ["Ach", ",", "die", "Wei\u00b7sen", "mit", "den", "blin\u00b7den", "Au\