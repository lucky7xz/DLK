{"textgrid.poem.24243": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Pandora", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich heute fr\u00fch im sch\u00f6nen Parke,", "tokens": ["Als", "ich", "heu\u00b7te", "fr\u00fch", "im", "sch\u00f6\u00b7nen", "Par\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der voll lauter Birken steht, spazierte,", "tokens": ["Der", "voll", "lau\u00b7ter", "Bir\u00b7ken", "steht", ",", "spa\u00b7zier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sah ich (nun, ihr brauchts ja nicht zu glauben)", "tokens": ["Sah", "ich", "(", "nun", ",", "ihr", "brauchts", "ja", "nicht", "zu", "glau\u00b7ben", ")"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine nackte Dame auf mich zugehn.", "tokens": ["Ei\u00b7ne", "nack\u00b7te", "Da\u00b7me", "auf", "mich", "zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+----+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "\u2013 Sag, wer bist du, sprach ich, nackte Dame?", "tokens": ["\u2013", "Sag", ",", "wer", "bist", "du", ",", "sprach", "ich", ",", "nack\u00b7te", "Da\u00b7me", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Reizend scheinst du mir und liebensw\u00fcrdig,", "tokens": ["Rei\u00b7zend", "scheinst", "du", "mir", "und", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine auserlesne Augenweide.", "tokens": ["Ei\u00b7ne", "au\u00b7ser\u00b7les\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selten sah ich noch so sch\u00f6ne Beine", "tokens": ["Sel\u00b7ten", "sah", "ich", "noch", "so", "sch\u00f6\u00b7ne", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und so wohlgef\u00fcgte volle Br\u00fcste,", "tokens": ["Und", "so", "wohl\u00b7ge\u00b7f\u00fcg\u00b7te", "vol\u00b7le", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Selten noch so sch\u00f6ne Haut: atlassen", "tokens": ["Sel\u00b7ten", "noch", "so", "sch\u00f6\u00b7ne", "Haut", ":", "at\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "$.", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Gl\u00e4nzend und vom Blut des frohsten Lebens", "tokens": ["Gl\u00e4n\u00b7zend", "und", "vom", "Blut", "des", "frohs\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So von innen her erw\u00e4rmt, durchleuchtet.", "tokens": ["So", "von", "in\u00b7nen", "her", "er\u00b7w\u00e4rmt", ",", "durch\u00b7leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "APZR", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "O wie sch\u00f6n (la\u00df mich dich nah betrachten!),", "tokens": ["O", "wie", "sch\u00f6n", "(", "la\u00df", "mich", "dich", "nah", "be\u00b7trach\u00b7ten", "!", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$(", "VVIMP", "PPER", "PRF", "ADJD", "VVINF", "$.", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "O wie sch\u00f6n sind deiner wei\u00dfen Br\u00fcste", "tokens": ["O", "wie", "sch\u00f6n", "sind", "dei\u00b7ner", "wei\u00b7\u00dfen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Blasse Rosen, holder Fr\u00fchlingsgarten.", "tokens": ["Blas\u00b7se", "Ro\u00b7sen", ",", "hol\u00b7der", "Fr\u00fch\u00b7lings\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ach, und welch Entz\u00fccken darf ich f\u00fchlen,", "tokens": ["Ach", ",", "und", "welch", "Ent\u00b7z\u00fc\u00b7cken", "darf", "ich", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PWAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sieht mein Aug den Glanz der blonden Haare,", "tokens": ["Sieht", "mein", "Aug", "den", "Glanz", "der", "blon\u00b7den", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Wie sie von der Stirn im sch\u00f6nsten Bogen,", "tokens": ["Wie", "sie", "von", "der", "Stirn", "im", "sch\u00f6ns\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Aber wellig, hinten\u00fcber fallen", "tokens": ["A\u00b7ber", "wel\u00b7lig", ",", "hin\u00b7ten\u00b7\u00fc\u00b7ber", "fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und hinab den R\u00fccken flie\u00dfen bis zum", "tokens": ["Und", "hin\u00b7ab", "den", "R\u00fc\u00b7cken", "flie\u00b7\u00dfen", "bis", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "APPRART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Wohlig rundlichen Geschwisterpaare", "tokens": ["Woh\u00b7lig", "rund\u00b7li\u00b7chen", "Ge\u00b7schwis\u00b7ter\u00b7paa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Zweier ganz vollkommnen Hemigloben.", "tokens": ["Zwei\u00b7er", "ganz", "voll\u00b7komm\u00b7nen", "He\u00b7mig\u00b7lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Diese Gr\u00fcbchenh\u00e4nde! Diese F\u00fc\u00dfchen!", "tokens": ["Die\u00b7se", "Gr\u00fcb\u00b7chen\u00b7h\u00e4n\u00b7de", "!", "Die\u00b7se", "F\u00fc\u00df\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Jedes Glied ein tadellos Gebilde,", "tokens": ["Je\u00b7des", "Glied", "ein", "ta\u00b7del\u00b7los", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Jedes N\u00e4gelchen ein Schild der Sch\u00f6nheit,", "tokens": ["Je\u00b7des", "N\u00e4\u00b7gel\u00b7chen", "ein", "Schild", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Und der Mund: des Eros goldner Bogen.", "tokens": ["Und", "der", "Mund", ":", "des", "E\u00b7ros", "gold\u00b7ner", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn aus Gold sind deine Lippen, \u2013 seltsam,", "tokens": ["Denn", "aus", "Gold", "sind", "dei\u00b7ne", "Lip\u00b7pen", ",", "\u2013", "selt\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$,", "$(", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bernsteingoldig deine Augen, \u2013 seltsam,", "tokens": ["Bern\u00b7stein\u00b7gol\u00b7dig", "dei\u00b7ne", "Au\u00b7gen", ",", "\u2013", "selt\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "$(", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und dein Nabel eine wei\u00dfe Perle.", "tokens": ["Und", "dein", "Na\u00b7bel", "ei\u00b7ne", "wei\u00b7\u00dfe", "Per\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ei, was tr\u00e4gst du da in deinen H\u00e4nden?", "tokens": ["Ei", ",", "was", "tr\u00e4gst", "du", "da", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Solch ein kostbar K\u00e4stchen sah ich niemals:", "tokens": ["Solch", "ein", "kost\u00b7bar", "K\u00e4st\u00b7chen", "sah", "ich", "nie\u00b7mals", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mattes Gold, durchsetzt mit Glutrubinen,", "tokens": ["Mat\u00b7tes", "Gold", ",", "durch\u00b7setzt", "mit", "Glu\u00b7tru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die, ein Rosenkranz von Licht und Farbe,", "tokens": ["Die", ",", "ein", "Ro\u00b7sen\u00b7kranz", "von", "Licht", "und", "Far\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ringsherum wie trunkne Augen leuchten.", "tokens": ["Rings\u00b7he\u00b7rum", "wie", "trunk\u00b7ne", "Au\u00b7gen", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Also sprach ich und sank in die Knie,", "tokens": ["Al\u00b7so", "sprach", "ich", "und", "sank", "in", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "K\u00fc\u00dfte ihrer F\u00fc\u00dfe Lilienbl\u00e4tter", "tokens": ["K\u00fc\u00df\u00b7te", "ih\u00b7rer", "F\u00fc\u00b7\u00dfe", "Li\u00b7li\u00b7en\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und lie\u00df meine Lippen an den F\u00fc\u00dfen,", "tokens": ["Und", "lie\u00df", "mei\u00b7ne", "Lip\u00b7pen", "an", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00fcnschend, da\u00df ich ewig liegen d\u00fcrfte", "tokens": ["W\u00fcn\u00b7schend", ",", "da\u00df", "ich", "e\u00b7wig", "lie\u00b7gen", "d\u00fcrf\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In so selig klarer, voller Andacht.", "tokens": ["In", "so", "se\u00b7lig", "kla\u00b7rer", ",", "vol\u00b7ler", "An\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Da umflossen mich die gelben Haare", "tokens": ["Da", "um\u00b7flos\u00b7sen", "mich", "die", "gel\u00b7ben", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie ein Strom von allen Wohlger\u00fcchen:", "tokens": ["Wie", "ein", "Strom", "von", "al\u00b7len", "Wohl\u00b7ge\u00b7r\u00fc\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Rosen, Veilchen, Lilien und Narzissen,", "tokens": ["Ro\u00b7sen", ",", "Veil\u00b7chen", ",", "Li\u00b7li\u00b7en", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Alle eins geworden, alle D\u00fcfte", "tokens": ["Al\u00b7le", "eins", "ge\u00b7wor\u00b7den", ",", "al\u00b7le", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "PIS", "VAPP", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Fr\u00fchlings und des Sommers eins geworden:", "tokens": ["Fr\u00fch\u00b7lings", "und", "des", "Som\u00b7mers", "eins", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "CARD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00f6chsten Lebens Atem, stark und lieblich.", "tokens": ["H\u00f6chs\u00b7ten", "Le\u00b7bens", "A\u00b7tem", ",", "stark", "und", "lieb\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Sch\u00f6ne sprach zu meinen H\u00e4upten:", "tokens": ["Und", "die", "Sch\u00f6\u00b7ne", "sprach", "zu", "mei\u00b7nen", "H\u00e4up\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Weil du gl\u00e4ubig bist und immer wieder", "tokens": ["Weil", "du", "gl\u00e4u\u00b7big", "bist", "und", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Deine H\u00e4nde, Adorant der Sch\u00f6nheit,", "tokens": ["Dei\u00b7ne", "H\u00e4n\u00b7de", ",", "A\u00b7do\u00b7rant", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Betend, hoffend hebst ins Licht der Sonne,", "tokens": ["Be\u00b7tend", ",", "hof\u00b7fend", "hebst", "ins", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Unbeirrt ein J\u00fcnger der Bejahung", "tokens": ["Un\u00b7be\u00b7irrt", "ein", "J\u00fcn\u00b7ger", "der", "Be\u00b7ja\u00b7hung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und der K\u00fcnste allertreuster Diener,", "tokens": ["Und", "der", "K\u00fcns\u00b7te", "al\u00b7ler\u00b7treus\u00b7ter", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Bin ich Nackte vor dich hingetreten,", "tokens": ["Bin", "ich", "Nack\u00b7te", "vor", "dich", "hin\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Ich: Pandora, des Hephaistos Bildwerk.", "tokens": ["Ich", ":", "Pan\u00b7do\u00b7ra", ",", "des", "He\u00b7phais\u00b7tos", "Bild\u00b7werk", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ich entstamme nicht der Kraft der Lenden,", "tokens": ["Ich", "ent\u00b7stam\u00b7me", "nicht", "der", "Kraft", "der", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Mich erschuf die Kunst des Feuergottes;", "tokens": ["Mich", "er\u00b7s