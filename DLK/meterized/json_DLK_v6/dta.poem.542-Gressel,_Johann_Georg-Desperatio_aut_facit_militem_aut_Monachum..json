{"dta.poem.542": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Desperatio aut facit militem aut  \n Monachum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich fliehe aus der b\u00f6sen Welt/", "tokens": ["Ich", "flie\u00b7he", "aus", "der", "b\u00f6\u00b7sen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil mir ihr Wesen nicht gef\u00e4llt;", "tokens": ["Weil", "mir", "ihr", "We\u00b7sen", "nicht", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mi\u00dfmuth treibet meine Sinnen", "tokens": ["Der", "Mi\u00df\u00b7muth", "trei\u00b7bet", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dem/ was ich jetzt mu\u00df beginnen.", "tokens": ["Zu", "dem", "/", "was", "ich", "jetzt", "mu\u00df", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ziehe Nonnen-Kleider an/", "tokens": ["Ich", "zie\u00b7he", "Non\u00b7nen\u00b7Klei\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sonsten nichts mehr helffen kan/", "tokens": ["Weil", "sons\u00b7ten", "nichts", "mehr", "helf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kan ich schon kein Sch\u00e4tzgen haben/", "tokens": ["Kan", "ich", "schon", "kein", "Sch\u00e4tz\u00b7gen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So wird mich doch ein ", "tokens": ["So", "wird", "mich", "doch", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}