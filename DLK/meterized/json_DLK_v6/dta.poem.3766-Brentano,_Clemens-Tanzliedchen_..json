{"dta.poem.3766": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzliedchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "sw:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tanz Kindlein tanz,               ", "tokens": ["Tanz", "Kin\u00b7dlein", "tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Deine Sch\u00fchlein sind noch ganz,", "tokens": ["Dei\u00b7ne", "Sch\u00fch\u00b7lein", "sind", "noch", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dir sie nit gereue,", "tokens": ["La\u00df", "dir", "sie", "nit", "ge\u00b7reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Schuster macht dir neue.", "tokens": ["Der", "Schus\u00b7ter", "macht", "dir", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}