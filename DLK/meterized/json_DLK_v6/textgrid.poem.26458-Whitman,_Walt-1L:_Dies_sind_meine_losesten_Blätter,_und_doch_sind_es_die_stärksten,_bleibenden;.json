{"textgrid.poem.26458": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies sind meine losesten Bl\u00e4tter, und doch sind es die st\u00e4rksten, bleibenden;", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies sind meine losesten Bl\u00e4tter, und doch sind es die st\u00e4rksten, bleibenden;", "tokens": ["Dies", "sind", "mei\u00b7ne", "lo\u00b7ses\u00b7ten", "Bl\u00e4t\u00b7ter", ",", "und", "doch", "sind", "es", "die", "st\u00e4rks\u00b7ten", ",", "blei\u00b7ben\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+--+--+---+--+-+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In ihnen beschatte und verh\u00fclle ich meine Gedanken \u2013 ich offenbare sie nicht,", "tokens": ["In", "ih\u00b7nen", "be\u00b7schat\u00b7te", "und", "ver\u00b7h\u00fcl\u00b7le", "ich", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "\u2013", "ich", "of\u00b7fen\u00b7ba\u00b7re", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+--+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Aber sie offenbaren mich mehr als alle andern Gedichte.", "tokens": ["A\u00b7ber", "sie", "of\u00b7fen\u00b7ba\u00b7ren", "mich", "mehr", "als", "al\u00b7le", "an\u00b7dern", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "KOKOM", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+--+-", "measure": "iambic.septa.invert"}}}}}