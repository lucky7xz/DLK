{"textgrid.poem.48643": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Palinode", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin tot; mein Tod, der lebt,", "tokens": ["Ich", "bin", "tot", ";", "mein", "Tod", ",", "der", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und ich leb' in meinem Tode.", "tokens": ["und", "ich", "leb'", "in", "mei\u00b7nem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Tod, der ist ", "tokens": ["Mein", "Tod", ",", "der", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "die mir so zuwider strebt.", "tokens": ["die", "mir", "so", "zu\u00b7wi\u00b7der", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie, meine ", "tokens": ["Und", "sie", ",", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "lebt und ist doch auch im Tode.", "tokens": ["lebt", "und", "ist", "doch", "auch", "im", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O da s\u00fc\u00dfe Tochter du", "tokens": ["O", "da", "s\u00fc\u00b7\u00dfe", "Toch\u00b7ter", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "der auch s\u00fc\u00dfen Pierinnen,", "tokens": ["der", "auch", "s\u00fc\u00b7\u00dfen", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "du Bezwingerin der Sinnen,", "tokens": ["du", "Be\u00b7zwin\u00b7ge\u00b7rin", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sich g\u00f6nnen keiner Ruh',", "tokens": ["die", "sich", "g\u00f6n\u00b7nen", "kei\u00b7ner", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVINF", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ph\u00f6bus hat dir das gegeben,", "tokens": ["Ph\u00f6\u00b7bus", "hat", "dir", "das", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df du Tote bringst ins Leben!", "tokens": ["da\u00df", "du", "To\u00b7te", "bringst", "ins", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Musik, edler G\u00f6tter Gast,", "tokens": ["Mu\u00b7sik", ",", "ed\u00b7ler", "G\u00f6t\u00b7ter", "Gast", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gieb ihr Leben, doch ihr Leben!", "tokens": ["gieb", "ihr", "Le\u00b7ben", ",", "doch", "ihr", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird sie mir wieder geben", "tokens": ["So", "wird", "sie", "mir", "wie\u00b7der", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "was du ihr geschenket hast.", "tokens": ["was", "du", "ihr", "ge\u00b7schen\u00b7ket", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df, was ihr ist und auch meine,", "tokens": ["Di\u00df", ",", "was", "ihr", "ist", "und", "auch", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VAFIN", "KON", "ADV", "PPOSAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "bleibt doch, G\u00f6ttin, allzeit deine.", "tokens": ["bleibt", "doch", ",", "G\u00f6t\u00b7tin", ",", "all\u00b7zeit", "dei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}