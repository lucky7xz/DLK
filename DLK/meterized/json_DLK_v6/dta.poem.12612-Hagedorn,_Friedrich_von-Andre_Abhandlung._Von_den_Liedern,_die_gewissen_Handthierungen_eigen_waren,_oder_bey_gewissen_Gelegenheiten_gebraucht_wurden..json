{"dta.poem.12612": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Andre Abhandlung.  \n Von den Liedern, die gewissen Handthierungen eigen waren,  \n oder bey gewissen Gelegenheiten gebraucht wurden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie braucht der Ammen Kunst, singt beider Kinder Ohr", "tokens": ["Sie", "braucht", "der", "Am\u00b7men", "Kunst", ",", "singt", "bei\u00b7der", "Kin\u00b7der", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein s\u00fcsses Liedchen vor.", "tokens": ["Ein", "s\u00fcs\u00b7ses", "Lied\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies Liedchen lockt den Schlaf, er k\u00f6mmt, und beide Br\u00fcder", "tokens": ["Dies", "Lied\u00b7chen", "lockt", "den", "Schlaf", ",", "er", "k\u00f6mmt", ",", "und", "bei\u00b7de", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschliessen schon die Augenlieder.", "tokens": ["Ver\u00b7schlies\u00b7sen", "schon", "die", "Au\u00b7gen\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}