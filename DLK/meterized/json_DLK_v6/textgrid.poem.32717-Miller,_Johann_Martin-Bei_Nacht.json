{"textgrid.poem.32717": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Bei Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, frohe Nacht, die du", "tokens": ["Will\u00b7kom\u00b7men", ",", "fro\u00b7he", "Nacht", ",", "die", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sch\u00f6nsten Tag vollendest,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Tag", "voll\u00b7en\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der Erinnrung s\u00fc\u00dfe Ruh", "tokens": ["Und", "der", "E\u00b7rinn\u00b7rung", "s\u00fc\u00b7\u00dfe", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Taumelfreuden sendest!", "tokens": ["Nach", "Tau\u00b7mel\u00b7freu\u00b7den", "sen\u00b7dest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wisch' aller Augen Thr\u00e4nen ab,", "tokens": ["Wisch'", "al\u00b7ler", "Au\u00b7gen", "Thr\u00e4\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch im Dunkel flie\u00dfen!", "tokens": ["Die", "noch", "im", "Dun\u00b7kel", "flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df jedes Gl\u00fcck, das mich umgab,", "tokens": ["La\u00df", "je\u00b7des", "Gl\u00fcck", ",", "das", "mich", "um\u00b7gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich noch einmal genie\u00dfen!", "tokens": ["Mich", "noch", "ein\u00b7mal", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Augen, die ihr heller mir,", "tokens": ["Ihr", "Au\u00b7gen", ",", "die", "ihr", "hel\u00b7ler", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als diese Sterne, lachtet,", "tokens": ["Als", "die\u00b7se", "Ster\u00b7ne", ",", "lach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ich mit s\u00fc\u00dferer Begier,", "tokens": ["Die", "ich", "mit", "s\u00fc\u00b7\u00dfe\u00b7rer", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als diesen Mond, betrachtet!", "tokens": ["Als", "die\u00b7sen", "Mond", ",", "be\u00b7trach\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die ihr, wie dieser Silberschein,", "tokens": ["Die", "ihr", ",", "wie", "die\u00b7ser", "Sil\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Freuden, mich umwalltet!", "tokens": ["Ihr", "Freu\u00b7den", ",", "mich", "um\u00b7wall\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Lieder, die ihr s\u00fc\u00df und rein,", "tokens": ["Ihr", "Lie\u00b7der", ",", "die", "ihr", "s\u00fc\u00df", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Abendfl\u00f6ten, schalltet!", "tokens": ["Wie", "A\u00b7bend\u00b7fl\u00f6\u00b7ten", ",", "schall\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du reine Seele, die du mich", "tokens": ["Du", "rei\u00b7ne", "See\u00b7le", ",", "die", "du", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Engelsku\u00df begl\u00fccktest,", "tokens": ["Durch", "En\u00b7gels\u00b7ku\u00df", "be\u00b7gl\u00fcck\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mehr, wie diese Stille, mich", "tokens": ["Und", "mehr", ",", "wie", "die\u00b7se", "Stil\u00b7le", ",", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PDAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Gott entz\u00fccktest!", "tokens": ["Zu", "mei\u00b7nem", "Gott", "ent\u00b7z\u00fcck\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Komm, meine Liebe, senke dich", "tokens": ["Komm", ",", "mei\u00b7ne", "Lie\u00b7be", ",", "sen\u00b7ke", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir im Traum' hernieder!", "tokens": ["Zu", "mir", "im", "Traum'", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm, s\u00fc\u00dfe Liebe, k\u00fcsse mich", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ",", "k\u00fcs\u00b7se", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So s\u00fc\u00df noch einmal wieder!", "tokens": ["So", "s\u00fc\u00df", "noch", "ein\u00b7mal", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach Gott! Sie schlummert; la\u00df sie ganz", "tokens": ["Ach", "Gott", "!", "Sie", "schlum\u00b7mert", ";", "la\u00df", "sie", "ganz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Wohlgefallen f\u00fchlen!", "tokens": ["Dein", "Wohl\u00b7ge\u00b7fal\u00b7len", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df es, wie Morgenwolkenglanz,", "tokens": ["La\u00df", "es", ",", "wie", "Mor\u00b7gen\u00b7wol\u00b7ken\u00b7glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihre Seele spielen!", "tokens": ["Um", "ih\u00b7re", "See\u00b7le", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Singt, Engel, den Gesang ihr vor,", "tokens": ["Singt", ",", "En\u00b7gel", ",", "den", "Ge\u00b7sang", "ihr", "vor", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ihr dereinst erschallet,", "tokens": ["Der", "ihr", "de\u00b7reinst", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann sie zu Gottes Thron empor,", "tokens": ["Wann", "sie", "zu", "Got\u00b7tes", "Thron", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie stille Flammen, wallet!", "tokens": ["Wie", "stil\u00b7le", "Flam\u00b7men", ",", "wal\u00b7let", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zeigt mich in frommen Tr\u00e4umen ihr,", "tokens": ["Zeigt", "mich", "in", "from\u00b7men", "Tr\u00e4u\u00b7men", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich hier dankend kniee;", "tokens": ["Wie", "ich", "hier", "dan\u00b7kend", "kni\u00b7ee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df immer ihre Seele mir", "tokens": ["Da\u00df", "im\u00b7mer", "ih\u00b7re", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In reiner Liebe gl\u00fche!", "tokens": ["In", "rei\u00b7ner", "Lie\u00b7be", "gl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}