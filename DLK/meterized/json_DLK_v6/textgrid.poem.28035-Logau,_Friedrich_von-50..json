{"textgrid.poem.28035": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De\u00df Todes Anfang zwar bringt mit ein hartes T;", "tokens": ["De\u00df", "To\u00b7des", "An\u00b7fang", "zwar", "bringt", "mit", "ein", "har\u00b7tes", "T", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Ende zeucht nach sich alsdann ein lindes D;", "tokens": ["Das", "En\u00b7de", "zeucht", "nach", "sich", "als\u00b7dann", "ein", "lin\u00b7des", "D", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Mittel ist ein O: es ist ein Augenblick,", "tokens": ["Das", "Mit\u00b7tel", "ist", "ein", "O", ":", "es", "ist", "ein", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So k\u00fcmmt f\u00fcr harte Pein ein immer sanfftes Gl\u00fcck.", "tokens": ["So", "k\u00fcmmt", "f\u00fcr", "har\u00b7te", "Pein", "ein", "im\u00b7mer", "sanff\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}