{"textgrid.poem.40303": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Adultera", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Schatz; komm, Katz; la\u00df das Wimmern!", "tokens": ["Komm", ",", "Schatz", ";", "komm", ",", "Katz", ";", "la\u00df", "das", "Wim\u00b7mern", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$.", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, das darf dich nicht bek\u00fcmmern,", "tokens": ["Nein", ",", "das", "darf", "dich", "nicht", "be\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ob ich auch \u00bbtreu\u00ab bin; r\u00fcck nur her!", "tokens": ["ob", "ich", "auch", "\u00bb", "treu", "\u00ab", "bin", ";", "r\u00fcck", "nur", "her", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$(", "ADJD", "$(", "VAFIN", "$.", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm: ich hab ein Dutzend Seelen,", "tokens": ["Komm", ":", "ich", "hab", "ein", "Dut\u00b7zend", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wer kann all die Kammern z\u00e4hlen,", "tokens": ["wer", "kann", "all", "die", "Kam\u00b7mern", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sechse stehen grade leer.", "tokens": ["sech\u00b7se", "ste\u00b7hen", "gra\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh nicht auf den Ring an meinem Finger;", "tokens": ["Sieh", "nicht", "auf", "den", "Ring", "an", "mei\u00b7nem", "Fin\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hoh, mein Kind, ich bin viel j\u00fcnger", "tokens": ["hoh", ",", "mein", "Kind", ",", "ich", "bin", "viel", "j\u00fcn\u00b7ger"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "als mein narbiges Gesicht", "tokens": ["als", "mein", "nar\u00b7bi\u00b7ges", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wei\u00dft du, die Runzeln und die Hiebe", "tokens": ["Wei\u00dft", "du", ",", "die", "Run\u00b7zeln", "und", "die", "Hie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "thun erst die W\u00fcrze zu Ehre und Liebe!", "tokens": ["thun", "erst", "die", "W\u00fcr\u00b7ze", "zu", "Eh\u00b7re", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ja, mein s\u00fc\u00dfer B\u00f6sewicht:", "tokens": ["Ja", ",", "mein", "s\u00fc\u00b7\u00dfer", "B\u00f6\u00b7se\u00b7wicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel geliebt, noch mehr getrunken,", "tokens": ["Viel", "ge\u00b7liebt", ",", "noch", "mehr", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "manchmal fast im Strom versunken,", "tokens": ["manch\u00b7mal", "fast", "im", "Strom", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "heida wie der Schl\u00e4ger pfiff!", "tokens": ["hei\u00b7da", "wie", "der", "Schl\u00e4\u00b7ger", "pfiff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll das Leben dir was n\u00fctzen,", "tokens": ["Soll", "das", "Le\u00b7ben", "dir", "was", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "lerne auch dein Blut verspr\u00fctzen:", "tokens": ["ler\u00b7ne", "auch", "dein", "Blut", "ver\u00b7spr\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nicht gezuckt! los! blick und triff!", "tokens": ["nicht", "ge\u00b7zuckt", "!", "los", "!", "blick", "und", "triff", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$.", "PTKVZ", "$.", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hast ja auch schon \u2013 Blut verspritzt,", "tokens": ["Hast", "ja", "auch", "schon", "\u2013", "Blut", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$(", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "oft ... ah! wie dein Auge blitzt:", "tokens": ["oft", "...", "ah", "!", "wie", "dein", "Au\u00b7ge", "blitzt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ITJ", "$.", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "z\u00fcrnst wol gar dem frechen Buben?", "tokens": ["z\u00fcrnst", "wol", "gar", "dem", "fre\u00b7chen", "Bu\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was denn: Thr\u00e4nen? o nicht doch! oh!", "tokens": ["Was", "denn", ":", "Thr\u00e4\u00b7nen", "?", "o", "nicht", "doch", "!", "oh", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "$.", "NN", "$.", "FM", "PTKNEG", "ADV", "$.", "FM", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Herzchen, so'was lernt man so", "tokens": ["Herz\u00b7chen", ",", "so'\u00b7was", "lernt", "man", "so"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "in der Luft der Ehestuben!", "tokens": ["in", "der", "Luft", "der", "E\u00b7hes\u00b7tu\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Komm: sei gut, Kind! Gieb mir die Hand!", "tokens": ["Komm", ":", "sei", "gut", ",", "Kind", "!", "Gieb", "mir", "die", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VAFIN", "ADJD", "$,", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Hast mich ja lieb, Kind \u2013 und hast Verstand:", "tokens": ["Hast", "mich", "ja", "lieb", ",", "Kind", "\u2013", "und", "hast", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$(", "KON", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "nein, ich will dich nicht verf\u00fchren.", "tokens": ["nein", ",", "ich", "will", "dich", "nicht", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber gelt, du w\u00e4rst gern Braut?", "tokens": ["A\u00b7ber", "gelt", ",", "du", "w\u00e4rst", "gern", "Braut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier das Venushalsband deiner Haut", "tokens": ["Hier", "das", "Ve\u00b7nus\u00b7hals\u00b7band", "dei\u00b7ner", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "l\u00e4\u00dft verhaltene W\u00fcnsche sp\u00fcren!", "tokens": ["l\u00e4\u00dft", "ver\u00b7hal\u00b7te\u00b7ne", "W\u00fcn\u00b7sche", "sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sieh mich doch an, du: bin kein Dieb!", "tokens": ["Sieh", "mich", "doch", "an", ",", "du", ":", "bin", "kein", "Dieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "$,", "PPER", "$.", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "habe das Halsband nur so lieb", "tokens": ["ha\u00b7be", "das", "Hals\u00b7band", "nur", "so", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und deine dunkeln Augenringe.", "tokens": ["und", "dei\u00b7ne", "dun\u00b7keln", "Au\u00b7gen\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh doch: mein Blick ist ein z\u00fcndender Pfeil,", "tokens": ["Sieh", "doch", ":", "mein", "Blick", "ist", "ein", "z\u00fcn\u00b7den\u00b7der", "Pfeil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "und meine Stimme ein sausendes Seil:", "tokens": ["und", "mei\u00b7ne", "Stim\u00b7me", "ein", "sau\u00b7sen\u00b7des", "Seil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "komm, durch H\u00f6llen und Himmel soll's dich schwingen!", "tokens": ["komm", ",", "durch", "H\u00f6l\u00b7len", "und", "Him\u00b7mel", "soll's", "dich", "schwin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "KON", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}