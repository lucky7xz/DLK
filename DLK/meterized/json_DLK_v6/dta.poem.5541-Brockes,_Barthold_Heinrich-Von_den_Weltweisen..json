{"dta.poem.5541": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist f\u00fcr uns die Welt", "tokens": ["Es", "ist", "f\u00fcr", "uns", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein unn\u00fctz Schauspiel nur. Jhr\u2019 Ordnung, ihr", "tokens": ["Ein", "un\u00b7n\u00fctz", "Schau\u00b7spiel", "nur", ".", "Ih\u00b7r'", "Ord\u00b7nung", ",", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geb\u00e4ude,", "tokens": ["Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Erregen unserm Blick nicht die geringste Freude,", "tokens": ["Er\u00b7re\u00b7gen", "un\u00b7serm", "Blick", "nicht", "die", "ge\u00b7rings\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sind uns nur vergebens vorgestellt.", "tokens": ["Und", "sind", "uns", "nur", "ver\u00b7ge\u00b7bens", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Welch\u2019 stille Tummheit h\u00e4lt, bey solchem Blick,", "tokens": ["Welch'", "stil\u00b7le", "Tumm\u00b7heit", "h\u00e4lt", ",", "bey", "sol\u00b7chem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die neubegierige Verwunderung zur\u00fcck;", "tokens": ["Die", "neu\u00b7be\u00b7gie\u00b7ri\u00b7ge", "Ver\u00b7wun\u00b7de\u00b7rung", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will, wenn sich unsre Sinne regen,", "tokens": ["Will", ",", "wenn", "sich", "uns\u00b7re", "Sin\u00b7ne", "re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Seel allein sich nicht bewegen?", "tokens": ["Die", "Seel", "al\u00b7lein", "sich", "nicht", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Will man bey aller Pracht des Himmels und der Erden,", "tokens": ["Will", "man", "bey", "al\u00b7ler", "Pracht", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PIAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur obenhin ger\u00fchrt, nie recht empfindlich werden?", "tokens": ["Nur", "o\u00b7ben\u00b7hin", "ge\u00b7r\u00fchrt", ",", "nie", "recht", "emp\u00b7find\u00b7lich", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}