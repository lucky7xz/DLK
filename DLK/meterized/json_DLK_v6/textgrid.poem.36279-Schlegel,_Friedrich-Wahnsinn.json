{"textgrid.poem.36279": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wahnsinn", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bittre Schmerzen rei\u00dfen wild.", "tokens": ["Bitt\u00b7re", "Schmer\u00b7zen", "rei\u00b7\u00dfen", "wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz sei mild!", "tokens": ["Herz", "sei", "mild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Denn du magst es doch nicht sagen;", "tokens": ["Denn", "du", "magst", "es", "doch", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer half ja noch dein Klagen,", "tokens": ["Nim\u00b7mer", "half", "ja", "noch", "dein", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit zerbrochen dir dein Bild.", "tokens": ["Seit", "zer\u00b7bro\u00b7chen", "dir", "dein", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tod w\u00e4r' Freude,", "tokens": ["Tod", "w\u00e4r'", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "N\u00e4hme nur die Erd' uns beide!", "tokens": ["N\u00e4h\u00b7me", "nur", "die", "Erd'", "uns", "bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fchlung saugen", "tokens": ["K\u00fch\u00b7lung", "sau\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "M\u00f6chten gern die trocknen Augen,", "tokens": ["M\u00f6ch\u00b7ten", "gern", "die", "trock\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Brennen hei\u00dfer stets im Leide.", "tokens": ["Bren\u00b7nen", "hei\u00b7\u00dfer", "stets", "im", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Laute, diesen Mi\u00dflaut sprich,", "tokens": ["Lau\u00b7te", ",", "die\u00b7sen", "Mi\u00df\u00b7laut", "sprich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dann brich,", "tokens": ["Und", "dann", "brich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Eh' ich ganz in Ha\u00df versunken,", "tokens": ["Eh'", "ich", "ganz", "in", "Ha\u00df", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahnsinn rede todestrunken,", "tokens": ["Wahn\u00b7sinn", "re\u00b7de", "to\u00b7des\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die Einz'ge von mir wich.", "tokens": ["Weil", "die", "Einz'\u00b7ge", "von", "mir", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebt mir Blut,", "tokens": ["Gebt", "mir", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df ich lindre diese Glut,", "tokens": ["Da\u00df", "ich", "lind\u00b7re", "die\u00b7se", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wer's tat,", "tokens": ["Und", "wer's", "tat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ewig schmacht' er ohne Rat,", "tokens": ["E\u00b7wig", "schmacht'", "er", "oh\u00b7ne", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Oder sink' in gleiche Flut.", "tokens": ["O\u00b7der", "sink'", "in", "glei\u00b7che", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}