{"dta.poem.8730": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ix .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem hohen G\u00f6tterpferde?", "tokens": ["Auf", "dem", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7pfer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Bellerophon zur Erde", "tokens": ["Wie", "Bel\u00b7le\u00b7ro\u00b7phon", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bebst du nicht zur\u00fcck zu sinken?", "tokens": ["Bebst", "du", "nicht", "zu\u00b7r\u00fcck", "zu", "sin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df sich nicht dein Herz verblute,", "tokens": ["Da\u00df", "sich", "nicht", "dein", "Herz", "ver\u00b7blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wisse deinem Trieb zu steuern:", "tokens": ["Wis\u00b7se", "dei\u00b7nem", "Trieb", "zu", "steu\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey wie Flaccus auf dem theuern,", "tokens": ["Sey", "wie", "Flac\u00b7cus", "auf", "dem", "theu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NE", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einzigen Sabinergute!", "tokens": ["Ein\u00b7zi\u00b7gen", "Sa\u00b7bi\u00b7ner\u00b7gu\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Bist du nicht gewohnt vor Allen,", "tokens": ["Bist", "du", "nicht", "ge\u00b7wohnt", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der Einsamkeit Geweihter,", "tokens": ["Als", "der", "Ein\u00b7sam\u00b7keit", "Ge\u00b7weih\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Fu\u00dfpfad und Begleiter", "tokens": ["Oh\u00b7ne", "Fu\u00df\u00b7pfad", "und", "Be\u00b7glei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den stillen Forst zu wallen?", "tokens": ["Durch", "den", "stil\u00b7len", "Forst", "zu", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir gen\u00fcge, wenn die F\u00f6hren,", "tokens": ["Dir", "ge\u00b7n\u00fc\u00b7ge", ",", "wenn", "die", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Schu\u00df der Wolken suchen,", "tokens": ["Die", "den", "Schu\u00df", "der", "Wol\u00b7ken", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die dickbelaubten Buchen,", "tokens": ["Wenn", "die", "dick\u00b7be\u00b7laub\u00b7ten", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine sanften Lieder h\u00f6ren!", "tokens": ["Dei\u00b7ne", "sanf\u00b7ten", "Lie\u00b7der", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wiesenblumen pfl\u00fcck' und schweige,", "tokens": ["Wie\u00b7sen\u00b7blu\u00b7men", "pfl\u00fcck", "und", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfl\u00fcck' und blicke nicht nach oben,", "tokens": ["Pfl\u00fcck", "und", "bli\u00b7cke", "nicht", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn f\u00fcr dich sind nicht gewoben", "tokens": ["Denn", "f\u00fcr", "dich", "sind", "nicht", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene dunkeln Lorbeerzweige!", "tokens": ["Je\u00b7ne", "dun\u00b7keln", "Lor\u00b7beer\u00b7zwei\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}