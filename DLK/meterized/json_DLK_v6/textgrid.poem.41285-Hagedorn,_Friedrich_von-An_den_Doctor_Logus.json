{"textgrid.poem.41285": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Doctor Logus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leicht besch\u00e4mst du den Macrin!", "tokens": ["Wie", "leicht", "be\u00b7sch\u00e4mst", "du", "den", "Ma\u00b7crin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schwach sind seine Kleinigkeiten,", "tokens": ["Wie", "schwach", "sind", "sei\u00b7ne", "Klei\u00b7nig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann deine Waffen sie bestreiten,", "tokens": ["Wann", "dei\u00b7ne", "Waf\u00b7fen", "sie", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit Soriten \u00fcberziehn.", "tokens": ["Und", "mit", "So\u00b7ri\u00b7ten", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein zu oft besiegst du ihn.", "tokens": ["Al\u00b7lein", "zu", "oft", "be\u00b7siegst", "du", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man mu\u00df, und dieses nur wei\u00df Doctor Logus nicht,", "tokens": ["Man", "mu\u00df", ",", "und", "die\u00b7ses", "nur", "wei\u00df", "Doc\u00b7tor", "Lo\u00b7gus", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "KON", "PDS", "ADV", "VVFIN", "NN", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht immer kl\u00fcger sein, als der, womit man spricht.", "tokens": ["Nicht", "im\u00b7mer", "kl\u00fc\u00b7ger", "sein", ",", "als", "der", ",", "wo\u00b7mit", "man", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAINF", "$,", "KOUS", "ART", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}