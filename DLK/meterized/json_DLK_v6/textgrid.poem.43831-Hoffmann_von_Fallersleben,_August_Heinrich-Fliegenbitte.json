{"textgrid.poem.43831": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fliegenbitte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6nnt doch dem kleinen Wintergast", "tokens": ["G\u00f6nnt", "doch", "dem", "klei\u00b7nen", "Win\u00b7ter\u00b7gast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im warmen Zimmer Ruh und Rast.", "tokens": ["Im", "war\u00b7men", "Zim\u00b7mer", "Ruh", "und", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da drau\u00dfen ist gar schlimme Zeit,", "tokens": ["Da", "drau\u00b7\u00dfen", "ist", "gar", "schlim\u00b7me", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es st\u00fcrmt und regnet, friert und schneit.", "tokens": ["Es", "st\u00fcrmt", "und", "reg\u00b7net", ",", "friert", "und", "schneit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, mein Begehren ist nur klein,", "tokens": ["Ach", ",", "mein", "Be\u00b7geh\u00b7ren", "ist", "nur", "klein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich nehme wenig Raum nur ein!", "tokens": ["Ich", "neh\u00b7me", "we\u00b7nig", "Raum", "nur", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Blumenbusch am Fenster hier,", "tokens": ["Im", "Blu\u00b7men\u00b7busch", "am", "Fens\u00b7ter", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da such' ich mir ein Nachtquartier.", "tokens": ["Da", "such'", "ich", "mir", "ein", "Nacht\u00b7quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wird es mir darin zu kalt,", "tokens": ["Und", "wird", "es", "mir", "da\u00b7rin", "zu", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "PAV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist mein liebster Aufenthalt", "tokens": ["So", "ist", "mein", "liebs\u00b7ter", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beim alten Fritzen auf dem Hut,", "tokens": ["Beim", "al\u00b7ten", "Frit\u00b7zen", "auf", "dem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sitz' ich sicher, warm und gut.", "tokens": ["Da", "sitz'", "ich", "si\u00b7cher", ",", "warm", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und kommt der heil'ge Christ heran,", "tokens": ["Und", "kommt", "der", "heil'\u00b7ge", "Christ", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann freu' ich mich wie Jedermann,", "tokens": ["Dann", "freu'", "ich", "mich", "wie", "Je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weihnachten soll's f\u00fcr mich auch sein,", "tokens": ["Weih\u00b7nach\u00b7ten", "soll's", "f\u00fcr", "mich", "auch", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kuchenkr\u00fcmchen wird schon mein.", "tokens": ["Ein", "Ku\u00b7chen\u00b7kr\u00fcm\u00b7chen", "wird", "schon", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df die arme Flieg' in Ruh,", "tokens": ["Drum", "la\u00df", "die", "ar\u00b7me", "Flieg'", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat ein Recht zu sein wie du.", "tokens": ["Sie", "hat", "ein", "Recht", "zu", "sein", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VAINF", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun, liebes Kind, nun freue dich", "tokens": ["Nun", ",", "lie\u00b7bes", "Kind", ",", "nun", "freu\u00b7e", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sei noch lustiger als ich!", "tokens": ["Und", "sei", "noch", "lus\u00b7ti\u00b7ger", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}