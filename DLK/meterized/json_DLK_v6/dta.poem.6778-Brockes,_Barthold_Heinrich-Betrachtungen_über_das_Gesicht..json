{"dta.poem.6778": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es nicht ausgemacht, da\u00df, wenn der Menschen Flei\u00df", "tokens": ["Ist", "es", "nicht", "aus\u00b7ge\u00b7macht", ",", "da\u00df", ",", "wenn", "der", "Men\u00b7schen", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den G\u00f6ttern Tempel baut, und den Monarchen wei\u00df", "tokens": ["Den", "G\u00f6t\u00b7tern", "Tem\u00b7pel", "baut", ",", "und", "den", "Mon\u00b7ar\u00b7chen", "wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Pall\u00e4ste zu erh\u00f6hn; im Blick nicht, blo\u00df im Geist", "tokens": ["Pal\u00b7l\u00e4s\u00b7te", "zu", "er\u00b7h\u00f6hn", ";", "im", "Blick", "nicht", ",", "blo\u00df", "im", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "APPRART", "NN", "PTKNEG", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Gleichma\u00df, der Entwurff, die Symmetrie sich weisst?", "tokens": ["Die", "Gleich\u00b7ma\u00df", ",", "der", "Ent\u00b7wurff", ",", "die", "Sym\u00b7me\u00b7trie", "sich", "weisst", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Himmlischen Geschenck, das auserlesne Licht,", "tokens": ["Die", "Himm\u00b7li\u00b7schen", "Ge\u00b7schenck", ",", "das", "au\u00b7ser\u00b7les\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wornach das Sch\u00f6ne sich zusammt dem Guten richt,", "tokens": ["Wor\u00b7nach", "das", "Sch\u00f6\u00b7ne", "sich", "zu\u00b7sammt", "dem", "Gu\u00b7ten", "richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die niemand lernen kan,", "tokens": ["Die", "nie\u00b7mand", "ler\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Durch welche Phidias die Wunder ansge\u00fcbet,", "tokens": ["Durch", "wel\u00b7che", "Phi\u00b7di\u00b7as", "die", "Wun\u00b7der", "ans\u00b7ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Die er zum ewgen Ruhm ersann,", "tokens": ["Die", "er", "zum", "ew\u00b7gen", "Ruhm", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die heitre Ur-Jdee, ein ewigs Muster-Bild,", "tokens": ["Die", "heit\u00b7re", "Ur\u00b7J\u00b7dee", ",", "ein", "e\u00b7wigs", "Mus\u00b7ter\u00b7Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "So wircket, da\u00df man gleich was sch\u00f6n ist, sch\u00e4tzt und liebet,", "tokens": ["So", "wir\u00b7cket", ",", "da\u00df", "man", "gleich", "was", "sch\u00f6n", "ist", ",", "sch\u00e4tzt", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PIS", "ADV", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die blo\u00df nur des Gem\u00fcthes-Gaben,", "tokens": ["Die", "blo\u00df", "nur", "des", "Ge\u00b7m\u00fc\u00b7thes\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So wir in der Gebuhrt empfangen haben;", "tokens": ["So", "wir", "in", "der", "Ge\u00b7buhrt", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Worinn ein edler Geist sich nie kan satt studiren,", "tokens": ["Wo\u00b7rinn", "ein", "ed\u00b7ler", "Geist", "sich", "nie", "kan", "satt", "stu\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und welchen er, jemehr ihn Licht und Sch\u00f6nheit r\u00fchren,", "tokens": ["Und", "wel\u00b7chen", "er", ",", "je\u00b7mehr", "ihn", "Licht", "und", "Sch\u00f6n\u00b7heit", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Um desto eifriger sucht nachzusp\u00fcren.", "tokens": ["Um", "des\u00b7to", "eif\u00b7ri\u00b7ger", "sucht", "nach\u00b7zu\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}