{"textgrid.poem.52809": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An einen Baumeister", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kaufmann sammelt Sch\u00e4tz' in der Levante,", "tokens": ["Der", "Kauf\u00b7mann", "sam\u00b7melt", "Sch\u00e4tz'", "in", "der", "Le\u00b7van\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sammelt Sch\u00e4tze in der Indier Lande,", "tokens": ["Er", "sam\u00b7melt", "Sch\u00e4t\u00b7ze", "in", "der", "In\u00b7dier", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sammelt Sch\u00e4tze in Australia,", "tokens": ["Er", "sam\u00b7melt", "Sch\u00e4t\u00b7ze", "in", "Aust\u00b7ra\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Afrika und in Amerika.", "tokens": ["In", "Af\u00b7ri\u00b7ka", "und", "in", "A\u00b7me\u00b7ri\u00b7ka", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es bl\u00fchen Veilchen rings um Attika,", "tokens": ["Es", "bl\u00fc\u00b7hen", "Veil\u00b7chen", "rings", "um", "At\u00b7ti\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es prangen Rosen in Arkadia,", "tokens": ["Es", "pran\u00b7gen", "Ro\u00b7sen", "in", "Ar\u00b7ka\u00b7dia", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es gl\u00e4nzen Lilien an Neapels Strand,", "tokens": ["Es", "gl\u00e4n\u00b7zen", "Li\u00b7li\u00b7en", "an", "Nea\u00b7pels", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6nsten Blumen aus der Flora Hand;", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "aus", "der", "Flo\u00b7ra", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Himmelszelt", "tokens": ["Am", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der K\u00fcnstlerwelt", "tokens": ["Der", "K\u00fcnst\u00b7ler\u00b7welt"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ergl\u00e4nzen in der H\u00f6h' und Ferne", "tokens": ["Er\u00b7gl\u00e4n\u00b7zen", "in", "der", "H\u00f6h'", "und", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als die erhabensten und ersten Sterne", "tokens": ["Als", "die", "er\u00b7ha\u00b7bens\u00b7ten", "und", "ers\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der alte D\u00e4dalos", "tokens": ["Der", "al\u00b7te", "D\u00e4\u00b7da\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der k\u00fchne Ikaros,", "tokens": ["Der", "k\u00fch\u00b7ne", "I\u00b7ka\u00b7ros", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der g\u00f6ttergleiche Phidias,", "tokens": ["Der", "g\u00f6t\u00b7ter\u00b7glei\u00b7che", "Phi\u00b7di\u00b7as", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Athenens Stolz und Attikas,", "tokens": ["A\u00b7the\u00b7nens", "Stolz", "und", "At\u00b7ti\u00b7kas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Erwin und Angelo.", "tokens": ["Er\u00b7win", "und", "An\u00b7ge\u00b7lo", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhochm\u00fcller\u00ab, Du sollst ebenso,", "tokens": ["\u00bb", "hoch\u00b7m\u00fcl\u00b7ler", "\u00ab", ",", "Du", "sollst", "e\u00b7ben\u00b7so", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$(", "$,", "PPER", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit \u00bbSch\u00f6n\u00ab dem gro\u00dfen Meister,", "tokens": ["Mit", "\u00bb", "Sch\u00f6n", "\u00ab", "dem", "gro\u00b7\u00dfen", "Meis\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NE", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Kreis der K\u00fcnstler und der ersten Geister,", "tokens": ["Im", "Kreis", "der", "K\u00fcnst\u00b7ler", "und", "der", "ers\u00b7ten", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Hochgestirn am Himmel stehen", "tokens": ["Ein", "Hoch\u00b7ges\u00b7tirn", "am", "Him\u00b7mel", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und um den Thron des Ew'gen gehen!", "tokens": ["Und", "um", "den", "Thron", "des", "Ew'\u00b7gen", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ambrosia enth\u00e4lt die Silberschale", "tokens": ["A\u00b7mbro\u00b7sia", "ent\u00b7h\u00e4lt", "die", "Sil\u00b7ber\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bei uns'rer Festlichkeit,", "tokens": ["Bei", "un\u00b7s'\u00b7rer", "Fest\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Nektar f\u00fcllt die Glaspokale \u2013", "tokens": ["Und", "Nek\u00b7tar", "f\u00fcllt", "die", "Glas\u00b7po\u00b7ka\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hochm\u00fcller lebe heut'!", "tokens": ["Hoch\u00b7m\u00fcl\u00b7ler", "le\u00b7be", "heut'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir leeren uns're Teller,", "tokens": ["Wir", "lee\u00b7ren", "un\u00b7s'\u00b7re", "Tel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wir leeren den Pokal,", "tokens": ["Wir", "lee\u00b7ren", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Haupt wird immer heller,", "tokens": ["Das", "Haupt", "wird", "im\u00b7mer", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Zimmer wird ein Himmelssaal.", "tokens": ["Das", "Zim\u00b7mer", "wird", "ein", "Him\u00b7mels\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Musen fangen an zu singen,", "tokens": ["Die", "Mu\u00b7sen", "fan\u00b7gen", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre Stimmen, die da neunmal klingen,", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7men", ",", "die", "da", "neun\u00b7mal", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhaben wie ein Wasserfall,", "tokens": ["Er\u00b7ha\u00b7ben", "wie", "ein", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Helikon, Parna\u00df, Olymp und Himmel nieder,", "tokens": ["Vom", "He\u00b7li\u00b7kon", ",", "Par\u00b7na\u00df", ",", "O\u00b7lymp", "und", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie hallen in uns Menschen wieder.", "tokens": ["Sie", "hal\u00b7len", "in", "uns", "Men\u00b7schen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Himmel und die G\u00f6tter schau'n erfreut", "tokens": ["Der", "Him\u00b7mel", "und", "die", "G\u00f6t\u00b7ter", "schau'n", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit heiterm Angesichte", "tokens": ["Mit", "hei\u00b7term", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und blendend wei\u00dfem Lichte", "tokens": ["Und", "blen\u00b7dend", "wei\u00b7\u00dfem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf uns're Lust und fromme Seligkeit.", "tokens": ["Auf", "un\u00b7s'\u00b7re", "Lust", "und", "from\u00b7me", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Doch der, der Alles hat gebaut,", "tokens": ["Doch", "der", ",", "der", "Al\u00b7les", "hat", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dem Du dieses Haus vertraut,", "tokens": ["Und", "dem", "Du", "die\u00b7ses", "Haus", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An den wir heut' zu Tage glauben,", "tokens": ["An", "den", "wir", "heut'", "zu", "Ta\u00b7ge", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und den uns Niemand mehr wird rauben,", "tokens": ["Und", "den", "uns", "Nie\u00b7mand", "mehr", "wird", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PIS", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er, der der gr\u00f6\u00dfte Bauherr ist,", "tokens": ["Er", ",", "der", "der", "gr\u00f6\u00df\u00b7te", "Bau\u00b7herr", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sch\u00fctze Dich, K\u00fcnstler, Freund und Christ!", "tokens": ["Er", "sch\u00fct\u00b7ze", "Dich", ",", "K\u00fcnst\u00b7ler", ",", "Freund", "und", "Christ", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "D'rum soll auch Wilhelm, unser Gastwirth, leben", "tokens": ["D'\u00b7rum", "soll", "auch", "Wil\u00b7helm", ",", "un\u00b7ser", "Gast\u00b7wirth", ",", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VMFIN", "ADV", "NE", "$,", "PPOSAT", "NN", "$,", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und seine Schwester auch daneben,", "tokens": ["Und", "sei\u00b7ne", "Schwes\u00b7ter", "auch", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie der Olymp und Himmel hoch!", "tokens": ["Wie", "der", "O\u00b7lymp", "und", "Him\u00b7mel", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie leben tausend Jahre noch!", "tokens": ["Sie", "le\u00b7ben", "tau\u00b7send", "Jah\u00b7re", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}