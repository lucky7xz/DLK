{"textgrid.poem.34953": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorhang f\u00e4llt, das St\u00fcck ist aus,", "tokens": ["Der", "Vor\u00b7hang", "f\u00e4llt", ",", "das", "St\u00fcck", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Herrn und Damen gehn nach Haus.", "tokens": ["Und", "Herrn", "und", "Da\u00b7men", "gehn", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ihnen auch das St\u00fcck gefallen?", "tokens": ["Ob", "ih\u00b7nen", "auch", "das", "St\u00fcck", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich glaub, ich h\u00f6rte Beifall schallen.", "tokens": ["Ich", "glaub", ",", "ich", "h\u00f6r\u00b7te", "Bei\u00b7fall", "schal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein hochverehrtes Publikum", "tokens": ["Ein", "hoch\u00b7ver\u00b7ehr\u00b7tes", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beklatschte dankbar seinen Dichter.", "tokens": ["Be\u00b7klatschte", "dank\u00b7bar", "sei\u00b7nen", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Jetzt aber ist das Haus so stumm,", "tokens": ["Jetzt", "a\u00b7ber", "ist", "das", "Haus", "so", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sind verschwunden Lust und Lichter.", "tokens": ["Und", "sind", "ver\u00b7schwun\u00b7den", "Lust", "und", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch horch! ein schollernd schn\u00f6der Klang", "tokens": ["Doch", "horch", "!", "ein", "schol\u00b7lernd", "schn\u00f6\u00b7der", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ert\u00f6nt unfern der \u00f6den B\u00fchne; \u2013", "tokens": ["Er\u00b7t\u00f6nt", "un\u00b7fern", "der", "\u00f6\u00b7den", "B\u00fch\u00b7ne", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vielleicht, da\u00df eine Saite sprang", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "ei\u00b7ne", "Sai\u00b7te", "sprang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "An einer alten Violine.", "tokens": ["An", "ei\u00b7ner", "al\u00b7ten", "Vi\u00b7o\u00b7li\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Verdrie\u00dflich rascheln im Parterr'", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "ra\u00b7scheln", "im", "Par\u00b7terr'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Etwelche Ratten hin und her,", "tokens": ["Et\u00b7wel\u00b7che", "Rat\u00b7ten", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und alles riecht nach ranz'gem \u00d6le.", "tokens": ["Und", "al\u00b7les", "riecht", "nach", "ranz'\u00b7gem", "\u00d6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die letzte Lampe \u00e4chzt und zischt", "tokens": ["Die", "letz\u00b7te", "Lam\u00b7pe", "\u00e4chzt", "und", "zischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Verzweiflungsvoll, und sie erlischt.", "tokens": ["Ver\u00b7zwei\u00b7flungs\u00b7voll", ",", "und", "sie", "er\u00b7lischt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das arme Licht war meine Seele.", "tokens": ["Das", "ar\u00b7me", "Licht", "war", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}