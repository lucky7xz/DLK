{"textgrid.poem.30148": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle K\u00fcnste sind zu viel; eine Kunst recht fassen k\u00fcnnen,", "tokens": ["Al\u00b7le", "K\u00fcns\u00b7te", "sind", "zu", "viel", ";", "ei\u00b7ne", "Kunst", "recht", "fas\u00b7sen", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKA", "PIS", "$.", "ART", "NN", "ADJD", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ist genug zu rechtem Ruhm, ist genug f\u00fcr Menschen-Sinnen.", "tokens": ["Ist", "ge\u00b7nug", "zu", "rech\u00b7tem", "Ruhm", ",", "ist", "ge\u00b7nug", "f\u00fcr", "Men\u00b7schen\u00b7Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$,", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Parcipromus machets so; pflegt zum geben sich zu sch\u00e4men,", "tokens": ["Par\u00b7ci\u00b7pro\u00b7mus", "ma\u00b7chets", "so", ";", "pflegt", "zum", "ge\u00b7ben", "sich", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "VVFIN", "APPRART", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Weil er solches nie gelernt, ist nur blo\u00df gelehrt zum nemen.", "tokens": ["Weil", "er", "sol\u00b7ches", "nie", "ge\u00b7lernt", ",", "ist", "nur", "blo\u00df", "ge\u00b7lehrt", "zum", "ne\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "VVPP", "$,", "VAFIN", "ADV", "ADV", "VVPP", "APPRART", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}