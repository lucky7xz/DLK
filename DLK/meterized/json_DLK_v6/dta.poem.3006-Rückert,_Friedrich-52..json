{"dta.poem.3006": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer etwas wei\u00df, der ist darum kein Weiser noch,", "tokens": ["Wer", "et\u00b7was", "wei\u00df", ",", "der", "ist", "da\u00b7rum", "kein", "Wei\u00b7ser", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PRELS", "VAFIN", "PAV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wisser ist er nur; was ist ein Weiser doch?", "tokens": ["Ein", "Wis\u00b7ser", "ist", "er", "nur", ";", "was", "ist", "ein", "Wei\u00b7ser", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "PWS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der ist ein Weiser, wem sich Wesenheit gewiesen", "tokens": ["Der", "ist", "ein", "Wei\u00b7ser", ",", "wem", "sich", "We\u00b7sen\u00b7heit", "ge\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWS", "PRF", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In allen Weisen, voll Gewisheit, unbewiesen.", "tokens": ["In", "al\u00b7len", "Wei\u00b7sen", ",", "voll", "Ge\u00b7wis\u00b7heit", ",", "un\u00b7be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der ist ein Weiser, wer der Weisheit hohen Geist", "tokens": ["Der", "ist", "ein", "Wei\u00b7ser", ",", "wer", "der", "Weis\u00b7heit", "ho\u00b7hen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An seinem Wesen selbst in eigner Weise weis't.", "tokens": ["An", "sei\u00b7nem", "We\u00b7sen", "selbst", "in", "eig\u00b7ner", "Wei\u00b7se", "weis'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}