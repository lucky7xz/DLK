{"dta.poem.1935": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht wer viel hat/ ist begl\u00fcckt/", "tokens": ["Nicht", "wer", "viel", "hat", "/", "ist", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "ADV", "VAFIN", "$(", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern wer es recht geneust/", "tokens": ["Son\u00b7dern", "wer", "es", "recht", "ge\u00b7neust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer sich auch in Armutt schickt", "tokens": ["Wer", "sich", "auch", "in", "Ar\u00b7mutt", "schickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von Lastern sich entreist/", "tokens": ["Und", "von", "Las\u00b7tern", "sich", "en\u00b7treist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer f\u00fcr Freund und Vaterland", "tokens": ["Wer", "f\u00fcr", "Freund", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig braucht Haubt/ Haut und Hand.", "tokens": ["Freu\u00b7dig", "braucht", "Haubt", "/", "Haut", "und", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}