{"textgrid.poem.25050": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir in das Auge nur zu blicken,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir in das Auge nur zu blicken,", "tokens": ["Dir", "in", "das", "Au\u00b7ge", "nur", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Adele, hatt' ich lang gezagt;", "tokens": ["A\u00b7de\u00b7le", ",", "hatt'", "ich", "lang", "ge\u00b7zagt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf deine Hand die Lippe dr\u00fccken,", "tokens": ["Auf", "dei\u00b7ne", "Hand", "die", "Lip\u00b7pe", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das K\u00fchnste war's, was ich gewagt.", "tokens": ["Das", "K\u00fchns\u00b7te", "wa\u00b7r's", ",", "was", "ich", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da go\u00df die gottgesandte Stunde", "tokens": ["Da", "go\u00df", "die", "gott\u00b7ge\u00b7sand\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmel her ins Herz mir Mut,", "tokens": ["Vom", "Him\u00b7mel", "her", "ins", "Herz", "mir", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df hei\u00df mein Mund auf deinem Munde", "tokens": ["Da\u00df", "hei\u00df", "mein", "Mund", "auf", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im ersten heil'gen Ku\u00df geruht.", "tokens": ["Im", "ers\u00b7ten", "heil'\u00b7gen", "Ku\u00df", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gebrochen war das Reich des Truges,", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "das", "Reich", "des", "Tru\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Seele in die Seele sank", "tokens": ["Wie", "See\u00b7le", "in", "die", "See\u00b7le", "sank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und langen, vollen Atemzuges", "tokens": ["Und", "lan\u00b7gen", ",", "vol\u00b7len", "A\u00b7tem\u00b7zu\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Strom des ew'gen Lebens trank.", "tokens": ["Vom", "Strom", "des", "ew'\u00b7gen", "Le\u00b7bens", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als die Blicke wir erhoben,", "tokens": ["Und", "als", "die", "Bli\u00b7cke", "wir", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O! strahlend, wie wir nie sie sahn,", "tokens": ["O", "!", "strah\u00b7lend", ",", "wie", "wir", "nie", "sie", "sahn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "$,", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zog da durchs tiefe Nachtblau droben", "tokens": ["Zog", "da", "durchs", "tie\u00b7fe", "Nacht\u00b7blau", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welt neben Welt die lichte Bahn.", "tokens": ["Welt", "ne\u00b7ben", "Welt", "die", "lich\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}