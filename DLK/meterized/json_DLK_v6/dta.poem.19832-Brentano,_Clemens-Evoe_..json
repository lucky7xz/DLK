{"dta.poem.19832": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Evoe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freut euch ihr lieben Knaben!               ", "tokens": ["Freut", "euch", "ihr", "lie\u00b7ben", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herbst erzeigt sich wohl,", "tokens": ["Der", "Herbst", "er\u00b7zeigt", "sich", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lang getrauert haben,", "tokens": ["Die", "lang", "ge\u00b7trau\u00b7ert", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut wollen wir werden voll.", "tokens": ["Heut", "wol\u00b7len", "wir", "wer\u00b7den", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir haben vormals den sauren Wein", "tokens": ["Wir", "ha\u00b7ben", "vor\u00b7mals", "den", "sau\u00b7ren", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gar theuer genommen an,", "tokens": ["Gar", "theu\u00b7er", "ge\u00b7nom\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das wollen wir heute bringen ein,", "tokens": ["Das", "wol\u00b7len", "wir", "heu\u00b7te", "brin\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der s\u00fc\u00dfe Most, der neue Wein,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Most", ",", "der", "neu\u00b7e", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wird uns gar gern eingahn.", "tokens": ["Wird", "uns", "gar", "gern", "ein\u00b7gahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was wir vers\u00e4umet haben,", "tokens": ["Was", "wir", "ver\u00b7s\u00e4u\u00b7met", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das machen wir nun gleich,", "tokens": ["Das", "ma\u00b7chen", "wir", "nun", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Wein wollen wir uns laben", "tokens": ["Mit", "Wein", "wol\u00b7len", "wir", "uns", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier und in Oesterreich,", "tokens": ["Hier", "und", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "In einer neuen Krausen", "tokens": ["In", "ei\u00b7ner", "neu\u00b7en", "Krau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wollen wir ihn nehmen an,", "tokens": ["Wol\u00b7len", "wir", "ihn", "neh\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob ihm soll uns nit grausen,", "tokens": ["Ob", "ihm", "soll", "uns", "nit", "grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis uns der Kopf thut sausen,", "tokens": ["Bis", "uns", "der", "Kopf", "thut", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nit eh gehn wir davon.", "tokens": ["Nit", "eh", "gehn", "wir", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}