{"dta.poem.11579": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nachts am Schreibepult.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Primeln, Sterne, Syringen, von stiller Kerze beleuchtet,", "tokens": ["Pri\u00b7meln", ",", "Ster\u00b7ne", ",", "Sy\u00b7rin\u00b7gen", ",", "von", "stil\u00b7ler", "Ker\u00b7ze", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hier im Glase: wie fremd blickt ihr, wie feenhaft, her!", "tokens": ["Hier", "im", "Gla\u00b7se", ":", "wie", "fremd", "blickt", "ihr", ",", "wie", "fe\u00b7en\u00b7haft", ",", "her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Sonne schien, als die Liebste euch trug; da wart ihr so", "tokens": ["Son\u00b7ne", "schien", ",", "als", "die", "Liebs\u00b7te", "euch", "trug", ";", "da", "wart", "ihr", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Mitternacht summt nun um euch, ach! und kein Lieb-", "tokens": ["Mit\u00b7ter\u00b7nacht", "summt", "nun", "um", "euch", ",", "ach", "!", "und", "kein", "Lie\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPER", "$,", "ITJ", "$.", "KON", "PIAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}