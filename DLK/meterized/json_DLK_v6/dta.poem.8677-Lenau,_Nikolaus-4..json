{"dta.poem.8677": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Ich trat in einen heilig d\u00fcstern", "tokens": ["Ich", "trat", "in", "ei\u00b7nen", "hei\u00b7lig", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eichwald, da h\u00f6rt' ich leis' und lind", "tokens": ["Eich\u00b7wald", ",", "da", "h\u00f6rt'", "ich", "leis'", "und", "lind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein B\u00e4chlein unter Blumen fl\u00fcstern,", "tokens": ["Ein", "B\u00e4ch\u00b7lein", "un\u00b7ter", "Blu\u00b7men", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie das Gebet von einem Kind;", "tokens": ["Wie", "das", "Ge\u00b7bet", "von", "ei\u00b7nem", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Und mich ergriff ein s\u00fc\u00dfes Grauen,", "tokens": ["Und", "mich", "er\u00b7griff", "ein", "s\u00fc\u00b7\u00dfes", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es rauscht' der Wald geheimni\u00dfvoll,", "tokens": ["Es", "rauscht'", "der", "Wald", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als m\u00f6cht' er mir was anvertrauen,", "tokens": ["Als", "m\u00f6cht'", "er", "mir", "was", "an\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das noch mein Herz nicht wissen soll;", "tokens": ["Das", "noch", "mein", "Herz", "nicht", "wis\u00b7sen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als m\u00f6cht' er heimlich mir entdecken,", "tokens": ["Als", "m\u00f6cht'", "er", "heim\u00b7lich", "mir", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was Gottes Liebe sinnt, und will;", "tokens": ["Was", "Got\u00b7tes", "Lie\u00b7be", "sinnt", ",", "und", "will", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$,", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schien er pl\u00f6tzlich zu erschrecken", "tokens": ["Doch", "schien", "er", "pl\u00f6tz\u00b7lich", "zu", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Gottes N\u00e4h' \u2014 und wurde still.", "tokens": ["Vor", "Got\u00b7tes", "N\u00e4h'", "und", "wur\u00b7de", "still", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}