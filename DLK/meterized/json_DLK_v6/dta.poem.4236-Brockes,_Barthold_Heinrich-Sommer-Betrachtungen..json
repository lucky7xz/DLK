OUS", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von allem Guten anzusehn,", "tokens": ["Von", "al\u00b7lem", "Gu\u00b7ten", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Jhn in unsrer Lust erh\u00f6hn:", "tokens": ["Und", "Jhn", "in", "uns\u00b7rer", "Lust", "er\u00b7h\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O GOtt, durch Dessen weisen Willen", "tokens": ["O", "Gott", ",", "durch", "Des\u00b7sen", "wei\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Felder sich mit Segen f\u00fcllen,", "tokens": ["Die", "Fel\u00b7der", "sich", "mit", "Se\u00b7gen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indem die Fr\u00fcchte, nach dem Schein,", "tokens": ["In\u00b7dem", "die", "Fr\u00fcch\u00b7te", ",", "nach", "dem", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Gold nur, wirklich g\u00fclden seyn!", "tokens": ["Nicht", "Gold", "nur", ",", "wirk\u00b7lich", "g\u00fcl\u00b7den", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "$,", "ADJD", "ADJD", "V