{"dta.poem.4236": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommer-Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann erst der Furchen Meng' in manchem Striche", "tokens": ["Wann", "erst", "der", "Fur\u00b7chen", "Meng'", "in", "man\u00b7chem", "Stri\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NE", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabnen kleinen Wellen gliche,", "tokens": ["Er\u00b7hab\u00b7nen", "klei\u00b7nen", "Wel\u00b7len", "gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nachher der Halmen reges Heer", "tokens": ["Nach\u00b7her", "der", "Hal\u00b7men", "re\u00b7ges", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein lieblich-wallend Segens-Meer;", "tokens": ["Ein", "lieb\u00b7lich\u00b7wal\u00b7lend", "Se\u00b7gens\u00b7Meer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So scheinet itzt, zu unsrer Freude,", "tokens": ["So", "schei\u00b7net", "itzt", ",", "zu", "uns\u00b7rer", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das schon gem\u00e4hete Getrayde,", "tokens": ["Das", "schon", "ge\u00b7m\u00e4\u00b7he\u00b7te", "Ge\u00b7tray\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da volle Garben sich erh\u00f6hn,", "tokens": ["Da", "vol\u00b7le", "Gar\u00b7ben", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und allbereit in Hocken stehn,", "tokens": ["Und", "all\u00b7be\u00b7reit", "in", "Ho\u00b7cken", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erhabne, doch auch stille, Wellen", "tokens": ["Er\u00b7hab\u00b7ne", ",", "doch", "auch", "stil\u00b7le", ",", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "ADV", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Augen lieblich vorzustellen.", "tokens": ["Den", "Au\u00b7gen", "lieb\u00b7lich", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort kann man in fast nicht zu z\u00e4hl\u2019nden Schocken", "tokens": ["Dort", "kann", "man", "in", "fast", "nicht", "zu", "z\u00e4hl'n\u00b7den", "Scho\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ADV", "PTKNEG", "PTKZU", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gelben aufgeth\u00fcrmten Hocken,", "tokens": ["Der", "gel\u00b7ben", "auf\u00b7ge\u00b7th\u00fcrm\u00b7ten", "Ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gersten, Weizen und von Rocken,", "tokens": ["Von", "Gers\u00b7ten", ",", "Wei\u00b7zen", "und", "von", "Ro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die langen zierlichen Alleen", "tokens": ["Die", "lan\u00b7gen", "zier\u00b7li\u00b7chen", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von g\u00fcldnen Segens-Bergen sehen.", "tokens": ["Von", "g\u00fcld\u00b7nen", "Se\u00b7gens\u00b7Ber\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir nun zwischen ihren H\u00f6hen,", "tokens": ["Wenn", "wir", "nun", "zwi\u00b7schen", "ih\u00b7ren", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit frohem Fu\u00df, spatzieren gehen,", "tokens": ["Mit", "fro\u00b7hem", "Fu\u00df", ",", "spat\u00b7zie\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So la\u00dft uns, bey so reichem Segen,", "tokens": ["So", "la\u00dft", "uns", ",", "bey", "so", "rei\u00b7chem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Gebers Lieb\u2019 und Macht erwegen,", "tokens": ["Des", "Ge\u00b7bers", "Lieb'", "und", "Macht", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der tiefen Weisheit Meer ermessen,", "tokens": ["Der", "tie\u00b7fen", "Weis\u00b7heit", "Meer", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und ja des Dankens nicht vergessen!", "tokens": ["Und", "ja", "des", "Dan\u00b7kens", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von wem entsteht die Fruchtbarkeit?", "tokens": ["Von", "wem", "ent\u00b7steht", "die", "Frucht\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wem stammt Sonnenschein und Regen?", "tokens": ["Von", "wem", "stammt", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von wem, in Saat und Frucht, der Segen", "tokens": ["Von", "wem", ",", "in", "Saat", "und", "Frucht", ",", "der", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "$,", "APPR", "NN", "KON", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der n\u00e4hrenden Beschaffenheit?", "tokens": ["Der", "n\u00e4h\u00b7ren\u00b7den", "Be\u00b7schaf\u00b7fen\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Sch\u00f6pfer und Erhalter! wir", "tokens": ["O", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", "!", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "KON", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empfangen es nur blo\u00df von Dir.", "tokens": ["Emp\u00b7fan\u00b7gen", "es", "nur", "blo\u00df", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, Seele, brauche deine Kr\u00e4fte zu deines Sch\u00f6pfers", "tokens": ["Komm", ",", "See\u00b7le", ",", "brau\u00b7che", "dei\u00b7ne", "Kr\u00e4f\u00b7te", "zu", "dei\u00b7nes", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Preis und Ehre!", "tokens": ["Preis", "und", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Erwege, was dazu geh\u00f6re!", "tokens": ["Er\u00b7we\u00b7ge", ",", "was", "da\u00b7zu", "ge\u00b7h\u00f6\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit, zu unserer Erhaltung, Lust und Freude,", "tokens": ["Da\u00b7mit", ",", "zu", "un\u00b7se\u00b7rer", "Er\u00b7hal\u00b7tung", ",", "Lust", "und", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das uns ern\u00e4hrende, vergn\u00fcgende Getrayde", "tokens": ["Das", "uns", "er\u00b7n\u00e4h\u00b7ren\u00b7de", ",", "ver\u00b7gn\u00fc\u00b7gen\u00b7de", "Ge\u00b7tray\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPER", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+---+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Geraht\u2019 und brauchbar sey.", "tokens": ["Ge\u00b7raht'", "und", "brauch\u00b7bar", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie viel, wie viel, wie vielerley", "tokens": ["Wie", "viel", ",", "wie", "viel", ",", "wie", "vie\u00b7ler\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PWAV", "PIS", "$,", "PWAV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Witterung, an Segen und an Flei\u00df,", "tokens": ["An", "Wit\u00b7te\u00b7rung", ",", "an", "Se\u00b7gen", "und", "an", "Flei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So, da\u00df kein Menschen-Witz es zu ergr\u00fcbeln wei\u00df,", "tokens": ["So", ",", "da\u00df", "kein", "Men\u00b7schen\u00b7Witz", "es", "zu", "er\u00b7gr\u00fc\u00b7beln", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh\u00f6rt dazu! Ach, la\u00dft es uns betrachten,", "tokens": ["Ge\u00b7h\u00f6rt", "da\u00b7zu", "!", "Ach", ",", "la\u00dft", "es", "uns", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$.", "ITJ", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und in der Gabe Gr\u00f6\u00df\u2019 und Wehrt", "tokens": ["Und", "in", "der", "Ga\u00b7be", "Gr\u00f6\u00df'", "und", "Wehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gr\u00f6sse De\u00df, Der sie beschehrt,", "tokens": ["Die", "Gr\u00f6s\u00b7se", "De\u00df", ",", "Der", "sie", "be\u00b7schehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Dank und mit Bewundrung achten!", "tokens": ["Mit", "Dank", "und", "mit", "Be\u00b7wund\u00b7rung", "ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn dir zu deinem Unterhalt das allern\u00f6htigste gebr\u00e4che,", "tokens": ["Wenn", "dir", "zu", "dei\u00b7nem", "Un\u00b7ter\u00b7halt", "das", "al\u00b7ler\u00b7n\u00f6h\u00b7tigs\u00b7te", "ge\u00b7br\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und jemand dir sodann verspr\u00e4che,", "tokens": ["Und", "je\u00b7mand", "dir", "so\u00b7dann", "ver\u00b7spr\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wolle dich die ganze Lebens-Zeit", "tokens": ["Er", "wol\u00b7le", "dich", "die", "gan\u00b7ze", "Le\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Ueberflu\u00df, Bequemlichkeit", "tokens": ["Mit", "Ue\u00b7berf\u00b7lu\u00df", ",", "Be\u00b7quem\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mannigfacher Lust ern\u00e4hren;", "tokens": ["Und", "man\u00b7nig\u00b7fa\u00b7cher", "Lust", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd\u2019 einen solchen nicht dein Geist,", "tokens": ["W\u00fcrd'", "ei\u00b7nen", "sol\u00b7chen", "nicht", "dein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach allen Kr\u00e4ften, loben, ehren,", "tokens": ["Nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "lo\u00b7ben", ",", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihm den schuld\u2019gen Dank gew\u00e4hren?", "tokens": ["Und", "ihm", "den", "schuld'\u00b7gen", "Dank", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie kommt es denn, da\u00df Gott nicht wird gepreist,", "tokens": ["Wie", "kommt", "es", "denn", ",", "da\u00df", "Gott", "nicht", "wird", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der dich wahrhaftig in der That,", "tokens": ["Der", "dich", "wahr\u00b7haf\u00b7tig", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So wie Er dich bisher gespeiset hat,", "tokens": ["So", "wie", "Er", "dich", "bis\u00b7her", "ge\u00b7spei\u00b7set", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dich im Getrayde stetig speiset.", "tokens": ["Dich", "im", "Ge\u00b7tray\u00b7de", "ste\u00b7tig", "spei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sprich nicht: Will ich Getrayde haben,", "tokens": ["Sprich", "nicht", ":", "Will", "ich", "Ge\u00b7tray\u00b7de", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$.", "VMFIN", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mu\u00df ich mirs saur gnug, aus meinem Land\u2019 ergraben.", "tokens": ["Mu\u00df", "ich", "mirs", "saur", "gnug", ",", "aus", "mei\u00b7nem", "Land'", "er\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "ADJD", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Sprich gleichfalls nicht: Die Frucht vom Felde", "tokens": ["Sprich", "gleich\u00b7falls", "nicht", ":", "Die", "Frucht", "vom", "Fel\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PTKNEG", "$.", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bezahl\u2019 ich ja mit meinem Gelde,", "tokens": ["Be\u00b7zahl'", "ich", "ja", "mit", "mei\u00b7nem", "Gel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und so wird mir ja nichts geschenkt.", "tokens": ["Und", "so", "wird", "mir", "ja", "nichts", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.18": {"text": "Ach nein! ungl\u00fccklich irrt, wer also denkt.", "tokens": ["Ach", "nein", "!", "un\u00b7gl\u00fcck\u00b7lich", "irrt", ",", "wer", "al\u00b7so", "denkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "ADJD", "VVFIN", "$,", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du Gr\u00e4ber, sprich! das Land, das dich ern\u00e4hrt,", "tokens": ["Du", "Gr\u00e4\u00b7ber", ",", "sprich", "!", "das", "Land", ",", "das", "dich", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hast du dir solches selbst beschehrt?", "tokens": ["Hast", "du", "dir", "sol\u00b7ches", "selbst", "be\u00b7schehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du den Saft darinn gesenket?", "tokens": ["Hast", "du", "den", "Saft", "da\u00b7rinn", "ge\u00b7sen\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du es, welcher Sonnenschein,", "tokens": ["Bist", "du", "es", ",", "wel\u00b7cher", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu seiner Fruchtbarkeit, darauf gelenket,", "tokens": ["Zu", "sei\u00b7ner", "Frucht\u00b7bar\u00b7keit", ",", "da\u00b7rauf", "ge\u00b7len\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und welcher es mit Regen tr\u00e4nket?", "tokens": ["Und", "wel\u00b7cher", "es", "mit", "Re\u00b7gen", "tr\u00e4n\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hast du die reichliche Vermehrungs-Kraft", "tokens": ["Hast", "du", "die", "reich\u00b7li\u00b7che", "Ver\u00b7meh\u00b7rungs\u00b7Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und die fast lebend\u2019 Eigenschaft,", "tokens": ["Und", "die", "fast", "le\u00b7bend'", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Da\u00df es sich in die H\u00f6he treibet,", "tokens": ["Da\u00df", "es", "sich", "in", "die", "H\u00f6\u00b7he", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durch deine Kunst, dem Saamen einverleibet?", "tokens": ["Durch", "dei\u00b7ne", "Kunst", ",", "dem", "Saa\u00b7men", "ein\u00b7ver\u00b7lei\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hast du, zu dem so n\u00f6htigen Gesch\u00e4fte,", "tokens": ["Hast", "du", ",", "zu", "dem", "so", "n\u00f6h\u00b7ti\u00b7gen", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "