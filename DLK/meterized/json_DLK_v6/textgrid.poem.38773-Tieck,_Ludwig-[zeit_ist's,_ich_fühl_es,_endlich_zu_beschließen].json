{"textgrid.poem.38773": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[zeit ist's, ich f\u00fchl es, endlich zu beschlie\u00dfen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeit ist's, ich f\u00fchl es, endlich zu beschlie\u00dfen,", "tokens": ["Zeit", "ist's", ",", "ich", "f\u00fchl", "es", ",", "end\u00b7lich", "zu", "be\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn auch Maria will nicht mehr beschirmen,", "tokens": ["Denn", "auch", "Ma\u00b7ria", "will", "nicht", "mehr", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie giebt dich Preis den Wettern, die sich th\u00fcrmen,", "tokens": ["Sie", "giebt", "dich", "Preis", "den", "Wet\u00b7tern", ",", "die", "sich", "th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Stern soll mir in \u00f6den N\u00e4chten sprie\u00dfen.", "tokens": ["Kein", "Stern", "soll", "mir", "in", "\u00f6\u00b7den", "N\u00e4ch\u00b7ten", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Weh mir! da\u00df Morgenroth mich wollte gr\u00fc\u00dfen,", "tokens": ["Weh", "mir", "!", "da\u00df", "Mor\u00b7gen\u00b7roth", "mich", "woll\u00b7te", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KOUS", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein l\u00e4chelnd Blicken, herzlich, lieblich Schirmen!", "tokens": ["Ein", "l\u00e4\u00b7chelnd", "Bli\u00b7cken", ",", "herz\u00b7lich", ",", "lieb\u00b7lich", "Schir\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun, Herz, vergeh sogleich in schnellen St\u00fcrmen,", "tokens": ["Nun", ",", "Herz", ",", "ver\u00b7geh", "sog\u00b7leich", "in", "schnel\u00b7len", "St\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df nicht dein Leben tropfenweis vergie\u00dfen!", "tokens": ["La\u00df", "nicht", "dein", "Le\u00b7ben", "trop\u00b7fen\u00b7weis", "ver\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Nacht empf\u00e4ngt mich wieder, \u00f6des Schweigen,", "tokens": ["Die", "Nacht", "emp\u00b7f\u00e4ngt", "mich", "wie\u00b7der", ",", "\u00f6\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schwarz Gew\u00e4sser, Gram, Qual, Angst und Weinen:", "tokens": ["Ein", "schwarz", "Ge\u00b7w\u00e4s\u00b7ser", ",", "Gram", ",", "Qual", ",", "Angst", "und", "Wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Licht! o Blick! was mu\u00dftest du dich zeigen?", "tokens": ["O", "Licht", "!", "o", "Blick", "!", "was", "mu\u00df\u00b7test", "du", "dich", "zei\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$.", "PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir schadenfroh in meiner W\u00fcst' erscheinen,", "tokens": ["Mir", "scha\u00b7den\u00b7froh", "in", "mei\u00b7ner", "W\u00fcst'", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dieser Schmerz mir auch noch w\u00fcrde eigen?", "tokens": ["Da\u00df", "die\u00b7ser", "Schmerz", "mir", "auch", "noch", "w\u00fcr\u00b7de", "ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und keinen Blick und Trost, Maria? \u2013 Keinen!", "tokens": ["Und", "kei\u00b7nen", "Blick", "und", "Trost", ",", "Ma\u00b7ria", "?", "\u2013", "Kei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "$,", "NE", "$.", "$(", "PIAT", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}