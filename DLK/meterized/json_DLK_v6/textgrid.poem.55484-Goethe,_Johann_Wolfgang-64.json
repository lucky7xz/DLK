{"textgrid.poem.55484": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "64", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df zu Ulrichs Gartenr\u00e4umen", "tokens": ["Da\u00df", "zu", "Ul\u00b7richs", "Gar\u00b7ten\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ein Verslein mir ertr\u00e4umen,", "tokens": ["Soll", "ein", "Ver\u00b7slein", "mir", "er\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ist ein wunderbarer Streich;", "tokens": ["Ist", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Streich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es war von s\u00fc\u00dfen Tr\u00e4umen", "tokens": ["Denn", "es", "war", "von", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den l\u00e4ndlich engen R\u00e4umen", "tokens": ["In", "den", "l\u00e4nd\u00b7lich", "en\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir ein Fr\u00fchling hold und reich.", "tokens": ["Mir", "ein", "Fr\u00fch\u00b7ling", "hold", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt es euch zu Lust und Frommen", "tokens": ["Sollt", "es", "euch", "zu", "Lust", "und", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch einmal zugute kommen,", "tokens": ["Auch", "ein\u00b7mal", "zu\u00b7gu\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Freut euch in dem engsten Raum.", "tokens": ["Freut", "euch", "in", "dem", "engs\u00b7ten", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Was begl\u00fcckt, es ist kein Traum.", "tokens": ["Was", "be\u00b7gl\u00fcckt", ",", "es", "ist", "kein", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}