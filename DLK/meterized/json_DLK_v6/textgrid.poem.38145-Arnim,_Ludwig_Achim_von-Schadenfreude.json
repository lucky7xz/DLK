{"textgrid.poem.38145": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schadenfreude", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hie auf dieser Liebes Matt", "tokens": ["Hie", "auf", "die\u00b7ser", "Lie\u00b7bes", "Matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Cupido vor dreien Tagen,", "tokens": ["Cu\u00b7pi\u00b7do", "vor", "drei\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er nichts zu schaffen hat,", "tokens": ["Weil", "er", "nichts", "zu", "schaf\u00b7fen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt sein Zelt und Lager schlagen:", "tokens": ["Wollt", "sein", "Zelt", "und", "La\u00b7ger", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach Cupido kleiner Schelm,", "tokens": ["Ach", "Cu\u00b7pi\u00b7do", "klei\u00b7ner", "Schelm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie machst du so gro\u00dfe Wunden.", "tokens": ["Wie", "machst", "du", "so", "gro\u00b7\u00dfe", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Als er nun ins Gr\u00fcne kam,", "tokens": ["Als", "er", "nun", "ins", "Gr\u00fc\u00b7ne", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses hier dort das wolt sehen,", "tokens": ["Die\u00b7ses", "hier", "dort", "das", "wolt", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Venus bei der Hand ihn nahm,", "tokens": ["Ve\u00b7nus", "bei", "der", "Hand", "ihn", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch wolt er nicht mit ihr gehen, ach Cupidon. usw.", "tokens": ["Doch", "wolt", "er", "nicht", "mit", "ihr", "ge\u00b7hen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$,", "XY", "NE", "$.", "NE"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Lief bald vor das Bienen Haus,", "tokens": ["Lief", "bald", "vor", "das", "Bie\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolt ein wenig Honig lecken,", "tokens": ["Wolt", "ein", "we\u00b7nig", "Ho\u00b7nig", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine kroch zum Korb heraus", "tokens": ["Ei\u00b7ne", "kroch", "zum", "Korb", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und flog nach dem jungen Gecken, ach Cupidon. usw.", "tokens": ["Und", "flog", "nach", "dem", "jun\u00b7gen", "Ge\u00b7cken", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "XY", "NE", "$.", "NE"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Cupido bald her bald hin,", "tokens": ["Cu\u00b7pi\u00b7do", "bald", "her", "bald", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt sich gern vor ihr verkrochen,", "tokens": ["H\u00e4tt", "sich", "gern", "vor", "ihr", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Bien flog stets auf ihn,", "tokens": ["Doch", "die", "Bi\u00b7en", "flog", "stets", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er von ihr war gestochen, ach Cupidon. usw.", "tokens": ["Bis", "er", "von", "ihr", "war", "ge\u00b7sto\u00b7chen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VAFIN", "VVPP", "$,", "XY", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Als er seinen Finger schaut,", "tokens": ["Als", "er", "sei\u00b7nen", "Fin\u00b7ger", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er armsdick aufgeloffen,", "tokens": ["Wie", "er", "arms\u00b7dick", "auf\u00b7ge\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fing er an zu schreien laut:", "tokens": ["Fing", "er", "an", "zu", "schrei\u00b7en", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "PTKZU", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O weh Mutter, ich bin troffen! Ach Cupidon. usw.", "tokens": ["O", "weh", "Mut\u00b7ter", ",", "ich", "bin", "trof\u00b7fen", "!", "Ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["ITJ", "ADV", "NN", "$,", "PPER", "VAFIN", "VVINF", "$.", "NN", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "O Weh liebe Mutter bald,", "tokens": ["O", "Weh", "lie\u00b7be", "Mut\u00b7ter", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich mu\u00df an dem Stich verderben,", "tokens": ["Ich", "mu\u00df", "an", "dem", "Stich", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Weh, ich lauf in den Wald,", "tokens": ["O", "Weh", ",", "ich", "lauf", "in", "den", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lasse mich drinn Hungers sterben. Ach Cupidon. usw.", "tokens": ["Las\u00b7se", "mich", "drinn", "Hun\u00b7gers", "ster\u00b7ben", ".", "Ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "NN", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Helft und helft ihr nicht geschwind,", "tokens": ["Helft", "und", "helft", "ihr", "nicht", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrz ich mich in einen Brunnen,", "tokens": ["St\u00fcrz", "ich", "mich", "in", "ei\u00b7nen", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie bald kann ein armes Kind", "tokens": ["Wie", "bald", "kann", "ein", "ar\u00b7mes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich, in der Hitz verbrennen, ach Cupidon. usw.", "tokens": ["Als", "ich", ",", "in", "der", "Hitz", "ver\u00b7bren\u00b7nen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "VVINF", "$,", "XY", "NE", "$.", "NE"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Rach o liebste Mutter Rach,", "tokens": ["Rach", "o", "liebs\u00b7te", "Mut\u00b7ter", "Rach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich werd noch verzweiffeln m\u00fcssen,", "tokens": ["Ich", "werd", "noch", "ver\u00b7zweif\u00b7feln", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Helft, ich spring sonst in den Bach,", "tokens": ["Helft", ",", "ich", "spring", "sonst", "in", "den", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder will mich selbst erschiessen, ach Cupidon. usw.", "tokens": ["O\u00b7der", "will", "mich", "selbst", "er\u00b7schies\u00b7sen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "$,", "XY", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Venus sprach vor Zorn kein Wort,", "tokens": ["Ve\u00b7nus", "sprach", "vor", "Zorn", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich nahm ein Hand voll Ruthen,", "tokens": ["End\u00b7lich", "nahm", "ein", "Hand", "voll", "Ru\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wart, ich will dich bringen fort,", "tokens": ["Wart", ",", "ich", "will", "dich", "brin\u00b7gen", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dir soll der Hintern bluten, ach Cupidon. usw.", "tokens": ["Da\u00df", "dir", "soll", "der", "Hin\u00b7tern", "blu\u00b7ten", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$,", "XY", "NE", "$.", "NE"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Hab ich dirs nicht vor gesagt,", "tokens": ["Hab", "ich", "dirs", "nicht", "vor", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "PTKNEG", "APPR", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du solt stupfens m\u00fcssig gehen,", "tokens": ["Du", "solt", "stup\u00b7fens", "m\u00fcs\u00b7sig", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wer nicht folgen will der wagt.", "tokens": ["Wer", "nicht", "fol\u00b7gen", "will", "der", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVINF", "VMFIN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm her, la\u00df den Finger sehen, ach Cupidon. usw.", "tokens": ["Komm", "her", ",", "la\u00df", "den", "Fin\u00b7ger", "se\u00b7hen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["VVFIN", "PTKVZ", "$,", "VVIMP", "ART", "NN", "VVINF", "$,", "XY", "NE", "$.", "NE"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Ey du ungerathner Sohn,", "tokens": ["Ey", "du", "un\u00b7ge\u00b7rath\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir ist eben recht geschehen,", "tokens": ["Dir", "ist", "e\u00b7ben", "recht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist dein verdienter Lohn,", "tokens": ["Das", "ist", "dein", "ver\u00b7dien\u00b7ter", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilt nicht mit der Mutter gehen, ach Cupidon. usw.", "tokens": ["Wilt", "nicht", "mit", "der", "Mut\u00b7ter", "ge\u00b7hen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,", "XY", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Indem b\u00fckt sie ihn herum:", "tokens": ["In\u00b7dem", "b\u00fckt", "sie", "ihn", "he\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt ich will dich lehren sitzen,", "tokens": ["Halt", "ich", "will", "dich", "leh\u00b7ren", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00df' g\u00df' noch einmal so kum,", "tokens": ["G\u00df'", "g\u00df'", "noch", "ein\u00b7mal", "so", "kum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann will ich dich besser fitzen, ach Cupidon. usw.", "tokens": ["Dann", "will", "ich", "dich", "bes\u00b7ser", "fit\u00b7zen", ",", "ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,", "XY", "NE", "$.", "NE"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Cupido fiel auf die Erd,", "tokens": ["Cu\u00b7pi\u00b7do", "fiel", "auf", "die", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ha! wie that ihn das verdriessen,", "tokens": ["Ha", "!", "wie", "that", "ihn", "das", "ver\u00b7dries\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie ein zaumloses Pferd,", "tokens": ["Und", "wie", "ein", "zaum\u00b7lo\u00b7ses", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Schlug um sich mit H\u00e4nd und F\u00fc\u00dfen, da Cupidon. usw.", "tokens": ["Schlug", "um", "sich", "mit", "H\u00e4nd", "und", "F\u00fc\u00b7\u00dfen", ",", "da", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$,", "KOUS", "NE", "$.", "NE"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Ach mein, klag dich nicht so sehr,", "tokens": ["Ach", "mein", ",", "klag", "dich", "nicht", "so", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach sie, und bald la\u00df die Possen,", "tokens": ["Sprach", "sie", ",", "und", "bald", "la\u00df", "die", "Pos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "ADV", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk' da\u00df du wohl andre mehr", "tokens": ["Denk'", "da\u00df", "du", "wohl", "and\u00b7re", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unverschuldet hast geschossen, ha Cupidon. usw.", "tokens": ["Un\u00b7ver\u00b7schul\u00b7det", "hast", "ge\u00b7schos\u00b7sen", ",", "ha", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["ADJD", "VAFIN", "VVPP", "$,", "NE", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Deine Pfeil sind voller Gift,", "tokens": ["Dei\u00b7ne", "Pfeil", "sind", "vol\u00b7ler", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gehn richtig zu dem Herzen,", "tokens": ["Und", "gehn", "rich\u00b7tig", "zu", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was aber den Finger trift,", "tokens": ["Was", "a\u00b7ber", "den", "Fin\u00b7ger", "trift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist nur ein Kinderscherzen. Ha Cupidon. usw.", "tokens": ["Das", "ist", "nur", "ein", "Kin\u00b7der\u00b7scher\u00b7zen", ".", "Ha", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$.", "NE", "NE", "$.", "NE"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Thuts dir schon ein wenig weh,", "tokens": ["Thuts", "dir", "schon", "ein", "we\u00b7nig", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darfst dir drum nicht lassen bangen,", "tokens": ["Darfst", "dir", "drum", "nicht", "las\u00b7sen", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh du dreimal Steh und Geh,", "tokens": ["Eh", "du", "drei\u00b7mal", "Steh", "und", "Geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagst, so wird es sein vergangen. Ach Cupidon. usw.", "tokens": ["Sagst", ",", "so", "wird", "es", "sein", "ver\u00b7gan\u00b7gen", ".", "Ach", "Cu\u00b7pi\u00b7don", ".", "us\u00b7w."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$.", "NN", "NE", "$.", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Wen der lose Vorwiz sticht,", "tokens": ["Wen", "der", "lo\u00b7se", "Vor\u00b7wiz", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und solch Leckerey will treiben,", "tokens": ["Und", "solch", "Le\u00b7cke\u00b7rey", "will", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem gerath es anders nicht,", "tokens": ["Dem", "ge\u00b7rath", "es", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum sollst bei der Mutter bleiben.", "tokens": ["Drum", "sollst", "bei", "der", "Mut\u00b7ter", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Cupidon kleiner Schelm,", "tokens": ["Ach", "Cu\u00b7pi\u00b7don", "klei\u00b7ner", "Schelm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie machst du so gro\u00dfe Wunden!", "tokens": ["Wie", "machst", "du", "so", "gro\u00b7\u00dfe", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Du Stupfer, du Hauser,", "tokens": ["Du", "Stup\u00b7fer", ",", "du", "Hau\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du Rupfer, du Zau\u00dfer,", "tokens": ["Du", "Rup\u00b7fer", ",", "du", "Zau\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du Lecker, du Lauser,", "tokens": ["Du", "Le\u00b7cker", ",", "du", "Lau\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du Schlecker, du Mauser,", "tokens": ["Du", "Schle\u00b7cker", ",", "du", "Mau\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So soll es dir gehn,", "tokens": ["So", "soll", "es", "dir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Recht ist dir geschehn,", "tokens": ["Recht", "ist", "dir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "So soll es dir gehn!!!", "tokens": ["So", "soll", "es", "dir", "gehn", "!!!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}