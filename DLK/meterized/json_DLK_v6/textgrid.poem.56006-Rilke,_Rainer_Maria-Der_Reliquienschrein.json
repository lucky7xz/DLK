{"textgrid.poem.56006": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Reliquienschrein", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen wartete auf alle Ringe", "tokens": ["Drau\u00b7\u00dfen", "war\u00b7te\u00b7te", "auf", "al\u00b7le", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "und auf jedes Kettenglied", "tokens": ["und", "auf", "je\u00b7des", "Ket\u00b7ten\u00b7glied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schicksal, das nicht ohne sie geschieht.", "tokens": ["Schick\u00b7sal", ",", "das", "nicht", "oh\u00b7ne", "sie", "ge\u00b7schieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Drinnen waren sie nur Dinge, Dinge", "tokens": ["Drin\u00b7nen", "wa\u00b7ren", "sie", "nur", "Din\u00b7ge", ",", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "die er schmiedete; denn vor dem Schmied", "tokens": ["die", "er", "schmie\u00b7de\u00b7te", ";", "denn", "vor", "dem", "Schmied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "VVFIN", "$.", "KON", "APPR", "ART", "NN"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "war sogar die Krone, die er bog,", "tokens": ["war", "so\u00b7gar", "die", "Kro\u00b7ne", ",", "die", "er", "bog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "nur ein Ding, ein zitterndes und eines", "tokens": ["nur", "ein", "Ding", ",", "ein", "zit\u00b7tern\u00b7des", "und", "ei\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "KON", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "das er finster wie im Zorn erzog", "tokens": ["das", "er", "fins\u00b7ter", "wie", "im", "Zorn", "er\u00b7zog"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADJD", "KOKOM", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "zu dem Tragen eines reinen Steines.", "tokens": ["zu", "dem", "Tra\u00b7gen", "ei\u00b7nes", "rei\u00b7nen", "Stei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Seine Augen wurden immer k\u00e4lter", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "wur\u00b7den", "im\u00b7mer", "k\u00e4l\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "von dem kalten t\u00e4glichen Getr\u00e4nk;", "tokens": ["von", "dem", "kal\u00b7ten", "t\u00e4g\u00b7li\u00b7chen", "Ge\u00b7tr\u00e4nk", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "aber als der herrliche Beh\u00e4lter", "tokens": ["a\u00b7ber", "als", "der", "herr\u00b7li\u00b7che", "Be\u00b7h\u00e4l\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "(goldgetrieben, k\u00f6stlich, vielkar\u00e4tig)", "tokens": ["(", "gold\u00b7ge\u00b7trie\u00b7ben", ",", "k\u00f6st\u00b7lich", ",", "viel\u00b7ka\u00b7r\u00e4\u00b7tig", ")"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVPP", "$,", "ADJD", "$,", "ADJD", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "fertig vor ihm stand, das Weihgeschenk,", "tokens": ["fer\u00b7tig", "vor", "ihm", "stand", ",", "das", "Weih\u00b7ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "da\u00df darin ein kleines Handgelenk", "tokens": ["da\u00df", "da\u00b7rin", "ein", "klei\u00b7nes", "Hand\u00b7ge\u00b7lenk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "f\u00fcrder wohne, wei\u00df und wundert\u00e4tig:", "tokens": ["f\u00fcr\u00b7der", "woh\u00b7ne", ",", "wei\u00df", "und", "wun\u00b7der\u00b7t\u00e4\u00b7tig", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "blieb er ohne Ende auf den Knien,", "tokens": ["blieb", "er", "oh\u00b7ne", "En\u00b7de", "auf", "den", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hingeworfen, weinend, nichtmehr wagend,", "tokens": ["hin\u00b7ge\u00b7wor\u00b7fen", ",", "wei\u00b7nend", ",", "nicht\u00b7mehr", "wa\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "seine Seele niederschlagend", "tokens": ["sei\u00b7ne", "See\u00b7le", "nie\u00b7der\u00b7schla\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "vor dem ruhigen Rubin,", "tokens": ["vor", "dem", "ru\u00b7hi\u00b7gen", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "der ihn zu gewahren schien", "tokens": ["der", "ihn", "zu", "ge\u00b7wah\u00b7ren", "schien"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und ihn, pl\u00f6tzlich um sein Dasein fragend,", "tokens": ["und", "ihn", ",", "pl\u00f6tz\u00b7lich", "um", "sein", "Da\u00b7sein", "fra\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "ansah wie aus Dynastien.", "tokens": ["an\u00b7sah", "wie", "aus", "Dy\u00b7nas\u00b7ti\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}