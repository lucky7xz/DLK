{"textgrid.poem.61810": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf das Meer, das fernhinaus", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf das Meer, das fernhinaus", "tokens": ["Auf", "das", "Meer", ",", "das", "fern\u00b7hin\u00b7aus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkelt wie von gr\u00fcnem Erze,", "tokens": ["Dun\u00b7kelt", "wie", "von", "gr\u00fc\u00b7nem", "Er\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4llt ein breiter Sonnenstreif", "tokens": ["F\u00e4llt", "ein", "brei\u00b7ter", "Son\u00b7nen\u00b7streif"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des Sturmgew\u00f6lkes Schw\u00e4rze.", "tokens": ["Durch", "des", "Sturm\u00b7ge\u00b7w\u00f6l\u00b7kes", "Schw\u00e4r\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, und bunt von Strand zu Strand", "tokens": ["Sieh", ",", "und", "bunt", "von", "Strand", "zu", "Strand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spannt sein Tor der Regenbogen;", "tokens": ["Spannt", "sein", "Tor", "der", "Re\u00b7gen\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df besegelt unter ihm", "tokens": ["Wei\u00df", "be\u00b7se\u00b7gelt", "un\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ein Orlogschiff gezogen.", "tokens": ["Kommt", "ein", "Or\u00b7log\u00b7schiff", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deutsche Flagge, sei gegr\u00fc\u00dft!", "tokens": ["Deut\u00b7sche", "Flag\u00b7ge", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steure k\u00fchn durch Wind und Welle,", "tokens": ["Steu\u00b7re", "k\u00fchn", "durch", "Wind", "und", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht und Wolken hinter dir,", "tokens": ["Nacht", "und", "Wol\u00b7ken", "hin\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dir Sonnenaufgangshelle!", "tokens": ["Vor", "dir", "Son\u00b7nen\u00b7auf\u00b7gangs\u00b7hel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}