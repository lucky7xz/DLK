": "Er vollbringt, hinweggewandt, mit Beben,", "tokens": ["Er", "voll\u00b7bringt", ",", "hin\u00b7weg\u00b7ge\u00b7wandt", ",", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Fast bet\u00e4ubt, das grausame Gebot.", "tokens": ["Fast", "be\u00b7t\u00e4ubt", ",", "das", "grau\u00b7sa\u00b7me", "Ge\u00b7bot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sieht, gleich Blitzen, Flammen sich erheben,", "tokens": ["Sieht", ",", "gleich", "Blit\u00b7zen", ",", "Flam\u00b7men", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "NN", "$,", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fchlet tief im Innern Tod.", "tokens": ["F\u00fch\u00b7let", "tief", "im", "In\u00b7nern", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schreiend ras't am Rand der rothen Gluten", "tokens": ["Schrei\u00b7end", "ras't", "am", "Rand", "der", "ro\u00b7then", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jedes Weib, zerrauft die Locken, w\u00e4scht", "tokens": ["Je\u00b7des", "Weib", ",", "zer\u00b7rauft", "die", "Lo\u00b7cken", ",", "w\u00e4scht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"