{"textgrid.poem.39370": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hingelehnt an dieses Palmbaums K\u00fchle,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hingelehnt an dieses Palmbaums K\u00fchle,", "tokens": ["Hin\u00b7ge\u00b7lehnt", "an", "die\u00b7ses", "Palm\u00b7baums", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weilen wir in schwermuthsvoller Ruh.", "tokens": ["Wei\u00b7len", "wir", "in", "schwer\u00b7muths\u00b7vol\u00b7ler", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bruder, hier ergie\u00df' ich was ich f\u00fchle,", "tokens": ["Bru\u00b7der", ",", "hier", "er\u00b7gie\u00df'", "ich", "was", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hauche du dein Mitgef\u00fchl mir zu.", "tokens": ["Hau\u00b7che", "du", "dein", "Mit\u00b7ge\u00b7f\u00fchl", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df uns hier der Menschen Loo\u00df erw\u00e4gen,", "tokens": ["La\u00df", "uns", "hier", "der", "Men\u00b7schen", "Loo\u00df", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und dann seufzen: die\u00df ist unser Loo\u00df!", "tokens": ["Und", "dann", "seuf\u00b7zen", ":", "die\u00df", "ist", "un\u00b7ser", "Loo\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hin zu irren auf bedornten Wegen", "tokens": ["Hin", "zu", "ir\u00b7ren", "auf", "be\u00b7dorn\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In des Grabes finstern Schoo\u00df.", "tokens": ["In", "des", "Gra\u00b7bes", "fins\u00b7tern", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Seele schmachtet aus dem Kerker,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "schmach\u00b7tet", "aus", "dem", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf gen Himmel bebt der matte Blick;", "tokens": ["Auf", "gen", "Him\u00b7mel", "bebt", "der", "mat\u00b7te", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unsichtbare Kraft nur wirft sie st\u00e4rker", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Kraft", "nur", "wirft", "sie", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In den Staub, worin sie wohnt, zur\u00fcck.", "tokens": ["In", "den", "Staub", ",", "wo\u00b7rin", "sie", "wohnt", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber einst, von Schmerz und Schw\u00e4ch' entladen,", "tokens": ["A\u00b7ber", "einst", ",", "von", "Schmerz", "und", "Schw\u00e4ch'", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Soll, mit ew'ger Sch\u00f6nheit angethan,", "tokens": ["Soll", ",", "mit", "ew'\u00b7ger", "Sch\u00f6n\u00b7heit", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sie die Heimat finden, und sich baden", "tokens": ["Sie", "die", "Hei\u00b7mat", "fin\u00b7den", ",", "und", "sich", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF", "$,", "KON", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In des Lichtes Ocean.", "tokens": ["In", "des", "Lich\u00b7tes", "O\u00b7cean", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber horch! aus tiefer Ferne hallet", "tokens": ["A\u00b7ber", "horch", "!", "aus", "tie\u00b7fer", "Fer\u00b7ne", "hal\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kaum vernommnes Rauschen; schw\u00fcl und schwer", "tokens": ["Kaum", "ver\u00b7nomm\u00b7nes", "Rau\u00b7schen", ";", "schw\u00fcl", "und", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ruht die Luft, und das Get\u00f6se wallet", "tokens": ["Ruht", "die", "Luft", ",", "und", "das", "Ge\u00b7t\u00f6\u00b7se", "wal\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Ebne langsam auf uns her.", "tokens": ["Durch", "die", "Eb\u00b7ne", "lang\u00b7sam", "auf", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Lauter st\u00f6hnt's und lauter in den Palmen,", "tokens": ["Lau\u00b7ter", "st\u00f6hnt's", "und", "lau\u00b7ter", "in", "den", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deren Wipfel schauervoll sich regt,", "tokens": ["De\u00b7ren", "Wip\u00b7fel", "schau\u00b7er\u00b7voll", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie, vom West durchw\u00fchlt, ein Feld mit Halmen", "tokens": ["Wie", ",", "vom", "West", "durch\u00b7w\u00fchlt", ",", "ein", "Feld", "mit", "Hal\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Immer h\u00f6h're Wellen schl\u00e4gt.", "tokens": ["Im\u00b7mer", "h\u00f6h'\u00b7re", "Wel\u00b7len", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdas ist Leichenzug. \u2013 Auf diesem wilden", "tokens": ["\u00bb", "das", "ist", "Lei\u00b7chen\u00b7zug", ".", "\u2013", "Auf", "die\u00b7sem", "wil\u00b7den"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "NN", "$.", "$(", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Acker, wo kein Thau auf Kr\u00e4uter flie\u00dft,", "tokens": ["A\u00b7cker", ",", "wo", "kein", "Thau", "auf", "Kr\u00e4u\u00b7ter", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die Sonne, nicht um zu verg\u00fclden,", "tokens": ["Wo", "die", "Son\u00b7ne", ",", "nicht", "um", "zu", "ver\u00b7g\u00fcl\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PTKNEG", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur zu d\u00f6rren, gl\u00fchnde Strahlen schie\u00dft,", "tokens": ["Nur", "zu", "d\u00f6r\u00b7ren", ",", "gl\u00fchn\u00b7de", "Strah\u00b7len", "schie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Br\u00fctet der Verwesung schwarzer Fl\u00fcgel", "tokens": ["Br\u00fc\u00b7tet", "der", "Ver\u00b7we\u00b7sung", "schwar\u00b7zer", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seit Aeonen \u00fcber manchem Staub,", "tokens": ["Seit", "A\u00b7e\u00b7o\u00b7nen", "\u00fc\u00b7ber", "man\u00b7chem", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und vor jedem morschen Knochenh\u00fcgel", "tokens": ["Und", "vor", "je\u00b7dem", "mor\u00b7schen", "Kno\u00b7chen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "W\u00f6lkt sich nichts, als Todtenstaub.", "tokens": ["W\u00f6lkt", "sich", "nichts", ",", "als", "Tod\u00b7ten\u00b7staub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Siehst du sie herannahn? Sch\u00e4del zittern", "tokens": ["Siehst", "du", "sie", "her\u00b7an\u00b7nahn", "?", "Sch\u00e4\u00b7del", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVIZU", "$.", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dumpfert\u00f6nend unter jedem Schritt.", "tokens": ["Dum\u00b7pfer\u00b7t\u00f6\u00b7nend", "un\u00b7ter", "je\u00b7dem", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Schaar von M\u00e4nnern, Greisen, M\u00fcttern,", "tokens": ["In", "der", "Schaar", "von", "M\u00e4n\u00b7nern", ",", "Grei\u00b7sen", ",", "M\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wanken Kinder, jetzt nicht h\u00fcpfend, mit.", "tokens": ["Wan\u00b7ken", "Kin\u00b7der", ",", "jetzt", "nicht", "h\u00fcp\u00b7fend", ",", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "PTKNEG", "VVPP", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht in Trauerfarben gehn die Weiber,", "tokens": ["Nicht", "in", "Trau\u00b7er\u00b7far\u00b7ben", "gehn", "die", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Prangen mit Geschmeid' und Perlen nicht,", "tokens": ["Pran\u00b7gen", "mit", "Ge\u00b7schmeid'", "und", "Per\u00b7len", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Kummer schm\u00fcckt die abgeh\u00e4rmten Leiber,", "tokens": ["Kum\u00b7mer", "schm\u00fcckt", "die", "ab\u00b7ge\u00b7h\u00e4rm\u00b7ten", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Thr\u00e4nen salben ihr Gesicht.", "tokens": ["Thr\u00e4\u00b7nen", "sal\u00b7ben", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weiber, bleibt zur\u00fcck! \u2013 die Gatten, Br\u00fcder", "tokens": ["Wei\u00b7ber", ",", "bleibt", "zu\u00b7r\u00fcck", "!", "\u2013", "die", "Gat\u00b7ten", ",", "Br\u00fc\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "St\u00f6ret sonst des Jammers Ungest\u00fcm.", "tokens": ["St\u00f6\u00b7ret", "sonst", "des", "Jam\u00b7mers", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schweigend setzen sie den Leichnam nieder,", "tokens": ["Schwei\u00b7gend", "set\u00b7zen", "sie", "den", "Leich\u00b7nam", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schichten dort ein Bett aus Cedern ihm.", "tokens": ["Schich\u00b7ten", "dort", "ein", "Bett", "aus", "Ce\u00b7dern", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df uns n\u00e4her an den Schauplatz treten.", "tokens": ["La\u00df", "uns", "n\u00e4\u00b7her", "an", "den", "Schau\u00b7platz", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn nun L\u00fcfte seinen Rest zerstreun,", "tokens": ["Wenn", "nun", "L\u00fcf\u00b7te", "sei\u00b7nen", "Rest", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wer es sei, la\u00df uns im Stillen beten:", "tokens": ["Wer", "es", "sei", ",", "la\u00df", "uns", "im", "Stil\u00b7len", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Friede segne sein Gebein!", "tokens": ["Frie\u00b7de", "seg\u00b7ne", "sein", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie entschleiern ihn. Die welken Wangen", "tokens": ["Sie", "ent\u00b7schlei\u00b7ern", "ihn", ".", "Die", "wel\u00b7ken", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die glatte Scheitel wie so sch\u00f6n!", "tokens": ["Und", "die", "glat\u00b7te", "Schei\u00b7tel", "wie", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KOKOM", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sehnlich soll mein Blick an ihnen hangen,", "tokens": ["Sehn\u00b7lich", "soll", "mein", "Blick", "an", "ih\u00b7nen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis vom Holzsto\u00df helle Flammen wehn.", "tokens": ["Bis", "vom", "Holz\u00b7sto\u00df", "hel\u00b7le", "Flam\u00b7men", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00e4chelnd f\u00fchltest du die Stirn erkalten,", "tokens": ["L\u00e4\u00b7chelnd", "f\u00fchl\u00b7test", "du", "die", "Stirn", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "L\u00e4chelnd sanken dir die Augen zu;", "tokens": ["L\u00e4\u00b7chelnd", "san\u00b7ken", "dir", "die", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Engelk\u00fc\u00dfe pr\u00e4gten in die Falten", "tokens": ["En\u00b7gel\u00b7k\u00fc\u00b7\u00dfe", "pr\u00e4g\u00b7ten", "in", "die", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wonn' und Paradiesesruh.", "tokens": ["Wonn'", "und", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbach, bist du's, Cudwallo? \u2013 H\u00f6re, h\u00f6re! \u2013", "tokens": ["\u00bb", "ach", ",", "bist", "du's", ",", "Cud\u00b7wal\u00b7lo", "?", "\u2013", "H\u00f6\u00b7re", ",", "h\u00f6\u00b7re", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PIS", "$,", "NE", "$.", "$(", "NE", "$,", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch, verschlo\u00dfen ist mir ja dein Ohr. \u2013", "tokens": ["Doch", ",", "ver\u00b7schlo\u00b7\u00dfen", "ist", "mir", "ja", "dein", "Ohr", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "VVPP", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einen liebevollen Blick gew\u00e4hre! \u2013", "tokens": ["Ei\u00b7nen", "lie\u00b7be\u00b7vol\u00b7len", "Blick", "ge\u00b7w\u00e4h\u00b7re", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! dein Aug' umnebelt d\u00fcstrer Flor. \u2013", "tokens": ["Ach", "!", "dein", "Aug'", "um\u00b7ne\u00b7belt", "d\u00fcst\u00b7rer", "Flor", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Frommer Greis, auch du bist hingeschieden!", "tokens": ["From\u00b7mer", "Greis", ",", "auch", "du", "bist", "hin\u00b7ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denke mein! Gehab' dich ewig wohl!", "tokens": ["Den\u00b7ke", "mein", "!", "Ge\u00b7hab'", "dich", "e\u00b7wig", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "$.", "NN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Lehrte doch dein Bildni\u00df jeden m\u00fcden", "tokens": ["Lehr\u00b7te", "doch", "dein", "Bild\u00b7ni\u00df", "je\u00b7den", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PIAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Pilger, wie er sterben soll.", "tokens": ["Pil\u00b7ger", ",", "wie", "er", "ster\u00b7ben", "soll", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Du entschliefst mit einer heil'gen Bitte,", "tokens": ["Du", "ent\u00b7schliefst", "mit", "ei\u00b7ner", "heil'\u00b7gen", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Flur im Mondenschimmer lag;", "tokens": ["Wenn", "die", "Flur", "im", "Mon\u00b7den\u00b7schim\u00b7mer", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit Hymnen tratst du vor die H\u00fctte,", "tokens": ["Und", "mit", "Hym\u00b7nen", "tratst", "du", "vor", "die", "H\u00fct\