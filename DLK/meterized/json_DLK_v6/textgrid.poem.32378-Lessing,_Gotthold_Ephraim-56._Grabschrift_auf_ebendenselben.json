{"textgrid.poem.32378": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "56. Grabschrift auf ebendenselben", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier faulet Mimulus, ein Affe.", "tokens": ["Hier", "fau\u00b7let", "Mi\u00b7mu\u00b7lus", ",", "ein", "Af\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leider! leider! welch ein Affe!", "tokens": ["Und", "lei\u00b7der", "!", "lei\u00b7der", "!", "welch", "ein", "Af\u00b7fe", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So zahm, als in der Welt kein Affe;", "tokens": ["So", "zahm", ",", "als", "in", "der", "Welt", "kein", "Af\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So rein, als in der Welt kein Affe;", "tokens": ["So", "rein", ",", "als", "in", "der", "Welt", "kein", "Af\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So keusch, als in der Welt kein Affe;", "tokens": ["So", "keusch", ",", "als", "in", "der", "Welt", "kein", "Af\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ernst, als in der Welt kein Affe;", "tokens": ["So", "ernst", ",", "als", "in", "der", "Welt", "kein", "Af\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So ohne Falsch. O welch ein Affe!", "tokens": ["So", "oh\u00b7ne", "Falsch", ".", "O", "welch", "ein", "Af\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "NE", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit ichs kurz zusammen raffe:", "tokens": ["Da\u00b7mit", "ichs", "kurz", "zu\u00b7sam\u00b7men", "raf\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein ganz originaler Affe.", "tokens": ["Ein", "ganz", "o\u00b7rig\u00b7i\u00b7na\u00b7ler", "Af\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}