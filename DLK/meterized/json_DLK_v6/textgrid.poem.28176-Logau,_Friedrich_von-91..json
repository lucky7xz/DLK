{"textgrid.poem.28176": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krieg war au\u00df dem Krieg entlauffen", "tokens": ["Krieg", "war", "au\u00df", "dem", "Krieg", "ent\u00b7lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem tollen Frevler-Hauffen,", "tokens": ["Zu", "dem", "tol\u00b7len", "Frev\u00b7ler\u00b7Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in seines Freundes Blute", "tokens": ["Der", "in", "sei\u00b7nes", "Freun\u00b7des", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehre suchte seinem Mute.", "tokens": ["Eh\u00b7re", "such\u00b7te", "sei\u00b7nem", "Mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt den Held, der mit Bedacht", "tokens": ["Lobt", "den", "Held", ",", "der", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Krieg zum Kriege wieder bracht!", "tokens": ["Krieg", "zum", "Krie\u00b7ge", "wie\u00b7der", "bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df nun Sieg und Krieges Zucht", "tokens": ["Da\u00df", "nun", "Sieg", "und", "Krie\u00b7ges", "Zucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder unser Land besucht.", "tokens": ["Wie\u00b7der", "un\u00b7ser", "Land", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Merckt und r\u00fchmt die edle Raute;", "tokens": ["Merckt", "und", "r\u00fchmt", "die", "ed\u00b7le", "Rau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Neiget euch f\u00fcr ihrem Kraute,", "tokens": ["Nei\u00b7get", "euch", "f\u00fcr", "ih\u00b7rem", "Krau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df f\u00fcr so viel Landes-Gifften", "tokens": ["Da\u00df", "f\u00fcr", "so", "viel", "Lan\u00b7des\u00b7Giff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Kan so heilsam Artzney stifften!", "tokens": ["Kan", "so", "heil\u00b7sam", "Artz\u00b7ney", "stiff\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}