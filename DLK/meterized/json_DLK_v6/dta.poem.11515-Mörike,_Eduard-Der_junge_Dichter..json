{"dta.poem.11515": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der junge Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sch\u00f6nheit sonst, der Anmuth", "tokens": ["Wenn", "der", "Sch\u00f6n\u00b7heit", "sonst", ",", "der", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer fl\u00fcchtige Erscheinung", "tokens": ["Im\u00b7mer", "fl\u00fcch\u00b7ti\u00b7ge", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein heller Glanz der Sonne", "tokens": ["Wie", "ein", "hel\u00b7ler", "Glanz", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal vor die Sinne wieder", "tokens": ["Ein\u00b7mal", "vor", "die", "Sin\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Neuheit Zauber trat,", "tokens": ["Mit", "der", "Neu\u00b7heit", "Zau\u00b7ber", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ein heimlich trunknes Jauchzen", "tokens": ["Da\u00df", "ein", "heim\u00b7lich", "trunk\u00b7nes", "Jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir der Ausdruck lautern Dankes", "tokens": ["Mir", "der", "Aus\u00b7druck", "lau\u00b7tern", "Dan\u00b7kes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr solch s\u00fc\u00dfes Daseyn war:", "tokens": ["F\u00fcr", "solch", "s\u00fc\u00b7\u00dfes", "Da\u00b7seyn", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O wie drang es da mich armen,", "tokens": ["O", "wie", "drang", "es", "da", "mich", "ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "KOUS", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich unm\u00fcnd'gen Sohn Apollens,", "tokens": ["Mich", "un\u00b7m\u00fcn\u00b7d'\u00b7gen", "Sohn", "A\u00b7pol\u00b7lens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dieses Alles auch in sch\u00f6ner,", "tokens": ["Die\u00b7ses", "Al\u00b7les", "auch", "in", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PIS", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Abgeschlossener Gestaltung", "tokens": ["Ab\u00b7ge\u00b7schlos\u00b7se\u00b7ner", "Ge\u00b7stal\u00b7tung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.13": {"text": "Fest, auf ewig festzuhalten,", "tokens": ["Fest", ",", "auf", "e\u00b7wig", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Es durch goldne Leierkl\u00e4nge", "tokens": ["Es", "durch", "gold\u00b7ne", "Lei\u00b7er\u00b7kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "So zum Einklang mit mir selber", "tokens": ["So", "zum", "Ein\u00b7klang", "mit", "mir", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Umzubilden, neu zu schaffen,", "tokens": ["Um\u00b7zu\u00b7bil\u00b7den", ",", "neu", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df ich, heiter wie ein Gott,", "tokens": ["Da\u00df", "ich", ",", "hei\u00b7ter", "wie", "ein", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Ueber der gediegnen Sch\u00f6ne,", "tokens": ["Ue\u00b7ber", "der", "ge\u00b7dieg\u00b7nen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Die aus mir herausgetreten,", "tokens": ["Die", "aus", "mir", "her\u00b7aus\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Die ich ganz mein eigen nenne,", "tokens": ["Die", "ich", "ganz", "mein", "ei\u00b7gen", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ruhig, klaren Auges schwebe.", "tokens": ["Ru\u00b7hig", ",", "kla\u00b7ren", "Au\u00b7ges", "schwe\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch, wenn mir das tief Empfundne", "tokens": ["Doch", ",", "wenn", "mir", "das", "tief", "Emp\u00b7fund\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "ADJD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nicht alsbald so rein und v\u00f6llig,", "tokens": ["Nicht", "als\u00b7bald", "so", "rein", "und", "v\u00f6l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie es in der Seele lebte,", "tokens": ["Wie", "es", "in", "der", "See\u00b7le", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Dichters zweite Seele,", "tokens": ["In", "des", "Dich\u00b7ters", "zwei\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Gesang, hin\u00fcberspielte,", "tokens": ["Den", "Ge\u00b7sang", ",", "hin\u00b7\u00fc\u00b7bers\u00b7piel\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich nur mit stumpfem Finger", "tokens": ["Wenn", "ich", "nur", "mit", "stump\u00b7fem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungelenk die Saiten r\u00fchrte,", "tokens": ["Un\u00b7ge\u00b7lenk", "die", "Sai\u00b7ten", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollt' ich dann nicht muthlos werden,", "tokens": ["Sollt'", "ich", "dann", "nicht", "muth\u00b7los", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df ich stets ein Sch\u00fcler bleibe?", "tokens": ["Da\u00df", "ich", "stets", "ein", "Sch\u00fc\u00b7ler", "blei\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber, Liebchen, sieh, bei dir", "tokens": ["A\u00b7ber", ",", "Lieb\u00b7chen", ",", "sieh", ",", "bei", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich pl\u00f6tzlich wie verwandelt,", "tokens": ["Bin", "ich", "pl\u00f6tz\u00b7lich", "wie", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im erw\u00e4rmten Winterst\u00fcbchen", "tokens": ["Im", "er\u00b7w\u00e4rm\u00b7ten", "Win\u00b7ter\u00b7st\u00fcb\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dem Schimmer dieser Lampe,", "tokens": ["Bei", "dem", "Schim\u00b7mer", "die\u00b7ser", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ich deinen Worten lausche,", "tokens": ["Wo", "ich", "dei\u00b7nen", "Wor\u00b7ten", "lau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hold bescheidnen Liebesworten.", "tokens": ["Hold", "be\u00b7scheid\u00b7nen", "Lie\u00b7bes\u00b7wor\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie du dann geruhig deine", "tokens": ["Wie", "du", "dann", "ge\u00b7ru\u00b7hig", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Braunen Lockenhaare schlichtest,", "tokens": ["Brau\u00b7nen", "Lo\u00b7cken\u00b7haa\u00b7re", "schlich\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Also legt sich sch\u00f6n gegl\u00e4ttet", "tokens": ["Al\u00b7so", "legt", "sich", "sch\u00f6n", "ge\u00b7gl\u00e4t\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All dies wirre Bilderwesen,", "tokens": ["All", "dies", "wir\u00b7re", "Bil\u00b7der\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "All des Herzens eitle Sorge,", "tokens": ["All", "des", "Her\u00b7zens", "eit\u00b7le", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Viel-zertheiltes Thun und Denken.", "tokens": ["Viel\u00b7zert\u00b7heil\u00b7tes", "Thun", "und", "Den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Froh begeistert, leicht gefiedert,", "tokens": ["Froh", "be\u00b7geis\u00b7tert", ",", "leicht", "ge\u00b7fie\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Flieg' ich aus der Dichtung engen", "tokens": ["Flieg'", "ich", "aus", "der", "Dich\u00b7tung", "en\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Rosenbanden, da\u00df ich nur", "tokens": ["Ro\u00b7sen\u00b7ban\u00b7den", ",", "da\u00df", "ich", "nur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Noch in ihrem reinen Dufte,", "tokens": ["Noch", "in", "ih\u00b7rem", "rei\u00b7nen", "Duf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Als im Elemente, lebe.", "tokens": ["Als", "im", "E\u00b7le\u00b7men\u00b7te", ",", "le\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Oder, M\u00e4dchen, sage mir,", "tokens": ["O\u00b7der", ",", "M\u00e4d\u00b7chen", ",", "sa\u00b7ge", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du gar die Muse selber,", "tokens": ["Bist", "du", "gar", "die", "Mu\u00b7se", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, wie wahre Dichtung pflegt,", "tokens": ["Die", ",", "wie", "wah\u00b7re", "Dich\u00b7tung", "pflegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst unwissend, wer sie sey,", "tokens": ["Selbst", "un\u00b7wis\u00b7send", ",", "wer", "sie", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich in ihren Armen h\u00e4lt,", "tokens": ["Mich", "in", "ih\u00b7ren", "Ar\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich selber, eins mit ihr,", "tokens": ["Da\u00df", "ich", "sel\u00b7ber", ",", "eins", "mit", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PIS", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur ein zart Gedicht erscheine?", "tokens": ["Nur", "ein", "zart", "Ge\u00b7dicht", "er\u00b7schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O du Liebliche, du l\u00e4chelst,", "tokens": ["O", "du", "Lieb\u00b7li\u00b7che", ",", "du", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttelst, k\u00fcssend mich, das K\u00f6pfchen,", "tokens": ["Sch\u00fct\u00b7telst", ",", "k\u00fcs\u00b7send", "mich", ",", "das", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und begreifst nicht, was ich meine.", "tokens": ["Und", "be\u00b7greifst", "nicht", ",", "was", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' ich selber es nicht wissen,", "tokens": ["M\u00f6cht'", "ich", "sel\u00b7ber", "es", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wissen nur, da\u00df du mich liebest,", "tokens": ["Wis\u00b7sen", "nur", ",", "da\u00df", "du", "mich", "lie\u00b7best", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich in dem Flug der Zeit", "tokens": ["Da\u00df", "ich", "in", "dem", "Flug", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Deine kleinen H\u00e4nde halte!", "tokens": ["Dei\u00b7ne", "klei\u00b7nen", "H\u00e4n\u00b7de", "hal\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}