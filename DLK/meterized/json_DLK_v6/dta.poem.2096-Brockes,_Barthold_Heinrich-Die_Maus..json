{"dta.poem.2096": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Maus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die\u00df immer weiter gienge! Um nun, nebst dem", "tokens": ["Wenn", "die\u00df", "im\u00b7mer", "wei\u00b7ter", "gien\u00b7ge", "!", "Um", "nun", ",", "nebst", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDS", "ADV", "ADV", "VVFIN", "$.", "KOUI", "ADV", "$,", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "schnellen Sterben,", "tokens": ["schnel\u00b7len", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Eine solche b\u00f6se Brut auch noch ferner zu verderben,", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "b\u00f6\u00b7se", "Brut", "auch", "noch", "fer\u00b7ner", "zu", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADV", "ADV", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Scheinet eine gro\u00dfe Menge Thier und V\u00f6gel uns zu", "tokens": ["Schei\u00b7net", "ei\u00b7ne", "gro\u00b7\u00dfe", "Men\u00b7ge", "Thier", "und", "V\u00f6\u00b7gel", "uns", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "KON", "NN", "PPER", "PTKZU"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "n\u00fctzen", "tokens": ["n\u00fct\u00b7zen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und uns gleichsam zu besch\u00fctzen,", "tokens": ["Und", "uns", "gleich\u00b7sam", "zu", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Recht mit Vorsicht zubereitet. St\u00f6rche, Raben, Ha-", "tokens": ["Recht", "mit", "Vor\u00b7sicht", "zu\u00b7be\u00b7rei\u00b7tet", ".", "St\u00f6r\u00b7che", ",", "Ra\u00b7ben", ",", "Ha"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "VVPP", "$.", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "bicht, Eulen,", "tokens": ["bicht", ",", "Eu\u00b7len", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "F\u00fcchse, D\u00e4chse, Wiesel, Jgel, m\u00fcssen uns hier H\u00fclf", "tokens": ["F\u00fcch\u00b7se", ",", "D\u00e4ch\u00b7se", ",", "Wie\u00b7sel", ",", "Jgel", ",", "m\u00fcs\u00b7sen", "uns", "hier", "H\u00fclf"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "ADV", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "ertheilen;", "tokens": ["er\u00b7thei\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ja noch viele andre mehr, wie wir denn sogar von", "tokens": ["Ja", "noch", "vie\u00b7le", "and\u00b7re", "mehr", ",", "wie", "wir", "denn", "so\u00b7gar", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PIAT", "PIS", "ADV", "$,", "PWAV", "PPER", "ADV", "ADV", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Schlangen", "tokens": ["Schlan\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Gegen M\u00e4use H\u00fclf empfangen.", "tokens": ["Ge\u00b7gen", "M\u00e4u\u00b7se", "H\u00fclf", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Aber auf besondre Weise scheinen gegen M\u00e4us\u2019 und Ra-", "tokens": ["A\u00b7ber", "auf", "be\u00b7sond\u00b7re", "Wei\u00b7se", "schei\u00b7nen", "ge\u00b7gen", "M\u00e4us'", "und", "Ra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "tzen,", "tokens": ["tzen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Als ein rechter Gegengift, zu der Menschen Nutz, die", "tokens": ["Als", "ein", "rech\u00b7ter", "Ge\u00b7gen\u00b7gift", ",", "zu", "der", "Men\u00b7schen", "Nutz", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,", "PRELS"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Katzen", "tokens": ["Kat\u00b7zen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ganz verwunderlich formiret. Wobey wir uns selbst", "tokens": ["Ganz", "ver\u00b7wun\u00b7der\u00b7lich", "for\u00b7mi\u00b7ret", ".", "Wo\u00b7bey", "wir", "uns", "selbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "KOUS", "PPER", "PRF", "ADV"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "zuweilen", "tokens": ["zu\u00b7wei\u00b7len"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Durch verschiedene Erfindung, Gift und Fallen, Rath", "tokens": ["Durch", "ver\u00b7schie\u00b7de\u00b7ne", "Er\u00b7fin\u00b7dung", ",", "Gift", "und", "Fal\u00b7len", ",", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "ertheilen.", "tokens": ["er\u00b7thei\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Wie von diesen schlimmen Thieren so verschiedne Sorten", "tokens": ["Wie", "von", "die\u00b7sen", "schlim\u00b7men", "Thie\u00b7ren", "so", "ver\u00b7schied\u00b7ne", "Sor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDAT", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Zeigte mir ein Zufall j\u00fcngst, da in einem Schlo\u00dfthurm", "tokens": ["Zeig\u00b7te", "mir", "ein", "Zu\u00b7fall", "j\u00fcngst", ",", "da", "in", "ei\u00b7nem", "Schlo\u00df\u00b7thurm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "sich", "tokens": ["sich"], "token_info": ["word"], "pos": ["PRF"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "(von den Eulen wohl vermuthlich hingetragen) ihrer", "tokens": ["(", "von", "den", "Eu\u00b7len", "wohl", "ver\u00b7muth\u00b7lich", "hin\u00b7ge\u00b7tra\u00b7gen", ")", "ih\u00b7rer"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$(", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "neun,", "tokens": ["neun", ","], "token_info": ["word", "punct"], "pos": ["CARD", "$,"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Und darunter sieben Sorten, welche alle sonderlich,", "tokens": ["Und", "da\u00b7run\u00b7ter", "sie\u00b7ben", "Sor\u00b7ten", ",", "wel\u00b7che", "al\u00b7le", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "CARD", "NN", "$,", "PRELS", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Und verschiedlich von Figur, auch von Farben, hin-", "tokens": ["Und", "ver\u00b7schied\u00b7lich", "von", "Fi\u00b7gur", ",", "auch", "von", "Far\u00b7ben", ",", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "gelegt", "tokens": ["ge\u00b7legt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "Und nur erst erbissen funden; woraus denn gar leicht zu", "tokens": ["Und", "nur", "erst", "er\u00b7bis\u00b7sen", "fun\u00b7den", ";", "wo\u00b7raus", "denn", "gar", "leicht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVINF", "VVFIN", "$.", "PWAV", "ADV", "ADV", "ADJD", "PTKZU"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "schlie\u00dfen,", "tokens": ["schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}}}}}