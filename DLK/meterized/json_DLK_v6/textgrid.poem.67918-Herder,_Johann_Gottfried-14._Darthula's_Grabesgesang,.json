{"textgrid.poem.67918": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Darthula's Grabesgesang,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen von Kola, du schl\u00e4fst!", "tokens": ["M\u00e4d\u00b7chen", "von", "Ko\u00b7la", ",", "du", "schl\u00e4fst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um dich schweigen die blauen Str\u00f6me Selma's!", "tokens": ["Um", "dich", "schwei\u00b7gen", "die", "blau\u00b7en", "Str\u00f6\u00b7me", "Sel\u00b7ma's", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie trauren um dich, den letzten Zweig", "tokens": ["Sie", "trau\u00b7ren", "um", "dich", ",", "den", "letz\u00b7ten", "Zweig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Thrutils Stamm!", "tokens": ["Von", "Thru\u00b7tils", "Stamm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn erstehst du wieder in deiner Sch\u00f6ne?", "tokens": ["Wenn", "er\u00b7stehst", "du", "wie\u00b7der", "in", "dei\u00b7ner", "Sch\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sch\u00f6nste der M\u00e4dchen in Erin!", "tokens": ["Sch\u00f6ns\u00b7te", "der", "M\u00e4d\u00b7chen", "in", "E\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du schl\u00e4fst im Grabe langen Schlaf,", "tokens": ["Du", "schl\u00e4fst", "im", "Gra\u00b7be", "lan\u00b7gen", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Morgenroth ist ferne!", "tokens": ["Dein", "Mor\u00b7gen\u00b7roth", "ist", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nimmer, o nimmer kommet mehr die Sonne,", "tokens": ["Nim\u00b7mer", ",", "o", "nim\u00b7mer", "kom\u00b7met", "mehr", "die", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weckend an deine Ruhest\u00e4tte: \u00bbwach auf!", "tokens": ["We\u00b7ckend", "an", "dei\u00b7ne", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", ":", "\u00bb", "wach", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$.", "$(", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wach auf Darthula!", "tokens": ["Wach", "auf", "Dar\u00b7thu\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Fr\u00fchling ist draussen,", "tokens": ["Fr\u00fch\u00b7ling", "ist", "draus\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die L\u00fcfte s\u00e4useln,", "tokens": ["Die", "L\u00fcf\u00b7te", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auf gr\u00fcnen H\u00fcgeln, holdseliges M\u00e4dchen,", "tokens": ["Auf", "gr\u00fc\u00b7nen", "H\u00fc\u00b7geln", ",", "hold\u00b7se\u00b7li\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weben die Blumen! im Hain wallt spriessendes Laub!\u00ab", "tokens": ["We\u00b7ben", "die", "Blu\u00b7men", "!", "im", "Hain", "wallt", "spries\u00b7sen\u00b7des", "Laub", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Auf immer, auf immer, so weiche denn, Sonne,", "tokens": ["Auf", "im\u00b7mer", ",", "auf", "im\u00b7mer", ",", "so", "wei\u00b7che", "denn", ",", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "$,", "APPR", "ADV", "$,", "ADV", "ADJA", "KON", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem M\u00e4dchen von Kola, sie schl\u00e4ft,", "tokens": ["Dem", "M\u00e4d\u00b7chen", "von", "Ko\u00b7la", ",", "sie", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie ersteht sie wieder in ihrer Sch\u00f6ne!", "tokens": ["Nie", "er\u00b7steht", "sie", "wie\u00b7der", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nie siehst du sie lieblich wandeln mehr.", "tokens": ["Nie", "siehst", "du", "sie", "lieb\u00b7lich", "wan\u00b7deln", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}