{"dta.poem.20276": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar k\u00f6stlich ist es wohl/ ein theil der welt regieren/", "tokens": ["Zwar", "k\u00f6st\u00b7lich", "ist", "es", "wohl", "/", "ein", "theil", "der", "welt", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$(", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr vieler herren seyn/ das schwerdt und scepter f\u00fchren/", "tokens": ["Herr", "vie\u00b7ler", "her\u00b7ren", "seyn", "/", "das", "schwerdt", "und", "scep\u00b7ter", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAINF", "$(", "PDS", "VVFIN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besitzen gut und blut; doch ist hier minder ruh", "tokens": ["Be\u00b7sit\u00b7zen", "gut", "und", "blut", ";", "doch", "ist", "hier", "min\u00b7der", "ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "NN", "$.", "ADV", "VAFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als auff der wilden see/ die grimmig ab und zu", "tokens": ["Als", "auff", "der", "wil\u00b7den", "see", "/", "die", "grim\u00b7mig", "ab", "und", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$(", "ART", "ADJD", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit ihren wellen jagt/ und nie vermag zu stehen.", "tokens": ["Mit", "ih\u00b7ren", "wel\u00b7len", "jagt", "/", "und", "nie", "ver\u00b7mag", "zu", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In einen grossen hof/ wo tausend leute gehen", "tokens": ["In", "ei\u00b7nen", "gros\u00b7sen", "hof", "/", "wo", "tau\u00b7send", "leu\u00b7te", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PWAV", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu suchen gnad und recht/ da schleichen auch hinein", "tokens": ["Zu", "su\u00b7chen", "gnad", "und", "recht", "/", "da", "schlei\u00b7chen", "auch", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "$(", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gefahr/ betrug und list. Es f\u00fchrt der grosse schein", "tokens": ["Ge\u00b7fahr", "/", "be\u00b7trug", "und", "list", ".", "Es", "f\u00fchrt", "der", "gros\u00b7se", "schein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Viel schatten hinter sich. Die auff dem throne sitzen", "tokens": ["Viel", "schat\u00b7ten", "hin\u00b7ter", "sich", ".", "Die", "auff", "dem", "thro\u00b7ne", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "$.", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In voller herrlichkeit/ und also h\u00e4uffig schwitzen/", "tokens": ["In", "vol\u00b7ler", "herr\u00b7lich\u00b7keit", "/", "und", "al\u00b7so", "h\u00e4uf\u00b7fig", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was meynstu/ da\u00df es sey? Der sommer thut es nicht/", "tokens": ["Was", "meyn\u00b7stu", "/", "da\u00df", "es", "sey", "?", "Der", "som\u00b7mer", "thut", "es", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "VAFIN", "$.", "ART", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sonne kan nicht hin: Was aus der stirne bricht/", "tokens": ["Die", "son\u00b7ne", "kan", "nicht", "hin", ":", "Was", "aus", "der", "stir\u00b7ne", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$.", "PWS", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist arbeit und beschwer. So viel hier leute dienen/", "tokens": ["Ist", "ar\u00b7beit", "und", "be\u00b7schwer", ".", "So", "viel", "hier", "leu\u00b7te", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJD", "$.", "ADV", "ADV", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sind ihnen mehrentheils zu dienste selbst erschienen:", "tokens": ["Sind", "ih\u00b7nen", "meh\u00b7ren\u00b7theils", "zu", "diens\u00b7te", "selbst", "er\u00b7schie\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie ehren nur die macht des f\u00fcrsten/ und nicht ihn/", "tokens": ["Sie", "eh\u00b7ren", "nur", "die", "macht", "des", "f\u00fcrs\u00b7ten", "/", "und", "nicht", "ihn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "VVFIN", "ART", "ADJA", "$(", "KON", "PTKNEG", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wenn sein gl\u00fccke f\u00e4llt/ so gehn sie auch dahin.", "tokens": ["Und", "wenn", "sein", "gl\u00fc\u00b7cke", "f\u00e4llt", "/", "so", "gehn", "sie", "auch", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}