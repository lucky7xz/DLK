{"dta.poem.15045": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "der schone dienten sinre hant", "tokens": ["der", "scho\u00b7ne", "dien\u00b7ten", "sin\u00b7re", "hant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der K&#250;nec von Egipte lant", "tokens": ["der", "K", "&#250;", "nec", "von", "E\u00b7gip\u00b7te", "lant"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "vnd der von Iturie", "tokens": ["vnd", "der", "von", "I\u00b7tu\u00b7rie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "mit grozer massenye", "tokens": ["mit", "gro\u00b7zer", "mas\u00b7se\u00b7nye"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "wurdent beide erkennet da", "tokens": ["wur\u00b7dent", "bei\u00b7de", "er\u00b7ken\u00b7net", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "der k&#250;nic rich von Libia", "tokens": ["der", "k", "&#250;", "nic", "rich", "von", "Li\u00b7bia"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PTKNEG", "ADJD", "APPR", "NE"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.7": {"text": "kam dar mit disen beiden", "tokens": ["kam", "dar", "mit", "di\u00b7sen", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "PDAT", "PIAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "ouch vuor da hin der Meden", "tokens": ["ouch", "vu\u00b7or", "da", "hin", "der", "Me\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "gebieter und ir Landevogt", "tokens": ["ge\u00b7bie\u00b7ter", "und", "ir", "Lan\u00b7de\u00b7vogt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von Barbarie dar gezogt", "tokens": ["von", "Bar\u00b7ba\u00b7rie", "dar", "ge\u00b7zogt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "waz der k&#250;nec schone", "tokens": ["waz", "der", "k", "&#250;", "nec", "scho\u00b7ne"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "ein here truoc die krone", "tokens": ["ein", "he\u00b7re", "truoc", "die", "kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "des riches in Arabia", "tokens": ["des", "ri\u00b7ches", "in", "A\u00b7ra\u00b7bia"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "den sach man werdeclichen da", "tokens": ["den", "sach", "man", "wer\u00b7de\u00b7cli\u00b7chen", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "zuo gahen unde riten", "tokens": ["zuo", "ga\u00b7hen", "un\u00b7de", "ri\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "ouch waz da bi den ziten", "tokens": ["ouch", "waz", "da", "bi", "den", "zi\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.17": {"text": "der k&#249;nec von Syrie", "tokens": ["der", "k", "&#249;", "nec", "von", "Sy\u00b7rie"], "token_info": ["word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "und der von Armenye", "tokens": ["und", "der", "von", "Ar\u00b7me\u00b7nye"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "brahte dar vil groz gedranc", "tokens": ["brah\u00b7te", "dar", "vil", "groz", "ge\u00b7dranc"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "ADV", "ADJD", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "von Baldach unde von Zaz amanc", "tokens": ["von", "Bal\u00b7dach", "un\u00b7de", "von", "Zaz", "a\u00b7manc"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "die zwene k&#250;nge vuoren dar", "tokens": ["die", "zwe\u00b7ne", "k", "&#250;", "nge", "vu\u00b7o\u00b7ren", "dar"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "so waz dahin mit sinre schar", "tokens": ["so", "waz", "da\u00b7hin", "mit", "sin\u00b7re", "schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "gestrichen der von Marroch", "tokens": ["ge\u00b7stri\u00b7chen", "der", "von", "Mar\u00b7roch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "ART", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "diz waren heiden alles doch", "tokens": ["diz", "wa\u00b7ren", "hei\u00b7den", "al\u00b7les", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "vnde hete si der Soldan", "tokens": ["vn\u00b7de", "he\u00b7te", "si", "der", "Sol\u00b7dan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "gefueret dar us des siges wan", "tokens": ["ge\u00b7fu\u00b7e\u00b7ret", "dar", "us", "des", "si\u00b7ges", "wan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "daz in div vrouwe solte erweln.", "tokens": ["daz", "in", "div", "vrou\u00b7we", "sol\u00b7te", "er\u00b7weln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "NE", "VMFIN", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Nu wil ich iv die K&#250;nge zeln", "tokens": ["Nu", "wil", "ich", "iv", "die", "K", "&#250;", "nge", "zeln"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NE", "$(", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "die von der werden cristenheit", "tokens": ["die", "von", "der", "wer\u00b7den", "cris\u00b7ten\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "ouch riten uf die heide breit", "tokens": ["ouch", "ri\u00b7ten", "uf", "die", "hei\u00b7de", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "vnd uf das wol geblvemte gras", "tokens": ["vnd", "uf", "das", "wol", "ge\u00b7bl\u00b7vem\u00b7te", "gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "der Keiser da von Rome was", "tokens": ["der", "Kei\u00b7ser", "da", "von", "Ro\u00b7me", "was"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NE", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "herlich und schone komen", "tokens": ["her\u00b7lich", "und", "scho\u00b7ne", "ko\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}