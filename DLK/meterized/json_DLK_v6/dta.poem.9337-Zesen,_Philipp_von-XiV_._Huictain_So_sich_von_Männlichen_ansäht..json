{"dta.poem.9337": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XiV .  \n Huictain/  \n So sich von M\u00e4nnlichen ans\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie darfstu schwartze Nacht doch nur so k\u00fchne seym", "tokens": ["Wie", "darfs\u00b7tu", "schwart\u00b7ze", "Nacht", "doch", "nur", "so", "k\u00fch\u00b7ne", "seym"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PAV", "VVFIN", "NN", "ADV", "ADV", "ADV", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und treten ins gemach/ wo Adelheit sich findet/", "tokens": ["und", "tre\u00b7ten", "ins", "ge\u00b7mach", "/", "wo", "A\u00b7del\u00b7heit", "sich", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADV", "$(", "PWAV", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Bl\u00fcthe dieser zeit/ wo Sie mit vollem schein", "tokens": ["die", "Bl\u00fc\u00b7the", "die\u00b7ser", "zeit", "/", "wo", "Sie", "mit", "vol\u00b7lem", "schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sich niederlie\u00df zur ruh/ und g\u00fcldne Kr\u00e4ntze windet:", "tokens": ["sich", "nie\u00b7der\u00b7lie\u00df", "zur", "ruh", "/", "und", "g\u00fcld\u00b7ne", "Kr\u00e4nt\u00b7ze", "win\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr Lichter in der Lufft/ ihr Himmels-\u00e4ugelein", "tokens": ["Ihr", "Lich\u00b7ter", "in", "der", "Lufft", "/", "ihr", "Him\u00b7mels\u00b7\u00e4u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie da\u00df ihr euch dann so je mehr und mehr entz\u00fcndet/", "tokens": ["wie", "da\u00df", "ihr", "euch", "dann", "so", "je", "mehr", "und", "mehr", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und brennet in der Luft? wie da\u00df jhr nicht verbleicht/", "tokens": ["und", "bren\u00b7net", "in", "der", "Luft", "?", "wie", "da\u00df", "jhr", "nicht", "ver\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "KOKOM", "KOUS", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "vor diesem glantz und licht/ dem selbst die Sonne", "tokens": ["vor", "die\u00b7sem", "glantz", "und", "licht", "/", "dem", "selbst", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$(", "ART", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "weicht?", "tokens": ["weicht", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}