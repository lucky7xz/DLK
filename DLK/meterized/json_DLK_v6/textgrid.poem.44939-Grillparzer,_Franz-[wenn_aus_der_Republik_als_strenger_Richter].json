{"textgrid.poem.44939": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn aus der Republik als strenger Richter]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus der Republik als strenger Richter", "tokens": ["Wenn", "aus", "der", "Re\u00b7pub\u00b7lik", "als", "stren\u00b7ger", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Dichter Plato bannt, der selbst ein Dichter,", "tokens": ["Die", "Dich\u00b7ter", "Pla\u00b7to", "bannt", ",", "der", "selbst", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nehmt das nicht h\u00f6her auf, als es gemeint,", "tokens": ["Nehmt", "das", "nicht", "h\u00f6\u00b7her", "auf", ",", "als", "es", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer war je sein und seinesgleichen Feind?", "tokens": ["Wer", "war", "je", "sein", "und", "sei\u00b7nes\u00b7glei\u00b7chen", "Feind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VAINF", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch was sich liebt, das neckt sich gern,", "tokens": ["Doch", "was", "sich", "liebt", ",", "das", "neckt", "sich", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "$,", "PDS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Muse will den Freund und nicht den Herrn,", "tokens": ["Die", "Mu\u00b7se", "will", "den", "Freund", "und", "nicht", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie wir launisch oft uns selbst betr\u00fcbten,", "tokens": ["Und", "wie", "wir", "lau\u00b7nisch", "oft", "uns", "selbst", "be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zankt man als zweitem Ich mit der Geliebten.", "tokens": ["Zankt", "man", "als", "zwei\u00b7tem", "Ich", "mit", "der", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJA", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}