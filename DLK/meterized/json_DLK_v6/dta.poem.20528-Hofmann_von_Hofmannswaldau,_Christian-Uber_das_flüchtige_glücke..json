{"dta.poem.20528": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Uber das fl\u00fcchtige gl\u00fccke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze fleuch das gl\u00fccke/", "tokens": ["Mein", "hert\u00b7ze", "fleuch", "das", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und la\u00df sein licht nicht deinen leit-stern seyn.", "tokens": ["Und", "la\u00df", "sein", "licht", "nicht", "dei\u00b7nen", "leit\u00b7stern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein englisch auge f\u00fchrt offt geist und drachen-blicke/", "tokens": ["Ein", "eng\u00b7lisch", "au\u00b7ge", "f\u00fchrt", "offt", "geist", "und", "dra\u00b7chen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der himmel selber mischt in sonne regen ein.", "tokens": ["Der", "him\u00b7mel", "sel\u00b7ber", "mischt", "in", "son\u00b7ne", "re\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kan sein angesicht auch lachen und doch plitzen/", "tokens": ["So", "kan", "sein", "an\u00b7ge\u00b7sicht", "auch", "la\u00b7chen", "und", "doch", "plit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und h\u00fcllt in rosen-pracht die sch\u00e4rffste dornen-spitzen.", "tokens": ["Und", "h\u00fcllt", "in", "ro\u00b7sen\u00b7pracht", "die", "sch\u00e4rffs\u00b7te", "dor\u00b7nen\u00b7spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die aller\u00e4rgste sclaven", "tokens": ["Die", "al\u00b7ler\u00b7\u00e4rgs\u00b7te", "scla\u00b7ven"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirfft offt ein sturm an sichres ufer an:", "tokens": ["Wirfft", "offt", "ein", "sturm", "an", "sich\u00b7res", "u\u00b7fer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das gl\u00fccke zeiget nur der hoffnung s\u00fcssen hafen;", "tokens": ["Das", "gl\u00fc\u00b7cke", "zei\u00b7get", "nur", "der", "hoff\u00b7nung", "s\u00fcs\u00b7sen", "ha\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fleucht aber/ wenn ihr fu\u00df am besten anckern kan.", "tokens": ["Fleucht", "a\u00b7ber", "/", "wenn", "ihr", "fu\u00df", "am", "bes\u00b7ten", "an\u00b7ckern", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KOUS", "PPER", "PTKVZ", "PTKA", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn lust und freude sind wie bunte regen-bogen/", "tokens": ["Denn", "lust", "und", "freu\u00b7de", "sind", "wie", "bun\u00b7te", "re\u00b7gen\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "NN", "VAFIN", "KOKOM", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die/ eh man sie erkennt/ schon wieder abgezogen.", "tokens": ["Die", "/", "eh", "man", "sie", "er\u00b7kennt", "/", "schon", "wie\u00b7der", "ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PIS", "PPER", "VVFIN", "$(", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der zucker unsers lebens", "tokens": ["Der", "zu\u00b7cker", "un\u00b7sers", "le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nur ein schaum/ der gall und wermuth deckt.", "tokens": ["Ist", "nur", "ein", "schaum", "/", "der", "gall", "und", "wer\u00b7muth", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vernunfft und klugheit sucht das gl\u00fccke selbst vergebens;", "tokens": ["Ver\u00b7nunfft", "und", "klug\u00b7heit", "sucht", "das", "gl\u00fc\u00b7cke", "selbst", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil schlang und natter auch in paradiesen steckt.", "tokens": ["Weil", "schlang", "und", "nat\u00b7ter", "auch", "in", "pa\u00b7ra\u00b7die\u00b7sen", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJA", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die gr\u00f6ste klugheit ist der zeiten grimm verlachen/", "tokens": ["Die", "gr\u00f6s\u00b7te", "klug\u00b7heit", "ist", "der", "zei\u00b7ten", "grimm", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wie ein bienen-wurm aus schierling honig machen.", "tokens": ["Und", "wie", "ein", "bie\u00b7nen\u00b7wurm", "aus", "schier\u00b7ling", "ho\u00b7nig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In saltz und thr\u00e4nen baden", "tokens": ["In", "saltz", "und", "thr\u00e4\u00b7nen", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist sichrer/ als auff sammt und purpur gehn.", "tokens": ["Ist", "sich\u00b7rer", "/", "als", "auff", "sammt", "und", "pur\u00b7pur", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOKOM", "APPR", "VVFIN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wenn die plitze gleich den ceder-\u00e4sten schaden/", "tokens": ["Denn", "wenn", "die", "plit\u00b7ze", "gleich", "den", "ce\u00b7der\u00b7\u00e4s\u00b7ten", "scha\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So l\u00e4st ihr donner doch geringe pappeln stehn.", "tokens": ["So", "l\u00e4st", "ihr", "don\u00b7ner", "doch", "ge\u00b7rin\u00b7ge", "pap\u00b7peln", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So f\u00e4llt ein reicher auch offt schimpfflich zu der erden/", "tokens": ["So", "f\u00e4llt", "ein", "rei\u00b7cher", "auch", "offt", "schimpf\u00b7flich", "zu", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn arm und niedrige zu grossen herren werden.", "tokens": ["Wenn", "arm", "und", "nied\u00b7ri\u00b7ge", "zu", "gros\u00b7sen", "her\u00b7ren", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "PIS", "PTKZU", "VVINF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was gl\u00fcck und gunst gebohren/", "tokens": ["Was", "gl\u00fcck", "und", "gunst", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schmeltzt mit der zeit wie schnee und kaltes ei\u00df.", "tokens": ["Sch\u00b7meltzt", "mit", "der", "zeit", "wie", "schnee", "und", "kal\u00b7tes", "ei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "KON", "ADJA", "NN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der aber hat noch nicht der freuden port verlohren/", "tokens": ["Der", "a\u00b7ber", "hat", "noch", "nicht", "der", "freu\u00b7den", "port", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nur den trauer-wind recht zu gebrauchen wei\u00df.", "tokens": ["Der", "nur", "den", "trau\u00b7e\u00b7rwind", "recht", "zu", "ge\u00b7brau\u00b7chen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn gl\u00fcck und ehre sind nicht kinder einer stunden/", "tokens": ["Denn", "gl\u00fcck", "und", "eh\u00b7re", "sind", "nicht", "kin\u00b7der", "ei\u00b7ner", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "VVFIN", "VAFIN", "PTKNEG", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und werden nur wie gold durch m\u00fch und schwei\u00df gefunden.", "tokens": ["Und", "wer\u00b7den", "nur", "wie", "gold", "durch", "m\u00fch", "und", "schwei\u00df", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "KOKOM", "NN", "APPR", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum fleuch das falsche gl\u00fccke/", "tokens": ["Drum", "fleuch", "das", "fal\u00b7sche", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und trau/ mein hertz/ auff seine sonnen nicht;", "tokens": ["Und", "trau", "/", "mein", "hertz", "/", "auff", "sei\u00b7ne", "son\u00b7nen", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zeuch der begierden fu\u00df von dieser spiegel-br\u00fccke/", "tokens": ["Zeuch", "der", "be\u00b7gier\u00b7den", "fu\u00df", "von", "die\u00b7ser", "spie\u00b7gel\u00b7br\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PTKVZ", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da gold und pfeiler so wie Porcellan zerbricht.", "tokens": ["Da", "gold", "und", "pfei\u00b7ler", "so", "wie", "Por\u00b7cel\u00b7lan", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "KOKOM", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht kan schmertz und leid/ die deinen geist noch binden/", "tokens": ["Viel\u00b7leicht", "kan", "schmertz", "und", "leid", "/", "die", "dei\u00b7nen", "geist", "noch", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "KON", "ADJD", "$(", "ART", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bald deiner ehren band auff festen marmol gr\u00fcnden.", "tokens": ["Bald", "dei\u00b7ner", "eh\u00b7ren", "band", "auff", "fes\u00b7ten", "mar\u00b7mol", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}