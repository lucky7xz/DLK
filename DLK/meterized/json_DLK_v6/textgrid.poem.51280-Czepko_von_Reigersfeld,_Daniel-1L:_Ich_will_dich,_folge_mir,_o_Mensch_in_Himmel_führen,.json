{"textgrid.poem.51280": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will dich, folge mir, o Mensch in Himmel f\u00fchren,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will dich, folge mir, o Mensch in Himmel f\u00fchren,", "tokens": ["Ich", "will", "dich", ",", "fol\u00b7ge", "mir", ",", "o", "Mensch", "in", "Him\u00b7mel", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkenne dich nur selbst, Gott wird dein Hertze r\u00fchren,", "tokens": ["Er\u00b7ken\u00b7ne", "dich", "nur", "selbst", ",", "Gott", "wird", "dein", "Hert\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "VAFIN", "PPOSAT", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist das, was du bist: die Ewig' Ewigkeit", "tokens": ["Du", "bist", "das", ",", "was", "du", "bist", ":", "die", "E\u00b7wig'", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird inner dir geeint, wird au\u00dfer dir gezweyt.", "tokens": ["Wird", "in\u00b7ner", "dir", "ge\u00b7eint", ",", "wird", "au\u00b7\u00dfer", "dir", "ge\u00b7zweyt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$,", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann sich die Seel erkennt. Du bist dir dein selbst Wesen,", "tokens": ["Wann", "sich", "die", "Seel", "er\u00b7kennt", ".", "Du", "bist", "dir", "dein", "selbst", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele hat ihr Gott, Gott ihm die Seel erlesen:", "tokens": ["Die", "See\u00b7le", "hat", "ihr", "Gott", ",", "Gott", "ihm", "die", "Seel", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie, die nu ewig ist, schleust sich in C\u00f6rper ein,", "tokens": ["Sie", ",", "die", "nu", "e\u00b7wig", "ist", ",", "schleust", "sich", "in", "C\u00f6r\u00b7per", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wil in ihm offenbar, erkannt und sichtbar seyn.", "tokens": ["Wil", "in", "ihm", "of\u00b7fen\u00b7bar", ",", "er\u00b7kannt", "und", "sicht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADJD", "$,", "VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Durch Leben kam der Leib: das Leben durch die Sinnen,", "tokens": ["Durch", "Le\u00b7ben", "kam", "der", "Leib", ":", "das", "Le\u00b7ben", "durch", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sinnen durch Verstand: durch Glauben er gewinnen:", "tokens": ["Die", "Sin\u00b7nen", "durch", "Ver\u00b7stand", ":", "durch", "Glau\u00b7ben", "er", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele, die beschaut sich in ihm, er in ihr,", "tokens": ["Die", "See\u00b7le", ",", "die", "be\u00b7schaut", "sich", "in", "ihm", ",", "er", "in", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "APPR", "PPER", "$,", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie in durchlauchtem Glantz, Er in verkl\u00e4rter Zier.", "tokens": ["Sie", "in", "durch\u00b7lauch\u00b7tem", "Glantz", ",", "Er", "in", "ver\u00b7kl\u00e4r\u00b7ter", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann sich der Leib erkennt. Du darffst nicht weiter fragen,", "tokens": ["Wann", "sich", "der", "Leib", "er\u00b7kennt", ".", "Du", "darffst", "nicht", "wei\u00b7ter", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses frage dich, sonst kan dirs niemand sagen.", "tokens": ["Und", "die\u00b7ses", "fra\u00b7ge", "dich", ",", "sonst", "kan", "dirs", "nie\u00b7mand", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$,", "ADV", "VMFIN", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}