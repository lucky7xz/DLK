{"textgrid.poem.41751": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist wohl eine Freudenthr\u00e4n'", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist wohl eine Freudenthr\u00e4n'", "tokens": ["Es", "ist", "wohl", "ei\u00b7ne", "Freu\u00b7den\u00b7thr\u00e4n'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir in das Aug getreten", "tokens": ["Mir", "in", "das", "Aug", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ich die Gegend hier gesehn,", "tokens": ["Als", "ich", "die", "Ge\u00b7gend", "hier", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wortlos stilles Beten", "tokens": ["Ein", "wort\u00b7los", "stil\u00b7les", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier, wo die Berge rings herum", "tokens": ["Hier", ",", "wo", "die", "Ber\u00b7ge", "rings", "he\u00b7rum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich heben wie Alt\u00e4re", "tokens": ["Sich", "he\u00b7ben", "wie", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVINF", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So feierlich, so ernst und stumm", "tokens": ["So", "fei\u00b7er\u00b7lich", ",", "so", "ernst", "und", "stumm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stark zu Gottes Ehre.", "tokens": ["So", "stark", "zu", "Got\u00b7tes", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es tr\u00e4gt das Haupt der Koppe Schnee,", "tokens": ["Es", "tr\u00e4gt", "das", "Haupt", "der", "Kop\u00b7pe", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell schimmert die Kapelle", "tokens": ["Hell", "schim\u00b7mert", "die", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es springen von der Berge H\u00f6h", "tokens": ["Es", "sprin\u00b7gen", "von", "der", "Ber\u00b7ge", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die muntern Wasserf\u00e4lle;", "tokens": ["Die", "mun\u00b7tern", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wiesen sind so frisch und gr\u00fcn,", "tokens": ["Die", "Wie\u00b7sen", "sind", "so", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sch\u00f6n die dichten W\u00e4lder", "tokens": ["So", "sch\u00f6n", "die", "dich\u00b7ten", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wunderbare Blumen bl\u00fchn,", "tokens": ["Und", "wun\u00b7der\u00b7ba\u00b7re", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hoch stehn die Saatenfelder.", "tokens": ["Hoch", "stehn", "die", "Saa\u00b7ten\u00b7fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist ich sei im Paradies", "tokens": ["Mir", "ist", "ich", "sei", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich so ringsum schaue!", "tokens": ["Wenn", "ich", "so", "ring\u00b7sum", "schau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hingesunken tr\u00e4um ich s\u00fc\u00df", "tokens": ["Und", "hin\u00b7ge\u00b7sun\u00b7ken", "tr\u00e4um", "ich", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIZU", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dufterf\u00fcllter Aue.", "tokens": ["Auf", "duf\u00b7ter\u00b7f\u00fcll\u00b7ter", "Au\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So traut, so heimlich ist's im Thal,", "tokens": ["So", "traut", ",", "so", "heim\u00b7lich", "ist's", "im", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und von den Bergen droben", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Klingts wie ein Gru\u00df von R\u00fcbezahl,", "tokens": ["Klingts", "wie", "ein", "Gru\u00df", "von", "R\u00fc\u00b7be\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der seine Stimme erhoben.", "tokens": ["Der", "sei\u00b7ne", "Stim\u00b7me", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch weiter setz ich meinen Fu\u00df,", "tokens": ["Doch", "wei\u00b7ter", "setz", "ich", "mei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hin wo die Menschen wohnen", "tokens": ["Hin", "wo", "die", "Men\u00b7schen", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PWAV", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich biete ihnen frohen Gru\u00df", "tokens": ["Ich", "bie\u00b7te", "ih\u00b7nen", "fro\u00b7hen", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie: \u00bbMag's Gott Euch lohnen!\u00ab", "tokens": ["Und", "sie", ":", "\u00bb", "Mag's", "Gott", "Euch", "loh\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$.", "$(", "NE", "NN", "PPER", "VVINF", "$.", "$("], "meter": "--++-+-", "measure": "anapaest.init"}, "line.5": {"text": "Das klingt so traurig, schmerzensreich,", "tokens": ["Das", "klingt", "so", "trau\u00b7rig", ",", "schmer\u00b7zens\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was blickt ihr so zur Erde?", "tokens": ["Was", "blickt", "ihr", "so", "zur", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Helf Gott! Du Weib \u2013 wie bist Du bleich,", "tokens": ["Helf", "Gott", "!", "Du", "Weib", "\u2013", "wie", "bist", "Du", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "NN", "$(", "KOKOM", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie schmerzlich von Geberde?", "tokens": ["Wie", "schmerz\u00b7lich", "von", "Ge\u00b7ber\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "$."], "meter": "-+-----", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "In Deine H\u00fctte la\u00df mich sehn \u2013", "tokens": ["In", "Dei\u00b7ne", "H\u00fct\u00b7te", "la\u00df", "mich", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da drinn am Webestuhle,", "tokens": ["Da", "drinn", "am", "We\u00b7be\u00b7stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gestalten voller Jammer stehn", "tokens": ["Ge\u00b7stal\u00b7ten", "vol\u00b7ler", "Jam\u00b7mer", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klappern mit der Spule.", "tokens": ["Und", "klap\u00b7pern", "mit", "der", "Spu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kinder schreien laut nach Brot,", "tokens": ["Die", "Kin\u00b7der", "schrei\u00b7en", "laut", "nach", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die blinde Alte singet", "tokens": ["Die", "blin\u00b7de", "Al\u00b7te", "sin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein d\u00fcstres Lied vom Freunde Tod,", "tokens": ["Ein", "d\u00fcst\u00b7res", "Lied", "vom", "Freun\u00b7de", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der einst Erl\u00f6sung bringet.", "tokens": ["Der", "einst", "Er\u00b7l\u00f6\u00b7sung", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist wohl eine Schmerzensthr\u00e4n',", "tokens": ["Es", "ist", "wohl", "ei\u00b7ne", "Schmer\u00b7zens\u00b7thr\u00e4n'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir in das Aug getreten", "tokens": ["Mir", "in", "das", "Aug", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ich die Menschen hier gesehn,", "tokens": ["Als", "ich", "die", "Men\u00b7schen", "hier", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wortlos stilles Beten,", "tokens": ["Ein", "wort\u00b7los", "stil\u00b7les", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis einen Schrei hervor ich stie\u00df. \u2013", "tokens": ["Bis", "ei\u00b7nen", "Schrei", "her\u00b7vor", "ich", "stie\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O h\u00f6rt ihn nicht vergebens! \u2013", "tokens": ["O", "h\u00f6rt", "ihn", "nicht", "ver\u00b7ge\u00b7bens", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Schlange ist im Paradies", "tokens": ["Die", "Schlan\u00b7ge", "ist", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fri\u00dft vom Baum des Lebens!", "tokens": ["Und", "fri\u00dft", "vom", "Baum", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}