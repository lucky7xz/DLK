{"dta.poem.3578": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O du mein Mopper, wo willt du hinaus,               ", "tokens": ["O", "du", "mein", "Mop\u00b7per", ",", "wo", "willt", "du", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich kann dir nicht erz\u00e4hlen", "tokens": ["Ich", "kann", "dir", "nicht", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Meine g\u00fcldene Klaus:", "tokens": ["Mei\u00b7ne", "g\u00fcl\u00b7de\u00b7ne", "Klaus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "La\u00df klinken, la\u00df klanken,", "tokens": ["La\u00df", "klin\u00b7ken", ",", "la\u00df", "klan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "La\u00df all herunter schwanken;", "tokens": ["La\u00df", "all", "her\u00b7un\u00b7ter", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df nicht, soll ich h\u00fcten", "tokens": ["Ich", "wei\u00df", "nicht", ",", "soll", "ich", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ochs oder Schaf,", "tokens": ["Ochs", "o\u00b7der", "Schaf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Oder soll ich essen", "tokens": ["O\u00b7der", "soll", "ich", "es\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Einen K\u00e4s und ein Brod.", "tokens": ["Ei\u00b7nen", "K\u00e4s", "und", "ein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Bei Ochsen und bei Schafen", "tokens": ["Bei", "Och\u00b7sen", "und", "bei", "Scha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann man nicht schlafen,", "tokens": ["Kann", "man", "nicht", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da thut es sich er\u00f6ffnen", "tokens": ["Da", "thut", "es", "sich", "er\u00b7\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das himmlische Thor,", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Thor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da kugeln die Engel", "tokens": ["Da", "ku\u00b7geln", "die", "En\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ganz haufenweis hervor.", "tokens": ["Ganz", "hau\u00b7fen\u00b7weis", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}