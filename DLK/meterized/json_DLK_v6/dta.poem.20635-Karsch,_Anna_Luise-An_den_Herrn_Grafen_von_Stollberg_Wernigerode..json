{"dta.poem.20635": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Herrn Grafen  \n von Stollberg Wernigerode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es rinnen dahin die fl\u00fcchtigen Jahre,                 ", "tokens": ["Es", "rin\u00b7nen", "da\u00b7hin", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fr\u00f6mmigkeit selber verz\u00f6gert, o Freund!", "tokens": ["Die", "Fr\u00f6m\u00b7mig\u00b7keit", "sel\u00b7ber", "ver\u00b7z\u00f6\u00b7gert", ",", "o", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "FM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nicht Runzeln des Alters, und silberne Haare,", "tokens": ["Nicht", "Run\u00b7zeln", "des", "Al\u00b7ters", ",", "und", "sil\u00b7ber\u00b7ne", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und jenen noch nimmer gez\u00e4hmeten Feind.", "tokens": ["Und", "je\u00b7nen", "noch", "nim\u00b7mer", "ge\u00b7z\u00e4h\u00b7me\u00b7ten", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Gelobtest du gleich zehntausend Ges\u00e4nge", "tokens": ["Ge\u00b7lob\u00b7test", "du", "gleich", "zehn\u00b7tau\u00b7send", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "CARD", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf heiliger Harfe; doch w\u00fcrde dein Spiel", "tokens": ["Auf", "hei\u00b7li\u00b7ger", "Har\u00b7fe", ";", "doch", "w\u00fcr\u00b7de", "dein", "Spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Den Pluto nicht r\u00fchren, dem niemals die Menge", "tokens": ["Den", "Plu\u00b7to", "nicht", "r\u00fch\u00b7ren", ",", "dem", "nie\u00b7mals", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hei\u00dfrollender Thr\u00e4nen zum Opfer gefiel.", "tokens": ["Hei\u00df\u00b7rol\u00b7len\u00b7der", "Thr\u00e4\u00b7nen", "zum", "Op\u00b7fer", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Der seinen Bezirk mit Wogen umschlossen,", "tokens": ["Der", "sei\u00b7nen", "Be\u00b7zirk", "mit", "Wo\u00b7gen", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die jeder beschiffen mu\u00df, welcher die Luft", "tokens": ["Die", "je\u00b7der", "be\u00b7schif\u00b7fen", "mu\u00df", ",", "wel\u00b7cher", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Getrunken, und Fr\u00fcchte der Erde genossen,", "tokens": ["Ge\u00b7trun\u00b7ken", ",", "und", "Fr\u00fcch\u00b7te", "der", "Er\u00b7de", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von der ihn das eiserne Schicksal entruft.", "tokens": ["Von", "der", "ihn", "das", "ei\u00b7ser\u00b7ne", "Schick\u00b7sal", "ent\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Es mag ihm ein Land gehorchen und dienen,", "tokens": ["Es", "mag", "ihm", "ein", "Land", "ge\u00b7hor\u00b7chen", "und", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es beten bezwungene V\u00f6lker ihn an;", "tokens": ["Es", "be\u00b7ten", "be\u00b7zwun\u00b7ge\u00b7ne", "V\u00f6l\u00b7ker", "ihn", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihm m\u00f6gen auch wenige Furchen nur gr\u00fcnen,", "tokens": ["Ihm", "m\u00f6\u00b7gen", "auch", "we\u00b7ni\u00b7ge", "Fur\u00b7chen", "nur", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er sey gleich ein armer, m\u00fchseeliger Mann.", "tokens": ["Er", "sey", "gleich", "ein", "ar\u00b7mer", ",", "m\u00fch\u00b7see\u00b7li\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir scheuen umsonst in herbstlichen Tagen", "tokens": ["Wir", "scheu\u00b7en", "um\u00b7sonst", "in", "herbst\u00b7li\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sch\u00e4dlichen Winde; vergebens entgehn", "tokens": ["Die", "sch\u00e4d\u00b7li\u00b7chen", "Win\u00b7de", ";", "ver\u00b7ge\u00b7bens", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir tobenden Wellen, und schrecklichem Schlagen,", "tokens": ["Wir", "to\u00b7ben\u00b7den", "Wel\u00b7len", ",", "und", "schreck\u00b7li\u00b7chem", "Schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Des blutigen Mavors, wo Furien stehn.", "tokens": ["Des", "blu\u00b7ti\u00b7gen", "Ma\u00b7vors", ",", "wo", "Fu\u00b7ri\u00b7en", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir m\u00fcssen Welt, Haus und Freunde verlassen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "Welt", ",", "Haus", "und", "Freun\u00b7de", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bl\u00fchender G\u00e4rten balsamischen Duft;", "tokens": ["Und", "bl\u00fc\u00b7hen\u00b7der", "G\u00e4r\u00b7ten", "bal\u00b7sa\u00b7mi\u00b7schen", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die falbe Cypresse folgt einzig dem blassen", "tokens": ["Die", "fal\u00b7be", "Cyp\u00b7res\u00b7se", "folgt", "ein\u00b7zig", "dem", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu kurzen Gebieter in traurige Gruft.", "tokens": ["Zu", "kur\u00b7zen", "Ge\u00b7bie\u00b7ter", "in", "trau\u00b7ri\u00b7ge", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Erbe holt den durch zehnmal zehn Schl\u00f6sser", "tokens": ["Sein", "Er\u00b7be", "holt", "den", "durch", "zehn\u00b7mal", "zehn", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "APPR", "ADV", "CARD", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verwahrten Tokayer; dann str\u00f6met der Saal", "tokens": ["Ver\u00b7wahr\u00b7ten", "To\u00b7ka\u00b7yer", ";", "dann", "str\u00f6\u00b7met", "der", "Saal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Begossen vom Weine, den selber nicht besser", "tokens": ["Be\u00b7gos\u00b7sen", "vom", "Wei\u00b7ne", ",", "den", "sel\u00b7ber", "nicht", "bes\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Pontifex schmauset bei festlichem Mahl.", "tokens": ["Der", "Pon\u00b7ti\u00b7fex", "schmau\u00b7set", "bei", "fest\u00b7li\u00b7chem", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}}}}