{"dta.poem.20853": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Schau ich der Rosen Purpurschein", "tokens": ["Schau", "ich", "der", "Ro\u00b7sen", "Pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In einem bunten Garten schweben/", "tokens": ["In", "ei\u00b7nem", "bun\u00b7ten", "Gar\u00b7ten", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kommt mir meine Phyllis ein/", "tokens": ["So", "kommt", "mir", "mei\u00b7ne", "Phyl\u00b7lis", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey der sie auf den Wangen leben.", "tokens": ["Bey", "der", "sie", "auf", "den", "Wan\u00b7gen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Diana/ so im Himmel sitzt/", "tokens": ["Di\u00b7a\u00b7na", "/", "so", "im", "Him\u00b7mel", "sitzt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit viel Lust und Sch\u00f6nheit pranget/", "tokens": ["Und", "mit", "viel", "Lust", "und", "Sch\u00f6n\u00b7heit", "pran\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Macht da\u00df sich Brust und Brust erhitzt/", "tokens": ["Macht", "da\u00df", "sich", "Brust", "und", "Brust", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PRF", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und mich nach gleicher Pracht verlanget.", "tokens": ["Und", "mich", "nach", "glei\u00b7cher", "Pracht", "ver\u00b7lan\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gr\u00fcst Ph\u00f6bus diese runde Welt/", "tokens": ["Gr\u00fcst", "Ph\u00f6\u00b7bus", "die\u00b7se", "run\u00b7de", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schau ich sie in gleichem Feuer/", "tokens": ["So", "schau", "ich", "sie", "in", "glei\u00b7chem", "Feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil alles f\u00fcr ihr nieder f\u00e4lt/", "tokens": ["Weil", "al\u00b7les", "f\u00fcr", "ihr", "nie\u00b7der", "f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als einem reinen Ungeheuer.", "tokens": ["Als", "ei\u00b7nem", "rei\u00b7nen", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Venus/ so Apelles macht/", "tokens": ["Die", "Ve\u00b7nus", "/", "so", "A\u00b7pel\u00b7les", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleine Gott und seine Kertze/", "tokens": ["Der", "klei\u00b7ne", "Gott", "und", "sei\u00b7ne", "Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und andre Farben Zier und Pracht", "tokens": ["Und", "and\u00b7re", "Far\u00b7ben", "Zier", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchrt mir die Phillis in das Hertze.", "tokens": ["F\u00fchrt", "mir", "die", "Phil\u00b7lis", "in", "das", "Hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}