{"textgrid.poem.39866": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dicht im Gestr\u00fcppe schleich' ich still und scheu,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht im Gestr\u00fcppe schleich' ich still und scheu,", "tokens": ["Dicht", "im", "Ge\u00b7str\u00fcp\u00b7pe", "schleich'", "ich", "still", "und", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da kommt ein Gast, einsam auch er, gegangen,", "tokens": ["Da", "kommt", "ein", "Gast", ",", "ein\u00b7sam", "auch", "er", ",", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "ADV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den frag' ich nach der Frau mit bleichen Wangen.", "tokens": ["Den", "frag'", "ich", "nach", "der", "Frau", "mit", "blei\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er spricht verwundert: \u00bbIst ihr Loos Euch neu?", "tokens": ["Er", "spricht", "ver\u00b7wun\u00b7dert", ":", "\u00bb", "Ist", "ihr", "Loos", "Euch", "neu", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$.", "$(", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr starb der Gatte treulos, doch in Reu',", "tokens": ["Ihr", "starb", "der", "Gat\u00b7te", "treu\u00b7los", ",", "doch", "in", "Reu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und eng umstrickt von des Gewissens Schlangen;", "tokens": ["Und", "eng", "um\u00b7strickt", "von", "des", "Ge\u00b7wis\u00b7sens", "Schlan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie aber h\u00e4lt sein h\u00fclflos Kind umfangen,", "tokens": ["Sie", "a\u00b7ber", "h\u00e4lt", "sein", "h\u00fcl\u00b7flos", "Kind", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und an der Untreu' Frucht \u00fcbt sie die Treu'.\u00ab", "tokens": ["Und", "an", "der", "Un\u00b7treu'", "Frucht", "\u00fcbt", "sie", "die", "Treu'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht Antwort fand ich auf des Fremden Worte,", "tokens": ["Nicht", "Ant\u00b7wort", "fand", "ich", "auf", "des", "Frem\u00b7den", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Ohre l\u00e4utete das Lied der Jugend,", "tokens": ["Im", "Oh\u00b7re", "l\u00e4u\u00b7te\u00b7te", "das", "Lied", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es r\u00fcttelte mir im bet\u00e4ubten Sinn.", "tokens": ["Es", "r\u00fct\u00b7tel\u00b7te", "mir", "im", "be\u00b7t\u00e4ub\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fort, nach des Fichtenwalds geheimstem Orte,", "tokens": ["Fort", ",", "nach", "des", "Fich\u00b7ten\u00b7walds", "ge\u00b7heims\u00b7tem", "Or\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dort zu versenken mich in ihre Tugend.", "tokens": ["Dort", "zu", "ver\u00b7sen\u00b7ken", "mich", "in", "ih\u00b7re", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie pflegt das Kind \u2013 das Kind der Buhlerin!", "tokens": ["Sie", "pflegt", "das", "Kind", "\u2013", "das", "Kind", "der", "Buh\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}