{"textgrid.poem.65239": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Weisheit im Wein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr werden weise Leute,", "tokens": ["Wollt", "ihr", "wer\u00b7den", "wei\u00b7se", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe Br\u00fcder, macht es heute,", "tokens": ["Lie\u00b7be", "Br\u00fc\u00b7der", ",", "macht", "es", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie es gestern ward gemacht.", "tokens": ["Wie", "es", "ge\u00b7stern", "ward", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Trinket Wein! Er weckt die Geister,", "tokens": ["Trin\u00b7ket", "Wein", "!", "Er", "weckt", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht die bl\u00f6den Zungen dreister", "tokens": ["Macht", "die", "bl\u00f6\u00b7den", "Zun\u00b7gen", "dreis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erhellet jede Nacht.", "tokens": ["Und", "er\u00b7hel\u00b7let", "je\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also haben es gehalten", "tokens": ["Al\u00b7so", "ha\u00b7ben", "es", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre lieben tapfern Alten,", "tokens": ["Uns\u00b7re", "lie\u00b7ben", "tap\u00b7fern", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie beriethen sich beim Glas,", "tokens": ["Sie", "be\u00b7rie\u00b7then", "sich", "beim", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die neuen Diplomaten", "tokens": ["Und", "die", "neu\u00b7en", "Dip\u00b7lo\u00b7ma\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Halten auch auf Wein und Braten", "tokens": ["Hal\u00b7ten", "auch", "auf", "Wein", "und", "Bra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und hernach auf dies und das.", "tokens": ["Und", "her\u00b7nach", "auf", "dies", "und", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDS", "KON", "PDS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Glaubt ihr, ohne gute Weine", "tokens": ["Glaubt", "ihr", ",", "oh\u00b7ne", "gu\u00b7te", "Wei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4me Deutschland auf die Beine?", "tokens": ["K\u00e4\u00b7me", "Deutschland", "auf", "die", "Bei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Liebe Br\u00fcder, glaubt es nicht!", "tokens": ["Lie\u00b7be", "Br\u00fc\u00b7der", ",", "glaubt", "es", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frankfurt zapft die allerbesten", "tokens": ["Frank\u00b7furt", "zapft", "die", "al\u00b7ler\u00b7bes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen hohen weisen G\u00e4sten,", "tokens": ["Sei\u00b7nen", "ho\u00b7hen", "wei\u00b7sen", "G\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie ehren ihre Pflicht.", "tokens": ["Und", "sie", "eh\u00b7ren", "ih\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freuet euch! An Gottes Segen", "tokens": ["Freu\u00b7et", "euch", "!", "An", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "APPR", "NN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ist das Meiste doch gelegen,", "tokens": ["Ist", "das", "Meis\u00b7te", "doch", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er segnet Main und Rhein.", "tokens": ["Und", "er", "seg\u00b7net", "Main", "und", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht bei Wassern oder Bieren", "tokens": ["Nicht", "bei", "Was\u00b7sern", "o\u00b7der", "Bie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will man uns konstituiren,", "tokens": ["Will", "man", "uns", "kons\u00b7ti\u00b7tu\u00b7i\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und die Freiheit lebt im Wein.", "tokens": ["Und", "die", "Frei\u00b7heit", "lebt", "im", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deutsches Recht und deutsche Reben,", "tokens": ["Deut\u00b7sches", "Recht", "und", "deut\u00b7sche", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsches Licht und deutsches Leben,", "tokens": ["Deut\u00b7sches", "Licht", "und", "deut\u00b7sches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigt empor im deutschen Land!", "tokens": ["Steigt", "em\u00b7por", "im", "deut\u00b7schen", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig folgen wir dem Z\u00fcgel", "tokens": ["Freu\u00b7dig", "fol\u00b7gen", "wir", "dem", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen, der vom besten H\u00fcgel", "tokens": ["Des\u00b7sen", ",", "der", "vom", "bes\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erntet an des Rheines Strand.", "tokens": ["Ern\u00b7tet", "an", "des", "Rhei\u00b7nes", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}