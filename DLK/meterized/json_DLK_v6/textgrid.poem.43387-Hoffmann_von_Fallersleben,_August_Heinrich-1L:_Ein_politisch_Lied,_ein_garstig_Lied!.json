{"textgrid.poem.43387": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein politisch Lied, ein garstig Lied!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein politisch Lied, ein garstig Lied!", "tokens": ["Ein", "po\u00b7li\u00b7tisch", "Lied", ",", "ein", "gars\u00b7tig", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So dachten die Dichter mit G\u00f6then", "tokens": ["So", "dach\u00b7ten", "die", "Dich\u00b7ter", "mit", "G\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und glaubten, sie h\u00e4tten genug gethan,", "tokens": ["Und", "glaub\u00b7ten", ",", "sie", "h\u00e4t\u00b7ten", "ge\u00b7nug", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn sie k\u00f6nnten girren und fl\u00f6ten", "tokens": ["Wenn", "sie", "k\u00f6nn\u00b7ten", "gir\u00b7ren", "und", "fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "KON", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von Nachtigallen, von Lieb und Wein,", "tokens": ["Von", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "von", "Lieb", "und", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von blauen Bergesfernen,", "tokens": ["Von", "blau\u00b7en", "Ber\u00b7ges\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Rosenduft und Lilienschein,", "tokens": ["Von", "Ro\u00b7sen\u00b7duft", "und", "Li\u00b7li\u00b7en\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Sonne, Mond und Sternen.", "tokens": ["Von", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein politisch Lied, ein garstig Lied!", "tokens": ["Ein", "po\u00b7li\u00b7tisch", "Lied", ",", "ein", "gars\u00b7tig", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So dachten die Dichter mit G\u00f6then", "tokens": ["So", "dach\u00b7ten", "die", "Dich\u00b7ter", "mit", "G\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und glaubten, sie h\u00e4tten genug gethan,", "tokens": ["Und", "glaub\u00b7ten", ",", "sie", "h\u00e4t\u00b7ten", "ge\u00b7nug", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn sie k\u00f6nnten girren und Fl\u00f6ten \u2013", "tokens": ["Wenn", "sie", "k\u00f6nn\u00b7ten", "gir\u00b7ren", "und", "Fl\u00f6\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "KON", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch anders dachte das Vaterland:", "tokens": ["Doch", "an\u00b7ders", "dach\u00b7te", "das", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das will von der Dichterinnung", "tokens": ["Das", "will", "von", "der", "Dich\u00b7te\u00b7rin\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "F\u00fcr den verbrauchten Leiertand", "tokens": ["F\u00fcr", "den", "ver\u00b7brauch\u00b7ten", "Lei\u00b7er\u00b7tand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Muth und biedre Gesinnung.", "tokens": ["Nur", "Muth", "und", "bied\u00b7re", "Ge\u00b7sin\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich sang nach alter Sitt' und Brauch", "tokens": ["Ich", "sang", "nach", "al\u00b7ter", "Sitt'", "und", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Mond und Sternen und Sonne,", "tokens": ["Von", "Mond", "und", "Ster\u00b7nen", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Wein und Nachtigallen auch,", "tokens": ["Von", "Wein", "und", "Nach\u00b7ti\u00b7gal\u00b7len", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Liebeslust und Wonne.", "tokens": ["Von", "Lie\u00b7bes\u00b7lust", "und", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da rief mir zu das Vaterland:", "tokens": ["Da", "rief", "mir", "zu", "das", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sollst das Alte lassen,", "tokens": ["Du", "sollst", "das", "Al\u00b7te", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den alten verbrauchten Leiertand,", "tokens": ["Den", "al\u00b7ten", "ver\u00b7brauch\u00b7ten", "Lei\u00b7er\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du sollst die Zeit erfassen!", "tokens": ["Du", "sollst", "die", "Zeit", "er\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn anders geworden ist die Welt,", "tokens": ["Denn", "an\u00b7ders", "ge\u00b7wor\u00b7den", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es leben andere Leute;", "tokens": ["Es", "le\u00b7ben", "an\u00b7de\u00b7re", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was gestern noch stand, schon heute f\u00e4llt,", "tokens": ["Was", "ge\u00b7stern", "noch", "stand", ",", "schon", "heu\u00b7te", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was gestern nicht galt, gilt heute.", "tokens": ["Was", "ge\u00b7stern", "nicht", "galt", ",", "gilt", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer nicht die Kunst ", "tokens": ["Und", "wer", "nicht", "die", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wei\u00df ", "tokens": ["Wei\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der werde nun endlich bei Zeiten gescheit", "tokens": ["Der", "wer\u00b7de", "nun", "end\u00b7lich", "bei", "Zei\u00b7ten", "ge\u00b7scheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "APPR", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und lasse lieber das Dichten!", "tokens": ["Und", "las\u00b7se", "lie\u00b7ber", "das", "Dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}