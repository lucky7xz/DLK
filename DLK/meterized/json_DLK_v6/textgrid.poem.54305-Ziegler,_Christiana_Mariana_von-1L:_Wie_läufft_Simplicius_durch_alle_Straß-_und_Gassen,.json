{"textgrid.poem.54305": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie l\u00e4ufft Simplicius durch alle Stra\u00df- und Gassen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie l\u00e4ufft Simplicius durch alle Stra\u00df- und Gassen,", "tokens": ["Wie", "l\u00e4ufft", "Sim\u00b7pli\u00b7cius", "durch", "al\u00b7le", "Stra\u00df", "und", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "APPR", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Der Hahn kan warlich nicht so auf die H\u00fchner passen.", "tokens": ["Der", "Hahn", "kan", "war\u00b7lich", "nicht", "so", "auf", "die", "H\u00fch\u00b7ner", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht! wie der arme Tropff nach seiner G\u00f6ttin rennt,", "tokens": ["Seht", "!", "wie", "der", "ar\u00b7me", "Tropff", "nach", "sei\u00b7ner", "G\u00f6t\u00b7tin", "rennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und vor Begierde recht wie warmer Gr\u00fctze brennt.", "tokens": ["Und", "vor", "Be\u00b7gier\u00b7de", "recht", "wie", "war\u00b7mer", "Gr\u00fct\u00b7ze", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geschicht es, da\u00df er nicht sein M\u00e4gdgen kan erblicken,", "tokens": ["Ge\u00b7schicht", "es", ",", "da\u00df", "er", "nicht", "sein", "M\u00e4gd\u00b7gen", "kan", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So heist die Sehnsucht ihn nach ihr viel Seuffzer schicken,", "tokens": ["So", "heist", "die", "Sehn\u00b7sucht", "ihn", "nach", "ihr", "viel", "Seuff\u00b7zer", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein halb verwelcktes Hertz bricht in die Klagen aus:", "tokens": ["Sein", "halb", "ver\u00b7welck\u00b7tes", "Hertz", "bricht", "in", "die", "Kla\u00b7gen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach Engel, sieh doch nur zu deinen Fenster raus!", "tokens": ["Ach", "En\u00b7gel", ",", "sieh", "doch", "nur", "zu", "dei\u00b7nen", "Fens\u00b7ter", "raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVIMP", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er suchet ihrer Magd beweglich zu erzehlen,", "tokens": ["Er", "su\u00b7chet", "ih\u00b7rer", "Magd", "be\u00b7weg\u00b7lich", "zu", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sich sein matter Geist mu\u00df um Rosildgen qu\u00e4len,", "tokens": ["Wie", "sich", "sein", "mat\u00b7ter", "Geist", "mu\u00df", "um", "Ro\u00b7sild\u00b7gen", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und spricht in voller Angst: Nimmt sie sich meiner an,", "tokens": ["Und", "spricht", "in", "vol\u00b7ler", "Angst", ":", "Nimmt", "sie", "sich", "mei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So fordre sie davor, womit ich dienen kan.", "tokens": ["So", "ford\u00b7re", "sie", "da\u00b7vor", ",", "wo\u00b7mit", "ich", "die\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vernimmt Rosildgen denn sein toll verliebt Beginnen,", "tokens": ["Ver\u00b7nimmt", "Ro\u00b7sild\u00b7gen", "denn", "sein", "toll", "ver\u00b7liebt", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "PPOSAT", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wei\u00df sie alsobald ein Mittel auszusinnen,", "tokens": ["So", "wei\u00df", "sie", "al\u00b7so\u00b7bald", "ein", "Mit\u00b7tel", "aus\u00b7zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Damit sie seine Pein und ihre Lust vermehrt,", "tokens": ["Da\u00b7mit", "sie", "sei\u00b7ne", "Pein", "und", "ih\u00b7re", "Lust", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Eh ihm der Haasen-Schroth noch das Gehirn versehrt.", "tokens": ["Eh", "ihm", "der", "Haa\u00b7sen\u00b7Schroth", "noch", "das", "Ge\u00b7hirn", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Sie macht, da\u00df alsobald ein Hauben-Stock erscheinet,", "tokens": ["Sie", "macht", ",", "da\u00df", "al\u00b7so\u00b7bald", "ein", "Hau\u00b7ben\u00b7Stock", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und weil der tolle Geck in seinen Hertzen meynet,", "tokens": ["Und", "weil", "der", "tol\u00b7le", "Geck", "in", "sei\u00b7nen", "Hert\u00b7zen", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df di\u00df die Sch\u00f6ne sey, so schwenckt er seinen Hut", "tokens": ["Da\u00df", "di\u00df", "die", "Sch\u00f6\u00b7ne", "sey", ",", "so", "schwenckt", "er", "sei\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So tieff, da\u00df er damit dem Pflaster Schaden thut;", "tokens": ["So", "tieff", ",", "da\u00df", "er", "da\u00b7mit", "dem", "Pflas\u00b7ter", "Scha\u00b7den", "thut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch wann er sich erhohlt und zu sich selber kommen,", "tokens": ["Doch", "wann", "er", "sich", "er\u00b7hohlt", "und", "zu", "sich", "sel\u00b7ber", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVPP", "KON", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So zweifelt er, ob sie den Gru\u00df wohl aufgenommen;", "tokens": ["So", "zwei\u00b7felt", "er", ",", "ob", "sie", "den", "Gru\u00df", "wohl", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Warum? er mercket erst, da\u00df sie sich nicht geneigt,", "tokens": ["Wa\u00b7rum", "?", "er", "mer\u00b7cket", "erst", ",", "da\u00df", "sie", "sich", "nicht", "ge\u00b7neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So tieff er doch vor ihr im Scharrfu\u00df sich gebeugt.", "tokens": ["So", "tieff", "er", "doch", "vor", "ihr", "im", "Scharr\u00b7fu\u00df", "sich", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPR", "PPOSAT", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja, weil er vollends gar zu seinen Schimpff ersiehet,", "tokens": ["Ja", ",", "weil", "er", "vol\u00b7lends", "gar", "zu", "sei\u00b7nen", "Schimpff", "er\u00b7sie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df man den Hauben-Stock mit List zur\u00fccke ziehet,", "tokens": ["Da\u00df", "man", "den", "Hau\u00b7ben\u00b7Stock", "mit", "List", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und da\u00df es in der That nicht sein Rosildgen ist,", "tokens": ["Und", "da\u00df", "es", "in", "der", "That", "nicht", "sein", "Ro\u00b7sild\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So sch\u00e4mt er sich, da\u00df er den Klotz so tieff gegr\u00fc\u00dft.", "tokens": ["So", "sch\u00e4mt", "er", "sich", ",", "da\u00df", "er", "den", "Klotz", "so", "tieff", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er sieht sich \u00e4ngstlich um, ob auch das Volck gesehen,", "tokens": ["Er", "sieht", "sich", "\u00e4ngst\u00b7lich", "um", ",", "ob", "auch", "das", "Volck", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was vor ein toller Streich ihm durch Betrug geschehen,", "tokens": ["Was", "vor", "ein", "tol\u00b7ler", "Streich", "ihm", "durch", "Be\u00b7trug", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und schleicht, di\u00df billig ist verliebter Hasen Lohn,", "tokens": ["Und", "schleicht", ",", "di\u00df", "bil\u00b7lig", "ist", "ver\u00b7lieb\u00b7ter", "Ha\u00b7sen", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "ADJD", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie ein bego\u00dfner Hund, so still er kan, davon.", "tokens": ["Wie", "ein", "be\u00b7go\u00df\u00b7ner", "Hund", ",", "so", "still", "er", "kan", ",", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}