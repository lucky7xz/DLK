{"textgrid.poem.40905": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Dem aufrichtigen und wohlerfahrnen Schweizerboten an seinem Hochzeittage", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "I ha 's jo gseit, und 's isch so cho!", "tokens": ["I", "ha", "'s", "jo", "gseit", ",", "und", "'s", "isch", "so", "cho", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "PPER", "NE", "NN", "$,", "KON", "PPER", "ADJD", "ADV", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Was hani gseit? 's werd nit lang goh,", "tokens": ["Was", "ha\u00b7ni", "gseit", "?", "'s", "werd", "nit", "lang", "goh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "se bringt der Bott vom Schwitzerland", "tokens": ["se", "bringt", "der", "Bott", "vom", "Schwit\u00b7zer\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "es Br\u00fctli an der weiche Hand,", "tokens": ["es", "Br\u00fct\u00b7li", "an", "der", "wei\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es lieblig Br\u00fctli mit'm Chranz", "tokens": ["es", "lieb\u00b7lig", "Br\u00fct\u00b7li", "mit'm", "Chranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NE", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "zum Chilgang und zum Hochzittanz.", "tokens": ["zum", "Chil\u00b7gang", "und", "zum", "Hoch\u00b7zit\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "'s isch frili wohr, und so ne Ma", "tokens": ["'s", "isch", "fri\u00b7li", "wohr", ",", "und", "so", "ne", "Ma"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "FM", "FM", "FM", "$,", "KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "es Fraueli, das mue\u00df er ha.", "tokens": ["es", "Frau\u00b7e\u00b7li", ",", "das", "mu\u00b7e\u00df", "er", "ha", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PDS", "VVFIN", "PPER", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Fr\u00fceih, wenn er mit'm Morgerot", "tokens": ["Fr\u00fc\u00b7eih", ",", "wenn", "er", "mit'm", "Mor\u00b7ge\u00b7rot"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "uf d'Stro\u00df go Brugg und Basel goht,", "tokens": ["uf", "d'\u00b7St\u00b7ro\u00df", "go", "Brugg", "und", "Ba\u00b7sel", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "wer nimmt en z' erst no lieb und warm,", "tokens": ["wer", "nimmt", "en", "z'", "erst", "no", "lieb", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "FM", "FM", "FM", "FM", "ADJD", "KON", "ADJD", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.12": {"text": "zum Bh\u00fcetdigott und Chu\u00df, in Arm?", "tokens": ["zum", "Bh\u00fcet\u00b7di\u00b7gott", "und", "Chu\u00df", ",", "in", "Arm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wenn er mittem Abedstern", "tokens": ["Und", "wenn", "er", "mit\u00b7tem", "A\u00b7beds\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "in d'Heimet chunnt, was h\u00e4tt er gern?", "tokens": ["in", "d'\u00b7Hei\u00b7met", "chunnt", ",", "was", "h\u00e4tt", "er", "gern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "'s s\u00f6tt n\u00e4umis an der Huust\u00fcr stoh,", "tokens": ["'s", "s\u00f6tt", "n\u00e4u\u00b7mis", "an", "der", "Huus\u00b7t\u00fcr", "stoh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "es s\u00f6tt em lieb eggege cho,", "tokens": ["es", "s\u00f6tt", "em", "lieb", "eg\u00b7ge\u00b7ge", "cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "und fr\u00fcndli s\u00e4ge: \u00bbGr\u00fce\u00df di Gott,", "tokens": ["und", "fr\u00fcnd\u00b7li", "s\u00e4\u00b7ge", ":", "\u00bb", "Gr\u00fce\u00df", "di", "Gott", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "$(", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "du liebe Ma und Schwizerbott!\u00ab", "tokens": ["du", "lie\u00b7be", "Ma", "und", "Schwi\u00b7zer\u00b7bott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und s\u00e4ge s\u00f6tt's em: \u00bbLiebe Ma,", "tokens": ["Und", "s\u00e4\u00b7ge", "s\u00f6tt's", "em", ":", "\u00bb", "Lie\u00b7be", "Ma", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "chumm weidli, leg d'Pantofflen a,", "tokens": ["chumm", "weid\u00b7li", ",", "leg", "d'\u00b7Pan\u00b7tofflen", "a", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "und 's Tsch\u00f6pli! Uffem Tischtuch stoht", "tokens": ["und", "'s", "Tsch\u00f6p\u00b7li", "!", "Uf\u00b7fem", "Tischtuch", "stoht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "$.", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "di 's S\u00fcppli scho vo wi\u00dfem Brot.", "tokens": ["di", "'s", "S\u00fcpp\u00b7li", "scho", "vo", "wi\u00b7\u00dfem", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Chumm liebi Seel, und i\u00df jez z' Nacht!", "tokens": ["Chumm", "lie\u00b7bi", "Seel", ",", "und", "i\u00df", "jez", "z'", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "VVFIN", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Und 's Bettli isch der au scho gmacht.\u00ab", "tokens": ["Und", "'s", "Bett\u00b7li", "isch", "der", "au", "scho", "gmacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "NN", "ADJD", "ART", "NE", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Das wei\u00df er wohl, mi Schwizerbott,", "tokens": ["Das", "wei\u00df", "er", "wohl", ",", "mi", "Schwi\u00b7zer\u00b7bott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "'s isch nit, as wennim 's s\u00e4ge wott.", "tokens": ["'s", "isch", "nit", ",", "as", "wen\u00b7nim", "'s", "s\u00e4\u00b7ge", "wott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKNEG", "$,", "NE", "KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Drum het er au am lange Rhi", "tokens": ["Drum", "het", "er", "au", "am", "lan\u00b7ge", "Rhi"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und Kanton us und Kanton i", "tokens": ["und", "Kan\u00b7ton", "us", "und", "Kan\u00b7ton", "i"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "KON", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "meng Meidschi scharf in d'Auge gno,", "tokens": ["meng", "Meid\u00b7schi", "scharf", "in", "d'\u00b7Au\u00b7ge", "gno", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "\u00f6b nit bald w\u00f6ll die rechti cho.", "tokens": ["\u00f6b", "nit", "bald", "w\u00f6ll", "die", "rech\u00b7ti", "cho", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VMFIN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und Kanton us und Kanton i,", "tokens": ["Und", "Kan\u00b7ton", "us", "und", "Kan\u00b7ton", "i", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "KON", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "bald an der Limmet, bald am Rhi,", "tokens": ["bald", "an", "der", "Lim\u00b7met", ",", "bald", "am", "Rhi", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "wol het er bravi Meidsch'ne gseh,", "tokens": ["wol", "het", "er", "bra\u00b7vi", "Mei\u00b7dsch'\u00b7ne", "gseh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NE", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.34": {"text": "wie 's R\u00f6sli rot, wi\u00df wie der Schnee,", "tokens": ["wie", "'s", "R\u00f6s\u00b7li", "rot", ",", "wi\u00df", "wie", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "so tusigsch\u00f6n und gut und froh.", "tokens": ["so", "tu\u00b7sig\u00b7sch\u00f6n", "und", "gut", "und", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die rechti het nit welle cho.", "tokens": ["Die", "rech\u00b7ti", "het", "nit", "wel\u00b7le", "cho", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKNEG", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "'s macht n\u00fct. Mi liebe Schwizerbott", "tokens": ["'s", "macht", "n\u00fct", ".", "Mi", "lie\u00b7be", "Schwi\u00b7zer\u00b7bott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "het gseit: \u00bbI find sie doch, will's Gott!\u00ab", "tokens": ["het", "gseit", ":", "\u00bb", "I", "find", "sie", "doch", ",", "will's", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "$.", "$(", "CARD", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "I glaub es schier, Herr Bottema!", "tokens": ["I", "glaub", "es", "schier", ",", "Herr", "Bot\u00b7te\u00b7ma", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "ADJD", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "L\u00e4ngst heit er 's in der N\u00f6chi 