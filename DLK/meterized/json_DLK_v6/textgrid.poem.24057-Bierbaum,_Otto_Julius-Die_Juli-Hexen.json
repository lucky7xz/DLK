s": ["Ganz", "vol\u00b7ler", "Haa\u00b7re", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "\u00bbund weiter nichts?\u00ab", "tokens": ["\u00bb", "und", "wei\u00b7ter", "nichts", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "PIS", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "\u203aoh ja: Bocksbeine! Und eine krumme Na\u2013ase!\u2039", "tokens": ["\u203a", "oh", "ja", ":", "Bocks\u00b7bei\u00b7ne", "!", "Und", "ei\u00b7ne", "krum\u00b7me", "Na", "\u2013", "a\u00b7se", "!", "\u2039"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["FM", "FM", "ADV", "$.", "NE", "$.", "KON", "ART", "ADJA", "NN", "$(", "FM", "$.", "$("], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "\u00bbhu! Wird er euch nicht bei\u00dfen?\u00ab", "tokens": ["\u00bb", "hu", "!", "Wird", "er", "euch", "nicht", "bei\u00b7\u00dfen", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "\u203afallt ihm net ein! Fallt ihm net ein!", "tokens": ["\u203a", "fallt", "ihm", "net", "ein", "!", "Fallt", "ihm", "net", "ein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NE", "PTKVZ", "$.", "VVFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Hat die kleinen M\u00e4deln so gerne und spielt", "tokens": ["Hat", "die", "klei\u00b7nen", "M\u00e4\u00b7deln", "so", "ger\u00b7ne", "und", "spielt"], "token_