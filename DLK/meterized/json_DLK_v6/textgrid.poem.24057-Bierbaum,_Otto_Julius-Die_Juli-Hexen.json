{"textgrid.poem.24057": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Juli-Hexen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u203ader Mond trinkt an der Erde,", "tokens": ["\u203a", "der", "Mond", "trinkt", "an", "der", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm heraus in die helle Nacht!\u2039", "tokens": ["Komm", "he\u00b7raus", "in", "die", "hel\u00b7le", "Nacht", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbwohin wollen wir gehen?\u00ab", "tokens": ["\u00bb", "wo\u00b7hin", "wol\u00b7len", "wir", "ge\u00b7hen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u203aauf die Waldwiese!", "tokens": ["\u203a", "auf", "die", "Wald\u00b7wie\u00b7se", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Auf die Waldwiese!\u2039", "tokens": ["Auf", "die", "Wald\u00b7wie\u00b7se", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00bbwas wollen wir denn dort machen?\u00ab", "tokens": ["\u00bb", "was", "wol\u00b7len", "wir", "denn", "dort", "ma\u00b7chen", "?", "\u00ab"], "t