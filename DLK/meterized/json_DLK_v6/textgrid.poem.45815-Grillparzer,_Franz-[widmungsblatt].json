{"textgrid.poem.45815": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[widmungsblatt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Erde Gl\u00fcck? Ein Schatten!", "tokens": ["Was", "ist", "der", "Er\u00b7de", "Gl\u00fcck", "?", "Ein", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist der Erde Ruhm? Ein Traum!", "tokens": ["Was", "ist", "der", "Er\u00b7de", "Ruhm", "?", "Ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch gibts ein Gl\u00fcck hienieden,", "tokens": ["Und", "doch", "gibts", "ein", "Gl\u00fcck", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das kein Schatten und kein Traum:", "tokens": ["Das", "kein", "Schat\u00b7ten", "und", "kein", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bewahrte Gottesfrieden", "tokens": ["Der", "be\u00b7wahr\u00b7te", "Got\u00b7tes\u00b7frie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des eignen Herzens Raum.", "tokens": ["In", "des", "eig\u00b7nen", "Her\u00b7zens", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}