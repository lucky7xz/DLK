{"dta.poem.3219": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Opferfeuer brennt, das nie erl\u00f6schen darf,", "tokens": ["Das", "Op\u00b7fer\u00b7feu\u00b7er", "brennt", ",", "das", "nie", "er\u00b7l\u00f6\u00b7schen", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wir sind's alle, die man drein als Brennstoff warf.", "tokens": ["Und", "wir", "sin\u00b7d's", "al\u00b7le", ",", "die", "man", "drein", "als", "Brenn\u00b7stoff", "warf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "$,", "PRELS", "PIS", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Der eine, Weihrauchduft, hinlodernd, leicht und heiter,", "tokens": ["Der", "ei\u00b7ne", ",", "Weih\u00b7rauch\u00b7duft", ",", "hin\u00b7lo\u00b7dernd", ",", "leicht", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und andre schwerere, der Kohle Nahrung, Scheiter.", "tokens": ["Und", "and\u00b7re", "schwe\u00b7re\u00b7re", ",", "der", "Koh\u00b7le", "Nah\u00b7rung", ",", "Schei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Befeuchtet von dem Gischt des gr\u00fcnen Reisigs zischt", "tokens": ["Be\u00b7feuch\u00b7tet", "von", "dem", "Gischt", "des", "gr\u00fc\u00b7nen", "Rei\u00b7sigs", "zischt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Brand, der nicht erlischt, vom Windzug angefrischt.", "tokens": ["Der", "Brand", ",", "der", "nicht", "er\u00b7lischt", ",", "vom", "Wind\u00b7zug", "an\u00b7ge\u00b7frischt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Flamme l\u00e4uft im Nu von einem andern zu;", "tokens": ["Die", "Flam\u00b7me", "l\u00e4uft", "im", "Nu", "von", "ei\u00b7nem", "an\u00b7dern", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn ich bin zur Ruh', kommst an die Reihe du.", "tokens": ["Und", "wenn", "ich", "bin", "zur", "Ruh'", ",", "kommst", "an", "die", "Rei\u00b7he", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00dft uns, wie man uns ruft, verlodern in die Luft,", "tokens": ["La\u00dft", "uns", ",", "wie", "man", "uns", "ruft", ",", "ver\u00b7lo\u00b7dern", "in", "die", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Himmel Opferduft, und Aschen in die Gruft.", "tokens": ["Zum", "Him\u00b7mel", "Op\u00b7fer\u00b7duft", ",", "und", "A\u00b7schen", "in", "die", "Gruft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Aus todter Asche stammt, was lebend wieder flammt.", "tokens": ["Aus", "tod\u00b7ter", "A\u00b7sche", "stammt", ",", "was", "le\u00b7bend", "wie\u00b7der", "flammt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gottes Wolkenzelt ist weben Rauches Amt.", "tokens": ["Und", "Got\u00b7tes", "Wol\u00b7ken\u00b7zelt", "ist", "we\u00b7ben", "Rau\u00b7ches", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}