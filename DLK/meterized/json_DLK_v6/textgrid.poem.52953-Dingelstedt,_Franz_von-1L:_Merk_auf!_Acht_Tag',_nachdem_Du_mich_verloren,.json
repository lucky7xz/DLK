{"textgrid.poem.52953": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Merk auf! Acht Tag', nachdem Du mich verloren,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Merk auf! Acht Tag', nachdem Du mich verloren,", "tokens": ["Merk", "auf", "!", "Acht", "Tag'", ",", "nach\u00b7dem", "Du", "mich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "CARD", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann werden fromme Tr\u00f6ster zu Dir kommen", "tokens": ["Dann", "wer\u00b7den", "from\u00b7me", "Tr\u00f6s\u00b7ter", "zu", "Dir", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und freundlich auf die rechte Stunde passen;", "tokens": ["Und", "freund\u00b7lich", "auf", "die", "rech\u00b7te", "Stun\u00b7de", "pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie raunen nachbarlich Dir in die Ohren:", "tokens": ["Sie", "rau\u00b7nen", "nach\u00b7bar\u00b7lich", "Dir", "in", "die", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbdu hast zu sehr zu Herzen es genommen,", "tokens": ["\u00bb", "du", "hast", "zu", "sehr", "zu", "Her\u00b7zen", "es", "ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKA", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er hat dich eigentlich doch schn\u00f6d verlassen,", "tokens": ["Er", "hat", "dich", "ei\u00b7gent\u00b7lich", "doch", "schn\u00f6d", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Versuch es ihn zu hassen\u00ab ... ! ... \u2013", "tokens": ["Ver\u00b7such", "es", "ihn", "zu", "has\u00b7sen", "\u00ab", "...", "!", "...", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "PPER", "PPER", "PTKZU", "VVINF", "$(", "$(", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Spei ihnen ins Gesicht den Pharis\u00e4ern", "tokens": ["Spei", "ih\u00b7nen", "ins", "Ge\u00b7sicht", "den", "Pha\u00b7ri\u00b7s\u00e4\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und schlie\u00dfe Dich in Deine stille Kammer,", "tokens": ["Und", "schlie\u00b7\u00dfe", "Dich", "in", "Dei\u00b7ne", "stil\u00b7le", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dort la\u00df, den Sp\u00f6ttern ferne wie den Sp\u00e4hern,", "tokens": ["Dort", "la\u00df", ",", "den", "Sp\u00f6t\u00b7tern", "fer\u00b7ne", "wie", "den", "Sp\u00e4\u00b7hern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ausbluten Deinen ersten Lebensjammer,", "tokens": ["Aus\u00b7blu\u00b7ten", "Dei\u00b7nen", "ers\u00b7ten", "Le\u00b7bens\u00b7jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und selbst die Wunde \u2013 glaubs \u2013 wird Dich begl\u00fccken,", "tokens": ["Und", "selbst", "die", "Wun\u00b7de", "\u2013", "glaubs", "\u2013", "wird", "Dich", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "NE", "$(", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn fremder T\u00f6lpel F\u00e4uste sie nicht dr\u00fccken.", "tokens": ["Wenn", "frem\u00b7der", "T\u00f6l\u00b7pel", "F\u00e4us\u00b7te", "sie", "nicht", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}