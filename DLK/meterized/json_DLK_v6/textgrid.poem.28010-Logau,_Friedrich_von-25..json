{"textgrid.poem.28010": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der mit dem Beutel gieng, hie\u00df Judas; Der zu legen", "tokens": ["Der", "mit", "dem", "Beu\u00b7tel", "gieng", ",", "hie\u00df", "Ju\u00b7das", ";", "Der", "zu", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "NE", "$.", "ART", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Haupt nicht hatte Raum, heist Christus. Zeitlich Segen", "tokens": ["Sein", "Haupt", "nicht", "hat\u00b7te", "Raum", ",", "heist", "Chris\u00b7tus", ".", "Zeit\u00b7lich", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "VAFIN", "NN", "$,", "VVFIN", "NE", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist lange lange nicht die rechte Lieverey,", "tokens": ["Ist", "lan\u00b7ge", "lan\u00b7ge", "nicht", "die", "rech\u00b7te", "Lie\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu kennen, wer ein Christ in Christus Kirche sey.", "tokens": ["Zu", "ken\u00b7nen", ",", "wer", "ein", "Christ", "in", "Chris\u00b7tus", "Kir\u00b7che", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "ART", "NN", "APPR", "NE", "NN", "VAFIN", "