{"textgrid.poem.51920": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mensch, das unterst und das oberst ist ein Reim,", "tokens": ["O", "Mensch", ",", "das", "un\u00b7terst", "und", "das", "o\u00b7berst", "ist", "ein", "Reim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "KON", "PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Wesen spricht, den Schall schickt der Entwurff ihm heim.", "tokens": ["Das", "We\u00b7sen", "spricht", ",", "den", "Schall", "schickt", "der", "Ent\u00b7wurff", "ihm", "heim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}