{"textgrid.poem.67833": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5. Vom verwundeten Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein M\u00e4dchen fr\u00fch aufstehn,", "tokens": ["Es", "wollt", "ein", "M\u00e4d\u00b7chen", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in den gr\u00fcnen Wald spazieren gehn.", "tokens": ["Und", "in", "den", "gr\u00fc\u00b7nen", "Wald", "spa\u00b7zie\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als sie nun in den gr\u00fcnen Wald kam,", "tokens": ["Und", "als", "sie", "nun", "in", "den", "gr\u00fc\u00b7nen", "Wald", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da fand sie einen verwundeten Knab'n.", "tokens": ["Da", "fand", "sie", "ei\u00b7nen", "ver\u00b7wun\u00b7de\u00b7ten", "Knab'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Knab, der war von Blut so roth,", "tokens": ["Der", "Knab", ",", "der", "war", "von", "Blut", "so", "roth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als sie sich verwand, war er schon todt.", "tokens": ["Und", "als", "sie", "sich", "ver\u00b7wand", ",", "war", "er", "schon", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo krieg ich nun zwei Leidfr\u00e4ulein,", "tokens": ["Wo", "krieg", "ich", "nun", "zwei", "Leid\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mein feines Liebchen zu Grabe wein'n?", "tokens": ["Die", "mein", "fei\u00b7nes", "Lieb\u00b7chen", "zu", "Gra\u00b7be", "wein'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo krieg' ich nun sechs Reuterknab'n,", "tokens": ["Wo", "krieg'", "ich", "nun", "sechs", "Reu\u00b7ter\u00b7knab'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mein feins Liebchen zu Grabe trag'n?", "tokens": ["Die", "mein", "feins", "Lieb\u00b7chen", "zu", "Gra\u00b7be", "trag'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie lang soll ich denn trauren gehn?", "tokens": ["Wie", "lang", "soll", "ich", "denn", "trau\u00b7ren", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis alle Wasser zusammen gehn?", "tokens": ["Bis", "al\u00b7le", "Was\u00b7ser", "zu\u00b7sam\u00b7men", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja alle Wasser gehn nicht zusamm'n,", "tokens": ["Ja", "al\u00b7le", "Was\u00b7ser", "gehn", "nicht", "zu\u00b7sam\u00b7m'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wird mein Trauren kein Ende han.", "tokens": ["So", "wird", "mein", "Trau\u00b7ren", "kein", "En\u00b7de", "han", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}