{"textgrid.poem.53891": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Saxo-Borussen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chten Sie Saxo-Borusse sein?", "tokens": ["M\u00f6ch\u00b7ten", "Sie", "Sa\u00b7xo\u00b7Bo\u00b7rus\u00b7se", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Domela hat sie genau beschrieben:", "tokens": ["Do\u00b7me\u00b7la", "hat", "sie", "ge\u00b7nau", "be\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "was sie auf ihrer Kneipe trieben \u2013", "tokens": ["was", "sie", "auf", "ih\u00b7rer", "Knei\u00b7pe", "trie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "(r\u00fclps)", "tokens": ["(", "r\u00fclps", ")"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "wie sie fechten, fressen und saufen,", "tokens": ["wie", "sie", "fech\u00b7ten", ",", "fres\u00b7sen", "und", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "ADJA", "KON", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "sich niemals ein Kollegheft kaufen \u2013", "tokens": ["sich", "nie\u00b7mals", "ein", "Kol\u00b7leg\u00b7heft", "kau\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "jeder ein hochfeudales Schwein . . .", "tokens": ["je\u00b7der", "ein", "hoch\u00b7feu\u00b7da\u00b7les", "Schwein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ein feiner Verein.", "tokens": ["Ein", "fei\u00b7ner", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "M\u00f6chten Sie Saxo-Borusse sein?", "tokens": ["M\u00f6ch\u00b7ten", "Sie", "Sa\u00b7xo\u00b7Bo\u00b7rus\u00b7se", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ramsch . . . Manieren: frech und beflissen \u2013", "tokens": ["Ramsch", ".", ".", ".", "Ma\u00b7nie\u00b7ren", ":", "frech", "und", "be\u00b7flis\u00b7sen", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$.", "$.", "NN", "$.", "ADJD", "KON", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00bbwerde zu Hause zu r\u00fchmen wissen!\u00ab", "tokens": ["\u00bb", "wer\u00b7de", "zu", "Hau\u00b7se", "zu", "r\u00fch\u00b7men", "wis\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "VVINF", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "(r\u00fclps)", "tokens": ["(", "r\u00fclps", ")"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "F\u00fcchsegetriez und Chargenspiel;", "tokens": ["F\u00fcch\u00b7se\u00b7ge\u00b7triez", "und", "Char\u00b7gen\u00b7spiel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ideal: der uralte Leutnantsstil . . .", "tokens": ["I\u00b7deal", ":", "der", "ur\u00b7al\u00b7te", "Leut\u00b7nants\u00b7stil", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "\u00bbkein B\u00fcrjerlicher kommt hier zu uns rein \u2013\u00ab", "tokens": ["\u00bb", "kein", "B\u00fcr\u00b7jer\u00b7li\u00b7cher", "kommt", "hier", "zu", "uns", "rein", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein feiner Verein.", "tokens": ["Ein", "fei\u00b7ner", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "M\u00f6chten Sie Saxo-Borusse sein?", "tokens": ["M\u00f6ch\u00b7ten", "Sie", "Sa\u00b7xo\u00b7Bo\u00b7rus\u00b7se", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das ist gar nicht \u00fcbel. Im Westen und Osten", "tokens": ["Das", "ist", "gar", "nicht", "\u00fc\u00b7bel", ".", "Im", "Wes\u00b7ten", "und", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ADJD", "$.", "APPRART", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "geh\u00f6ren ihnen die Botschafterposten \u2013", "tokens": ["ge\u00b7h\u00f6\u00b7ren", "ih\u00b7nen", "die", "Bot\u00b7schaf\u00b7ter\u00b7pos\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie beherrschen Deutschland. Sie sind dran.", "tokens": ["sie", "be\u00b7herr\u00b7schen", "Deutschland", ".", "Sie", "sind", "dran", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "PPER", "VAFIN", "PAV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sie intrigieren. Mann f\u00fcr Mann.", "tokens": ["Sie", "int\u00b7ri\u00b7gie\u00b7ren", ".", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Peking, in Rio und in Madrid:", "tokens": ["In", "Pe\u00b7king", ",", "in", "Rio", "und", "in", "Mad\u00b7rid", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "immer l\u00e4uft ein Korpsband mit.", "tokens": ["im\u00b7mer", "l\u00e4uft", "ein", "Korps\u00b7band", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mit diesem Korpsband zieht die Blase", "tokens": ["Und", "mit", "die\u00b7sem", "Korps\u00b7band", "zieht", "die", "Bla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "ein ganzes Volk an seiner Nase.", "tokens": ["ein", "gan\u00b7zes", "Volk", "an", "sei\u00b7ner", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir fressens aus. Sie brockens uns ein.", "tokens": ["Wir", "fres\u00b7sens", "aus", ".", "Sie", "bro\u00b7ckens", "uns", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Wer m\u00f6cht da nicht Saxo-Borusse sein \u2013!", "tokens": ["Wer", "m\u00f6cht", "da", "nicht", "Sa\u00b7xo\u00b7Bo\u00b7rus\u00b7se", "sein", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}