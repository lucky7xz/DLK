{"dta.poem.19870": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  Lob Helenens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "O Br\u00e4utigam, welch' eine Braut               ", "tokens": ["O", "Br\u00e4u\u00b7ti\u00b7gam", ",", "welch'", "ei\u00b7ne", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird deinem Arm\u2019 zur Beute!", "tokens": ["Wird", "dei\u00b7nem", "Arm'", "zur", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei meiner Leier schw\u00f6r\u2019 ich\u2019s laut:", "tokens": ["Bei", "mei\u00b7ner", "Lei\u00b7er", "schw\u00f6r'", "ich's", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krone sch\u00f6ner Br\u00e4ute!", "tokens": ["Die", "Kro\u00b7ne", "sch\u00f6\u00b7ner", "Br\u00e4u\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer zweifelt, wandre hin und her,", "tokens": ["Wer", "zwei\u00b7felt", ",", "wand\u00b7re", "hin", "und", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings um die alten Gleichen!", "tokens": ["Rings", "um", "die", "al\u00b7ten", "Glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein sch\u00f6ner Fr\u00e4ulein findet er,", "tokens": ["Kein", "sch\u00f6\u00b7ner", "Fr\u00e4u\u00b7lein", "fin\u00b7det", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen K\u00f6nigreichen. \u2014", "tokens": ["In", "al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Blik verheist ein Paradies;", "tokens": ["Ihr", "Blik", "ver\u00b7heist", "ein", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wang\u2019 ist Morgenr\u00f6te;", "tokens": ["Die", "Wang'", "ist", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Stimme t\u00f6nt so s\u00fcs,", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7me", "t\u00f6nt", "so", "s\u00fcs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie K\u00f6nig Friedrichs Fl\u00f6te.", "tokens": ["Wie", "K\u00f6\u00b7nig", "Fried\u00b7richs", "Fl\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch mehr! Des Dichters Fantasei", "tokens": ["Noch", "mehr", "!", "Des", "Dich\u00b7ters", "Fan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verr\u00e4th es seiner Leier,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "es", "sei\u00b7ner", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ihre Lippe s\u00fcsser sey,", "tokens": ["Da\u00df", "ih\u00b7re", "Lip\u00b7pe", "s\u00fcs\u00b7ser", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Honig und Tokaier.", "tokens": ["Als", "Ho\u00b7nig", "und", "To\u00b7kai\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihr schlanker Wuchs \u2014 Doch wie vermag", "tokens": ["Ihr", "schlan\u00b7ker", "Wuchs", "Doch", "wie", "ver\u00b7mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "PWAV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich jeden Reiz zu singen?", "tokens": ["Ich", "je\u00b7den", "Reiz", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kaum reicht\u2019 ein langer Sommertag,", "tokens": ["Kaum", "reicht'", "ein", "lan\u00b7ger", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Loblied zu volbringen.", "tokens": ["Ihr", "Lob\u00b7lied", "zu", "vol\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie weichet nicht in Griechenland", "tokens": ["Sie", "wei\u00b7chet", "nicht", "in", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nen Namensschwester;", "tokens": ["Der", "sch\u00f6\u00b7nen", "Na\u00b7mens\u00b7schwes\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00e4lt ihr Herz das g\u00fcldne Band", "tokens": ["Doch", "h\u00e4lt", "ihr", "Herz", "das", "g\u00fcld\u00b7ne", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebestreu\u2019 weit fester. \u2014", "tokens": ["Der", "Lie\u00b7bes\u00b7treu'", "weit", "fes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Einst h\u00e4tten in der Wunderzeit", "tokens": ["Einst", "h\u00e4t\u00b7ten", "in", "der", "Wun\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Riesen und der Moren", "tokens": ["Der", "Rie\u00b7sen", "und", "der", "Mo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Paladine weit und breit", "tokens": ["Die", "Pa\u00b7la\u00b7di\u00b7ne", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Dame sie erkoren.", "tokens": ["Zur", "Da\u00b7me", "sie", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Namen h\u00e4tt\u2019 im Feldpanier", "tokens": ["Ihr", "Na\u00b7men", "h\u00e4tt'", "im", "Feld\u00b7pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rittern Mut geschimmert,", "tokens": ["Den", "Rit\u00b7tern", "Mut", "ge\u00b7schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Schild\u2019 und Lanzen im Turnier", "tokens": ["Und", "Schild'", "und", "Lan\u00b7zen", "im", "Tur\u00b7nier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu tausenden zertr\u00fcmmert.", "tokens": ["Zu", "tau\u00b7sen\u00b7den", "zer\u00b7tr\u00fcm\u00b7mert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "W\u00e4r\u2019 sie geboren auf der Flur,", "tokens": ["W\u00e4r'", "sie", "ge\u00b7bo\u00b7ren", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jenen goldnen Jahren,", "tokens": ["In", "je\u00b7nen", "gold\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ritterliche Lanzen nur", "tokens": ["Als", "rit\u00b7ter\u00b7li\u00b7che", "Lan\u00b7zen", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Hirtenst\u00e4be waren:", "tokens": ["Noch", "Hir\u00b7ten\u00b7st\u00e4\u00b7be", "wa\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So h\u00e4tt\u2019 um sie, in Flur und Hain,", "tokens": ["So", "h\u00e4tt'", "um", "sie", ",", "in", "Flur", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jedes Lied geworben.", "tokens": ["Ein", "je\u00b7des", "Lied", "ge\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wol mancher w\u00e4r\u2019 in Liebespein,", "tokens": ["Wol", "man\u00b7cher", "w\u00e4r'", "in", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "APPR", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Sch\u00e4ferart, gestorben. \u2014", "tokens": ["Nach", "Sch\u00e4\u00b7fer\u00b7art", ",", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sieh, solche Braut zieht deine Hand", "tokens": ["Sieh", ",", "sol\u00b7che", "Braut", "zieht", "dei\u00b7ne", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinweg aus unsern Blicken.", "tokens": ["Hin\u00b7weg", "aus", "un\u00b7sern", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie neiden wir das fremde Land,", "tokens": ["Wie", "nei\u00b7den", "wir", "das", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Helena sol schm\u00fccken!", "tokens": ["Das", "He\u00b7le\u00b7na", "sol", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ach! welche Nachbarin ersezt", "tokens": ["Ach", "!", "wel\u00b7che", "Nach\u00b7ba\u00b7rin", "er\u00b7sezt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie unsern Nachbars\u00f6hnen?", "tokens": ["Sie", "un\u00b7sern", "Nach\u00b7bar\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und welche wird die Reigen jezt,", "tokens": ["Und", "wel\u00b7che", "wird", "die", "Rei\u00b7gen", "jezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Helena, versch\u00f6nen?", "tokens": ["Wie", "He\u00b7le\u00b7na", ",", "ver\u00b7sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du m\u00fcstest wol mit blankem Speer,", "tokens": ["Du", "m\u00fcs\u00b7test", "wol", "mit", "blan\u00b7kem", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Man, sie erst erwerben,", "tokens": ["O", "Man", ",", "sie", "erst", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und billig sch\u00e4ferlich vorher", "tokens": ["Und", "bil\u00b7lig", "sch\u00e4\u00b7fer\u00b7lich", "vor\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein paarmal f\u00fcr sie sterben! \u2014", "tokens": ["Ein", "paar\u00b7mal", "f\u00fcr", "sie", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Doch wirst du k\u00fcnftig, ohne Leid,", "tokens": ["Doch", "wirst", "du", "k\u00fcnf\u00b7tig", ",", "oh\u00b7ne", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie auf den H\u00e4nden tragen,", "tokens": ["Sie", "auf", "den", "H\u00e4n\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer, nach Verdienst, wie heut,", "tokens": ["Und", "im\u00b7mer", ",", "nach", "Ver\u00b7dienst", ",", "wie", "heut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Honigw\u00f6rtchen sagen:", "tokens": ["Ihr", "Ho\u00b7nig\u00b7w\u00f6rt\u00b7chen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So sey es drum! Wir lassen sie", "tokens": ["So", "sey", "es", "drum", "!", "Wir", "las\u00b7sen", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Frieden unsertwegen.", "tokens": ["In", "Frie\u00b7den", "un\u00b7sert\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Liebe segne dich und sie,", "tokens": ["Die", "Lie\u00b7be", "seg\u00b7ne", "dich", "und", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem besten Segen!", "tokens": ["Mit", "ih\u00b7rem", "bes\u00b7ten", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}