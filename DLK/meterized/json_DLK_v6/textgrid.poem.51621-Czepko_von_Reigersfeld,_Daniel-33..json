{"textgrid.poem.51621": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann sie aufs h\u00f6chste kommt, und ihr das best erlesen,", "tokens": ["Wann", "sie", "aufs", "h\u00f6chs\u00b7te", "kommt", ",", "und", "ihr", "das", "best", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "KON", "PPER", "PDS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So st\u00fcrtzet die Natur denn alles in Ein Wesen.", "tokens": ["So", "st\u00fcrt\u00b7zet", "die", "Na\u00b7tur", "denn", "al\u00b7les", "in", "Ein", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}