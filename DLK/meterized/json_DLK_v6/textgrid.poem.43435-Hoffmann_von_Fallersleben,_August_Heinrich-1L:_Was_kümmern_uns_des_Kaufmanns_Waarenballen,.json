{"textgrid.poem.43435": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was k\u00fcmmern uns des Kaufmanns Waarenballen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00fcmmern uns des Kaufmanns Waarenballen,", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "des", "Kauf\u00b7manns", "Waa\u00b7ren\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Einmaleins, Credit und Geld?", "tokens": ["Sein", "Ein\u00b7mal\u00b7eins", ",", "Cre\u00b7dit", "und", "Geld", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder z\u00e4hl' und rechne nach Gefallen,", "tokens": ["Ein", "je\u00b7der", "z\u00e4hl'", "und", "rech\u00b7ne", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur la\u00df' er uns was uns gef\u00e4llt.", "tokens": ["Nur", "la\u00df'", "er", "uns", "was", "uns", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen den Menschen wie Gott ihn erdacht,", "tokens": ["Wir", "wol\u00b7len", "den", "Men\u00b7schen", "wie", "Gott", "ihn", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KOKOM", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und nicht wie ihn Z\u00e4hlen und Rechnen gemacht.", "tokens": ["Und", "nicht", "wie", "ihn", "Z\u00e4h\u00b7len", "und", "Rech\u00b7nen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Was k\u00fcmmert uns das Wissen und das K\u00f6nnen,", "tokens": ["Was", "k\u00fcm\u00b7mert", "uns", "das", "Wis\u00b7sen", "und", "das", "K\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gelehrter Kram und blauer Dunst?", "tokens": ["Ge\u00b7lehr\u00b7ter", "Kram", "und", "blau\u00b7er", "Dunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen jedem seine Gaben g\u00f6nnen,", "tokens": ["Wir", "wol\u00b7len", "je\u00b7dem", "sei\u00b7ne", "Ga\u00b7ben", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur bleib uns wahre Lebenskunst.", "tokens": ["Nur", "bleib", "uns", "wah\u00b7re", "Le\u00b7bens\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen den Menschen wie Gott ihn schuf,", "tokens": ["Wir", "wol\u00b7len", "den", "Men\u00b7schen", "wie", "Gott", "ihn", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KOKOM", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und nicht wie ihn machte Talent und Beruf.", "tokens": ["Und", "nicht", "wie", "ihn", "mach\u00b7te", "Ta\u00b7lent", "und", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Was k\u00fcmmert uns der Geistlichen Gez\u00e4nke", "tokens": ["Was", "k\u00fcm\u00b7mert", "uns", "der", "Geist\u00b7li\u00b7chen", "Ge\u00b7z\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um unser eignes Seelenheil?", "tokens": ["Um", "un\u00b7ser", "eig\u00b7nes", "See\u00b7len\u00b7heil", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Recht thue jeder, jeder Gutes denke,", "tokens": ["Recht", "thue", "je\u00b7der", ",", "je\u00b7der", "Gu\u00b7tes", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und la\u00df uns unsern Himmelstheil.", "tokens": ["Und", "la\u00df", "uns", "un\u00b7sern", "Him\u00b7mel\u00b7stheil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen den Menschen wie Gott ihn gemacht,", "tokens": ["Wir", "wol\u00b7len", "den", "Men\u00b7schen", "wie", "Gott", "ihn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KOKOM", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und nicht wie sich's Geistliche haben gedacht.", "tokens": ["Und", "nicht", "wie", "sich's", "Geist\u00b7li\u00b7che", "ha\u00b7ben", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Was k\u00fcmmern uns die ernsten Kriegesheere?", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "die", "erns\u00b7ten", "Krie\u00b7ges\u00b7hee\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Ehrenzeichen an der Brust?", "tokens": ["Was", "Eh\u00b7ren\u00b7zei\u00b7chen", "an", "der", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder diene gern' um Sold und Ehre,", "tokens": ["Ein", "je\u00b7der", "die\u00b7ne", "gern'", "um", "Sold", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PDS", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur la\u00df er uns in Friedenslust.", "tokens": ["Nur", "la\u00df", "er", "uns", "in", "Frie\u00b7dens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen den Menschen wie Gott ihn gewollt,", "tokens": ["Wir", "wol\u00b7len", "den", "Men\u00b7schen", "wie", "Gott", "ihn", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KOKOM", "NN", "PPER", "VMPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und nicht was er ward durch Dienen und Sold.", "tokens": ["Und", "nicht", "was", "er", "ward", "durch", "Die\u00b7nen", "und", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PWS", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Was k\u00fcmmern W\u00fcrden uns, was St\u00e4nd' und R\u00e4nge?", "tokens": ["Was", "k\u00fcm\u00b7mern", "W\u00fcr\u00b7den", "uns", ",", "was", "St\u00e4nd'", "und", "R\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "PPER", "$,", "PWS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freud' ist Jeglichem gemein.", "tokens": ["Die", "Freud'", "ist", "Jeg\u00b7li\u00b7chem", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Reich des Becherklangs und der Ges\u00e4nge", "tokens": ["Im", "Reich", "des", "Be\u00b7cher\u00b7klangs", "und", "der", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll nur der Frohste K\u00f6nig sein.", "tokens": ["Soll", "nur", "der", "Frohs\u00b7te", "K\u00f6\u00b7nig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen den Menschen aus Gottes Hand,", "tokens": ["Wir", "wol\u00b7len", "den", "Men\u00b7schen", "aus", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und nicht wie ihn machte der Rang und der Stand.", "tokens": ["Und", "nicht", "wie", "ihn", "mach\u00b7te", "der", "Rang", "und", "der", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Wir sind allhier vereint zu frohen Dingen,", "tokens": ["Wir", "sind", "all\u00b7hier", "ver\u00b7eint", "zu", "fro\u00b7hen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sto\u00dft an! la\u00dft uns den Bund erneu'n!", "tokens": ["Sto\u00dft", "an", "!", "la\u00dft", "uns", "den", "Bund", "er\u00b7neu'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen fr\u00f6hlich trinken, fr\u00f6hlich singen!", "tokens": ["Wir", "wol\u00b7len", "fr\u00f6h\u00b7lich", "trin\u00b7ken", ",", "fr\u00f6h\u00b7lich", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir wollen uns des Lebens freu'n!", "tokens": ["Wir", "wol\u00b7len", "uns", "des", "Le\u00b7bens", "freu'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen nur fr\u00f6hlich, nur Menschen sein \u2013", "tokens": ["Wir", "wol\u00b7len", "nur", "fr\u00f6h\u00b7lich", ",", "nur", "Men\u00b7schen", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "$,", "ADV", "NN", "VAINF", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sto\u00dft an! hoch lebe der frohe Verein!", "tokens": ["Sto\u00dft", "an", "!", "hoch", "le\u00b7be", "der", "fro\u00b7he", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}