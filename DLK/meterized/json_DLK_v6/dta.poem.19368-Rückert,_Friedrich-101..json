{"dta.poem.19368": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob einmal siegen wird das Gute auf der Welt", "tokens": ["Ob", "ein\u00b7mal", "sie\u00b7gen", "wird", "das", "Gu\u00b7te", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVINF", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder das B\u00f6se ihm die Wag' auf ewig h\u00e4lt;", "tokens": ["O\u00b7der", "das", "B\u00f6\u00b7se", "ihm", "die", "Wag'", "auf", "e\u00b7wig", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "NN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der alte Streit ist nicht geschlichtet, nicht zu schlichten,", "tokens": ["Der", "al\u00b7te", "Streit", "ist", "nicht", "ge\u00b7schlich\u00b7tet", ",", "nicht", "zu", "schlich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch irren kann er dich in deinem Thun mitnichten.", "tokens": ["Doch", "ir\u00b7ren", "kann", "er", "dich", "in", "dei\u00b7nem", "Thun", "mit\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast zu handeln so, da\u00df Gutes m\u00f6ge siegen,", "tokens": ["Du", "hast", "zu", "han\u00b7deln", "so", ",", "da\u00df", "Gu\u00b7tes", "m\u00f6\u00b7ge", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "ADV", "$,", "KOUS", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dich zu tr\u00f6sten, wo du's siehest unterliegen.", "tokens": ["Und", "dich", "zu", "tr\u00f6s\u00b7ten", ",", "wo", "du's", "sie\u00b7hest", "un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "$,", "PWAV", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}