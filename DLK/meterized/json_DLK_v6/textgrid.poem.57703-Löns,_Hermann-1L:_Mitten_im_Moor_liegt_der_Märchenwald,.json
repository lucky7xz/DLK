{"textgrid.poem.57703": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mitten im Moor liegt der M\u00e4rchenwald,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitten im Moor liegt der M\u00e4rchenwald,", "tokens": ["Mit\u00b7ten", "im", "Moor", "liegt", "der", "M\u00e4r\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Vom Zauberbache begrenzt;", "tokens": ["Vom", "Zau\u00b7ber\u00b7ba\u00b7che", "be\u00b7grenzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Lockender Zauberruf dort erschallt,", "tokens": ["Lo\u00b7cken\u00b7der", "Zau\u00b7ber\u00b7ruf", "dort", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Distel und Dorn ihn umkr\u00e4nzt.", "tokens": ["Dis\u00b7tel", "und", "Dorn", "ihn", "um\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Mitten im Wald eine Wiese sprie\u00dft,", "tokens": ["Mit\u00b7ten", "im", "Wald", "ei\u00b7ne", "Wie\u00b7se", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihr Gras ist weich und lang;", "tokens": ["Ihr", "Gras", "ist", "weich", "und", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unter der Wiese die Quelle flie\u00dft,", "tokens": ["Un\u00b7ter", "der", "Wie\u00b7se", "die", "Quel\u00b7le", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die hat eigenen Klang.", "tokens": ["Die", "hat", "ei\u00b7ge\u00b7nen", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Hinter der Quelle steht ein Baum,", "tokens": ["Hin\u00b7ter", "der", "Quel\u00b7le", "steht", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sein Silberlaub zittert im Wind;", "tokens": ["Sein", "Sil\u00b7ber\u00b7laub", "zit\u00b7tert", "im", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da sang mir ein Vogel den Wundertraum", "tokens": ["Da", "sang", "mir", "ein", "Vo\u00b7gel", "den", "Wun\u00b7der\u00b7traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von dir und mir, mein Kind.", "tokens": ["Von", "dir", "und", "mir", ",", "mein", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unter dem Baume da w\u00e4chst ein Moos,", "tokens": ["Un\u00b7ter", "dem", "Bau\u00b7me", "da", "w\u00e4chst", "ein", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das schimmert und leuchtet wie Gold;", "tokens": ["Das", "schim\u00b7mert", "und", "leuch\u00b7tet", "wie", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Farne wuchern da stolz und gro\u00df,", "tokens": ["Far\u00b7ne", "wu\u00b7chern", "da", "stolz", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ihr Laub ist seltsam gerollt.", "tokens": ["Ihr", "Laub", "ist", "selt\u00b7sam", "ge\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Da wo die beiden Machangeln stehn,", "tokens": ["Da", "wo", "die", "bei\u00b7den", "Ma\u00b7chan\u00b7geln", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da f\u00fchrt der Weg in den Wald;", "tokens": ["Da", "f\u00fchrt", "der", "Weg", "in", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur wer das Wort kennt, der kann ihn gehn,", "tokens": ["Nur", "wer", "das", "Wort", "kennt", ",", "der", "kann", "ihn", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm bieten die Dornen nicht Halt.", "tokens": ["Ihm", "bie\u00b7ten", "die", "Dor\u00b7nen", "nicht", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Komm, Geliebte, und k\u00fcsse mich,", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7te", ",", "und", "k\u00fcs\u00b7se", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Komm, ich wei\u00df ja das Wort;", "tokens": ["Komm", ",", "ich", "wei\u00df", "ja", "das", "Wort", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und das Wort, das hei\u00dft \u00bbIch liebe dich!\u00ab", "tokens": ["Und", "das", "Wort", ",", "das", "hei\u00dft", "\u00bb", "Ich", "lie\u00b7be", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das dr\u00e4ngt jedes Hindernis fort.", "tokens": ["Das", "dr\u00e4ngt", "je\u00b7des", "Hin\u00b7der\u00b7nis", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Komm, mein Lieb, und f\u00fcrchte dich nicht,", "tokens": ["Komm", ",", "mein", "Lieb", ",", "und", "f\u00fcrch\u00b7te", "dich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Komm doch, das Gl\u00fcck das lacht;", "tokens": ["Komm", "doch", ",", "das", "Gl\u00fcck", "das", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwei Machangeln, schwarz und dicht,", "tokens": ["Zwei", "Ma\u00b7chan\u00b7geln", ",", "schwarz", "und", "dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halten treuliche Wacht.", "tokens": ["Hal\u00b7ten", "treu\u00b7li\u00b7che", "Wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}