{"textgrid.poem.36246": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wasserfall", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn langsam Welle sich an Welle schlie\u00dfet,", "tokens": ["Wenn", "lang\u00b7sam", "Wel\u00b7le", "sich", "an", "Wel\u00b7le", "schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im breiten Bette flie\u00dfet still das Leben,", "tokens": ["Im", "brei\u00b7ten", "Bet\u00b7te", "flie\u00b7\u00dfet", "still", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird jeder Wunsch verschweben in den einen:", "tokens": ["Wird", "je\u00b7der", "Wunsch", "ver\u00b7schwe\u00b7ben", "in", "den", "ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "APPR", "ART", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts soll des Daseins reinen Flu\u00df dir st\u00f6ren.", "tokens": ["Nichts", "soll", "des", "Da\u00b7seins", "rei\u00b7nen", "Flu\u00df", "dir", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "L\u00e4\u00dft du dein Herz bet\u00f6ren durch die Liebe,", "tokens": ["L\u00e4\u00dft", "du", "dein", "Herz", "be\u00b7t\u00f6\u00b7ren", "durch", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So werden alle Triebe, losgelassen,", "tokens": ["So", "wer\u00b7den", "al\u00b7le", "Trie\u00b7be", ",", "los\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Kraft in vollen Massen sich entladen,", "tokens": ["Der", "Kraft", "in", "vol\u00b7len", "Mas\u00b7sen", "sich", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df unten tief sich baden die Gef\u00fchle,", "tokens": ["Da\u00df", "un\u00b7ten", "tief", "sich", "ba\u00b7den", "die", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Im buntesten Gew\u00fchle wilder rauschen,", "tokens": ["Im", "bun\u00b7tes\u00b7ten", "Ge\u00b7w\u00fch\u00b7le", "wil\u00b7der", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Bis ferne M\u00e4nner lauschen, und voll Bangen", "tokens": ["Bis", "fer\u00b7ne", "M\u00e4n\u00b7ner", "lau\u00b7schen", ",", "und", "voll", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das nah zu sehn verlangen, was mit Grausen", "tokens": ["Das", "nah", "zu", "sehn", "ver\u00b7lan\u00b7gen", ",", "was", "mit", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "PTKZU", "VVINF", "VVINF", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Seel' erf\u00fcllt im Sausen solcher Wogen,", "tokens": ["Die", "Seel'", "er\u00b7f\u00fcllt", "im", "Sau\u00b7sen", "sol\u00b7cher", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die manchen schon betrogen, und nicht ruhten,", "tokens": ["Die", "man\u00b7chen", "schon", "be\u00b7tro\u00b7gen", ",", "und", "nicht", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVPP", "$,", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bis tiefer in die Fluten ew'ger Leiden", "tokens": ["Bis", "tie\u00b7fer", "in", "die", "Flu\u00b7ten", "ew'\u00b7ger", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Verschlungen sie die beiden, die vereinet", "tokens": ["Ver\u00b7schlun\u00b7gen", "sie", "die", "bei\u00b7den", ",", "die", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "PIAT", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Im Silberschaum den s\u00fc\u00dfen Tod beweinet.", "tokens": ["Im", "Sil\u00b7ber\u00b7schaum", "den", "s\u00fc\u00b7\u00dfen", "Tod", "be\u00b7wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}