{"textgrid.poem.59531": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kind wollt' Blumen pfl\u00fccken gehn", "tokens": ["Ein", "Kind", "wollt'", "Blu\u00b7men", "pfl\u00fc\u00b7cken", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Morgens fr\u00fch im Taue,", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", "im", "Tau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tausend Bl\u00fcmlein bunt und sch\u00f6n", "tokens": ["Und", "tau\u00b7send", "Bl\u00fcm\u00b7lein", "bunt", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entbl\u00fchten auf der Aue;", "tokens": ["Ent\u00b7bl\u00fch\u00b7ten", "auf", "der", "Au\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lenz war es rings und Sonnenschein,", "tokens": ["Lenz", "war", "es", "rings", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und alle Bl\u00fcmlein gro\u00df und klein", "tokens": ["Und", "al\u00b7le", "Bl\u00fcm\u00b7lein", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Standen da in s\u00fc\u00dfer Freude.", "tokens": ["Stan\u00b7den", "da", "in", "s\u00fc\u00b7\u00dfer", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als das Kindlein tritt ins Feld,", "tokens": ["Und", "als", "das", "Kin\u00b7dlein", "tritt", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00fcmlein werden munter,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "wer\u00b7den", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jedes gleich sein K\u00f6pfchen h\u00e4lt", "tokens": ["Und", "je\u00b7des", "gleich", "sein", "K\u00f6pf\u00b7chen", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaufw\u00e4rts und hinunter,", "tokens": ["Hin\u00b7auf\u00b7w\u00e4rts", "und", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohin des Kindchens H\u00e4ndchen langt:", "tokens": ["Wo\u00b7hin", "des", "Kind\u00b7chens", "H\u00e4nd\u00b7chen", "langt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jedes Bl\u00fcmlein sehr verlangt", "tokens": ["Ein", "je\u00b7des", "Bl\u00fcm\u00b7lein", "sehr", "ver\u00b7langt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seiner Hand zu sterben.", "tokens": ["In", "sei\u00b7ner", "Hand", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich tritt ein Engel wei\u00df", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "tritt", "ein", "En\u00b7gel", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar freundlich zwischen beide", "tokens": ["Gar", "freund\u00b7lich", "zwi\u00b7schen", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und spricht: \u00bbGegr\u00fc\u00dft der Jugend Preis!", "tokens": ["Und", "spricht", ":", "\u00bb", "Ge\u00b7gr\u00fc\u00dft", "der", "Ju\u00b7gend", "Preis", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Bl\u00fcmlein auf der Heide!", "tokens": ["Und", "Bl\u00fcm\u00b7lein", "auf", "der", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Himmelslust und Himmelschein,", "tokens": ["Voll", "Him\u00b7mels\u00b7lust", "und", "Him\u00b7mel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von innen und von au\u00dfen rein,", "tokens": ["Von", "in\u00b7nen", "und", "von", "au\u00b7\u00dfen", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Blumen sch\u00f6n und fromme Kinder!", "tokens": ["Blu\u00b7men", "sch\u00f6n", "und", "from\u00b7me", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Willkommen, Veilchen still und zart!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Veil\u00b7chen", "still", "und", "zart", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen, Lilie reine!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Li\u00b7lie", "rei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und du, von K\u00f6niginnenart", "tokens": ["Und", "du", ",", "von", "K\u00f6\u00b7ni\u00b7gin\u00b7nen\u00b7art"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und K\u00f6nigin alleine,", "tokens": ["Und", "K\u00f6\u00b7ni\u00b7gin", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du Rose, hohes Purpurrot!", "tokens": ["Du", "Ro\u00b7se", ",", "ho\u00b7hes", "Pur\u00b7pur\u00b7rot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch, Holde, segne alle Gott,", "tokens": ["Euch", ",", "Hol\u00b7de", ",", "seg\u00b7ne", "al\u00b7le", "Gott", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie er dies Kindlein segnet!\u00ab", "tokens": ["Wie", "er", "dies", "Kin\u00b7dlein", "seg\u00b7net", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PDS", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er drauf das Kindlein freundlich k\u00fc\u00dft", "tokens": ["Er", "drauf", "das", "Kin\u00b7dlein", "freund\u00b7lich", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00dft die Blumen sch\u00f6ne,", "tokens": ["Und", "k\u00fc\u00dft", "die", "Blu\u00b7men", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann rauscht er, wie er kommen ist,", "tokens": ["Dann", "rauscht", "er", ",", "wie", "er", "kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin wie Saitent\u00f6ne.", "tokens": ["Da\u00b7hin", "wie", "Sai\u00b7ten\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Kindlein schaut ihm br\u00fcnstig nach", "tokens": ["Das", "Kin\u00b7dlein", "schaut", "ihm", "br\u00fcns\u00b7tig", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lauscht den Worten, die er sprach,", "tokens": ["Und", "lauscht", "den", "Wor\u00b7ten", ",", "die", "er", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ruft: \u00bbAch! komm doch wieder!\u00ab", "tokens": ["Und", "ruft", ":", "\u00bb", "Ach", "!", "komm", "doch", "wie\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ITJ", "$.", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als er doch nicht wiederk\u00f6mmt,", "tokens": ["Und", "als", "er", "doch", "nicht", "wie\u00b7der\u00b7k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So geht es traurig weiter,", "tokens": ["So", "geht", "es", "trau\u00b7rig", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nichts die hei\u00dfen Tr\u00e4nen hemmt,", "tokens": ["Und", "nichts", "die", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", "hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die fallen auf die Kr\u00e4uter", "tokens": ["Die", "fal\u00b7len", "auf", "die", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und auf die Blumen ringsumher;", "tokens": ["Und", "auf", "die", "Blu\u00b7men", "rings\u00b7um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Kindlein wird das Herz so schwer", "tokens": ["Dem", "Kin\u00b7dlein", "wird", "das", "Herz", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und will ihm fast zerbrechen.", "tokens": ["Und", "will", "ihm", "fast", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da, siehe! wie ein Himmelschein", "tokens": ["Da", ",", "sie\u00b7he", "!", "wie", "ein", "Him\u00b7mel\u00b7schein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVIMP", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt ihm ein Glanz entgegen,", "tokens": ["F\u00e4llt", "ihm", "ein", "Glanz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schie\u00dft ein helles Kr\u00e4nzelein", "tokens": ["Es", "schie\u00dft", "ein", "hel\u00b7les", "Kr\u00e4n\u00b7zel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab als Himmelssegen", "tokens": ["Her\u00b7ab", "als", "Him\u00b7mels\u00b7se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und f\u00e4llt dem Kindlein in den Scho\u00df,", "tokens": ["Und", "f\u00e4llt", "dem", "Kin\u00b7dlein", "in", "den", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm wird das Herz in Freuden gro\u00df", "tokens": ["Ihm", "wird", "das", "Herz", "in", "Freu\u00b7den", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wohl ob dem lieben Kr\u00e4nzel.", "tokens": ["Wohl", "ob", "dem", "lie\u00b7ben", "Kr\u00e4n\u00b7zel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und diesen Kranz von Engelhand", "tokens": ["Und", "die\u00b7sen", "Kranz", "von", "En\u00b7gel\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kindlein hat getragen,", "tokens": ["Das", "Kin\u00b7dlein", "hat", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solang es ging im Erdentand,", "tokens": ["So\u00b7lang", "es", "ging", "im", "Er\u00b7den\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In N\u00e4chten und an Tagen.", "tokens": ["In", "N\u00e4ch\u00b7ten", "und", "an", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Kr\u00e4nzlein sch\u00f6n von Himmelsart", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "sch\u00f6n", "von", "Him\u00b7mel\u00b7sart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat wei\u00df und rein das Kind bewahrt", "tokens": ["Hat", "wei\u00df", "und", "rein", "das", "Kind", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVFIN", "KON", "ADJD", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihm das Herz beh\u00fctet.", "tokens": ["Und", "ihm", "das", "Herz", "be\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sooft nun Kinder Blumen sehn,", "tokens": ["Sooft", "nun", "Kin\u00b7der", "Blu\u00b7men", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie solln des Engels denken,", "tokens": ["Sie", "solln", "des", "En\u00b7gels", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihnen auch er wolle sch\u00f6n", "tokens": ["Da\u00df", "ih\u00b7nen", "auch", "er", "wol\u00b7le", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein solches Kr\u00e4nzel schenken.", "tokens": ["Ein", "sol\u00b7ches", "Kr\u00e4n\u00b7zel", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Erdenblumen spielt der Wind,", "tokens": ["Mit", "Er\u00b7den\u00b7blu\u00b7men", "spielt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Blumen, die vom Himmel sind,", "tokens": ["Doch", "Blu\u00b7men", ",", "die", "vom", "Him\u00b7mel", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die bl\u00fchen unverg\u00e4nglich.", "tokens": ["Die", "bl\u00fc\u00b7hen", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}