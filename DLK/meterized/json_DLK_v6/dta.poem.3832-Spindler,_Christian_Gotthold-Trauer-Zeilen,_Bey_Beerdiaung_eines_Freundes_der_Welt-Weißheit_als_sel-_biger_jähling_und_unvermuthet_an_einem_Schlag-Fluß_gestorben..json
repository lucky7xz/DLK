{"dta.poem.3832": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Zeilen,  \n Bey Beerdiaung eines Freundes der Welt-Wei\u00dfheit/ als sel-  \n biger j\u00e4hling und unvermuthet an einem Schlag-Flu\u00df  \n gestorben.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Elende Sterblichkeit! ach! alles ist verlohren,", "tokens": ["E\u00b7len\u00b7de", "Sterb\u00b7lich\u00b7keit", "!", "ach", "!", "al\u00b7les", "ist", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "XY", "$.", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Mensch, der arme Mensch, wird ja zum", "tokens": ["Der", "Mensch", ",", "der", "ar\u00b7me", "Mensch", ",", "wird", "ja", "zum"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tod gebohren!", "tokens": ["Tod", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sein Leben ist ein Schaum, der j\u00e4h und pl\u00f6tzlich", "tokens": ["Sein", "Le\u00b7ben", "ist", "ein", "Schaum", ",", "der", "j\u00e4h", "und", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "weicht,", "tokens": ["weicht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Bevor er sich kaum recht dem Augen-Lichte zeigt.", "tokens": ["Be\u00b7vor", "er", "sich", "kaum", "recht", "dem", "Au\u00b7gen\u00b7Lich\u00b7te", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er ist ein blosses Nichts, ein Nichts der schn\u00f6den", "tokens": ["Er", "ist", "ein", "blos\u00b7ses", "Nichts", ",", "ein", "Nichts", "der", "schn\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erden,", "tokens": ["Er\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und mu\u00df, so bald er stirbt, Asch, Staub und Mo-", "tokens": ["Und", "mu\u00df", ",", "so", "bald", "er", "stirbt", ",", "Asch", ",", "Staub", "und", "Mo"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "NE", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "der werden.", "tokens": ["der", "wer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die H\u00fclle seines Geists dient ihm zum Sterbe-Kleid,", "tokens": ["Die", "H\u00fcl\u00b7le", "sei\u00b7nes", "Geists", "dient", "ihm", "zum", "Ster\u00b7be\u00b7Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "Mit der Erinnerung: Dein Ende ist nicht weit.", "tokens": ["Mit", "der", "E\u00b7rin\u00b7ne\u00b7rung", ":", "Dein", "En\u00b7de", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00e4llt nun der C\u00f6rper hin, so kom\u0303t ein banges Weine\u0303,", "tokens": ["F\u00e4llt", "nun", "der", "C\u00f6r\u00b7per", "hin", ",", "so", "kom\u0303t", "ein", "ban\u00b7ges", "Wein\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da klagt man seinen Freund bey morschen Todes-", "tokens": ["Da", "klagt", "man", "sei\u00b7nen", "Freund", "bey", "mor\u00b7schen", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Beinen,", "tokens": ["Bei\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Da ist kein eintziger, er sey auch wer er sey,", "tokens": ["Da", "ist", "kein", "eint\u00b7zi\u00b7ger", ",", "er", "sey", "auch", "wer", "er", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "$,", "PPER", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von Baare, Sarg und Grufft, vom Tod und", "tokens": ["Von", "Baa\u00b7re", ",", "Sarg", "und", "Grufft", ",", "vom", "Tod", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$,", "APPRART", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sterben frey.", "tokens": ["Ster\u00b7ben", "frey", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Di\u00df zeigt sich auch an dir, o Freund! der Wei\u00df-", "tokens": ["Di\u00df", "zeigt", "sich", "auch", "an", "dir", ",", "o", "Freund", "!", "der", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "heits-Lehren,", "tokens": ["heits\u00b7Leh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Auch da will sich der Tod an keine Gr\u00fcnde kehren.", "tokens": ["Auch", "da", "will", "sich", "der", "Tod", "an", "kei\u00b7ne", "Gr\u00fcn\u00b7de", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du suchtest, wie gewohnt, die Lehre der Natur,", "tokens": ["Du", "such\u00b7test", ",", "wie", "ge\u00b7wohnt", ",", "die", "Leh\u00b7re", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der grossen Welt-Wei\u00dfheit; du folgtest ihrer Spur,", "tokens": ["Der", "gros\u00b7sen", "Welt\u00b7Wei\u00df\u00b7heit", ";", "du", "folg\u00b7test", "ih\u00b7rer", "Spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Du trachtetst sie mit Flei\u00df und Nachdruck zu er-", "tokens": ["Du", "trach\u00b7tetst", "sie", "mit", "Flei\u00df", "und", "Nach\u00b7druck", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "wegen,", "tokens": ["we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Was Wolff in Schrifften sagt, kontst du zum", "tokens": ["Was", "Wolff", "in", "Schriff\u00b7ten", "sagt", ",", "kontst", "du", "zum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NE", "APPR", "NN", "VVFIN", "$,", "VMFIN", "PPER", "APPRART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Grunde legen.", "tokens": ["Grun\u00b7de", "le\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Was dieser grosse Mann in seinen Schrifften sprach,", "tokens": ["Was", "die\u00b7ser", "gros\u00b7se", "Mann", "in", "sei\u00b7nen", "Schriff\u00b7ten", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dem sonnest du mit Flei\u00df, mit gr\u00f6stem Eifer nach.", "tokens": ["Dem", "son\u00b7nest", "du", "mit", "Flei\u00df", ",", "mit", "gr\u00f6s\u00b7tem", "Ei\u00b7fer", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Di\u00df war dein Augenmerck, woran du fest gebunden,", "tokens": ["Di\u00df", "war", "dein", "Au\u00b7gen\u00b7merck", ",", "wo\u00b7ran", "du", "fest", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und so vergieng die Zeit in Anmuths-vollen Stunde\u0303.", "tokens": ["Und", "so", "ver\u00b7gieng", "die", "Zeit", "in", "An\u00b7muths\u00b7vol\u00b7len", "Stun\u00b7d\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und letztens, als du schon die Helffte einer Nacht", "tokens": ["Und", "letz\u00b7tens", ",", "als", "du", "schon", "die", "Helff\u00b7te", "ei\u00b7ner", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "In der Besch\u00e4fftigung mit Anmuth zugebracht,", "tokens": ["In", "der", "Be\u00b7sch\u00e4ff\u00b7ti\u00b7gung", "mit", "An\u00b7muth", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So mu\u00df ein Schlag-Flu\u00df dich entseelt zu Boden", "tokens": ["So", "mu\u00df", "ein", "Schlag\u00b7Flu\u00df", "dich", "ent\u00b7seelt", "zu", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "schmeissen,", "tokens": ["schmeis\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Und uns, o herber Fall! den besten Freund entreissen.", "tokens": ["Und", "uns", ",", "o", "her\u00b7ber", "Fall", "!", "den", "bes\u00b7ten", "Freund", "en\u00b7treis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn dorten ", "tokens": ["Wenn", "dor\u00b7ten"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Ob schon die gantze Stadt die Wuth der Feinde flieht,", "tokens": ["Ob", "schon", "die", "gant\u00b7ze", "Stadt", "die", "Wuth", "der", "Fein\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bleibt er doch unbewegt, er l\u00e4st sich nichts verhindern", "tokens": ["Bleibt", "er", "doch", "un\u00b7be\u00b7wegt", ",", "er", "l\u00e4st", "sich", "nichts", "ver\u00b7hin\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PRF", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Auch Lermen und Geschrey kan nicht das Zirckeln", "tokens": ["Auch", "Ler\u00b7men", "und", "Ge\u00b7schrey", "kan", "nicht", "das", "Zir\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VMFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "mindern,", "tokens": ["min\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Bis eine freche Faust den Zirckel ihm entrei\u00dft,", "tokens": ["Bis", "ei\u00b7ne", "fre\u00b7che", "Faust", "den", "Zir\u00b7ckel", "ihm", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und ihn mit Ungest\u00fcm erbla\u00dft zu Boden schmei\u00dft.", "tokens": ["Und", "ihn", "mit", "Un\u00b7ge\u00b7st\u00fcm", "er\u00b7bla\u00dft", "zu", "Bo\u00b7den", "schmei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So eben gieng es dir, kaum eh du dichs versehen,", "tokens": ["So", "e\u00b7ben", "gieng", "es", "dir", ",", "kaum", "eh", "du", "dichs", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So ists durch einen Schlag, ", "tokens": ["So", "ists", "durch", "ei\u00b7nen", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "geschehen.", "tokens": ["ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Da liegst du nun erstarrt; du denckst den C\u00f6rpern", "tokens": ["Da", "liegst", "du", "nun", "er\u00b7starrt", ";", "du", "denckst", "den", "C\u00f6r\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "nach,", "tokens": ["nach", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+", "measure": "single.up"}, "line.49": {"text": "Und da du kaum gedacht, findst du dein Schlaf-", "tokens": ["Und", "da", "du", "kaum", "ge\u00b7dacht", ",", "findst", "du", "dein", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Gemach.", "tokens": ["Ge\u00b7mach", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "lin