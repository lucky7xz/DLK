{"textgrid.poem.52641": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist mir oft der Wunsch gekommen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist mir oft der Wunsch gekommen", "tokens": ["Ist", "mir", "oft", "der", "Wunsch", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abzusch\u00fctteln diese Glieder,", "tokens": ["Ab\u00b7zu\u00b7sch\u00fct\u00b7teln", "die\u00b7se", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Herz voll Sturm und Wunden \u2013", "tokens": ["Die\u00b7ses", "Herz", "voll", "Sturm", "und", "Wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid mir theuer, bittre Stunden,", "tokens": ["Seid", "mir", "theu\u00b7er", ",", "bitt\u00b7re", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber kehret niemals wieder!", "tokens": ["A\u00b7ber", "keh\u00b7ret", "nie\u00b7mals", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst du zwischen Zeilen lesen,", "tokens": ["Kannst", "du", "zwi\u00b7schen", "Zei\u00b7len", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht es flammend dir geschrieben:", "tokens": ["Steht", "es", "flam\u00b7mend", "dir", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Wahnsinn flucht dem Leben,", "tokens": ["Nur", "der", "Wahn\u00b7sinn", "flucht", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur den Thoren macht es beben \u2013", "tokens": ["Nur", "den", "Tho\u00b7ren", "macht", "es", "be\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wers begriffen, wird es lieben.", "tokens": ["Wers", "be\u00b7grif\u00b7fen", ",", "wird", "es", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}