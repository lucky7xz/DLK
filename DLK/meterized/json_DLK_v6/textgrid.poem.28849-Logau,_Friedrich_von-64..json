{"textgrid.poem.28849": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo viel Gemeinschafft ist, ist Ansehn nicht gemein;", "tokens": ["Wo", "viel", "Ge\u00b7mein\u00b7schafft", "ist", ",", "ist", "An\u00b7sehn", "nicht", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "VAFIN", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Ansehn mehr nicht ist, wil auch nicht Folge seyn;", "tokens": ["Wo", "An\u00b7sehn", "mehr", "nicht", "ist", ",", "wil", "auch", "nicht", "Fol\u00b7ge", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PTKNEG", "VAFIN", "$,", "VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Folge reisset au\u00df, kan Ordnung nicht bestehen;", "tokens": ["Wo", "Fol\u00b7ge", "reis\u00b7set", "au\u00df", ",", "kan", "Ord\u00b7nung", "nicht", "be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,", "VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Ordnung nicht besteht, mu\u00df Wolfahrt untergehen.", "tokens": ["Wo", "Ord\u00b7nung", "nicht", "be\u00b7steht", ",", "mu\u00df", "Wol\u00b7fahrt", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "VVFIN", "$,", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}