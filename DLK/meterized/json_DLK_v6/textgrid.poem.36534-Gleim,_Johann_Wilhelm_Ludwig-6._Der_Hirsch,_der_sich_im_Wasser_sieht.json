{"textgrid.poem.36534": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Der Hirsch, der sich im Wasser sieht", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hirsch bewunderte sein pr\u00e4chtiges Geweih", "tokens": ["Ein", "Hirsch", "be\u00b7wun\u00b7der\u00b7te", "sein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7weih"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und als er sie besieht, mit ernstlichem Gesicht,", "tokens": ["Und", "als", "er", "sie", "be\u00b7sieht", ",", "mit", "ernst\u00b7li\u00b7chem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rt er im nahen Busch ein J\u00e4gerhorn erschallen,", "tokens": ["H\u00f6rt", "er", "im", "na\u00b7hen", "Busch", "ein", "J\u00e4\u00b7ger\u00b7horn", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieht eine Jagd von dem Gebirge fallen,", "tokens": ["Sieht", "ei\u00b7ne", "Jagd", "von", "dem", "Ge\u00b7bir\u00b7ge", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschrickt und flieht! Nun aber hilft ihm nicht", "tokens": ["Er\u00b7schrickt", "und", "flieht", "!", "Nun", "a\u00b7ber", "hilft", "ihm", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "$.", "ADV", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das pr\u00e4chtige Geweih dem nahen Tod entfliehn,", "tokens": ["Das", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7weih", "dem", "na\u00b7hen", "Tod", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht sein vollkommner Leib, die Beine retten ihn!", "tokens": ["Nicht", "sein", "voll\u00b7komm\u00b7ner", "Leib", ",", "die", "Bei\u00b7ne", "ret\u00b7ten", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die rei\u00dfen, wie ein Pfeil, die pr\u00e4chtige Gestalt", "tokens": ["Die", "rei\u00b7\u00dfen", ",", "wie", "ein", "Pfeil", ",", "die", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit sich durchs weite Feld, und fliegen in den Wald!", "tokens": ["Mit", "sich", "durchs", "wei\u00b7te", "Feld", ",", "und", "flie\u00b7gen", "in", "den", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier aber halten ihn, im vogelschnellen Lauf,", "tokens": ["Hier", "a\u00b7ber", "hal\u00b7ten", "ihn", ",", "im", "vo\u00b7gel\u00b7schnel\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An starken Zweigen oft die vierzehn Enden auf.", "tokens": ["An", "star\u00b7ken", "Zwei\u00b7gen", "oft", "die", "vier\u00b7zehn", "En\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er rei\u00dft sich los, er flucht darauf;", "tokens": ["Er", "rei\u00dft", "sich", "los", ",", "er", "flucht", "da\u00b7rauf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Lobt seine Beine nun, und lernet noch im Fliehn,", "tokens": ["Lobt", "sei\u00b7ne", "Bei\u00b7ne", "nun", ",", "und", "ler\u00b7net", "noch", "im", "Fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}