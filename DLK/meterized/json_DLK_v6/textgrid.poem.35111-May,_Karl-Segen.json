{"textgrid.poem.35111": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Segen", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schlehdorn steht in Bl\u00fcthen,", "tokens": ["Der", "Schleh\u00b7dorn", "steht", "in", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun da ich scheiden mu\u00df.", "tokens": ["Nun", "da", "ich", "schei\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwalbe aus dem S\u00fcden", "tokens": ["Die", "Schwal\u00b7be", "aus", "dem", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringt mir den Abschiedsgru\u00df.", "tokens": ["Bringt", "mir", "den", "Ab\u00b7schieds\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Schlehdorn steht in Bl\u00fcthen;", "tokens": ["Der", "Schleh\u00b7dorn", "steht", "in", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bl\u00fchst, mein Kind, auch du.", "tokens": ["So", "bl\u00fchst", ",", "mein", "Kind", ",", "auch", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Brich sie f\u00fcr mich, dem M\u00fcden,", "tokens": ["Brich", "sie", "f\u00fcr", "mich", ",", "dem", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deck mich mit ihnen zu.", "tokens": ["Deck", "mich", "mit", "ih\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der Schlehdorn steht in Bl\u00fcthen;", "tokens": ["Der", "Schleh\u00b7dorn", "steht", "in", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Welch eine s\u00fc\u00dfe Last.", "tokens": ["Welch", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag dich der Herr beh\u00fcten,", "tokens": ["Mag", "dich", "der", "Herr", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du mich nicht mehr hast!", "tokens": ["Wenn", "du", "mich", "nicht", "mehr", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}