{"textgrid.poem.50547": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "12. Welscher von einem Frantzosen ausgefodert", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fodert' ein ", "tokens": ["Es", "fo\u00b7dert'", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Tag war angesetzt, und doch ward nichts daraus;", "tokens": ["Der", "Tag", "war", "an\u00b7ge\u00b7setzt", ",", "und", "doch", "ward", "nichts", "da\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ADV", "VAFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn als sie auf der ", "tokens": ["Denn", "als", "sie", "auf", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So wolte dieser hier die ", "tokens": ["So", "wol\u00b7te", "die\u00b7ser", "hier", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDS", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Ursach die ist diss, dieweil euch nicht geb\u00fchrt,", "tokens": ["Die", "Ur\u00b7sach", "die", "ist", "diss", ",", "die\u00b7weil", "euch", "nicht", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PDS", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Freund, dass ihr was ich im ", "tokens": ["Mein", "Freund", ",", "dass", "ihr", "was", "ich", "im"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PWS", "PPER", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was f\u00fchrt ihr denn im Schild'? Es ist ein", "tokens": ["Was", "f\u00fchrt", "ihr", "denn", "im", "Schild'", "?", "Es", "ist", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.8": {"text": "O, sagt' hierauf, der arme Tropf,", "tokens": ["O", ",", "sagt'", "hier\u00b7auf", ",", "der", "ar\u00b7me", "Tropf", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PAV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn diesem also ist, so stellet euch zur Ruhe;", "tokens": ["Wenn", "die\u00b7sem", "al\u00b7so", "ist", ",", "so", "stel\u00b7let", "euch", "zur", "Ru\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn was ich f\u00fchr' ist nur ein ", "tokens": ["Denn", "was", "ich", "f\u00fchr'", "ist", "nur", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "ADV", "ART"], "meter": "+---+-+", "measure": "dactylic.init"}}}}}