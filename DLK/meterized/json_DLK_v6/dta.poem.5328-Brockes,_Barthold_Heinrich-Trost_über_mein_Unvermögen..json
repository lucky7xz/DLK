{"dta.poem.5328": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost \u00fcber mein Unverm\u00f6gen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Auszug sch\u00f6ner W\u00e4lder,", "tokens": ["In", "ei\u00b7nem", "Aus\u00b7zug", "sch\u00f6\u00b7ner", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worin so gar die gr\u00fcnen Schatten gl\u00e4ntzten,", "tokens": ["Wo\u00b7rin", "so", "gar", "die", "gr\u00fc\u00b7nen", "Schat\u00b7ten", "gl\u00e4ntz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den fast nicht abzusehnde Felder,", "tokens": ["Den", "fast", "nicht", "ab\u00b7zu\u00b7sehn\u00b7de", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie ein g\u00fcldnes Meer, begr\u00e4ntzten,", "tokens": ["Als", "wie", "ein", "g\u00fcld\u00b7nes", "Meer", ",", "be\u00b7gr\u00e4ntz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Besch\u00e4ftigt\u2019 ich mich j\u00fcngst, der sch\u00f6nen B\u00e4ume Pracht,", "tokens": ["Be\u00b7sch\u00e4f\u00b7tigt'", "ich", "mich", "j\u00fcngst", ",", "der", "sch\u00f6\u00b7nen", "B\u00e4u\u00b7me", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Ehren dem, der sie gemacht,", "tokens": ["Zu", "Eh\u00b7ren", "dem", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit sch\u00f6nen Worten zu beschreiben.", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Wor\u00b7ten", "zu", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Trotz aller meiner M\u00fch,", "tokens": ["Trotz", "al\u00b7ler", "mei\u00b7ner", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Weil die entworffene Copie", "tokens": ["Weil", "die", "ent\u00b7worf\u00b7fe\u00b7ne", "Co\u00b7pie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dem Urbild \u00fcberall nicht glich,", "tokens": ["Dem", "Ur\u00b7bild", "\u00fc\u00b7be\u00b7rall", "nicht", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Must alles unterbleiben.", "tokens": ["Must", "al\u00b7les", "un\u00b7ter\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Doch fiel mir die\u00df dar\u00fcber ein:", "tokens": ["Doch", "fiel", "mir", "die\u00df", "da\u00b7r\u00fc\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Indem ich von der B\u00e4ume Bildern", "tokens": ["In\u00b7dem", "ich", "von", "der", "B\u00e4u\u00b7me", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Sch\u00f6nheit nicht vermag zu schildern,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "nicht", "ver\u00b7mag", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht w\u00fcrdig sie beschreiben kann:", "tokens": ["Nicht", "w\u00fcr\u00b7dig", "sie", "be\u00b7schrei\u00b7ben", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So bin ich darum nicht betr\u00fcbet;", "tokens": ["So", "bin", "ich", "da\u00b7rum", "nicht", "be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dieweil es mir die Nachricht giebet:", "tokens": ["Die\u00b7weil", "es", "mir", "die", "Nach\u00b7richt", "gie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mit unserm Witz sey nichts gethan.", "tokens": ["Mit", "un\u00b7serm", "Witz", "sey", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mein Unverm\u00f6gen zeigts zwar an;", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "zeigts", "zwar", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch dien\u2019 ich auch auf diese Weise,", "tokens": ["Doch", "dien'", "ich", "auch", "auf", "die\u00b7se", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Mit meiner Schwachheit, GOtt zum Preise;", "tokens": ["Mit", "mei\u00b7ner", "Schwach\u00b7heit", ",", "Gott", "zum", "Prei\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Weil es doch immer wahr wird bleiben,", "tokens": ["Weil", "es", "doch", "im\u00b7mer", "wahr", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Was ich je mehr und mehr vermercke,", "tokens": ["Was", "ich", "je", "mehr", "und", "mehr", "ver\u00b7mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df unsers grossen Sch\u00f6pfers Wercke", "tokens": ["Da\u00df", "un\u00b7sers", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Nach W\u00fcrden nimmer zu beschreiben.", "tokens": ["Nach", "W\u00fcr\u00b7den", "nim\u00b7mer", "zu", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}