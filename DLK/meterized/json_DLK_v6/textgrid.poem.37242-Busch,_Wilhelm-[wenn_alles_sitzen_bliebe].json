{"textgrid.poem.37242": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[wenn alles sitzen bliebe]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alles sitzen bliebe,", "tokens": ["Wenn", "al\u00b7les", "sit\u00b7zen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wir in Ha\u00df und Liebe", "tokens": ["Was", "wir", "in", "Ha\u00df", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So voneinander schwatzen;", "tokens": ["So", "von\u00b7ein\u00b7an\u00b7der", "schwat\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn L\u00fcgen Haare w\u00e4ren,", "tokens": ["Wenn", "L\u00fc\u00b7gen", "Haa\u00b7re", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir w\u00e4ren rauh wie B\u00e4ren", "tokens": ["Wir", "w\u00e4\u00b7ren", "rauh", "wie", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und h\u00e4tten keine Glatzen.", "tokens": ["Und", "h\u00e4t\u00b7ten", "kei\u00b7ne", "Glat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}