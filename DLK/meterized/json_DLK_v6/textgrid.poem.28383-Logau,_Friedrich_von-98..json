{"textgrid.poem.28383": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seither de\u00df Krieges Arg das Gute fast vertrieben,", "tokens": ["Sei\u00b7ther", "de\u00df", "Krie\u00b7ges", "Arg", "das", "Gu\u00b7te", "fast", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So ist uns, wahrer Freund, di\u00df einig \u00fcberblieben,", "tokens": ["So", "ist", "uns", ",", "wah\u00b7rer", "Freund", ",", "di\u00df", "ei\u00b7nig", "\u00fc\u00b7berb\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "PDS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das lieblich heissen mag: wir zeugen Kind auff Kind,", "tokens": ["Das", "lieb\u00b7lich", "heis\u00b7sen", "mag", ":", "wir", "zeu\u00b7gen", "Kind", "auff", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Denckmahl hinter uns, das wir gewesen sind.", "tokens": ["Ein", "Denck\u00b7mahl", "hin\u00b7ter", "uns", ",", "das", "wir", "ge\u00b7we\u00b7sen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gut, gut! was kan uns sonst au\u00df Wermut Zucker machen,", "tokens": ["Gut", ",", "gut", "!", "was", "kan", "uns", "sonst", "au\u00df", "Wer\u00b7mut", "Zu\u00b7cker", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$.", "PWS", "VMFIN", "PPER", "ADV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wann das liebe Kind mit k\u00fcrmeln und mit lachen", "tokens": ["Als", "wann", "das", "lie\u00b7be", "Kind", "mit", "k\u00fcr\u00b7meln", "und", "mit", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "ART", "ADJA", "NN", "APPR", "VVINF", "KON", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An unser Haupt sich dr\u00fcckt, uns lieber Vater nennt", "tokens": ["An", "un\u00b7ser", "Haupt", "sich", "dr\u00fcckt", ",", "uns", "lie\u00b7ber", "Va\u00b7ter", "nennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "PPER", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und macht, da\u00df man in ihm sich wie im Spiegel kennt.", "tokens": ["Und", "macht", ",", "da\u00df", "man", "in", "ihm", "sich", "wie", "im", "Spie\u00b7gel", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "APPR", "PPER", "PRF", "KOKOM", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie sind die andren wir; wir leben nach dem Leben", "tokens": ["Sie", "sind", "die", "an\u00b7dren", "wir", ";", "wir", "le\u00b7ben", "nach", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In ihnen; unser seyn ist darumb uns gegeben,", "tokens": ["In", "ih\u00b7nen", ";", "un\u00b7ser", "seyn", "ist", "da\u00b7rumb", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PPOSAT", "VAINF", "VAFIN", "PAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Da\u00df sie so k\u00fcnnen seyn, wie wir von denen sind,", "tokens": ["Da\u00df", "sie", "so", "k\u00fcn\u00b7nen", "seyn", ",", "wie", "wir", "von", "de\u00b7nen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VAINF", "$,", "PWAV", "PPER", "APPR", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von welchen wir ererbt den s\u00fcssen Namen Kind.", "tokens": ["Von", "wel\u00b7chen", "wir", "er\u00b7erbt", "den", "s\u00fcs\u00b7sen", "Na\u00b7men", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wolan! wolan mein Freund! so mu\u00df man dann nur d\u00e4mpffen", "tokens": ["Wo\u00b7lan", "!", "wo\u00b7lan", "mein", "Freund", "!", "so", "mu\u00df", "man", "dann", "nur", "d\u00e4mpf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "PIS", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Rauch der bittren Zeit; so mu\u00df man lernen k\u00e4mpffen", "tokens": ["Den", "Rauch", "der", "bit\u00b7tren", "Zeit", ";", "so", "mu\u00df", "man", "ler\u00b7nen", "k\u00e4mpf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "ADV", "VMFIN", "PIS", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit dieser Sterbligkeit, auff da\u00df ihr strenger Krieg", "tokens": ["Mit", "die\u00b7ser", "Ster\u00b7blig\u00b7keit", ",", "auff", "da\u00df", "ihr", "stren\u00b7ger", "Krieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nicht \u00fcber uns erh\u00e4lt so gar geschwinden Sieg!", "tokens": ["Nicht", "\u00fc\u00b7ber", "uns", "er\u00b7h\u00e4lt", "so", "gar", "ge\u00b7schwin\u00b7den", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gott gebe dieses nur, da\u00df kein Kind uns mag gleichen", "tokens": ["Gott", "ge\u00b7be", "die\u00b7ses", "nur", ",", "da\u00df", "kein", "Kind", "uns", "mag", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PDAT", "ADV", "$,", "KOUS", "PIAT", "NN", "PPER", "VMFIN", "VVINF"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Und n\u00e4mlich nicht wie wir f\u00fcr solcher Noth erbleichen!", "tokens": ["Und", "n\u00e4m\u00b7lich", "nicht", "wie", "wir", "f\u00fcr", "sol\u00b7cher", "Noth", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "KOKOM", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach dem, so w\u00fcntsch ich mir, da\u00df nach uns so dein Kind", "tokens": ["Nach", "dem", ",", "so", "w\u00fcnt\u00b7sch", "ich", "mir", ",", "da\u00df", "nach", "uns", "so", "dein", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "ADV", "ADJD", "PPER", "PPER", "$,", "KOUS", "APPR", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und mein Kind, wie auch du und ich, vertreulich sind.", "tokens": ["Und", "mein", "Kind", ",", "wie", "auch", "du", "und", "ich", ",", "ver\u00b7treu\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "ADV", "PPER", "KON", "PPER", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}