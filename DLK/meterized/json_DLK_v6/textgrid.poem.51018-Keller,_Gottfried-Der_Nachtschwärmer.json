{"textgrid.poem.51018": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtschw\u00e4rmer", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von hei\u00dfer Lebenslust entgl\u00fcht,", "tokens": ["Von", "hei\u00b7\u00dfer", "Le\u00b7bens\u00b7lust", "ent\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich das Sommerland durchstreift,", "tokens": ["Hab", "ich", "das", "Som\u00b7mer\u00b7land", "durch\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dar\u00fcber ist der Tag verbl\u00fcht", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ist", "der", "Tag", "ver\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zu der sch\u00f6nsten Nacht gereift.", "tokens": ["Und", "zu", "der", "sch\u00f6ns\u00b7ten", "Nacht", "ge\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich steige auf des Berges R\u00fccken", "tokens": ["Ich", "stei\u00b7ge", "auf", "des", "Ber\u00b7ges", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Kanzel von Granit empor", "tokens": ["Zur", "Kan\u00b7zel", "von", "Gra\u00b7nit", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und beuge mich mit trunknen Blicken", "tokens": ["Und", "beu\u00b7ge", "mich", "mit", "trunk\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In die entschlafne Landschaft vor.", "tokens": ["In", "die", "ent\u00b7schlaf\u00b7ne", "Land\u00b7schaft", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am andern Berge dr\u00fcben steht", "tokens": ["Am", "an\u00b7dern", "Ber\u00b7ge", "dr\u00fc\u00b7ben", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sternenschein der Liebe Haus,", "tokens": ["Im", "Ster\u00b7nen\u00b7schein", "der", "Lie\u00b7be", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem offnen Fenster weht", "tokens": ["Aus", "sei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vorhang in die Nacht hinaus;", "tokens": ["Ein", "Vor\u00b7hang", "in", "die", "Nacht", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist f\u00fcrwahr ein luftig Gitter,", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "ein", "luf\u00b7tig", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das mir das Fr\u00e4ulein dort verschlie\u00dft,", "tokens": ["Das", "mir", "das", "Fr\u00e4u\u00b7lein", "dort", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur schade, da\u00df mir armem Ritter", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "mir", "ar\u00b7mem", "Rit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der tiefe Strom dazwischen flie\u00dft!", "tokens": ["Der", "tie\u00b7fe", "Strom", "da\u00b7zwi\u00b7schen", "flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So will ich ihr ein St\u00e4ndchen bringen,", "tokens": ["So", "will", "ich", "ihr", "ein", "St\u00e4nd\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das weithin durch die L\u00fcfte schallt,", "tokens": ["Das", "weit\u00b7hin", "durch", "die", "L\u00fcf\u00b7te", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spiele du zu meinem Singen,", "tokens": ["Und", "spie\u00b7le", "du", "zu", "mei\u00b7nem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Geist der Nacht, auf Tal und Wald!", "tokens": ["O", "Geist", "der", "Nacht", ",", "auf", "Tal", "und", "Wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Wind la\u00df mit den Tannen kosen,", "tokens": ["Den", "Wind", "la\u00df", "mit", "den", "Tan\u00b7nen", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die wie gespannte Saiten stehn,", "tokens": ["Die", "wie", "ge\u00b7spann\u00b7te", "Sai\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit der Wellen fernem Tosen", "tokens": ["Und", "mit", "der", "Wel\u00b7len", "fer\u00b7nem", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Der Nachtigallen Chor verwehn!", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Chor", "ver\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Osten zieht ein Wetter hin,", "tokens": ["Im", "Os\u00b7ten", "zieht", "ein", "Wet\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das stellen wir als Helfer an:", "tokens": ["Das", "stel\u00b7len", "wir", "als", "Hel\u00b7fer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie leuchtend schwingt sein Tamburin", "tokens": ["Wie", "leuch\u00b7tend", "schwingt", "sein", "Tam\u00b7bu\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Horizonte der Titan!", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "der", "Ti\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die M\u00fchlen sind die Zitherschl\u00e4ger", "tokens": ["Die", "M\u00fch\u00b7len", "sind", "die", "Zi\u00b7ther\u00b7schl\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beim Wassersturz im Felsengrund;", "tokens": ["Beim", "Was\u00b7ser\u00b7sturz", "im", "Fel\u00b7sen\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Wagen f\u00e4hrt mein Fackeltr\u00e4ger", "tokens": ["Im", "Wa\u00b7gen", "f\u00e4hrt", "mein", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hoch vor mir her am Himmelsrund!", "tokens": ["Hoch", "vor", "mir", "her", "am", "Him\u00b7mels\u00b7rund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun will ich singen \u00fcberlaut", "tokens": ["Nun", "will", "ich", "sin\u00b7gen", "\u00fc\u00b7berl\u00b7aut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allem Land, das gr\u00fcnt und bl\u00fcht,", "tokens": ["Vor", "al\u00b7lem", "Land", ",", "das", "gr\u00fcnt", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist kein Turm so hoch gebaut,", "tokens": ["Es", "ist", "kein", "Turm", "so", "hoch", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcberhin mein Sang nicht zieht!", "tokens": ["Da\u00b7r\u00fc\u00b7berh\u00b7in", "mein", "Sang", "nicht", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "So eine k\u00fchne Br\u00fccke schlagend,", "tokens": ["So", "ei\u00b7ne", "k\u00fch\u00b7ne", "Br\u00fc\u00b7cke", "schla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Such ich zu ihrem Ohr den Weg:", "tokens": ["Such", "ich", "zu", "ih\u00b7rem", "Ohr", "den", "Weg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Betritt im Traum das Seelchen zagend", "tokens": ["Be\u00b7tritt", "im", "Traum", "das", "Seel\u00b7chen", "za\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des wilden L\u00e4rmers schwanken Steg?", "tokens": ["Des", "wil\u00b7den", "L\u00e4r\u00b7mers", "schwan\u00b7ken", "Steg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}