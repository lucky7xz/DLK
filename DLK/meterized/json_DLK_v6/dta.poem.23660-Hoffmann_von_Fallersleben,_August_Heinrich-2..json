{"dta.poem.23660": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die aus dem gewaltigen Meer von T\u00f6nen", "tokens": ["Die", "aus", "dem", "ge\u00b7wal\u00b7ti\u00b7gen", "Meer", "von", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Fischen die Perle des Edelen, Sch\u00f6nen,", "tokens": ["Fi\u00b7schen", "die", "Per\u00b7le", "des", "E\u00b7de\u00b7len", ",", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Die uns des Lebens Mi\u00dfklang entw\u00f6hnen,", "tokens": ["Die", "uns", "des", "Le\u00b7bens", "Mi\u00df\u00b7klang", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Allem Jammern, Klagen und St\u00f6hnen,", "tokens": ["Al\u00b7lem", "Jam\u00b7mern", ",", "Kla\u00b7gen", "und", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Uns mit dem Weltgew\u00fchle vers\u00f6hnen,", "tokens": ["Uns", "mit", "dem", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Uns das Leben erheitern, versch\u00f6nen \u2014", "tokens": ["Uns", "das", "Le\u00b7ben", "er\u00b7hei\u00b7tern", ",", "ver\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die, was ein Dichter irgend gesagt hat,", "tokens": ["Die", ",", "was", "ein", "Dich\u00b7ter", "ir\u00b7gend", "ge\u00b7sagt", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Was er gelacht und was er geklagt hat,", "tokens": ["Was", "er", "ge\u00b7lacht", "und", "was", "er", "ge\u00b7klagt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was er zu ahnen kaum gewagt hat,", "tokens": ["Was", "er", "zu", "ah\u00b7nen", "kaum", "ge\u00b7wagt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Rastlos streben und ringen", "tokens": ["Rast\u00b7los", "stre\u00b7ben", "und", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Sch\u00f6ner in T\u00f6nen darzubringen.", "tokens": ["Sch\u00f6\u00b7ner", "in", "T\u00f6\u00b7nen", "dar\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVIZU", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "In allen Herzen mu\u00df das Sch\u00f6n' ersprie\u00dfen,", "tokens": ["In", "al\u00b7len", "Her\u00b7zen", "mu\u00df", "das", "Sch\u00f6n'", "er\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn sie das Sch\u00f6n' in T\u00f6n' ergie\u00dfen;", "tokens": ["Wenn", "sie", "das", "Sch\u00f6n'", "in", "T\u00f6n'", "er\u00b7gie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und wir wollen den Dank im Becherklang bringen,", "tokens": ["Und", "wir", "wol\u00b7len", "den", "Dank", "im", "Be\u00b7cher\u00b7klang", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Wenn sie uns ihren Zechersang singen.", "tokens": ["Wenn", "sie", "uns", "ih\u00b7ren", "Ze\u00b7cher\u00b7sang", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}