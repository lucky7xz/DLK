{"dta.poem.3800": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die altv\u00e4terische und doch treue Liebe,  \n nach Hr. Fr\u00f6schmeuslers Schreib-Art.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm an, du sehr Geliebte fein,", "tokens": ["Nimm", "an", ",", "du", "sehr", "Ge\u00b7lieb\u00b7te", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein klein und treues Briefelein,", "tokens": ["Mein", "klein", "und", "treu\u00b7es", "Brie\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das drob nunmehro wohlgemuth", "tokens": ["Das", "drob", "nun\u00b7meh\u00b7ro", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traun mit viel Liebe kommen thut.", "tokens": ["Traun", "mit", "viel", "Lie\u00b7be", "kom\u00b7men", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nechst thate ich spatziren gahn,", "tokens": ["Nechst", "tha\u00b7te", "ich", "spat\u00b7zi\u00b7ren", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und traff dich unterwegens an,", "tokens": ["Und", "traff", "dich", "un\u00b7ter\u00b7we\u00b7gens", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da hab ich mit dir fort und fort", "tokens": ["Da", "hab", "ich", "mit", "dir", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geredet gar sehr viele Wort.", "tokens": ["Ge\u00b7re\u00b7det", "gar", "sehr", "vie\u00b7le", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du sahst mir so fein ehrlich aus,", "tokens": ["Du", "sahst", "mir", "so", "fein", "ehr\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So balde als ich kam nach Hau\u00df,", "tokens": ["So", "bal\u00b7de", "als", "ich", "kam", "nach", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So habe ich den gantzen Tag", "tokens": ["So", "ha\u00b7be", "ich", "den", "gant\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dir nachgesonn ohn alle Plag.", "tokens": ["Dir", "nach\u00b7ge\u00b7sonn", "ohn", "al\u00b7le", "Plag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kurtzum/ ich find in mir den Trieb", "tokens": ["Kurt\u00b7zum", "/", "ich", "find", "in", "mir", "den", "Trieb"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von einer rein und frommen Lieb,", "tokens": ["Von", "ei\u00b7ner", "rein", "und", "from\u00b7men", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df ich mich hierob fast verga\u00df", "tokens": ["Da\u00df", "ich", "mich", "hier\u00b7ob", "fast", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vor grosser Lieb ohn allen Spa\u00df.", "tokens": ["Vor", "gros\u00b7ser", "Lieb", "ohn", "al\u00b7len", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Daher ich mich erk\u00fchne keck,", "tokens": ["Da\u00b7her", "ich", "mich", "er\u00b7k\u00fch\u00b7ne", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich bitt den Fehler mir zudeck.", "tokens": ["Ich", "bitt", "den", "Feh\u00b7ler", "mir", "zu\u00b7deck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ob du denn wohl mein schlecht Person", "tokens": ["Ob", "du", "denn", "wohl", "mein", "schlecht", "Per\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lieben willst ohn Spott und Hohn.", "tokens": ["Lie\u00b7ben", "willst", "ohn", "Spott", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Denn Liebe auf der b\u00f6sen Welt", "tokens": ["Denn", "Lie\u00b7be", "auf", "der", "b\u00f6\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sonst balde \u00fcbern Hauffen f\u00e4lt.", "tokens": ["Sonst", "bal\u00b7de", "\u00fc\u00b7bern", "Hauf\u00b7fen", "f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wenn nun bald Antwort kommen thut,", "tokens": ["Wenn", "nun", "bald", "Ant\u00b7wort", "kom\u00b7men", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So ist mir traun recht wohlgemuth.", "tokens": ["So", "ist", "mir", "traun", "recht", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ich hab dich lieb von Hertzengrund,", "tokens": ["Ich", "hab", "dich", "lieb", "von", "Hert\u00b7zen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die\u00df bleib ich auch zu jeder Stund.", "tokens": ["Die\u00df", "bleib", "ich", "auch", "zu", "je\u00b7der", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Denn glaube mir nur sicherlich,", "tokens": ["Denn", "glau\u00b7be", "mir", "nur", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mein Hertze liebt dich inniglich.", "tokens": ["Mein", "Hert\u00b7ze", "liebt", "dich", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wenn du nun bilgest meine Sach\u2019n,", "tokens": ["Wenn", "du", "nun", "bil\u00b7gest", "mei\u00b7ne", "Sach'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So wolln wir balde Hochzeit mach\u2019n,", "tokens": ["So", "wolln", "wir", "bal\u00b7de", "Hoch\u00b7zeit", "mach'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Denn heist es alles wohlgethan,", "tokens": ["Denn", "heist", "es", "al\u00b7les", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Zu meiner Freude Lobesan.", "tokens": ["Zu", "mei\u00b7ner", "Freu\u00b7de", "Lo\u00b7be\u00b7san", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Schencke mir nun dein Cr\u00e4ntzelein,", "tokens": ["Schen\u00b7cke", "mir", "nun", "dein", "Cr\u00e4nt\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "O allerliebstes Jungfr\u00e4ulein,", "tokens": ["O", "al\u00b7ler\u00b7liebs\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Du findest bey mir gnug zu zehr\u2019n,", "tokens": ["Du", "fin\u00b7dest", "bey", "mir", "gnug", "zu", "zehr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ich will dich auch von Hertzen ehr\u2019n.", "tokens": ["Ich", "will", "dich", "auch", "von", "Hert\u00b7zen", "ehr'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Drum werde balde ohnbeschwehr", "tokens": ["Drum", "wer\u00b7de", "bal\u00b7de", "ohn\u00b7be\u00b7schwehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mein Kind und ehelich Hau\u00df-Ehr.", "tokens": ["Mein", "Kind", "und", "e\u00b7he\u00b7lich", "Hau\u00df\u00b7Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ich bleib dir stetig zugethan,", "tokens": ["Ich", "bleib", "dir", "ste\u00b7tig", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Bis da\u00df der grosse Sensemann", "tokens": ["Bis", "da\u00df", "der", "gros\u00b7se", "Sen\u00b7se\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sein altes Stunden-Glas zerbricht,", "tokens": ["Sein", "al\u00b7tes", "Stun\u00b7den\u00b7Glas", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und eher la\u00df ich dich auch nicht.", "tokens": ["Und", "e\u00b7her", "la\u00df", "ich", "dich", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}