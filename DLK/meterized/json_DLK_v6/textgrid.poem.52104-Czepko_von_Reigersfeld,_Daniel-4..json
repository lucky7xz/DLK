{"textgrid.poem.52104": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Weisen habet Recht, die goldne Zeit geht an,", "tokens": ["Ihr", "Wei\u00b7sen", "ha\u00b7bet", "Recht", ",", "die", "gold\u00b7ne", "Zeit", "geht", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ihr durch Geist und Schrifft vor diesem kund gethan:", "tokens": ["Die", "Ihr", "durch", "Geist", "und", "Schrifft", "vor", "die\u00b7sem", "kund", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt erneuert sich: Sie streicht vom alten R\u00fccken", "tokens": ["Die", "Welt", "er\u00b7neu\u00b7ert", "sich", ":", "Sie", "streicht", "vom", "al\u00b7ten", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die tieffen Runtzeln hin, und wil sich wieder schm\u00fccken.", "tokens": ["Die", "tief\u00b7fen", "Runt\u00b7zeln", "hin", ",", "und", "wil", "sich", "wie\u00b7der", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was wollt ihr mehr, ihr k\u00f6nnt die rechten Zeichen sehn,", "tokens": ["Was", "wollt", "ihr", "mehr", ",", "ihr", "k\u00f6nnt", "die", "rech\u00b7ten", "Zei\u00b7chen", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem L\u00f6wen wil der Hahn ja in die Ohren kr\u00e4hn:", "tokens": ["Dem", "L\u00f6\u00b7wen", "wil", "der", "Hahn", "ja", "in", "die", "Oh\u00b7ren", "kr\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Biene Honigseim aus gelben Lilgen machen,", "tokens": ["Die", "Bie\u00b7ne", "Ho\u00b7ni\u00b7gseim", "aus", "gel\u00b7ben", "Lil\u00b7gen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und auch die Henne selbst wil vor den Adler wachen.", "tokens": ["Und", "auch", "die", "Hen\u00b7ne", "selbst", "wil", "vor", "den", "Ad\u00b7ler", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}