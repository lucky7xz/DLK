{"textgrid.poem.61160": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Anstand", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau ist der Morgen, streif'ge Nebel wallen,", "tokens": ["Grau", "ist", "der", "Mor\u00b7gen", ",", "streif'\u00b7ge", "Ne\u00b7bel", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein leiser Regen spinnt sich tr\u00fcb und kalt;", "tokens": ["Ein", "lei\u00b7ser", "Re\u00b7gen", "spinnt", "sich", "tr\u00fcb", "und", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die roten Bl\u00e4tter seh' ich langsam fallen -", "tokens": ["Die", "ro\u00b7ten", "Bl\u00e4t\u00b7ter", "seh'", "ich", "lang\u00b7sam", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jagdwetter schien's, drum zogen wir zu Wald.", "tokens": ["Jagd\u00b7wet\u00b7ter", "schien's", ",", "drum", "zo\u00b7gen", "wir", "zu", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon sp\u00fcrt die Meute fern, sie bellt im Suchen,", "tokens": ["Schon", "sp\u00fcrt", "die", "Meu\u00b7te", "fern", ",", "sie", "bellt", "im", "Su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihr Gebell verhei\u00dft uns gute Pirsch;", "tokens": ["Und", "ihr", "Ge\u00b7bell", "ver\u00b7hei\u00dft", "uns", "gu\u00b7te", "Pirsch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich steh' im feuchten Herbstlaub an den Buchen,", "tokens": ["Ich", "steh'", "im", "feuch\u00b7ten", "Herbst\u00b7laub", "an", "den", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gespannt die B\u00fcchse, pass' ich auf den Hirsch.", "tokens": ["Ge\u00b7spannt", "die", "B\u00fcch\u00b7se", ",", "pass'", "ich", "auf", "den", "Hirsch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich fr\u00f6stelt. - Sollt' in meiner Weidmannstasche", "tokens": ["Mich", "fr\u00f6s\u00b7telt", ".", "Sollt'", "in", "mei\u00b7ner", "Weid\u00b7manns\u00b7ta\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei Blei und Pulver nicht Erquickung sein? -", "tokens": ["Bei", "Blei", "und", "Pul\u00b7ver", "nicht", "Er\u00b7quic\u00b7kung", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrwahr, das ist die korbumflochtne Flasche!", "tokens": ["F\u00fcr\u00b7wahr", ",", "das", "ist", "die", "kor\u00b7bum\u00b7flocht\u00b7ne", "Fla\u00b7sche", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein t\u00fccht'ger Zug! - Ha, das ist Zyperwein!", "tokens": ["Ein", "t\u00fccht'\u00b7ger", "Zug", "!", "Ha", ",", "das", "ist", "Zy\u00b7per\u00b7wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "ITJ", "$,", "PDS", "VAFIN", "NE", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hei\u00df rinnt et durch die Adern, durch die Glieder -", "tokens": ["Hei\u00df", "rinnt", "et", "durch", "die", "A\u00b7dern", ",", "durch", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Flo\u00df durch die Wipfel pl\u00f6tzlich Sonnenglanz?", "tokens": ["Flo\u00df", "durch", "die", "Wip\u00b7fel", "pl\u00f6tz\u00b7lich", "Son\u00b7nen\u00b7glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die griech'sche Feuertraube ruft mir wieder", "tokens": ["Die", "griech'\u00b7sche", "Feu\u00b7er\u00b7trau\u00b7be", "ruft", "mir", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im Herzen wach die Bilder Griechenlands.", "tokens": ["Im", "Her\u00b7zen", "wach", "die", "Bil\u00b7der", "Grie\u00b7chen\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwei Jahre sind's! Ei, wie so anders schaute,", "tokens": ["Zwei", "Jah\u00b7re", "sin\u00b7d's", "!", "Ei", ",", "wie", "so", "an\u00b7ders", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$.", "NN", "$,", "PWAV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie froh der Herbst mir damals ins Gesicht!", "tokens": ["Wie", "froh", "der", "Herbst", "mir", "da\u00b7mals", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lau war die Luft, der tiefe Himmel blaute,", "tokens": ["Lau", "war", "die", "Luft", ",", "der", "tie\u00b7fe", "Him\u00b7mel", "blau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Feige schwoll, die Traub' im Sonnenlicht.", "tokens": ["Die", "Fei\u00b7ge", "schwoll", ",", "die", "Traub'", "im", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da lie\u00dfen, matt noch von des Sommers Gluten,", "tokens": ["Da", "lie\u00b7\u00dfen", ",", "matt", "noch", "von", "des", "Som\u00b7mers", "Glu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUI", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Ernst, den Ernst wir in Athen zu Haus", "tokens": ["Mein", "Ernst", ",", "den", "Ernst", "wir", "in", "A\u00b7then", "zu", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PPER", "APPR", "NE", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und zogen durch des Inselmeeres Fluten,", "tokens": ["Und", "zo\u00b7gen", "durch", "des", "In\u00b7sel\u00b7mee\u00b7res", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zwei sel'ge Schw\u00e4rmer, abenteuernd aus.", "tokens": ["Zwei", "sel'\u00b7ge", "Schw\u00e4r\u00b7mer", ",", "a\u00b7bent\u00b7eu\u00b7ernd", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gedenkst du, wie bei Paros durch die Brandung", "tokens": ["Ge\u00b7denkst", "du", ",", "wie", "bei", "Pa\u00b7ros", "durch", "die", "Bran\u00b7dung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Boot wir zw\u00e4ngten? - d\u00e4mmernd stieg der Mond -", "tokens": ["Das", "Boot", "wir", "zw\u00e4ng\u00b7ten", "?", "d\u00e4m\u00b7mernd", "stieg", "der", "Mond"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "$(", "VVPP", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie so sch\u00f6n uns dann die k\u00fchne Landung", "tokens": ["Und", "wie", "so", "sch\u00f6n", "uns", "dann", "die", "k\u00fch\u00b7ne", "Lan\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die rebumkr\u00e4nzte Marmorstadt belohnt?", "tokens": ["Die", "re\u00b7bum\u00b7kr\u00e4nz\u00b7te", "Mar\u00b7mor\u00b7stadt", "be\u00b7lohnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denkst du der Zithern, die die Nacht durchklangen,", "tokens": ["Denkst", "du", "der", "Zi\u00b7thern", ",", "die", "die", "Nacht", "durch\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Brunnen, die uns in den Schlaf gerauscht,", "tokens": ["Der", "Brun\u00b7nen", ",", "die", "uns", "in", "den", "Schlaf", "ge\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jenes M\u00e4dchens, das mit gl\u00fchnden Wangen", "tokens": ["Und", "je\u00b7nes", "M\u00e4d\u00b7chens", ",", "das", "mit", "gl\u00fchn\u00b7den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr leichten Schmuck Orangen uns vertauscht?", "tokens": ["F\u00fcr", "leich\u00b7ten", "Schmuck", "O\u00b7ran\u00b7gen", "uns", "ver\u00b7tauscht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denkst du an Naxos noch? Ich seh' sie liegen,", "tokens": ["Denkst", "du", "an", "Na\u00b7xos", "noch", "?", "Ich", "seh'", "sie", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADV", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Kl\u00f6ster und das Schlo\u00df auf hohem Stein,", "tokens": ["Die", "Kl\u00f6s\u00b7ter", "und", "das", "Schlo\u00df", "auf", "ho\u00b7hem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den S\u00e4ulenhof, wo sich die Palmen wiegen,", "tokens": ["Den", "S\u00e4u\u00b7len\u00b7hof", ",", "wo", "sich", "die", "Pal\u00b7men", "wie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Felswand \u00fcbergr\u00fcnt von eitel Wein,", "tokens": ["Die", "Fels\u00b7wand", "\u00fc\u00b7berg\u00b7r\u00fcnt", "von", "ei\u00b7tel", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das reiche Tal, in dessen bucht'ge Weiten", "tokens": ["Das", "rei\u00b7che", "Tal", ",", "in", "des\u00b7sen", "bucht'\u00b7ge", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein buntgez\u00e4umtes Saumtier leicht uns trug -", "tokens": ["Ein", "bunt\u00b7ge\u00b7z\u00e4um\u00b7tes", "Saum\u00b7tier", "leicht", "uns", "trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da blinkten Becher rings, da klangen Saiten;", "tokens": ["Da", "blink\u00b7ten", "Be\u00b7cher", "rings", ",", "da", "klan\u00b7gen", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcrwahr, es war ein neuer Bakchuszug!", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "war", "ein", "neu\u00b7er", "Bak\u00b7chus\u00b7zug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und als wir sonnverbrannt mit staub'gen Ballen", "tokens": ["Und", "als", "wir", "sonn\u00b7ver\u00b7brannt", "mit", "staub'\u00b7gen", "Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Ruh' verlangten nach der hei\u00dfen Fahrt,", "tokens": ["Zur", "Ruh'", "ver\u00b7lang\u00b7ten", "nach", "der", "hei\u00b7\u00dfen", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da nahm uns in die k\u00fchlen Klosterhallen", "tokens": ["Da", "nahm", "uns", "in", "die", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der wackre Pater mit dem langen Bart.", "tokens": ["Der", "wack\u00b7re", "Pa\u00b7ter", "mit", "dem", "lan\u00b7gen", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hoch \u00fcberm Meer auf seinem Laubensitze,", "tokens": ["Hoch", "\u00fc\u00b7berm", "Meer", "auf", "sei\u00b7nem", "Lau\u00b7ben\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie schollen unsre Lieder da so frisch!", "tokens": ["Wie", "schol\u00b7len", "uns\u00b7re", "Lie\u00b7der", "da", "so", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "