{"textgrid.poem.62489": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "4. Grabgesang", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6ne milder, t\u00f6ne leise,", "tokens": ["T\u00f6\u00b7ne", "mil\u00b7der", ",", "t\u00f6\u00b7ne", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer Schmerz und Klagelaut,", "tokens": ["Tie\u00b7fer", "Schmerz", "und", "Kla\u00b7ge\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn von ihrer Erdenreise", "tokens": ["Denn", "von", "ih\u00b7rer", "Er\u00b7den\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rastet eine Gottesbraut.", "tokens": ["Ras\u00b7tet", "ei\u00b7ne", "Got\u00b7tes\u00b7braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00fc\u00dfet sie mit Friedenst\u00e4nzen,", "tokens": ["Gr\u00fc\u00b7\u00dfet", "sie", "mit", "Frie\u00b7den\u00b7st\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel, bringt ein wei\u00dfes Kleid,", "tokens": ["En\u00b7gel", ",", "bringt", "ein", "wei\u00b7\u00dfes", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringet Palmen, sie zu kr\u00e4nzen,", "tokens": ["Brin\u00b7get", "Pal\u00b7men", ",", "sie", "zu", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie kommt aus hartem Streit.", "tokens": ["Weil", "sie", "kommt", "aus", "har\u00b7tem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Siegerin, genug gestritten", "tokens": ["Sie\u00b7ge\u00b7rin", ",", "ge\u00b7nug", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du mit der b\u00f6sen Macht,", "tokens": ["Hast", "du", "mit", "der", "b\u00f6\u00b7sen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dulderin, genug gelitten", "tokens": ["Dul\u00b7de\u00b7rin", ",", "ge\u00b7nug", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der dunkeln Erdennacht.", "tokens": ["In", "der", "dun\u00b7keln", "Er\u00b7den\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herz, du magst nun ruhn und rasten,", "tokens": ["Herz", ",", "du", "magst", "nun", "ruhn", "und", "ras\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem stillen Gotteshaus,", "tokens": ["In", "dem", "stil\u00b7len", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Wunden, von den Lasten,", "tokens": ["Von", "den", "Wun\u00b7den", ",", "von", "den", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heldenherz, nun ruhe aus.", "tokens": ["Hel\u00b7den\u00b7herz", ",", "nun", "ru\u00b7he", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo die Palmen, wo die Kreuze,", "tokens": ["Wo", "die", "Pal\u00b7men", ",", "wo", "die", "Kreu\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tod und Lebensbilder, stehn,", "tokens": ["Tod", "und", "Le\u00b7bens\u00b7bil\u00b7der", ",", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll der Traum von ird'schem Reize", "tokens": ["Soll", "der", "Traum", "von", "ird'\u00b7schem", "Rei\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von ird'scher Macht vergehn.", "tokens": ["Und", "von", "ird'\u00b7scher", "Macht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus dem Leid zur ew'gen Wonne", "tokens": ["Aus", "dem", "Leid", "zur", "ew'\u00b7gen", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenken Engel deinen Schritt,", "tokens": ["Len\u00b7ken", "En\u00b7gel", "dei\u00b7nen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Nacht zur hellen Sonne,", "tokens": ["Aus", "der", "Nacht", "zur", "hel\u00b7len", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst ein Engel, walle mit.", "tokens": ["Selbst", "ein", "En\u00b7gel", ",", "wal\u00b7le", "mit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Milder t\u00f6ne, tief und leise,", "tokens": ["Mil\u00b7der", "t\u00f6\u00b7ne", ",", "tief", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6lkerschmerz und Klagelaut,", "tokens": ["V\u00f6l\u00b7ker\u00b7schmerz", "und", "Kla\u00b7ge\u00b7laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn von ihrer Erdenreise", "tokens": ["Denn", "von", "ih\u00b7rer", "Er\u00b7den\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht hier eine Gottesbraut.", "tokens": ["Ruht", "hier", "ei\u00b7ne", "Got\u00b7tes\u00b7braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}