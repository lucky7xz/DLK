{"textgrid.poem.46581": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du hast gewacht und gesprungen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast gewacht und gesprungen,", "tokens": ["Du", "hast", "ge\u00b7wacht", "und", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du hast gelacht und gesungen,", "tokens": ["Du", "hast", "ge\u00b7lacht", "und", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du hast gelacht und gesungen,", "tokens": ["Du", "hast", "ge\u00b7lacht", "und", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nie war das Herz dir schwer;", "tokens": ["Nie", "war", "das", "Herz", "dir", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun hat die Nacht dich umrungen,", "tokens": ["Nun", "hat", "die", "Nacht", "dich", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Nun hat die Nacht dich umrungen,", "tokens": ["Nun", "hat", "die", "Nacht", "dich", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und von dem Schattenheer", "tokens": ["Und", "von", "dem", "Schat\u00b7ten\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist deine Pracht verschlungen;", "tokens": ["Ist", "dei\u00b7ne", "Pracht", "ver\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ist deine Pracht verschlungen?", "tokens": ["Ist", "dei\u00b7ne", "Pracht", "ver\u00b7schlun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ist Alles so sehr,", "tokens": ["Wie", "ist", "Al\u00b7les", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADV", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Was ich gedacht, mi\u00dflungen!", "tokens": ["Was", "ich", "ge\u00b7dacht", ",", "mi\u00df\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Was ich gedacht, ist mi\u00dflungen;", "tokens": ["Was", "ich", "ge\u00b7dacht", ",", "ist", "mi\u00df\u00b7lun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie ist Alles so leer!", "tokens": ["Wie", "ist", "Al\u00b7les", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von Todesmacht bezwungen,", "tokens": ["Von", "To\u00b7des\u00b7macht", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Von Todesmacht bezwungen,", "tokens": ["Von", "To\u00b7des\u00b7macht", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lachst du und springst nicht mehr.", "tokens": ["Lachst", "du", "und", "springst", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast gelacht und gesprungen;", "tokens": ["Du", "hast", "ge\u00b7lacht", "und", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}