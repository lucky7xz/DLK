{"textgrid.poem.52970": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, unter deiner Vaterhut", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, unter deiner Vaterhut", "tokens": ["Gott", ",", "un\u00b7ter", "dei\u00b7ner", "Va\u00b7ter\u00b7hut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich die Nacht so sanft geruht,", "tokens": ["Hab'", "ich", "die", "Nacht", "so", "sanft", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich erquickt nun in die H\u00f6h", "tokens": ["Da\u00df", "ich", "er\u00b7quickt", "nun", "in", "die", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgensonn' entgegen seh.", "tokens": ["Der", "Mor\u00b7gen\u00b7sonn'", "ent\u00b7ge\u00b7gen", "seh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohin ich blicke, redest du", "tokens": ["Wo\u00b7hin", "ich", "bli\u00b7cke", ",", "re\u00b7dest", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wohlthat mir und G\u00fcte zu;", "tokens": ["Mit", "Wohlt\u00b7hat", "mir", "und", "G\u00fc\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein erster Hauch sey Lobgesang,", "tokens": ["Mein", "ers\u00b7ter", "Hauch", "sey", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letzter Athemzug sey Dank.", "tokens": ["Mein", "letz\u00b7ter", "A\u00b7them\u00b7zug", "sey", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gie\u00dfest Freuden, wie ein Meer,", "tokens": ["Du", "gie\u00b7\u00dfest", "Freu\u00b7den", ",", "wie", "ein", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um alle deine Kinder her;", "tokens": ["Um", "al\u00b7le", "dei\u00b7ne", "Kin\u00b7der", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur allein der Thor vergi\u00dft,", "tokens": ["Und", "nur", "al\u00b7lein", "der", "Thor", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er ein Mensch mit Menschen ist.", "tokens": ["Da\u00df", "er", "ein", "Mensch", "mit", "Men\u00b7schen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gib, da\u00df ich diesen ganzen Tag", "tokens": ["Gib", ",", "da\u00df", "ich", "die\u00b7sen", "gan\u00b7zen", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich deiner G\u00fcte freuen mag;", "tokens": ["Mich", "dei\u00b7ner", "G\u00fc\u00b7te", "freu\u00b7en", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wend' Ungl\u00fcck ab nach deiner Huld,", "tokens": ["Wend'", "Un\u00b7gl\u00fcck", "ab", "nach", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn es kommt, gib mir Geduld.", "tokens": ["Und", "wenn", "es", "kommt", ",", "gib", "mir", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur deine Hand theilt Segen aus,", "tokens": ["Nur", "dei\u00b7ne", "Hand", "theilt", "Se\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib Segen in mein kleines Haus;", "tokens": ["Gib", "Se\u00b7gen", "in", "mein", "klei\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df gern mich nutzen jedermann,", "tokens": ["La\u00df", "gern", "mich", "nut\u00b7zen", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und willig helfen, wo ich kann.", "tokens": ["Und", "wil\u00b7lig", "hel\u00b7fen", ",", "wo", "ich", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Erde k\u00f6stlichster Gewinn", "tokens": ["Der", "Er\u00b7de", "k\u00f6st\u00b7lichs\u00b7ter", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist frohes Herz und reiner Sinn;", "tokens": ["Ist", "fro\u00b7hes", "Herz", "und", "rei\u00b7ner", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diesen, Vater, schenke mir,", "tokens": ["Und", "die\u00b7sen", ",", "Va\u00b7ter", ",", "schen\u00b7ke", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wall' ich ruhig hin zu dir.", "tokens": ["So", "wall'", "ich", "ru\u00b7hig", "hin", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du hast mir wieder neue Kraft", "tokens": ["Du", "hast", "mir", "wie\u00b7der", "neu\u00b7e", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinem Tagewerk geschafft;", "tokens": ["Zu", "mei\u00b7nem", "Ta\u00b7ge\u00b7werk", "ge\u00b7schafft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verj\u00fcngt sind wieder Fu\u00df und Hand", "tokens": ["Ver\u00b7j\u00fcngt", "sind", "wie\u00b7der", "Fu\u00df", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrer Arbeit leicht gespannt.", "tokens": ["Zu", "ih\u00b7rer", "Ar\u00b7beit", "leicht", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn einst nach meines Todes Nacht", "tokens": ["Wenn", "einst", "nach", "mei\u00b7nes", "To\u00b7des", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem Licht mein Aug' erwacht,", "tokens": ["Zu", "dei\u00b7nem", "Licht", "mein", "Aug'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann eil' ich, himmlischer erfreut,", "tokens": ["Dann", "eil'", "ich", ",", "himm\u00b7li\u00b7scher", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jenes Lebens Ewigkeit.", "tokens": ["In", "je\u00b7nes", "Le\u00b7bens", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}