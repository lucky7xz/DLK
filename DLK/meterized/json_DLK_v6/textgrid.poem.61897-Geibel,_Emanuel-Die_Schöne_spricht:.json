{"textgrid.poem.61897": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6ne spricht:", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ward zur Kerz' im Saale", "tokens": ["Ich", "ward", "zur", "Ker\u00b7z'", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bestimmt durch Schicksalsschlu\u00df", "tokens": ["Be\u00b7stimmt", "durch", "Schick\u00b7sals\u00b7schlu\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich leucht' und strahle,", "tokens": ["Und", "wenn", "ich", "leucht'", "und", "strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So tu' ich, was ich mu\u00df.", "tokens": ["So", "tu'", "ich", ",", "was", "ich", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wagt's und zeiht der T\u00fccke", "tokens": ["Wer", "wagt's", "und", "zeiht", "der", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein reines Element,", "tokens": ["Mein", "rei\u00b7nes", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil sich die trunkne M\u00fccke", "tokens": ["Weil", "sich", "die", "trunk\u00b7ne", "M\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Fl\u00fcgel dran verbrennt?", "tokens": ["Die", "Fl\u00fc\u00b7gel", "dran", "ver\u00b7brennt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann hie\u00df ich keck dich schweifen", "tokens": ["Wann", "hie\u00df", "ich", "keck", "dich", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um diese Flammen? Sprich!", "tokens": ["Um", "die\u00b7se", "Flam\u00b7men", "?", "Sprich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$.", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum, wenn sie dich ergreifen,", "tokens": ["Drum", ",", "wenn", "sie", "dich", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schilt dich selbst, nicht mich.", "tokens": ["So", "schilt", "dich", "selbst", ",", "nicht", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer sich des holden Scheines", "tokens": ["Wer", "sich", "des", "hol\u00b7den", "Schei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wunschlos freun mag, ei,", "tokens": ["Nicht", "wun\u00b7schlos", "freun", "mag", ",", "ei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$,", "ITJ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Schicksal trag' er \u2013 meines", "tokens": ["Sein", "Schick\u00b7sal", "trag'", "er", "\u2013", "mei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist, sch\u00f6n zu sein und frei.", "tokens": ["Ist", ",", "sch\u00f6n", "zu", "sein", "und", "frei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "PTKZU", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}