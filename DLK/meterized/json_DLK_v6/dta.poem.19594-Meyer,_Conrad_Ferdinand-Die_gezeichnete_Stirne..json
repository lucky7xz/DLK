{"dta.poem.19594": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die gezeichnete Stirne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eweib, verrathe mir, von wem gerufen", "tokens": ["\u201e", "weib", ",", "ver\u00b7ra\u00b7the", "mir", ",", "von", "wem", "ge\u00b7ru\u00b7fen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PIS", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du zur Leidgesellin dich gegeben?", "tokens": ["Du", "zur", "Leid\u00b7ge\u00b7sel\u00b7lin", "dich", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wer herunter dieses Kerkers Stufen", "tokens": ["Wer", "her\u00b7un\u00b7ter", "die\u00b7ses", "Ker\u00b7kers", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich gezogen, du mein s\u00fc\u00dfes Leben?\u201c", "tokens": ["Dich", "ge\u00b7zo\u00b7gen", ",", "du", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u2014 K\u00f6nig Enzio, keine Menschen haben", "tokens": ["K\u00f6\u00b7nig", "En\u00b7zio", ",", "kei\u00b7ne", "Men\u00b7schen", "ha\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "NE", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich vermocht im Kerker zu verbleichen!", "tokens": ["Mich", "ver\u00b7mocht", "im", "Ker\u00b7ker", "zu", "ver\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nein, ein Schicksal war mir eingegraben,", "tokens": ["Nein", ",", "ein", "Schick\u00b7sal", "war", "mir", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine junge Stirne trug ein Zeichen.", "tokens": ["Mei\u00b7ne", "jun\u00b7ge", "Stir\u00b7ne", "trug", "ein", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Unsre V\u00e4ter nahmen dich gefangen", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "nah\u00b7men", "dich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wir Kinder hatten's bald erfahren,", "tokens": ["Und", "wir", "Kin\u00b7der", "hat\u00b7ten's", "bald", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df du nimmer wirst ans Licht gelangen,", "tokens": ["Da\u00df", "du", "nim\u00b7mer", "wirst", "ans", "Licht", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nig Enzio mit den Ringelhaaren!", "tokens": ["K\u00f6\u00b7nig", "En\u00b7zio", "mit", "den", "Rin\u00b7gel\u00b7haa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df du nimmer tragen eine helle", "tokens": ["Da\u00df", "du", "nim\u00b7mer", "tra\u00b7gen", "ei\u00b7ne", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "R\u00fcstung wirst, wo die Drommeten klingen,", "tokens": ["R\u00fcs\u00b7tung", "wirst", ",", "wo", "die", "Drom\u00b7me\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df du nimmer rauschen Wald und Quelle", "tokens": ["Da\u00df", "du", "nim\u00b7mer", "rau\u00b7schen", "Wald", "und", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6rst, noch einen freien Vogel singen!", "tokens": ["H\u00f6rst", ",", "noch", "ei\u00b7nen", "frei\u00b7en", "Vo\u00b7gel", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und wir Kinder lauschten sachte, sachte", "tokens": ["Und", "wir", "Kin\u00b7der", "lauschten", "sach\u00b7te", ",", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "NN", "VVINF", "VVFIN", "$,", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch das Gitter in des Kerkers Tiefe,", "tokens": ["Durch", "das", "Git\u00b7ter", "in", "des", "Ker\u00b7kers", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Leis und heftig streitend, ob Er wachte", "tokens": ["Leis", "und", "hef\u00b7tig", "strei\u00b7tend", ",", "ob", "Er", "wach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVPP", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwerbek\u00fcmmert oder ob Er schliefe \u2014", "tokens": ["Schwer\u00b7be\u00b7k\u00fcm\u00b7mert", "o\u00b7der", "ob", "Er", "schlie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Meine Stirne dr\u00fcckt' ich an das Eisen,", "tokens": ["Mei\u00b7ne", "Stir\u00b7ne", "dr\u00fcckt'", "ich", "an", "das", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Drinnen lagst du schlummernd, wie mir deuchte,", "tokens": ["Drin\u00b7nen", "lagst", "du", "schlum\u00b7mernd", ",", "wie", "mir", "deuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blickte ... blickte, war nicht wegzuweisen,", "tokens": ["Blick\u00b7te", "...", "blick\u00b7te", ",", "war", "nicht", "weg\u00b7zu\u00b7wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis der W\u00e4chter drohend mich verscheuchte.", "tokens": ["Bis", "der", "W\u00e4ch\u00b7ter", "dro\u00b7hend", "mich", "ver\u00b7scheuch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVPP", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "M\u00fctterlein ersah mich und wehklagte,", "tokens": ["M\u00fct\u00b7ter\u00b7lein", "er\u00b7sah", "mich", "und", "weh\u00b7klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlug die H\u00e4nde jammervoll zusammen:", "tokens": ["Schlug", "die", "H\u00e4n\u00b7de", "jam\u00b7mer\u00b7voll", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u201ekind, wer hat dir in die Stirne\u201c \u2014 fragte", "tokens": ["\u201e", "kind", ",", "wer", "hat", "dir", "in", "die", "Stir\u00b7ne", "\u201c", "frag\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "ADJD", "$,", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "$(", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie \u2014 \u201egezeichnet dieses Kreuz von Flammen?\u201c", "tokens": ["Sie", "\u201e", "ge\u00b7zeich\u00b7net", "die\u00b7ses", "Kreuz", "von", "Flam\u00b7men", "?", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$(", "$(", "VVPP", "PDAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hie\u00df mich dann in ihren Spiegel schauen \u2014", "tokens": ["Hie\u00df", "mich", "dann", "in", "ih\u00b7ren", "Spie\u00b7gel", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Theuer werther Herr, so wahr ich lebe,", "tokens": ["Theu\u00b7er", "wert\u00b7her", "Herr", ",", "so", "wahr", "ich", "le\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eingezeichnet \u00fcber meinen Brauen", "tokens": ["Ein\u00b7ge\u00b7zeich\u00b7net", "\u00fc\u00b7ber", "mei\u00b7nen", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Waren deines Kerkers Eisenst\u00e4be!", "tokens": ["Wa\u00b7ren", "dei\u00b7nes", "Ker\u00b7kers", "Ei\u00b7sen\u00b7st\u00e4\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Au\u00dfen wich das Zeichen; aber innen", "tokens": ["Au\u00b7\u00dfen", "wich", "das", "Zei\u00b7chen", ";", "a\u00b7ber", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blieb's, da ich zur Maid erwuchs, geschrieben", "tokens": ["Blie\u00b7b's", ",", "da", "ich", "zur", "Maid", "er\u00b7wuchs", ",", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr, seit jenem Tag war all mein Sinnen", "tokens": ["Herr", ",", "seit", "je\u00b7nem", "Tag", "war", "all", "mein", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PDAT", "NN", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich und deinen Kerker nur zu lieben.", "tokens": ["Dich", "und", "dei\u00b7nen", "Ker\u00b7ker", "nur", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}