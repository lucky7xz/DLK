{"textgrid.poem.44589": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn dich die Dichtkunst schaffen hei\u00dft]", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich die Dichtkunst schaffen hei\u00dft,", "tokens": ["Wenn", "dich", "die", "Dicht\u00b7kunst", "schaf\u00b7fen", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du das Drama w\u00e4hlst,", "tokens": ["Und", "du", "das", "Dra\u00b7ma", "w\u00e4hlst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn dich aufs Epos f\u00fchrt der Geist,", "tokens": ["Wenn", "dich", "aufs", "E\u00b7pos", "f\u00fchrt", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du dem Volk erz\u00e4hlst:", "tokens": ["Und", "du", "dem", "Volk", "er\u00b7z\u00e4hlst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bist kaum du noch als Dichter hier,", "tokens": ["Bist", "kaum", "du", "noch", "als", "Dich\u00b7ter", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist nur, was du schufst,", "tokens": ["Es", "ist", "nur", ",", "was", "du", "schufst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jene Geister sind statt dir,", "tokens": ["Und", "je\u00b7ne", "Geis\u00b7ter", "sind", "statt", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zauberhaft du rufst.", "tokens": ["Die", "zau\u00b7ber\u00b7haft", "du", "rufst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wenn die Leier an du klingst", "tokens": ["Doch", "wenn", "die", "Lei\u00b7er", "an", "du", "klingst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00f6nst von Gram und Lust,", "tokens": ["Und", "t\u00f6nst", "von", "Gram", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann bist du selber, was du singst,", "tokens": ["Dann", "bist", "du", "sel\u00b7ber", ",", "was", "du", "singst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied ist deine Brust.", "tokens": ["Das", "Lied", "ist", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nichts sichtbar als nur du und ich,", "tokens": ["Nichts", "sicht\u00b7bar", "als", "nur", "du", "und", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "ADV", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts h\u00f6rbar als nur du,", "tokens": ["Nichts", "h\u00f6r\u00b7bar", "als", "nur", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Innre ist allein mit sich,", "tokens": ["Das", "Inn\u00b7re", "ist", "al\u00b7lein", "mit", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Kein Mittler tritt hinzu.", "tokens": ["Kein", "Mitt\u00b7ler", "tritt", "hin\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da aber nimm dich nur in acht,", "tokens": ["Da", "a\u00b7ber", "nimm", "dich", "nur", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du du selber seist,", "tokens": ["Da\u00df", "du", "du", "sel\u00b7ber", "seist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nicht, was du getan, gedacht,", "tokens": ["Da\u00df", "nicht", ",", "was", "du", "ge\u00b7tan", ",", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als andern dich erweist.", "tokens": ["Als", "an\u00b7dern", "dich", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sprichst du von tiefem Seelenschmerz", "tokens": ["Sprichst", "du", "von", "tie\u00b7fem", "See\u00b7len\u00b7schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warst ein eitler Tor;", "tokens": ["Und", "warst", "ein", "eit\u00b7ler", "Tor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ewger Dauer f\u00fcr dein Herz,", "tokens": ["Von", "ew\u00b7ger", "Dau\u00b7er", "f\u00fcr", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wetterhahn zuvor;", "tokens": ["Ein", "Wet\u00b7ter\u00b7hahn", "zu\u00b7vor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Singst du das Lob der Einsamkeit,", "tokens": ["Singst", "du", "das", "Lob", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst laut im Volksgew\u00fchl;", "tokens": ["Sonst", "laut", "im", "Volks\u00b7ge\u00b7w\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nennst du die Welt, so gro\u00df, so weit,", "tokens": ["Nennst", "du", "die", "Welt", ",", "so", "gro\u00df", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu eng f\u00fcr dein Gef\u00fchl:", "tokens": ["Zu", "eng", "f\u00fcr", "dein", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie ist ein schlimmres Schauspielhaus,", "tokens": ["Sie", "ist", "ein", "schlimm\u00b7res", "Schau\u00b7spiel\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wo man spielt zu Nacht \u2013", "tokens": ["Als", "wo", "man", "spielt", "zu", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIS", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier lacht man nur den ", "tokens": ["Hier", "lacht", "man", "nur", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dort wird der ", "tokens": ["Dort", "wird", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}