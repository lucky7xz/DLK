{"textgrid.poem.46012": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich baute eine Mauer]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich baute eine Mauer", "tokens": ["Ich", "bau\u00b7te", "ei\u00b7ne", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Gold und Edelstein,", "tokens": ["Aus", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Draus wohnet Nacht und Schauer", "tokens": ["Draus", "woh\u00b7net", "Nacht", "und", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drin lichter Gnadenschein.", "tokens": ["Drin", "lich\u00b7ter", "Gna\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hab' alles Licht gezogen,", "tokens": ["Hab'", "al\u00b7les", "Licht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Gottes durst'gem Mund,", "tokens": ["Mit", "Got\u00b7tes", "dur\u00b7st'\u00b7gem", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verw\u00f6lbt den Himmelsbogen", "tokens": ["Ver\u00b7w\u00f6lbt", "den", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meines Herzens Grund.", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hab' allen Mai gerissen", "tokens": ["Hab'", "al\u00b7len", "Mai", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In die geheime Nacht,", "tokens": ["In", "die", "ge\u00b7hei\u00b7me", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df in den Finsternissen", "tokens": ["Da\u00df", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Herrn ein G\u00e4rtlein lacht.", "tokens": ["Dem", "Herrn", "ein", "G\u00e4rt\u00b7lein", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er, er soll alles haben,", "tokens": ["Er", ",", "er", "soll", "al\u00b7les", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm bin ich selig still,", "tokens": ["Ihm", "bin", "ich", "se\u00b7lig", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm hab' ich mich begraben,", "tokens": ["Ihm", "hab'", "ich", "mich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm, dem ich leben will.", "tokens": ["Ihm", ",", "dem", "ich", "le\u00b7ben", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie eine fromme Imme", "tokens": ["Wie", "ei\u00b7ne", "from\u00b7me", "Im\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trag' ich ihm alles ein,", "tokens": ["Trag'", "ich", "ihm", "al\u00b7les", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der mir die innre Stimme,", "tokens": ["Der", "mir", "die", "inn\u00b7re", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Giebt durch mein Fensterlein.", "tokens": ["Giebt", "durch", "mein", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der mich l\u00e4\u00dft Bl\u00fcmlein sehen", "tokens": ["Der", "mich", "l\u00e4\u00dft", "Bl\u00fcm\u00b7lein", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch den Rosendorn,", "tokens": ["Und", "auch", "den", "Ro\u00b7sen\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lehrt durch die Mauer gehen", "tokens": ["Lehrt", "durch", "die", "Mau\u00b7er", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hin zu dem Honigborn.", "tokens": ["Hin", "zu", "dem", "Ho\u00b7nig\u00b7born", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "So f\u00fcll' ich Zell' an Zelle,", "tokens": ["So", "f\u00fcll'", "ich", "Zell'", "an", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm, ihm dem Herrn allein,", "tokens": ["Ihm", ",", "ihm", "dem", "Herrn", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum Epheu an der Schwelle", "tokens": ["Drum", "E\u00b7pheu", "an", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "NN", "APPR", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Sollst du nicht traurig sein.", "tokens": ["Sollst", "du", "nicht", "trau\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es m\u00fc\u00dft' mich billig jammern,", "tokens": ["Es", "m\u00fc\u00dft'", "mich", "bil\u00b7lig", "jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du mir untergingst,", "tokens": ["Wenn", "du", "mir", "un\u00b7ter\u00b7gingst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der du mit Lebensklammern,", "tokens": ["Der", "du", "mit", "Le\u00b7bens\u00b7klam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wand mir gr\u00fcn umschlingst.", "tokens": ["Die", "Wand", "mir", "gr\u00fcn", "um\u00b7schlingst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn draus du betend rauschest", "tokens": ["Wenn", "draus", "du", "be\u00b7tend", "rau\u00b7schest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner Kammer Wand,", "tokens": ["An", "mei\u00b7ner", "Kam\u00b7mer", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn draus du sinnig lauschest,", "tokens": ["Wenn", "draus", "du", "sin\u00b7nig", "lau\u00b7schest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Klingt, gl\u00e4nzt die innre Wand.", "tokens": ["Klingt", ",", "gl\u00e4nzt", "die", "inn\u00b7re", "Wand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dann mu\u00df ich f\u00fcr dich beten,", "tokens": ["Dann", "mu\u00df", "ich", "f\u00fcr", "dich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bei dir kehren ein,", "tokens": ["Und", "bei", "dir", "keh\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mich die Nacht betreten", "tokens": ["Wenn", "mich", "die", "Nacht", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Draus wo kein Gnadenschein.", "tokens": ["Draus", "wo", "kein", "Gna\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Mit allen Wurzeln trinkest", "tokens": ["Mit", "al\u00b7len", "Wur\u00b7zeln", "trin\u00b7kest"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Leben ja aus mir,", "tokens": ["Du", "Le\u00b7ben", "ja", "aus", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn du niedersinkest,", "tokens": ["Und", "wenn", "du", "nie\u00b7der\u00b7sin\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reich' ich die H\u00e4nde dir.", "tokens": ["Reich'", "ich", "die", "H\u00e4n\u00b7de", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die kalten, frommen H\u00e4nde,", "tokens": ["Die", "kal\u00b7ten", ",", "from\u00b7men", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Draus kalt, da\u00df Gott erbarm',", "tokens": ["Draus", "kalt", ",", "da\u00df", "Gott", "er\u00b7barm'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drin schimmern, gl\u00fchn die W\u00e4nde,", "tokens": ["Drin", "schim\u00b7mern", ",", "gl\u00fchn", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drin ist es licht und warm.", "tokens": ["Drin", "ist", "es", "licht", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Draus kann ich dir nur schweigen,", "tokens": ["Draus", "kann", "ich", "dir", "nur", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O klimme treu am Stein,", "tokens": ["O", "klim\u00b7me", "treu", "am", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du findst mit deinen Zweigen", "tokens": ["Du", "findst", "mit", "dei\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch einst mein Fensterlein.", "tokens": ["Noch", "einst", "mein", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dann magst du niederblicken", "tokens": ["Dann", "magst", "du", "nie\u00b7der\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deinem bittern Leid,", "tokens": ["Mit", "dei\u00b7nem", "bit\u00b7tern", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und magst mir Tr\u00e4nen nicken", "tokens": ["Und", "magst", "mir", "Tr\u00e4\u00b7nen", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meine Seligkeit.", "tokens": ["In", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und wenn ich ausgebauet,", "tokens": ["Und", "wenn", "ich", "aus\u00b7ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Haus mit Liebe voll,", "tokens": ["Mein", "Haus", "mit", "Lie\u00b7be", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nun angetrauet", "tokens": ["Da\u00df", "ich", "nun", "an\u00b7ge\u00b7trau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Liebsten werden soll", "tokens": ["Dem", "Liebs\u00b7ten", "wer\u00b7den", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Will ich mein Kr\u00e4nzlein brechen", "tokens": ["Will", "ich", "mein", "Kr\u00e4nz\u00b7lein", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von deinem dunkeln Gr\u00fcn,", "tokens": ["Von", "dei\u00b7nem", "dun\u00b7keln", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zu dem Liebsten sprechen,", "tokens": ["Und", "zu", "dem", "Liebs\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr la\u00df mir diesen bl\u00fchn.", "tokens": ["Herr", "la\u00df", "mir", "die\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "La\u00df ziehn mich ihn von dannen", "tokens": ["La\u00df", "ziehn", "mich", "ihn", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis \u00fcber diese Welt,", "tokens": ["Bis", "\u00fc\u00b7ber", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er soll mir dorten spannen", "tokens": ["Er", "soll", "mir", "dor\u00b7ten", "span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein hochzeitliches Zelt.", "tokens": ["Mein", "hoch\u00b7zeit\u00b7li\u00b7ches", "Zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Rein hilf du mir ihn machen,", "tokens": ["Rein", "hilf", "du", "mir", "ihn", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn, der mich nicht erschreckt,", "tokens": ["Ihn", ",", "der", "mich", "nicht", "er\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn, der mir b\u00f6se Drachen", "tokens": ["Ihn", ",", "der", "mir", "b\u00f6\u00b7se", "Dra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Laub entdeckt,", "tokens": ["In", "sei\u00b7nem", "Laub", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Da\u00df ich jungfr\u00e4ulich trete", "tokens": ["Da\u00df", "ich", "jung\u00b7fr\u00e4u\u00b7lich", "tre\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf dieses Drachenhaupt,", "tokens": ["Auf", "die\u00b7ses", "Dra\u00b7chen\u00b7haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit er mit mir bete,", "tokens": ["Da\u00b7mit", "er", "mit", "mir", "be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit er mit mir glaubt.", "tokens": ["Da\u00b7mit", "er", "mit", "mir", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Damit den Schatz ich hebe,", "tokens": ["Da\u00b7mit", "den", "Schatz", "ich", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem der Drache ruht,", "tokens": ["Auf", "dem", "der", "Dra\u00b7che", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dir mit mir ihn gebe", "tokens": ["Und", "dir", "mit", "mir", "ihn", "ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "PPER", "VVFIN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "O Jesu f\u00fcr dein Blut.", "tokens": ["O", "Je\u00b7su", "f\u00fcr", "dein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Wer Sch\u00e4tze hebt, mu\u00df schweigen", "tokens": ["Wer", "Sch\u00e4t\u00b7ze", "hebt", ",", "mu\u00df", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bedenk mein Epheu dies,", "tokens": ["Be\u00b7denk", "mein", "E\u00b7pheu", "dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PDS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umflicht nur treu mit Zweigen", "tokens": ["Um\u00b7flicht", "nur", "treu", "mit", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein stummes Paradies.", "tokens": ["Dein", "stum\u00b7mes", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Treu will ich dich ern\u00e4hren", "tokens": ["Treu", "will", "ich", "dich", "er\u00b7n\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": 