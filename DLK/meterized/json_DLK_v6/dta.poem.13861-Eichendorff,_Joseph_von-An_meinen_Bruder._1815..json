{"dta.poem.13861": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder.  \n 1815.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df' bilden die Gewalten!", "tokens": ["La\u00df'", "bil\u00b7den", "die", "Ge\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was davon himmlisch war,", "tokens": ["Was", "da\u00b7von", "himm\u00b7lisch", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann nimmermehr veralten,", "tokens": ["Kann", "nim\u00b7mer\u00b7mehr", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird in der Brust gestalten", "tokens": ["Wird", "in", "der", "Brust", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich manches stille Jahr.", "tokens": ["Sich", "man\u00b7ches", "stil\u00b7le", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}