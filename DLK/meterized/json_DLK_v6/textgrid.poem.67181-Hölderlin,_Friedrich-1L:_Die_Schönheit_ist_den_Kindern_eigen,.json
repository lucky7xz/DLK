{"textgrid.poem.67181": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sch\u00f6nheit ist den Kindern eigen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit ist den Kindern eigen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "den", "Kin\u00b7dern", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Gottes Ebenbild vielleicht, \u2013", "tokens": ["Ist", "Got\u00b7tes", "E\u00b7ben\u00b7bild", "viel\u00b7leicht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "NN", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Eigentum ist Ruh und Schweigen,", "tokens": ["Ihr", "Ei\u00b7gen\u00b7tum", "ist", "Ruh", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Engeln auch zum Lob gereicht.", "tokens": ["Das", "En\u00b7geln", "auch", "zum", "Lob", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}