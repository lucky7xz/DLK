{"textgrid.poem.32666": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Daphnis' Klagen", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie lacht der Lenz auf allen H\u00fcgeln!", "tokens": ["O", "wie", "lacht", "der", "Lenz", "auf", "al\u00b7len", "H\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welch ein s\u00fc\u00dfer Maienblumenduft", "tokens": ["Welch", "ein", "s\u00fc\u00b7\u00dfer", "Mai\u00b7en\u00b7blu\u00b7men\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hebt sich auf der Weste Purpurfl\u00fcgeln", "tokens": ["Hebt", "sich", "auf", "der", "Wes\u00b7te", "Pur\u00b7pur\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und durchbalsamt rings umher die Luft!", "tokens": ["Und", "durch\u00b7bal\u00b7samt", "rings", "um\u00b7her", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle Blumen, die des Maien harrten,", "tokens": ["Al\u00b7le", "Blu\u00b7men", ",", "die", "des", "Mai\u00b7en", "harr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heben sich aus jungem Gras empor;", "tokens": ["He\u00b7ben", "sich", "aus", "jun\u00b7gem", "Gras", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein zauberischer Blumengarten", "tokens": ["Wie", "ein", "zau\u00b7be\u00b7ri\u00b7scher", "Blu\u00b7men\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Steht die Flur in tausendfachem Flor.", "tokens": ["Steht", "die", "Flur", "in", "tau\u00b7send\u00b7fa\u00b7chem", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sammelten die V\u00f6gelchen sich alle,", "tokens": ["Sam\u00b7mel\u00b7ten", "die", "V\u00f6\u00b7gel\u00b7chen", "sich", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "PIS", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Diesen Tag zu feiren? Sangen sie", "tokens": ["Die\u00b7sen", "Tag", "zu", "fei\u00b7ren", "?", "San\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Je in solchem wonniglichen Schalle", "tokens": ["Je", "in", "sol\u00b7chem", "won\u00b7nig\u00b7li\u00b7chen", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre s\u00fc\u00dfe Fr\u00fchlingsmelodie?", "tokens": ["Ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7me\u00b7lo\u00b7die", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Freude schallt aus bl\u00fchenden Gestr\u00e4uchen,", "tokens": ["Freu\u00b7de", "schallt", "aus", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo das Laub in Netze sich verflicht:", "tokens": ["Wo", "das", "Laub", "in", "Net\u00b7ze", "sich", "ver\u00b7flicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aller Anmut, alle Sorgen weichen,", "tokens": ["Al\u00b7ler", "An\u00b7mut", ",", "al\u00b7le", "Sor\u00b7gen", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber nur aus meinem Herzen nicht.", "tokens": ["A\u00b7ber", "nur", "aus", "mei\u00b7nem", "Her\u00b7zen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "G\u00f6tter! ach, wenn keine Daphne w\u00e4re,", "tokens": ["G\u00f6t\u00b7ter", "!", "ach", ",", "wenn", "kei\u00b7ne", "Daph\u00b7ne", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "$,", "KOUS", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O so mischte sich mein Jubel auch", "tokens": ["O", "so", "mischte", "sich", "mein", "Ju\u00b7bel", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In der Sch\u00f6pfung allgemeine Ch\u00f6re,", "tokens": ["In", "der", "Sch\u00f6p\u00b7fung", "all\u00b7ge\u00b7mei\u00b7ne", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und durchwirbelte den Blumenhauch!", "tokens": ["Und", "durch\u00b7wir\u00b7bel\u00b7te", "den", "Blu\u00b7men\u00b7hauch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber ach! ihr schuft zu meinem Leiden,", "tokens": ["A\u00b7ber", "ach", "!", "ihr", "schuft", "zu", "mei\u00b7nem", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Daphnen, die mit Lieb' und Treue spielt,", "tokens": ["Daph\u00b7nen", ",", "die", "mit", "Lieb'", "und", "Treu\u00b7e", "spielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Liebe s\u00fc\u00dfe G\u00f6tterfreuden", "tokens": ["Und", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "G\u00f6t\u00b7ter\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nie in ihrer falschen Brust gef\u00fchlt!", "tokens": ["Nie", "in", "ih\u00b7rer", "fal\u00b7schen", "Brust", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Zwar sie lacht aus allen ihren Z\u00fcgen;", "tokens": ["Zwar", "sie", "lacht", "aus", "al\u00b7len", "ih\u00b7ren", "Z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unschuld wohnt im bl\u00fchenden Gesicht;", "tokens": ["Un\u00b7schuld", "wohnt", "im", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber nur mich Armen zu betr\u00fcgen;", "tokens": ["A\u00b7ber", "nur", "mich", "Ar\u00b7men", "zu", "be\u00b7tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! in ihrem Herzen wohnt sie nicht.", "tokens": ["Ach", "!", "in", "ih\u00b7rem", "Her\u00b7zen", "wohnt", "sie", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Als sie mich an ihren Busen dr\u00fcckte,", "tokens": ["Als", "sie", "mich", "an", "ih\u00b7ren", "Bu\u00b7sen", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebe mir und stete Treue schwur;", "tokens": ["Lie\u00b7be", "mir", "und", "ste\u00b7te", "Treu\u00b7e", "schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als sie mich ins Himmelreich entz\u00fcckte,", "tokens": ["Als", "sie", "mich", "ins", "Him\u00b7mel\u00b7reich", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Damals lachte, so wie jetzt, die Flur.", "tokens": ["Da\u00b7mals", "lach\u00b7te", ",", "so", "wie", "jetzt", ",", "die", "Flur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "KOKOM", "ADV", "$,", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Liebesg\u00f6tter scherzten um das M\u00e4dchen,", "tokens": ["Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "scherz\u00b7ten", "um", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Maienblumen bl\u00fchten \u00fcberall;", "tokens": ["Mai\u00b7en\u00b7blu\u00b7men", "bl\u00fch\u00b7ten", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zephyrs deckten sie mit Rosenbl\u00e4ttchen,", "tokens": ["Ze\u00b7phyrs", "deck\u00b7ten", "sie", "mit", "Ro\u00b7sen\u00b7bl\u00e4tt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es fl\u00f6tete die Nachtigall.", "tokens": ["Und", "es", "fl\u00f6\u00b7te\u00b7te", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "G\u00f6tter! welch ein Leben! taumelnd flogen", "tokens": ["G\u00f6t\u00b7ter", "!", "welch", "ein", "Le\u00b7ben", "!", "tau\u00b7melnd", "flo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PWAT", "ART", "NN", "$.", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcber uns die goldnen Stunden hin,", "tokens": ["\u00dc\u00b7ber", "uns", "die", "gold\u00b7nen", "Stun\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend K\u00fcsse wurden eingesogen", "tokens": ["Tau\u00b7send", "K\u00fcs\u00b7se", "wur\u00b7den", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von dem Rosenmund der Zauberin.", "tokens": ["Von", "dem", "Ro\u00b7sen\u00b7mund", "der", "Zau\u00b7be\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Aber jene Nacht! \u2013 Schon harrt' ich lange", "tokens": ["A\u00b7ber", "je\u00b7ne", "Nacht", "!", "\u2013", "Schon", "harrt'", "ich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Geliebten; und Diana hing", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", ";", "und", "Di\u00b7a\u00b7na", "hing"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "NE", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Traurig zwischen Wolken, sah mich bange,", "tokens": ["Trau\u00b7rig", "zwi\u00b7schen", "Wol\u00b7ken", ",", "sah", "mich", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als ich nach des M\u00e4dchens H\u00fctte ging.", "tokens": ["Als", "ich", "nach", "des", "M\u00e4d\u00b7chens", "H\u00fct\u00b7te", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Und sie sa\u00df, dem Frevler in den Armen;", "tokens": ["Und", "sie", "sa\u00df", ",", "dem", "Frev\u00b7ler", "in", "den", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Diana sah vom Himmel her,", "tokens": ["Und", "Di\u00b7a\u00b7na", "sah", "vom", "Him\u00b7mel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "F\u00fchlte sanftes Mitleid und Erbarmen,", "tokens": ["F\u00fchl\u00b7te", "sanf\u00b7tes", "Mit\u00b7leid", "und", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und verschwand und leuchtete nicht mehr.", "tokens": ["Und", "ver\u00b7schwand", "und", "leuch\u00b7te\u00b7te", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "O vernichtet, G\u00f6tter! diese Stunde,", "tokens": ["O", "ver\u00b7nich\u00b7tet", ",", "G\u00f6t\u00b7ter", "!", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "NN", "$.", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Diesen bangen Augenblick, und mich!", "tokens": ["Die\u00b7sen", "ban\u00b7gen", "Au\u00b7gen\u00b7blick", ",", "und", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn, o wenn verblutet sich die Wunde,", "tokens": ["Wenn", ",", "o", "wenn", "ver\u00b7blu\u00b7tet", "sich", "die", "Wun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "FM", "KOUS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn vergess' ich, falsches M\u00e4dchen, dich! \u2013", "tokens": ["Wenn", "ver\u00b7ge\u00b7ss'", "ich", ",", "fal\u00b7sches", "M\u00e4d\u00b7chen", ",", "dich", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Ha! da handeln Wetterwolken! G\u00f6tter!", "tokens": ["Ha", "!", "da", "han\u00b7deln", "Wet\u00b7ter\u00b7wol\u00b7ken", "!", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVINF", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sendet Tod aus ihnen mir herab!", "tokens": ["Sen\u00b7det", "Tod", "aus", "ih\u00b7nen", "mir", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Euer Donner, w\u00e4r' er mein Erretter,", "tokens": ["Eu\u00b7er", "Don\u00b7ner", ",", "w\u00e4r'", "er", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und begr\u00fcb' er mich ins \u00f6de Grab!", "tokens": ["Und", "be\u00b7gr\u00fcb'", "er", "mich", "ins", "\u00f6\u00b7de", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}