{"textgrid.poem.66874": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sausende Funken,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sausende Funken,", "tokens": ["Sau\u00b7sen\u00b7de", "Fun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Rollende R\u00e4der,", "tokens": ["Rol\u00b7len\u00b7de", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00fchende Massen", "tokens": ["M\u00fc\u00b7hen\u00b7de", "Mas\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wandeln die Welt ...", "tokens": ["Wan\u00b7deln", "die", "Welt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wenn ich mich fl\u00fcchte,", "tokens": ["Wenn", "ich", "mich", "fl\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bin ich ein Feiger,", "tokens": ["Bin", "ich", "ein", "Fei\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wenn ich mich nahe,", "tokens": ["Wenn", "ich", "mich", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Tr\u00e4gt es die Feder,", "tokens": ["Tr\u00e4gt", "es", "die", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Oder wird sie vom riesigen Schwungrad zerschellt?", "tokens": ["O\u00b7der", "wird", "sie", "vom", "rie\u00b7si\u00b7gen", "Schwun\u00b7grad", "zer\u00b7schellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Ich steh inmitten", "tokens": ["Ich", "steh", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kampfhei\u00dfer Tage,", "tokens": ["Kampf\u00b7hei\u00b7\u00dfer", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich bin geworfen", "tokens": ["Ich", "bin", "ge\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In wirbelnde Flut.", "tokens": ["In", "wir\u00b7beln\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die Seele blutet,", "tokens": ["Die", "See\u00b7le", "blu\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die weiche Seele,", "tokens": ["Die", "wei\u00b7che", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie kann von Vater", "tokens": ["Wie", "kann", "von", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und Mutter sie los?", "tokens": ["Und", "Mut\u00b7ter", "sie", "los", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "An Herzensf\u00e4den", "tokens": ["An", "Her\u00b7zens\u00b7f\u00e4\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Bin ich gebunden,", "tokens": ["Bin", "ich", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Durch Herzensf\u00e4den", "tokens": ["Durch", "Her\u00b7zens\u00b7f\u00e4\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Schneidet die Zeit.", "tokens": ["Schnei\u00b7det", "die", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "So la\u00df sie schneiden!", "tokens": ["So", "la\u00df", "sie", "schnei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Ich will's drum leiden,", "tokens": ["Ich", "will's", "drum", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Den Weg zu finden:", "tokens": ["Den", "Weg", "zu", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Wie werd' ich gr\u00f6\u00dfer?", "tokens": ["Wie", "werd'", "ich", "gr\u00f6\u00b7\u00dfer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Wie werde mit andern ich selber befreit?", "tokens": ["Wie", "wer\u00b7de", "mit", "an\u00b7dern", "ich", "sel\u00b7ber", "be\u00b7freit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "ADJA", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Worin ist Freiheit?", "tokens": ["Wo\u00b7rin", "ist", "Frei\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Worin ist Gr\u00f6\u00dfe?", "tokens": ["Wo\u00b7rin", "ist", "Gr\u00f6\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Im Mut der Menschheit,", "tokens": ["Im", "Mut", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der dich erf\u00fcllt.", "tokens": ["Der", "dich", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Gattung Gr\u00f6\u00dfe,", "tokens": ["In", "Gat\u00b7tung", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Freiheit im Volke,", "tokens": ["Frei\u00b7heit", "im", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die dich geboren,", "tokens": ["Die", "dich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Das dich erzeugt.", "tokens": ["Das", "dich", "er\u00b7zeugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Voraus der Gattung,", "tokens": ["Vo\u00b7raus", "der", "Gat\u00b7tung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Voran dem Volke,", "tokens": ["Vo\u00b7ran", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ihr Vorw\u00e4rts-F\u00fchlen", "tokens": ["Ihr", "Vor\u00b7w\u00e4rts\u00b7F\u00fch\u00b7len"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Von dir gepr\u00e4gt!", "tokens": ["Von", "dir", "ge\u00b7pr\u00e4gt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ihr h\u00f6chstes Sehnen", "tokens": ["Ihr", "h\u00f6chs\u00b7tes", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Von dir verk\u00fcndet,", "tokens": ["Von", "dir", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ihr tiefstes Leiden", "tokens": ["Ihr", "tiefs\u00b7tes", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Von dir verkl\u00e4rt.", "tokens": ["Von", "dir", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und nicht geschwiegen", "tokens": ["Und", "nicht", "ge\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKNEG", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Von deinem Schmerze,", "tokens": ["Von", "dei\u00b7nem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Da\u00df allzu grausam", "tokens": ["Da\u00df", "all\u00b7zu", "grau\u00b7sam"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PTKA", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Die lange Frist,", "tokens": ["Die", "lan\u00b7ge", "Frist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Die von dem Neuland", "tokens": ["Die", "von", "dem", "Neu\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "In deiner Seele", "tokens": ["In", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Zum neuen Lande", "tokens": ["Zum", "neu\u00b7en", "Lan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Des Lebens mi\u00dft!", "tokens": ["Des", "Le\u00b7bens", "mi\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Spritzende Funken,", "tokens": ["Sprit\u00b7zen\u00b7de", "Fun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sausende R\u00e4der,", "tokens": ["Sau\u00b7sen\u00b7de", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00fchende Menschen", "tokens": ["M\u00fc\u00b7hen\u00b7de", "Men\u00b7schen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wandeln die Welt.", "tokens": ["Wan\u00b7deln", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Menschen sind Herzen,", "tokens": ["Men\u00b7schen", "sind", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Massen sind Seelen,", "tokens": ["Mas\u00b7sen", "sind", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Geistermillionen", "tokens": ["Geis\u00b7ter\u00b7mil\u00b7li\u00b7o\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Glimmen und gl\u00fchn.", "tokens": ["Glim\u00b7men", "und", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sei's an der Esse,", "tokens": ["Sei's", "an", "der", "Es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Sei es am Webstuhl,", "tokens": ["Sei", "es", "am", "Web\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Sei es am Bauger\u00fcst,", "tokens": ["Sei", "es", "am", "Bau\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Sei es am Pflug \u2013", "tokens": ["Sei", "es", "am", "Pflug", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Sei's auf der Berge H\u00f6h,", "tokens": ["Sei's", "auf", "der", "Ber\u00b7ge", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Sei's in der Grube,", "tokens": ["Sei's", "in", "der", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Sei es im M\u00e4nnerrat,", "tokens": ["Sei", "es", "im", "M\u00e4n\u00b7ner\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sei es am Herd \u2013", "tokens": ["Sei", "es", "am", "Herd", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wo nur mein Blick hinschweift", "tokens": ["Wo", "nur", "mein", "Blick", "hin\u00b7schweift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "\u00dcber die Lande weit,", "tokens": ["\u00dc\u00b7ber", "die", "Lan\u00b7de", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "W\u00e4chst eine neue Welt", "tokens": ["W\u00e4chst", "ei\u00b7ne", "neu\u00b7e", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Drunten empor.", "tokens": ["Drun\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "S\u00e4h sie mein Auge nicht,", "tokens": ["S\u00e4h", "sie", "mein", "Au\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "M\u00fc\u00dft' ich ein Blinder sein", "tokens": ["M\u00fc\u00dft'", "ich", "ein", "Blin\u00b7der", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Oder ein Heuchler,", "tokens": ["O\u00b7der", "ein", "Heuch\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Wollt' ich's nicht sehn:", "tokens": ["Wollt'", "ich's", "nicht", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Kommendes Erd