{"textgrid.poem.64472": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "94. In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsohn, reize nicht durch deinen Witz die Thoren!\u00ab", "tokens": ["\u00bb", "sohn", ",", "rei\u00b7ze", "nicht", "durch", "dei\u00b7nen", "Witz", "die", "Tho\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Warnung hat vordem mein Vater oft verloren;", "tokens": ["Die", "War\u00b7nung", "hat", "vor\u00b7dem", "mein", "Va\u00b7ter", "oft", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Les't auch, warum sein Wort bey mir so wenig galt:", "tokens": ["Les't", "auch", ",", "wa\u00b7rum", "sein", "Wort", "bey", "mir", "so", "we\u00b7nig", "galt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPOSAT", "NN", "APPR", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er, ohne Falsch, wohlth\u00e4tig, Gott ergeben,", "tokens": ["Er", ",", "oh\u00b7ne", "Falsch", ",", "wohlt\u00b7h\u00e4\u00b7tig", ",", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "NN", "$,", "ADJD", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich nur Thoren reimend schalt,", "tokens": ["Wenn", "ich", "nur", "Tho\u00b7ren", "rei\u00b7mend", "schalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schalt gar Juristen durch sein Leben.", "tokens": ["Schalt", "gar", "Ju\u00b7ris\u00b7ten", "durch", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Mancher w\u00fcrde mich um diesen Vers verklagen,", "tokens": ["Wie", "Man\u00b7cher", "w\u00fcr\u00b7de", "mich", "um", "die\u00b7sen", "Vers", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, ", "tokens": ["Dir", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "+", "measure": "single.up"}}}}}