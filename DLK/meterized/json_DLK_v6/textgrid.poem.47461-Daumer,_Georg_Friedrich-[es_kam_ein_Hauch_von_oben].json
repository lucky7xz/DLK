{"textgrid.poem.47461": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es kam ein Hauch von oben]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Hauch von oben,", "tokens": ["Es", "kam", "ein", "Hauch", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mir in's Ohr die Worte blies:", "tokens": ["Der", "mir", "in's", "Ohr", "die", "Wor\u00b7te", "blies", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnicht w\u00e4hn' aus eignem Innern", "tokens": ["\u00bb", "nicht", "w\u00e4hn'", "aus", "eig\u00b7nem", "In\u00b7nern"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entstr\u00f6me dein Gesang, Hafis!", "tokens": ["Ent\u00b7str\u00f6\u00b7me", "dein", "Ge\u00b7sang", ",", "Ha\u00b7fis", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Vom Urbeginn der Zeiten", "tokens": ["Vom", "Ur\u00b7be\u00b7ginn", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Rosen und auf Lilien", "tokens": ["Auf", "Ro\u00b7sen", "und", "auf", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steh'n seine Zauberformeln", "tokens": ["Steh'n", "sei\u00b7ne", "Zau\u00b7ber\u00b7for\u00b7meln"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschrieben hoch im Paradies.\u00ab", "tokens": ["Ge\u00b7schrie\u00b7ben", "hoch", "im", "Pa\u00b7ra\u00b7dies", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}