{"textgrid.poem.55023": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe, neues Leben", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz, mein Herz, was soll das geben?", "tokens": ["Herz", ",", "mein", "Herz", ",", "was", "soll", "das", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was bedr\u00e4nget dich so sehr?", "tokens": ["Was", "be\u00b7dr\u00e4n\u00b7get", "dich", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch ein fremdes, neues Leben!", "tokens": ["Welch", "ein", "frem\u00b7des", ",", "neu\u00b7es", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich erkenne dich nicht mehr.", "tokens": ["Ich", "er\u00b7ken\u00b7ne", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg ist alles, was du liebtest,", "tokens": ["Weg", "ist", "al\u00b7les", ",", "was", "du", "lieb\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weg, warum du dich betr\u00fcbtest,", "tokens": ["Weg", ",", "wa\u00b7rum", "du", "dich", "be\u00b7tr\u00fcb\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weg dein Flei\u00df und deine Ruh \u2013", "tokens": ["Weg", "dein", "Flei\u00df", "und", "dei\u00b7ne", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, wie kamst du nur dazu!", "tokens": ["Ach", ",", "wie", "kamst", "du", "nur", "da\u00b7zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fesselt dich die Jugendbl\u00fcte,", "tokens": ["Fes\u00b7selt", "dich", "die", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese liebliche Gestalt,", "tokens": ["Die\u00b7se", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Blick voll Treu und G\u00fcte", "tokens": ["Die\u00b7ser", "Blick", "voll", "Treu", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit unendlicher Gewalt?", "tokens": ["Mit", "un\u00b7end\u00b7li\u00b7cher", "Ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will ich rasch mich ihr entziehen,", "tokens": ["Will", "ich", "rasch", "mich", "ihr", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich ermannen, ihr entfliehen,", "tokens": ["Mich", "er\u00b7man\u00b7nen", ",", "ihr", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchret mich im Augenblick,", "tokens": ["F\u00fch\u00b7ret", "mich", "im", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, mein Weg zu ihr zur\u00fcck.", "tokens": ["Ach", ",", "mein", "Weg", "zu", "ihr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und an diesem Zauberf\u00e4dchen,", "tokens": ["Und", "an", "die\u00b7sem", "Zau\u00b7ber\u00b7f\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich nicht zerrei\u00dfen l\u00e4\u00dft,", "tokens": ["Das", "sich", "nicht", "zer\u00b7rei\u00b7\u00dfen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt das liebe lose M\u00e4dchen", "tokens": ["H\u00e4lt", "das", "lie\u00b7be", "lo\u00b7se", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich so wider Willen fest;", "tokens": ["Mich", "so", "wi\u00b7der", "Wil\u00b7len", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df in ihrem Zauberkreise", "tokens": ["Mu\u00df", "in", "ih\u00b7rem", "Zau\u00b7ber\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben nun auf ihre Weise.", "tokens": ["Le\u00b7ben", "nun", "auf", "ih\u00b7re", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Ver\u00e4ndrung, ach, wie gro\u00df!", "tokens": ["Die", "Ver\u00b7\u00e4n\u00b7drung", ",", "ach", ",", "wie", "gro\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebe! Liebe! la\u00df mich los!", "tokens": ["Lie\u00b7be", "!", "Lie\u00b7be", "!", "la\u00df", "mich", "los", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}