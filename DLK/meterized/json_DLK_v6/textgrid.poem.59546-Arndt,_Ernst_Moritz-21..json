{"textgrid.poem.59546": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie sehr tut mich verlangen", "tokens": ["O", "wie", "sehr", "tut", "mich", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den s\u00fc\u00dfen Himmelsauen,", "tokens": ["Nach", "den", "s\u00fc\u00b7\u00dfen", "Him\u00b7mel\u00b7sau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Tage selig prangen,", "tokens": ["Wo", "die", "Ta\u00b7ge", "se\u00b7lig", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die N\u00e4chte Wonne tauen,", "tokens": ["Wo", "die", "N\u00e4ch\u00b7te", "Won\u00b7ne", "tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Unschuld und die Freude", "tokens": ["Wo", "die", "Un\u00b7schuld", "und", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehn als Himmelswache beide!", "tokens": ["Stehn", "als", "Him\u00b7mels\u00b7wa\u00b7che", "bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie sehr tu' ich mich sehnen", "tokens": ["O", "wie", "sehr", "tu'", "ich", "mich", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den s\u00fc\u00dfen Paradiesen,", "tokens": ["Nach", "den", "s\u00fc\u00b7\u00dfen", "Pa\u00b7ra\u00b7die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie benetzt von Trauertr\u00e4nen", "tokens": ["Nie", "be\u00b7netzt", "von", "Trau\u00b7er\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die kalten Erdenwiesen!", "tokens": ["Wie", "die", "kal\u00b7ten", "Er\u00b7den\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelsrosen, Himmelsnelken", "tokens": ["Him\u00b7mels\u00b7ro\u00b7sen", ",", "Him\u00b7mels\u00b7nel\u00b7ken"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00fchen dort, die nie verwelken.", "tokens": ["Bl\u00fc\u00b7hen", "dort", ",", "die", "nie", "ver\u00b7wel\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O die sch\u00f6nen Reigent\u00e4nze,", "tokens": ["O", "die", "sch\u00f6\u00b7nen", "Rei\u00b7gen\u00b7t\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche selige Engel schwingen!", "tokens": ["Wel\u00b7che", "se\u00b7li\u00b7ge", "En\u00b7gel", "schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "O die immergr\u00fcnen Kr\u00e4nze,", "tokens": ["O", "die", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Frommen Stirn umschlingen!", "tokens": ["Die", "der", "From\u00b7men", "Stirn", "um\u00b7schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Land der Sehnsucht, Land der Frommen,", "tokens": ["Land", "der", "Sehn\u00b7sucht", ",", "Land", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O wann werd' ich zu dir kommen!", "tokens": ["O", "wann", "werd'", "ich", "zu", "dir", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! die Erde ist ein Schwanken", "tokens": ["Ach", "!", "die", "Er\u00b7de", "ist", "ein", "Schwan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf und ab von Ruh' zum Streite,", "tokens": ["Auf", "und", "ab", "von", "Ruh'", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel wollen die Gedanken,", "tokens": ["Him\u00b7mel", "wol\u00b7len", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die S\u00fcnde stellt auf Beute:", "tokens": ["Doch", "die", "S\u00fcn\u00b7de", "stellt", "auf", "Beu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drinnen lechzet Sternenliebe,", "tokens": ["Drin\u00b7nen", "lech\u00b7zet", "Ster\u00b7nen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drau\u00dfen locken Erdentriebe.", "tokens": ["Drau\u00b7\u00dfen", "lo\u00b7cken", "Er\u00b7den\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O du s\u00fc\u00dfer Himmelsfrieden,", "tokens": ["O", "du", "s\u00fc\u00b7\u00dfer", "Him\u00b7mels\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm mit deiner Engelmilde!", "tokens": ["Komm", "mit", "dei\u00b7ner", "En\u00b7gel\u00b7mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchre doch den Streitesm\u00fcden", "tokens": ["F\u00fch\u00b7re", "doch", "den", "Strei\u00b7tes\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder auf die Lustgefilde,", "tokens": ["Wie\u00b7der", "auf", "die", "Lust\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir wie die Kinder spielten", "tokens": ["Wo", "wir", "wie", "die", "Kin\u00b7der", "spiel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "KOKOM", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nur Lust und Unschuld f\u00fchlten.", "tokens": ["Und", "nur", "Lust", "und", "Un\u00b7schuld", "f\u00fchl\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Komm, du s\u00fc\u00dfer Friede! kehre", "tokens": ["Komm", ",", "du", "s\u00fc\u00b7\u00dfer", "Frie\u00b7de", "!", "keh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Kinderfreuden wieder!", "tokens": ["Mit", "den", "Kin\u00b7der\u00b7freu\u00b7den", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiller Engel, komm und lehre", "tokens": ["Stil\u00b7ler", "En\u00b7gel", ",", "komm", "und", "leh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die alte Unschuld wieder,", "tokens": ["Mir", "die", "al\u00b7te", "Un\u00b7schuld", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich schon auf diesen Auen", "tokens": ["Da\u00df", "ich", "schon", "auf", "die\u00b7sen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann das Leben Gottes schauen.", "tokens": ["Kann", "das", "Le\u00b7ben", "Got\u00b7tes", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}