{"textgrid.poem.46308": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Schrifft hat zimlich klar gelehrt]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schrifft hat zimlich klar gelehrt,", "tokens": ["Die", "Schrifft", "hat", "zim\u00b7lich", "klar", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wer nur in gedancken,", "tokens": ["Da\u00df", "wer", "nur", "in", "ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frey, wissend, willig, vnbeschw\u00e4rt,", "tokens": ["Frey", ",", "wis\u00b7send", ",", "wil\u00b7lig", ",", "vn\u00b7be\u00b7schw\u00e4rt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnzweifflig, vnd ohn wancken,", "tokens": ["Vn\u00b7zweif\u00b7flig", ",", "vnd", "ohn", "wan\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "APPR", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zur s\u00fcnden eingewilligt hat;", "tokens": ["Zur", "s\u00fcn\u00b7den", "ein\u00b7ge\u00b7wil\u00b7ligt", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Solchs werd so hoch erwogen,", "tokens": ["Solchs", "werd", "so", "hoch", "er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als wan er schon hett in der that,", "tokens": ["Als", "wan", "er", "schon", "hett", "in", "der", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "VAFIN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die S\u00fcnd zu werck gezogen.", "tokens": ["Die", "S\u00fcnd", "zu", "werck", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "De\u00dfwegen auch zu sterben frey", "tokens": ["De\u00df\u00b7we\u00b7gen", "auch", "zu", "ster\u00b7ben", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PTKZU", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sich bereit bef\u00fcnde,", "tokens": ["Wer", "sich", "be\u00b7reit", "be\u00b7f\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd nur im hertzen fiele bey,", "tokens": ["Vnd", "nur", "im", "hert\u00b7zen", "fie\u00b7le", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wan er f\u00fcr Christum k\u00f6ndte;", "tokens": ["Wan", "er", "f\u00fcr", "Chris\u00b7tum", "k\u00f6nd\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "VMFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hat schon vor Gott so viel gethan,", "tokens": ["Hat", "schon", "vor", "Gott", "so", "viel", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd wird so hoch erwogen,", "tokens": ["Vnd", "wird", "so", "hoch", "er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als wan er in der that voran", "tokens": ["Als", "wan", "er", "in", "der", "that", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPER", "APPR", "ART", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Marter hett vollzogen.", "tokens": ["Die", "Mar\u00b7ter", "hett", "voll\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}