{"dta.poem.14116": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der verzweifelte Liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "nl:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt', ich jagt' ger\u00fcstet", "tokens": ["Ich", "wollt'", ",", "ich", "jagt'", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und legt' die Lanze aus,", "tokens": ["Und", "legt'", "die", "Lan\u00b7ze", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jagte all' Philister", "tokens": ["Und", "jag\u00b7te", "all'", "Phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur sch\u00f6nen Welt hinaus.", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}