{"textgrid.poem.52137": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich wundert nicht, da\u00df mich und dich gibt Matho an,", "tokens": ["Mich", "wun\u00b7dert", "nicht", ",", "da\u00df", "mich", "und", "dich", "gibt", "Ma\u00b7tho", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "KON", "PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er den leeren Bauch mit nichts sonst f\u00fcllen kan:", "tokens": ["Weil", "er", "den", "lee\u00b7ren", "Bauch", "mit", "nichts", "sonst", "f\u00fcl\u00b7len", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das wundert mich, da\u00df sich sein Wirth l\u00e4\u00dft so betriegen,", "tokens": ["Das", "wun\u00b7dert", "mich", ",", "da\u00df", "sich", "sein", "Wirth", "l\u00e4\u00dft", "so", "be\u00b7trie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann ihn versorgt der Wirth mit Fleisch, er ihn mit L\u00fcgen!", "tokens": ["Dann", "ihn", "ver\u00b7sorgt", "der", "Wirth", "mit", "Fleisch", ",", "er", "ihn", "mit", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}