{"textgrid.poem.61476": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Erd' und Himmel leise", "tokens": ["Durch", "Erd'", "und", "Him\u00b7mel", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinflutet eine Weise", "tokens": ["Hin\u00b7flu\u00b7tet", "ei\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sanftes Harfenwehn,", "tokens": ["Wie", "sanf\u00b7tes", "Har\u00b7fen\u00b7wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die jedem Dinge k\u00fcndet,", "tokens": ["Die", "je\u00b7dem", "Din\u00b7ge", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wozu es ward gegr\u00fcndet,", "tokens": ["Wo\u00b7zu", "es", "ward", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Woran es soll vergehn.", "tokens": ["Wo\u00b7ran", "es", "soll", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie spricht zum Adler: \u00bbDringe", "tokens": ["Sie", "spricht", "zum", "Ad\u00b7ler", ":", "\u00bb", "Drin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Sonne, bis die Schwinge", "tokens": ["Zur", "Son\u00b7ne", ",", "bis", "die", "Schwin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir trifft ein Wetterschlag!\u00ab", "tokens": ["Dir", "trifft", "ein", "Wet\u00b7ter\u00b7schlag", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Spricht zu den Wolken: \u00bbRegnet,", "tokens": ["Spricht", "zu", "den", "Wol\u00b7ken", ":", "\u00bb", "Reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$(", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Flur gesegnet,", "tokens": ["Und", "wenn", "die", "Flur", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerrinnt am goldnen Tag!\u00ab", "tokens": ["Zer\u00b7rinnt", "am", "gold\u00b7nen", "Tag", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie spricht zum Schwan: \u00bbDurchwalle", "tokens": ["Sie", "spricht", "zum", "Schwan", ":", "\u00bb", "Durch\u00b7wal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Flut und dann mit Schalle", "tokens": ["Die", "Flut", "und", "dann", "mit", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein selig Grab erwirb!\u00ab", "tokens": ["Ein", "se\u00b7lig", "Grab", "er\u00b7wirb", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sie spricht zur Feuernelke:", "tokens": ["Sie", "spricht", "zur", "Feu\u00b7ern\u00b7el\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbin Duft gl\u00fch' auf und welke!\u00ab", "tokens": ["\u00bb", "in", "Duft", "gl\u00fch'", "auf", "und", "wel\u00b7ke", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PTKVZ", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Weibe: \u00bbLieb' und stirb!\u00ab", "tokens": ["Zum", "Wei\u00b7be", ":", "\u00bb", "Lieb'", "und", "stirb", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "NN", "KON", "VVIMP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}