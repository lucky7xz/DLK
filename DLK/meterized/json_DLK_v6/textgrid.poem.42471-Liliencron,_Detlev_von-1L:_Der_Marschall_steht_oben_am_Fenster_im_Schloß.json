{"textgrid.poem.42471": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Marschall steht oben am Fenster im Schlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Marschall steht oben am Fenster im Schlo\u00df", "tokens": ["Der", "Mar\u00b7schall", "steht", "o\u00b7ben", "am", "Fens\u00b7ter", "im", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und starrt in den einsamen Garten.", "tokens": ["Und", "starrt", "in", "den", "ein\u00b7sa\u00b7men", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schon ein Jahrzehnt, das ihm verflo\u00df;", "tokens": ["Schon", "ein", "Jahr\u00b7zehnt", ",", "das", "ihm", "ver\u00b7flo\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lang l\u00e4\u00dft der Tod auf sich warten.", "tokens": ["Wie", "lang", "l\u00e4\u00dft", "der", "Tod", "auf", "sich", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u00bbwas soll mir das Leben, was soll mir der Tag,", "tokens": ["\u00bb", "was", "soll", "mir", "das", "Le\u00b7ben", ",", "was", "soll", "mir", "der", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zu dem ich mich freuen nicht kann und nicht mag;", "tokens": ["Zu", "dem", "ich", "mich", "freu\u00b7en", "nicht", "kann", "und", "nicht", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "PTKNEG", "VMFIN", "KON", "PTKNEG", "VMFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "L\u00e4ngst bin ich vergessen, vergessen.", "tokens": ["L\u00e4ngst", "bin", "ich", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und nicht ertr\u00fcg ich, wenn je ein Soldat", "tokens": ["Und", "nicht", "er\u00b7tr\u00fcg", "ich", ",", "wenn", "je", "ein", "Sol\u00b7dat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei meinem Hause marschierte,", "tokens": ["Vor\u00b7bei", "mei\u00b7nem", "Hau\u00b7se", "mar\u00b7schier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und gar, wenn hier unten im staubigen Staat", "tokens": ["Und", "gar", ",", "wenn", "hier", "un\u00b7ten", "im", "stau\u00b7bi\u00b7gen", "Staat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Bataillon pr\u00e4sentierte.", "tokens": ["Ein", "Ba\u00b7tail\u00b7lon", "pr\u00e4\u00b7sen\u00b7tier\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zehn Jahre bald sah ich kein Regiment;", "tokens": ["Zehn", "Jah\u00b7re", "bald", "sah", "ich", "kein", "Re\u00b7gi\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und z\u00f6g eins vorbei, dann w\u00e4r es mein End,", "tokens": ["Und", "z\u00f6g", "eins", "vor\u00b7bei", ",", "dann", "w\u00e4r", "es", "mein", "End", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich k\u00f6nnts nicht ertragen, ertragen.\u00ab", "tokens": ["Ich", "k\u00f6nnts", "nicht", "er\u00b7tra\u00b7gen", ",", "er\u00b7tra\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Horch! Horch! Pumplum, ganz schwach und leis,", "tokens": ["Horch", "!", "Horch", "!", "Pum\u00b7plum", ",", "ganz", "schwach", "und", "leis", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Trommelget\u00f6n in der Leere.", "tokens": ["Wie", "Trom\u00b7mel\u00b7ge\u00b7t\u00f6n", "in", "der", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Walde dort dr\u00fcben, im Sonnenstrahl hei\u00df,", "tokens": ["Im", "Wal\u00b7de", "dort", "dr\u00fc\u00b7ben", ",", "im", "Son\u00b7nen\u00b7strahl", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es blitzen wohl tausend Gewehre.", "tokens": ["Es", "blit\u00b7zen", "wohl", "tau\u00b7send", "Ge\u00b7weh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nun zieht es heran, nun zeigt es sich schon,", "tokens": ["Nun", "zieht", "es", "he\u00b7ran", ",", "nun", "zeigt", "es", "sich", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit lustigen Liedern ein Bataillon,", "tokens": ["Mit", "lus\u00b7ti\u00b7gen", "Lie\u00b7dern", "ein", "Ba\u00b7tail\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Soldatenges\u00e4nge, Ges\u00e4nge.", "tokens": ["Sol\u00b7da\u00b7ten\u00b7ge\u00b7s\u00e4n\u00b7ge", ",", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und dem Marschall wird kalt, und der Marschall wird bleich,", "tokens": ["Und", "dem", "Mar\u00b7schall", "wird", "kalt", ",", "und", "der", "Mar\u00b7schall", "wird", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Es beben ihm alle Glieder.", "tokens": ["Es", "be\u00b7ben", "ihm", "al\u00b7le", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Rasch st\u00fcrzt er ins Zimmer; im Waffenrock gleich", "tokens": ["Rasch", "st\u00fcrzt", "er", "ins", "Zim\u00b7mer", ";", "im", "Waf\u00b7fen\u00b7rock", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$.", "APPRART", "NN", "ADV"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Steht er am Fenster wieder.", "tokens": ["Steht", "er", "am", "Fens\u00b7ter", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Knopfloch h\u00e4ngt am blutroten Band,", "tokens": ["Im", "Kno\u00b7pfloch", "h\u00e4ngt", "am", "blut\u00b7ro\u00b7ten", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zum ersten Mal tr\u00e4gt ers, seitdem er verbannt,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "tr\u00e4gt", "ers", ",", "seit\u00b7dem", "er", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Das Kreuz der Ehre, der Ehre.", "tokens": ["Das", "Kreuz", "der", "Eh\u00b7re", ",", "der", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Bataillon steht links eingeschwenkt,", "tokens": ["Das", "Ba\u00b7tail\u00b7lon", "steht", "links", "ein\u00b7ge\u00b7schwenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Kommandeur vor der Mitte;", "tokens": ["Der", "Kom\u00b7man\u00b7deur", "vor", "der", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fahne ist tief zur Erde gesenkt,", "tokens": ["Die", "Fah\u00b7ne", "ist", "tief", "zur", "Er\u00b7de", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie eine stumme Bitte.", "tokens": ["Wie", "ei\u00b7ne", "stum\u00b7me", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch dann bricht ein Hurra wie Donner heraus,", "tokens": ["Doch", "dann", "bricht", "ein", "Hur\u00b7ra", "wie", "Don\u00b7ner", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Burghof zittert, der Garten, das Haus:", "tokens": ["Der", "Burg\u00b7hof", "zit\u00b7tert", ",", "der", "Gar\u00b7ten", ",", "das", "Haus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es lebe der Kaiser, der Kaiser!", "tokens": ["Es", "le\u00b7be", "der", "Kai\u00b7ser", ",", "der", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und in Sektionen rechts abgeschwenkt,", "tokens": ["Und", "in", "Sek\u00b7ti\u00b7o\u00b7nen", "rechts", "ab\u00b7ge\u00b7schwenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Kommandeur an der Tete.", "tokens": ["Der", "Kom\u00b7man\u00b7deur", "an", "der", "Te\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Schlo\u00dfherr hat schwer das Haupt gesenkt:", "tokens": ["Der", "Schlo\u00df\u00b7herr", "hat", "schwer", "das", "Haupt", "ge\u00b7senkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die Fahne, sie wehte, sie wehte.", "tokens": ["Die", "Fah\u00b7ne", ",", "sie", "weh\u00b7te", ",", "sie", "weh\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie wehte noch immer, die Trommel klang,", "tokens": ["Sie", "weh\u00b7te", "noch", "im\u00b7mer", ",", "die", "Trom\u00b7mel", "klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als der Marschall sich \u00fcber die Br\u00fcstung schwang \u2013", "tokens": ["Als", "der", "Mar\u00b7schall", "sich", "\u00fc\u00b7ber", "die", "Br\u00fcs\u00b7tung", "schwang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Lebt wohl, Soldaten, Soldaten.", "tokens": ["Lebt", "wohl", ",", "Sol\u00b7da\u00b7ten", ",", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}