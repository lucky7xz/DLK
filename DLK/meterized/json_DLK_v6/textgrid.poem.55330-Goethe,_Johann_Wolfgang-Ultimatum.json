{"textgrid.poem.55330": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ultimatum", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so sag ich zum letzten Male:", "tokens": ["Und", "so", "sag", "ich", "zum", "letz\u00b7ten", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Natur hat weder Kern", "tokens": ["Na\u00b7tur", "hat", "we\u00b7der", "Kern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch Schale;", "tokens": ["Noch", "Scha\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du pr\u00fcfe dich nur allermeist,", "tokens": ["Du", "pr\u00fc\u00b7fe", "dich", "nur", "al\u00b7ler\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob du Kern oder Schale seist!", "tokens": ["Ob", "du", "Kern", "o\u00b7der", "Scha\u00b7le", "seist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbwir kennen dich, du Schalk!", "tokens": ["\u00bb", "wir", "ken\u00b7nen", "dich", ",", "du", "Schalk", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du machst nur Possen;", "tokens": ["Du", "machst", "nur", "Pos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vor unsrer Nase doch", "tokens": ["Vor", "uns\u00b7rer", "Na\u00b7se", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist viel verschlossen.\u00ab", "tokens": ["Ist", "viel", "ver\u00b7schlos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr folget falscher Spur,", "tokens": ["Ihr", "fol\u00b7get", "fal\u00b7scher", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Denkt nicht, wir scherzen!", "tokens": ["Denkt", "nicht", ",", "wir", "scher\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist nicht der Kern der Natur", "tokens": ["Ist", "nicht", "der", "Kern", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Menschen im Herzen?", "tokens": ["Men\u00b7schen", "im", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}