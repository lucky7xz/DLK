{"textgrid.poem.60040": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe Nachtigall!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Nachtigall!", "tokens": ["Lie\u00b7be", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sch\u00f6ner Bl\u00fcthenregen!", "tokens": ["Sch\u00f6\u00b7ner", "Bl\u00fc\u00b7then\u00b7re\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die Knospen all'", "tokens": ["Wie", "die", "Knos\u00b7pen", "all'"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PIAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unter Lerchen-Schl\u00e4gen", "tokens": ["Un\u00b7ter", "Ler\u00b7chen\u00b7Schl\u00e4\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "An der Quelle sich bewegen!", "tokens": ["An", "der", "Quel\u00b7le", "sich", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O wie lieblich Alles ist!", "tokens": ["O", "wie", "lieb\u00b7lich", "Al\u00b7les", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wenig Freude", "tokens": ["A\u00b7ber", "we\u00b7nig", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "F\u00fcr den Mann im gr\u00fcnen Kleide,", "tokens": ["F\u00fcr", "den", "Mann", "im", "gr\u00fc\u00b7nen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welcher dort gekommen ist,", "tokens": ["Wel\u00b7cher", "dort", "ge\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur die B\u00e4ume z\u00e4hlt und mi\u00dft,", "tokens": ["Nur", "die", "B\u00e4u\u00b7me", "z\u00e4hlt", "und", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und das frische Laub zu sehen,", "tokens": ["Und", "das", "fri\u00b7sche", "Laub", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die Lerche zu verstehen,", "tokens": ["Und", "die", "Ler\u00b7che", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und den Bl\u00fcthenkranz am Silberquell vergi\u00dft!", "tokens": ["Und", "den", "Bl\u00fc\u00b7then\u00b7kranz", "am", "Sil\u00b7ber\u00b7quell", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}