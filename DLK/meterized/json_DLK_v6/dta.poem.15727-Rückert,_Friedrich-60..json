{"dta.poem.15727": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O klage nicht, mein Geist, im finstern Hause b\u00e4nglich,", "tokens": ["O", "kla\u00b7ge", "nicht", ",", "mein", "Geist", ",", "im", "fins\u00b7tern", "Hau\u00b7se", "b\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir verliehene Vernunft sei unzul\u00e4nglich.", "tokens": ["Die", "dir", "ver\u00b7lie\u00b7he\u00b7ne", "Ver\u00b7nunft", "sei", "un\u00b7zu\u00b7l\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Hauses Mitte macht die Leuchte hell genung,", "tokens": ["Des", "Hau\u00b7ses", "Mit\u00b7te", "macht", "die", "Leuch\u00b7te", "hell", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die Winkel nur birgt sich die D\u00e4mmerung.", "tokens": ["Und", "in", "die", "Win\u00b7kel", "nur", "birgt", "sich", "die", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In welchem Winkel du was sehn willst, o Gesell,", "tokens": ["In", "wel\u00b7chem", "Win\u00b7kel", "du", "was", "sehn", "willst", ",", "o", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PIS", "VVINF", "VMFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trag' hin die Leuchte schnell, so ist der Winkel hell.", "tokens": ["Trag'", "hin", "die", "Leuch\u00b7te", "schnell", ",", "so", "ist", "der", "Win\u00b7kel", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}