{"textgrid.poem.29895": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andren gehet auff die Sonne, wann sie uns geht nieder;", "tokens": ["An\u00b7dren", "ge\u00b7het", "auff", "die", "Son\u00b7ne", ",", "wann", "sie", "uns", "geht", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wann sie andren nieder geht, k\u00fcmt sie zu uns wieder.", "tokens": ["Wann", "sie", "an\u00b7dren", "nie\u00b7der", "geht", ",", "k\u00fcmt", "sie", "zu", "uns", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PTKVZ", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Was uns GOTT nicht heute schenckte, kan er morgen schicken,", "tokens": ["Was", "uns", "GoTT", "nicht", "heu\u00b7te", "schenck\u00b7te", ",", "kan", "er", "mor\u00b7gen", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NE", "PTKNEG", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Kan uns, was er heute schickte, morgen auch entz\u00fccken.", "tokens": ["Kan", "uns", ",", "was", "er", "heu\u00b7te", "schick\u00b7te", ",", "mor\u00b7gen", "auch", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}