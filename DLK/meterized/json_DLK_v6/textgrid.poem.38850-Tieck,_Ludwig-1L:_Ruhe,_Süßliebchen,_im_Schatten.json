{"textgrid.poem.38850": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruhe, S\u00fc\u00dfliebchen, im Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhe, S\u00fc\u00dfliebchen, im Schatten", "tokens": ["Ru\u00b7he", ",", "S\u00fc\u00df\u00b7lieb\u00b7chen", ",", "im", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der gr\u00fcnen d\u00e4mmernden Nacht,", "tokens": ["Der", "gr\u00fc\u00b7nen", "d\u00e4m\u00b7mern\u00b7den", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es s\u00e4uselt das Gras auf den Matten,", "tokens": ["Es", "s\u00e4u\u00b7selt", "das", "Gras", "auf", "den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es f\u00e4chelt und k\u00fchlt dich der Schatten,", "tokens": ["Es", "f\u00e4\u00b7chelt", "und", "k\u00fchlt", "dich", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und treue Liebe wacht.", "tokens": ["Und", "treu\u00b7e", "Lie\u00b7be", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schlafe, schlaf ein,", "tokens": ["Schla\u00b7fe", ",", "schlaf", "ein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Leiser rauschet der Hain, \u2013", "tokens": ["Lei\u00b7ser", "rau\u00b7schet", "der", "Hain", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ewig bin ich dein.", "tokens": ["E\u00b7wig", "bin", "ich", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schweigt, ihr versteckten Ges\u00e4nge,", "tokens": ["Schweigt", ",", "ihr", "ver\u00b7steck\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und st\u00f6rt nicht die s\u00fc\u00dfeste Ruh!", "tokens": ["Und", "st\u00f6rt", "nicht", "die", "s\u00fc\u00b7\u00dfes\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es lauscht der V\u00f6gel Gedr\u00e4nge,", "tokens": ["Es", "lauscht", "der", "V\u00f6\u00b7gel", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es ruhen die lauten Ges\u00e4nge,", "tokens": ["Es", "ru\u00b7hen", "die", "lau\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schlie\u00df, Liebchen, dein Auge zu.", "tokens": ["Schlie\u00df", ",", "Lieb\u00b7chen", ",", "dein", "Au\u00b7ge", "zu", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schlafe, schlaf ein,", "tokens": ["Schla\u00b7fe", ",", "schlaf", "ein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im d\u00e4mmernden Schein, \u2013", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Schein", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ich will dein W\u00e4chter sein.", "tokens": ["Ich", "will", "dein", "W\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Murmelt fort ihr Melodieen,", "tokens": ["Mur\u00b7melt", "fort", "ihr", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rausche nur, du stiller Bach,", "tokens": ["Rau\u00b7sche", "nur", ",", "du", "stil\u00b7ler", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Liebesphantasieen", "tokens": ["Sch\u00f6\u00b7ne", "Lie\u00b7be\u00b7sphan\u00b7ta\u00b7si\u00b7e\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sprechen in den Melodieen,", "tokens": ["Spre\u00b7chen", "in", "den", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zarte Tr\u00e4ume schwimmen nach.", "tokens": ["Zar\u00b7te", "Tr\u00e4u\u00b7me", "schwim\u00b7men", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den fl\u00fcsternden Hain", "tokens": ["Durch", "den", "fl\u00fcs\u00b7tern\u00b7den", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Schw\u00e4rmen goldene Bienelein,", "tokens": ["Schw\u00e4r\u00b7men", "gol\u00b7de\u00b7ne", "Bie\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Und summen zum Schlummer dich ein.", "tokens": ["Und", "sum\u00b7men", "zum", "Schlum\u00b7mer", "dich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}