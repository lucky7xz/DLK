{"dta.poem.13241": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich h\u00f6re, da\u00df die Mord- und Schlacht-Trompete klingt;", "tokens": ["\u201e", "ich", "h\u00f6\u00b7re", ",", "da\u00df", "die", "Mord", "und", "Schlacht\u00b7Trom\u00b7pe\u00b7te", "klingt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ART", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eda\u00df man schon wiederum auf unsre Mauern dringt;", "tokens": ["\u201e", "da\u00df", "man", "schon", "wie\u00b7de\u00b7rum", "auf", "uns\u00b7re", "Mau\u00b7ern", "dringt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eman will noch einen Streich mit jenem Rach-Stahl wagen;", "tokens": ["\u201e", "man", "will", "noch", "ei\u00b7nen", "Streich", "mit", "je\u00b7nem", "Rach\u00b7Stahl", "wa\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "140\u201dH\u00f6rt ihr nicht an dem Wall der Grenzen Lermen schlagen?", "tokens": ["\"", "H\u00f6rt", "ihr", "nicht", "an", "dem", "Wall", "der", "Gren\u00b7zen", "Ler\u00b7men", "schla\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie\u00df schreckt mir weder Muth, noch das Vertrauen ab,", "tokens": ["\u201e", "die\u00df", "schreckt", "mir", "we\u00b7der", "Muth", ",", "noch", "das", "Ver\u00b7trau\u00b7en", "ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "KON", "NN", "$,", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas ich zu GOttes Schuz und zu den Rechten hab.", "tokens": ["\u201e", "das", "ich", "zu", "Got\u00b7tes", "Schuz", "und", "zu", "den", "Rech\u00b7ten", "hab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "APPR", "NN", "NN", "KON", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie Menschen fechten zwar, sie schaffen, herrschen, wachen,", "tokens": ["\u201e", "die", "Men\u00b7schen", "fech\u00b7ten", "zwar", ",", "sie", "schaf\u00b7fen", ",", "herr\u00b7schen", ",", "wa\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VVINF", "$,", "VVIZU", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eallein der Himmel bringt den Ausgang aller Sachen;", "tokens": ["\u201e", "al\u00b7lein", "der", "Him\u00b7mel", "bringt", "den", "Aus\u00b7gang", "al\u00b7ler", "Sa\u00b7chen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil diese Rede den 15. Mer-\nzen 1745. geschieht/ so ziehlt auch der\n\nInnhalt auf das jenige/ was zu der-\nselbigen Zeit vorgegangen.", "tokens": ["Weil", "die\u00b7se", "Re\u00b7de", "den", "15.", "Mer", "zen", "1745", ".", "ge\u00b7schieht", "/", "so", "ziehlt", "auch", "der", "Inn\u00b7halt", "auf", "das", "je\u00b7ni\u00b7ge", "/", "was", "zu", "der", "sel\u00b7bi\u00b7gen", "Zeit", "vor\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "ordinal", "word", "word", "number", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "ADJA", "TRUNC", "NN", "CARD", "$.", "VVFIN", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "PIAT", "$(", "PWS", "APPR", "TRUNC", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+--+-+-+---+-+---+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "145\u201dWann auch der Feinde Macht un\u00fcberwindlich lie\u00df,", "tokens": ["\"", "Wann", "auch", "der", "Fein\u00b7de", "Macht", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "lie\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edreht er den Ruthen-Streich, so folgt der Sieg gewi\u00df.", "tokens": ["\u201e", "dreht", "er", "den", "Ru\u00b7then\u00b7Streich", ",", "so", "folgt", "der", "Sieg", "ge\u00b7wi\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}