{"textgrid.poem.40346": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Universa", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sahst durch meine Seele in die Welt,", "tokens": ["Du", "sahst", "durch", "mei\u00b7ne", "See\u00b7le", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "es war auch Deine Seele: still versanken", "tokens": ["es", "war", "auch", "Dei\u00b7ne", "See\u00b7le", ":", "still", "ver\u00b7san\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "im Strom des Schauens zwischen uns die Schranken,", "tokens": ["im", "Strom", "des", "Schau\u00b7ens", "zwi\u00b7schen", "uns", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es ruhten Welt und Du in Mir gesellt.", "tokens": ["es", "ruh\u00b7ten", "Welt", "und", "Du", "in", "Mir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "PPER", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Auge sah ich grenzenlos erhellt:", "tokens": ["Dein", "Au\u00b7ge", "sah", "ich", "gren\u00b7zen\u00b7los", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erleuchtung fluteten, Erleuchtung tranken", "tokens": ["Er\u00b7leuch\u00b7tung", "flu\u00b7te\u00b7ten", ",", "Er\u00b7leuch\u00b7tung", "tran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zusammenstr\u00f6mend unsre Zwiegedanken,", "tokens": ["zu\u00b7sam\u00b7men\u00b7str\u00f6\u00b7mend", "uns\u00b7re", "Zwie\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in Deiner Seele ruhte Meine Welt.", "tokens": ["in", "Dei\u00b7ner", "See\u00b7le", "ruh\u00b7te", "Mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ganz im Weltgrund, wo sonst blindgeballt", "tokens": ["Und", "ganz", "im", "Welt\u00b7grund", ",", "wo", "sonst", "blind\u00b7ge\u00b7ballt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PWAV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "entzweite L\u00fcste hausen voller Fehle,", "tokens": ["ent\u00b7zwei\u00b7te", "L\u00fcs\u00b7te", "hau\u00b7sen", "vol\u00b7ler", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "enth\u00fcllten sich auf einmal unsre Hehle", "tokens": ["ent\u00b7h\u00fcll\u00b7ten", "sich", "auf", "ein\u00b7mal", "uns\u00b7re", "Heh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "vereint als lauter Liebeslustgewalt.", "tokens": ["ver\u00b7eint", "als", "lau\u00b7ter", "Lie\u00b7bes\u00b7lust\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn Liebe ist die Freiheit der Gestalt", "tokens": ["Denn", "Lie\u00b7be", "ist", "die", "Frei\u00b7heit", "der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom Bann der Welt, vom Wahn der eignen Seele.", "tokens": ["vom", "Bann", "der", "Welt", ",", "vom", "Wahn", "der", "eig\u00b7nen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.6": {"line.1": {"text": "Das ist Liebe. Und mit leichtem Sinn", "tokens": ["Das", "ist", "Lie\u00b7be", ".", "Und", "mit", "leich\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "g\u00e4b'ich all mein ernstes Selbstbeschauen", "tokens": ["g\u00e4b'\u00b7ich", "all", "mein", "erns\u00b7tes", "Selbst\u00b7be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "spielbereit f\u00fcr Dein Empfinden hin,", "tokens": ["spiel\u00b7be\u00b7reit", "f\u00fcr", "Dein", "Emp\u00b7fin\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "du liebseligste der Frauen!", "tokens": ["du", "lieb\u00b7se\u00b7ligs\u00b7te", "der", "Frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja, solch Spiel das ganze Leben,", "tokens": ["Ja", ",", "solch", "Spiel", "das", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieberes k\u00f6nnt ich nicht erwerben;", "tokens": ["Lie\u00b7be\u00b7res", "k\u00f6nnt", "ich", "nicht", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Frohsinn hast du mir gegeben!", "tokens": ["Froh\u00b7sinn", "hast", "du", "mir", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wild und wehe und zum letzten Mal", "tokens": ["Wild", "und", "we\u00b7he", "und", "zum", "letz\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wird mein Herz an deinen Leichnam schlagen;", "tokens": ["wird", "mein", "Herz", "an", "dei\u00b7nen", "Leich\u00b7nam", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "still in unserm Freudensaal", "tokens": ["still", "in", "un\u00b7serm", "Freu\u00b7den\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wird dein steinern Bildnis ragen.", "tokens": ["wird", "dein", "stei\u00b7nern", "Bild\u00b7nis", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Einsam werd'ich wieder dann erschauern", "tokens": ["Ein\u00b7sam", "werd'\u00b7ich", "wie\u00b7der", "dann", "er\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "vor den wirren Weltgewalten;", "tokens": ["vor", "den", "wir\u00b7ren", "Welt\u00b7ge\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "oh Vernunft, sie \u00fcberdauern", "tokens": ["oh", "Ver\u00b7nunft", ",", "sie", "\u00fc\u00b7berd\u00b7au\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unser menschliches Gestalten.", "tokens": ["un\u00b7ser", "menschli\u00b7ches", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.10": {"line.1": {"text": "Bla\u00df im Leeren steht der Morgenstern,", "tokens": ["Bla\u00df", "im", "Lee\u00b7ren", "steht", "der", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "nur noch wie ein \u00fcberfl\u00fcssiges P\u00fcnktchen;", "tokens": ["nur", "noch", "wie", "ein", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7si\u00b7ges", "P\u00fcnkt\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und doch h\u00e4ngt sich immer wieder gern", "tokens": ["und", "doch", "h\u00e4ngt", "sich", "im\u00b7mer", "wie\u00b7der", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "jede Seele an dies F\u00fcnkchen.", "tokens": ["je\u00b7de", "See\u00b7le", "an", "dies", "F\u00fcnk\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bis aufs Meer hin sieht mein Geist es stehn", "tokens": ["Bis", "aufs", "Meer", "hin", "sieht", "mein", "Geist", "es", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcber tausend angstbefahrenen Gleisen,", "tokens": ["\u00fc\u00b7ber", "tau\u00b7send", "angst\u00b7be\u00b7fah\u00b7re\u00b7nen", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "sieht's in teilnahmloser Bahn sich drehn", "tokens": ["sieht's", "in", "teil\u00b7nahm\u00b7lo\u00b7ser", "Bahn", "sich", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "bis ans Ende aller Erdenreisen \u2013", "tokens": ["bis", "ans", "En\u00b7de", "al\u00b7ler", "Er\u00b7den\u00b7rei\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "sieht die Schaaren der vom Sturm Umbrandeten,", "tokens": ["sieht", "die", "Schaa\u00b7ren", "der", "vom", "Sturm", "Um\u00b7bran\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "die Myriaden der nach Rettung Winkenden,", "tokens": ["die", "My\u00b7ria\u00b7den", "der", "nach", "Ret\u00b7tung", "Win\u00b7ken\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "der Gescheiterten, Gestrandeten,", "tokens": ["der", "Ge\u00b7schei\u00b7ter\u00b7ten", ",", "Ge\u00b7stran\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.4": {"text": "der Verschmachtenden, Ertrinkenden \u2013", "tokens": ["der", "Ver\u00b7schmach\u00b7ten\u00b7den", ",", "Er\u00b7trin\u00b7ken\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$("], "meter": "--+-+-+--", "measure": "anapaest.init"}}, "stanza.13": {"line.1": {"text": "sieht sich mitgequ\u00e4lt von all der Qual:", "tokens": ["sieht", "sich", "mit\u00b7ge\u00b7qu\u00e4lt", "von", "all", "der", "Qual", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "APPR", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Seele, Seele, stirbst du nicht vor Grausen?!", "tokens": ["See\u00b7le", ",", "See\u00b7le", ",", "stirbst", "du", "nicht", "vor", "Grau\u00b7sen", "?!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber da vertreibt den tr\u00fcben Schwall", "tokens": ["A\u00b7ber", "da", "ver\u00b7treibt", "den", "tr\u00fc\u00b7ben", "Schwall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "eine Stimme, sternhin ein Erbrausen:", "tokens": ["ei\u00b7ne", "Stim\u00b7me", ",", "stern\u00b7hin", "ein", "Er\u00b7brau\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}