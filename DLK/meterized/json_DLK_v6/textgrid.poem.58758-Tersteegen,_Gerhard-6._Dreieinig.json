{"textgrid.poem.58758": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "6. Dreieinig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schaust, du liebst im Gottheitsthron", "tokens": ["Du", "schaust", ",", "du", "liebst", "im", "Got\u00b7theits\u00b7thron"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich selbst. Dies Selbst ist dein selbst ", "tokens": ["Dich", "selbst", ".", "Dies", "Selbst", "ist", "dein", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$.", "PDS", "ADV", "VAFIN", "PPOSAT", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Herz, dein's Herzens Wohlgefallen,", "tokens": ["Dein", "Herz", ",", "dein's", "Her\u00b7zens", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So da\u00df du selbst, dein ", "tokens": ["So", "da\u00df", "du", "selbst", ",", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "$,", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie im Triumph aus beiden fleu\u00dft", "tokens": ["Wie", "im", "Tri\u00b7umph", "aus", "bei\u00b7den", "fleu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Mit ew'gem Lieb's- und Freudewallen.", "tokens": ["Mit", "ew'\u00b7gem", "Lie\u00b7b'\u00b7s", "und", "Freu\u00b7de\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein'n Sohn, o ", "tokens": ["Dein'n", "Sohn", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Dein Geist rei\u00dft mich von all'm zu dir.", "tokens": ["Dein", "Geist", "rei\u00dft", "mich", "von", "all'm", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}