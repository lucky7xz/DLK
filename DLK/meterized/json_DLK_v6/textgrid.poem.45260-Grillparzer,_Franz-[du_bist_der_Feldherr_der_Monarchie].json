{"textgrid.poem.45260": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du bist der Feldherr der Monarchie]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Feldherr der Monarchie,", "tokens": ["Du", "bist", "der", "Feld\u00b7herr", "der", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch leider auch der Aristokratie,", "tokens": ["Doch", "lei\u00b7der", "auch", "der", "A\u00b7ris\u00b7to\u00b7kra\u00b7tie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo du sie findest, schonst du sie,", "tokens": ["Wo", "du", "sie", "fin\u00b7dest", ",", "schonst", "du", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6nnst ihr im Feinde Sympathie,", "tokens": ["G\u00f6nnst", "ihr", "im", "Fein\u00b7de", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kommst daher zu Ende nie.", "tokens": ["Und", "kommst", "da\u00b7her", "zu", "En\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}