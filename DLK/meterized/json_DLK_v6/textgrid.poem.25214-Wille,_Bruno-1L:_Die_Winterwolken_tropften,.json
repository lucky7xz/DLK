{"textgrid.poem.25214": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Winterwolken tropften,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Winterwolken tropften,", "tokens": ["Die", "Win\u00b7ter\u00b7wol\u00b7ken", "tropf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Gr\u00e4bern lag der Schnee.", "tokens": ["Auf", "Gr\u00e4\u00b7bern", "lag", "der", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zween hei\u00dfe Herzen klopften,", "tokens": ["Zween", "hei\u00b7\u00dfe", "Her\u00b7zen", "klopf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Scheiden tat so weh.", "tokens": ["Ihr", "Schei\u00b7den", "tat", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbund wirst du mir genommen,", "tokens": ["\u00bb", "und", "wirst", "du", "mir", "ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bitters\u00fc\u00dfer Knabe,", "tokens": ["Du", "bit\u00b7ter\u00b7s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einst sollst du wiederkommen,", "tokens": ["Einst", "sollst", "du", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich dich ewig habe.\u00ab", "tokens": ["Da\u00df", "ich", "dich", "e\u00b7wig", "ha\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Knabe hub die Augen,", "tokens": ["Der", "Kna\u00b7be", "hub", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielherbe zuckt sein Mund:", "tokens": ["Viel\u00b7her\u00b7be", "zuckt", "sein", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu hoffest noch, wir taugen", "tokens": ["\u00bb", "du", "hof\u00b7fest", "noch", ",", "wir", "tau\u00b7gen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu einem Erdenbund?", "tokens": ["Zu", "ei\u00b7nem", "Er\u00b7den\u00b7bund", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mag sein, es wird gefreiet,", "tokens": ["Mag", "sein", ",", "es", "wird", "ge\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzallerliebste mein,", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn's wei\u00dfe Lilien schneiet,", "tokens": ["Wenn's", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", "schnei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und regnet Hochzeitswein.\u00ab", "tokens": ["Und", "reg\u00b7net", "Hoch\u00b7zeits\u00b7wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er schied. Und nur im Traume", "tokens": ["Er", "schied", ".", "Und", "nur", "im", "Trau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam Trost f\u00fcr ihre Pein:", "tokens": ["Kam", "Trost", "f\u00fcr", "ih\u00b7re", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sah beim Gr\u00e4berbaume", "tokens": ["Sie", "sah", "beim", "Gr\u00e4\u00b7ber\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wein regnen, Lilien schnein.", "tokens": ["Wein", "reg\u00b7nen", ",", "Li\u00b7li\u00b7en", "schnein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und wie sie dann erwachte,", "tokens": ["Und", "wie", "sie", "dann", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So war es lauter Nichts.", "tokens": ["So", "war", "es", "lau\u00b7ter", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da weinte sie und lachte", "tokens": ["Da", "wein\u00b7te", "sie", "und", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob ihres Wahngesichts:", "tokens": ["Ob", "ih\u00b7res", "Wahn\u00b7ge\u00b7sichts", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbla\u00dft mich zum Gr\u00e4bergarten,", "tokens": ["\u00bb", "la\u00dft", "mich", "zum", "Gr\u00e4\u00b7ber\u00b7gar\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum k\u00fchlen Erdverlie\u00df,", "tokens": ["Zum", "k\u00fch\u00b7len", "Erd\u00b7ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wunder zu erwarten,", "tokens": ["Das", "Wun\u00b7der", "zu", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So doch mein Schatz verhie\u00df.", "tokens": ["So", "doch", "mein", "Schatz", "ver\u00b7hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sprach er denn nicht: gefreiet", "tokens": ["Sprach", "er", "denn", "nicht", ":", "ge\u00b7frei\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$.", "VVPP"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Wird, Allerliebste mein,", "tokens": ["Wird", ",", "Al\u00b7ler\u00b7liebs\u00b7te", "mein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn's wei\u00dfe Lilien schneiet,", "tokens": ["Wenn's", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", "schnei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und regnet Hochzeitswein?", "tokens": ["Und", "reg\u00b7net", "Hoch\u00b7zeits\u00b7wein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Dach soll mich bedecken,", "tokens": ["Ein", "Dach", "soll", "mich", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn endlich Lilien schnein,", "tokens": ["Wenn", "end\u00b7lich", "Li\u00b7li\u00b7en", "schnein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein H\u00fcgel mich verstecken,", "tokens": ["Ein", "H\u00fc\u00b7gel", "mich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn's regnet Hochzeitswein.\u00ab", "tokens": ["Wenn's", "reg\u00b7net", "Hoch\u00b7zeits\u00b7wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Bald raunten dumpfe Glocken:", "tokens": ["Bald", "raun\u00b7ten", "dump\u00b7fe", "Glo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbwillkommen unterm Dach,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "un\u00b7term", "Dach", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tu Myrten um die Locken", "tokens": ["Tu", "Myr\u00b7ten", "um", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ruh im Brautgemach!\u00ab", "tokens": ["Und", "ruh", "im", "Braut\u00b7ge\u00b7mach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nach Jahren kommt gegangen", "tokens": ["Nach", "Jah\u00b7ren", "kommt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Knabe durch das Gras,", "tokens": ["Der", "Kna\u00b7be", "durch", "das", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erblichen seine Wangen,", "tokens": ["Er\u00b7bli\u00b7chen", "sei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen kummerna\u00df.", "tokens": ["Die", "Au\u00b7gen", "kum\u00b7mer\u00b7na\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da hat sein Fu\u00df geholpert,", "tokens": ["Da", "hat", "sein", "Fu\u00df", "ge\u00b7hol\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und \u00fcbers Totenhaus", "tokens": ["Und", "\u00fc\u00b7bers", "To\u00b7ten\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist er dahingestolpert,", "tokens": ["Ist", "er", "da\u00b7hin\u00b7ge\u00b7stol\u00b7pert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Odem ging ihm aus.", "tokens": ["Der", "O\u00b7dem", "ging", "ihm", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nun horch, es lacht im Grabe:", "tokens": ["Nun", "horch", ",", "es", "lacht", "im", "Gra\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bberf\u00fcllt soll also sein,", "tokens": ["\u00bb", "er\u00b7f\u00fcllt", "soll", "al\u00b7so", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VMFIN", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Spruch, getreuer Knabe:", "tokens": ["Dein", "Spruch", ",", "ge\u00b7treu\u00b7er", "Kna\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wein regnet, Lilien schnein!", "tokens": ["Wein", "reg\u00b7net", ",", "Li\u00b7li\u00b7en", "schnein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Der Schnee sind deine Wangen,", "tokens": ["Der", "Schnee", "sind", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Augentau ist Wein.", "tokens": ["Dein", "Au\u00b7gen\u00b7tau", "ist", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun halten sich umfangen", "tokens": ["Nun", "hal\u00b7ten", "sich", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ewig Mein und Dein.\u00ab", "tokens": ["Auf", "e\u00b7wig", "Mein", "und", "Dein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "KON", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}