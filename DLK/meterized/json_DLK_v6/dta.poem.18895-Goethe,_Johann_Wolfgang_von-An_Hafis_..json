{"dta.poem.18895": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "An Hafis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Was alle wollen weisst du schon", "tokens": ["Was", "al\u00b7le", "wol\u00b7len", "weisst", "du", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "VMFIN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hast es wohl verstanden:", "tokens": ["Und", "hast", "es", "wohl", "ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Sehnsucht h\u00e4lt, von Staub zu Thron,", "tokens": ["Denn", "Sehn\u00b7sucht", "h\u00e4lt", ",", "von", "Staub", "zu", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns all\u2019 in strengen Banden.", "tokens": ["Uns", "all'", "in", "stren\u00b7gen", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es thut so weh, so wohl hernach,", "tokens": ["Es", "thut", "so", "weh", ",", "so", "wohl", "her\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer str\u00e4ubte sich dagegen?", "tokens": ["Wer", "str\u00e4ub\u00b7te", "sich", "da\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn den Hals der eine brach,", "tokens": ["Und", "wenn", "den", "Hals", "der", "ei\u00b7ne", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre bliebt verwegen.", "tokens": ["Der", "and\u00b7re", "bliebt", "ver\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verzeihe Meister, wie du weisst", "tokens": ["Ver\u00b7zei\u00b7he", "Meis\u00b7ter", ",", "wie", "du", "weisst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dass ich mich oft vermesse,", "tokens": ["Dass", "ich", "mich", "oft", "ver\u00b7mes\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie das Auge nach sich reisst", "tokens": ["Wenn", "sie", "das", "Au\u00b7ge", "nach", "sich", "reisst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wandelnde Cypresse.", "tokens": ["Die", "wan\u00b7deln\u00b7de", "Cyp\u00b7res\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie Wurzelfasern schleicht ihr Fuss", "tokens": ["Wie", "Wur\u00b7zel\u00b7fa\u00b7sern", "schleicht", "ihr", "Fuss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und buhlet mit dem Boden;", "tokens": ["Und", "buh\u00b7let", "mit", "dem", "Bo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie leicht Gew\u00f6lk verschmilzt ihr Gruss", "tokens": ["Wie", "leicht", "Ge\u00b7w\u00f6lk", "ver\u00b7schmilzt", "ihr", "Gruss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Ost-Gekos ihr Oden.", "tokens": ["Wie", "Ost\u00b7Ge\u00b7kos", "ihr", "O\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das alles dr\u00e4ngt uns ahndevoll,", "tokens": ["Das", "al\u00b7les", "dr\u00e4ngt", "uns", "ahn\u00b7de\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lock\u2019 an Locke kr\u00e4uselt,", "tokens": ["Wo", "Lock'", "an", "Lo\u00b7cke", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In brauner F\u00fclle ringelnd schwoll,", "tokens": ["In", "brau\u00b7ner", "F\u00fcl\u00b7le", "rin\u00b7gelnd", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So dann im Winde s\u00e4uselt.", "tokens": ["So", "dann", "im", "Win\u00b7de", "s\u00e4u\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun \u00f6ffnet sich die Stirne klar", "tokens": ["Nun", "\u00f6ff\u00b7net", "sich", "die", "Stir\u00b7ne", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herz damit zu gl\u00e4tten,", "tokens": ["Dein", "Herz", "da\u00b7mit", "zu", "gl\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vernimmst ein Lied so froh und wahr", "tokens": ["Ver\u00b7nimmst", "ein", "Lied", "so", "froh", "und", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Geist darin zu betten.", "tokens": ["Den", "Geist", "da\u00b7rin", "zu", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn die Lippen sich dabey", "tokens": ["Und", "wenn", "die", "Lip\u00b7pen", "sich", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf\u2019s niedlichste bewegen,", "tokens": ["Auf's", "nied\u00b7lichs\u00b7te", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie machen dich auf einmal frey", "tokens": ["Sie", "ma\u00b7chen", "dich", "auf", "ein\u00b7mal", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Fesseln dich zu legen.", "tokens": ["In", "Fes\u00b7seln", "dich", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Athem will nicht mehr zur\u00fcck", "tokens": ["Der", "A\u00b7them", "will", "nicht", "mehr", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seel\u2019 zur Seele fliehend,", "tokens": ["Die", "Seel'", "zur", "See\u00b7le", "flie\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ger\u00fcche winden sich durchs Gl\u00fcck", "tokens": ["Ge\u00b7r\u00fc\u00b7che", "win\u00b7den", "sich", "durchs", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unsichtbar wolkig ziehend.", "tokens": ["Un\u00b7sicht\u00b7bar", "wol\u00b7kig", "zie\u00b7hend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch wenn es allgewaltig brennt", "tokens": ["Doch", "wenn", "es", "all\u00b7ge\u00b7wal\u00b7tig", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann greifst du nach der Schaale:", "tokens": ["Dann", "greifst", "du", "nach", "der", "Schaa\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schenke l\u00e4uft, der Schenke k\u00f6mmt", "tokens": ["Der", "Schen\u00b7ke", "l\u00e4uft", ",", "der", "Schen\u00b7ke", "k\u00f6mmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum erst- und zweytenmale.", "tokens": ["Zum", "er\u00b7st", "und", "zwey\u00b7ten\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sein Auge blitzt, sein Herz erbebt,", "tokens": ["Sein", "Au\u00b7ge", "blitzt", ",", "sein", "Herz", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hofft auf deine Lehren,", "tokens": ["Er", "hofft", "auf", "dei\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich, wenn der Wein den Geist erhebt,", "tokens": ["Dich", ",", "wenn", "der", "Wein", "den", "Geist", "er\u00b7hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im h\u00f6chsten Sinn zu h\u00f6ren.", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Sinn", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ihm \u00f6ffnet sich der Welten Raum", "tokens": ["Ihm", "\u00f6ff\u00b7net", "sich", "der", "Wel\u00b7ten", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Innern Heil und Orden,", "tokens": ["Im", "In\u00b7nern", "Heil", "und", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schwillt die Brust, es br\u00e4unt der Pflaum,", "tokens": ["Es", "schwillt", "die", "Brust", ",", "es", "br\u00e4unt", "der", "Pflaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist ein J\u00fcngling worden.", "tokens": ["Er", "ist", "ein", "J\u00fcng\u00b7ling", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und wenn dir kein Geheimniss blieb", "tokens": ["Und", "wenn", "dir", "kein", "Ge\u00b7heim\u00b7niss", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Herz und Welt enthalte,", "tokens": ["Was", "Herz", "und", "Welt", "ent\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Denker winkst du treu und lieb,", "tokens": ["Dem", "Den\u00b7ker", "winkst", "du", "treu", "und", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dass sich der Sinn entfalte.", "tokens": ["Dass", "sich", "der", "Sinn", "ent\u00b7fal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auch dass vom Throne F\u00fcrstenhort", "tokens": ["Auch", "dass", "vom", "Thro\u00b7ne", "F\u00fcrs\u00b7ten\u00b7hort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nicht f\u00fcr uns verliere,", "tokens": ["Sich", "nicht", "f\u00fcr", "uns", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Giebst du dem Schach ein gutes Wort", "tokens": ["Giebst", "du", "dem", "Schach", "ein", "gu\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebst es dem Veziere.", "tokens": ["Und", "giebst", "es", "dem", "Ve\u00b7zie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Das alles kennst und singst du heut", "tokens": ["Das", "al\u00b7les", "kennst", "und", "singst", "du", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singst es morgen eben:", "tokens": ["Und", "singst", "es", "mor\u00b7gen", "e\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00e4gt uns freundlich dein Geleit", "tokens": ["So", "tr\u00e4gt", "uns", "freund\u00b7lich", "dein", "Ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs rauhe, milde Leben.", "tokens": ["Durchs", "rau\u00b7he", ",", "mil\u00b7de", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}