{"textgrid.poem.59452": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Saale", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch manchmal in Sommertagen", "tokens": ["Doch", "manch\u00b7mal", "in", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch die schw\u00fcle Einsamkeit", "tokens": ["Durch", "die", "schw\u00fc\u00b7le", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt man mittags die Turmuhr schlagen,", "tokens": ["H\u00f6rt", "man", "mit\u00b7tags", "die", "Tur\u00b7muhr", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie aus einer fremden Zeit.", "tokens": ["Wie", "aus", "ei\u00b7ner", "frem\u00b7den", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ein Schiffer zu dieser Stunde", "tokens": ["Und", "ein", "Schif\u00b7fer", "zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sah einst eine sch\u00f6ne Frau", "tokens": ["Sah", "einst", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vom Erker schaun zum Grunde \u2013", "tokens": ["Vom", "Er\u00b7ker", "schaun", "zum", "Grun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ruderte schneller vor Graun.", "tokens": ["Er", "ru\u00b7der\u00b7te", "schnel\u00b7ler", "vor", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Sie sch\u00fcttelt' die dunklen Locken", "tokens": ["Sie", "sch\u00fct\u00b7telt'", "die", "dunk\u00b7len", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus ihrem Angesicht:", "tokens": ["Aus", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwas ruderst du so erschrocken?", "tokens": ["\u00bb", "was", "ru\u00b7derst", "du", "so", "er\u00b7schro\u00b7cken", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beh\u00fct dich Gott, dich mein ich nicht!\u00ab", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "dich", "mein", "ich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "PPOSAT", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie zog ein Ringlein vom Finger,", "tokens": ["Sie", "zog", "ein", "Rin\u00b7glein", "vom", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Warf's tief in die Saale hinein:", "tokens": ["Wa\u00b7rf's", "tief", "in", "die", "Saa\u00b7le", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "\u00bbund der mir es wiederbringet,", "tokens": ["\u00bb", "und", "der", "mir", "es", "wie\u00b7der\u00b7brin\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der soll mein Liebster sein!\u00ab", "tokens": ["Der", "soll", "mein", "Liebs\u00b7ter", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}