{"textgrid.poem.24981": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Im Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wagt' ich mich von des Lebens Strand", "tokens": ["Wagt'", "ich", "mich", "von", "des", "Le\u00b7bens", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu weit hinaus? In Dunkel schwand", "tokens": ["Zu", "weit", "hin\u00b7aus", "?", "In", "Dun\u00b7kel", "schwand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "PTKVZ", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tages letzter Schimmer;", "tokens": ["Des", "Ta\u00b7ges", "letz\u00b7ter", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur hie und da hinunter gie\u00dft", "tokens": ["Nur", "hie", "und", "da", "hin\u00b7un\u00b7ter", "gie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Blitz, der durch die Wolken schie\u00dft,", "tokens": ["Ein", "Blitz", ",", "der", "durch", "die", "Wol\u00b7ken", "schie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein zackiges Geflimmer.", "tokens": ["Sein", "za\u00b7cki\u00b7ges", "Ge\u00b7flim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis auf des Meeres schwarzen Grund", "tokens": ["Bis", "auf", "des", "Mee\u00b7res", "schwar\u00b7zen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab rei\u00dft uns der Wogenschlund;", "tokens": ["Hin\u00b7ab", "rei\u00dft", "uns", "der", "Wo\u00b7gen\u00b7schlund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wieder auf den Wellen", "tokens": ["Dann", "wie\u00b7der", "auf", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirft himmelw\u00e4rts der Sturm das Schiff;", "tokens": ["Wirft", "him\u00b7mel\u00b7w\u00e4rts", "der", "Sturm", "das", "Schiff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Sto\u00df nur, und am Felsenriff", "tokens": ["Ein", "Sto\u00df", "nur", ",", "und", "am", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Kaps mu\u00df es zerschellen.", "tokens": ["Des", "Kaps", "mu\u00df", "es", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch du, zu dem als Kind empor", "tokens": ["Auch", "du", ",", "zu", "dem", "als", "Kind", "em\u00b7por"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "APPR", "ART", "KOUS", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meines Vaterhauses Thor", "tokens": ["An", "mei\u00b7nes", "Va\u00b7ter\u00b7hau\u00b7ses", "Thor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schon in Andacht schaute,", "tokens": ["Ich", "schon", "in", "An\u00b7dacht", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verh\u00fcllst du dich in Finsternis,", "tokens": ["Ver\u00b7h\u00fcllst", "du", "dich", "in", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Stern, auf den ich siegsgewi\u00df", "tokens": ["O", "Stern", ",", "auf", "den", "ich", "siegs\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens Hoffnung baute?", "tokens": ["Des", "Le\u00b7bens", "Hoff\u00b7nung", "bau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du h\u00f6rtest meinen Seelenschwur,", "tokens": ["Du", "h\u00f6r\u00b7test", "mei\u00b7nen", "See\u00b7len\u00b7schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df nicht auf Erden meine Spur", "tokens": ["Da\u00df", "nicht", "auf", "Er\u00b7den", "mei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wind verwehen solle,", "tokens": ["Im", "Wind", "ver\u00b7we\u00b7hen", "sol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gabst mir Mut auf meinem Gang", "tokens": ["Und", "gabst", "mir", "Mut", "auf", "mei\u00b7nem", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Kraft, wenn ich empor mich rang", "tokens": ["Und", "Kraft", ",", "wenn", "ich", "em\u00b7por", "mich", "rang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Staub der niedern Scholle.", "tokens": ["Vom", "Staub", "der", "nie\u00b7dern", "Schol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Strahl auf! Ich f\u00e4nde Ruhe nicht", "tokens": ["Strahl", "auf", "!", "Ich", "f\u00e4n\u00b7de", "Ru\u00b7he", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort unten, wenn ich Luft und Licht", "tokens": ["Dort", "un\u00b7ten", ",", "wenn", "ich", "Luft", "und", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu fr\u00fch verlassen m\u00fc\u00dfte!", "tokens": ["Zu", "fr\u00fch", "ver\u00b7las\u00b7sen", "m\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch ist mein Tagwerk nicht vollbracht;", "tokens": ["Noch", "ist", "mein", "Tag\u00b7werk", "nicht", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O f\u00fchr zur\u00fcck durch Sturm und Nacht", "tokens": ["O", "f\u00fchr", "zu\u00b7r\u00fcck", "durch", "Sturm", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich an des Lebens K\u00fcste!", "tokens": ["Mich", "an", "des", "Le\u00b7bens", "K\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}