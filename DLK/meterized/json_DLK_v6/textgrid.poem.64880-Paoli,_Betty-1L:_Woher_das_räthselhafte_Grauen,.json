{"textgrid.poem.64880": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Woher das r\u00e4thselhafte Grauen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher das r\u00e4thselhafte Grauen,", "tokens": ["Wo\u00b7her", "das", "r\u00e4th\u00b7sel\u00b7haf\u00b7te", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das n\u00e4chtig meine Seele tr\u00fcbt?", "tokens": ["Das", "n\u00e4ch\u00b7tig", "mei\u00b7ne", "See\u00b7le", "tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df ich die Seele dessen schauen,", "tokens": ["Mu\u00df", "ich", "die", "See\u00b7le", "des\u00b7sen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ich so tief, so hei\u00df geliebt!", "tokens": ["Den", "ich", "so", "tief", ",", "so", "hei\u00df", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist nicht Schmerz, da\u00df nun zerrissen", "tokens": ["Es", "ist", "nicht", "Schmerz", ",", "da\u00df", "nun", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das festgeschlungne Seelenband,", "tokens": ["Das", "fest\u00b7ge\u00b7schlung\u00b7ne", "See\u00b7len\u00b7band", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df ich mu\u00df im Dunkel missen", "tokens": ["Und", "da\u00df", "ich", "mu\u00df", "im", "Dun\u00b7kel", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die liebgewohnte F\u00fchrerhand!", "tokens": ["Die", "lieb\u00b7ge\u00b7wohn\u00b7te", "F\u00fch\u00b7rer\u00b7hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch ist's das ungest\u00fcme Sehnen", "tokens": ["Noch", "ist's", "das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mitleidslos verlassnen Braut,", "tokens": ["Der", "mit\u00b7leids\u00b7los", "ver\u00b7lass\u00b7nen", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die unter Str\u00f6men hei\u00dfer Thr\u00e4nen", "tokens": ["Die", "un\u00b7ter", "Str\u00f6\u00b7men", "hei\u00b7\u00dfer", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck auf sch\u00f6n're Tage schaut.", "tokens": ["Zu\u00b7r\u00fcck", "auf", "sch\u00f6n'\u00b7re", "Ta\u00b7ge", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die holden T\u00e4uschungen beschr\u00e4nken", "tokens": ["Die", "hol\u00b7den", "T\u00e4u\u00b7schun\u00b7gen", "be\u00b7schr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir nicht mehr den erloschnen Blick;", "tokens": ["Mir", "nicht", "mehr", "den", "er\u00b7lo\u00b7schnen", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit kaltem L\u00e4cheln kann ich denken", "tokens": ["Mit", "kal\u00b7tem", "L\u00e4\u00b7cheln", "kann", "ich", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An fr\u00fch're Zeiten und ihr Gl\u00fcck.", "tokens": ["An", "fr\u00fch'\u00b7re", "Zei\u00b7ten", "und", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dieses ist's, was tr\u00fcb und traurig", "tokens": ["Und", "die\u00b7ses", "ist's", ",", "was", "tr\u00fcb", "und", "trau\u00b7rig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch meine tiefste Seele geht,", "tokens": ["Durch", "mei\u00b7ne", "tiefs\u00b7te", "See\u00b7le", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein Hauch des Todes schaurig", "tokens": ["Und", "wie", "ein", "Hauch", "des", "To\u00b7des", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meine bleiche Stirne weht:", "tokens": ["Um", "mei\u00b7ne", "blei\u00b7che", "Stir\u00b7ne", "weht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df dieser Blick, der einst entz\u00fcndet", "tokens": ["Da\u00df", "die\u00b7ser", "Blick", ",", "der", "einst", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In mir d\u00e4monisch wilde Lust,", "tokens": ["In", "mir", "d\u00e4\u00b7mo\u00b7nisch", "wil\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun nichts als todte Asche findet", "tokens": ["Nun", "nichts", "als", "tod\u00b7te", "A\u00b7sche", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner ausgebrannten Brust;", "tokens": ["In", "mei\u00b7ner", "aus\u00b7ge\u00b7brann\u00b7ten", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df die melodisch s\u00fc\u00dfe Rede,", "tokens": ["Da\u00df", "die", "me\u00b7lo\u00b7disch", "s\u00fc\u00b7\u00dfe", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der einst ich lauschte wonnerschreckt,", "tokens": ["Der", "einst", "ich", "lauschte", "won\u00b7ner\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In meiner Seele W\u00fcsten\u00f6de", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "W\u00fcs\u00b7te\u00b7n\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein freudig Echo mehr erweckt;", "tokens": ["Kein", "freu\u00b7dig", "E\u00b7cho", "mehr", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df ich die\u00df Bild, de\u00df Sch\u00f6nheitsprangen", "tokens": ["Da\u00df", "ich", "die\u00df", "Bild", ",", "de\u00df", "Sch\u00f6n\u00b7heits\u00b7pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich einst durchflammt mit trunknem Wahn,", "tokens": ["Mich", "einst", "durch\u00b7flammt", "mit", "trunk\u00b7nem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ohne Wunsch, ohne Verlangen,", "tokens": ["Nun", "oh\u00b7ne", "Wunsch", ",", "oh\u00b7ne", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit eis'gem Ernst betrachten kann.", "tokens": ["Mit", "eis'\u00b7gem", "Ernst", "be\u00b7trach\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df wie ein leiser Klang der Leier", "tokens": ["Da\u00df", "wie", "ein", "lei\u00b7ser", "Klang", "der", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwand, was unsterblich ich geglaubt,", "tokens": ["Schwand", ",", "was", "uns\u00b7terb\u00b7lich", "ich", "ge\u00b7glaubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das wirft den dunkeln Nonnenschleier", "tokens": ["Das", "wirft", "den", "dun\u00b7keln", "Non\u00b7nen\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf mein dem Schmerz verfallnes Haupt!", "tokens": ["Auf", "mein", "dem", "Schmerz", "ver\u00b7fall\u00b7nes", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O welche Macht der Erde schriebe", "tokens": ["O", "wel\u00b7che", "Macht", "der", "Er\u00b7de", "schrie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In's Herz mir noch den sel'gen Schwur,", "tokens": ["In's", "Herz", "mir", "noch", "den", "sel'\u00b7gen", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit ich die Sterblichkeit der Liebe", "tokens": ["Seit", "ich", "die", "Sterb\u00b7lich\u00b7keit", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vernichtet an mir selbst erfuhr!", "tokens": ["Ver\u00b7nich\u00b7tet", "an", "mir", "selbst", "er\u00b7fuhr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich f\u00fchl' es: zwischen mich und Jeden,", "tokens": ["Ich", "f\u00fchl'", "es", ":", "zwi\u00b7schen", "mich", "und", "Je\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "PPER", "KON", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den hei\u00df die Sehnsucht zu mir rei\u00dft,", "tokens": ["Den", "hei\u00df", "die", "Sehn\u00b7sucht", "zu", "mir", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt sich mit h\u00f6hnisch bittern Reden", "tokens": ["Dr\u00e4ngt", "sich", "mit", "h\u00f6h\u00b7nisch", "bit\u00b7tern", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der abgeschiednen Liebe Geist.", "tokens": ["Der", "ab\u00b7ge\u00b7schied\u00b7nen", "Lie\u00b7be", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}