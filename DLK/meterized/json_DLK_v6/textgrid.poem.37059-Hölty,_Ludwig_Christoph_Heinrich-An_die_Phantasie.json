{"textgrid.poem.37059": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosenwangigte Phantasie,", "tokens": ["Ro\u00b7sen\u00b7wan\u00b7gig\u00b7te", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Die du Bilder ins Herz deiner Vertrauten mahlst,", "tokens": ["Die", "du", "Bil\u00b7der", "ins", "Herz", "dei\u00b7ner", "Ver\u00b7trau\u00b7ten", "mahlst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Vergangenheit aus dem Schoos", "tokens": ["Die", "Ver\u00b7gan\u00b7gen\u00b7heit", "aus", "dem", "Schoos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihrer Mitternacht rufst, hinter den Schleyer blickst,", "tokens": ["Ih\u00b7rer", "Mit\u00b7ter\u00b7nacht", "rufst", ",", "hin\u00b7ter", "den", "Schle\u00b7yer", "blickst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Der das Auge der Zukunft deckt,", "tokens": ["Der", "das", "Au\u00b7ge", "der", "Zu\u00b7kunft", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Dich gen Himmel erhebst, unter Verkl\u00e4rten wallst,", "tokens": ["Dich", "gen", "Him\u00b7mel", "er\u00b7hebst", ",", "un\u00b7ter", "Ver\u00b7kl\u00e4r\u00b7ten", "wallst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "In die Harfen der Engel singst,", "tokens": ["In", "die", "Har\u00b7fen", "der", "En\u00b7gel", "singst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und den blendenden Thron Gottes von ferne schaust;", "tokens": ["Und", "den", "blen\u00b7den\u00b7den", "Thron", "Got\u00b7tes", "von", "fer\u00b7ne", "schaust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Beut mir immer den Schwanenarm!", "tokens": ["Beut", "mir", "im\u00b7mer", "den", "Schwa\u00b7nen\u00b7arm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Rei\u00df mich fl\u00fcgelgeschwind, \u00fcber die Wolkenbahn,", "tokens": ["Rei\u00df", "mich", "fl\u00fc\u00b7gel\u00b7ge\u00b7schwind", ",", "\u00fc\u00b7ber", "die", "Wol\u00b7ken\u00b7bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.11": {"text": "In den goldenen Sternensaal!", "tokens": ["In", "den", "gol\u00b7de\u00b7nen", "Ster\u00b7nen\u00b7saal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Oder wandle mit mir, holde Begleiterin,", "tokens": ["O\u00b7der", "wand\u00b7le", "mit", "mir", ",", "hol\u00b7de", "Be\u00b7glei\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "In die Tage des Fl\u00fcgelkleids,", "tokens": ["In", "die", "Ta\u00b7ge", "des", "Fl\u00fc\u00b7gel\u00b7kleids", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Die, im scherzenden Tanz, \u00fcber mein Haupt entflohn,", "tokens": ["Die", ",", "im", "scher\u00b7zen\u00b7den", "Tanz", ",", "\u00fc\u00b7ber", "mein", "Haupt", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "In die Tage der ersten Glut! \u2013 \u2013", "tokens": ["In", "die", "Ta\u00b7ge", "der", "ers\u00b7ten", "Glut", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Rollt mein Leben zur\u00fcck? Zauberin Phantasie,", "tokens": ["Rollt", "mein", "Le\u00b7ben", "zu\u00b7r\u00fcck", "?", "Zau\u00b7be\u00b7rin", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "NN", "NN", "$,"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Wohin zauberst du meinen Tritt? \u2013 \u2013 \u2013", "tokens": ["Wo\u00b7hin", "zau\u00b7berst", "du", "mei\u00b7nen", "Tritt", "?", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "$(", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Gaukelnd h\u00fcpf' ich daher, hasche den Schmetterling,", "tokens": ["Gau\u00b7kelnd", "h\u00fcpf'", "ich", "da\u00b7her", ",", "ha\u00b7sche", "den", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PAV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Der am Busen der Rose trinkt,", "tokens": ["Der", "am", "Bu\u00b7sen", "der", "Ro\u00b7se", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Baue H\u00fctten mir auf, fl\u00fcgle den bunten Ball", "tokens": ["Bau\u00b7e", "H\u00fct\u00b7ten", "mir", "auf", ",", "fl\u00fcg\u00b7le", "den", "bun\u00b7ten", "Ball"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Durch die Bl\u00e4ue der Sommerluft! \u2013 \u2013", "tokens": ["Durch", "die", "Bl\u00e4u\u00b7e", "der", "Som\u00b7mer\u00b7luft", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Welche G\u00f6ttergestalt! Unschuld, die Minnerin", "tokens": ["Wel\u00b7che", "G\u00f6t\u00b7ter\u00b7ge\u00b7stalt", "!", "Un\u00b7schuld", ",", "die", "Min\u00b7ne\u00b7rin"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$.", "NN", "$,", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.23": {"text": "Dieser friedlichen Sch\u00e4ferflur,", "tokens": ["Die\u00b7ser", "fried\u00b7li\u00b7chen", "Sch\u00e4\u00b7fer\u00b7flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "F\u00fchrt ein M\u00e4dchen am Arm. Heller und r\u00f6ther bl\u00fchn", "tokens": ["F\u00fchrt", "ein", "M\u00e4d\u00b7chen", "am", "Arm", ".", "Hel\u00b7ler", "und", "r\u00f6\u00b7ther", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "NE", "KON", "ADJD", "VVINF"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Alle Wangen des Blumenvolks,", "tokens": ["Al\u00b7le", "Wan\u00b7gen", "des", "Blu\u00b7men\u00b7volks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Das den schmeichelnden Ku\u00df ihres Gewandes f\u00fchlt.", "tokens": ["Das", "den", "schmei\u00b7cheln\u00b7den", "Ku\u00df", "ih\u00b7res", "Ge\u00b7wan\u00b7des", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Itzt, itzt schl\u00fcpft sie dahin, und mir", "tokens": ["Itzt", ",", "itzt", "schl\u00fcpft", "sie", "da\u00b7hin", ",", "und", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PAV", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Lacht ihr Seelenblick, mir! \u2013 \u2013 Seh ich die Laube dort,", "tokens": ["Lacht", "ihr", "See\u00b7len\u00b7blick", ",", "mir", "!", "\u2013", "\u2013", "Seh", "ich", "die", "Lau\u00b7be", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "$.", "$(", "$(", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Wo mein Busen an Agathons", "tokens": ["Wo", "mein", "Bu\u00b7sen", "an", "A\u00b7gat\u00b7hons"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Busen fr\u00f6hlicher schlug, wo wir den Abendstern", "tokens": ["Bu\u00b7sen", "fr\u00f6h\u00b7li\u00b7cher", "schlug", ",", "wo", "wir", "den", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Oft den Himmel besteigen sahn?", "tokens": ["Oft", "den", "Him\u00b7mel", "be\u00b7stei\u00b7gen", "sahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Reizend bist du mir stets, schattendes Rebendach,", "tokens": ["Rei\u00b7zend", "bist", "du", "mir", "stets", ",", "schat\u00b7ten\u00b7des", "Re\u00b7ben\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Wo dein Wonnegespr\u00e4ch, o Freund,", "tokens": ["Wo", "dein", "Won\u00b7ne\u00b7ge\u00b7spr\u00e4ch", ",", "o", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.34": {"text": "Dein geselliger Scherz, Fl\u00fcgel des Augenblicks", "tokens": ["Dein", "ge\u00b7sel\u00b7li\u00b7ger", "Scherz", ",", "Fl\u00fc\u00b7gel", "des", "Au\u00b7gen\u00b7blicks"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Mancher seeligen Stunde gab! \u2013 \u2013", "tokens": ["Man\u00b7cher", "see\u00b7li\u00b7gen", "Stun\u00b7de", "gab", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Flieh das blumichte Grab, fl\u00fcchtige F\u00fchrerin,", "tokens": ["Flieh", "das", "blu\u00b7mich\u00b7te", "Grab", ",", "fl\u00fcch\u00b7ti\u00b7ge", "F\u00fch\u00b7re\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.37": {"text": "Wo die g\u00f6ttliche Lilla schl\u00e4ft!", "tokens": ["Wo", "die", "g\u00f6tt\u00b7li\u00b7che", "Lil\u00b7la", "schl\u00e4ft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Flieh, sonst bricht mir das Herz! \u2013 Schwinge dich wolkenan,", "tokens": ["Flieh", ",", "sonst", "bricht", "mir", "das", "Herz", "!", "\u2013", "Schwin\u00b7ge", "dich", "wol\u00b7ke\u00b7nan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "NN", "PPER", "VMFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Und bewalle mit mir den Stern,", "tokens": ["Und", "be\u00b7wal\u00b7le", "mit", "mir", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Wo, mit Morgengew\u00f6lk r\u00f6thlich umh\u00fcllt, ihr Geist", "tokens": ["Wo", ",", "mit", "Mor\u00b7gen\u00b7ge\u00b7w\u00f6lk", "r\u00f6th\u00b7lich", "um\u00b7h\u00fcllt", ",", "ihr", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "NN", "ADJD", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "An melodischen Quellen irrt,", "tokens": ["An", "me\u00b7lo\u00b7di\u00b7schen", "Quel\u00b7len", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "Und den Strom des Gesangs, welcher den goldenen", "tokens": ["Und", "den", "Strom", "des", "Ge\u00b7sangs", ",", "wel\u00b7cher", "den", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.43": {"text": "Engelharfen entrauschet, trinkt! \u2013 \u2013", "tokens": ["En\u00b7gel\u00b7har\u00b7fen", "ent\u00b7rau\u00b7schet", ",", "trinkt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.44": {"text": "Wonne! Wonne! die Welt taumelt zur\u00fcck! Ich bin", "tokens": ["Won\u00b7ne", "!", "Won\u00b7ne", "!", "die", "Welt", "tau\u00b7melt", "zu\u00b7r\u00fcck", "!", "Ich", "bin"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.45": {"text": "Am Gestade des lichten Sterns!", "tokens": ["Am", "Ge\u00b7sta\u00b7de", "des", "lich\u00b7ten", "Sterns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.46": {"text": "Lilla h\u00fcpfet heran, leitet mich an der Hand", "tokens": ["Lil\u00b7la", "h\u00fcp\u00b7fet", "he\u00b7ran", ",", "lei\u00b7tet", "mich", "an", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Unter Ch\u00f6re der Seeligen.", "tokens": ["Un\u00b7ter", "Ch\u00f6\u00b7re", "der", "See\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.48": {"text": "Engel stehen umher, werfen mir Kronen zu,", "tokens": ["En\u00b7gel", "ste\u00b7hen", "um\u00b7her", ",", "wer\u00b7fen", "mir", "Kro\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Winden Palmen mir um den Schlaf \u2013 \u2013", "tokens": ["Win\u00b7den", "Pal\u00b7men", "mir", "um", "den", "Schlaf", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "PPER", "APPR", "ART", "NN", "$(", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.50": {"text": "Weil auf diesem Gestirn immer, o Phantasie!", "tokens": ["Weil", "auf", "die\u00b7sem", "Ge\u00b7stirn", "im\u00b7mer", ",", "o", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}