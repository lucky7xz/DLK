{"dta.poem.2764": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Braut-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein auserw\u00e4hlter Schatz/ der du mich hast entz\u00fcndet", "tokens": ["Mein", "au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "Schatz", "/", "der", "du", "mich", "hast", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deiner Augen Pracht/", "tokens": ["Durch", "dei\u00b7ner", "Au\u00b7gen", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun kommt die s\u00fcsse Nacht/", "tokens": ["Nun", "kommt", "die", "s\u00fcs\u00b7se", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So beyder Hertz und Seel\u2019 in reiner Treu verbindet/", "tokens": ["So", "bey\u00b7der", "Hertz", "und", "Seel'", "in", "rei\u00b7ner", "Treu", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unsrer Liebe Licht und Schein", "tokens": ["Und", "uns\u00b7rer", "Lie\u00b7be", "Licht", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+