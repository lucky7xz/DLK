{"textgrid.poem.53283": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Petrus redet alle arme S\u00fcnder an, wegen erlangter Vergebung seiner Verleugnung Christi", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wegen seiner S\u00fcnden", "tokens": ["Wer", "we\u00b7gen", "sei\u00b7ner", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Herren Christo sich", "tokens": ["Zum", "Her\u00b7ren", "Chris\u00b7to", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "PRF"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit Glauben nicht darff finden,", "tokens": ["Mit", "Glau\u00b7ben", "nicht", "darff", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der komm vnd schawe mich!", "tokens": ["Der", "komm", "vnd", "scha\u00b7we", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich Petrus bin gewesen", "tokens": ["Ich", "Pet\u00b7rus", "bin", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "FM", "VAFIN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In solcher Missethat,", "tokens": ["In", "sol\u00b7cher", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als nirgends wird gelesen,", "tokens": ["Als", "nir\u00b7gends", "wird", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd sieh, ich finde Raht:", "tokens": ["Vnd", "sieh", ",", "ich", "fin\u00b7de", "Raht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe Nein gesaget", "tokens": ["Ich", "ha\u00b7be", "Nein", "ge\u00b7sa\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd Warheit sehr gespart,", "tokens": ["Vnd", "War\u00b7heit", "sehr", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich vmb Ihn gefraget", "tokens": ["Als", "ich", "vmb", "Ihn", "ge\u00b7fra\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd hart besprochen ward:", "tokens": ["Vnd", "hart", "be\u00b7spro\u00b7chen", "ward", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schwur mit falscher Zungen,", "tokens": ["Ich", "schwur", "mit", "fal\u00b7scher", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als man mit Vngest\u00fcmm", "tokens": ["Als", "man", "mit", "Vn\u00b7ge\u00b7st\u00fcmm"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd Macht in mich gedrungen,", "tokens": ["Vnd", "Macht", "in", "mich", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich w\u00fcste nichts von Ihm.", "tokens": ["Ich", "w\u00fcs\u00b7te", "nichts", "von", "Ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Solt ich von Dem nicht wissen,", "tokens": ["Solt", "ich", "von", "Dem", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An dessen Wort es hieng,", "tokens": ["An", "des\u00b7sen", "Wort", "es", "hieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir die Netze rissen", "tokens": ["Da\u00df", "mir", "die", "Net\u00b7ze", "ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Fischen, die ich fieng?", "tokens": ["Von", "Fi\u00b7schen", ",", "die", "ich", "fi\u00b7eng", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der an sich mich gezogen,", "tokens": ["Der", "an", "sich", "mich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich Wunder lassen sehn,", "tokens": ["Mich", "Wun\u00b7der", "las\u00b7sen", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die auff den Wasserwogen", "tokens": ["Die", "auff", "den", "Was\u00b7ser\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch seine Krafft geschehn?", "tokens": ["Durch", "sei\u00b7ne", "Krafft", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von dem ich selbst bekennet", "tokens": ["Von", "dem", "ich", "selbst", "be\u00b7ken\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wehre Gottes Sohn,", "tokens": ["Er", "weh\u00b7re", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem ewig bleibt benennet", "tokens": ["Dem", "e\u00b7wig", "bleibt", "be\u00b7nen\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Vaters Krafft vnd Thron?", "tokens": ["Des", "Va\u00b7ters", "Krafft", "vnd", "Thron", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der manche sch\u00f6ne Stunde", "tokens": ["Der", "man\u00b7che", "sch\u00f6\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Seele mir durchnam,", "tokens": ["Die", "See\u00b7le", "mir", "durch\u00b7nam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn au\u00df dem s\u00fcssen Munde", "tokens": ["Wenn", "au\u00df", "dem", "s\u00fcs\u00b7sen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm manche Predigt kam.", "tokens": ["Ihm", "man\u00b7che", "Pre\u00b7digt", "kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe gantz kein Leben,", "tokens": ["Ich", "ha\u00b7be", "gantz", "kein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne kenn ich nicht,", "tokens": ["Die", "Son\u00b7ne", "kenn", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zunge bleibt mir kleben,", "tokens": ["Die", "Zun\u00b7ge", "bleibt", "mir", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Geist in mir gebricht,", "tokens": ["Der", "Geist", "in", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bedenck ich mein Gebrechen,", "tokens": ["Be\u00b7denck", "ich", "mein", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die vnerh\u00f6rte That,", "tokens": ["Die", "vner\u00b7h\u00f6r\u00b7te", "That", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Das Sinnen-lose Sprechen", "tokens": ["Das", "Sin\u00b7nen\u00b7lo\u00b7se", "Spre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Ihn verleugnet hat.", "tokens": ["Das", "Ihn", "ver\u00b7leug\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch dennoch find ich Gnade", "tokens": ["Noch", "den\u00b7noch", "find", "ich", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr alle meine Schuld,", "tokens": ["F\u00fcr", "al\u00b7le", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein m\u00e4chtig grosser Schade", "tokens": ["Mein", "m\u00e4ch\u00b7tig", "gros\u00b7ser", "Scha\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat doch bey Ihm Gedult:", "tokens": ["Hat", "doch", "bey", "Ihm", "Ge\u00b7dult", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Herr ist kaum erstanden,", "tokens": ["Der", "Herr", "ist", "kaum", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir wird es angesagt,", "tokens": ["Mir", "wird", "es", "an\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Gnade sey verhanden", "tokens": ["Die", "Gna\u00b7de", "sey", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auff alles, was mich plagt.", "tokens": ["Auff", "al\u00b7les", ",", "was", "mich", "plagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was wil das Wort mir schencken:", "tokens": ["Was", "wil", "das", "Wort", "mir", "schen\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht, sagt es Petro an?", "tokens": ["Geht", ",", "sagt", "es", "Pe\u00b7tro", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wil nicht mehr gedencken", "tokens": ["Er", "wil", "nicht", "mehr", "ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An das, so ich gethan;", "tokens": ["An", "das", ",", "so", "ich", "ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er rufft mich Ihn zu finden", "tokens": ["Er", "rufft", "mich", "Ihn", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ins Galileer-Land,", "tokens": ["Ins", "Ga\u00b7li\u00b7leer\u00b7Land", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df Tilgung meinen S\u00fcnden", "tokens": ["Da\u00df", "Til\u00b7gung", "mei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja w\u00fcrde zugewandt.", "tokens": ["Ja", "w\u00fcr\u00b7de", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Ihr nun auch mit Schulden", "tokens": ["Die", "Ihr", "nun", "auch", "mit", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehr gr\u00f6blich seyd befleckt,", "tokens": ["Sehr", "gr\u00f6b\u00b7lich", "seyd", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd spricht: Wie kan Gott dulden", "tokens": ["Vnd", "spricht", ":", "Wie", "kan", "Gott", "dul\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worinn ich mich gesteckt?", "tokens": ["Wo\u00b7rinn", "ich", "mich", "ge\u00b7steckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist zu sehr versehen,", "tokens": ["Es", "ist", "zu", "sehr", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die S\u00fcnd ist gar zu gro\u00df!", "tokens": ["Die", "S\u00fcnd", "ist", "gar", "zu", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wird mir doch geschehen?", "tokens": ["Wie", "wird", "mir", "doch", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich werd jhr nimmer lo\u00df.", "tokens": ["Ich", "werd", "jhr", "nim\u00b7mer", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Lasst mich ein Beyspiel werden,", "tokens": ["Lasst", "mich", "ein", "Bey\u00b7spiel", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df niemand, ob er wol", "tokens": ["Da\u00df", "nie\u00b7mand", ",", "ob", "er", "wol"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ges\u00fcndigt viel auff Erden,", "tokens": ["Ge\u00b7s\u00fcn\u00b7digt", "viel", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darumb verzagen sol!", "tokens": ["Da\u00b7rumb", "ver\u00b7za\u00b7gen", "sol", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der gn\u00e4dig mir gewesen,", "tokens": ["Der", "gn\u00e4\u00b7dig", "mir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird gn\u00e4dig dir auch seyn,", "tokens": ["Wird", "gn\u00e4\u00b7dig", "dir", "auch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du wirst, wie ich, genesen", "tokens": ["Du", "wirst", ",", "wie", "ich", ",", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von aller Noht vnd Pein.", "tokens": ["Von", "al\u00b7ler", "Noht", "vnd", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nicht da\u00df du auff die G\u00fcte", "tokens": ["Nicht", "da\u00df", "du", "auff", "die", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Solt leben, wie du wilt;", "tokens": ["Solt", "le\u00b7ben", ",", "wie", "du", "wilt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaw, da\u00df dich Recht beh\u00fcte,", "tokens": ["Schaw", ",", "da\u00df", "dich", "Recht", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gott-f\u00fcrchten sey dein Schild,", "tokens": ["Got\u00b7tf\u00fcrch\u00b7ten", "sey", "dein", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob S\u00fcnden dich denn haben", "tokens": ["Ob", "S\u00fcn\u00b7den", "dich", "denn", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beth\u00f6rt durch schn\u00f6den Lauff;", "tokens": ["Be\u00b7th\u00f6rt", "durch", "schn\u00f6\u00b7den", "Lauff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was Christus hat vergraben,", "tokens": ["Was", "Chris\u00b7tus", "hat", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das scharre du nicht auff.", "tokens": ["Das", "schar\u00b7re", "du", "nicht", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}