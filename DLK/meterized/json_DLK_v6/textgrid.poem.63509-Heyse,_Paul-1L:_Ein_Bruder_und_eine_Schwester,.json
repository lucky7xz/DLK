{"textgrid.poem.63509": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bruder und eine Schwester,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bruder und eine Schwester,", "tokens": ["Ein", "Bru\u00b7der", "und", "ei\u00b7ne", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nichts Treueres kennt die Welt,", "tokens": ["Nichts", "Treu\u00b7e\u00b7res", "kennt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Goldkettlein h\u00e4lt fester,", "tokens": ["Kein", "Gold\u00b7ket\u00b7tlein", "h\u00e4lt", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als eins am andern h\u00e4lt.", "tokens": ["Als", "eins", "am", "an\u00b7dern", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwei Liebsten so oft sich scheiden,", "tokens": ["Zwei", "Liebs\u00b7ten", "so", "oft", "sich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ADV", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn Minne vergl\u00fcht geschwind.", "tokens": ["Denn", "Min\u00b7ne", "ver\u00b7gl\u00fcht", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geschwister in Lust und Leiden", "tokens": ["Ge\u00b7schwis\u00b7ter", "in", "Lust", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die bleiben sich hold gesinnt,", "tokens": ["Die", "blei\u00b7ben", "sich", "hold", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So treulich als wie zusammen", "tokens": ["So", "treu\u00b7lich", "als", "wie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "KOKOM", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Mond und die Erde gehn,", "tokens": ["Der", "Mond", "und", "die", "Er\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der ewigen Sterne Flammen", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Ster\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alle Nacht beieinander stehn.", "tokens": ["Al\u00b7le", "Nacht", "bei\u00b7ein\u00b7an\u00b7der", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Die Engel im himmlischen Reigen", "tokens": ["Die", "En\u00b7gel", "im", "himm\u00b7li\u00b7schen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Frohlocken dem trauten Bund,", "tokens": ["Froh\u00b7lo\u00b7cken", "dem", "trau\u00b7ten", "Bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn Bruder und Schwester sich neigen", "tokens": ["Wenn", "Bru\u00b7der", "und", "Schwes\u00b7ter", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und k\u00fcssen sich auf den Mund.", "tokens": ["Und", "k\u00fcs\u00b7sen", "sich", "auf", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}