{"dta.poem.21664": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Meiner Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mutter, aus der Ferne eilst Du,", "tokens": ["Mut\u00b7ter", ",", "aus", "der", "Fer\u00b7ne", "eilst", "Du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Deinen Sohn zu sehen,", "tokens": ["Dei\u00b7nen", "Sohn", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, die kranke Seele heilst Du,", "tokens": ["Ach", ",", "die", "kran\u00b7ke", "See\u00b7le", "heilst", "Du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Linderst ihre Wehen.", "tokens": ["Lin\u00b7derst", "ih\u00b7re", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bin zermartert, bin zerschlagen", "tokens": ["Bin", "zer\u00b7mar\u00b7tert", ",", "bin", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Sturm die Eiche,", "tokens": ["Wie", "im", "Sturm", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch bei Dir vergeht mein Klagen,", "tokens": ["Doch", "bei", "Dir", "ver\u00b7geht", "mein", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gute, Milde, Weiche!", "tokens": ["Gu\u00b7te", ",", "Mil\u00b7de", ",", "Wei\u00b7che", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer der Zeit Meduse schaute", "tokens": ["Wer", "der", "Zeit", "Me\u00b7du\u00b7se", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon mit jungen Jahren,", "tokens": ["Schon", "mit", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wem\u2019s in H\u00f6llentiefen graute,", "tokens": ["Wem's", "in", "H\u00f6l\u00b7len\u00b7tie\u00b7fen", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fch hinabgefahren:", "tokens": ["Fr\u00fch", "hin\u00b7ab\u00b7ge\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wen zu Eis der Frost des Lebens", "tokens": ["Wen", "zu", "Eis", "der", "Frost", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft das Herz erstarrt hat,", "tokens": ["Oft", "das", "Herz", "er\u00b7starrt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wen der Irrthum dunklen Strebens", "tokens": ["Wen", "der", "Irr\u00b7thum", "dunk\u00b7len", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcgerisch genarrt hat:", "tokens": ["Tr\u00fc\u00b7ge\u00b7risch", "ge\u00b7narrt", "hat", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00dft ihn in die treuen Augen", "tokens": ["La\u00dft", "ihn", "in", "die", "treu\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Mutter blicken,", "tokens": ["Sei\u00b7ner", "Mut\u00b7ter", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hei\u00dfe Wonne wird er saugen", "tokens": ["Hei\u00b7\u00dfe", "Won\u00b7ne", "wird", "er", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich hei\u00df erquicken.", "tokens": ["Und", "sich", "hei\u00df", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "VVINF",