{"dta.poem.9265": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Spielt/ spielt ihr liebgen spielt/ legt eure thaler ein", "tokens": ["Spielt", "/", "spielt", "ihr", "lieb\u00b7gen", "spielt", "/", "legt", "eu\u00b7re", "tha\u00b7ler", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "VVFIN", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und last die liebe nur des spieles richter seyn.", "tokens": ["Und", "last", "die", "lie\u00b7be", "nur", "des", "spie\u00b7les", "rich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht auff das gl\u00fccke nicht/ das offters steigt und f\u00e4llt/", "tokens": ["Seht", "auff", "das", "gl\u00fc\u00b7cke", "nicht", "/", "das", "off\u00b7ters", "steigt", "und", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "$(", "PDS", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mancher mit verdru\u00df die oberhand erh\u00e4lt.", "tokens": ["Da\u00df", "man\u00b7cher", "mit", "ver\u00b7dru\u00df", "die", "o\u00b7ber\u00b7hand", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die briefe m\u00f6gen falsch und durch einander gehn/", "tokens": ["Die", "brie\u00b7fe", "m\u00f6\u00b7gen", "falsch", "und", "durch", "ein\u00b7an\u00b7der", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "KON", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So l\u00e4st der widerpart doch alls zu dienste stehn.", "tokens": ["So", "l\u00e4st", "der", "wi\u00b7der\u00b7part", "doch", "alls", "zu", "diens\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Seht keine farben an/ verwerfft das h\u00f6chste blat/", "tokens": ["Seht", "kei\u00b7ne", "far\u00b7ben", "an", "/", "ver\u00b7werfft", "das", "h\u00f6chs\u00b7te", "blat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob eure liebgen gleich kaum eine sieben hat:", "tokens": ["Ob", "eu\u00b7re", "lieb\u00b7gen", "gleich", "kaum", "ei\u00b7ne", "sie\u00b7ben", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wagt alles geld daran/ und denckt wenn ihr verspielt/", "tokens": ["Wagt", "al\u00b7les", "geld", "da\u00b7ran", "/", "und", "denckt", "wenn", "ihr", "ver\u00b7spielt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PAV", "$(", "KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df eure hertzen-\u00e4\u00df nach ihrem hertzen ziehlt.", "tokens": ["Da\u00df", "eu\u00b7re", "hert\u00b7zen\u00b7\u00e4\u00df", "nach", "ih\u00b7rem", "hert\u00b7zen", "ziehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr thut es nicht umsonst: Habt ihr was eingeb\u00fcst/", "tokens": ["Ihr", "thut", "es", "nicht", "um\u00b7sonst", ":", "Habt", "ihr", "was", "ein\u00b7ge\u00b7b\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "VAFIN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So trefft ihr einen tausch der wohl zu w\u00fcnschen ist.", "tokens": ["So", "trefft", "ihr", "ei\u00b7nen", "tausch", "der", "wohl", "zu", "w\u00fcn\u00b7schen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Nymphen sind was stoltz/ die meisten zweiffeln nicht/", "tokens": ["Die", "Nym\u00b7phen", "sind", "was", "stoltz", "/", "die", "meis\u00b7ten", "zweif\u00b7feln", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "$(", "ART", "PIS", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als w\u00e4r ihm alles geld und alles gut verpflicht:", "tokens": ["Als", "w\u00e4r", "ihm", "al\u00b7les", "geld", "und", "al\u00b7les", "gut", "ver\u00b7pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIAT", "NN", "KON", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer ihrer einfalt nun so viel zu willen thut/", "tokens": ["Wer", "ih\u00b7rer", "ein\u00b7falt", "nun", "so", "viel", "zu", "wil\u00b7len", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Demselben sind sie auch von gantzem hertzen gut.", "tokens": ["Dem\u00b7sel\u00b7ben", "sind", "sie", "auch", "von", "gant\u00b7zem", "hert\u00b7zen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wohl dem der solch ein gl\u00fcck in seinem spiel empfindt/", "tokens": ["Wohl", "dem", "der", "solch", "ein", "gl\u00fcck", "in", "sei\u00b7nem", "spiel", "emp\u00b7findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "PIAT", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df wenn er gleich verspielt noch was dazu gewinnt.", "tokens": ["Da\u00df", "wenn", "er", "gleich", "ver\u00b7spielt", "noch", "was", "da\u00b7zu", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "PWS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}