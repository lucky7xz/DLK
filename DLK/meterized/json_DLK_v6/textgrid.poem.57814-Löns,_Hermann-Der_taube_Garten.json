{"textgrid.poem.57814": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der taube Garten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lachst, weil ich dich liebe,", "tokens": ["Du", "lachst", ",", "weil", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast deinen Spott mit mir;", "tokens": ["Hast", "dei\u00b7nen", "Spott", "mit", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n bist du von Gesichte,", "tokens": ["Sch\u00f6n", "bist", "du", "von", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch fehlt das Herze dir.", "tokens": ["Doch", "fehlt", "das", "Her\u00b7ze", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darum sind deine Wangen", "tokens": ["Da\u00b7rum", "sind", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie der Schnee so wei\u00df;", "tokens": ["Als", "wie", "der", "Schnee", "so", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deine blauen Augen", "tokens": ["Und", "dei\u00b7ne", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sind so kalt wie Eis.", "tokens": ["Die", "sind", "so", "kalt", "wie", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn andre M\u00e4dchen lieben,", "tokens": ["Wenn", "and\u00b7re", "M\u00e4d\u00b7chen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann bist du ganz allein;", "tokens": ["Dann", "bist", "du", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es bl\u00fchen keine Rosen", "tokens": ["Es", "bl\u00fc\u00b7hen", "kei\u00b7ne", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinem G\u00e4rtelein.", "tokens": ["In", "dei\u00b7nem", "G\u00e4r\u00b7tel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Garten ohne Rosen", "tokens": ["Ein", "Gar\u00b7ten", "oh\u00b7ne", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht keinem Menschen Freud';", "tokens": ["Macht", "kei\u00b7nem", "Men\u00b7schen", "Freud'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00e4dchen ohne Liebe", "tokens": ["Ein", "M\u00e4d\u00b7chen", "oh\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das tut sich selber leid.", "tokens": ["Das", "tut", "sich", "sel\u00b7ber", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}