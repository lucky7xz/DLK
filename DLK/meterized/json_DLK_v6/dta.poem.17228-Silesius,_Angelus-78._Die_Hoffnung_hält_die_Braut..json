{"dta.poem.17228": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "78.  Die Hoffnung h\u00e4lt die Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung h\u00e4lt mich noch; sonst w\u00e4r\u2019 ich l\u00e4ngst dahin:", "tokens": ["Die", "Hoff\u00b7nung", "h\u00e4lt", "mich", "noch", ";", "sonst", "w\u00e4r'", "ich", "l\u00e4ngst", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum\u0304? dieweil ich nicht bey meinem Br\u00e4utgam bin", "tokens": ["War\u00b7um\u0304", "?", "die\u00b7weil", "ich", "nicht", "bey", "mei\u00b7nem", "Br\u00e4ut\u00b7gam", "bin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}