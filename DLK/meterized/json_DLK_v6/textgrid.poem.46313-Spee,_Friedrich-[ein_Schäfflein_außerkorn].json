{"textgrid.poem.46313": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein Sch\u00e4fflein au\u00dferkorn]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00e4fflein au\u00dferkorn", "tokens": ["Ein", "Sch\u00e4f\u00b7flein", "au\u00b7\u00dfer\u00b7korn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Such ich so manchen tag,", "tokens": ["Such", "ich", "so", "man\u00b7chen", "tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat sich so gar verlorn,", "tokens": ["Hat", "sich", "so", "gar", "ver\u00b7lorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mans nitt finden mag.", "tokens": ["Da\u00df", "mans", "nitt", "fin\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PIS", "VVFIN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Will ruffen laut mit heller stimm,", "tokens": ["Will", "ruf\u00b7fen", "laut", "mit", "hel\u00b7ler", "stimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ADJD", "APPR", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df obs ich nit bald vernim.", "tokens": ["Wer", "wei\u00df", "obs", "ich", "nit", "bald", "ver\u00b7nim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00e4fflein, Sch\u00e4fflein, wo hast verkrochen dich?", "tokens": ["Sch\u00e4f\u00b7flein", ",", "Sch\u00e4f\u00b7flein", ",", "wo", "hast", "ver\u00b7kro\u00b7chen", "dich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "VAFIN", "VVPP", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Ach folge mir: hilff selber dir,", "tokens": ["Ach", "fol\u00b7ge", "mir", ":", "hilff", "sel\u00b7ber", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$.", "NN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nit lang la\u00df suchen mich.", "tokens": ["Nit", "lang", "la\u00df", "su\u00b7chen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, ach, du liebes blut,", "tokens": ["Ach", ",", "ach", ",", "du", "lie\u00b7bes", "blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer dich nur finden k\u00f6ndt,", "tokens": ["Wer", "dich", "nur", "fin\u00b7den", "k\u00f6ndt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald mir hertz vnd muth,", "tokens": ["Wie", "bald", "mir", "hertz", "vnd", "muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In lauter frewden st\u00fcnd?", "tokens": ["In", "lau\u00b7ter", "frew\u00b7den", "st\u00fcnd", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Starck ruffen will ich widerumb,", "tokens": ["Starck", "ruf\u00b7fen", "will", "ich", "wi\u00b7de\u00b7rumb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df es m\u00f6chte kehren vmb?", "tokens": ["Wer", "wei\u00df", "es", "m\u00f6ch\u00b7te", "keh\u00b7ren", "vmb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VMFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00e4fflein Sch\u00e4fflein, mein junges thierlein zart,", "tokens": ["Sch\u00e4f\u00b7flein", "Sch\u00e4f\u00b7flein", ",", "mein", "jun\u00b7ges", "thier\u00b7lein", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Wo solle dann: dich treffen an?", "tokens": ["Wo", "sol\u00b7le", "dann", ":", "dich", "tref\u00b7fen", "an", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil bin betr\u00fcbet hart.", "tokens": ["Weil", "bin", "be\u00b7tr\u00fc\u00b7bet", "hart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach was wird dencken doch", "tokens": ["Ach", "was", "wird", "den\u00b7cken", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VAFIN", "PDS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der fromme Vatter mein?", "tokens": ["Der", "from\u00b7me", "Vat\u00b7ter", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich au\u00dfbleibe noch,", "tokens": ["Da\u00df", "ich", "au\u00df\u00b7blei\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Sch\u00e4ffelein?", "tokens": ["Mit", "sei\u00b7nem", "Sch\u00e4f\u00b7fe\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will vnterde\u00df ihm sprechen zu,", "tokens": ["Will", "vn\u00b7ter\u00b7de\u00df", "ihm", "spre\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst findet er auch keine ruh:", "tokens": ["Sonst", "fin\u00b7det", "er", "auch", "kei\u00b7ne", "ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vatter, Vatter, hoff bald in kurtzer weil,", "tokens": ["Vat\u00b7ter", ",", "Vat\u00b7ter", ",", "hoff", "bald", "in", "kurt\u00b7zer", "weil", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ADV", "APPR", "ADJA", "KOUS", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "In jenem thall, ich ie zumahl", "tokens": ["In", "je\u00b7nem", "thall", ",", "ich", "ie", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PPER", "ADV", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es endlich doch ereyll.", "tokens": ["Es", "end\u00b7lich", "doch", "e\u00b7reyll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ey da, da duncket mich", "tokens": ["Ey", "da", ",", "da", "dun\u00b7cket", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In jener stauden dick,", "tokens": ["In", "je\u00b7ner", "stau\u00b7den", "dick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r ichs bewegen sich,", "tokens": ["H\u00f6r", "ichs", "be\u00b7we\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In schnellem augenblick:", "tokens": ["In", "schnel\u00b7lem", "au\u00b7gen\u00b7blick", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Halt, halt, da wird es warlich sein,", "tokens": ["Halt", ",", "halt", ",", "da", "wird", "es", "war\u00b7lich", "sein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ruffen starck zum wald hinein:", "tokens": ["Will", "ruf\u00b7fen", "starck", "zum", "wald", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00e4fflein, Sch\u00e4fflein, du liebstes Sch\u00e4fflein mein,", "tokens": ["Sch\u00e4f\u00b7flein", ",", "Sch\u00e4f\u00b7flein", ",", "du", "liebs\u00b7tes", "Sch\u00e4f\u00b7flein", "mein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Kehr widerumb: kehr widerum,", "tokens": ["Kehr", "wi\u00b7de\u00b7rumb", ":", "kehr", "wi\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sehr gro\u00df ist meine pein.", "tokens": ["Sehr", "gro\u00df", "ist", "mei\u00b7ne", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach aber nein, ach nein,", "tokens": ["Ach", "a\u00b7ber", "nein", ",", "ach", "nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PTKANT", "$,", "XY", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist noch mein thierlein nitt:", "tokens": ["Ist", "noch", "mein", "thier\u00b7lein", "nitt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey Sonn- vnd Monat-schein,", "tokens": ["Bey", "Sonn", "vnd", "Mo\u00b7nat\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df thun noch manchen tritt;", "tokens": ["Mu\u00df", "thun", "noch", "man\u00b7chen", "tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df immer, immer ruffen fort,", "tokens": ["Mu\u00df", "im\u00b7mer", ",", "im\u00b7mer", "ruf\u00b7fen", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch berg vnd thall, vnd aller ortt:", "tokens": ["Durch", "berg", "vnd", "thall", ",", "vnd", "al\u00b7ler", "ortt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sch\u00e4fflein, Sch\u00e4fflein, was will ich fangen an?", "tokens": ["Sch\u00e4f\u00b7flein", ",", "Sch\u00e4f\u00b7flein", ",", "was", "will", "ich", "fan\u00b7gen", "an", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Ach lauter ach, \u00f4 tr\u00fcbe sach!", "tokens": ["Ach", "lau\u00b7ter", "ach", ",", "\u00f4", "tr\u00fc\u00b7be", "sach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "ITJ", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weill dich nit finden kan.", "tokens": ["Weill", "dich", "nit", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Ey da, da treff ich an", "tokens": ["Ey", "da", ",", "da", "treff", "ich", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Creutzbaum wol bekandt,", "tokens": ["Ein", "Creutz\u00b7baum", "wol", "be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff einem berge stahn,", "tokens": ["Auff", "ei\u00b7nem", "ber\u00b7ge", "stahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Calvarien genandt:", "tokens": ["Cal\u00b7va\u00b7ri\u00b7en", "ge\u00b7nandt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hie duncket mich, hats seinen lauff:", "tokens": ["Hie", "dun\u00b7cket", "mich", ",", "hats", "sei\u00b7nen", "lauff", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hie mu\u00df ich starcker schreien auff:", "tokens": ["Hie", "mu\u00df", "ich", "star\u00b7cker", "schrei\u00b7en", "auff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00e4fflein, Sch\u00e4fflein, nun bin ich aller matt;", "tokens": ["Sch\u00e4f\u00b7flein", ",", "Sch\u00e4f\u00b7flein", ",", "nun", "bin", "ich", "al\u00b7ler", "matt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Dein warten hie: noch weichen ie,", "tokens": ["Dein", "war\u00b7ten", "hie", ":", "noch", "wei\u00b7chen", "ie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Will ich von dieser statt.", "tokens": ["Will", "ich", "von", "die\u00b7ser", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mehr kan ich leben kaum,", "tokens": ["Mehr", "kan", "ich", "le\u00b7ben", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr matt- vnd m\u00fcdigkeit,", "tokens": ["F\u00fcr", "mat\u00b7t", "vnd", "m\u00fc\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehn mich an disen baum,", "tokens": ["Lehn", "mich", "an", "di\u00b7sen", "baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zu sterben bin bereit.", "tokens": ["Zu", "ster\u00b7ben", "bin", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach Thierlein zart, ach kemest doch!", "tokens": ["Ach", "Thier\u00b7lein", "zart", ",", "ach", "ke\u00b7mest", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJD", "$,", "ITJ", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil kleine zeit ich lebe noch,", "tokens": ["Weil", "klei\u00b7ne", "zeit", "ich", "le\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vatter, Vatter, nun ruff ich allermeist:", "tokens": ["Vat\u00b7ter", ",", "Vat\u00b7ter", ",", "nun", "ruff", "ich", "al\u00b7ler\u00b7meist", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "In deine h\u00e4nd: zu dir gewend,", "tokens": ["In", "dei\u00b7ne", "h\u00e4nd", ":", "zu", "dir", "ge\u00b7wend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Befihl ich meinen geist.", "tokens": ["Be\u00b7fihl", "ich", "mei\u00b7nen", "geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}