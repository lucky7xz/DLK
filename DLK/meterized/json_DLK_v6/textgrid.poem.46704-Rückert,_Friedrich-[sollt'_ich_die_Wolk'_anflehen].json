{"textgrid.poem.46704": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sollt' ich die Wolk' anflehen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt' ich die Wolk' anflehen,", "tokens": ["Sollt'", "ich", "die", "Wol\u00b7k'", "an\u00b7fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit Thr\u00e4nen beizustehen", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "bei\u00b7zu\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Auge, das da weint?", "tokens": ["Dem", "Au\u00b7ge", ",", "das", "da", "weint", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Soll ich die Sonne bitten,", "tokens": ["Soll", "ich", "die", "Son\u00b7ne", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df in des Kummers Mitten", "tokens": ["Da\u00df", "in", "des", "Kum\u00b7mers", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie einen Trost mir scheint?", "tokens": ["Sie", "ei\u00b7nen", "Trost", "mir", "scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es macht der Wolke Schauer", "tokens": ["Es", "macht", "der", "Wol\u00b7ke", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur d\u00fcstrer meine Trauer,", "tokens": ["Nur", "d\u00fcst\u00b7rer", "mei\u00b7ne", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinweg mit Wolkenrauch!", "tokens": ["Hin\u00b7weg", "mit", "Wol\u00b7ken\u00b7rauch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir zeigt das Licht der Sonne", "tokens": ["Mir", "zeigt", "das", "Licht", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur was mir fehlt zur Wonne,", "tokens": ["Nur", "was", "mir", "fehlt", "zur", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weg mit der Sonne auch!", "tokens": ["Weg", "mit", "der", "Son\u00b7ne", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "La\u00df Sonn' und Wolkenschatten", "tokens": ["La\u00df", "Sonn'", "und", "Wol\u00b7ken\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich nur in L\u00fcften gatten", "tokens": ["Sich", "nur", "in", "L\u00fcf\u00b7ten", "gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wechseln auf der Flur!", "tokens": ["Und", "wech\u00b7seln", "auf", "der", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schattenbild sind beide,", "tokens": ["Ein", "Schat\u00b7ten\u00b7bild", "sind", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Wiederschein vom Leide", "tokens": ["Ein", "Wie\u00b7der\u00b7schein", "vom", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In deinem Herzen nur.", "tokens": ["In", "dei\u00b7nem", "Her\u00b7zen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}