{"textgrid.poem.35751": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "11. Overbecks zweiundsechzigstem Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du in rosiger Wolk' hertrugst den \u00e4therischen Spr\u00f6\u00dfling", "tokens": ["Der", "du", "in", "ro\u00b7si\u00b7ger", "Wol\u00b7k'", "her\u00b7trugst", "den", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "Spr\u00f6\u00df\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Overbeck, zu erfreun viele mit That und Gesang,", "tokens": ["O\u00b7ver\u00b7beck", ",", "zu", "er\u00b7freun", "vie\u00b7le", "mit", "That", "und", "Ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKZU", "VVINF", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Genius, h\u00f6re den Preis f\u00fcr den liebenden Gatten und Vater,", "tokens": ["Ge\u00b7nius", ",", "h\u00f6\u00b7re", "den", "Preis", "f\u00fcr", "den", "lie\u00b7ben\u00b7den", "Gat\u00b7ten", "und", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "F\u00fcr den Besorger der Stadt, und f\u00fcr den redlichen Freund.", "tokens": ["F\u00fcr", "den", "Be\u00b7sor\u00b7ger", "der", "Stadt", ",", "und", "f\u00fcr", "den", "red\u00b7li\u00b7chen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "La\u00df noch lang' ihn walten im Sinn der erhabenen Hansa,", "tokens": ["La\u00df", "noch", "lang'", "ihn", "wal\u00b7ten", "im", "Sinn", "der", "er\u00b7ha\u00b7be\u00b7nen", "Han\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df das Gemeinwohl neu bl\u00fche mit Schul' und Altar.", "tokens": ["Da\u00df", "das", "Ge\u00b7mein\u00b7wohl", "neu", "bl\u00fc\u00b7he", "mit", "Schul'", "und", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Und wenn der Greis schwanhell mit Gesang heimschwebet zum \u00c4ther;", "tokens": ["Und", "wenn", "der", "Greis", "schwan\u00b7hell", "mit", "Ge\u00b7sang", "heim\u00b7schwe\u00b7bet", "zum", "\u00c4\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mich, den verbr\u00fcderten Geist, f\u00fchre mein Genius mit.", "tokens": ["Mich", ",", "den", "ver\u00b7br\u00fc\u00b7der\u00b7ten", "Geist", ",", "f\u00fch\u00b7re", "mein", "Ge\u00b7nius", "mit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}}}}