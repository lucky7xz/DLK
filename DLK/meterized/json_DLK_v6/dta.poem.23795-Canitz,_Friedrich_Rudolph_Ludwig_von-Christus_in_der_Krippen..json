{"dta.poem.23795": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Christus in der Krippen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Das Kind das dort in Heu und Stroh verstecke\nlieget/", "tokens": ["Das", "Kind", "das", "dort", "in", "Heu", "und", "Stroh", "ver\u00b7ste\u00b7cke", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dem das tumme Vieh aus seiner Wiegen fri\u00dft/", "tokens": ["Und", "dem", "das", "tum\u00b7me", "Vieh", "aus", "sei\u00b7ner", "Wie\u00b7gen", "fri\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist gr\u00f6sser als die Welt/ weil es GOtt selber ist/", "tokens": ["Ist", "gr\u00f6s\u00b7ser", "als", "die", "Welt", "/", "weil", "es", "Gott", "sel\u00b7ber", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$(", "KOUS", "PPER", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der \u00fcber H\u00f6ll und Tod in seiner Armuth sieget.", "tokens": ["Der", "\u00fc\u00b7ber", "H\u00f6ll", "und", "Tod", "in", "sei\u00b7ner", "Ar\u00b7muth", "sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.5": {"text": "Was mag die Ursach seyn/ da\u00df Er so schlecht erschienen", "tokens": ["Was", "mag", "die", "Ur\u00b7sach", "seyn", "/", "da\u00df", "Er", "so", "schlecht", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Thron der k\u00f6nte ja seyn von Saphir bereit/", "tokens": ["Sein", "Thron", "der", "k\u00f6n\u00b7te", "ja", "seyn", "von", "Sa\u00b7phir", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VMFIN", "ADV", "VAINF", "APPR", "NE", "ADJD", "$("], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Sein Lager mit dem Glantz der Sternen \u00fcberstreut/", "tokens": ["Sein", "La\u00b7ger", "mit", "dem", "Glantz", "der", "Ster\u00b7nen", "\u00fc\u00b7bers\u00b7treut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Warum bedient Ihn nicht ein Heer von Cherubinen?", "tokens": ["Wa\u00b7rum", "be\u00b7dient", "Ihn", "nicht", "ein", "Heer", "von", "Che\u00b7ru\u00b7bi\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kaum findet sich ein Raum den Heyland zu bewirthen;", "tokens": ["Kaum", "fin\u00b7det", "sich", "ein", "Raum", "den", "Hey\u00b7land", "zu", "be\u00b7wirt\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Krippe wird sein Bett\u2019/ ein Stall ist sein Pallast;", "tokens": ["Die", "Krip\u00b7pe", "wird", "sein", "Bett'", "/", "ein", "Stall", "ist", "sein", "Pal\u00b7last", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn Er die keusche Brust der Mutter hat umfa\u00dft/", "tokens": ["Wenn", "Er", "die", "keu\u00b7sche", "Brust", "der", "Mut\u00b7ter", "hat", "um\u00b7fa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So h\u00e4lt Er sein Pancket/ sein Hoff besteht aus Hirten.", "tokens": ["So", "h\u00e4lt", "Er", "sein", "Pan\u00b7cket", "/", "sein", "Hoff", "be\u00b7steht", "aus", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Ihr Grossen die ihr euch als G\u00f6tter la\u00dft verehren/", "tokens": ["Ihr", "Gros\u00b7sen", "die", "ihr", "euch", "als", "G\u00f6t\u00b7ter", "la\u00dft", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "PPER", "KOUS", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die ihr von eurem Stuhl aus Diamanten blitzt/", "tokens": ["Die", "ihr", "von", "eu\u00b7rem", "Stuhl", "aus", "Di\u00b7a\u00b7man\u00b7ten", "blitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und eurer Meynung nach dem Himmel n\u00e4her sitzt/", "tokens": ["Und", "eu\u00b7rer", "Mey\u00b7nung", "nach", "dem", "Him\u00b7mel", "n\u00e4\u00b7her", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als die so Menschen sind/ di\u00df wil euch etwas lehren:", "tokens": ["Als", "die", "so", "Men\u00b7schen", "sind", "/", "di\u00df", "wil", "euch", "et\u00b7was", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "NN", "VAFIN", "$(", "PDS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der H\u00f6chste spottet hier der G\u00fcter dieser Erden/", "tokens": ["Der", "H\u00f6chs\u00b7te", "spot\u00b7tet", "hier", "der", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die offt ein Sterblicher f\u00fcr seinen Himmel h\u00e4lt/", "tokens": ["Die", "offt", "ein", "Sterb\u00b7li\u00b7cher", "f\u00fcr", "sei\u00b7nen", "Him\u00b7mel", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und zeiget euch dabey/ da\u00df wenn es Ihm gef\u00e4llt/", "tokens": ["Und", "zei\u00b7get", "euch", "da\u00b7bey", "/", "da\u00df", "wenn", "es", "Ihm", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$(", "KOUS", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Purpur uns zu Heu/ und Heu zu Purpur werden.", "tokens": ["Der", "Pur\u00b7pur", "uns", "zu", "Heu", "/", "und", "Heu", "zu", "Pur\u00b7pur", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "$(", "KON", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}