{"textgrid.poem.38543": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann aus leichter Silberh\u00fclle", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann aus leichter Silberh\u00fclle", "tokens": ["Wann", "aus", "leich\u00b7ter", "Sil\u00b7ber\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Luna niederschaut,", "tokens": ["Lu\u00b7na", "nie\u00b7der\u00b7schaut", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sehn' ich mich nach dir, o Stille,", "tokens": ["Sehn'", "ich", "mich", "nach", "dir", ",", "o", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der J\u00fcngling nach der Braut!", "tokens": ["Wie", "der", "J\u00fcng\u00b7ling", "nach", "der", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! mit wehmuthsvoller R\u00fchrung,", "tokens": ["Ach", "!", "mit", "weh\u00b7muths\u00b7vol\u00b7ler", "R\u00fch\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freundin! denk ich dein,", "tokens": ["Freun\u00b7din", "!", "denk", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier wo Leichtsinn und Verf\u00fchrung", "tokens": ["Hier", "wo", "Leicht\u00b7sinn", "und", "Ver\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giftbethaute Rosen streun!", "tokens": ["Gift\u00b7be\u00b7thau\u00b7te", "Ro\u00b7sen", "streun", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo des Lasters Stirn zu kr\u00e4nzen", "tokens": ["Wo", "des", "Las\u00b7ters", "Stirn", "zu", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Blumen bl\u00fchn,", "tokens": ["Tau\u00b7send", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo vor wilden Taumelt\u00e4nzen", "tokens": ["Wo", "vor", "wil\u00b7den", "Tau\u00b7mel\u00b7t\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grazien und Unschuld fliehn;", "tokens": ["Gra\u00b7zi\u00b7en", "und", "Un\u00b7schuld", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo der Name des Verbrechers", "tokens": ["Wo", "der", "Na\u00b7me", "des", "Ver\u00b7bre\u00b7chers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zu den Sternen dringt,", "tokens": ["Zu", "den", "Ster\u00b7nen", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und das Haupt des Tugendr\u00e4chers", "tokens": ["Und", "das", "Haupt", "des", "Tu\u00b7gen\u00b7dr\u00e4\u00b7chers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Kerkers N\u00e4chte sinkt!", "tokens": ["In", "des", "Ker\u00b7kers", "N\u00e4ch\u00b7te", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O begl\u00fcckt, wen in des Haines", "tokens": ["O", "be\u00b7gl\u00fcckt", ",", "wen", "in", "des", "Hai\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "PWS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmerung versteckt,", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "An der Quelle Rand, ein kleines,", "tokens": ["An", "der", "Quel\u00b7le", "Rand", ",", "ein", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Buschumw\u00f6lbtes Strohdach deckt!", "tokens": ["Bu\u00b7schum\u00b7w\u00f6lb\u00b7tes", "Stroh\u00b7dach", "deckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du nur, heilge Stille, fl\u00fcgelst", "tokens": ["Du", "nur", ",", "heil\u00b7ge", "Stil\u00b7le", ",", "fl\u00fc\u00b7gelst"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch den Geist empor!", "tokens": ["Hoch", "den", "Geist", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchrst der Hofnung Schifflein, spiegelst", "tokens": ["F\u00fchrst", "der", "Hof\u00b7nung", "Schif\u00b7flein", ",", "spie\u00b7gelst"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns des Himmels Freuden vor! ", "tokens": ["Uns", "des", "Him\u00b7mels", "Freu\u00b7den", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}