{"textgrid.poem.59748": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Schlutz aller Lebensverse", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich L\u00f6wenm\u00e4hnen sch\u00fctteln", "tokens": ["K\u00f6nnt'", "ich", "L\u00f6\u00b7wen\u00b7m\u00e4h\u00b7nen", "sch\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Zorn und Mut der Jugend,", "tokens": ["Mit", "dem", "Zorn", "und", "Mut", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie gewaltig wollt' ich r\u00fctteln", "tokens": ["Wie", "ge\u00b7wal\u00b7tig", "wollt'", "ich", "r\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Tages blasser Tugend,", "tokens": ["An", "des", "Ta\u00b7ges", "blas\u00b7ser", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem Trug der Feigen, Matten \u2013", "tokens": ["An", "dem", "Trug", "der", "Fei\u00b7gen", ",", "Mat\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer will ihre Namen nennen?", "tokens": ["Wer", "will", "ih\u00b7re", "Na\u00b7men", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Die der V\u00e4ter Heldenschatten", "tokens": ["Die", "der", "V\u00e4\u00b7ter", "Hel\u00b7den\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur als Leichenschatten kennen.", "tokens": ["Nur", "als", "Lei\u00b7chen\u00b7schat\u00b7ten", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eisen galt in meinen Tagen.", "tokens": ["Ei\u00b7sen", "galt", "in", "mei\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch' ich solchen Stundenweisern,", "tokens": ["Horch'", "ich", "sol\u00b7chen", "Stun\u00b7den\u00b7wei\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r' ich sagen, fragen, klagen,", "tokens": ["H\u00f6r'", "ich", "sa\u00b7gen", ",", "fra\u00b7gen", ",", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eisern sei ich, \u00fcbereisern,", "tokens": ["Ei\u00b7sern", "sei", "ich", ",", "\u00fc\u00b7be\u00b7rei\u00b7sern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern sei mir das Los gefallen", "tokens": ["Fern", "sei", "mir", "das", "Los", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den edlen Glanzmetallen,", "tokens": ["Von", "den", "ed\u00b7len", "Glanz\u00b7me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fern, o fern von jenen allen,", "tokens": ["Fern", ",", "o", "fern", "von", "je\u00b7nen", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJD", "APPR", "PDAT", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Woraus feine Kl\u00e4nge schallen.", "tokens": ["Wo\u00b7raus", "fei\u00b7ne", "Kl\u00e4n\u00b7ge", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weg vom Silber denn, vom Golde", "tokens": ["Weg", "vom", "Sil\u00b7ber", "denn", ",", "vom", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin, wohin die Weiser weisen!", "tokens": ["Hin", ",", "wo\u00b7hin", "die", "Wei\u00b7ser", "wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trage, wie dein Schmied es wollte,", "tokens": ["Tra\u00b7ge", ",", "wie", "dein", "Schmied", "es", "woll\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trage mutig durch dein Eisen!", "tokens": ["Tra\u00b7ge", "mu\u00b7tig", "durch", "dein", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Preis ihm, der es hart geschmiedet!", "tokens": ["Preis", "ihm", ",", "der", "es", "hart", "ge\u00b7schmie\u00b7det", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer magst du w\u00fcrdig preisen,", "tokens": ["Nim\u00b7mer", "magst", "du", "w\u00fcr\u00b7dig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer, was die Welt befriedet,", "tokens": ["Nim\u00b7mer", ",", "was", "die", "Welt", "be\u00b7frie\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was die Welt erh\u00e4lt, das Eisen.", "tokens": ["Was", "die", "Welt", "er\u00b7h\u00e4lt", ",", "das", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O du Segenglanz des Pfluges!", "tokens": ["O", "du", "Se\u00b7gen\u00b7glanz", "des", "Pflu\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gold der \u00c4hren, Gold der Reben!", "tokens": ["Gold", "der", "\u00c4h\u00b7ren", ",", "Gold", "der", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O du Blitz des Degenzuges,", "tokens": ["O", "du", "Blitz", "des", "De\u00b7gen\u00b7zu\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem die V\u00f6lkerzwinger beben!", "tokens": ["Dem", "die", "V\u00f6l\u00b7ker\u00b7zwin\u00b7ger", "be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebenhalter, Ehrenhalter,", "tokens": ["Le\u00b7ben\u00b7hal\u00b7ter", ",", "Eh\u00b7ren\u00b7hal\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bestes Ding von besten Dingen,", "tokens": ["Bes\u00b7tes", "Ding", "von", "bes\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O ich k\u00f6nnte tausend Psalter", "tokens": ["O", "ich", "k\u00f6nn\u00b7te", "tau\u00b7send", "Psal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Voll von deinen Ehren klingen.", "tokens": ["Voll", "von", "dei\u00b7nen", "Eh\u00b7ren", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum Preis dem Rauhen, Harten,", "tokens": ["Da\u00b7rum", "Preis", "dem", "Rau\u00b7hen", ",", "Har\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Preis dem Menschenschirmer Eisen!", "tokens": ["Preis", "dem", "Men\u00b7schen\u00b7schir\u00b7mer", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag vom Blanken, Feinen, Zarten", "tokens": ["Mag", "vom", "Blan\u00b7ken", ",", "Fei\u00b7nen", ",", "Zar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "APPRART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich ein andrer Seines preisen,", "tokens": ["Sich", "ein", "an\u00b7drer", "Sei\u00b7nes", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich nur ein F\u00fcnkchen z\u00e4hlen", "tokens": ["Kann", "ich", "nur", "ein", "F\u00fcnk\u00b7chen", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In mir echter M\u00e4nnergluten,", "tokens": ["In", "mir", "ech\u00b7ter", "M\u00e4n\u00b7ner\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6nn' ich gern den weichen Seelen", "tokens": ["G\u00f6nn'", "ich", "gern", "den", "wei\u00b7chen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Volle Weibersehnsuchtsfluten.", "tokens": ["Vol\u00b7le", "Wei\u00b7ber\u00b7sehn\u00b7suchts\u00b7flu\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}