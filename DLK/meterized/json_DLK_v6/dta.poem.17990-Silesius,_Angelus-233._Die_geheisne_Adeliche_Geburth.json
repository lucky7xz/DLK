{"dta.poem.17990": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "233.  Die geheisne Adeliche Geburth", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Au\u00df GOtt bin ich gebohrn/ erzeugt in seinem Sohn/", "tokens": ["Au\u00df", "Gott", "bin", "ich", "ge\u00b7bohrn", "/", "er\u00b7zeugt", "in", "sei\u00b7nem", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geheiliget im Geist/ di\u00df ist mein adels Kron!", "tokens": ["Ge\u00b7hei\u00b7li\u00b7get", "im", "Geist", "/", "di\u00df", "ist", "mein", "a\u00b7dels", "Kron", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}