{"textgrid.poem.67146": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Wiedersehens Tr\u00e4nen, des Wiedersehns", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Wiedersehens Tr\u00e4nen, des Wiedersehns", "tokens": ["Des", "Wie\u00b7der\u00b7se\u00b7hens", "Tr\u00e4\u00b7nen", ",", "des", "Wie\u00b7der\u00b7sehns"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umfangen, und dein Auge bei seinem Gru\u00df, \u2013", "tokens": ["Um\u00b7fan\u00b7gen", ",", "und", "dein", "Au\u00b7ge", "bei", "sei\u00b7nem", "Gru\u00df", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weissagend m\u00f6cht ich dies und all der", "tokens": ["Weis\u00b7sa\u00b7gend", "m\u00f6cht", "ich", "dies", "und", "all", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "PDS", "KON", "PIAT", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zaubrischen Liebe Geschick dir singen.", "tokens": ["Zaub\u00b7ri\u00b7schen", "Lie\u00b7be", "Ge\u00b7schick", "dir", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwar jetzt auch, junger Genius! bist du sch\u00f6n,", "tokens": ["Zwar", "jetzt", "auch", ",", "jun\u00b7ger", "Ge\u00b7nius", "!", "bist", "du", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch einsam, und es freuet sich in sich selbst,", "tokens": ["Auch", "ein\u00b7sam", ",", "und", "es", "freu\u00b7et", "sich", "in", "sich", "selbst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "PPER", "VVFIN", "PRF", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es bl\u00fcht von eignem Geist und liebem", "tokens": ["Es", "bl\u00fcht", "von", "eig\u00b7nem", "Geist", "und", "lie\u00b7bem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herzensgesange die Musentochter.", "tokens": ["Her\u00b7zens\u00b7ge\u00b7san\u00b7ge", "die", "Mu\u00b7sen\u00b7toch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch anders ists in seliger Gegenwart,", "tokens": ["Doch", "an\u00b7ders", "ists", "in", "se\u00b7li\u00b7ger", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn an des Neugefundnen Blicke dein Geist sich kennt,", "tokens": ["Wenn", "an", "des", "Neu\u00b7ge\u00b7fund\u00b7nen", "Bli\u00b7cke", "dein", "Geist", "sich", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn friedlich du vor seinem Anschaun", "tokens": ["Wenn", "fried\u00b7lich", "du", "vor", "sei\u00b7nem", "An\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder in goldener Wolke wandelst.", "tokens": ["Wie\u00b7der", "in", "gol\u00b7de\u00b7ner", "Wol\u00b7ke", "wan\u00b7delst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Indessen denk, ihm leuchte das Sonnenlicht,", "tokens": ["In\u00b7des\u00b7sen", "denk", ",", "ihm", "leuch\u00b7te", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn tr\u00f6st und mahne, wenn er im Felde schl\u00e4ft,", "tokens": ["Ihn", "tr\u00f6st", "und", "mah\u00b7ne", ",", "wenn", "er", "im", "Fel\u00b7de", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Liebe Stern, und heitre Tage", "tokens": ["Der", "Lie\u00b7be", "Stern", ",", "und", "heit\u00b7re", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spare zum Ende das Herz sich immer.", "tokens": ["Spa\u00b7re", "zum", "En\u00b7de", "das", "Herz", "sich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und wenn er da ist, und die gefl\u00fcgelten,", "tokens": ["Und", "wenn", "er", "da", "ist", ",", "und", "die", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "KON", "ART", "ADJA", "$,"], "meter": "--+-+-+-+--", "measure": "anapaest.init"}, "line.2": {"text": "Die Liebesstunden schneller und schneller sind,", "tokens": ["Die", "Lie\u00b7bess\u00b7tun\u00b7den", "schnel\u00b7ler", "und", "schnel\u00b7ler", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann sich dein Brauttag neigt und trunkner", "tokens": ["Dann", "sich", "dein", "Braut\u00b7tag", "neigt", "und", "trunk\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "PPOSAT", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon die begl\u00fcckenden Sterne leuchten \u2013", "tokens": ["Schon", "die", "be\u00b7gl\u00fc\u00b7cken\u00b7den", "Ster\u00b7ne", "leuch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Nein, ihr Geliebten! nein, ich beneid euch nicht!", "tokens": ["Nein", ",", "ihr", "Ge\u00b7lieb\u00b7ten", "!", "nein", ",", "ich", "be\u00b7neid", "euch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Unsch\u00e4dlich, wie vom Lichte die Blume lebt,", "tokens": ["Un\u00b7sch\u00e4d\u00b7lich", ",", "wie", "vom", "Lich\u00b7te", "die", "Blu\u00b7me", "lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So leben, gern vom sch\u00f6nen Bilde", "tokens": ["So", "le\u00b7ben", ",", "gern", "vom", "sch\u00f6\u00b7nen", "Bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4umend, und selig und arm, die Dichter.", "tokens": ["Tr\u00e4u\u00b7mend", ",", "und", "se\u00b7lig", "und", "arm", ",", "die", "Dich\u00b7ter."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$,", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}