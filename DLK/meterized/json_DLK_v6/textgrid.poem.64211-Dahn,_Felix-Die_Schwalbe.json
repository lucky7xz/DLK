{"textgrid.poem.64211": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwalbe", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du schweben die Schwalbe dort,", "tokens": ["Siehst", "du", "schwe\u00b7ben", "die", "Schwal\u00b7be", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Herz, hoch oben im \u00c4therblau?", "tokens": ["Herz", ",", "hoch", "o\u00b7ben", "im", "\u00c4\u00b7ther\u00b7blau", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "So hoch kannst du dich schwingen auch: \u2013", "tokens": ["So", "hoch", "kannst", "du", "dich", "schwin\u00b7gen", "auch", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PRF", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herz, entfalte die Fl\u00fcgel!", "tokens": ["Herz", ",", "ent\u00b7fal\u00b7te", "die", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}