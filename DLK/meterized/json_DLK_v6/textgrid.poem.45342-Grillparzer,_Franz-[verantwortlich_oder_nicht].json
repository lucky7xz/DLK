{"textgrid.poem.45342": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[verantwortlich oder nicht]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verantwortlich oder nicht;", "tokens": ["Ver\u00b7ant\u00b7wort\u00b7lich", "o\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn wirklich letztres w\u00e4re,", "tokens": ["Wenn", "wirk\u00b7lich", "letz\u00b7tres", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verstummt wohl das Gericht,", "tokens": ["Ver\u00b7stummt", "wohl", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch darum nicht die Ehre.", "tokens": ["Doch", "da\u00b7rum", "nicht", "die", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}