{"textgrid.poem.39340": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Menschenherz! Was gleichet deinem scharfen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Menschenherz! Was gleichet deinem scharfen", "tokens": ["O", "Men\u00b7schen\u00b7herz", "!", "Was", "glei\u00b7chet", "dei\u00b7nem", "schar\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerechten Feinsinn? Wer versteht, wie Du,", "tokens": ["Ge\u00b7rech\u00b7ten", "Fein\u00b7sinn", "?", "Wer", "ver\u00b7steht", ",", "wie", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was nie verstehbar ist dem rohen Hartsinn?", "tokens": ["Was", "nie", "ver\u00b7steh\u00b7bar", "ist", "dem", "ro\u00b7hen", "Har\u00b7tsinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Dir ist, wie Mittagsstral, so manche Nacht hell,", "tokens": ["Dir", "ist", ",", "wie", "Mit\u00b7tags\u00b7stral", ",", "so", "man\u00b7che", "Nacht", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "NN", "$,", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ein Greuel Dir die Sylbenklauberey", "tokens": ["Ein", "Greu\u00b7el", "Dir", "die", "Syl\u00b7ben\u00b7klau\u00b7be\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Schalkheit, welche laut vom h\u00f6chsten Recht spricht,", "tokens": ["Der", "Schalk\u00b7heit", ",", "wel\u00b7che", "laut", "vom", "h\u00f6chs\u00b7ten", "Recht", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Und h\u00f6hnend lacht, wenn schlichte Menschlichkeit", "tokens": ["Und", "h\u00f6h\u00b7nend", "lacht", ",", "wenn", "schlich\u00b7te", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es Teufeley und h\u00f6chstes Unrecht nennet.", "tokens": ["Es", "Teu\u00b7fe\u00b7ley", "und", "h\u00f6chs\u00b7tes", "Un\u00b7recht", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Menschenherz! An Stralen ist die Sonne", "tokens": ["O", "Men\u00b7schen\u00b7herz", "!", "An", "Stra\u00b7len", "ist", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht reicher, nicht der Ocean an Wogen", "tokens": ["Nicht", "rei\u00b7cher", ",", "nicht", "der", "O\u00b7cean", "an", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als Du an feinen Freuderfindungen.", "tokens": ["Als", "Du", "an", "fei\u00b7nen", "Freu\u00b7der\u00b7fin\u00b7dun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den vollsten Kelch der Menschenlust genie\u00dfet", "tokens": ["Den", "volls\u00b7ten", "Kelch", "der", "Men\u00b7schen\u00b7lust", "ge\u00b7nie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur der, der nie das Ziel der Freuderfindung", "tokens": ["Nur", "der", ",", "der", "nie", "das", "Ziel", "der", "Freu\u00b7der\u00b7fin\u00b7dung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr Freuded\u00fcrftige zu seh'n vermag.", "tokens": ["F\u00fcr", "Freu\u00b7de\u00b7d\u00fcrf\u00b7ti\u00b7ge", "zu", "seh'n", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur der empfindet tief den hohen Adel", "tokens": ["Nur", "der", "emp\u00b7fin\u00b7det", "tief", "den", "ho\u00b7hen", "A\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Menschheit, l\u00e4\u00dft sich nie die G\u00f6tterw\u00fcrde", "tokens": ["Der", "Menschheit", ",", "l\u00e4\u00dft", "sich", "nie", "die", "G\u00f6t\u00b7ter\u00b7w\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der menschlichen Gestalt verleumden, der", "tokens": ["Der", "menschli\u00b7chen", "Ge\u00b7stalt", "ver\u00b7leum\u00b7den", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PRELS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Stets unersch\u00f6pflich ist an Menschenfreuden.", "tokens": ["Stets", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "ist", "an", "Men\u00b7schen\u00b7freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gott ist nur Gott durch Lieb' und Sch\u00f6pfungswonne.", "tokens": ["Gott", "ist", "nur", "Gott", "durch", "Lieb'", "und", "Sch\u00f6p\u00b7fungs\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erfinden ist's was anders als erschaffen?", "tokens": ["Er\u00b7fin\u00b7den", "ist's", "was", "an\u00b7ders", "als", "er\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wer nichts erfand, kennt Sch\u00f6pfungsfreude nicht.", "tokens": ["Wer", "nichts", "er\u00b7fand", ",", "kennt", "Sch\u00f6p\u00b7fungs\u00b7freu\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "An Gottes Statt, in Gottes hohem Namen", "tokens": ["An", "Got\u00b7tes", "Statt", ",", "in", "Got\u00b7tes", "ho\u00b7hem", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Erschafft der Freuderfinder. Lieber, froher", "tokens": ["Er\u00b7schafft", "der", "Freu\u00b7der\u00b7fin\u00b7der", ".", "Lie\u00b7ber", ",", "fro\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$.", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Erschuff der Sch\u00f6pfer nie als durch den Guten,", "tokens": ["Er\u00b7schuff", "der", "Sch\u00f6p\u00b7fer", "nie", "als", "durch", "den", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der nie sich selig nennt als wenn er Br\u00fcder", "tokens": ["Der", "nie", "sich", "se\u00b7lig", "nennt", "als", "wenn", "er", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "ADJD", "VVFIN", "KOKOM", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Beseligt! Herz, Du bist's, was Gott erkohr!", "tokens": ["Be\u00b7se\u00b7ligt", "!", "Herz", ",", "Du", "bist's", ",", "was", "Gott", "er\u00b7kohr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "$,", "PPER", "NE", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Das Be\u00dfte w\u00e4hlt der Beste nur. Der Thron", "tokens": ["Das", "Be\u00df\u00b7te", "w\u00e4hlt", "der", "Bes\u00b7te", "nur", ".", "Der", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Geb\u00fchrt dem K\u00f6nige, das Herz dem Vater", "tokens": ["Ge\u00b7b\u00fchrt", "dem", "K\u00f6\u00b7ni\u00b7ge", ",", "das", "Herz", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Der Menschenherzen nur. Der ewig liebt,", "tokens": ["Der", "Men\u00b7schen\u00b7her\u00b7zen", "nur", ".", "Der", "e\u00b7wig", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Bewohnt der H\u00f6hen H\u00f6h' und Dich, o Du,", "tokens": ["Be\u00b7wohnt", "der", "H\u00f6\u00b7hen", "H\u00f6h'", "und", "Dich", ",", "o", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "PPER", "$,", "FM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Geisterwelten Ehre, Menschenherz,", "tokens": ["Der", "Geis\u00b7ter\u00b7wel\u00b7ten", "Eh\u00b7re", ",", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wo Freuderfindung sich mit Einfalt paart!", "tokens": ["Wo", "Freu\u00b7der\u00b7fin\u00b7dung", "sich", "mit", "Ein\u00b7falt", "paart", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}