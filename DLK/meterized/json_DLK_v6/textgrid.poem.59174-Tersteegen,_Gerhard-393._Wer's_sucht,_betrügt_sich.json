{"textgrid.poem.59174": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "393. Wer's sucht, betr\u00fcgt sich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kreuz, wenn ich geh' und suche dich,", "tokens": ["Kreuz", ",", "wenn", "ich", "geh'", "und", "su\u00b7che", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Kannst du dein Hartes sch\u00f6n bedecken;", "tokens": ["Kannst", "du", "dein", "Har\u00b7tes", "sch\u00f6n", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis ich dich liebe, lockst du mich,", "tokens": ["Bis", "ich", "dich", "lie\u00b7be", ",", "lockst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hernach so l\u00e4\u00dft du mich betrogen stecken.", "tokens": ["Her\u00b7nach", "so", "l\u00e4\u00dft", "du", "mich", "be\u00b7tro\u00b7gen", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}