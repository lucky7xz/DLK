{"dta.poem.20915": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rudolph an Ermegarden.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht was dein Brief vor Regung in", "tokens": ["Ich", "wei\u00df", "nicht", "was", "dein", "Brief", "vor", "Re\u00b7gung", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPOSAT", "NN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mich jaget/", "tokens": ["mich", "ja\u00b7get", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ein Wort das warnet mich/ das andre dreu-", "tokens": ["Ein", "Wort", "das", "war\u00b7net", "mich", "/", "das", "and\u00b7re", "dreu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "PPER", "$(", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "et mir/", "tokens": ["et", "mir", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Es scheint wie ieder Reim mir in die Ohren saget/", "tokens": ["Es", "scheint", "wie", "ie\u00b7der", "Reim", "mir", "in", "die", "Oh\u00b7ren", "sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach Rudolph siehe dich auch vor dir selber f\u00fcr.", "tokens": ["Ach", "Ru\u00b7dolph", "sie\u00b7he", "dich", "auch", "vor", "dir", "sel\u00b7ber", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PPER", "ADV", "APPR", "PPER", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich sage wie es ist/ ich kam hieher zufragen/", "tokens": ["Ich", "sa\u00b7ge", "wie", "es", "ist", "/", "ich", "kam", "hie\u00b7her", "zu\u00b7fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VAFIN", "$(", "PPER", "VVFIN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was vor ein stoltzes Haubt die welsche Crone sucht/", "tokens": ["Was", "vor", "ein", "stolt\u00b7zes", "Haubt", "die", "wel\u00b7sche", "Cro\u00b7ne", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man schaute dieses Heer Schwerdt/ Pfeil und Feuer", "tokens": ["Man", "schau\u00b7te", "die\u00b7ses", "Heer", "Schwerdt", "/", "Pfeil", "und", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "NE", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "tragen/", "tokens": ["tra\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Es ward Pavie und du von iederman verflucht.", "tokens": ["Es", "ward", "Pa\u00b7vie", "und", "du", "von", "ie\u00b7der\u00b7man", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein heisses Hertze lag voll heisser Zornes Flammen/", "tokens": ["Mein", "heis\u00b7ses", "Hert\u00b7ze", "lag", "voll", "heis\u00b7ser", "Zor\u00b7nes", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VVFIN", "ADJD", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich deucht/ ein Blick von mir der steckte D\u00f6rfer an/", "tokens": ["Mich", "deucht", "/", "ein", "Blick", "von", "mir", "der", "steck\u00b7te", "D\u00f6r\u00b7fer", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie reimt sich aber heut und gestern doch zusammen?", "tokens": ["Wie", "reimt", "sich", "a\u00b7ber", "heut", "und", "ge\u00b7stern", "doch", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADV", "KON", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Wohl dem der allezeit best\u00e4ndig bleiben kan.", "tokens": ["Wohl", "dem", "der", "al\u00b7le\u00b7zeit", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhr Frauen traget nur das Kraut in euren H\u00e4nden/", "tokens": ["Ihr", "Frau\u00b7en", "tra\u00b7get", "nur", "das", "Kraut", "in", "eu\u00b7ren", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So Stahl zu weichem Wachs und Stein zu Wasser", "tokens": ["So", "Stahl", "zu", "wei\u00b7chem", "Wachs", "und", "Stein", "zu", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Jhr k\u00f6nt/ O sch\u00f6ne Kunst/ den Himmel selbst verblen-", "tokens": ["Ihr", "k\u00f6nt", "/", "O", "sch\u00f6\u00b7ne", "Kunst", "/", "den", "Him\u00b7mel", "selbst", "ver\u00b7blen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NE", "ADJA", "NN", "$(", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Und seyd bey eurer Lust auf unsre Noth bedacht.", "tokens": ["Und", "seyd", "bey", "eu\u00b7rer", "Lust", "auf", "uns\u00b7re", "Noth", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr brauchet unsern Witz/ als wie das Schilff im", "tokens": ["Ihr", "brau\u00b7chet", "un\u00b7sern", "Witz", "/", "als", "wie", "das", "Schilff", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KOUS", "KOKOM", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Strande/", "tokens": ["Stran\u00b7de", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Bald richtet ihr ihn auf/ bald dr\u00fccket ihr ihn ein/", "tokens": ["Bald", "rich\u00b7tet", "ihr", "ihn", "auf", "/", "bald", "dr\u00fc\u00b7cket", "ihr", "ihn", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "$(", "ADV", "VVFIN", "PPER", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jhr baut euch eine Burg aus Steinen unsrer Schan-", "tokens": ["Ihr", "baut", "euch", "ei\u00b7ne", "Burg", "aus", "Stei\u00b7nen", "uns\u00b7rer", "Schan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "de/", "tokens": ["de", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Und heist uns offtermahls nur viertel Menschen seyn.", "tokens": ["Und", "heist", "uns", "off\u00b7ter\u00b7mahls", "nur", "vier\u00b7tel", "Men\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Jhr streicht oft unser Schwerd damit ihr wolt ver-", "tokens": ["Ihr", "streicht", "oft", "un\u00b7ser", "Schwerd", "da\u00b7mit", "ihr", "wolt", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KOUS", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "wunden/", "tokens": ["wun\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["ADJA", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Mit s\u00fcssen Balsam an/ schlagt und beklagt zugleich/", "tokens": ["Mit", "s\u00fcs\u00b7sen", "Bal\u00b7sam", "an", "/", "schlagt", "und", "be\u00b7klagt", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$(", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Krancken lachet ihr und schont nicht der Gesun-", "tokens": ["Der", "Kran\u00b7cken", "la\u00b7chet", "ihr", "und", "schont", "nicht", "der", "Ge\u00b7sun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Und unsre Dienstbarkeit ist euer K\u00f6nigreich.", "tokens": ["Und", "uns\u00b7re", "Dienst\u00b7bar\u00b7keit", "ist", "eu\u00b7er", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das weigern wisset ihr mit Freundschafft zuverkleiden/", "tokens": ["Das", "wei\u00b7gern", "wis\u00b7set", "ihr", "mit", "Freund\u00b7schafft", "zu\u00b7ver\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr weint bey dessen Noth/ der euch doch Th\u00e4ter nennt/", "tokens": ["Ihr", "weint", "bey", "des\u00b7sen", "Noth", "/", "der", "euch", "doch", "Th\u00e4\u00b7ter", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELAT", "NN", "$(", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jhr \u00fcberredet uns in Wehmuth selbst zu leiden/", "tokens": ["Ihr", "\u00fc\u00b7berr\u00b7e\u00b7det", "uns", "in", "Weh\u00b7muth", "selbst", "zu", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In dem uns Hertz und Geist ohn alle H\u00fclffe brennt.", "tokens": ["In", "dem", "uns", "Hertz", "und", "Geist", "ohn", "al\u00b7le", "H\u00fclf\u00b7fe", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jhr seyd ja der Natur ber\u00fchmte Wunderwercke;", "tokens": ["Ihr", "seyd", "ja", "der", "Na\u00b7tur", "be\u00b7r\u00fchm\u00b7te", "Wun\u00b7der\u00b7wer\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Man nennt euch kalt von Arth/ und steckt die M\u00e4nner", "tokens": ["Man", "nennt", "euch", "kalt", "von", "Ar\u00b7th", "/", "und", "steckt", "die", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "NE", "$(", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "an/", "tokens": ["an", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.41": {"text": "Man heist euch schwachen Zeug/ und spottet unsrer", "tokens": ["Man", "heist", "euch", "schwa\u00b7chen", "Zeug", "/", "und", "spot\u00b7tet", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "$(", "KON", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "St\u00e4rcke/", "tokens": ["St\u00e4r\u00b7cke", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Man braucht euch nicht in Krieg/ und f\u00fchrt die Sie-", "tokens": ["Man", "braucht", "euch", "nicht", "in", "Krieg", "/", "und", "f\u00fchrt", "die", "Sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$(", "KON", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "ges Fahn;", "tokens": ["ges", "Fahn", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.45": {"text": "Was wil ich aber euch noch Ehren S\u00e4ulen bauen/", "tokens": ["Was", "wil", "ich", "a\u00b7ber", "euch", "noch", "Eh\u00b7ren", "S\u00e4u\u00b7len", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PPER", "ADV", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Es ist zuviel gebaut/ man macht mich selbst dazu/", "tokens": ["Es", "ist", "zu\u00b7viel", "ge\u00b7baut", "/", "man", "macht", "mich", "selbst", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$(", "PIS", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich meinte Pavie im Feuer anzuschauen/", "tokens": ["Ich", "mein\u00b7te", "Pa\u00b7vie", "im", "Feu\u00b7er", "an\u00b7zu\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "VVIZU", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Was itzo brennen soll/ O Hertze/ das bist du.", "tokens": ["Was", "it\u00b7zo", "bren\u00b7nen", "soll", "/", "O", "Hert\u00b7ze", "/", "das", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$(", "NE", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich bin nicht was ich war/ ich bin mir frembde wor-", "tokens": ["Ich", "bin", "nicht", "was", "ich", "war", "/", "ich", "bin", "mir", "fremb\u00b7de", "wor"], "token_info": ["word", "word", "word", "word", "word", "word