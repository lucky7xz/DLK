 "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.123": {"text": "Und sagte gleich die Welt/ ich h\u00e4tte sehr gefehlet/", "tokens": ["Und", "sag\u00b7te", "gleich", "die", "Welt", "/", "ich", "h\u00e4t\u00b7te", "sehr", "ge\u00b7feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.124": {"text": "Wer fehlt und f\u00e4llet nicht? Ich bin ein Erdenklo\u00df/", "tokens": ["Wer", "fehlt", "und", "f\u00e4l\u00b7let", "nicht", "?", "Ich", "bin", "ein", "Er\u00b7den\u00b7klo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.125": {"text": "Es ist mir/ fall\u2019 ich gleich/ ein sch\u00f6ner Orth erwehlet/", "tokens": ["Es", "ist", "mir", "/", "fall'", "ich", "gleich", "/", "ein", "sch\u00f6\u00b7ner", "Orth", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.126": {"text": "Ich falle nirgends hin/ als nur in deine Scho\u00df.", "tokens": ["Ich", "fal\u00b7le", "nir\u00b7gends", "hin", "/", "als", "nur", "in", "dei\u00b7ne", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$(", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}