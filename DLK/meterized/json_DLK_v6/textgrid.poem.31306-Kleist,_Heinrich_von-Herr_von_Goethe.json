{"textgrid.poem.31306": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Herr von Goethe", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, das nenn ich doch w\u00fcrdig, f\u00fcrwahr, sich im Alter besch\u00e4ftgen!", "tokens": ["Sie\u00b7he", ",", "das", "nenn", "ich", "doch", "w\u00fcr\u00b7dig", ",", "f\u00fcr\u00b7wahr", ",", "sich", "im", "Al\u00b7ter", "be\u00b7sch\u00e4ft\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "$,", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er zerlegt jetzt den Strahl, den seine Jugend sonst warf.", "tokens": ["Er", "zer\u00b7legt", "jetzt", "den", "Strahl", ",", "den", "sei\u00b7ne", "Ju\u00b7gend", "sonst", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}