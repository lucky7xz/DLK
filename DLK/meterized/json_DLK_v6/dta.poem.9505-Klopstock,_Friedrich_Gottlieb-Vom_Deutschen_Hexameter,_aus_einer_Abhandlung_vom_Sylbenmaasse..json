{"dta.poem.9505": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Vom  \n  Deutschen Hexameter,  \n aus einer  \n Abhandlung vom Sylbenmaasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1769", "urn": "urn:nbn:de:kobv:b4-200905199341", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eile dahin, wo der Tod, und das Grab, und die Nacht dich erwarten.", "tokens": ["Ei\u00b7le", "da\u00b7hin", ",", "wo", "der", "Tod", ",", "und", "das", "Grab", ",", "und", "die", "Nacht", "dich", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "PWAV", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wende dich weg, wehm\u00fcthiger Blick, von der Angst des Erdulders.", "tokens": ["Wen\u00b7de", "dich", "weg", ",", "weh\u00b7m\u00fct\u00b7hi\u00b7ger", "Blick", ",", "von", "der", "Angst", "des", "E\u00b7rdul\u00b7ders", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+----", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nenne sie, Klagestimme des Nachhalls, ihrem Geliebten.", "tokens": ["Nen\u00b7ne", "sie", ",", "Kla\u00b7ge\u00b7stim\u00b7me", "des", "Nach\u00b7halls", ",", "ih\u00b7rem", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Streit, und komm zu dem Miterbtheile des ewigen Lebens.", "tokens": ["Streit", ",", "und", "komm", "zu", "dem", "Mi\u00b7terb\u00b7thei\u00b7le", "des", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Freudig stieg ihr Geno\u00df zu dem Lichterbtheile des Heils auf.", "tokens": ["Freu\u00b7dig", "stieg", "ihr", "Ge\u00b7no\u00df", "zu", "dem", "Lich\u00b7terb\u00b7thei\u00b7le", "des", "Heils", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Schreckliche Todesangst, graunvolle Verzweiflungsstimmen.", "tokens": ["Schreck\u00b7li\u00b7che", "To\u00b7de\u00b7sangst", ",", "graun\u00b7vol\u00b7le", "Ver\u00b7zwei\u00b7flungs\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Furchtbarer Wehausruf, der hinab in das Thal aus der Kluft scholl.", "tokens": ["Furcht\u00b7ba\u00b7rer", "We\u00b7haus\u00b7ruf", ",", "der", "hin\u00b7ab", "in", "das", "Thal", "aus", "der", "Kluft", "scholl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ewiges Anschaun de\u00df, der im Lichtreich Dulder belohnet.", "tokens": ["E\u00b7wi\u00b7ges", "An\u00b7schaun", "de\u00df", ",", "der", "im", "Lich\u00b7treich", "Dul\u00b7der", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "$,", "PRELS", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Bebend erschollst, Nachtthal, und zur\u00fcckgabst deine Verwesten.", "tokens": ["Be\u00b7bend", "er\u00b7schollst", ",", "Nachtt\u00b7hal", ",", "und", "zu\u00b7r\u00fcck\u00b7gabst", "dei\u00b7ne", "Ver\u00b7wes\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}