{"textgrid.poem.45485": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Titel meiner St\u00fccke]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Titel meiner St\u00fccke", "tokens": ["Die", "Ti\u00b7tel", "mei\u00b7ner", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat man mir reichlich bezahlt,", "tokens": ["Hat", "man", "mir", "reich\u00b7lich", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Man gibt mir Titel f\u00fcr Titel,", "tokens": ["Man", "gibt", "mir", "Ti\u00b7tel", "f\u00fcr", "Ti\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als h\u00e4tten sie keinen Gehalt.", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "kei\u00b7nen", "Ge\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}