{"dta.poem.23352": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehrlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lernet beten, lesen, schreiben,", "tokens": ["Ler\u00b7net", "be\u00b7ten", ",", "le\u00b7sen", ",", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernet alle K\u00fcnste treiben,", "tokens": ["Ler\u00b7net", "al\u00b7le", "K\u00fcns\u00b7te", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lernet was der Welt gef\u00e4llt,", "tokens": ["Ler\u00b7net", "was", "der", "Welt", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernt euch schicken in die Welt;", "tokens": ["Lernt", "euch", "schi\u00b7cken", "in", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lernet aller Weisheit S\u00e4tze,", "tokens": ["Ler\u00b7net", "al\u00b7ler", "Weis\u00b7heit", "S\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernet alles Wissens Sch\u00e4tze,", "tokens": ["Ler\u00b7net", "al\u00b7les", "Wis\u00b7sens", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lernet Griechisch und Latein \u2014", "tokens": ["Ler\u00b7net", "Grie\u00b7chisch", "und", "La\u00b7tein"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ehrlich braucht ihr nicht zu sein.", "tokens": ["Ehr\u00b7lich", "braucht", "ihr", "nicht", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}