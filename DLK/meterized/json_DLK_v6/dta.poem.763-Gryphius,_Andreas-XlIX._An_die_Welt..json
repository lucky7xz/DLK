{"dta.poem.763": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.  An die Welt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mein offt best\u00fcrmbtes Schiff der grimmen winde spiel/ ", "tokens": ["Mein", "offt", "be\u00b7st\u00fcrmb\u00b7tes", "Schiff", "der", "grim\u00b7men", "win\u00b7de", "spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frechen Wellen Baall/ das schier die flut getrennet/", "tokens": ["Der", "fre\u00b7chen", "Wel\u00b7len", "Baall", "/", "das", "schier", "die", "flut", "ge\u00b7tren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "ART", "ADJA", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das vber klip auff klip/ vnd schaum/ vnd sandt gerennet? ", "tokens": ["Das", "vber", "klip", "auff", "klip", "/", "vnd", "schaum", "/", "vnd", "sandt", "ge\u00b7ren\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "APPR", "NE", "$(", "KON", "ADV", "$(", "KON", "ADJD", "VVPP", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kombt vor der zeit an port/ den meine Seele will.", "tokens": ["Kombt", "vor", "der", "zeit", "an", "port", "/", "den", "mei\u00b7ne", "See\u00b7le", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$(", "ART", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Offt wenn vns schwartze Nacht im mittag vberfiel:", "tokens": ["Offt", "wenn", "vns", "schwart\u00b7ze", "Nacht", "im", "mit\u00b7tag", "vber\u00b7fiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hat der geschwinde plitz die Seegel schier verbrennet! ", "tokens": ["Hat", "der", "ge\u00b7schwin\u00b7de", "plitz", "die", "See\u00b7gel", "schier", "ver\u00b7bren\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie offt hab ich den Windt/ vnd Nord\u2019 vnd Sud verkennet!", "tokens": ["Wie", "offt", "hab", "ich", "den", "Windt", "/", "vnd", "Nord'", "vnd", "Sud", "ver\u00b7ken\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "$(", "KON", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie schadthafft ist der Mast/ ", "tokens": ["Wie", "schadt\u00b7hafft", "ist", "der", "Mast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kiell.", "tokens": ["Kiell", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Steig au\u00df du m\u00fcder Geist/ steig aus! wir sindt am Lande!", "tokens": ["Steig", "au\u00df", "du", "m\u00fc\u00b7der", "Geist", "/", "steig", "aus", "!", "wir", "sindt", "am", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJA", "NN", "$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was grawt dir f\u00fcr dem port/ jtzt wirst du aller bande", "tokens": ["Was", "grawt", "dir", "f\u00fcr", "dem", "port", "/", "jtzt", "wirst", "du", "al\u00b7ler", "ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NE", "$(", "ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd angst/ vnd herber pein/ vnd schwerer schmertzen los. ", "tokens": ["Vnd", "angst", "/", "vnd", "her\u00b7ber", "pein", "/", "vnd", "schwe\u00b7rer", "schmert\u00b7zen", "los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "KON", "ADV", "NE", "$(", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ade/ verfluchte Welt: du See voll rawer st\u00fcrme:", "tokens": ["A\u00b7de", "/", "ver\u00b7fluch\u00b7te", "Welt", ":", "du", "See", "voll", "ra\u00b7wer", "st\u00fcr\u00b7me", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$.", "PPER", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gl\u00fcck zu mein ", "tokens": ["Gl\u00fcck", "zu", "mein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vnd schutz vnd friden h\u00e4lt/ du ewig-lichtes ", "tokens": ["Vnd", "schutz", "vnd", "fri\u00b7den", "h\u00e4lt", "/", "du", "e\u00b7wig\u00b7lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$(", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}