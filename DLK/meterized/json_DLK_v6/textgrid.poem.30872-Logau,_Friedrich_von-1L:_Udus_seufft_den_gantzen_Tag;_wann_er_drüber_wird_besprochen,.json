{"textgrid.poem.30872": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Udus seufft den gantzen Tag; wann er dr\u00fcber wird besprochen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Udus seufft den gantzen Tag; wann er dr\u00fcber wird besprochen,", "tokens": ["U\u00b7dus", "seufft", "den", "gant\u00b7zen", "Tag", ";", "wann", "er", "dr\u00fc\u00b7ber", "wird", "be\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$.", "PWAV", "PPER", "PAV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Spricht er: einen halben Tag hab ich mich am Durst gerochen;", "tokens": ["Spricht", "er", ":", "ei\u00b7nen", "hal\u00b7ben", "Tag", "hab", "ich", "mich", "am", "Durst", "ge\u00b7ro\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Drauff den andren halben Tag pfleg ich zuvor an zu sauffen,", "tokens": ["Drauff", "den", "an\u00b7dren", "hal\u00b7ben", "Tag", "pfleg", "ich", "zu\u00b7vor", "an", "zu", "sauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wann mich ja de\u00df Durstes Trotz wolte wieder \u00fcberlauffen.", "tokens": ["Wann", "mich", "ja", "de\u00df", "Durs\u00b7tes", "Trotz", "wol\u00b7te", "wie\u00b7der", "\u00fc\u00b7berl\u00b7auf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}