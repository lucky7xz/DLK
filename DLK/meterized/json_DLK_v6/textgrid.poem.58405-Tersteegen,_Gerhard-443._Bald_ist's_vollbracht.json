{"textgrid.poem.58405": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "443. Bald ist's vollbracht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Regen folget Sonnenschein,", "tokens": ["Auf", "Re\u00b7gen", "fol\u00b7get", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf K\u00e4lt' und St\u00fcrmen Sommertage,", "tokens": ["Auf", "K\u00e4lt'", "und", "St\u00fcr\u00b7men", "Som\u00b7mer\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommet Freude nach der Pein,", "tokens": ["Es", "kom\u00b7met", "Freu\u00b7de", "nach", "der", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergn\u00fcgte Ruh nach Kreuz und Plage,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "Ruh", "nach", "Kreuz", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier wird es Tag, hier wird es Nacht,", "tokens": ["Hier", "wird", "es", "Tag", ",", "hier", "wird", "es", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald singen wir: Es ist vollbracht!", "tokens": ["Bald", "sin\u00b7gen", "wir", ":", "Es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}