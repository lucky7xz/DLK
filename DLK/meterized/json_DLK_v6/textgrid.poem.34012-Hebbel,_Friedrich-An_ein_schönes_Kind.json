{"textgrid.poem.34012": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An ein sch\u00f6nes Kind", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blickst, um deiner Mutter Hals dich schmiegend,", "tokens": ["Du", "blickst", ",", "um", "dei\u00b7ner", "Mut\u00b7ter", "Hals", "dich", "schmie\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PPOSAT", "NN", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich hold und l\u00e4chelnd an, ein sel'ger Stummer;", "tokens": ["Mich", "hold", "und", "l\u00e4\u00b7chelnd", "an", ",", "ein", "sel'\u00b7ger", "Stum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wonne schlie\u00dft den Mund, ihn l\u00f6s't der Kummer,", "tokens": ["Die", "Won\u00b7ne", "schlie\u00dft", "den", "Mund", ",", "ihn", "l\u00f6s't", "der", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du brauchst die Sprache nicht, in Luft dich wiegend.", "tokens": ["Du", "brauchst", "die", "Spra\u00b7che", "nicht", ",", "in", "Luft", "dich", "wie\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch jetzt, der Kraft des Lenzes still erliegend,", "tokens": ["Doch", "jetzt", ",", "der", "Kraft", "des", "Len\u00b7zes", "still", "er\u00b7lie\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Bienen eingefurrt und and're Summer,", "tokens": ["Durch", "Bie\u00b7nen", "ein\u00b7ge\u00b7furrt", "und", "an\u00b7d'\u00b7re", "Sum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Duft bet\u00e4ubt, f\u00e4llst du in tiefen Schlummer,", "tokens": ["Von", "Duft", "be\u00b7t\u00e4ubt", ",", "f\u00e4llst", "du", "in", "tie\u00b7fen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Rosenblatt, in einen Brunnen fliegend.", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", ",", "in", "ei\u00b7nen", "Brun\u00b7nen", "flie\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O! w\u00fcrdest du der Maler und der Dichter", "tokens": ["O", "!", "w\u00fcr\u00b7dest", "du", "der", "Ma\u00b7ler", "und", "der", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewaltigster, du wirst durch all dein Ringen", "tokens": ["Ge\u00b7wal\u00b7tigs\u00b7ter", ",", "du", "wirst", "durch", "all", "dein", "Rin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das H\u00f6chste nie, wie jetzt im Spiel, verrathen,", "tokens": ["Das", "H\u00f6chs\u00b7te", "nie", ",", "wie", "jetzt", "im", "Spiel", ",", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PWAV", "ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nie so das Sch\u00f6ne durch der Farbe Lichter,", "tokens": ["Nie", "so", "das", "Sch\u00f6\u00b7ne", "durch", "der", "Far\u00b7be", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie so das Reine durch dein fr\u00f6mmstes Singen,", "tokens": ["Nie", "so", "das", "Rei\u00b7ne", "durch", "dein", "fr\u00f6mms\u00b7tes", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie so das Menschlich-G\u00f6ttliche durch Thaten!", "tokens": ["Nie", "so", "das", "Menschlich\u00b7G\u00f6tt\u00b7li\u00b7che", "durch", "Tha\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}}}}