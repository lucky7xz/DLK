{"dta.poem.3785": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die z\u00e4hre noch genuss", "tokens": ["Wo", "die", "z\u00e4h\u00b7re", "noch", "ge\u00b7nuss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVFIN", "ADV", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Starb im reif der sommerfalter \u00b7", "tokens": ["Starb", "im", "reif", "der", "som\u00b7mer\u00b7fal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJD", "ART", "NN", "XY"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem ein atem schon ein kuss?", "tokens": ["Dem", "ein", "a\u00b7tem", "schon", "ein", "kuss", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der auf gras und klee und garbe", "tokens": ["Der", "auf", "gras", "und", "klee", "und", "gar\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "KON", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und in reiche g\u00e4rten flog", "tokens": ["Und", "in", "rei\u00b7che", "g\u00e4r\u00b7ten", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen hauch von duft und farbe", "tokens": ["Ei\u00b7nen", "hauch", "von", "duft", "und", "far\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rasch aus allen bl\u00fcten sog", "tokens": ["Rasch", "aus", "al\u00b7len", "bl\u00fc\u00b7ten", "sog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem die nacht ein gut erteilte", "tokens": ["Dem", "die", "nacht", "ein", "gut", "er\u00b7teil\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ART", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das er tags umsonst ersp\u00e4ht", "tokens": ["Das", "er", "tags", "um\u00b7sonst", "er\u00b7sp\u00e4ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den sie mit der hoffnung heilte", "tokens": ["Den", "sie", "mit", "der", "hoff\u00b7nung", "heil\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass ihn doch die tulpe l\u00e4dt", "tokens": ["Dass", "ihn", "doch", "die", "tul\u00b7pe", "l\u00e4dt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommt er wieder mit der meisen", "tokens": ["Kommt", "er", "wie\u00b7der", "mit", "der", "mei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der lerchen erstem ton", "tokens": ["Mit", "der", "ler\u00b7chen", "ers\u00b7tem", "ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er neu den juni preisen", "tokens": ["Wird", "er", "neu", "den", "ju\u00b7ni", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4ft er oder starb er schon?", "tokens": ["Schl\u00e4ft", "er", "o\u00b7der", "starb", "er", "schon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}