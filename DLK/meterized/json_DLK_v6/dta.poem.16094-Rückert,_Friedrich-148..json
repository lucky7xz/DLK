{"dta.poem.16094": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "148.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie durch Gew\u00f6hnung lernt das Aug' im Dunkeln sehn,", "tokens": ["Wie", "durch", "Ge\u00b7w\u00f6h\u00b7nung", "lernt", "das", "Aug'", "im", "Dun\u00b7keln", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lernt man Dunkles, durch Vertiefung drein, verstehn.", "tokens": ["So", "lernt", "man", "Dunk\u00b7les", ",", "durch", "Ver\u00b7tie\u00b7fung", "drein", ",", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "$,", "APPR", "NN", "PTKVZ", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Geistes Augen gehn dir auf, und wunderbar,", "tokens": ["Des", "Geis\u00b7tes", "Au\u00b7gen", "gehn", "dir", "auf", ",", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was nie schien einzusehn, scheint dir nun v\u00f6llig klar.", "tokens": ["Was", "nie", "schien", "ein\u00b7zu\u00b7sehn", ",", "scheint", "dir", "nun", "v\u00f6l\u00b7lig", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "VVIZU