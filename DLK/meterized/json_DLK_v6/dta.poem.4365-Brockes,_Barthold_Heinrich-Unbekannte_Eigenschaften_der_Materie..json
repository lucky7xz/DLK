{"dta.poem.4365": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unbekannte Eigenschaften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir werden ja gestehen m\u00fcssen,", "tokens": ["Wir", "wer\u00b7den", "ja", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wir von allem in der Welt nichts, als nur", "tokens": ["Da\u00df", "wir", "von", "al\u00b7lem", "in", "der", "Welt", "nichts", ",", "als", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "APPR", "ART", "NN", "PIS", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "durch Erfahrung, wissen.", "tokens": ["durch", "Er\u00b7fah\u00b7rung", ",", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn des Gef\u00fchles Sinn uns fehlt\u2019; unm\u00f6glich h\u00e4tten", "tokens": ["Wenn", "des", "Ge\u00b7f\u00fch\u00b7les", "Sinn", "uns", "fehlt'", ";", "un\u00b7m\u00f6g\u00b7lich", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wir Jdeen", "tokens": ["wir", "Jdeen"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Von C\u00f6rpern, da\u00df sie ausgespannt. Wir w\u00fcrden nimmer-", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "da\u00df", "sie", "aus\u00b7ge\u00b7spannt", ".", "Wir", "w\u00fcr\u00b7den", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "KOUS", "PPER", "VVPP", "$.", "PPER", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mehr verstehen,", "tokens": ["mehr", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df die Materie beweglich, wenn wirs Bewegen nie ver-", "tokens": ["Da\u00df", "die", "Ma\u00b7te\u00b7rie", "be\u00b7weg\u00b7lich", ",", "wenn", "wirs", "Be\u00b7we\u00b7gen", "nie", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "sp\u00fchrt.", "tokens": ["sp\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ohn\u2019 unsre Augen k\u00f6nnten wir der Sonnen Licht unm\u00f6glich", "tokens": ["Ohn'", "uns\u00b7re", "Au\u00b7gen", "k\u00f6nn\u00b7ten", "wir", "der", "Son\u00b7nen", "Licht", "un\u00b7m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "sehen.", "tokens": ["se\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Durch eine kleine Zahl von Sinnen, die GOtt uns schenkt,", "tokens": ["Durch", "ei\u00b7ne", "klei\u00b7ne", "Zahl", "von", "Sin\u00b7nen", ",", "die", "Gott", "uns", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "wird man gef\u00fchrt", "tokens": ["wird", "man", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auch nur auf eine kleine Zahl von Eigenschaften, welche", "tokens": ["Auch", "nur", "auf", "ei\u00b7ne", "klei\u00b7ne", "Zahl", "von", "Ei\u00b7gen\u00b7schaf\u00b7ten", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "man", "tokens": ["man"], "token_info": ["word"], "pos": ["PIS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "In C\u00f6rpern hier entdecken kann.", "tokens": ["In", "C\u00f6r\u00b7pern", "hier", "ent\u00b7de\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unsere Vernunft ersetzet, was uns an Sinnen fehlt,", "tokens": ["Und", "un\u00b7se\u00b7re", "Ver\u00b7nunft", "er\u00b7set\u00b7zet", ",", "was", "uns", "an", "Sin\u00b7nen", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "und lehrt,", "tokens": ["und", "lehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da\u00df an dem nicht bekannten Stoff gewi\u00df noch andre Ei-", "tokens": ["Da\u00df", "an", "dem", "nicht", "be\u00b7kann\u00b7ten", "Stoff", "ge\u00b7wi\u00df", "noch", "and\u00b7re", "Ei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PRELS", "PTKNEG", "ADJA", "NN", "ADV", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "genschaften,", "tokens": ["gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Als wir daran erkennen, haften.", "tokens": ["Als", "wir", "da\u00b7ran", "er\u00b7ken\u00b7nen", ",", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von etwas mehrerm wird von ihr uns wenig oder nichts", "tokens": ["Von", "et\u00b7was", "meh\u00b7rerm", "wird", "von", "ihr", "uns", "we\u00b7nig", "o\u00b7der", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PIS", "VAFIN", "APPR", "PPER", "PRF", "PIS", "KON", "PIS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "erkl\u00e4rt.", "tokens": ["er\u00b7kl\u00e4rt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}