{"textgrid.poem.42313": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An die blinde Virtuosin Mademoiselle Paradies", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Schicksal werde nicht gescholten!", "tokens": ["Dein", "Schick\u00b7sal", "wer\u00b7de", "nicht", "ge\u00b7schol\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar raubts dir Ph\u00f6bus goldnen Strahl:", "tokens": ["Zwar", "raubts", "dir", "Ph\u00f6\u00b7bus", "gold\u00b7nen", "Strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat dir diesen tausendmal", "tokens": ["Doch", "hat", "dir", "die\u00b7sen", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein goldnes Saitenspiel vergolten.", "tokens": ["Sein", "gold\u00b7nes", "Sai\u00b7ten\u00b7spiel", "ver\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}