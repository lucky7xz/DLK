{"dta.poem.4219": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df zum Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An diesem abgelegnen Ort, entfernt vom st\u00e4dtischen", "tokens": ["An", "die\u00b7sem", "ab\u00b7ge\u00b7leg\u00b7nen", "Ort", ",", "ent\u00b7fernt", "vom", "st\u00e4d\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Get\u00fcmmel,", "tokens": ["Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Erweg\u2019 und ", "tokens": ["Er\u00b7weg'", "und"], "token_info": ["word", "word"], "pos": ["NN", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "sch\u00f6nen Bluhmen Heer;", "tokens": ["sch\u00f6\u00b7nen", "Bluh\u00b7men", "Heer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "meinem GOtt zur Ehr,", "tokens": ["mei\u00b7nem", "Gott", "zur", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der kleinen V\u00f6gel holdes Zwitschern; ich ", "tokens": ["Der", "klei\u00b7nen", "V\u00f6\u00b7gel", "hol\u00b7des", "Zwit\u00b7schern", ";", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "dem weiten Meer,", "tokens": ["dem", "wei\u00b7ten", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Erden Sch\u00e4tze, Wald und Feld, das Firmament,", "tokens": ["Der", "Er\u00b7den", "Sch\u00e4t\u00b7ze", ",", "Wald", "und", "Feld", ",", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "den Sternen-Himmel.", "tokens": ["den", "Ster\u00b7nen\u00b7Him\u00b7mel", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Hier sitz\u2019 ich, tadle niemand sonst, als mich. Ich rede", "tokens": ["Hier", "sitz'", "ich", ",", "tad\u00b7le", "nie\u00b7mand", "sonst", ",", "als", "mich", ".", "Ich", "re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "hier allein", "tokens": ["hier", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Mit meinen B\u00fcchern und mit mir. O wohl ein recht-", "tokens": ["Mit", "mei\u00b7nen", "B\u00fc\u00b7chern", "und", "mit", "mir", ".", "O", "wohl", "ein", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PPER", "$.", "NE", "ADV", "ART", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "und wahres Leben!", "tokens": ["und", "wah\u00b7res", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "O s\u00fc\u00df- und edler M\u00fc\u00dfiggang! Wer stimmt hierinn", "tokens": ["O", "s\u00fc\u00df", "und", "ed\u00b7ler", "M\u00fc\u00b7\u00dfig\u00b7gang", "!", "Wer", "stimmt", "hie\u00b7rinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "TRUNC", "KON", "ADJA", "NN", "$.", "PWS", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nicht \u00fcberein,", "tokens": ["nicht", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Da\u00df, f\u00fcr den wichtigsten Gesch\u00e4ften, dir noch der", "tokens": ["Da\u00df", ",", "f\u00fcr", "den", "wich\u00b7tigs\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7ten", ",", "dir", "noch", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "ART"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Vorzug fast zu geben?", "tokens": ["Vor\u00b7zug", "fast", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Ich f\u00fchl\u2019, in mir von GOtt geg\u00f6nntem nicht unem-", "tokens": ["Ich", "f\u00fchl'", ",", "in", "mir", "von", "Gott", "ge\u00b7g\u00f6nn\u00b7tem", "nicht", "un\u00b7em"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "APPR", "NN", "ADJA", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "pfundenem Vergn\u00fcgen,", "tokens": ["pfun\u00b7de\u00b7nem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.21": {"text": "Hier meine gegenw\u00e4rt\u2019ge Stunden sich sanft zu den", "tokens": ["Hier", "mei\u00b7ne", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7ge", "Stun\u00b7den", "sich", "sanft", "zu", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "vergangnen f\u00fcgen.", "tokens": ["ver\u00b7gang\u00b7nen", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Hier treib\u2019 ich, mit dem Flu\u00df der Tage, den stillen", "tokens": ["Hier", "treib'", "ich", ",", "mit", "dem", "Flu\u00df", "der", "Ta\u00b7ge", ",", "den", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Strohm der Zeit hinab,", "tokens": ["Strohm", "der", "Zeit", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Zum tiefen Meer der Ewigkeit, gelassen. Meinen", "tokens": ["Zum", "tie\u00b7fen", "Meer", "der", "E\u00b7wig\u00b7keit", ",", "ge\u00b7las\u00b7sen", ".", "Mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "VVPP", "$.", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "sanften Lauf", "tokens": ["sanf\u00b7ten", "Lauf"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Beschleunigt keine Leidenschaft, doch h\u00e4lt ihn ein ge-", "tokens": ["Be\u00b7schleu\u00b7nigt", "kei\u00b7ne", "Lei\u00b7den\u00b7schaft", ",", "doch", "h\u00e4lt", "ihn", "ein", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "f\u00fcrchtet Grab,", "tokens": ["f\u00fcrch\u00b7tet", "Grab", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Mit, ohnedem vergebnen, Sorgen und eitlen Schrecken,", "tokens": ["Mit", ",", "oh\u00b7ne\u00b7dem", "ver\u00b7geb\u00b7nen", ",", "Sor\u00b7gen", "und", "eit\u00b7len", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PAV", "VVINF", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "auch nicht auf.", "tokens": ["auch", "nicht", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Hiedurch, zumahl wenn mich zum \u00f6ftern des Sch\u00f6p-", "tokens": ["Hie\u00b7durch", ",", "zu\u00b7mahl", "wenn", "mich", "zum", "\u00f6f\u00b7tern", "des", "Sch\u00f6p"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "KOUS", "PPER", "APPRART", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "fers sch\u00f6n Gesch\u00f6pf ergetzt,", "tokens": ["fers", "sch\u00f6n", "Ge\u00b7sch\u00f6pf", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Verhoff\u2019 ich, in gela\u00dfner Demuht und Ehrerbietig-", "tokens": ["Ver\u00b7hoff'", "ich", ",", "in", "ge\u00b7la\u00df\u00b7ner", "De\u00b7muht", "und", "Ehr\u00b7er\u00b7bie\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "APPR", "ADJA", "NN", "KON", "TRUNC"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "keit, den Willen", "tokens": ["keit", ",", "den", "Wil\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Des grossen Wesens aller Wesen, Das mich in diese", "tokens": ["Des", "gros\u00b7sen", "We\u00b7sens", "al\u00b7ler", "We\u00b7sen", ",", "Das", "mich", "in", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,", "PDS", "PRF", "APPR", "PDAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Zeit gesetzt,", "tokens": ["Zeit", "ge\u00b7setzt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Wie, wo und wann es Jhm gefiel, am allerbesten zu", "tokens": ["Wie", ",", "wo", "und", "wann", "es", "Jhm", "ge\u00b7fiel", ",", "am", "al\u00b7ler\u00b7bes\u00b7ten", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "APPRART", "PIS", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.38": {"text": "erf\u00fcllen.", "tokens": ["er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}