{"textgrid.poem.65805": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "79", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich allein um deinen Beistand bat,", "tokens": ["Als", "ich", "al\u00b7lein", "um", "dei\u00b7nen", "Bei\u00b7stand", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War all dein Herz allein mit meinem Dichten:", "tokens": ["War", "all", "dein", "Herz", "al\u00b7lein", "mit", "mei\u00b7nem", "Dich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun aber welkt der Lieder holde Saat,", "tokens": ["Nun", "a\u00b7ber", "welkt", "der", "Lie\u00b7der", "hol\u00b7de", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und scheu vor andern mu\u00df die Muse fl\u00fcchten.", "tokens": ["Und", "scheu", "vor", "an\u00b7dern", "mu\u00df", "die", "Mu\u00b7se", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gesteh' ich's nur: Du, s\u00fc\u00dfer Gegenstand,", "tokens": ["Ge\u00b7steh'", "ich's", "nur", ":", "Du", ",", "s\u00fc\u00b7\u00dfer", "Ge\u00b7gen\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$.", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verdienest w\u00fcrdigerer Meister Streben.", "tokens": ["Ver\u00b7die\u00b7nest", "w\u00fcr\u00b7di\u00b7ge\u00b7rer", "Meis\u00b7ter", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch, was dein Dichter je von dir erfand,", "tokens": ["Doch", ",", "was", "dein", "Dich\u00b7ter", "je", "von", "dir", "er\u00b7fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nahm er von dir, um dir's zur\u00fcckzugeben.", "tokens": ["Nahm", "er", "von", "dir", ",", "um", "dir's", "zu\u00b7r\u00fcck\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "KOUI", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er leiht dir Tugend, und von deinem Wert", "tokens": ["Er", "leiht", "dir", "Tu\u00b7gend", ",", "und", "von", "dei\u00b7nem", "Wert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Stahl er dies Wort; r\u00fchmt deiner Sch\u00f6nheit Prangen,", "tokens": ["Stahl", "er", "dies", "Wort", ";", "r\u00fchmt", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "NN", "$.", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Das deine Wang' ihm bot: wie er dich ehrt,", "tokens": ["Das", "dei\u00b7ne", "Wang'", "ihm", "bot", ":", "wie", "er", "dich", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So war's in dir lebendig aufgegangen.", "tokens": ["So", "wa\u00b7r's", "in", "dir", "le\u00b7ben\u00b7dig", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Drum dank' ihm nicht f\u00fcr das, was er gemalt.", "tokens": ["Drum", "dank'", "ihm", "nicht", "f\u00fcr", "das", ",", "was", "er", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Was er dir schuldig, hast du selbst bezahlt.", "tokens": ["Was", "er", "dir", "schul\u00b7dig", ",", "hast", "du", "selbst", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}