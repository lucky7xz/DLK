{"textgrid.poem.24794": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00fcngere", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr gleicht nun v\u00f6llig dem vertriebnen Herzog,", "tokens": ["Ihr", "gleicht", "nun", "v\u00f6l\u00b7lig", "dem", "ver\u00b7trieb\u00b7nen", "Her\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der zaubern kann und eine Tochter hat:", "tokens": ["Der", "zau\u00b7bern", "kann", "und", "ei\u00b7ne", "Toch\u00b7ter", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "KON", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem im Theaterst\u00fcck, dem Prospero.", "tokens": ["Dem", "im", "The\u00b7a\u00b7ter\u00b7st\u00fcck", ",", "dem", "Pros\u00b7pe\u00b7ro", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn ihr seid stark genug, in dieser Stadt", "tokens": ["Denn", "ihr", "seid", "stark", "ge\u00b7nug", ",", "in", "die\u00b7ser", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADV", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit eurem Kind so frei dahinzuleben,", "tokens": ["Mit", "eu\u00b7rem", "Kind", "so", "frei", "da\u00b7hin\u00b7zu\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als w\u00e4ret ihr auf einer w\u00fcsten Insel.", "tokens": ["Als", "w\u00e4\u00b7ret", "ihr", "auf", "ei\u00b7ner", "w\u00fcs\u00b7ten", "In\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Ihr habt den Zaubermantel und die B\u00fccher,", "tokens": ["Ihr", "habt", "den", "Zau\u00b7ber\u00b7man\u00b7tel", "und", "die", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Geistern zur Bedienung und zur Lust,", "tokens": ["Mit", "Geis\u00b7tern", "zur", "Be\u00b7die\u00b7nung", "und", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Euch und die Tochter zu umgeben, nicht?", "tokens": ["Euch", "und", "die", "Toch\u00b7ter", "zu", "um\u00b7ge\u00b7ben", ",", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie kommen, wenn ihr winkt, und sie verlassen,", "tokens": ["Sie", "kom\u00b7men", ",", "wenn", "ihr", "winkt", ",", "und", "sie", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wenn ihr die Stirne runzelt. Dieses Kind", "tokens": ["Wenn", "ihr", "die", "Stir\u00b7ne", "run\u00b7zelt", ".", "Die\u00b7ses", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Lernt fr\u00fch, was wir erst sp\u00e4t begreifen lernten:", "tokens": ["Lernt", "fr\u00fch", ",", "was", "wir", "erst", "sp\u00e4t", "be\u00b7grei\u00b7fen", "lern\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df alles Lebende aus solchem Stoff", "tokens": ["Da\u00df", "al\u00b7les", "Le\u00b7ben\u00b7de", "aus", "sol\u00b7chem", "Stoff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.14": {"text": "Wie Tr\u00e4ume und ganz \u00e4hnlich auch zergeht.", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "und", "ganz", "\u00e4hn\u00b7lich", "auch", "zer\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sie w\u00e4chst so auf und f\u00fcrchtet sich vor nichts:", "tokens": ["Sie", "w\u00e4chst", "so", "auf", "und", "f\u00fcrch\u00b7tet", "sich", "vor", "nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mit Tieren und mit Toten redet sie", "tokens": ["Mit", "Tie\u00b7ren", "und", "mit", "To\u00b7ten", "re\u00b7det", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zutraulich wie mit ihresgleichen, bl\u00fcht", "tokens": ["Zu\u00b7trau\u00b7lich", "wie", "mit", "ih\u00b7res\u00b7glei\u00b7chen", ",", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Schamhafter als die festverschlo\u00dfne Knospe,", "tokens": ["Scham\u00b7haf\u00b7ter", "als", "die", "fest\u00b7ver\u00b7schlo\u00df\u00b7ne", "Knos\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Weil sie auch aus der leeren Luft so etwas", "tokens": ["Weil", "sie", "auch", "aus", "der", "lee\u00b7ren", "Luft", "so", "et\u00b7was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "ADV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wie Augen stets auf sich gerichtet f\u00fchlt.", "tokens": ["Wie", "Au\u00b7gen", "stets", "auf", "sich", "ge\u00b7rich\u00b7tet", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Allm\u00e4hlich wird sie gr\u00f6\u00dfer und ihr lehrt sie:", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "wird", "sie", "gr\u00f6\u00b7\u00dfer", "und", "ihr", "lehrt", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbhab du das Leben lieb, doch nicht zu lieb,", "tokens": ["\u00bb", "hab", "du", "das", "Le\u00b7ben", "lieb", ",", "doch", "nicht", "zu", "lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und nur um seiner selbst, doch immerfort", "tokens": ["Und", "nur", "um", "sei\u00b7ner", "selbst", ",", "doch", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Nur um des Guten willen, das darin ist.\u00ab", "tokens": ["Nur", "um", "des", "Gu\u00b7ten", "wil\u00b7len", ",", "das", "da\u00b7rin", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PAV", "VAFIN", "$.", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.25": {"text": "In all dem ist f\u00fcr sie kein Widerspruch,", "tokens": ["In", "all", "dem", "ist", "f\u00fcr", "sie", "kein", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDS", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Denn so wie bunte Muscheln oder V\u00f6gel", "tokens": ["Denn", "so", "wie", "bun\u00b7te", "Mu\u00b7scheln", "o\u00b7der", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Hat sie die Tugend lieb. Bis eines Tages", "tokens": ["Hat", "sie", "die", "Tu\u00b7gend", "lieb", ".", "Bis", "ei\u00b7nes", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ihr sie verm\u00e4hlt mit einem, den ihr v\u00f6llig", "tokens": ["Ihr", "sie", "ver\u00b7m\u00e4hlt", "mit", "ei\u00b7nem", ",", "den", "ihr", "v\u00f6l\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "APPR", "ART", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Durchschaut, den ihr gepr\u00fcft auf solche Art,", "tokens": ["Durch\u00b7schaut", ",", "den", "ihr", "ge\u00b7pr\u00fcft", "auf", "sol\u00b7che", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die kein unedler Mensch ertr\u00e4gt, als w\u00e4re er", "tokens": ["Die", "kein", "u\u00b7ned\u00b7ler", "Mensch", "er\u00b7tr\u00e4gt", ",", "als", "w\u00e4\u00b7re", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Schiffbr\u00fcchig ausgeworfen auf der Insel,", "tokens": ["Schiff\u00b7br\u00fc\u00b7chig", "aus\u00b7ge\u00b7wor\u00b7fen", "auf", "der", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.32": {"text": "Die ihr beherrscht und ganz euch zugefallen", "tokens": ["Die", "ihr", "be\u00b7herrscht", "und", "ganz", "euch", "zu\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wie Strandgut.", "tokens": ["Wie", "Strand\u00b7gut", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}