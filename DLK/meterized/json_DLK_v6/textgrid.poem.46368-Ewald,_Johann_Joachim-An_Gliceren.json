{"textgrid.poem.46368": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An Gliceren", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glycere komm auf gr\u00fcne Fluren!", "tokens": ["Gly\u00b7ce\u00b7re", "komm", "auf", "gr\u00fc\u00b7ne", "Flu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sind des goldnen Alters Spuren;", "tokens": ["Da", "sind", "des", "gold\u00b7nen", "Al\u00b7ters", "Spu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da herscht die Liebe fern vom Neid,", "tokens": ["Da", "herscht", "die", "Lie\u00b7be", "fern", "vom", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da macht das Herz den Unterscheid.", "tokens": ["Da", "macht", "das", "Herz", "den", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh wie die Zweige lieblich winken,", "tokens": ["Sieh", "wie", "die", "Zwei\u00b7ge", "lieb\u00b7lich", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wodurch itzt tr\u00e4ge B\u00e4che blinken!", "tokens": ["Wo\u00b7durch", "itzt", "tr\u00e4\u00b7ge", "B\u00e4\u00b7che", "blin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Komm, komm in jene Rosenhecken", "tokens": ["Komm", ",", "komm", "in", "je\u00b7ne", "Ro\u00b7sen\u00b7he\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich f\u00fcr die Mutter zu verstecken!", "tokens": ["Dich", "f\u00fcr", "die", "Mut\u00b7ter", "zu", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Amors Blume bl\u00fcht nur dir,", "tokens": ["Des", "A\u00b7mors", "Blu\u00b7me", "bl\u00fcht", "nur", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dort singen wir, dort k\u00fcssen wir;", "tokens": ["Dort", "sin\u00b7gen", "wir", ",", "dort", "k\u00fcs\u00b7sen", "wir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wir wollen einsam dort in Gr\u00fcnden", "tokens": ["Wir", "wol\u00b7len", "ein\u00b7sam", "dort", "in", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Himmel zwischen Rosen finden.", "tokens": ["Den", "Him\u00b7mel", "zwi\u00b7schen", "Ro\u00b7sen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}