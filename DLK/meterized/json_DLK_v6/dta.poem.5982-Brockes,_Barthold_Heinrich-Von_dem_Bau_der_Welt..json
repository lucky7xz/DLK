{"dta.poem.5982": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf gleiche Weis entstehen \u00f6ffters Flecken,", "tokens": ["Auf", "glei\u00b7che", "Weis", "ent\u00b7ste\u00b7hen", "\u00f6ff\u00b7ters", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wir so gar im Glantz der Sonnen selbst entdecken.", "tokens": ["Die", "wir", "so", "gar", "im", "Glantz", "der", "Son\u00b7nen", "selbst", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das was bey ihr am meisten klar und rein,", "tokens": ["Das", "was", "bey", "ihr", "am", "meis\u00b7ten", "klar", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "APPR", "PPOSAT", "PTKA", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kan, wie ein Schaum vereint, und so entstanden seyn,", "tokens": ["Kan", ",", "wie", "ein", "Schaum", "ver\u00b7eint", ",", "und", "so", "ent\u00b7stan\u00b7den", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$,", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}