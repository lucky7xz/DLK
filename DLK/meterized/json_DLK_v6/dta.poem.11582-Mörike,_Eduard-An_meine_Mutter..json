{"dta.poem.11582": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An meine Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe, von alle den Liedern nicht Eines gilt dir, o Mutter!", "tokens": ["Sie\u00b7he", ",", "von", "al\u00b7le", "den", "Lie\u00b7dern", "nicht", "Ei\u00b7nes", "gilt", "dir", ",", "o", "Mut\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "APPR", "PIS", "ART", "NN", "PTKNEG", "PIS", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dich zu preisen, o glaub's, bin ich zu arm und zu", "tokens": ["Dich", "zu", "prei\u00b7sen", ",", "o", "glaub's", ",", "bin", "ich", "zu", "arm", "und", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "FM", "NE", "$,", "VAFIN", "PPER", "PTKA", "ADJD", "KON", "APPR"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ein noch ungesungenes Lied ruhst du mir im Busen,", "tokens": ["Ein", "noch", "un\u00b7ge\u00b7sun\u00b7ge\u00b7nes", "Lied", "ruhst", "du", "mir", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Keinem vernehmbar sonst, mich nur zu tr\u00f6sten bestimmt,", "tokens": ["Kei\u00b7nem", "ver\u00b7nehm\u00b7bar", "sonst", ",", "mich", "nur", "zu", "tr\u00f6s\u00b7ten", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wenn sich das Herz unmuthig der Welt abwendet und", "tokens": ["Wenn", "sich", "das", "Herz", "un\u00b7mut\u00b7hig", "der", "Welt", "ab\u00b7wen\u00b7det", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "KON"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Seines himmlischen Theils bleibenden Frieden bedenkt.", "tokens": ["Sei\u00b7nes", "himm\u00b7li\u00b7schen", "Theils", "blei\u00b7ben\u00b7den", "Frie\u00b7den", "be\u00b7denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}