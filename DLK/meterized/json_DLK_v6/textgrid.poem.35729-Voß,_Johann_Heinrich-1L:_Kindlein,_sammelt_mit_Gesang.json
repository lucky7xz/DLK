{"textgrid.poem.35729": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kindlein, sammelt mit Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kindlein, sammelt mit Gesang", "tokens": ["Kin\u00b7dlein", ",", "sam\u00b7melt", "mit", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Kartoffeln \u00dcberschwang!", "tokens": ["Der", "Kar\u00b7tof\u00b7feln", "\u00dc\u00b7bersc\u00b7hwang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ob wir voll bis oben sch\u00fctten", "tokens": ["Ob", "wir", "voll", "bis", "o\u00b7ben", "sch\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Mulden, K\u00f6rb' und B\u00fctten;", "tokens": ["Al\u00b7le", "Mul\u00b7den", ",", "K\u00f6rb'", "und", "B\u00fct\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch ist immer kein Vergang!", "tokens": ["Noch", "ist", "im\u00b7mer", "kein", "Ver\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo man nur den Bulten hebt,", "tokens": ["Wo", "man", "nur", "den", "Bul\u00b7ten", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut, wie voll es lebt und webt!", "tokens": ["Schaut", ",", "wie", "voll", "es", "lebt", "und", "webt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O die sch\u00f6ngekerbten Knollen,", "tokens": ["O", "die", "sch\u00f6n\u00b7ge\u00b7kerb\u00b7ten", "Knol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df und rot, und dick geschwollen!", "tokens": ["Wei\u00df", "und", "rot", ",", "und", "dick", "ge\u00b7schwol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer mehr, je mehr man gr\u00e4bt!", "tokens": ["Im\u00b7mer", "mehr", ",", "je", "mehr", "man", "gr\u00e4bt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht umsonst in bunter Schau", "tokens": ["Nicht", "um\u00b7sonst", "in", "bun\u00b7ter", "Schau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcht' es r\u00f6tlich, wei\u00df und blau!", "tokens": ["Bl\u00fcht'", "es", "r\u00f6t\u00b7lich", ",", "wei\u00df", "und", "blau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward gej\u00e4tet, ward geh\u00e4ufet:", "tokens": ["Ward", "ge\u00b7j\u00e4\u00b7tet", ",", "ward", "ge\u00b7h\u00e4u\u00b7fet", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kindlein, Gottes Segen reifet!", "tokens": ["Kin\u00b7dlein", ",", "Got\u00b7tes", "Se\u00b7gen", "rei\u00b7fet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rief ich oft, und traf's genau!", "tokens": ["Rief", "ich", "oft", ",", "und", "traf's", "ge\u00b7nau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einst vom Himmel schaute Gott", "tokens": ["Einst", "vom", "Him\u00b7mel", "schau\u00b7te", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Armen bittre Not:", "tokens": ["Auf", "der", "Ar\u00b7men", "bitt\u00b7re", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahe ging's ihm; und was that er", "tokens": ["Na\u00b7he", "ging's", "ihm", ";", "und", "was", "that", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "KON", "PWS", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zum Trost, der gute Vater?", "tokens": ["Uns", "zum", "Trost", ",", "der", "gu\u00b7te", "Va\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Regnet' er uns Mannabrot?", "tokens": ["Reg\u00b7net'", "er", "uns", "Man\u00b7na\u00b7brot", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NE", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Nein, ein Mann ward ausgesandt,", "tokens": ["Nein", ",", "ein", "Mann", "ward", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die neue Welt erfand!", "tokens": ["Der", "die", "neu\u00b7e", "Welt", "er\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiche nennen's Land des Goldes:", "tokens": ["Rei\u00b7che", "nen\u00b7nen's", "Land", "des", "Gol\u00b7des", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Doch der Arme nennt's sein holdes", "tokens": ["Doch", "der", "Ar\u00b7me", "nennt's", "sein", "hol\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4hrendes Kartoffelland!", "tokens": ["N\u00e4h\u00b7ren\u00b7des", "Kar\u00b7tof\u00b7fel\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Nur ein Kn\u00f6llchen eingesteckt,", "tokens": ["Nur", "ein", "Kn\u00f6ll\u00b7chen", "ein\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Erde zugedeckt!", "tokens": ["Und", "mit", "Er\u00b7de", "zu\u00b7ge\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unten treibt dann Gott sein Wesen!", "tokens": ["Un\u00b7ten", "treibt", "dann", "Gott", "sein", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum sind H\u00e4nde gnug zum Lesen,", "tokens": ["Kaum", "sind", "H\u00e4n\u00b7de", "gnug", "zum", "Le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie es unten w\u00fchlt und heckt!", "tokens": ["Wie", "es", "un\u00b7ten", "w\u00fchlt", "und", "heckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Was ist nun f\u00fcr Sorge noch?", "tokens": ["Was", "ist", "nun", "f\u00fcr", "Sor\u00b7ge", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klar im irdnen Napf und hoch,", "tokens": ["Klar", "im", "ird\u00b7nen", "Napf", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dampft Kartoffelschmaus f\u00fcr alle!", "tokens": ["Dampft", "Kar\u00b7tof\u00b7fel\u00b7schmaus", "f\u00fcr", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Milchkuh auch im Stalle", "tokens": ["Uns\u00b7re", "Milch\u00b7kuh", "auch", "im", "Stal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmt ihr Teil, und brummt am Trog!", "tokens": ["Nimmt", "ihr", "Teil", ",", "und", "brummt", "am", "Trog", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber, Kindlein, h\u00f6rt! ihr sollt", "tokens": ["A\u00b7ber", ",", "Kin\u00b7dlein", ",", "h\u00f6rt", "!", "ihr", "sollt"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht verschm\u00e4hn das liebe Gold!", "tokens": ["Nicht", "ver\u00b7schm\u00e4hn", "das", "lie\u00b7be", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr Gold, ihr k\u00f6nnt bei Haufen", "tokens": ["Habt", "ihr", "Gold", ",", "ihr", "k\u00f6nnt", "bei", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Saatkartoffeln kaufen,", "tokens": ["Sch\u00f6\u00b7ne", "Saat\u00b7kar\u00b7tof\u00b7feln", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Grad' aus Holland, wenn ihr wollt!", "tokens": ["Grad'", "aus", "Hol\u00b7land", ",", "wenn", "ihr", "wollt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}