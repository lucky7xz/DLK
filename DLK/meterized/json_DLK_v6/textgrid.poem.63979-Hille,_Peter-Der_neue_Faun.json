{"textgrid.poem.63979": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Faun", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lau leuchtet die Gr\u00f6\u00dfe des Himmels hernieder,", "tokens": ["Lau", "leuch\u00b7tet", "die", "Gr\u00f6\u00b7\u00dfe", "des", "Him\u00b7mels", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "++--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In weichem Lichte gl\u00e4nzen die Glieder.", "tokens": ["In", "wei\u00b7chem", "Lich\u00b7te", "gl\u00e4n\u00b7zen", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur ist es verboten sie anzuschauen:", "tokens": ["Nur", "ist", "es", "ver\u00b7bo\u00b7ten", "sie", "an\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit m\u00e4nnlichem Auge die badenden Frauen.", "tokens": ["Mit", "m\u00e4nn\u00b7li\u00b7chem", "Au\u00b7ge", "die", "ba\u00b7den\u00b7den", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Kein knisterndes Schilf, kein kicherndes Lachen,", "tokens": ["Kein", "knis\u00b7tern\u00b7des", "Schilf", ",", "kein", "ki\u00b7chern\u00b7des", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Augen der Badew\u00e4rter wachen.", "tokens": ["Die", "Au\u00b7gen", "der", "Ba\u00b7de\u00b7w\u00e4r\u00b7ter", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da hab' ich nun mein Fernglas genommen", "tokens": ["Da", "hab'", "ich", "nun", "mein", "Fern\u00b7glas", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+--+++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und habe von ferne die D\u00fcnen erklommen.", "tokens": ["Und", "ha\u00b7be", "von", "fer\u00b7ne", "die", "D\u00fc\u00b7nen", "er\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wie Kandidaten der Venus mit wallenden", "tokens": ["Wie", "Kan\u00b7di\u00b7da\u00b7ten", "der", "Ve\u00b7nus", "mit", "wal\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00e4nteln kommen an sie gegangen:", "tokens": ["M\u00e4n\u00b7teln", "kom\u00b7men", "an", "sie", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein Musenchor mit glatten abfallenden", "tokens": ["Ein", "Mu\u00b7sen\u00b7chor", "mit", "glat\u00b7ten", "ab\u00b7fal\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wei\u00dfscheinenden M\u00e4nteln kommt es gegangen.", "tokens": ["Wei\u00df\u00b7schei\u00b7nen\u00b7den", "M\u00e4n\u00b7teln", "kommt", "es", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Musenchor: wie gro\u00dfe V\u00f6gel flatternd fallenden.", "tokens": ["Ein", "Mu\u00b7sen\u00b7chor", ":", "wie", "gro\u00b7\u00dfe", "V\u00f6\u00b7gel", "flat\u00b7ternd", "fal\u00b7len\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}