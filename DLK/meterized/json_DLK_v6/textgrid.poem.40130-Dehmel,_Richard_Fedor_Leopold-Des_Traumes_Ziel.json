{"textgrid.poem.40130": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Des Traumes Ziel", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4ume oft von einer bleichen Rose.", "tokens": ["Ich", "tr\u00e4u\u00b7me", "oft", "von", "ei\u00b7ner", "blei\u00b7chen", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie bl\u00fcht in eines hohen Berges Schatten,", "tokens": ["Sie", "bl\u00fcht", "in", "ei\u00b7nes", "ho\u00b7hen", "Ber\u00b7ges", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zum Lichte fleht sie mit dem schmachtend matten", "tokens": ["zum", "Lich\u00b7te", "fleht", "sie", "mit", "dem", "schmach\u00b7tend", "mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dem Blumenblick aus ihrem dunklen Loose.", "tokens": ["dem", "Blu\u00b7men\u00b7blick", "aus", "ih\u00b7rem", "dunk\u00b7len", "Loo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mich ergreift's, da\u00df tr\u00f6stend ich sie kose.", "tokens": ["Und", "mich", "er\u00b7greift's", ",", "da\u00df", "tr\u00f6s\u00b7tend", "ich", "sie", "ko\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch weiter mu\u00df auf meinem Pfad ich schreiten:", "tokens": ["Doch", "wei\u00b7ter", "mu\u00df", "auf", "mei\u00b7nem", "Pfad", "ich", "schrei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zum Gipfel, wo die Lorbeern stolz sich breiten!", "tokens": ["zum", "Gip\u00b7fel", ",", "wo", "die", "Lor\u00b7beern", "stolz", "sich", "brei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So steh' ich zaudernd zwischen Berg und Rose.", "tokens": ["So", "steh'", "ich", "zau\u00b7dernd", "zwi\u00b7schen", "Berg", "und", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn wie ich auch den Fu\u00df mag ", "tokens": ["Denn", "wie", "ich", "auch", "den", "Fu\u00df", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ich kann der tiefen Sehnsucht nicht mehr ", "tokens": ["ich", "kann", "der", "tie\u00b7fen", "Sehn\u00b7sucht", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "aus ihrem Schoo\u00df den s\u00fc\u00dfen Duft zu schl\u00fcrfen.", "tokens": ["aus", "ih\u00b7rem", "Schoo\u00df", "den", "s\u00fc\u00b7\u00dfen", "Duft", "zu", "schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da: bin ein ", "tokens": ["Da", ":", "bin", "ein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "auf schweb' ich ", "tokens": ["auf", "schweb'", "ich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "wo auch die Rosen immergr\u00fcnen d\u00fcrfen!", "tokens": ["wo", "auch", "die", "Ro\u00b7sen", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "d\u00fcr\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}