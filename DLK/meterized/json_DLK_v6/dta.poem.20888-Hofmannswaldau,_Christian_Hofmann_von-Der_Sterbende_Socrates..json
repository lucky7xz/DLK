{"dta.poem.20888": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ist die Best\u00e4ndigkeit gestorben?", "tokens": ["Ist", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4fft mir dann heute Krafft und Muth?", "tokens": ["Schl\u00e4fft", "mir", "dann", "heu\u00b7te", "Krafft", "und", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hertze r\u00fchre deine Glut/", "tokens": ["Mein", "Hert\u00b7ze", "r\u00fch\u00b7re", "dei\u00b7ne", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeige dich nicht gantz verdorben;", "tokens": ["Und", "zei\u00b7ge", "dich", "nicht", "gantz", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie da\u00df dir di\u00df so bange thut/", "tokens": ["Wie", "da\u00df", "dir", "di\u00df", "so", "ban\u00b7ge", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PDS", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit du vormals Ruhm erworben?", "tokens": ["Da\u00b7mit", "du", "vor\u00b7mals", "Ruhm", "er\u00b7wor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}