{"textgrid.poem.53483": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Blasbalg", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da merkt' ich tief betroffen:", "tokens": ["Da", "merkt'", "ich", "tief", "be\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer friedlich nur sich selbst bezweckt,", "tokens": ["Wer", "fried\u00b7lich", "nur", "sich", "selbst", "be\u00b7zweckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht sich bei aller Welt suspekt.", "tokens": ["Macht", "sich", "bei", "al\u00b7ler", "Welt", "sus\u00b7pekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Und bin nach Haus geloffen.", "tokens": ["\u2013", "Und", "bin", "nach", "Haus", "ge\u00b7lof\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Konkreta und Abstrakta", "tokens": ["Kon\u00b7kre\u00b7ta", "und", "A\u00b7bstrak\u00b7ta"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zaust mir der Alte aus dem Leib.", "tokens": ["Zaust", "mir", "der", "Al\u00b7te", "aus", "dem", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hat er satt den Zeitvertreib,", "tokens": ["Und", "hat", "er", "satt", "den", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann legt er mich ad acta.", "tokens": ["Dann", "legt", "er", "mich", "ad", "ac\u00b7ta", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "FM", "FM", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Fliegst du mal so zehn Minuten", "tokens": ["Fliegst", "du", "mal", "so", "zehn", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quasi ungebunden,", "tokens": ["Qua\u00b7si", "un\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mu\u00dft du nachher kleben, bluten", "tokens": ["Mu\u00dft", "du", "nach\u00b7her", "kle\u00b7ben", ",", "blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreiundzwanzig Stunden!", "tokens": ["Drei\u00b7und\u00b7zwan\u00b7zig", "Stun\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Das Jetzt zerflie\u00dft wie Flocken auf der Hand", "tokens": ["Das", "Jetzt", "zer\u00b7flie\u00dft", "wie", "Flo\u00b7cken", "auf", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und sickert in den nimmersatten Sand.", "tokens": ["und", "si\u00b7ckert", "in", "den", "nim\u00b7mer\u00b7sat\u00b7ten", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dein H\u00e4mmerlein f\u00fcgt zierlich Glied an Glied,", "tokens": ["Dein", "H\u00e4m\u00b7mer\u00b7lein", "f\u00fcgt", "zier\u00b7lich", "Glied", "an", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erinnerung, du Gold- und Silberschmied.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", ",", "du", "Gold", "und", "Sil\u00b7ber\u00b7schmied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und hab' ich denn nicht Haus noch Rast", "tokens": ["Und", "hab'", "ich", "denn", "nicht", "Haus", "noch", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Ruh des Herzens,", "tokens": ["Noch", "Ruh", "des", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So leg ich mich nieder am Waldesrand,", "tokens": ["So", "leg", "ich", "mich", "nie\u00b7der", "am", "Wal\u00b7des\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "PRF", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So streck' ich mich aus im Ufersand.", "tokens": ["So", "streck'", "ich", "mich", "aus", "im", "U\u00b7fer\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihr habt euch still und heimlich weggemacht.", "tokens": ["Ihr", "habt", "euch", "still", "und", "heim\u00b7lich", "weg\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun gl\u00e4nzt um euer Gl\u00fcck die helle Nacht", "tokens": ["Nun", "gl\u00e4nzt", "um", "eu\u00b7er", "Gl\u00fcck", "die", "hel\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und s\u00fc\u00dfe Ruh.", "tokens": ["und", "s\u00fc\u00b7\u00dfe", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Flu\u00df rauscht fort und fort in eurem Traum.", "tokens": ["Der", "Flu\u00df", "rauscht", "fort", "und", "fort", "in", "eu\u00b7rem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Leis f\u00e4llt ins Gras die reife Frucht vom Baum", "tokens": ["Leis", "f\u00e4llt", "ins", "Gras", "die", "rei\u00b7fe", "Frucht", "vom", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und rollt euch zu.", "tokens": ["und", "rollt", "euch", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}