{"textgrid.poem.58548": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "586. Die wahre Gottseligkeit oder Gottesdienstlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott f\u00fcrchtet, glaubt und liebet,", "tokens": ["Wer", "Gott", "f\u00fcrch\u00b7tet", ",", "glaubt", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm recht dient und Ehre giebet:", "tokens": ["Ihm", "recht", "dient", "und", "Eh\u00b7re", "gie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man schl\u00e4gt vor Reu und Leid auf seine Brust und klagt,", "tokens": ["Man", "schl\u00e4gt", "vor", "Reu", "und", "Leid", "auf", "sei\u00b7ne", "Brust", "und", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man meid't, was b\u00f6se ist, und sich zum Guten schicket", "tokens": ["Man", "meid't", ",", "was", "b\u00f6\u00b7se", "ist", ",", "und", "sich", "zum", "Gu\u00b7ten", "schi\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KON", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und nach der Reinigung von toten Werken tracht't.", "tokens": ["Und", "nach", "der", "Rei\u00b7ni\u00b7gung", "von", "to\u00b7ten", "Wer\u00b7ken", "tracht'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Besprengt mit Jesu Blut wie jener Rauchaltar,", "tokens": ["Be\u00b7sprengt", "mit", "Je\u00b7su", "Blut", "wie", "je\u00b7ner", "Rauch\u00b7al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da Jesus selbst als Brot ihm Geist und Leben reichet,", "tokens": ["Da", "Je\u00b7sus", "selbst", "als", "Brot", "ihm", "Geist", "und", "Le\u00b7ben", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "KOUS", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und das erleuchtend' Licht macht alle Wahrheit klar.", "tokens": ["Und", "das", "er\u00b7leuch\u00b7tend'", "Licht", "macht", "al\u00b7le", "Wahr\u00b7heit", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "NN", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die reine ", "tokens": ["Die", "rei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Da tief im Dunkeln Gott im stillen Geist will ruhn,", "tokens": ["Da", "tief", "im", "Dun\u00b7keln", "Gott", "im", "stil\u00b7len", "Geist", "will", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der so vereinigt, kann die Gottheit selbst anbeten,", "tokens": ["Der", "so", "ver\u00b7ei\u00b7nigt", ",", "kann", "die", "Got\u00b7theit", "selbst", "an\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Umfassen und beschaun wie Cherubinen tun.", "tokens": ["Um\u00b7fas\u00b7sen", "und", "be\u00b7schaun", "wie", "Che\u00b7ru\u00b7bi\u00b7nen", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dies sei dein Gottesdienst! In diesen ", "tokens": ["Dies", "sei", "dein", "Got\u00b7tes\u00b7dienst", "!", "In", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wird Vater, Sohn und Geist verehret recht und rein,", "tokens": ["Wird", "Va\u00b7ter", ",", "Sohn", "und", "Geist", "ver\u00b7eh\u00b7ret", "recht", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sonst hilft kein Heuchelschein. Wer dieses kann verrichten,", "tokens": ["Sonst", "hilft", "kein", "Heu\u00b7chel\u00b7schein", ".", "Wer", "die\u00b7ses", "kann", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$.", "PWS", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der mag gottselig und \u2013 in Gott recht selig sein.", "tokens": ["Der", "mag", "gott\u00b7se\u00b7lig", "und", "\u2013", "in", "Gott", "recht", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "KON", "$(", "APPR", "NN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}