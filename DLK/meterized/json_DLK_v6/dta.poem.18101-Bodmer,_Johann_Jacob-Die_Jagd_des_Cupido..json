{"dta.poem.18101": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagd des Cupido.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der sch\u00f6nesten der Zeiten,", "tokens": ["In", "der", "sch\u00f6\u00b7nes\u00b7ten", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wann verj\u00fcngt wird alle Welt,", "tokens": ["Wann", "ver\u00b7j\u00fcngt", "wird", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann die Flora Blumen spreiten", "tokens": ["Wann", "die", "Flo\u00b7ra", "Blu\u00b7men", "sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut, durch Wiesen, Wald und Feld,", "tokens": ["Thut", ",", "durch", "Wie\u00b7sen", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kam der Venus Sohn gegangen,", "tokens": ["Kam", "der", "Ve\u00b7nus", "Sohn", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh sich Lucifer er\u00e4ugt,", "tokens": ["Eh", "sich", "Lu\u00b7ci\u00b7fer", "er\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eh Aurora ihre Wangen", "tokens": ["Eh", "Au\u00b7ro\u00b7ra", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und goldgelbes Haare zeigt.", "tokens": ["Und", "gold\u00b7gel\u00b7bes", "Haa\u00b7re", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Venus lag ohn Sorg und Zagen", "tokens": ["Ve\u00b7nus", "lag", "ohn", "Sorg", "und", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gantz des sanften Schlafes voll,", "tokens": ["Gantz", "des", "sanf\u00b7ten", "Schla\u00b7fes", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Mutter, sagt er, ich geh jagen,", "tokens": ["Mut\u00b7ter", ",", "sagt", "er", ",", "ich", "geh", "ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Unterde\u00df gehabt euch wohl.", "tokens": ["Un\u00b7ter\u00b7de\u00df", "ge\u00b7habt", "euch", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da erwachte die G\u00f6ttinne,", "tokens": ["Da", "er\u00b7wach\u00b7te", "die", "G\u00f6t\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Sprach: Cupido liebes Kind,", "tokens": ["Sprach", ":", "Cu\u00b7pi\u00b7do", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Weil du dieses hast im Sinne,", "tokens": ["Weil", "du", "die\u00b7ses", "hast", "im", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sey es gerne dir verg\u00fcnt,", "tokens": ["Sey", "es", "ger\u00b7ne", "dir", "ver\u00b7g\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Und ich w\u00fcnsche da\u00df dein Bogen", "tokens": ["Und", "ich", "w\u00fcn\u00b7sche", "da\u00df", "dein", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Richtig schiesse f\u00fcr und f\u00fcr.", "tokens": ["Rich\u00b7tig", "schies\u00b7se", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wann du dann di\u00df Werck vollzogen,", "tokens": ["Wann", "du", "dann", "di\u00df", "Werck", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Komm auch wieder her zu mir.", "tokens": ["Komm", "auch", "wie\u00b7der", "her", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Di\u00df derhalben zu vollf\u00fchren,", "tokens": ["Di\u00df", "der\u00b7hal\u00b7ben", "zu", "voll\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "War er bald zur Jagd bereit,", "tokens": ["War", "er", "bald", "zur", "Jagd", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Nicht zur Jagd nach wilden Thieren,", "tokens": ["Nicht", "zur", "Jagd", "nach", "wil\u00b7den", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie Adonis vor der Zeit,", "tokens": ["Wie", "A\u00b7do\u00b7nis", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Sondern da\u00df er m\u00f6chte zwingen,", "tokens": ["Son\u00b7dern", "da\u00df", "er", "m\u00f6ch\u00b7te", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Diese grosse weite Welt,", "tokens": ["Die\u00b7se", "gros\u00b7se", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und in seine Netze bringen,", "tokens": ["Und", "in", "sei\u00b7ne", "Net\u00b7ze", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Was der Himmel in sich h\u00e4lt.", "tokens": ["Was", "der", "Him\u00b7mel", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Als der Zephyrus vernommen,", "tokens": ["Als", "der", "Ze\u00b7phy\u00b7rus", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Was das Kind gesonnen w\u00e4r,", "tokens": ["Was", "das", "Kind", "ge\u00b7son\u00b7nen", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Ist er mit der Aura kommen,", "tokens": ["Ist", "er", "mit", "der", "Au\u00b7ra", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Zu verk\u00fcnden diese M\u00e4hr.", "tokens": ["Zu", "ver\u00b7k\u00fcn\u00b7den", "die\u00b7se", "M\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Doch th\u00e4t er sich pl\u00f6tzlich n\u00e4hen,", "tokens": ["Doch", "th\u00e4t", "er", "sich", "pl\u00f6tz\u00b7lich", "n\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Eh man f\u00fcr ihm fliehen kunnt:", "tokens": ["Eh", "man", "f\u00fcr", "ihm", "flie\u00b7hen", "kunnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Eh man seiner sich versehen,", "tokens": ["Eh", "man", "sei\u00b7ner", "sich", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Hatt er schon sehr viel verwundt.", "tokens": ["Hatt", "er", "schon", "sehr", "viel", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Also wird sehr offt betrogen", "tokens": ["Al\u00b7so", "wird", "sehr", "offt", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Die gelehrte Nachtigall,", "tokens": ["Die", "ge\u00b7lehr\u00b7te", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Eh sie kaum hinzu geflogen,", "tokens": ["Eh", "sie", "kaum", "hin\u00b7zu", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Ist sie kommen schon zu Fall,", "tokens": ["Ist", "sie", "kom\u00b7men", "schon", "zu", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Jupiter, der Donnerkeile", "tokens": ["Ju\u00b7pi\u00b7ter", ",", "der", "Don\u00b7ner\u00b7kei\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.42": {"text": "Nur f\u00fcr Spiel und Schertze h\u00e4lt,", "tokens": ["Nur", "f\u00fcr", "Spiel", "und", "Schert\u00b7ze", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Ward durch dieses Kindes Pfeile", "tokens": ["Ward", "durch", "die\u00b7ses", "Kin\u00b7des", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "In der Buhler Zahl gestellt.", "tokens": ["In", "der", "Buh\u00b7ler", "Zahl", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ph\u00f6bus hatte Kunst und Witzen,", "tokens": ["Ph\u00f6\u00b7bus", "hat\u00b7te", "Kunst", "und", "Wit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Plutus war an Golde reich,", "tokens": ["Plu\u00b7tus", "war", "an", "Gol\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Es konnt ihnen doch nicht n\u00fctzen,", "tokens": ["Es", "konnt", "ih\u00b7nen", "doch", "nicht", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Es war Amor alles gleich.", "tokens": ["Es", "war", "A\u00b7mor", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Mars der sonst sich auszur\u00fcsten", "tokens": ["Mars", "der", "sonst", "sich", "aus\u00b7zu\u00b7r\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Und zu streiten war bedacht,", "tokens": ["Und", "zu", "strei\u00b7ten", "war", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Sauget an der Venus Br\u00fcsten,", "tokens": ["Sau\u00b7get", "an", "der", "Ve\u00b7nus", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und verga\u00df der Krieges-Macht.", "tokens": ["Und", "ver\u00b7ga\u00df", "der", "Krie\u00b7ge\u00b7sMacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Bacchus wu\u00dfte nichts von Trauben,", "tokens": ["Bac\u00b7chus", "wu\u00df\u00b7te", "nichts", "von", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Gantz entz\u00fcndt in s\u00fcsser Pein,", "tokens": ["Gantz", "ent\u00b7z\u00fcndt", "in", "s\u00fcs\u00b7ser", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Mu\u00dfte Liebes-Speise klauben,", "tokens": ["Mu\u00df\u00b7te", "Lie\u00b7bes\u00b7Spei\u00b7se", "klau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Thr\u00e4nen giessen vor den Wein.", "tokens": ["Thr\u00e4\u00b7nen", "gies\u00b7sen", "vor", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Eolus lie\u00df Nord und Osten,", "tokens": ["E\u00b7o\u00b7lus", "lie\u00df", "Nord", "und", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Pan lie\u00df Schaf und Hirten stehn.", "tokens": ["Pan", "lie\u00df", "Schaf", "und", "Hir\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "G\u00f6tter und G\u00f6ttinnen mu\u00dften", "tokens": ["G\u00f6t\u00b7ter", "und", "G\u00f6t\u00b7tin\u00b7nen", "mu\u00df\u00b7ten"], "token_info": ["word