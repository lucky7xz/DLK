{"textgrid.poem.50874": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Wilhelm v. Humboldts Landhaus am Tegelsee", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzt ein heitres stilles Haus", "tokens": ["Es", "gl\u00e4nzt", "ein", "heit\u00b7res", "stil\u00b7les", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus stillen gr\u00fcnen Kronen;", "tokens": ["Aus", "stil\u00b7len", "gr\u00fc\u00b7nen", "Kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf seinen Warten ruhen aus", "tokens": ["Auf", "sei\u00b7nen", "War\u00b7ten", "ru\u00b7hen", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Winde aller Zonen.", "tokens": ["Die", "Win\u00b7de", "al\u00b7ler", "Zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf ihrem Hauch ein edler Klang", "tokens": ["Auf", "ih\u00b7rem", "Hauch", "ein", "ed\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sich hinausgeschwungen,", "tokens": ["Hat", "sich", "hin\u00b7aus\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Meer zu Meer gr\u00fc\u00dft ihn Gesang,", "tokens": ["Von", "Meer", "zu", "Meer", "gr\u00fc\u00dft", "ihn", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesang in allen Zungen.", "tokens": ["Ge\u00b7sang", "in", "al\u00b7len", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Hause sind Gemach und Saal", "tokens": ["Im", "Hau\u00b7se", "sind", "Ge\u00b7mach", "und", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fcllt von Glanzgestalten,", "tokens": ["Ge\u00b7f\u00fcllt", "von", "Glanz\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in vergangner Tage Strahl", "tokens": ["Die", "in", "ver\u00b7gang\u00b7ner", "Ta\u00b7ge", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die stumme Wache halten.", "tokens": ["Die", "stum\u00b7me", "Wa\u00b7che", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Marmorlippen scheinen sich", "tokens": ["Die", "Mar\u00b7mor\u00b7lip\u00b7pen", "schei\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Just aufzutun wie Bl\u00fcten,", "tokens": ["Just", "auf\u00b7zu\u00b7tun", "wie", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erhobne H\u00e4nde feierlich", "tokens": ["Er\u00b7hob\u00b7ne", "H\u00e4n\u00b7de", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heilig Gut zu h\u00fcten.", "tokens": ["Ein", "hei\u00b7lig", "Gut", "zu", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df hinter dir, was tr\u00fcb und wild,", "tokens": ["La\u00df", "hin\u00b7ter", "dir", ",", "was", "tr\u00fcb", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der du dies Haus betreten,", "tokens": ["Der", "du", "dies", "Haus", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn zu der Hoffnung reinem Bild", "tokens": ["Denn", "zu", "der", "Hoff\u00b7nung", "rei\u00b7nem", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darfst du gefa\u00dft hier beten.", "tokens": ["Darfst", "du", "ge\u00b7fa\u00dft", "hier", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Trittst du hinaus, den F\u00f6hrensaum", "tokens": ["Trittst", "du", "hin\u00b7aus", ",", "den", "F\u00f6h\u00b7ren\u00b7saum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieh ernst den See umgeben!", "tokens": ["Sieh", "ernst", "den", "See", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In seinen Wipfeln rauscht der Traum", "tokens": ["In", "sei\u00b7nen", "Wip\u00b7feln", "rauscht", "der", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom ferneblauen Leben.", "tokens": ["Vom", "fer\u00b7neb\u00b7lau\u00b7en", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und auf dem Walde wandeln sacht", "tokens": ["Und", "auf", "dem", "Wal\u00b7de", "wan\u00b7deln", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wei\u00dfen Wolkenfrauen,", "tokens": ["Die", "wei\u00b7\u00dfen", "Wol\u00b7ken\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in der Flut kristallner Nacht", "tokens": ["Die", "in", "der", "Flut", "kris\u00b7tall\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr klares Bild beschauen.", "tokens": ["Ihr", "kla\u00b7res", "Bild", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In leisrem Blau die Sonne schweift,", "tokens": ["In", "leis\u00b7rem", "Blau", "die", "Son\u00b7ne", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr eigner Schein ist blasser,", "tokens": ["Ihr", "eig\u00b7ner", "Schein", "ist", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von feuchter Reiherschwinge tr\u00e4uft", "tokens": ["Von", "feuch\u00b7ter", "Rei\u00b7her\u00b7schwin\u00b7ge", "tr\u00e4uft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er perlenbleich ins Wasser.", "tokens": ["Er", "per\u00b7len\u00b7bleich", "ins", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "F\u00fchlst nach der Heimat du das Weh,", "tokens": ["F\u00fchlst", "nach", "der", "Hei\u00b7mat", "du", "das", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Fremdling, dich durchschauern,", "tokens": ["O", "Fremd\u00b7ling", ",", "dich", "durch\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Land an dem nord'schen Geistersee,", "tokens": ["Land", "an", "dem", "nord'\u00b7schen", "Geis\u00b7ter\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hier ist es sch\u00f6n zu trauern!", "tokens": ["Hier", "ist", "es", "sch\u00f6n", "zu", "trau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}