{"textgrid.poem.60167": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kenner von dem saphischen Gesange!", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kenner von dem saphischen Gesange!", "tokens": ["Ken\u00b7ner", "von", "dem", "sa\u00b7phi\u00b7schen", "Ge\u00b7san\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter deinem weissen Ueberhange", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "weis\u00b7sen", "Ue\u00b7ber\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Klopft ein Herze, voller Gluth in dir!", "tokens": ["Klopft", "ein", "Her\u00b7ze", ",", "vol\u00b7ler", "Gluth", "in", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "$,", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Liebe ward es unterrichtet", "tokens": ["Von", "der", "Lie\u00b7be", "ward", "es", "un\u00b7ter\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dieses Herze, aber ganz erdichtet", "tokens": ["Die\u00b7ses", "Her\u00b7ze", ",", "a\u00b7ber", "ganz", "er\u00b7dich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nennst du sie die Lehrerin von mir!", "tokens": ["Nennst", "du", "sie", "die", "Leh\u00b7re\u00b7rin", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine Jugend ward gedr\u00fcckt von Sorgen,", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", "ward", "ge\u00b7dr\u00fcckt", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seufzend sang an manchem Sommermorgen", "tokens": ["Seuf\u00b7zend", "sang", "an", "man\u00b7chem", "Som\u00b7mer\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Einfalt ihr gestammelt Lied;", "tokens": ["Mei\u00b7ne", "Ein\u00b7falt", "ihr", "ge\u00b7stam\u00b7melt", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht dem J\u00fcngling th\u00f6neten Ges\u00e4nge,", "tokens": ["Nicht", "dem", "J\u00fcng\u00b7ling", "th\u00f6\u00b7ne\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein, dem Gott, der auf der Menschen Menge,", "tokens": ["Nein", ",", "dem", "Gott", ",", "der", "auf", "der", "Men\u00b7schen", "Men\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie auf Ameishaufen niedersieht!", "tokens": ["Wie", "auf", "Am\u00b7eis\u00b7hau\u00b7fen", "nie\u00b7der\u00b7sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ohne Regung, die ich oft beschreibe,", "tokens": ["Oh\u00b7ne", "Re\u00b7gung", ",", "die", "ich", "oft", "be\u00b7schrei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ohne Z\u00e4rtlichkeit ward ich zum Weibe,", "tokens": ["Oh\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", "ward", "ich", "zum", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ward zur Mutter! wie im wilden Krieg,", "tokens": ["Ward", "zur", "Mut\u00b7ter", "!", "wie", "im", "wil\u00b7den", "Krieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$.", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unverliebt ein M\u00e4dchen werden m\u00fc\u00dfte,", "tokens": ["Un\u00b7ver\u00b7liebt", "ein", "M\u00e4d\u00b7chen", "wer\u00b7den", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die ein Krieger halb gezwungen k\u00fc\u00dfte,", "tokens": ["Die", "ein", "Krie\u00b7ger", "halb", "ge\u00b7zwun\u00b7gen", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der die Mauer einer Stadt erstieg.", "tokens": ["Der", "die", "Mau\u00b7er", "ei\u00b7ner", "Stadt", "er\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sing ich Lieder f\u00fcr der Liebe Kenner:", "tokens": ["Sing", "ich", "Lie\u00b7der", "f\u00fcr", "der", "Lie\u00b7be", "Ken\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dann denk ich den z\u00e4rtlichsten der M\u00e4nner,", "tokens": ["Dann", "denk", "ich", "den", "z\u00e4rt\u00b7lichs\u00b7ten", "der", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den ich immer w\u00fcnschte, nie erhielt;", "tokens": ["Den", "ich", "im\u00b7mer", "w\u00fcnschte", ",", "nie", "er\u00b7hielt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keine Gattin k\u00fc\u00dfte je getreuer,", "tokens": ["Kei\u00b7ne", "Gat\u00b7tin", "k\u00fc\u00df\u00b7te", "je", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Als ich in der Sapho sanftem Feuer", "tokens": ["Als", "ich", "in", "der", "Sa\u00b7pho", "sanf\u00b7tem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lippen k\u00fc\u00dfte, die ich nie gef\u00fchlt!", "tokens": ["Lip\u00b7pen", "k\u00fc\u00df\u00b7te", ",", "die", "ich", "nie", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Was wir heftig lange w\u00fcnschen m\u00fcssen,", "tokens": ["Was", "wir", "hef\u00b7tig", "lan\u00b7ge", "w\u00fcn\u00b7schen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und was wir nicht zu erhalten wissen,", "tokens": ["Und", "was", "wir", "nicht", "zu", "er\u00b7hal\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dr\u00fcckt sich tiefer unserm Herzen ein;", "tokens": ["Dr\u00fcckt", "sich", "tie\u00b7fer", "un\u00b7serm", "Her\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rebensaft verschwender der Gesunde,", "tokens": ["Re\u00b7ben\u00b7saft", "ver\u00b7schwen\u00b7der", "der", "Ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und erquickend schmeckt des Kranken Munde", "tokens": ["Und", "er\u00b7qui\u00b7ckend", "schmeckt", "des", "Kran\u00b7ken", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auch im Traum der ungetrunkne Wein.", "tokens": ["Auch", "im", "Traum", "der", "un\u00b7ge\u00b7trunk\u00b7ne", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}