{"dta.poem.13222": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier fieng man an im Saal sich mit Ger\u00e4usch zu regen;", "tokens": ["Hier", "fi\u00b7eng", "man", "an", "im", "Saal", "sich", "mit", "Ge\u00b7r\u00e4usch", "zu", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "APPRART", "NN", "PRF", "APPR", "NN", "APPR", "ADJA", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ein Gegen-Eifer schien die Sinnen zu bewegen,", "tokens": ["Ein", "Ge\u00b7gen\u00b7Ei\u00b7fer", "schien", "die", "Sin\u00b7nen", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ob die Fr\u00f6mmigkeit nun vorgezogen w\u00e4r;", "tokens": ["Als", "ob", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "nun", "vor\u00b7ge\u00b7zo\u00b7gen", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum sie dann den Plaz f\u00fcr sich allein begehr.", "tokens": ["Wa\u00b7rum", "sie", "dann", "den", "Plaz", "f\u00fcr", "sich", "al\u00b7lein", "be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "515Es wollten einige der Bitte wiedersprechen;", "tokens": ["woll\u00b7ten", "ei\u00b7ni\u00b7ge", "der", "Bit\u00b7te", "wie\u00b7der\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Allein ", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "\u201enicht nur der Fr\u00f6mmigkeit\u201e, so fuhr sie weiter fort,", "tokens": ["\u201e", "nicht", "nur", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "\u201e", ",", "so", "fuhr", "sie", "wei\u00b7ter", "fort", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "ART", "NN", "$(", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ebestimm\u2019 ich, was sie sucht: euch allen sey der Ort", "tokens": ["\u201e", "be\u00b7stimm'", "ich", ",", "was", "sie", "sucht", ":", "euch", "al\u00b7len", "sey", "der", "Ort"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$.", "PPER", "PIS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eden sie verlangt, gemein. Jhr werdet ihn besizen.", "tokens": ["\u201e", "den", "sie", "ver\u00b7langt", ",", "ge\u00b7mein", ".", "Ihr", "wer\u00b7det", "ihn", "be\u00b7si\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "520\u201dIch nenn' euch insgesamt derselben H\u00fctte St\u00fczen.", "tokens": ["\"", "Ich", "nenn'", "euch", "ins\u00b7ge\u00b7samt", "der\u00b7sel\u00b7ben", "H\u00fct\u00b7te", "St\u00fc\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf diese Worte blieb der Saal bes\u00e4nftiget,", "tokens": ["Auf", "die\u00b7se", "Wor\u00b7te", "blieb", "der", "Saal", "be\u00b7s\u00e4nf\u00b7ti\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Wahn der Eifersucht mit Ruh geendiget.", "tokens": ["Der", "Wahn", "der", "Ei\u00b7fer\u00b7sucht", "mit", "Ruh", "ge\u00b7en\u00b7di\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}