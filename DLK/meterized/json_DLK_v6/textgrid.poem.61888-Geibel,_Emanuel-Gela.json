{"textgrid.poem.61888": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gela", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frische L\u00fcfte, die von Osten", "tokens": ["Fri\u00b7sche", "L\u00fcf\u00b7te", ",", "die", "von", "Os\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcbers Meer befl\u00fcgelt ziehn,", "tokens": ["\u00dc\u00b7bers", "Meer", "be\u00b7fl\u00fc\u00b7gelt", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lassen Fr\u00fchlingslust mich kosten,", "tokens": ["Las\u00b7sen", "Fr\u00fch\u00b7lings\u00b7lust", "mich", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der Sommer l\u00e4ngst erschien.", "tokens": ["Ob", "der", "Som\u00b7mer", "l\u00e4ngst", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also l\u00e4\u00dft bei reifen Jahren", "tokens": ["Al\u00b7so", "l\u00e4\u00dft", "bei", "rei\u00b7fen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz der Narben im Gem\u00fct", "tokens": ["Trotz", "der", "Nar\u00b7ben", "im", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gela mich ein Gl\u00fcck erfahren,", "tokens": ["Ge\u00b7la", "mich", "ein", "Gl\u00fcck", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie es nur der Jugend bl\u00fcht.", "tokens": ["Wie", "es", "nur", "der", "Ju\u00b7gend", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfen Tiefsinn bald im Munde,", "tokens": ["S\u00fc\u00b7\u00dfen", "Tief\u00b7sinn", "bald", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schalkhaft bald wie Ariel,", "tokens": ["Schalk\u00b7haft", "bald", "wie", "A\u00b7riel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weckt sie mir im Herzensgrunde", "tokens": ["Weckt", "sie", "mir", "im", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeglicher Empfindung Quell.", "tokens": ["Jeg\u00b7li\u00b7cher", "Emp\u00b7fin\u00b7dung", "Quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Oftmals plaudert sie erg\u00f6tzlich,", "tokens": ["Oft\u00b7mals", "plau\u00b7dert", "sie", "er\u00b7g\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch dazwischen zauberhaft", "tokens": ["Doch", "da\u00b7zwi\u00b7schen", "zau\u00b7ber\u00b7haft"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00fcht's aus ihren Wimpern pl\u00f6tzlich", "tokens": ["Spr\u00fcht's", "aus", "ih\u00b7ren", "Wim\u00b7pern", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Blitz der Leidenschaft.", "tokens": ["Wie", "ein", "Blitz", "der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Spricht sie dann: \u00bbDu bist mir teuer\u00ab,", "tokens": ["Spricht", "sie", "dann", ":", "\u00bb", "Du", "bist", "mir", "teu\u00b7er", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So erbebt mir Herz und Sinn,", "tokens": ["So", "er\u00b7bebt", "mir", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und ein zart \u00e4therisch Feuer", "tokens": ["Und", "ein", "zart", "\u00e4t\u00b7he\u00b7risch", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Str\u00f6mt durch meine Adern hin.", "tokens": ["Str\u00f6mt", "durch", "mei\u00b7ne", "A\u00b7dern", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, da fa\u00dft mich wohl ein Bangen", "tokens": ["Ach", ",", "da", "fa\u00dft", "mich", "wohl", "ein", "Ban\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des eignen Mais Verlust,", "tokens": ["Um", "des", "eig\u00b7nen", "Mais", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie wirft mit hei\u00dfen Wangen", "tokens": ["Doch", "sie", "wirft", "mit", "hei\u00b7\u00dfen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmisch sich an meine Brust,", "tokens": ["St\u00fcr\u00b7misch", "sich", "an", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lacht mich an aus Tr\u00e4neng\u00fcssen,", "tokens": ["Lacht", "mich", "an", "aus", "Tr\u00e4\u00b7nen\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr lachend Auge spricht:", "tokens": ["Und", "ihr", "la\u00b7chend", "Au\u00b7ge", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse nur und la\u00df dich k\u00fcssen,", "tokens": ["K\u00fcs\u00b7se", "nur", "und", "la\u00df", "dich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ein Dichter altert nicht.", "tokens": ["Denn", "ein", "Dich\u00b7ter", "al\u00b7tert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}