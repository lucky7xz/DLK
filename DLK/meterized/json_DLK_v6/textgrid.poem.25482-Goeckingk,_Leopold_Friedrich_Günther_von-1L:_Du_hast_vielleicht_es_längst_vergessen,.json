{"textgrid.poem.25482": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast vielleicht es l\u00e4ngst vergessen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast vielleicht es l\u00e4ngst vergessen,", "tokens": ["Du", "hast", "viel\u00b7leicht", "es", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich seit Monden dir versprach;", "tokens": ["Was", "ich", "seit", "Mon\u00b7den", "dir", "ver\u00b7sprach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch traue du dem Worte dessen,", "tokens": ["Doch", "trau\u00b7e", "du", "dem", "Wor\u00b7te", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nie sein Wort den Sch\u00f6nen brach.", "tokens": ["Der", "nie", "sein", "Wort", "den", "Sch\u00f6\u00b7nen", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir gab das Gl\u00fcck nicht eigne Jagden,", "tokens": ["Mir", "gab", "das", "Gl\u00fcck", "nicht", "eig\u00b7ne", "Jag\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja nicht einmal Gelegenheit", "tokens": ["Ja", "nicht", "ein\u00b7mal", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein fremdes Wildrevier zu pachten,", "tokens": ["Ein", "frem\u00b7des", "Wild\u00b7re\u00b7vier", "zu", "pach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am wenigsten zum Jagen Zeit.", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "zum", "Ja\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPRART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Was konnt' ich, Wort zu halten, w\u00e4hlen,", "tokens": ["Was", "konnt'", "ich", ",", "Wort", "zu", "hal\u00b7ten", ",", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "PTKZU", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich, der am liebsten dir es h\u00e4lt?", "tokens": ["Ich", ",", "der", "am", "liebs\u00b7ten", "dir", "es", "h\u00e4lt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "ADJA", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was blieb mir \u00fcbrig, als: zu stehlen?", "tokens": ["Was", "blieb", "mir", "\u00fcb\u00b7rig", ",", "als", ":", "zu", "steh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fcr dich best\u00f6hl' ich alle Welt!", "tokens": ["F\u00fcr", "dich", "be\u00b7st\u00f6hl'", "ich", "al\u00b7le", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Doch weil von zarterem Gewissen,", "tokens": ["Doch", "weil", "von", "zar\u00b7te\u00b7rem", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "(das wir beim J\u00e4ger oft vermissen,)", "tokens": ["(", "das", "wir", "beim", "J\u00e4\u00b7ger", "oft", "ver\u00b7mis\u00b7sen", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PRELS", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Du selbst in diesem Punkte bist,", "tokens": ["Du", "selbst", "in", "die\u00b7sem", "Punk\u00b7te", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und weil dein Mund sonst keinen Bissen", "tokens": ["Und", "weil", "dein", "Mund", "sonst", "kei\u00b7nen", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Von dem gebratnen Vogel i\u00dft,", "tokens": ["Von", "dem", "ge\u00b7brat\u00b7nen", "Vo\u00b7gel", "i\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So wi\u00df': ein Liebling von Dianen", "tokens": ["So", "wi\u00df'", ":", "ein", "Lieb\u00b7ling", "von", "Di\u00b7a\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hat willig ihn f\u00fcr dich gemi\u00dft.", "tokens": ["Hat", "wil\u00b7lig", "ihn", "f\u00fcr", "dich", "ge\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die\u00df, hoff' ich, wird ja dem Fasanen", "tokens": ["Die\u00df", ",", "hoff'", "ich", ",", "wird", "ja", "dem", "Fa\u00b7sa\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Den Weg zu deinem Munde bahnen;", "tokens": ["Den", "Weg", "zu", "dei\u00b7nem", "Mun\u00b7de", "bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wo f\u00e4nd' er besser auch sein Grab?", "tokens": ["Wo", "f\u00e4nd'", "er", "bes\u00b7ser", "auch", "sein", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "H\u00e4tt' eine F\u00fcrstin ihn hinab", "tokens": ["H\u00e4tt'", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "ihn", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mit manchem falschen Zahn' geschlungen:", "tokens": ["Mit", "man\u00b7chem", "fal\u00b7schen", "Zahn'", "ge\u00b7schlun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da h\u00e4tten Gl\u00e4ser nicht geklungen,", "tokens": ["Da", "h\u00e4t\u00b7ten", "Gl\u00e4\u00b7ser", "nicht", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Kein Nachbar einen Ku\u00df getauscht,", "tokens": ["Kein", "Nach\u00b7bar", "ei\u00b7nen", "Ku\u00df", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kein Mund das Rheinweinlied gesungen;", "tokens": ["Kein", "Mund", "das", "Rhein\u00b7wein\u00b7lied", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Statt da\u00df der Gast mit Ohr und Herzen", "tokens": ["Statt", "da\u00df", "der", "Gast", "mit", "Ohr", "und", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Auf T\u00f6ne deiner Kehl' itzt lauscht,", "tokens": ["Auf", "T\u00f6\u00b7ne", "dei\u00b7ner", "Kehl'", "itzt", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ja selbst, bei deines Mundes Scherzen,", "tokens": ["Ja", "selbst", ",", "bei", "dei\u00b7nes", "Mun\u00b7des", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Beinah die Weisheit sich berauscht.", "tokens": ["Bei\u00b7nah", "die", "Weis\u00b7heit", "sich", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}