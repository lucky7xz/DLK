{"textgrid.poem.46555": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[seufzer, die ihr wehet]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seufzer, die ihr wehet,", "tokens": ["Seuf\u00b7zer", ",", "die", "ihr", "we\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Thr\u00e4nen, die ihr gehet,", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "ihr", "ge\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnt ich selber mit euch gehn,", "tokens": ["K\u00f6nnt", "ich", "sel\u00b7ber", "mit", "euch", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So entging ich meinen Wehn.", "tokens": ["So", "ent\u00b7ging", "ich", "mei\u00b7nen", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seufzer, die ihr wehet,", "tokens": ["Seuf\u00b7zer", ",", "die", "ihr", "we\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von mir angeflehet:", "tokens": ["Von", "mir", "an\u00b7ge\u00b7fle\u00b7het", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bringt zur\u00fcck einmal mein Gl\u00fcck!", "tokens": ["Bringt", "zu\u00b7r\u00fcck", "ein\u00b7mal", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ihr immer leer zur\u00fcck.", "tokens": ["Kommt", "ihr", "im\u00b7mer", "leer", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thr\u00e4nen, die ihr gehet", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "ihr", "ge\u00b7het"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und nie stille stehet,", "tokens": ["Und", "nie", "stil\u00b7le", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steht einmal im Auge still,", "tokens": ["Steht", "ein\u00b7mal", "im", "Au\u00b7ge", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das in euch zergehen will.", "tokens": ["Das", "in", "euch", "zer\u00b7ge\u00b7hen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seufzer, die ihr wehet,", "tokens": ["Seuf\u00b7zer", ",", "die", "ihr", "we\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Thr\u00e4nen, die ihr gehet,", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "ihr", "ge\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4r' ich selber, was ihr seid,", "tokens": ["W\u00e4r'", "ich", "sel\u00b7ber", ",", "was", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgel\u00f6stes Liebesleid!", "tokens": ["Auf\u00b7ge\u00b7l\u00f6s\u00b7tes", "Lie\u00b7be\u00b7sleid", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}