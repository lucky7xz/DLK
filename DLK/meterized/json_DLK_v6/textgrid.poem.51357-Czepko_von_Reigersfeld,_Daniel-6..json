{"textgrid.poem.51357": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Gastgebot, kein Spiel,", "tokens": ["Kein", "Gast\u00b7ge\u00b7bot", ",", "kein", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kein Tantzen und kein Wincken,", "tokens": ["Kein", "Tant\u00b7zen", "und", "kein", "Win\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Nahmen und kein Trincken,", "tokens": ["Kein", "Nah\u00b7men", "und", "kein", "Trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach der Buchstaben Ziel.", "tokens": ["Nach", "der", "Buch\u00b7sta\u00b7ben", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Kein Krantz, kein Gru\u00df, kein Brief,", "tokens": ["Kein", "Krantz", ",", "kein", "Gru\u00df", ",", "kein", "Brief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auch sonst kein Fund noch Grief", "tokens": ["Auch", "sonst", "kein", "Fund", "noch", "Grief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan bey den Liebes Sachen", "tokens": ["Kan", "bey", "den", "Lie\u00b7bes", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch nicht das minste machen.", "tokens": ["Auch", "nicht", "das", "mins\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es ist in uns ein Bronnen,", "tokens": ["Es", "ist", "in", "uns", "ein", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Draus kommt, was angenehm,", "tokens": ["Draus", "kommt", ",", "was", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Beh\u00e4glich und bequem,", "tokens": ["Be\u00b7h\u00e4g\u00b7lich", "und", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Nach seiner Art geronnen:", "tokens": ["Nach", "sei\u00b7ner", "Art", "ge\u00b7ron\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Das wird numehr geliebt,", "tokens": ["Das", "wird", "nu\u00b7mehr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Weil sich es mehr ergiebt;", "tokens": ["Weil", "sich", "es", "mehr", "er\u00b7giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Als Gastgebot, als Spiel,", "tokens": ["Als", "Gast\u00b7ge\u00b7bot", ",", "als", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Als Tantzen und als Wincken,", "tokens": ["Als", "Tant\u00b7zen", "und", "als", "Win\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Als Nahmen und als Trincken", "tokens": ["Als", "Nah\u00b7men", "und", "als", "Trin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Nach der Buchstaben Ziel.", "tokens": ["Nach", "der", "Buch\u00b7sta\u00b7ben", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Als Krantz, als Gru\u00df, als Brief,", "tokens": ["Als", "Krantz", ",", "als", "Gru\u00df", ",", "als", "Brief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und mehr, als Fund und Grief:", "tokens": ["Und", "mehr", ",", "als", "Fund", "und", "Grief", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wer Liebe wil gen\u00fcssen,", "tokens": ["Wer", "Lie\u00b7be", "wil", "ge\u00b7n\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Mu\u00df diesen Brunnen wissen.", "tokens": ["Mu\u00df", "die\u00b7sen", "Brun\u00b7nen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}