{"textgrid.poem.29008": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich irre, bleibt gewi\u00df, alldieweil ein Mensch ich bin;", "tokens": ["Da\u00df", "ich", "ir\u00b7re", ",", "bleibt", "ge\u00b7wi\u00df", ",", "all\u00b7die\u00b7weil", "ein", "Mensch", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der nun mehr ist als ein Mensch, mag mich durch die Hechel ziehn,", "tokens": ["Der", "nun", "mehr", "ist", "als", "ein", "Mensch", ",", "mag", "mich", "durch", "die", "He\u00b7chel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "KOKOM", "ART", "NN", "$,", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sonst werd ich ihn von mir weg an sich selbsten weisen hin.", "tokens": ["Sonst", "werd", "ich", "ihn", "von", "mir", "weg", "an", "sich", "selbs\u00b7ten", "wei\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "PPER", "ADV", "APPR", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}