{"textgrid.poem.37481": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Krischan mit der Piepe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Vader seggt: \u00bbEck mot nu gahn!", "tokens": ["De", "Va\u00b7der", "seggt", ":", "\u00bb", "Eck", "mot", "nu", "gahn", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "NE", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Krischan! lat de Piepen stahn!\u00ab", "tokens": ["Krisc\u00b7han", "!", "lat", "de", "Pie\u00b7pen", "stahn", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "NE", "NE", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kum awer geiht he ut der Doer,", "tokens": ["Kum", "a\u00b7wer", "geiht", "he", "ut", "der", "Doer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Krigt Krischan all de Piepen her.", "tokens": ["Krigt", "Krisc\u00b7han", "all", "de", "Pie\u00b7pen", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Min Krischan steckt ok gar nich fuul", "tokens": ["Min", "Krisc\u00b7han", "steckt", "ok", "gar", "nich", "fuul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PTKNEG", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Smoekepiepen in dat Muul,", "tokens": ["De", "Smoe\u00b7ke\u00b7pie\u00b7pen", "in", "dat", "Mu\u00b7ul", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "He smoekt! \u2013 Wat, Deuker, is denn dat?!", "tokens": ["He", "smoekt", "!", "\u2013", "Wat", ",", "Deu\u00b7ker", ",", "is", "denn", "dat", "?!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "NN", "$,", "NN", "$,", "FM", "KON", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mi d\u00fccht, dar achter r\u00f6gt sick wat.", "tokens": ["Mi", "d\u00fccht", ",", "dar", "ach\u00b7ter", "r\u00f6gt", "sick", "wat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "De Stock is mit den Schirm in Gange,", "tokens": ["De", "Stock", "is", "mit", "den", "Schirm", "in", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De Aben danzet mit der Tange.", "tokens": ["De", "A\u00b7ben", "dan\u00b7zet", "mit", "der", "Tan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "De Slaprock danzt mit den Stohl, juheh!", "tokens": ["De", "Sla\u00b7prock", "danzt", "mit", "den", "Stohl", ",", "ju\u00b7heh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$,", "ITJ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Un de Disch mit den olen Kanapeh.", "tokens": ["Un", "de", "Disch", "mit", "den", "o\u00b7len", "Ka\u00b7na\u00b7peh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Up eenmal \u2013 puff! \u2013 do werd et dunkel:", "tokens": ["Up", "e\u00b7en\u00b7mal", "\u2013", "puff", "!", "\u2013", "do", "werd", "et", "dun\u00b7kel", ":"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "VVIMP", "$.", "$(", "ADV", "VAFIN", "NE", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dat is de ole Runkelmunkel!", "tokens": ["Dat", "is", "de", "o\u00b7le", "Run\u00b7kel\u00b7mun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Un \u2013 puff! \u2013 kummt no'n Keerel an:", "tokens": ["Un", "\u2013", "puff", "!", "\u2013", "kummt", "no'n", "Kee\u00b7rel", "an", ":"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "$.", "$(", "VVFIN", "NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dat ist de swarte Morian.", "tokens": ["Dat", "ist", "de", "swar\u00b7te", "Mo\u00b7ri\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Se danzet un springet un dreihet sick,", "tokens": ["Se", "dan\u00b7zet", "un", "sprin\u00b7get", "un", "drei\u00b7het", "sick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Krischan werd so wunderlick.", "tokens": ["Den", "Krisc\u00b7han", "werd", "so", "wun\u00b7der\u00b7lick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Se danzet, dildi, se trampelt, schrum schrum!", "tokens": ["Se", "dan\u00b7zet", ",", "dil\u00b7di", ",", "se", "tram\u00b7pelt", ",", "schrum", "schrum", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "$,", "ADV", "VVFIN", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wupp! dreiht sick de ganze Stube um!", "tokens": ["Wupp", "!", "dreiht", "sick", "de", "gan\u00b7ze", "Stu\u00b7be", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "NE", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "J\u00fcst tret de Moder in de Doer,", "tokens": ["J\u00fcst", "tret", "de", "Mo\u00b7der", "in", "de", "Doer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Krischan ligt ganz krumm und quer.", "tokens": ["De", "Krisc\u00b7han", "ligt", "ganz", "krumm", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "He ligt to Bed; de Keerels winkt,", "tokens": ["He", "ligt", "to", "Bed", ";", "de", "Kee\u00b7rels", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "FM", "FM", "$.", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Moder swarten Kaffe bringt.", "tokens": ["Als", "Mo\u00b7der", "swar\u00b7ten", "Kaf\u00b7fe", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "He drinkt den swarten Kaffe ut,", "tokens": ["He", "drinkt", "den", "swar\u00b7ten", "Kaf\u00b7fe", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dat deiht min lewen Krischan gut.", "tokens": ["Dat", "deiht", "min", "le\u00b7wen", "Krisc\u00b7han", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.14": {"line.1": {"text": "Un Vader sitt dabi un lacht", "tokens": ["Un", "Va\u00b7der", "sitt", "da\u00b7bi", "un", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PAV", "FM", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un seggt: \u00bbDat heb eck lange dacht!", "tokens": ["Un", "seggt", ":", "\u00bb", "Dat", "heb", "eck", "lan\u00b7ge", "dacht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "NN", "VVFIN", "NE", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.15": {"line.1": {"text": "Ja ja, min Jung! so mot et gahn!", "tokens": ["Ja", "ja", ",", "min", "Jung", "!", "so", "mot", "et", "gahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "PPOSAT", "NN", "$.", "ADV", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Krischan, lat de Piepe stahn!!\u00ab", "tokens": ["Krisc\u00b7han", ",", "lat", "de", "Pie\u00b7pe", "stahn", "!!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}