{"dta.poem.19385": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "118.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Obstb\u00e4ume sind genug, o Kinder, hier im Garten;", "tokens": ["Obst\u00b7b\u00e4u\u00b7me", "sind", "ge\u00b7nug", ",", "o", "Kin\u00b7der", ",", "hier", "im", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr m\u00fc\u00dft beim ersten Baum die Reife nur erwarten.", "tokens": ["Ihr", "m\u00fc\u00dft", "beim", "ers\u00b7ten", "Baum", "die", "Rei\u00b7fe", "nur", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4ume l\u00f6sen sich von Wochen ab zu Wochen,", "tokens": ["Die", "B\u00e4u\u00b7me", "l\u00f6\u00b7sen", "sich", "von", "Wo\u00b7chen", "ab", "zu", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df neugereifte Frucht in jeder sei gebrochen.", "tokens": ["Da\u00df", "neu\u00b7ge\u00b7reif\u00b7te", "Frucht", "in", "je\u00b7der", "sei", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und kaum an einem Baum habt ihr euch satt gepfl\u00fcckt,", "tokens": ["Und", "kaum", "an", "ei\u00b7nem", "Baum", "habt", "ihr", "euch", "satt", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als schon der folgende f\u00fcr euch die Tafel schm\u00fcckt.", "tokens": ["Als", "schon", "der", "fol\u00b7gen\u00b7de", "f\u00fcr", "euch", "die", "Ta\u00b7fel", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wenn beim ersten ihr zu fr\u00fch beginnt den Schmaus,", "tokens": ["Doch", "wenn", "beim", "ers\u00b7ten", "ihr", "zu", "fr\u00fch", "be\u00b7ginnt", "den", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "PPER", "PTKA", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seid ihr dann \u00fcberall der rechten Zeit voraus.", "tokens": ["Seid", "ihr", "dann", "\u00fc\u00b7be\u00b7rall", "der", "rech\u00b7ten", "Zeit", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Euch wird von einem Baum Begier zum andern treiben,", "tokens": ["Euch", "wird", "von", "ei\u00b7nem", "Baum", "Be\u00b7gier", "zum", "an\u00b7dern", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN", "APPRART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keinem wird die Zeit, die Frucht zu reifen, bleiben.", "tokens": ["Und", "kei\u00b7nem", "wird", "die", "Zeit", ",", "die", "Frucht", "zu", "rei\u00b7fen", ",", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr habt das ganze Jahr zu essen herbe Frucht,", "tokens": ["Ihr", "habt", "das", "gan\u00b7ze", "Jahr", "zu", "es\u00b7sen", "her\u00b7be", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil von dem ersten Baum ihr habt zu fr\u00fch versucht.", "tokens": ["Weil", "von", "dem", "ers\u00b7ten", "Baum", "ihr", "habt", "zu", "fr\u00fch", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}