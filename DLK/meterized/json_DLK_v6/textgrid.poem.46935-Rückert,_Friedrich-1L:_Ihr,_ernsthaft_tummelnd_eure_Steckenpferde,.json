{"textgrid.poem.46935": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr, ernsthaft tummelnd eure Steckenpferde,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, ernsthaft tummelnd eure Steckenpferde,", "tokens": ["Ihr", ",", "ernst\u00b7haft", "tum\u00b7melnd", "eu\u00b7re", "Ste\u00b7cken\u00b7pfer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr, streitend in der Spiegelfechter Trosse,", "tokens": ["Ihr", ",", "strei\u00b7tend", "in", "der", "Spie\u00b7gel\u00b7fech\u00b7ter", "Tros\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr, zielend mit nie treffendem Geschosse,", "tokens": ["Ihr", ",", "zie\u00b7lend", "mit", "nie", "tref\u00b7fen\u00b7dem", "Ge\u00b7schos\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr, Streiche f\u00fchrend mit papiernem Schwerte!", "tokens": ["Ihr", ",", "Strei\u00b7che", "f\u00fch\u00b7rend", "mit", "pa\u00b7pier\u00b7nem", "Schwer\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr, die ihr euch von der sichern Erde", "tokens": ["Und", "ihr", ",", "die", "ihr", "euch", "von", "der", "si\u00b7chern", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf eurer Musen fabelhaftem Rosse", "tokens": ["Auf", "eu\u00b7rer", "Mu\u00b7sen", "fa\u00b7bel\u00b7haf\u00b7tem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gen Himmel spornt, ihr treibt die \u00e4rgste Posse,", "tokens": ["Gen", "Him\u00b7mel", "spornt", ",", "ihr", "treibt", "die", "\u00e4rgs\u00b7te", "Pos\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr seid die r\u00e4udigsten der ganzen Herde.", "tokens": ["Ihr", "seid", "die", "r\u00e4u\u00b7digs\u00b7ten", "der", "gan\u00b7zen", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Werft von euch eurer Thorheit bunte Wappen,", "tokens": ["Werft", "von", "euch", "eu\u00b7rer", "Thor\u00b7heit", "bun\u00b7te", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Womit ihr prunkt, und greift zu wahren Waffen,", "tokens": ["Wo\u00b7mit", "ihr", "prunkt", ",", "und", "greift", "zu", "wah\u00b7ren", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Statt eurer Steckenpferde z\u00e4umet Rappen;", "tokens": ["Statt", "eu\u00b7rer", "Ste\u00b7cken\u00b7pfer\u00b7de", "z\u00e4u\u00b7met", "Rap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Setzt Helme auf statt eurer Narrenkappen,", "tokens": ["Setzt", "Hel\u00b7me", "auf", "statt", "eu\u00b7rer", "Nar\u00b7ren\u00b7kap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Seid wahre M\u00e4nner statt der G\u00f6tter Affen,", "tokens": ["Seid", "wah\u00b7re", "M\u00e4n\u00b7ner", "statt", "der", "G\u00f6t\u00b7ter", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und, wenn ihr nicht k\u00f6nnt Ritter sein, seid Knappen!", "tokens": ["Und", ",", "wenn", "ihr", "nicht", "k\u00f6nnt", "Rit\u00b7ter", "sein", ",", "seid", "Knap\u00b7pen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "NN", "VAINF", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}