{"textgrid.poem.63997": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Herbstmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater, herrlicher Vater,", "tokens": ["Va\u00b7ter", ",", "herr\u00b7li\u00b7cher", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Soll ich meine Seele dir senden,", "tokens": ["Soll", "ich", "mei\u00b7ne", "See\u00b7le", "dir", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Was soll ich mit ihr,", "tokens": ["Was", "soll", "ich", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich verstehe sie ja nicht mal zu halten,", "tokens": ["Ich", "ver\u00b7ste\u00b7he", "sie", "ja", "nicht", "mal", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nicht zu gestalten?", "tokens": ["Nicht", "zu", "ge\u00b7stal\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und sie liebt dich so,", "tokens": ["Und", "sie", "liebt", "dich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und ich treibe sie weit,", "tokens": ["Und", "ich", "trei\u00b7be", "sie", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Weit ab von dir,", "tokens": ["Weit", "ab", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In Nesseln und in Sumpf,", "tokens": ["In", "Nes\u00b7seln", "und", "in", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und ihre scharfen Sinne", "tokens": ["Und", "ih\u00b7re", "schar\u00b7fen", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wurden dumpf.", "tokens": ["Wur\u00b7den", "dumpf", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Wie dieser blaue,", "tokens": ["Wie", "die\u00b7ser", "blau\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "R\u00fcstige Morgen,", "tokens": ["R\u00fcs\u00b7ti\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Wie er sich \u00f6ffnet", "tokens": ["Wie", "er", "sich", "\u00f6ff\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Deiner starken Sonne", "tokens": ["Dei\u00b7ner", "star\u00b7ken", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Freundlichem Gold,", "tokens": ["Freund\u00b7li\u00b7chem", "Gold", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "So auf zu dir.", "tokens": ["So", "auf", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Und wie jung und wei\u00df umflimmert", "tokens": ["Und", "wie", "jung", "und", "wei\u00df", "um\u00b7flim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "KON", "VVFIN", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "Die Herbstblumen bunte", "tokens": ["Die", "Herbst\u00b7blu\u00b7men", "bun\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.20": {"text": "Kinderwelt", "tokens": ["Kin\u00b7der\u00b7welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Hier auf dem Schulhof,", "tokens": ["Hier", "auf", "dem", "Schul\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "So sollen munter", "tokens": ["So", "sol\u00b7len", "mun\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Meiner Seele", "tokens": ["Mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Ewige Jugendkr\u00e4fte", "tokens": ["E\u00b7wi\u00b7ge", "Ju\u00b7gend\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.25": {"text": "Wandeln vor dir.", "tokens": ["Wan\u00b7deln", "vor", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}