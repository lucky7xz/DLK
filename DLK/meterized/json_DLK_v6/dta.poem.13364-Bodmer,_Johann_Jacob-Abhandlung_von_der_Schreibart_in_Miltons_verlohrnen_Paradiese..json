{"dta.poem.13364": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn in dem Finsterni\u00df, worinn wir sind begraben,", "tokens": ["Wenn", "in", "dem", "Fins\u00b7ter\u00b7ni\u00df", ",", "wo\u00b7rinn", "wir", "sind", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Denken noch nicht fest, und an den Sinnen klein,", "tokens": ["Mit", "Den\u00b7ken", "noch", "nicht", "fest", ",", "und", "an", "den", "Sin\u00b7nen", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du eine l\u00e4ngre Zeit gehangen w\u00fcrdest seyn.", "tokens": ["Du", "ei\u00b7ne", "l\u00e4ng\u00b7re", "Zeit", "ge\u00b7han\u00b7gen", "w\u00fcr\u00b7dest", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}