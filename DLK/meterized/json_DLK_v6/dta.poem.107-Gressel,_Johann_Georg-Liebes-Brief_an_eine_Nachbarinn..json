{"dta.poem.107": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an eine Nachbarinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Galante Nachbarin erbrich ein kleines Schreiben/", "tokens": ["Ga\u00b7lan\u00b7te", "Nach\u00b7ba\u00b7rin", "er\u00b7brich", "ein", "klei\u00b7nes", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ein entflammter Geist aufs weisse Blat gestellt;", "tokens": ["Das", "ein", "ent\u00b7flamm\u00b7ter", "Geist", "aufs", "weis\u00b7se", "Blat", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Feuer m\u00fcste zwar noch wol verborgen bleiben/", "tokens": ["Mein", "Feu\u00b7er", "m\u00fcs\u00b7te", "zwar", "noch", "wol", "ver\u00b7bor\u00b7gen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "ADV", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein! wer widersteht wenn Dach und Giebel f\u00e4llt.", "tokens": ["Al\u00b7lein", "!", "wer", "wi\u00b7der\u00b7steht", "wenn", "Dach", "und", "Gie\u00b7bel", "f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VVFIN", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn der gesammte Bau in lichten Flammen stehet/", "tokens": ["Wenn", "der", "ge\u00b7samm\u00b7te", "Bau", "in", "lich\u00b7ten", "Flam\u00b7men", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So fliehet jedermann/ und sucht bey andern Raht/", "tokens": ["So", "flie\u00b7het", "je\u00b7der\u00b7mann", "/", "und", "sucht", "bey", "an\u00b7dern", "Raht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Eh\u2019 als das gantze Haus im Rauch und Dampff vergehet/", "tokens": ["Eh'", "als", "das", "gant\u00b7ze", "Haus", "im", "Rauch", "und", "Dampff", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die erbo\u00dfte Flamm zu weit gefressen hat.", "tokens": ["Und", "die", "er\u00b7bo\u00df\u00b7te", "Flamm", "zu", "weit", "ge\u00b7fres\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKA", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Hertz brennt lichterloh/ die heissen Funcken stieben", "tokens": ["Mein", "Hertz", "brennt", "lich\u00b7ter\u00b7loh", "/", "die", "heis\u00b7sen", "Fun\u00b7cken", "stie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir aus den Augen raus: die helle Liebes-Gluht", "tokens": ["Mir", "aus", "den", "Au\u00b7gen", "raus", ":", "die", "hel\u00b7le", "Lie\u00b7bes\u00b7Gluht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist schon neun Monden lang bey mir verborgen blieben/", "tokens": ["Ist", "schon", "neun", "Mon\u00b7den", "lang", "bey", "mir", "ver\u00b7bor\u00b7gen", "blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "ADJD", "APPR", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wodurch mir ist verseigt mein Nahrungs-Oehl/ das Blut.", "tokens": ["Wo\u00b7durch", "mir", "ist", "ver\u00b7seigt", "mein", "Nah\u00b7rungs\u00b7Oehl", "/", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist nun von deiner Hand nicht H\u00fclffe zu erlangen?", "tokens": ["Ist", "nun", "von", "dei\u00b7ner", "Hand", "nicht", "H\u00fclf\u00b7fe", "zu", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mu\u00df mein Hertzens-Bau in Flammen untergehn;", "tokens": ["So", "mu\u00df", "mein", "Hert\u00b7zens\u00b7Bau", "in", "Flam\u00b7men", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch stoltze Hoffnung l\u00e4sst mich schon mit Beystand prangen/", "tokens": ["Doch", "stolt\u00b7ze", "Hoff\u00b7nung", "l\u00e4sst", "mich", "schon", "mit", "Beys\u00b7tand", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil meinen Untergang dein Auge nicht kan sehn.", "tokens": ["Weil", "mei\u00b7nen", "Un\u00b7ter\u00b7gang", "dein", "Au\u00b7ge", "nicht", "kan", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn brennt des N\u00e4chsten Wand/ so ist Gefahr verhanden", "tokens": ["Denn", "brennt", "des", "N\u00e4chs\u00b7ten", "Wand", "/", "so", "ist", "Ge\u00b7fahr", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vor jenen/ der zun\u00e4chst an solchen Flammen wohnt;", "tokens": ["Vor", "je\u00b7nen", "/", "der", "zu\u00b7n\u00e4chst", "an", "sol\u00b7chen", "Flam\u00b7men", "wohnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$(", "ART", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es pflegt der harte Sinn/ am Wehmuhts-Fels zu stranden/", "tokens": ["Es", "pflegt", "der", "har\u00b7te", "Sinn", "/", "am", "Wehm\u00b7uhts\u00b7Fels", "zu", "stran\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil die erz\u00fcrnte Gluht auch seinen Bau nicht schont.", "tokens": ["Weil", "die", "er\u00b7z\u00fcrn\u00b7te", "Gluht", "auch", "sei\u00b7nen", "Bau", "nicht", "schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein jeder tr\u00e4get bey was zur Errettung dienet/", "tokens": ["Ein", "je\u00b7der", "tr\u00e4\u00b7get", "bey", "was", "zur", "Er\u00b7ret\u00b7tung", "die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PRELS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Setzt Feindschaft an die Seit\u2019/ und steurt den wilden Brand/", "tokens": ["Setzt", "Feind\u00b7schaft", "an", "die", "Seit'", "/", "und", "steurt", "den", "wil\u00b7den", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil durch gethane H\u00fclff die Wohlfahrts-Pflantze gr\u00fcnet/", "tokens": ["Weil", "durch", "ge\u00b7tha\u00b7ne", "H\u00fclff", "die", "Wohl\u00b7fahrts\u00b7Pflant\u00b7ze", "gr\u00fc\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und angewandter Flei\u00df besch\u00fctzet seine Wand.", "tokens": ["Und", "an\u00b7ge\u00b7wand\u00b7ter", "Flei\u00df", "be\u00b7sch\u00fct\u00b7zet", "sei\u00b7ne", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allem! hier seh\u2019 ich gern/ da\u00df auch mit heissen Flammen", "tokens": ["Al\u00b7lem", "!", "hier", "seh'", "ich", "gern", "/", "da\u00df", "auch", "mit", "heis\u00b7sen", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$.", "ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein annoch kaltes Hertz in Liebe werd\u2019 entbrennt;", "tokens": ["Dein", "an\u00b7noch", "kal\u00b7tes", "Hertz", "in", "Lie\u00b7be", "werd'", "ent\u00b7brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df du magst meine Gluht/ als sch\u00e4dlich/ nicht verdammen/", "tokens": ["Da\u00df", "du", "magst", "mei\u00b7ne", "Gluht", "/", "als", "sch\u00e4d\u00b7lich", "/", "nicht", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "$(", "KOUS", "ADJD", "$(", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So da der Tugend-Bahn im minsten nicht verkennt.", "tokens": ["So", "da", "der", "Tu\u00b7gen\u00b7dBahn", "im", "mins\u00b7ten", "nicht", "ver\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Schl\u00e4gt nun mein Bitten an/ werd\u2019 ich Erh\u00f6rung finden/", "tokens": ["Schl\u00e4gt", "nun", "mein", "Bit\u00b7ten", "an", "/", "werd'", "ich", "Er\u00b7h\u00f6\u00b7rung", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$(", "VAFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wird mein krancker Geist ins Paradi\u00df verr\u00fcckt.", "tokens": ["So", "wird", "mein", "kran\u00b7cker", "Geist", "ins", "Pa\u00b7ra\u00b7di\u00df", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich werde vor ", "tokens": ["Ich", "wer\u00b7de", "vor"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Wo sich ", "tokens": ["Wo", "sich"], "token_info": ["word", "word"], "pos": ["PWAV", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Ich werde in dem Port der frohen Hoffnung l\u00e4nden/", "tokens": ["Ich", "wer\u00b7de", "in", "dem", "Port", "der", "fro\u00b7hen", "Hoff\u00b7nung", "l\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und treiben an das Land der Gl\u00fcckes-Insul an/", "tokens": ["Und", "trei\u00b7ben", "an", "das", "Land", "der", "Gl\u00fc\u00b7ckes\u00b7In\u00b7sul", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die lange Nacht wird sich mit hellem Morgen enden/", "tokens": ["Die", "lan\u00b7ge", "Nacht", "wird", "sich", "mit", "hel\u00b7lem", "Mor\u00b7gen", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.38": {"text": "Nachdem ihm seine Sonn\u2019 geht in der N\u00e4he auf/", "tokens": ["Nach\u00b7dem", "ihm", "sei\u00b7ne", "Sonn'", "geht", "in", "der", "N\u00e4\u00b7he", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich werde durch den Sturm zum gr\u00fcnen Berge dringen/", "tokens": ["Ich", "wer\u00b7de", "durch", "den", "Sturm", "zum", "gr\u00fc\u00b7nen", "Ber\u00b7ge", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil mir kein ", "tokens": ["Weil", "mir", "kein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Wird nun auch kein Verzug an deiner Seiten liegen/", "tokens": ["Wird", "nun", "auch", "kein", "Ver\u00b7zug", "an", "dei\u00b7ner", "Sei\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So schreib mir Nachbarinn gew\u00fcnscht bald wieder zu/", "tokens": ["So", "schreib", "mir", "Nach\u00b7ba\u00b7rinn", "ge\u00b7w\u00fcnscht", "bald", "wie\u00b7der", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVPP", "ADV", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Gedenck/ da\u00df ich den Brand schon lang\u2019 genug verschwiegen/", "tokens": ["Ge\u00b7denck", "/", "da\u00df", "ich", "den", "Brand", "schon", "lang'", "ge\u00b7nug", "ver\u00b7schwie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und setz durch gute Schrifft mein traurges Hertz in Ruh.", "tokens": ["Und", "setz", "durch", "gu\u00b7te", "Schrifft", "mein", "traur\u00b7ges", "Hertz", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}