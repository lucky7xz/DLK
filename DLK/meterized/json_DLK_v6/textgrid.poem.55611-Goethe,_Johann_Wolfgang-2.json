{"textgrid.poem.55611": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df wie Lilien, reine Kerzen,", "tokens": ["Wei\u00df", "wie", "Li\u00b7li\u00b7en", ",", "rei\u00b7ne", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sternen gleich, bescheidner Beugung,", "tokens": ["Ster\u00b7nen", "gleich", ",", "be\u00b7scheid\u00b7ner", "Beu\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtet aus dem Mittelherzen", "tokens": ["Leuch\u00b7tet", "aus", "dem", "Mit\u00b7tel\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rot ges\u00e4umt die Glut der Neigung.", "tokens": ["Rot", "ge\u00b7s\u00e4umt", "die", "Glut", "der", "Nei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So fr\u00fchzeitige Narzissen", "tokens": ["So", "fr\u00fch\u00b7zei\u00b7ti\u00b7ge", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchen reihenweis im Garten.", "tokens": ["Bl\u00fc\u00b7hen", "rei\u00b7hen\u00b7weis", "im", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6gen wohl die guten wissen,", "tokens": ["M\u00f6\u00b7gen", "wohl", "die", "gu\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wen sie so spaliert erwarten.", "tokens": ["Wen", "sie", "so", "spa\u00b7liert", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}