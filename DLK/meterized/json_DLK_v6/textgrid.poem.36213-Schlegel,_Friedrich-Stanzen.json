{"textgrid.poem.36213": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Stanzen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Bl\u00e4tter dunkles Gr\u00fcn um Blumen ranken,", "tokens": ["Wie", "Bl\u00e4t\u00b7ter", "dunk\u00b7les", "Gr\u00fcn", "um", "Blu\u00b7men", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob es gern die Glut der Farben k\u00fchlte,", "tokens": ["Als", "ob", "es", "gern", "die", "Glut", "der", "Far\u00b7ben", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil sonst das Auge w\u00fcrd' im Glanz erkranken,", "tokens": ["Weil", "sonst", "das", "Au\u00b7ge", "w\u00fcrd'", "im", "Glanz", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn es berauscht im Blumenfeuer w\u00fchlte,", "tokens": ["Wenn", "es", "be\u00b7rauscht", "im", "Blu\u00b7men\u00b7feu\u00b7er", "w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo rote, wei\u00dfe, bunte Strahlen wanken,", "tokens": ["Wo", "ro\u00b7te", ",", "wei\u00b7\u00dfe", ",", "bun\u00b7te", "Strah\u00b7len", "wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht auch im Gr\u00fcn das Licht gemildert f\u00fchlte;", "tokens": ["Nicht", "auch", "im", "Gr\u00fcn", "das", "Licht", "ge\u00b7mil\u00b7dert", "f\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So m\u00f6cht' ich, dich umarmend, M\u00e4rchen weben,", "tokens": ["So", "m\u00f6cht'", "ich", ",", "dich", "um\u00b7ar\u00b7mend", ",", "M\u00e4r\u00b7chen", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPER", "VVPP", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Flammen durch Geschw\u00e4tz zu lindern streben.", "tokens": ["Die", "Flam\u00b7men", "durch", "Ge\u00b7schw\u00e4tz", "zu", "lin\u00b7dern", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Flammen, wo den s\u00fc\u00dfen Tod wir starben,", "tokens": ["Die", "Flam\u00b7men", ",", "wo", "den", "s\u00fc\u00b7\u00dfen", "Tod", "wir", "star\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den du, ein scheues Kind sonst, nimmer scheutest,", "tokens": ["Den", "du", ",", "ein", "scheu\u00b7es", "Kind", "sonst", ",", "nim\u00b7mer", "scheu\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "ADJA", "NN", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit meine Bitten deine Huld erwarben,", "tokens": ["Seit", "mei\u00b7ne", "Bit\u00b7ten", "dei\u00b7ne", "Huld", "er\u00b7war\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sterbend du zum Leben mich erneutest,", "tokens": ["Wo", "ster\u00b7bend", "du", "zum", "Le\u00b7ben", "mich", "er\u00b7neu\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, glaubt' ich schon im \u00dcberflu\u00df zu darben,", "tokens": ["Und", ",", "glaubt'", "ich", "schon", "im", "\u00dc\u00b7berf\u00b7lu\u00df", "zu", "dar\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch Reiz im Reiz dich freuend mich erfreutest:", "tokens": ["Durch", "Reiz", "im", "Reiz", "dich", "freu\u00b7end", "mich", "er\u00b7freu\u00b7test", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PPER", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie werden unsern holden Leib verzehren,", "tokens": ["Sie", "wer\u00b7den", "un\u00b7sern", "hol\u00b7den", "Leib", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn wir nicht ihrem s\u00fc\u00dfen Gifte wehren.", "tokens": ["Wenn", "wir", "nicht", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Gif\u00b7te", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch weil, wo Frag' und Antwort wechselnd spielen,", "tokens": ["Doch", "weil", ",", "wo", "Frag'", "und", "Ant\u00b7wort", "wech\u00b7selnd", "spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lippen bald sich inniger verm\u00e4hlen,", "tokens": ["Die", "Lip\u00b7pen", "bald", "sich", "in\u00b7ni\u00b7ger", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und im Gefl\u00fcster s\u00fc\u00dfe Pfeile zielen;", "tokens": ["Und", "im", "Ge\u00b7fl\u00fcs\u00b7ter", "s\u00fc\u00b7\u00dfe", "Pfei\u00b7le", "zie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So m\u00f6cht' ich andre Stundent\u00e4uschung w\u00e4hlen,", "tokens": ["So", "m\u00f6cht'", "ich", "and\u00b7re", "Stun\u00b7den\u00b7t\u00e4u\u00b7schung", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00fc\u00dft' ich nur, wie sie dir wohlgefielen,", "tokens": ["Und", "w\u00fc\u00dft'", "ich", "nur", ",", "wie", "sie", "dir", "wohl\u00b7ge\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir reizende Geschichtchen neu erz\u00e4hlen,", "tokens": ["Dir", "rei\u00b7zen\u00b7de", "Ge\u00b7schicht\u00b7chen", "neu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die du anh\u00f6rtest, weichlich hingegossen,", "tokens": ["Die", "du", "an\u00b7h\u00f6r\u00b7test", ",", "weich\u00b7lich", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als k\u00e4men s\u00fc\u00df vom Himmel sie geflossen.", "tokens": ["Als", "k\u00e4\u00b7men", "s\u00fc\u00df", "vom", "Him\u00b7mel", "sie", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}