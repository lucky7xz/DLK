{"textgrid.poem.36652": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Monte Cavo", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich vielleicht im Olymp? Hier seh' ich Fabel, Geschichte,", "tokens": ["W\u00e4r'", "ich", "viel\u00b7leicht", "im", "O\u00b7lymp", "?", "Hier", "seh'", "ich", "Fa\u00b7bel", ",", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Rom, Aeneas, Uly\u00df und die homerische See,", "tokens": ["Rom", ",", "A\u00b7e\u00b7neas", ",", "U\u00b7ly\u00df", "und", "die", "ho\u00b7me\u00b7ri\u00b7sche", "See", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ist es der Pegasus, der den Wandrer zum Berge herauftrug?", "tokens": ["Ist", "es", "der", "Pe\u00b7ga\u00b7sus", ",", "der", "den", "Wand\u00b7rer", "zum", "Ber\u00b7ge", "her\u00b7auf\u00b7trug", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-----+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nein! die moderne Welt kommt hier zu Esel herauf.", "tokens": ["Nein", "!", "die", "mo\u00b7der\u00b7ne", "Welt", "kommt", "hier", "zu", "E\u00b7sel", "her\u00b7auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}