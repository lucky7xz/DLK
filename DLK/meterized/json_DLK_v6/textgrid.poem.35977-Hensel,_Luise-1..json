{"textgrid.poem.35977": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der stille Herbst gekommen,", "tokens": ["Nun", "ist", "der", "stil\u00b7le", "Herbst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gr\u00fc\u00dft er mich so hold, so weh.", "tokens": ["Wie", "gr\u00fc\u00dft", "er", "mich", "so", "hold", ",", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat die Blumen hingenommen,", "tokens": ["Er", "hat", "die", "Blu\u00b7men", "hin\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr lieben Blumen, ade!", "tokens": ["Ihr", "lie\u00b7ben", "Blu\u00b7men", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die V\u00f6glein all sind weggezogen,", "tokens": ["Die", "V\u00f6\u00b7glein", "all", "sind", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ahnen schon den kalten Schnee;", "tokens": ["Sie", "ah\u00b7nen", "schon", "den", "kal\u00b7ten", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind zur Heimat hingeflogen,", "tokens": ["Sie", "sind", "zur", "Hei\u00b7mat", "hin\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr holden S\u00e4nger, ade!", "tokens": ["Ihr", "hol\u00b7den", "S\u00e4n\u00b7ger", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonn' ist auch schon hingegangen,", "tokens": ["Die", "Sonn'", "ist", "auch", "schon", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich so einsam hier noch steh'.", "tokens": ["Wie", "ich", "so", "ein\u00b7sam", "hier", "noch", "steh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dahin ist Lust und Sommerprangen,", "tokens": ["Da\u00b7hin", "ist", "Lust", "und", "Som\u00b7mer\u00b7pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du milde Sonne, ade!", "tokens": ["Du", "mil\u00b7de", "Son\u00b7ne", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich will euch gerne scheiden sehen,", "tokens": ["Ich", "will", "euch", "ger\u00b7ne", "schei\u00b7den", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen Sterben, selig Weh!", "tokens": ["Will\u00b7kom\u00b7men", "Ster\u00b7ben", ",", "se\u00b7lig", "Weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald werd' auch ich zur Heimat gehen,", "tokens": ["Bald", "werd'", "auch", "ich", "zur", "Hei\u00b7mat", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann kalte Erde, ade!", "tokens": ["Dann", "kal\u00b7te", "Er\u00b7de", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}