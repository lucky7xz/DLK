{"dta.poem.23827": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Lotharius I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Strich im Teutschen Reich Austrasien genant/", "tokens": ["Ein", "Strich", "im", "Teut\u00b7schen", "Reich", "Aus\u00b7tra\u00b7si\u00b7en", "ge\u00b7nant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rom und Italien/ zusamt der K\u00e4ysers-W\u00fcrde/", "tokens": ["Rom", "und", "I\u00b7ta\u00b7li\u00b7en", "/", "zu\u00b7samt", "der", "K\u00e4y\u00b7ser\u00b7sW\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$(", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ward mir nach hartem Streit zum Erbtheil zu erkant.", "tokens": ["Ward", "mir", "nach", "har\u00b7tem", "Streit", "zum", "E\u00b7rbtheil", "zu", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Purpur ward zuletzt mir eine solche B\u00fcrde/", "tokens": ["Der", "Pur\u00b7pur", "ward", "zu\u00b7letzt", "mir", "ei\u00b7ne", "sol\u00b7che", "B\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich den M\u00fcnchs-Habit im Closter besser fand.", "tokens": ["Da\u00df", "ich", "den", "M\u00fcnchs\u00b7Ha\u00b7bit", "im", "Clos\u00b7ter", "bes\u00b7ser", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}