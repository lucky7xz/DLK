{"textgrid.poem.54246": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es qu\u00e4lte sich mein Geist mit eyfrigen Verlangen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es qu\u00e4lte sich mein Geist mit eyfrigen Verlangen,", "tokens": ["Es", "qu\u00e4l\u00b7te", "sich", "mein", "Geist", "mit", "ey\u00b7fri\u00b7gen", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh mir, beliebter Freund, dein angenehmer Brief,", "tokens": ["Eh", "mir", ",", "be\u00b7lieb\u00b7ter", "Freund", ",", "dein", "an\u00b7ge\u00b7neh\u00b7mer", "Brief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den ich, ich schw\u00f6r es dir, mit rechter Lust empfangen,", "tokens": ["Den", "ich", ",", "ich", "schw\u00f6r", "es", "dir", ",", "mit", "rech\u00b7ter", "Lust", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach langen Hoffen doch noch in die H\u00e4nde lieff.", "tokens": ["Nach", "lan\u00b7gen", "Hof\u00b7fen", "doch", "noch", "in", "die", "H\u00e4n\u00b7de", "lieff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Du m\u00fc\u00dftest, meynt ich, schon im Reich der Toden stecken,", "tokens": ["Du", "m\u00fc\u00df\u00b7test", ",", "meynt", "ich", ",", "schon", "im", "Reich", "der", "To\u00b7den", "ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum schickt ich den Mercur an Charon w\u00fcrcklich ab,", "tokens": ["Drum", "schickt", "ich", "den", "Mer\u00b7cur", "an", "Cha\u00b7ron", "w\u00fcrck\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Um deinen Auffenthalt mir schleunig zu entdecken,", "tokens": ["Um", "dei\u00b7nen", "Auf\u00b7fent\u00b7halt", "mir", "schleu\u00b7nig", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der aber mir von dir gar keine Nachricht gab.", "tokens": ["Der", "a\u00b7ber", "mir", "von", "dir", "gar", "kei\u00b7ne", "Nach\u00b7richt", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn, dacht ich, w\u00e4re noch der liebe Freund am Leben,", "tokens": ["Denn", ",", "dacht", "ich", ",", "w\u00e4\u00b7re", "noch", "der", "lie\u00b7be", "Freund", "am", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So lieffe wohl ein Blatt von seinen H\u00e4nden ein,", "tokens": ["So", "lief\u00b7fe", "wohl", "ein", "Blatt", "von", "sei\u00b7nen", "H\u00e4n\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wolt er selber mir davon nicht Nachricht geben,", "tokens": ["Und", "wolt", "er", "sel\u00b7ber", "mir", "da\u00b7von", "nicht", "Nach\u00b7richt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "PAV", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So w\u00fcrd ein andrer doch an seiner Stelle seyn.", "tokens": ["So", "w\u00fcrd", "ein", "an\u00b7drer", "doch", "an", "sei\u00b7ner", "Stel\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch ich bin nun froh, von deinen Wohl zu h\u00f6ren,", "tokens": ["Je\u00b7doch", "ich", "bin", "nun", "froh", ",", "von", "dei\u00b7nen", "Wohl", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum such ich wiederum erfreut die Leyer vor.", "tokens": ["Drum", "such", "ich", "wie\u00b7de\u00b7rum", "er\u00b7freut", "die", "Le\u00b7yer", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich lasse mich nicht mehr Verdru\u00df und Sorgen st\u00f6hren,", "tokens": ["Ich", "las\u00b7se", "mich", "nicht", "mehr", "Ver\u00b7dru\u00df", "und", "Sor\u00b7gen", "st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und mein Muse hebt ihr mattes Haupt empor.", "tokens": ["Und", "mein", "Mu\u00b7se", "hebt", "ihr", "mat\u00b7tes", "Haupt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Das lieblichste Concert ist mir zu H\u00e4nden kommen,", "tokens": ["Das", "lieb\u00b7lichs\u00b7te", "Con\u00b7cert", "ist", "mir", "zu", "H\u00e4n\u00b7den", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es ist, ich l\u00e4ugne nicht, nach meinen Gout gemacht,", "tokens": ["Es", "ist", ",", "ich", "l\u00e4ug\u00b7ne", "nicht", ",", "nach", "mei\u00b7nen", "Gout", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was Wunder wenn ich es mit Freuden auffgenommen,", "tokens": ["Was", "Wun\u00b7der", "wenn", "ich", "es", "mit", "Freu\u00b7den", "auff\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "PPER", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es klingt als h\u00e4tt H-- di\u00df Kind zur Welt gebracht.", "tokens": ["Es", "klingt", "als", "h\u00e4tt", "H\u00b7", "di\u00df", "Kind", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "TRUNC", "PDS", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.21": {"text": "Der Componiste sey von S\u00fcden, West, und Norden,", "tokens": ["Der", "Com\u00b7po\u00b7nis\u00b7te", "sey", "von", "S\u00fc\u00b7den", ",", "West", ",", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "NE", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So r\u00fchm ich doch die Kunst, die niemand tadlen kan;", "tokens": ["So", "r\u00fchm", "ich", "doch", "die", "Kunst", ",", "die", "nie\u00b7mand", "tad\u00b7len", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich sehe, da\u00df er nicht an dir zum L\u00fcgner worden,", "tokens": ["Ich", "se\u00b7he", ",", "da\u00df", "er", "nicht", "an", "dir", "zum", "L\u00fcg\u00b7ner", "wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und gibt er dir noch mehr, so nimm es willig an.", "tokens": ["Und", "gibt", "er", "dir", "noch", "mehr", ",", "so", "nimm", "es", "wil\u00b7lig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "ADV", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Geitz mu\u00df, denckst du wohl, sie gantz besessen haben,", "tokens": ["Der", "Geitz", "mu\u00df", ",", "denckst", "du", "wohl", ",", "sie", "gantz", "be\u00b7ses\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil sie, dem Fodern nach, gantz uners\u00e4tlich heist.", "tokens": ["Weil", "sie", ",", "dem", "Fo\u00b7dern", "nach", ",", "gantz", "un\u00b7er\u00b7s\u00e4t\u00b7lich", "heist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du weist, da\u00df mich nichts mehr als die Music kan laben,", "tokens": ["Du", "weist", ",", "da\u00df", "mich", "nichts", "mehr", "als", "die", "Mu\u00b7sic", "kan", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "PIS", "KOKOM", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn dieses Element ernehret Seel und Geist.", "tokens": ["Denn", "die\u00b7ses", "E\u00b7le\u00b7ment", "er\u00b7neh\u00b7ret", "Seel", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es kan mir in der That kein gr\u00f6\u00dfrer Dienst geschehen,", "tokens": ["Es", "kan", "mir", "in", "der", "That", "kein", "gr\u00f6\u00df\u00b7rer", "Dienst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als wenn ich, wie du selbst davon kanst Zeuge seyn,", "tokens": ["Als", "wenn", "ich", ",", "wie", "du", "selbst", "da\u00b7von", "kanst", "Zeu\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "$,", "PWAV", "PPER", "ADV", "PAV", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vom lieben Noten-Volck mich soll umringet sehen,", "tokens": ["Vom", "lie\u00b7ben", "No\u00b7ten\u00b7Volck", "mich", "soll", "um\u00b7rin\u00b7get", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VMFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich r\u00e4umte, gieng es an, ihm alle Zimmer ein.", "tokens": ["Ich", "r\u00e4um\u00b7te", ",", "gieng", "es", "an", ",", "ihm", "al\u00b7le", "Zim\u00b7mer", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Indessen geb ich dir mein Beyleid zu verstehen,", "tokens": ["In\u00b7des\u00b7sen", "geb", "ich", "dir", "mein", "Bey\u00b7leid", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil du, wie mir dein Kiel die sichre Nachricht giebt,", "tokens": ["Weil", "du", ",", "wie", "mir", "dein", "Kiel", "die", "sich\u00b7re", "Nach\u00b7richt", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wilst in der Einsamkeit, wie Eremiten, gehen,", "tokens": ["Wilst", "in", "der", "Ein\u00b7sam\u00b7keit", ",", "wie", "E\u00b7re\u00b7mi\u00b7ten", ",", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$,", "PWAV", "NN", "$,", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.36": {"text": "Da du doch ehemahls den Umgang sehr geliebt.", "tokens": ["Da", "du", "doch", "e\u00b7he\u00b7mahls", "den", "Um\u00b7gang", "sehr", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich kan durch Nachsinn nicht, ich schw\u00f6r es dir, erreichen,", "tokens": ["Ich", "kan", "durch", "Nach\u00b7sinn", "nicht", ",", "ich", "schw\u00f6r", "es", "dir", ",", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Warum dein muntrer Geist Gesellschafft fliehen will.", "tokens": ["Wa\u00b7rum", "dein", "mun\u00b7trer", "Geist", "Ge\u00b7sell\u00b7schafft", "flie\u00b7hen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "O d\u00fcrfft ich dich einmal nur heimlich \u00fcberschleichen,", "tokens": ["O", "d\u00fcrfft", "ich", "dich", "ein\u00b7mal", "nur", "heim\u00b7lich", "\u00fc\u00b7bersc\u00b7hlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So f\u00e4nd ich also dann vielleicht das Widerspiel.", "tokens": ["So", "f\u00e4nd", "ich", "al\u00b7so", "dann", "viel\u00b7leicht", "das", "Wi\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wird dir die Zeit zu lang, du kanst sie dir verk\u00fcrtzen,", "tokens": ["Wird", "dir", "die", "Zeit", "zu", "lang", ",", "du", "kanst", "sie", "dir", "ver\u00b7k\u00fcrt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ruff deine Muse nur, sie wird, Geliebter Freund,", "tokens": ["Ruff", "dei\u00b7ne", "Mu\u00b7se", "nur", ",", "sie", "wird", ",", "Ge\u00b7lieb\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "PPER", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die Stunden alle dir mit Zucker \u00fcberw\u00fcrtzen,", "tokens": ["Die", "Stun\u00b7den", "al\u00b7le", "dir", "mit", "Zu\u00b7cker", "\u00fc\u00b7ber\u00b7w\u00fcrt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die deiner Meynung nach recht herb und bitter seyn,", "tokens": ["Die", "dei\u00b7ner", "Mey\u00b7nung", "nach", "recht", "herb", "und", "bit\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und mir wird auch zugleich ein sch\u00f6ner Vortheil bleiben,", "tokens": ["Und", "mir", "wird", "auch", "zu\u00b7gleich", "ein", "sch\u00f6\u00b7ner", "Vor\u00b7theil", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Denn l\u00e4st die Clio sich bey dir gesch\u00e4fftigt sehn,", "tokens": ["Denn", "l\u00e4st", "die", "Clio", "sich", "bey", "dir", "ge\u00b7sch\u00e4ff\u00b7tigt", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "PRF", "APPR", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "So wirst du k\u00fcnfftig hin an mich noch eher schreiben,", "tokens": ["So", "wirst", "du", "k\u00fcnff\u00b7tig", "hin", "an", "mich", "noch", "e\u00b7her", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Als es die Zeit daher zu meinen Schmertz, geschehn", "tokens": ["Als", "es", "die", "Zeit", "da\u00b7her", "zu", "mei\u00b7nen", "Schmertz", ",", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PAV", "APPR", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Dein virtuoser Kiel kan mir viel St\u00e4rcke schencken,", "tokens": ["Dein", "vir\u00b7tu\u00b7o\u00b7ser", "Kiel", "kan", "mir", "viel", "St\u00e4r\u00b7cke", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Du weist, wie schwach und lahm mein Pegasus noch heist,", "tokens": ["Du", "weist", ",", "wie", "schwach", "und", "lahm", "mein", "Pe\u00b7ga\u00b7sus", "noch", "heist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich wei\u00df noch leider! nicht den Ziegel recht zu lencken,", "tokens": ["Ich", "wei\u00df", "noch", "lei\u00b7der", "!", "nicht", "den", "Zie\u00b7gel", "recht", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PTKNEG", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Den Phoebus offt vor Zorn mir aus den H\u00e4nden reist.", "tokens": ["Den", "Phoe\u00b7bus", "offt", "vor", "Zorn", "mir", "aus", "den", "H\u00e4n\u00b7den", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Drum will ich fernerhin von dir viel sch\u00f6nes hoffen,", "tokens": ["Drum", "will", "ich", "fer\u00b7ner\u00b7hin", "von", "dir", "viel", "sch\u00f6\u00b7nes", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "La\u00df deiner Fl\u00f6then Thon mir eine Vorschrifft seyn,", "tokens": ["La\u00df", "dei\u00b7ner", "Fl\u00f6\u00b7then", "Thon", "mir", "ei\u00b7ne", "Vor\u00b7schrifft", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und ist, geschickter Freund, mein W\u00fcnschen eingetroffen,", "tokens": ["Und", "ist", ",", "ge\u00b7schick\u00b7ter", "Freund", ",", "mein", "W\u00fcn\u00b7schen", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "So will ich dir viel Danck vor deine Dienste weyhn.", "tokens": ["So", "will", "ich", "dir", "viel", "Danck", "vor", "dei\u00b7ne", "Diens\u00b7te", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}