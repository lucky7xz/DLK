{"textgrid.poem.45878": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie sich auch die Zeit will wenden, enden]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich auch die Zeit will wenden, enden", "tokens": ["Wie", "sich", "auch", "die", "Zeit", "will", "wen\u00b7den", ",", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will sich nimmer doch die Ferne,", "tokens": ["Will", "sich", "nim\u00b7mer", "doch", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude mag der Mai mir spenden, senden", "tokens": ["Freu\u00b7de", "mag", "der", "Mai", "mir", "spen\u00b7den", ",", "sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00f6cht' Dir alles gerne, weil ich Freude mir erlerne,", "tokens": ["M\u00f6cht'", "Dir", "al\u00b7les", "ger\u00b7ne", ",", "weil", "ich", "Freu\u00b7de", "mir", "er\u00b7ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Wenn Du mit gefaltnen H\u00e4nden", "tokens": ["Wenn", "Du", "mit", "ge\u00b7falt\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig hebst der Augen Sterne.", "tokens": ["Freu\u00b7dig", "hebst", "der", "Au\u00b7gen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Blumen mich nicht gr\u00fc\u00dfen, s\u00fc\u00dfen", "tokens": ["Al\u00b7le", "Blu\u00b7men", "mich", "nicht", "gr\u00fc\u00b7\u00dfen", ",", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "PPER", "PTKNEG", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gru\u00df nehm' ich von Deinem Munde.", "tokens": ["Gru\u00df", "nehm'", "ich", "von", "Dei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was nicht bl\u00fchet Dir zu F\u00fc\u00dfen, b\u00fc\u00dfen", "tokens": ["Was", "nicht", "bl\u00fc\u00b7het", "Dir", "zu", "F\u00fc\u00b7\u00dfen", ",", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PTKNEG", "VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00df es bald zur Stunde, eher ich auch nicht gesunde,", "tokens": ["Mu\u00df", "es", "bald", "zur", "Stun\u00b7de", ",", "e\u00b7her", "ich", "auch", "nicht", "ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "PPER", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Bis Du mir mit frohen K\u00fcssen", "tokens": ["Bis", "Du", "mir", "mit", "fro\u00b7hen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringest meines Fr\u00fchlings Kunde.", "tokens": ["Brin\u00b7gest", "mei\u00b7nes", "Fr\u00fch\u00b7lings", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wen die Abendl\u00fcfte wehen, sehen", "tokens": ["Wen", "die", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "we\u00b7hen", ",", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich die lieben V\u00f6glein kleine", "tokens": ["Mich", "die", "lie\u00b7ben", "V\u00f6\u00b7glein", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig an der Linde stehen, sp\u00e4hen", "tokens": ["Trau\u00b7rig", "an", "der", "Lin\u00b7de", "ste\u00b7hen", ",", "sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NE", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wen ich wohl so ernstlich meine, da\u00df ich helle Tr\u00e4nen weine,", "tokens": ["Wen", "ich", "wohl", "so", "ernst\u00b7lich", "mei\u00b7ne", ",", "da\u00df", "ich", "hel\u00b7le", "Tr\u00e4\u00b7nen", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wollen auch nicht schlafen gehen,", "tokens": ["Wol\u00b7len", "auch", "nicht", "schla\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sonst w\u00e4r' ich ganz alleine.", "tokens": ["Denn", "sonst", "w\u00e4r'", "ich", "ganz", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "V\u00f6glein euch mag's nicht gelingen, klingen", "tokens": ["V\u00f6\u00b7glein", "euch", "mag's", "nicht", "ge\u00b7lin\u00b7gen", ",", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVINF"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Darf es nur von ihrem Sange,", "tokens": ["Darf", "es", "nur", "von", "ih\u00b7rem", "San\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie des Maies Wonneschlingen, singen", "tokens": ["Wie", "des", "Mai\u00b7es", "Won\u00b7ne\u00b7schlin\u00b7gen", ",", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Alles ein in neuem Zwange; aber da\u00df ich Dein verlange", "tokens": ["Al\u00b7les", "ein", "in", "neu\u00b7em", "Zwan\u00b7ge", ";", "a\u00b7ber", "da\u00df", "ich", "Dein", "ver\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "APPR", "ADJA", "NN", "$.", "ADV", "KOUS", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Und Du mein, mu\u00dft Du auch singen,", "tokens": ["Und", "Du", "mein", ",", "mu\u00dft", "Du", "auch", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ach das ist schon ewig lange.", "tokens": ["Ach", "das", "ist", "schon", "e\u00b7wig", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}