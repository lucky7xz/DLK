{"dta.poem.11798": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Versammelt sah ich j\u00fcngst in sommerlicher Stille", "tokens": ["Ver\u00b7sam\u00b7melt", "sah", "ich", "j\u00fcngst", "in", "som\u00b7mer\u00b7li\u00b7cher", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Graspferdchen und Cicad', ein Heimchen und die Grille.", "tokens": ["Gras\u00b7pferd\u00b7chen", "und", "Ci\u00b7cad'", ",", "ein", "Heim\u00b7chen", "und", "die", "Gril\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Mir schienen alle vier sehr \u00e4hnlich, doch nicht gleich,", "tokens": ["Mir", "schie\u00b7nen", "al\u00b7le", "vier", "sehr", "\u00e4hn\u00b7lich", ",", "doch", "nicht", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "CARD", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes r\u00fchmte sich der Lust in seinem Reich.", "tokens": ["Und", "je\u00b7des", "r\u00fchm\u00b7te", "sich", "der", "Lust", "in", "sei\u00b7nem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Graspferdchen, da\u00df es frei k\u00f6nn' \u00fcber Gr\u00e4ser springen,", "tokens": ["Gras\u00b7pferd\u00b7chen", ",", "da\u00df", "es", "frei", "k\u00f6nn'", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ser", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Cicade, da\u00df sie hoch vom Baume k\u00f6nne singen.", "tokens": ["Ci\u00b7ca\u00b7de", ",", "da\u00df", "sie", "hoch", "vom", "Bau\u00b7me", "k\u00f6n\u00b7ne", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Heimchen, da\u00df daheim es sei am trauten Herde,", "tokens": ["Das", "Heim\u00b7chen", ",", "da\u00df", "da\u00b7heim", "es", "sei", "am", "trau\u00b7ten", "Her\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Grille, da\u00df geheim sie wohn' im Spalt der Erde.", "tokens": ["Und", "Gril\u00b7le", ",", "da\u00df", "ge\u00b7heim", "sie", "wohn'", "im", "Spalt", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "ADJD", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich sprach: O da\u00df, wie die in Gras und Laubeskronen,", "tokens": ["Ich", "sprach", ":", "O", "da\u00df", ",", "wie", "die", "in", "Gras", "und", "Lau\u00b7bes\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "KOUS", "$,", "PWAV", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Haus und Feld, vergn\u00fcgt so Menschen k\u00f6nnten wohnen!", "tokens": ["Im", "Haus", "und", "Feld", ",", "ver\u00b7gn\u00fcgt", "so", "Men\u00b7schen", "k\u00f6nn\u00b7ten", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "VVFIN", "ADV", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dann dacht' ich, da\u00df sie sind so friedlicher Geberde,", "tokens": ["Dann", "dacht'", "ich", ",", "da\u00df", "sie", "sind", "so", "fried\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht, da\u00df sie einzeln sind, nicht eine ganze Herde.", "tokens": ["Macht", ",", "da\u00df", "sie", "ein\u00b7zeln", "sind", ",", "nicht", "ei\u00b7ne", "gan\u00b7ze", "Her\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.7": {"line.1": {"text": "Graspferd, Cicade, Grill' und Heimchen, ohne Harm", "tokens": ["Gras\u00b7pferd", ",", "Ci\u00b7ca\u00b7de", ",", "Grill'", "und", "Heim\u00b7chen", ",", "oh\u00b7ne", "Harm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "KOUI", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Jedwedes, dichtgedr\u00e4ngt sind sie ein Heuschreckschwarm.", "tokens": ["Jed\u00b7we\u00b7des", ",", "dicht\u00b7ge\u00b7dr\u00e4ngt", "sind", "sie", "ein", "Heu\u00b7schreck\u00b7schwarm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "VVFIN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}}}}}