{"textgrid.poem.44090": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bin ich nun, du h\u00f6chstes Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bin ich nun, du h\u00f6chstes Wesen,", "tokens": ["So", "bin", "ich", "nun", ",", "du", "h\u00f6chs\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wieder meiner Schulden frey.", "tokens": ["Schon", "wie\u00b7der", "mei\u00b7ner", "Schul\u00b7den", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, las auch aus den Wercken lesen,", "tokens": ["Ach", ",", "las", "auch", "aus", "den", "Wer\u00b7cken", "le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df meine Bu\u00dfe kr\u00e4ftig sey,", "tokens": ["Da\u00df", "mei\u00b7ne", "Bu\u00b7\u00dfe", "kr\u00e4f\u00b7tig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zwinge durch geheime Kraft", "tokens": ["Und", "zwin\u00b7ge", "durch", "ge\u00b7hei\u00b7me", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Unart wilder Leidenschaft.", "tokens": ["Die", "Un\u00b7art", "wil\u00b7der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4st dich gern und willig bitten", "tokens": ["Du", "l\u00e4st", "dich", "gern", "und", "wil\u00b7lig", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rst mit Lust das Abba schreyn;", "tokens": ["Und", "h\u00f6rst", "mit", "Lust", "das", "Ab\u00b7ba", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, richte Wandel, Wuntsch und Sitten", "tokens": ["Ach", ",", "rich\u00b7te", "Wan\u00b7del", ",", "Wunt\u00b7sch", "und", "Sit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach deiner weisen Absicht ein.", "tokens": ["Nach", "dei\u00b7ner", "wei\u00b7sen", "Ab\u00b7sicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Tugend gieb mir \u00dcberflu\u00df,", "tokens": ["An", "Tu\u00b7gend", "gieb", "mir", "\u00dc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Nothdurft, was man haben mu\u00df.", "tokens": ["An", "Noth\u00b7durft", ",", "was", "man", "ha\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast mir die Vernunft gegeben,", "tokens": ["Du", "hast", "mir", "die", "Ver\u00b7nunft", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erleuchte sie durch Geist und Wort", "tokens": ["Er\u00b7leuch\u00b7te", "sie", "durch", "Geist", "und", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchre meinen Fu\u00df im Leben", "tokens": ["Und", "f\u00fch\u00b7re", "mei\u00b7nen", "Fu\u00df", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Gl\u00fcck und Ungl\u00fcck sicher fort,", "tokens": ["Durch", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "si\u00b7cher", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit er nicht durch Thal und H\u00f6h", "tokens": ["Da\u00b7mit", "er", "nicht", "durch", "Thal", "und", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Lust die breite Stra\u00dfe geh.", "tokens": ["Mit", "Lust", "die", "brei\u00b7te", "Stra\u00b7\u00dfe", "geh."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du kennst mein Herz, es ist bescheiden", "tokens": ["Du", "kennst", "mein", "Herz", ",", "es", "ist", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und liebet die Zufriedenheit,", "tokens": ["Und", "lie\u00b7bet", "die", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kan des Nechsten Fehler leiden,", "tokens": ["Es", "kan", "des", "Nechs\u00b7ten", "Feh\u00b7ler", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es dient ihm auch nach M\u00f6gligkeit;", "tokens": ["Es", "dient", "ihm", "auch", "nach", "M\u00f6g\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schwachheit h\u00e4ngt ihm freylich an \u2013", "tokens": ["Die", "Schwach\u00b7heit", "h\u00e4ngt", "ihm", "frey\u00b7lich", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo lebt ein ganz gerechter Mann?", "tokens": ["Wo", "lebt", "ein", "ganz", "ge\u00b7rech\u00b7ter", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hilf du nur mir mich selbst bezwingen", "tokens": ["Hilf", "du", "nur", "mir", "mich", "selbst", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rei\u00df mich mit Gewalt zu dir,", "tokens": ["Und", "rei\u00df", "mich", "mit", "Ge\u00b7walt", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Las, was mir gut ist, wohl gelingen;", "tokens": ["Las", ",", "was", "mir", "gut", "ist", ",", "wohl", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vergeh ich mich, so wincke mir,", "tokens": ["Ver\u00b7geh", "ich", "mich", ",", "so", "win\u00b7cke", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bringt auch dieses keine Frucht,", "tokens": ["Und", "bringt", "auch", "die\u00b7ses", "kei\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So nimm die Ruthe deiner Zucht.", "tokens": ["So", "nimm", "die", "Ru\u00b7the", "dei\u00b7ner", "Zucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Las Heucheley und Aberglauben", "tokens": ["Las", "Heu\u00b7che\u00b7ley", "und", "A\u00b7berg\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir niemahls den Verstand verdrehn,", "tokens": ["Mir", "nie\u00b7mahls", "den", "Ver\u00b7stand", "ver\u00b7drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein redlich Herze von den Tauben,", "tokens": ["Ein", "red\u00b7lich", "Her\u00b7ze", "von", "den", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schlangenwiz die List zu sehn,", "tokens": ["Von", "Schlan\u00b7gen\u00b7wiz", "die", "List", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Womit die ungerechte Welt", "tokens": ["Wo\u00b7mit", "die", "un\u00b7ge\u00b7rech\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Fr\u00f6mmsten Nez und Angel stellt.", "tokens": ["Dem", "Fr\u00f6mms\u00b7ten", "Nez", "und", "An\u00b7gel", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bekehre meinen Feind mit G\u00fcte", "tokens": ["Be\u00b7keh\u00b7re", "mei\u00b7nen", "Feind", "mit", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und las mir keine Rachgier zu;", "tokens": ["Und", "las", "mir", "kei\u00b7ne", "Rach\u00b7gier", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halt Leib und Sinnen und Gem\u00fcthe", "tokens": ["Halt", "Leib", "und", "Sin\u00b7nen", "und", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesund, bey Kr\u00e4ften und in Ruh", "tokens": ["Ge\u00b7sund", ",", "bey", "Kr\u00e4f\u00b7ten", "und", "in", "Ruh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und beuth mir mein bescheiden Theil", "tokens": ["Und", "beuth", "mir", "mein", "be\u00b7schei\u00b7den", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor guten Flei\u00df und Arbeit feil.", "tokens": ["Vor", "gu\u00b7ten", "Flei\u00df", "und", "Ar\u00b7beit", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gef\u00e4llt es deinen Vorsichtsschl\u00fc\u00dfen,", "tokens": ["Ge\u00b7f\u00e4llt", "es", "dei\u00b7nen", "Vor\u00b7sichts\u00b7schl\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gieb mir einen treuen Freund,", "tokens": ["So", "gieb", "mir", "ei\u00b7nen", "treu\u00b7en", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der alles, doch auch mit Gewi\u00dfen,", "tokens": ["Der", "al\u00b7les", ",", "doch", "auch", "mit", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Mit mir wie mit sich selber meint", "tokens": ["Mit", "mir", "wie", "mit", "sich", "sel\u00b7ber", "meint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "APPR", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wenn sich Leid und Freude regt,", "tokens": ["Und", ",", "wenn", "sich", "Leid", "und", "Freu\u00b7de", "regt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00e4lfte mit Vergn\u00fcgung tr\u00e4gt.", "tokens": ["Die", "H\u00e4lf\u00b7te", "mit", "Ver\u00b7gn\u00fc\u00b7gung", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So will ich dir mit treuem Herzen", "tokens": ["So", "will", "ich", "dir", "mit", "treu\u00b7em", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Lebenszeit vertraun", "tokens": ["Die", "gan\u00b7ze", "Le\u00b7bens\u00b7zeit", "ver\u00b7traun"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als ein Christ mit klugem Scherzen", "tokens": ["Und", "als", "ein", "Christ", "mit", "klu\u00b7gem", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Elend dieses Lebens baun,", "tokens": ["Das", "E\u00b7lend", "die\u00b7ses", "Le\u00b7bens", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis da\u00df der Tag, den du bestimmt,", "tokens": ["Bis", "da\u00df", "der", "Tag", ",", "den", "du", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir dieses, was verweslich, nimmt.", "tokens": ["Mir", "die\u00b7ses", ",", "was", "ver\u00b7wes\u00b7lich", ",", "nimmt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PDS", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}