{"dta.poem.15664": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Andacht sprach ich: Gott, ich danke dir, da\u00df du", "tokens": ["Mit", "An\u00b7dacht", "sprach", "ich", ":", "Gott", ",", "ich", "dan\u00b7ke", "dir", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir wandtest diesen Schmerz, mir sandtest diese Ruh.", "tokens": ["Mir", "wand\u00b7test", "die\u00b7sen", "Schmerz", ",", "mir", "sand\u00b7test", "die\u00b7se", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verstand dazwischen sprach: Der Ew'ge sollte wenden", "tokens": ["Ver\u00b7stand", "da\u00b7zwi\u00b7schen", "sprach", ":", "Der", "Ew'\u00b7ge", "soll\u00b7te", "wen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PAV", "VVFIN", "$.", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dich den ew'gen Plan, dir etwas Eignes senden?", "tokens": ["F\u00fcr", "dich", "den", "ew'\u00b7gen", "Plan", ",", "dir", "et\u00b7was", "Eig\u00b7nes", "sen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch Andacht sprach: So dank' ich eben jenem Plan,", "tokens": ["Doch", "An\u00b7dacht", "sprach", ":", "So", "dank'", "ich", "e\u00b7ben", "je\u00b7nem", "Plan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem ewigen, in dem ich mit bin eingethan,", "tokens": ["Dem", "e\u00b7wi\u00b7gen", ",", "in", "dem", "ich", "mit", "bin", "ein\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PRELS", "PPER", "APPR", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem ew'gen Plane, dem gem\u00e4\u00df in diesem Nu", "tokens": ["Dem", "ew'\u00b7gen", "Pla\u00b7ne", ",", "dem", "ge\u00b7m\u00e4\u00df", "in", "die\u00b7sem", "Nu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir ward gewandt der Schmerz, mir ward gesandt die Ruh.", "tokens": ["Mir", "ward", "ge\u00b7wandt", "der", "Schmerz", ",", "mir", "ward", "ge\u00b7sandt", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}