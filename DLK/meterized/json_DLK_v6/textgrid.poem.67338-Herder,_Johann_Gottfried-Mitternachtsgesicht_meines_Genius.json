{"textgrid.poem.67338": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Mitternachtsgesicht meines Genius", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er stand! noch beb' ich, dem ich verwegner Thor,", "tokens": ["Er", "stand", "!", "noch", "beb'", "ich", ",", "dem", "ich", "ver\u00b7weg\u00b7ner", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verwirrt und nachtvoll, Leben und Tod umringt,", "tokens": ["Ver\u00b7wirrt", "und", "nacht\u00b7voll", ",", "Le\u00b7ben", "und", "Tod", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Pochend murrete! \u2013 Mittern\u00e4chte,", "tokens": ["Po\u00b7chend", "mur\u00b7re\u00b7te", "!", "\u2013", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wag' ich die Stimme des R\u00e4chers? Weh!", "tokens": ["Wag'", "ich", "die", "Stim\u00b7me", "des", "R\u00e4\u00b7chers", "?", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "$.", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbmich sandt' (Dein Trotz hat seinen Olymp erst\u00fcrmt),", "tokens": ["\u00bb", "mich", "sandt'", "(", "Dein", "Trotz", "hat", "sei\u00b7nen", "O\u00b7lymp", "er\u00b7st\u00fcrmt", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der, eh Du wardst, tief schauend Aeonen durch,", "tokens": ["Der", ",", "eh", "Du", "wardst", ",", "tief", "schau\u00b7end", "A\u00b7e\u00b7o\u00b7nen", "durch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VAFIN", "$,", "ADJD", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich gew\u00e4hlt zum menschlichen Liebling,", "tokens": ["Dich", "ge\u00b7w\u00e4hlt", "zum", "menschli\u00b7chen", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fleisch aus Staube Dir webt', und sandte", "tokens": ["Fleisch", "aus", "Stau\u00b7be", "Dir", "webt'", ",", "und", "sand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit dem von seinem Feu'rmeer entflossnen Tropf,", "tokens": ["Mit", "dem", "von", "sei\u00b7nem", "Feu'r\u00b7meer", "ent\u00b7floss\u00b7nen", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich zu durchgie\u00dfen, oft den Ersterbenden", "tokens": ["Dich", "zu", "durch\u00b7gie\u00b7\u00dfen", ",", "oft", "den", "E\u00b7rster\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "ART", "NN"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aufzuwecken und zart zu bilden,", "tokens": ["Auf\u00b7zu\u00b7we\u00b7cken", "und", "zart", "zu", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Murrender J\u00fcngling, und Dich zu leiten;", "tokens": ["Mur\u00b7ren\u00b7der", "J\u00fcng\u00b7ling", ",", "und", "Dich", "zu", "lei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Mich, den nur Jova sendet: Dein Genius", "tokens": ["Mich", ",", "den", "nur", "Jo\u00b7va", "sen\u00b7det", ":", "Dein", "Ge\u00b7nius"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "NE", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sei Du Dir (ernsthaft r\u00fchrt' er mein Auge an),", "tokens": ["Sei", "Du", "Dir", "(", "ernst\u00b7haft", "r\u00fchrt'", "er", "mein", "Au\u00b7ge", "an", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$(", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$(", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Licht und Dunkel zu sehn, und Menschheit", "tokens": ["Licht", "und", "Dun\u00b7kel", "zu", "sehn", ",", "und", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Herzhaft zu wagen, und kenn und hab Dich!\u00ab", "tokens": ["Herz\u00b7haft", "zu", "wa\u00b7gen", ",", "und", "kenn", "und", "hab", "Dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "KON", "VAFIN", "PPER", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Da schwand er. Weh mir! F\u00fchrer auf immer mir,", "tokens": ["Da", "schwand", "er", ".", "Weh", "mir", "!", "F\u00fch\u00b7rer", "auf", "im\u00b7mer", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "PPER", "$.", "NN", "APPR", "ADV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem k\u00fchnen Knaben, der, aus den Armen ihm", "tokens": ["Dem", "k\u00fch\u00b7nen", "Kna\u00b7ben", ",", "der", ",", "aus", "den", "Ar\u00b7men", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Losgerissen und gl\u00fchnd im Auge,", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "und", "gl\u00fchnd", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Rennt in den Orcus, ein Sklav! denn ach!", "tokens": ["Rennt", "in", "den", "Or\u00b7cus", ",", "ein", "Sklav", "!", "denn", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "$,", "ART", "NN", "$.", "KON", "XY", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Mein F\u00fcrst ich? \u2013 Scepter, Sklaven, wo seid Ihr denn?", "tokens": ["Mein", "F\u00fcrst", "ich", "?", "\u2013", "Scep\u00b7ter", ",", "Skla\u00b7ven", ",", "wo", "seid", "Ihr", "denn", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$.", "$(", "NN", "$,", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Herz br\u00fcllt Aufruhr; Chaosruinen sind", "tokens": ["Mein", "Herz", "br\u00fcllt", "Auf\u00b7ruhr", ";", "Chaos\u00b7ru\u00b7i\u00b7nen", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Haupt und Busen; der Seufzer schw\u00e4chster", "tokens": ["Haupt", "und", "Bu\u00b7sen", ";", "der", "Seuf\u00b7zer", "schw\u00e4chs\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ART", "NN", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tritt meine Krone zu Staub! Wer sch\u00fctzt,", "tokens": ["Tritt", "mei\u00b7ne", "Kro\u00b7ne", "zu", "Staub", "!", "Wer", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$.", "PWS", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Den Du fliehst, Engel? H\u00f6re! nicht wein' ich Dir,", "tokens": ["Den", "Du", "fliehst", ",", "En\u00b7gel", "?", "H\u00f6\u00b7re", "!", "nicht", "wein'", "ich", "Dir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "NE", "$.", "NE", "$.", "PTKNEG", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Gott ruft! Geh, doch bringe dies Wort vor Gott,", "tokens": ["Den", "Gott", "ruft", "!", "Geh", ",", "doch", "brin\u00b7ge", "dies", "Wort", "vor", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$,", "ADV", "VVFIN", "PDS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Meine Seel' in dem Wort! denn, Seele,", "tokens": ["Mei\u00b7ne", "Seel'", "in", "dem", "Wort", "!", "denn", ",", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "ADV", "$,", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Au\u00dfer ihm g\u00f6ltest Du (wiss' es!) nichts.", "tokens": ["Au\u00b7\u00dfer", "ihm", "g\u00f6l\u00b7test", "Du", "(", "wiss'", "es", "!", ")", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$.", "$(", "PIS", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}