{"dta.poem.182": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er den Ku\u00df erhalten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin begl\u00fcckt nun ich den Ku\u00df genossen/", "tokens": ["Ich", "bin", "be\u00b7gl\u00fcckt", "nun", "ich", "den", "Ku\u00df", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe recht was Sonderlichs geschmeckt/", "tokens": ["Ich", "ha\u00b7be", "recht", "was", "Son\u00b7der\u00b7lichs", "ge\u00b7schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist es doch/ das in den K\u00fcssen steckt?", "tokens": ["Was", "ist", "es", "doch", "/", "das", "in", "den", "K\u00fcs\u00b7sen", "steckt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und woher ist die s\u00fcsse Kost entsprossen?", "tokens": ["Und", "wo\u00b7her", "ist", "die", "s\u00fcs\u00b7se", "Kost", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom Himmel? Ja! mein Geist ist recht erquickt;", "tokens": ["Vom", "Him\u00b7mel", "?", "Ja", "!", "mein", "Geist", "ist", "recht", "er\u00b7quickt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin begl\u00fcckt.", "tokens": ["Ich", "bin", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich dancke dir vor diese G\u00f6tter-Gabe/", "tokens": ["Ich", "dan\u00b7cke", "dir", "vor", "die\u00b7se", "G\u00f6t\u00b7ter\u00b7Ga\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die an dem Werth nicht ihres Gleichen hat", "tokens": ["Die", "an", "dem", "Werth", "nicht", "ih\u00b7res", "Glei\u00b7chen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O s\u00fcsser Ku\u00df! o angenehme That!", "tokens": ["O", "s\u00fcs\u00b7ser", "Ku\u00df", "!", "o", "an\u00b7ge\u00b7neh\u00b7me", "That", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O G\u00f6tter-Lust! die ich versp\u00fchret habe;", "tokens": ["O", "G\u00f6t\u00b7ter\u00b7Lust", "!", "die", "ich", "ver\u00b7sp\u00fch\u00b7ret", "ha\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Brunette h\u00f6r/ mein Hertze spricht in mir", "tokens": ["Bru\u00b7net\u00b7te", "h\u00f6r", "/", "mein", "Hert\u00b7ze", "spricht", "in", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PPOSAT", "VVFIN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich dancke dir.", "tokens": ["Ich", "dan\u00b7cke", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}