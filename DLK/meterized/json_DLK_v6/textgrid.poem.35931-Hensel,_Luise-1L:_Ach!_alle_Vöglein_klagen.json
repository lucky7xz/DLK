{"textgrid.poem.35931": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! alle V\u00f6glein klagen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! alle V\u00f6glein klagen", "tokens": ["Ach", "!", "al\u00b7le", "V\u00f6\u00b7glein", "kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um dich ihr Trauerlied", "tokens": ["Um", "dich", "ihr", "Trau\u00b7er\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Bl\u00fcmlein fragen:", "tokens": ["Und", "al\u00b7le", "Bl\u00fcm\u00b7lein", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warum so fr\u00fch verbl\u00fcht?", "tokens": ["Wa\u00b7rum", "so", "fr\u00fch", "ver\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6r', V\u00f6glein, h\u00f6r' es, Blume:", "tokens": ["H\u00f6r'", ",", "V\u00f6\u00b7glein", ",", "h\u00f6r'", "es", ",", "Blu\u00b7me", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das M\u00e4gdlein ist nicht todt;", "tokens": ["Das", "M\u00e4gd\u00b7lein", "ist", "nicht", "todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es bl\u00fcht im Heiligthume,", "tokens": ["Es", "bl\u00fcht", "im", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem nie ein Winter droht.", "tokens": ["Dem", "nie", "ein", "Win\u00b7ter", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hob zum Himmelsgarten", "tokens": ["Es", "hob", "zum", "Him\u00b7mels\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des G\u00e4rtners Hand empor,", "tokens": ["Des", "G\u00e4rt\u00b7ners", "Hand", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo lichte Englein warten", "tokens": ["Wo", "lich\u00b7te", "En\u00b7glein", "war\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des reinen Kelches Flor.", "tokens": ["Des", "rei\u00b7nen", "Kel\u00b7ches", "Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort soll ich's wiederfinden", "tokens": ["Dort", "soll", "ich's", "wie\u00b7der\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Gottes Angesicht,", "tokens": ["Vor", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sel'ge Kr\u00e4nze winden", "tokens": ["Und", "sel'\u00b7ge", "Kr\u00e4n\u00b7ze", "win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihm im ew'gen Licht.", "tokens": ["Mit", "ihm", "im", "ew'\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}