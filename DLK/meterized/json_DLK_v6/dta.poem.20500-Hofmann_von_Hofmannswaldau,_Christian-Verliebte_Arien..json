{"dta.poem.20500": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es will die ungerathne zeit/", "tokens": ["Es", "will", "die", "un\u00b7ge\u00b7rath\u00b7ne", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich zwey lippen soll verlassen/", "tokens": ["Da\u00df", "ich", "zwey", "lip\u00b7pen", "soll", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da tugend/ lieb und freundlichkeit/", "tokens": ["Da", "tu\u00b7gend", "/", "lieb", "und", "freund\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als treue schwestern sich umfassen/", "tokens": ["Als", "treu\u00b7e", "schwes\u00b7tern", "sich", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sch\u00f6ne rosen stehn/", "tokens": ["Wo", "sch\u00f6\u00b7ne", "ro\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die auch im winter nicht vergehn.", "tokens": ["Die", "auch", "im", "win\u00b7ter", "nicht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kan fast nicht von deiner hand", "tokens": ["Ich", "kan", "fast", "nicht", "von", "dei\u00b7ner", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den schwachen arm zur\u00fccke ziehen/", "tokens": ["Den", "schwa\u00b7chen", "arm", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle wie ein strenges band", "tokens": ["Ich", "f\u00fch\u00b7le", "wie", "ein", "stren\u00b7ges", "band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mich zu f\u00e4sseln will bem\u00fchen/", "tokens": ["Sich", "mich", "zu", "f\u00e4s\u00b7seln", "will", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ohren klingen mir/", "tokens": ["Die", "oh\u00b7ren", "klin\u00b7gen", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Getreuer Damon bleibe hier.", "tokens": ["Ge\u00b7treu\u00b7er", "Da\u00b7mon", "blei\u00b7be", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kenne meine fehler wohl/", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7ne", "feh\u00b7ler", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df die schuldigkeit vermeiden/", "tokens": ["Ich", "mu\u00df", "die", "schul\u00b7dig\u00b7keit", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df da\u00df ich dir folgen soll/", "tokens": ["Ich", "wei\u00df", "da\u00df", "ich", "dir", "fol\u00b7gen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mein verh\u00e4ngni\u00df heist mich scheiden/", "tokens": ["Doch", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "heist", "mich", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einer solchen schuld", "tokens": ["Mit", "ei\u00b7ner", "sol\u00b7chen", "schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat auch die ungedult gedult.", "tokens": ["Hat", "auch", "die", "un\u00b7ge\u00b7dult", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein geist ist dir genug bekandt/", "tokens": ["Mein", "geist", "ist", "dir", "ge\u00b7nug", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du weist ja meine treue sinnen/", "tokens": ["Du", "weist", "ja", "mei\u00b7ne", "treu\u00b7e", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt ich dich auff das gr\u00fcne land/", "tokens": ["H\u00e4tt", "ich", "dich", "auff", "das", "gr\u00fc\u00b7ne", "land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich gew\u00fcnscht/ begleiten k\u00f6nnen/", "tokens": ["Wie", "ich", "ge\u00b7w\u00fcnscht", "/", "be\u00b7glei\u00b7ten", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$(", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So sagt ich gar gewi\u00df/", "tokens": ["So", "sagt", "ich", "gar", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es ist dein feld mein paradi\u00df.", "tokens": ["Es", "ist", "dein", "feld", "mein", "pa\u00b7ra\u00b7di\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ist gleich hand und fu\u00df nicht frey/", "tokens": ["Doch", "ist", "gleich", "hand", "und", "fu\u00df", "nicht", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "KON", "PTKVZ", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wei\u00df mein geist kein band zu nennen/", "tokens": ["So", "wei\u00df", "mein", "geist", "kein", "band", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er reist der zeiten garn entzwey/", "tokens": ["Er", "reist", "der", "zei\u00b7ten", "garn", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht dir freudig nachzurennen;", "tokens": ["Und", "w\u00fcnscht", "dir", "freu\u00b7dig", "nach\u00b7zu\u00b7ren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein geist wird dir allein", "tokens": ["Mein", "geist", "wird", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Viel n\u00e4her als der schatten seyn.", "tokens": ["Viel", "n\u00e4\u00b7her", "als", "der", "schat\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der gipffel/ so sich itzt bewegt/", "tokens": ["Der", "gipf\u00b7fel", "/", "so", "sich", "itzt", "be\u00b7wegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das rauschen so dein ohre sp\u00fcret/", "tokens": ["Das", "rau\u00b7schen", "so", "dein", "oh\u00b7re", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird durch der seuffzer trieb erregt/", "tokens": ["Wird", "durch", "der", "seuff\u00b7zer", "trieb", "er\u00b7regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meine matte zunge f\u00fchret/", "tokens": ["Die", "mei\u00b7ne", "mat\u00b7te", "zun\u00b7ge", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df/ da\u00df iedes blat", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "ie\u00b7des", "blat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit meiner noth erbarmni\u00df hat.", "tokens": ["Mit", "mei\u00b7ner", "noth", "er\u00b7barm\u00b7ni\u00df", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch schwebt erbarmni\u00df auch um dich/", "tokens": ["Doch", "schwebt", "er\u00b7barm\u00b7ni\u00df", "auch", "um", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So brich nicht g\u00e4ntzlich mein gel\u00fccke/", "tokens": ["So", "brich", "nicht", "g\u00e4ntz\u00b7lich", "mein", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und denck in meiner noth auff mich;", "tokens": ["Und", "denck", "in", "mei\u00b7ner", "noth", "auff", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gib mir ein halbes wort zur\u00fccke/", "tokens": ["Gib", "mir", "ein", "hal\u00b7bes", "wort", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein wort so mir beliebt/", "tokens": ["Ein", "wort", "so", "mir", "be\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und wieder neue kr\u00e4ffte giebt.", "tokens": ["Und", "wie\u00b7der", "neu\u00b7e", "kr\u00e4ff\u00b7te", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}