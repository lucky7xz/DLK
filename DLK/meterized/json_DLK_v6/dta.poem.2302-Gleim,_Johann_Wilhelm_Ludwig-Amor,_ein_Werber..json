{"dta.poem.2302": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Werber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Amor wirbt, ich seh ihn werben.", "tokens": ["A\u00b7mor", "wirbt", ",", "ich", "seh", "ihn", "wer\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gesch\u00e4ftig, und wie freundlich", "tokens": ["Wie", "ge\u00b7sch\u00e4f\u00b7tig", ",", "und", "wie", "freund\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KON", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dringt er sich in alle Haufen.", "tokens": ["Dringt", "er", "sich", "in", "al\u00b7le", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch! er ist nicht iedem sichtbar.", "tokens": ["Doch", "!", "er", "ist", "nicht", "ie\u00b7dem", "sicht\u00b7bar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht! ietzt geht er mit spatziren,", "tokens": ["Seht", "!", "ietzt", "geht", "er", "mit", "spat\u00b7zi\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht! ietzt f\u00fchrt er die Geworb\u2019nen", "tokens": ["Seht", "!", "ietzt", "f\u00fchrt", "er", "die", "Ge\u00b7wor\u00b7b'\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.7": {"text": "An den H\u00e4nden treuer Freunde,", "tokens": ["An", "den", "H\u00e4n\u00b7den", "treu\u00b7er", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter Weiden oder Linden;", "tokens": ["Un\u00b7ter", "Wei\u00b7den", "o\u00b7der", "Lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und, gesichert f\u00fcr Verr\u00e4tern,", "tokens": ["Und", ",", "ge\u00b7si\u00b7chert", "f\u00fcr", "Ver\u00b7r\u00e4\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schweren sie zu seiner Fahne.", "tokens": ["Schwe\u00b7ren", "sie", "zu", "sei\u00b7ner", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seht ihn bei den Uberl\u00e4ufern,", "tokens": ["Seht", "ihn", "bei", "den", "U\u00b7berl\u00b7\u00e4u\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seht doch! er bedekkt mit Larven", "tokens": ["Seht", "doch", "!", "er", "be\u00b7dekkt", "mit", "Lar\u00b7ven"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wangen, welche leicht err\u00f6ten,", "tokens": ["Wan\u00b7gen", ",", "wel\u00b7che", "leicht", "er\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und entf\u00fchret sie den W\u00e4chtern,", "tokens": ["Und", "ent\u00b7f\u00fch\u00b7ret", "sie", "den", "W\u00e4ch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und verbirgt sie f\u00fcr Verr\u00e4tern,", "tokens": ["Und", "ver\u00b7birgt", "sie", "f\u00fcr", "Ver\u00b7r\u00e4\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und begleitet sie zum Tanze,", "tokens": ["Und", "be\u00b7glei\u00b7tet", "sie", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und entdekkt sie nur dem T\u00e4nzer,", "tokens": ["Und", "ent\u00b7dekkt", "sie", "nur", "dem", "T\u00e4n\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dem er sie zum Tanze bringet.", "tokens": ["Dem", "er", "sie", "zum", "Tan\u00b7ze", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Graun und Cato ", "tokens": ["Graun", "und", "Ca\u00b7to"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Er bestellt in weissen S\u00e4len", "tokens": ["Er", "be\u00b7stellt", "in", "weis\u00b7sen", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Spieler zu den Spielerinnen,", "tokens": ["Spie\u00b7ler", "zu", "den", "Spie\u00b7le\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "T\u00e4nzerinnen zu den T\u00e4nzern,", "tokens": ["T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "zu", "den", "T\u00e4n\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und Verliebte zu Verliebten;", "tokens": ["Und", "Ver\u00b7lieb\u00b7te", "zu", "Ver\u00b7lieb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und dann wirbt er sich die Besten.", "tokens": ["Und", "dann", "wirbt", "er", "sich", "die", "Bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wenn es ihm an Volke fehlet-", "tokens": ["Wenn", "es", "ihm", "an", "Vol\u00b7ke", "feh\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Darf er keine Trommel r\u00fchren.", "tokens": ["Darf", "er", "kei\u00b7ne", "Trom\u00b7mel", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Alle Strassen voller Schlitten,", "tokens": ["Al\u00b7le", "Stras\u00b7sen", "vol\u00b7ler", "Schlit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Alle S\u00e4le voller Larven,", "tokens": ["Al\u00b7le", "S\u00e4\u00b7le", "vol\u00b7ler", "Lar\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Alle B\u00f6den voller T\u00e4nze,", "tokens": ["Al\u00b7le", "B\u00f6\u00b7den", "vol\u00b7ler", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Alle St\u00fchle voller Andacht,", "tokens": ["Al\u00b7le", "St\u00fch\u00b7le", "vol\u00b7ler", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Alle B\u00e4nke voller Weisen,", "tokens": ["Al\u00b7le", "B\u00e4n\u00b7ke", "vol\u00b7ler", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Alle G\u00e4rten voller Rosen,", "tokens": ["Al\u00b7le", "G\u00e4r\u00b7ten", "vol\u00b7ler", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Alle Ufer klarer B\u00e4che,", "tokens": ["Al\u00b7le", "U\u00b7fer", "kla\u00b7rer", "B\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Alle Logen und Parterren", "tokens": ["Al\u00b7le", "Lo\u00b7gen", "und", "Par\u00b7ter\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Dienen ihm zu Werbepl\u00e4tzen.", "tokens": ["Die\u00b7nen", "ihm", "zu", "Wer\u00b7be\u00b7pl\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Seht! dort f\u00fchrt er die Geworb\u2019nen", "tokens": ["Seht", "!", "dort", "f\u00fchrt", "er", "die", "Ge\u00b7wor\u00b7b'\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.37": {"text": "Durch die Th\u00fcr des Operhauses;", "tokens": ["Durch", "die", "Th\u00fcr", "des", "O\u00b7per\u00b7hau\u00b7ses", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Sagt mir, konnten einst die Preussen", "tokens": ["Sagt", "mir", ",", "konn\u00b7ten", "einst", "die", "Preus\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ihre Riesen besser werben?", "tokens": ["Ih\u00b7re", "Rie\u00b7sen", "bes\u00b7ser", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}