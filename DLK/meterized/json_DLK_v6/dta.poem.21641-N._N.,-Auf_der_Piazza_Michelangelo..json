{"dta.poem.21641": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Piazza Michelangelo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein L\u00e4uten und Weinen", "tokens": ["Es", "ist", "ein", "L\u00e4u\u00b7ten", "und", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Abendstunde im Thale,", "tokens": ["Der", "A\u00b7bends\u00b7tun\u00b7de", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Florenz, von deinen Glocken.", "tokens": ["Flo\u00b7renz", ",", "von", "dei\u00b7nen", "Glo\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gluthvoll sah ich verscheinen", "tokens": ["Gluth\u00b7voll", "sah", "ich", "ver\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "An Wolken die Purpurmale", "tokens": ["An", "Wol\u00b7ken", "die", "Pur\u00b7pur\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Sonne. Tieferschrocken", "tokens": ["Der", "Son\u00b7ne", ".", "Tie\u00b7fer\u00b7schro\u00b7cken"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vereinsamt schweigt die Seele.", "tokens": ["Ver\u00b7ein\u00b7samt", "schweigt", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Was will dies L\u00e4uten und Locken?", "tokens": ["Was", "will", "dies", "L\u00e4u\u00b7ten", "und", "Lo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was will dies tr\u00e4umende Rufen", "tokens": ["Was", "will", "dies", "tr\u00e4u\u00b7men\u00b7de", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PDS", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Sehnsuchtsmelodieen?", "tokens": ["In", "Sehn\u00b7suchts\u00b7me\u00b7lo\u00b7di\u00b7e\u00b7en", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich seh mich heimw\u00e4rts ziehen", "tokens": ["Ich", "seh", "mich", "heim\u00b7w\u00e4rts", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinan, wie auf geweihter Tempel Stufen!", "tokens": ["Hi\u00b7nan", ",", "wie", "auf", "ge\u00b7weih\u00b7ter", "Tem\u00b7pel", "Stu\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und st\u00fcnd\u2019 in dunklem Drange", "tokens": ["Und", "st\u00fcnd'", "in", "dunk\u00b7lem", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich suchend auf der H\u00f6he", "tokens": ["Ich", "su\u00b7chend", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Appenins in Wolken stumm und bange!", "tokens": ["Des", "Ap\u00b7pe\u00b7nins", "in", "Wol\u00b7ken", "stumm", "und", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschw\u00e4nde nun versinkend", "tokens": ["Ver\u00b7schw\u00e4n\u00b7de", "nun", "ver\u00b7sin\u00b7kend"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dort hinter deinem Scheitel", "tokens": ["Dort", "hin\u00b7ter", "dei\u00b7nem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Mondlicht milde blinkend!", "tokens": ["Im", "Mond\u00b7licht", "mil\u00b7de", "blin\u00b7kend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verschollen w\u00e4r\u2019 dies L\u00e4uten,", "tokens": ["Ver\u00b7schol\u00b7len", "w\u00e4r'", "dies", "L\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Melodisch aller T\u00f6ne fernes Singen \u2014", "tokens": ["Me\u00b7lo\u00b7disch", "al\u00b7ler", "T\u00f6\u00b7ne", "fer\u00b7nes", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Tonlos durch Wolken ringen", "tokens": ["Ton\u00b7los", "durch", "Wol\u00b7ken", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "M\u00fc\u00dft\u2019 ich bei dir zu sein.", "tokens": ["M\u00fc\u00dft'", "ich", "bei", "dir", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wer kann den Pfad mir deuten?", "tokens": ["Wer", "kann", "den", "Pfad", "mir", "deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich seh mich weiter schreiten", "tokens": ["Ich", "seh", "mich", "wei\u00b7ter", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Auf Bergesh\u00f6h\u2019n erscheinen", "tokens": ["Auf", "Ber\u00b7ges\u00b7h\u00f6h'n", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und wieder nieder wandern,", "tokens": ["Und", "wie\u00b7der", "nie\u00b7der", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In stummen Seligkeiten", "tokens": ["In", "stum\u00b7men", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mit dir mich zu vereinen", "tokens": ["Mit", "dir", "mich", "zu", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und niemals wieder einsam dich zu lassen.", "tokens": ["Und", "nie\u00b7mals", "wie\u00b7der", "ein\u00b7sam", "dich", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Wiedersehn, Umfassen \u2014", "tokens": ["Im", "Wie\u00b7der\u00b7sehn", ",", "Um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was will mein Herz mir stocken?", "tokens": ["Was", "will", "mein", "Herz", "mir", "sto\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verstummt sind deine Glocken", "tokens": ["Ver\u00b7stummt", "sind", "dei\u00b7ne", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Florenz, im dunklen Thale \u2014", "tokens": ["Flo\u00b7renz", ",", "im", "dunk\u00b7len", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und ach, wie j\u00e4h erschrocken", "tokens": ["Und", "ach", ",", "wie", "j\u00e4h", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bin ganz vereinsamt ich zum andren Male! \u2014", "tokens": ["Bin", "ganz", "ver\u00b7ein\u00b7samt", "ich", "zum", "an\u00b7dren", "Ma\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}