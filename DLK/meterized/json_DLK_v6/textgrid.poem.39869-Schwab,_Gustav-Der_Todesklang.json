{"textgrid.poem.39869": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Todesklang", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht an Finnlands Gr\u00e4nzen", "tokens": ["Es", "steht", "an", "Finn\u00b7lands", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein festes Schlo\u00df erbaut,", "tokens": ["Ein", "fes\u00b7tes", "Schlo\u00df", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das in des Mondes Gl\u00e4nzen", "tokens": ["Das", "in", "des", "Mon\u00b7des", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit \u00fcber die Lande schaut.", "tokens": ["Weit", "\u00fc\u00b7ber", "die", "Lan\u00b7de", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu F\u00fc\u00dfen ihm in Eile", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "ihm", "in", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schie\u00dft bodenlos ein Flu\u00df,", "tokens": ["Schie\u00dft", "bo\u00b7den\u00b7los", "ein", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man h\u00f6rt auf eine Meile", "tokens": ["Man", "h\u00f6rt", "auf", "ei\u00b7ne", "Mei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fernher den rauschenden Gru\u00df.", "tokens": ["Fern\u00b7her", "den", "rau\u00b7schen\u00b7den", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Dort ist seit alten Tagen", "tokens": ["Dort", "ist", "seit", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oft schon in stiller Nacht", "tokens": ["Oft", "schon", "in", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Klingen, ein s\u00fc\u00dfes Klagen", "tokens": ["Ein", "Klin\u00b7gen", ",", "ein", "s\u00fc\u00b7\u00dfes", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hellmahnend aufgewacht.", "tokens": ["Hell\u00b7mah\u00b7nend", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wem gilt es wieder heute?", "tokens": ["Wem", "gilt", "es", "wie\u00b7der", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist ein m\u00e4chtiger Tro\u00df,", "tokens": ["Es", "ist", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Hauptmann und seine Leute,", "tokens": ["Der", "Haupt\u00b7mann", "und", "sei\u00b7ne", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gekommen als Wach' auf's Schlo\u00df.", "tokens": ["Ge\u00b7kom\u00b7men", "als", "Wach'", "auf's", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Hauptmann in dem Saale", "tokens": ["Der", "Haupt\u00b7mann", "in", "dem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liegt schon im halben Schlaf,", "tokens": ["Liegt", "schon", "im", "hal\u00b7ben", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gek\u00fc\u00dft vom Mondenstrale", "tokens": ["Ge\u00b7k\u00fc\u00dft", "vom", "Mon\u00b7dens\u00b7tra\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein sch\u00f6ner, schwedischer Graf.", "tokens": ["Ein", "sch\u00f6\u00b7ner", ",", "schwe\u00b7di\u00b7scher", "Graf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "\u00bbwer drunten musiciret,", "tokens": ["\u00bb", "wer", "drun\u00b7ten", "mu\u00b7si\u00b7ci\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und spielt so sch\u00f6ne St\u00fcck'?", "tokens": ["Und", "spielt", "so", "sch\u00f6\u00b7ne", "St\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer mir zu Ehren r\u00fchret", "tokens": ["Wer", "mir", "zu", "Eh\u00b7ren", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So holde Feldmusik?\u00ab", "tokens": ["So", "hol\u00b7de", "Feld\u00b7mu\u00b7sik", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die T\u00f6n' ach! die dich laden,", "tokens": ["Die", "T\u00f6n'", "ach", "!", "die", "dich", "la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ITJ", "$.", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spielt einer, den du nicht kennst:", "tokens": ["Spielt", "ei\u00b7ner", ",", "den", "du", "nicht", "kennst", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich seh's wohl tauchen und baden", "tokens": ["Ich", "seh's", "wohl", "tau\u00b7chen", "und", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Flusse das Gespenst.", "tokens": ["Im", "Flus\u00b7se", "das", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbes tragen mich die Laute", "tokens": ["\u00bb", "es", "tra\u00b7gen", "mich", "die", "Lau\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Fl\u00fcgeln in's Schwedenland,", "tokens": ["Auf", "Fl\u00fc\u00b7geln", "in's", "Schwe\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort sitzt die Liebe, die Traute,", "tokens": ["Dort", "sitzt", "die", "Lie\u00b7be", ",", "die", "Trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was st\u00fctzt sie sich auf die Hand?\u00ab", "tokens": ["Was", "st\u00fctzt", "sie", "sich", "auf", "die", "Hand", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie m\u00f6chte wohl sitzen in Thr\u00e4nen,", "tokens": ["Sie", "m\u00f6ch\u00b7te", "wohl", "sit\u00b7zen", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "War' ihr dein Loos bekannt!", "tokens": ["Wa\u00b7r'", "ihr", "dein", "Loos", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Du fliegest auf diesen T\u00f6nen", "tokens": ["Du", "flie\u00b7gest", "auf", "die\u00b7sen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ein gar ander Land!", "tokens": ["In", "ein", "gar", "an\u00b7der", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Schlafe schon spricht der Knabe;", "tokens": ["Im", "Schla\u00b7fe", "schon", "spricht", "der", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbnun, glaub' ich, schlummr' ich ein!", "tokens": ["\u00bb", "nun", ",", "glaub'", "ich", ",", "schlumm\u00b7r'", "ich", "ein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mir tr\u00e4umt', ich l\u00e4g' im Grabe,", "tokens": ["Mir", "tr\u00e4umt'", ",", "ich", "l\u00e4g'", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich z\u00f6g' in Himmel ein!\u00ab", "tokens": ["Ich", "z\u00f6g'", "in", "Him\u00b7mel", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ach Tr\u00e4umen ist es nimmer,", "tokens": ["Ach", "Tr\u00e4u\u00b7men", "ist", "es", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du junger Schwedengraf!", "tokens": ["Du", "jun\u00b7ger", "Schwe\u00b7den\u00b7graf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Haupt im Mondenschimmer", "tokens": ["Dein", "Haupt", "im", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Neigt sich zum ewigen Schlaf.", "tokens": ["Neigt", "sich", "zum", "e\u00b7wi\u00b7gen", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Der Geist taucht unter in Schweigen,", "tokens": ["Der", "Geist", "taucht", "un\u00b7ter", "in", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er hat sein Werk vollbracht;", "tokens": ["Er", "hat", "sein", "Werk", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wellen fallen und steigen,", "tokens": ["Die", "Wel\u00b7len", "fal\u00b7len", "und", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Flu\u00df rauscht durch die Nacht.", "tokens": ["Der", "Flu\u00df", "rauscht", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}