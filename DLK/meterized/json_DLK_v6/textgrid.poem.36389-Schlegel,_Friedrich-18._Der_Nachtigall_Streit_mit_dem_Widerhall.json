{"textgrid.poem.36389": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18. Der Nachtigall Streit mit dem Widerhall", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, s\u00fc\u00dfe Nachtigall,", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfe", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommst mir zur rechten Stunde;", "tokens": ["Kommst", "mir", "zur", "rech\u00b7ten", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durchdring' die Luft mit rechtem Schall,", "tokens": ["Durch\u00b7dring'", "die", "Luft", "mit", "rech\u00b7tem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Klag', ruf aus tiefem Grunde.", "tokens": ["Klag'", ",", "ruf", "aus", "tie\u00b7fem", "Grun\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ruf ihn, ruf ihn, o Schwesterlein,", "tokens": ["Ruf", "ihn", ",", "ruf", "ihn", ",", "o", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Den Liebsten zu mir lade;", "tokens": ["Den", "Liebs\u00b7ten", "zu", "mir", "la\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hilf treulich mir, sieh meine Pein,", "tokens": ["Hilf", "treu\u00b7lich", "mir", ",", "sieh", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich in Tr\u00e4nen bade.", "tokens": ["Wie", "ich", "in", "Tr\u00e4\u00b7nen", "ba\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Schwester mein, sing s\u00fc\u00df und rein,", "tokens": ["Ach", "Schwes\u00b7ter", "mein", ",", "sing", "s\u00fc\u00df", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPOSAT", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Liebsten ruf mit Namen;", "tokens": ["Den", "Liebs\u00b7ten", "ruf", "mit", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann kurz, dann lang, zieh den Gesang,", "tokens": ["Dann", "kurz", ",", "dann", "lang", ",", "zieh", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "All' T\u00f6ne nimm zusammen.", "tokens": ["All'", "T\u00f6\u00b7ne", "nimm", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Scheint wohl, sie mich verstanden hat,", "tokens": ["Scheint", "wohl", ",", "sie", "mich", "ver\u00b7stan\u00b7den", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meisterin in W\u00e4lden;", "tokens": ["Die", "Meis\u00b7te\u00b7rin", "in", "W\u00e4l\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr's allbereit geht wohl von Statt,", "tokens": ["Ih\u00b7r's", "all\u00b7be\u00b7reit", "geht", "wohl", "von", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die T\u00f6nlein schon sich melden.", "tokens": ["Die", "T\u00f6n\u00b7lein", "schon", "sich", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun noch einmal, mit starkem Schall", "tokens": ["Nun", "noch", "ein\u00b7mal", ",", "mit", "star\u00b7kem", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie den Gesang erhebet,", "tokens": ["Sie", "den", "Ge\u00b7sang", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil Widerhall, aus gr\u00fcnem Tal", "tokens": ["Weil", "Wi\u00b7der\u00b7hall", ",", "aus", "gr\u00fc\u00b7nem", "Tal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich entgegen strebet.", "tokens": ["Freund\u00b7lich", "ent\u00b7ge\u00b7gen", "stre\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die Nachtigall den Schall nicht kennt,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "den", "Schall", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie h\u00e4lt's f\u00fcr ihr Gespielen;", "tokens": ["Sie", "h\u00e4lt's", "f\u00fcr", "ihr", "Ge\u00b7spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwundert sich, wie sie behend", "tokens": ["Ver\u00b7wun\u00b7dert", "sich", ",", "wie", "sie", "be\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So gleichen Ton erzielen.", "tokens": ["So", "glei\u00b7chen", "Ton", "er\u00b7zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Erst bleibt sie stumm, schl\u00e4gt wiederum,", "tokens": ["Erst", "bleibt", "sie", "stumm", ",", "schl\u00e4gt", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denkt ihr bald obzusiegen;", "tokens": ["Denkt", "ihr", "bald", "ob\u00b7zu\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch Widerhall, mit gleichem Schall,", "tokens": ["Doch", "Wi\u00b7der\u00b7hall", ",", "mit", "glei\u00b7chem", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein T\u00f6nlein l\u00e4\u00dft verschwiegen.", "tokens": ["Kein", "T\u00f6n\u00b7lein", "l\u00e4\u00dft", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun steiget auf die Nachtigall,", "tokens": ["Nun", "stei\u00b7get", "auf", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf je mehr und mehre;", "tokens": ["Hoch", "auf", "je", "mehr", "und", "meh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich folget auch der Widerhall,", "tokens": ["Gleich", "fol\u00b7get", "auch", "der", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann's schon noch h\u00f6her w\u00e4re.", "tokens": ["Wann's", "schon", "noch", "h\u00f6\u00b7her", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So recht, geliebte Nachtigall,", "tokens": ["So", "recht", ",", "ge\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du jenem Schall nicht weiche;", "tokens": ["Du", "je\u00b7nem", "Schall", "nicht", "wei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So recht, du treuer Widerhall,", "tokens": ["So", "recht", ",", "du", "treu\u00b7er", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich stets mit ihr vergleiche.", "tokens": ["Dich", "stets", "mit", "ihr", "ver\u00b7glei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.10": {"line.1": {"text": "Zur sch\u00f6nen Wett', nun beide tret',", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Wett'", ",", "nun", "bei\u00b7de", "tret'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft Jesu Nam' erklingen;", "tokens": ["La\u00dft", "Je\u00b7su", "Nam'", "er\u00b7klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann schon im Streit der Schw\u00e4chste heut", "tokens": ["Wann", "schon", "im", "Streit", "der", "Schw\u00e4chs\u00b7te", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tod sich m\u00fc\u00dft' erringen.", "tokens": ["Den", "Tod", "sich", "m\u00fc\u00dft'", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun hoch und immer h\u00f6her schl\u00e4gt", "tokens": ["Nun", "hoch", "und", "im\u00b7mer", "h\u00f6\u00b7her", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachtigall reich an Stimmen;", "tokens": ["Nach\u00b7ti\u00b7gall", "reich", "an", "Stim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Doch Widerhall, so fort erregt,", "tokens": ["Doch", "Wi\u00b7der\u00b7hall", ",", "so", "fort", "er\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df ihr wohl nachzuklimmen.", "tokens": ["Wei\u00df", "ihr", "wohl", "nach\u00b7zu\u00b7klim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nun sammelt Atem sie und Blut", "tokens": ["Nun", "sam\u00b7melt", "A\u00b7tem", "sie", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem sch\u00f6nen Kriege,", "tokens": ["In", "die\u00b7sem", "sch\u00f6\u00b7nen", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will noch mit letzter Kraft und Mut", "tokens": ["Will", "noch", "mit", "letz\u00b7ter", "Kraft", "und", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im einz'gen Liede siegen.", "tokens": ["Im", "einz'\u00b7gen", "Lie\u00b7de", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ach da bricht ihr das mut'ge Herz,", "tokens": ["Ach", "da", "bricht", "ihr", "das", "mut'\u00b7ge", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gesang und Ton verschwinden,", "tokens": ["Ge\u00b7sang", "und", "Ton", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wie die helle leuchtend' Kerz'", "tokens": ["So", "wie", "die", "hel\u00b7le", "leuch\u00b7tend'", "Ker\u00b7z'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erlischt im heft'gen Winde.", "tokens": ["Er\u00b7lischt", "im", "heft'\u00b7gen", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O muntre Kerz', o mutig Herz!", "tokens": ["O", "mun\u00b7tre", "Ker\u00b7z'", ",", "o", "mu\u00b7tig", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl, bist du gleich gestorben,", "tokens": ["Wohl", ",", "bist", "du", "gleich", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lorbeerkron, im letzten Ton,", "tokens": ["Die", "Lor\u00b7beer\u00b7kron", ",", "im", "letz\u00b7ten", "Ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dennoch hast erworben.", "tokens": ["Du", "den\u00b7noch", "hast", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Weil du ein Seufzerlein gar zart", "tokens": ["Weil", "du", "ein", "Seuf\u00b7zer\u00b7lein", "gar", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tod hast lassen klingen,", "tokens": ["Im", "Tod", "hast", "las\u00b7sen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sanft, da\u00df es dein Widerpart", "tokens": ["So", "sanft", ",", "da\u00df", "es", "dein", "Wi\u00b7der\u00b7part"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit nichten mocht' erzwingen.", "tokens": ["Mit", "nich\u00b7ten", "mocht'", "er\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Verzage nicht, dein ist der Sieg,", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", ",", "dein", "ist", "der", "Sieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kr\u00e4nzlein dir geb\u00fchret;", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "dir", "ge\u00b7b\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dich allein, von Bl\u00fcmelein", "tokens": ["F\u00fcr", "dich", "al\u00b7lein", ",", "von", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich's schon fein gezieret.", "tokens": ["Hab", "ich's", "schon", "fein", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ade, geliebte Nachtigall,", "tokens": ["A\u00b7de", ",", "ge\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vom bleichen Tod entf\u00e4rbet;", "tokens": ["Vom", "blei\u00b7chen", "Tod", "ent\u00b7f\u00e4r\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun liegst du da im gr\u00fcnen Tal,", "tokens": ["Nun", "liegst", "du", "da", "im", "gr\u00fc\u00b7nen", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sag, wer dein Stimmlein erbet?", "tokens": ["Sag", ",", "wer", "dein", "Stimm\u00b7lein", "er\u00b7bet", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "K\u00f6nnt' es nicht sein, es w\u00fcrde mein?", "tokens": ["K\u00f6nnt'", "es", "nicht", "sein", ",", "es", "w\u00fcr\u00b7de", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VAINF", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott, k\u00f6nnt' ich es erben!", "tokens": ["O", "Gott", ",", "k\u00f6nnt'", "ich", "es", "er\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt' singen st\u00e4t, so fr\u00fch als sp\u00e4t,", "tokens": ["Wollt'", "sin\u00b7gen", "st\u00e4t", ",", "so", "fr\u00fch", "als", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVFIN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis im Gesang t\u00e4t sterben.", "tokens": ["Bis", "im", "Ge\u00b7sang", "t\u00e4t", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}