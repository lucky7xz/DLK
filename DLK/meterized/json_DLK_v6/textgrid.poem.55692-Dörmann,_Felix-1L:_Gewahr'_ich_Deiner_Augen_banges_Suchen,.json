{"textgrid.poem.55692": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewahr' ich Deiner Augen banges Suchen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewahr' ich Deiner Augen banges Suchen,", "tokens": ["Ge\u00b7wahr'", "ich", "Dei\u00b7ner", "Au\u00b7gen", "ban\u00b7ges", "Su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr liebevolles Hangen an den meinen,", "tokens": ["Ihr", "lie\u00b7be\u00b7vol\u00b7les", "Han\u00b7gen", "an", "den", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So steigt in mir empor ein wildes Weinen,", "tokens": ["So", "steigt", "in", "mir", "em\u00b7por", "ein", "wil\u00b7des", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mein und Dein Geschick m\u00f6cht' ich verfluchen.", "tokens": ["Und", "mein", "und", "Dein", "Ge\u00b7schick", "m\u00f6cht'", "ich", "ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, Du bist mir grenzenlos ergeben,", "tokens": ["Ich", "wei\u00df", ",", "Du", "bist", "mir", "gren\u00b7zen\u00b7los", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jede Stunde zeigt es mir aufs neue,", "tokens": ["Und", "je\u00b7de", "Stun\u00b7de", "zeigt", "es", "mir", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch verbittert diese Liebestreue,", "tokens": ["Und", "doch", "ver\u00b7bit\u00b7tert", "die\u00b7se", "Lie\u00b7be\u00b7streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So r\u00fchrend sch\u00f6n und s\u00fc\u00df, mein ganzes Leben.", "tokens": ["So", "r\u00fch\u00b7rend", "sch\u00f6n", "und", "s\u00fc\u00df", ",", "mein", "gan\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu trautem Gl\u00fcck willst Du mein Sein beschr\u00e4nken,", "tokens": ["Zu", "trau\u00b7tem", "Gl\u00fcck", "willst", "Du", "mein", "Sein", "be\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein stilles Leben soll ich bei Dir f\u00fchren \u2013", "tokens": ["Ein", "stil\u00b7les", "Le\u00b7ben", "soll", "ich", "bei", "Dir", "f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine tiefste Wollust ist: zu Denken,", "tokens": ["Und", "mei\u00b7ne", "tiefs\u00b7te", "Wol\u00b7lust", "ist", ":", "zu", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dem R\u00e4tsel unsres Daseins nachzusp\u00fcren.", "tokens": ["Dem", "R\u00e4t\u00b7sel", "uns\u00b7res", "Da\u00b7seins", "nach\u00b7zu\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann Dir nicht mein Ich als Mitgift schenken,", "tokens": ["Ich", "kann", "Dir", "nicht", "mein", "Ich", "als", "Mit\u00b7gift", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um stillvergn\u00fcgt im Erdenstaub zu sch\u00fcren.", "tokens": ["Um", "still\u00b7ver\u00b7gn\u00fcgt", "im", "Er\u00b7dens\u00b7taub", "zu", "sch\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}