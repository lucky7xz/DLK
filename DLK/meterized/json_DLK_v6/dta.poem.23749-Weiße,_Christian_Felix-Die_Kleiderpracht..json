{"dta.poem.23749": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Kleiderpracht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Tulipanen prangen sch\u00f6n", "tokens": ["Tu\u00b7li\u00b7pa\u00b7nen", "pran\u00b7gen", "sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Farben, die sie schm\u00fccken;", "tokens": ["In", "den", "Far\u00b7ben", ",", "die", "sie", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch man l\u00e4\u00dft sie traurig stehn,", "tokens": ["Doch", "man", "l\u00e4\u00dft", "sie", "trau\u00b7rig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sie sonst durch nichts entz\u00fccken.", "tokens": ["Da", "sie", "sonst", "durch", "nichts", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aller Kleider Herrlichkeit", "tokens": ["Al\u00b7ler", "Klei\u00b7der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag sich auch ein Geck verschaffen;", "tokens": ["Mag", "sich", "auch", "ein", "Geck", "ver\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man verkennt im bunten Kleid", "tokens": ["Man", "ver\u00b7kennt", "im", "bun\u00b7ten", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nicht den geputzten Affen.", "tokens": ["Doch", "nicht", "den", "ge\u00b7putz\u00b7ten", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}