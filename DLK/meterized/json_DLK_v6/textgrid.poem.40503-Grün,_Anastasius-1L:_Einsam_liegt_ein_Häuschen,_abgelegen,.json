{"textgrid.poem.40503": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam liegt ein H\u00e4uschen, abgelegen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam liegt ein H\u00e4uschen, abgelegen,", "tokens": ["Ein\u00b7sam", "liegt", "ein", "H\u00e4usc\u00b7hen", ",", "ab\u00b7ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hart am Meer, das an die W\u00e4nde braust,", "tokens": ["Hart", "am", "Meer", ",", "das", "an", "die", "W\u00e4n\u00b7de", "braust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df sie ewig zitternd sich bewegen,", "tokens": ["Da\u00df", "sie", "e\u00b7wig", "zit\u00b7ternd", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie so manches Herz, das drinnen haust.", "tokens": ["Wie", "so", "man\u00b7ches", "Herz", ",", "das", "drin\u00b7nen", "haust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dieses niedre Pf\u00f6rtlein, will's nicht deuten,", "tokens": ["Die\u00b7ses", "nied\u00b7re", "Pf\u00f6rt\u00b7lein", ",", "will's", "nicht", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df nur Niedres ungehemmt hier zieht,", "tokens": ["Da\u00df", "nur", "Nie\u00b7dres", "un\u00b7ge\u00b7hemmt", "hier", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch der Reinheit Kranz, beim Dr\u00fcberschreiten,", "tokens": ["Doch", "der", "Rein\u00b7heit", "Kranz", ",", "beim", "Dr\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leicht vom Haupt sich abstreift und verbl\u00fcht?", "tokens": ["Leicht", "vom", "Haupt", "sich", "ab\u00b7streift", "und", "ver\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PRF", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ein Tempel ist's, der S\u00fcnd' erschlossen!", "tokens": ["Denn", "ein", "Tem\u00b7pel", "ist's", ",", "der", "S\u00fcnd'", "er\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und doch seht, wie gl\u00e4nzt das Fr\u00fchroth drauf,", "tokens": ["Und", "doch", "seht", ",", "wie", "gl\u00e4nzt", "das", "Fr\u00fch\u00b7roth", "drauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df er, wie aus reinem Gold gegossen,", "tokens": ["Da\u00df", "er", ",", "wie", "aus", "rei\u00b7nem", "Gold", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ragt als heil'ger Sonnentempel auf!", "tokens": ["Ragt", "als", "heil'\u00b7ger", "Son\u00b7nen\u00b7tem\u00b7pel", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Horch, des schmalen Fensters Fl\u00fcgel klingen!", "tokens": ["Horch", ",", "des", "schma\u00b7len", "Fens\u00b7ters", "Fl\u00fc\u00b7gel", "klin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und es blickt mit welkem Busenstrau\u00df,", "tokens": ["Und", "es", "blickt", "mit", "wel\u00b7kem", "Bu\u00b7sen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fahlem Kranz und schlaffen Lockenringen", "tokens": ["Fah\u00b7lem", "Kranz", "und", "schlaf\u00b7fen", "Lo\u00b7cken\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Priest'rin dieses Doms heraus.", "tokens": ["Ei\u00b7ne", "Priest'\u00b7rin", "die\u00b7ses", "Doms", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Bla\u00df sind ihrer Wangen kalte Fl\u00e4chen,", "tokens": ["Bla\u00df", "sind", "ih\u00b7rer", "Wan\u00b7gen", "kal\u00b7te", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie des Richters wei\u00dfes Pergament,", "tokens": ["Wie", "des", "Rich\u00b7ters", "wei\u00b7\u00dfes", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das des Schuldigen geheimst Verbrechen", "tokens": ["Das", "des", "Schul\u00b7di\u00b7gen", "ge\u00b7heimst", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und zugleich sein strenges Urtheil nennt.", "tokens": ["Und", "zu\u00b7gleich", "sein", "stren\u00b7ges", "Ur\u00b7theil", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wie so matt die tr\u00fcben Augen schimmern,", "tokens": ["Wie", "so", "matt", "die", "tr\u00fc\u00b7ben", "Au\u00b7gen", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fast wie Kerzen, \u00fcber Nacht gebrannt,", "tokens": ["Fast", "wie", "Ker\u00b7zen", ",", "\u00fc\u00b7ber", "Nacht", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die nun k\u00e4rglich fahl und m\u00fcde flimmern,", "tokens": ["Die", "nun", "k\u00e4rg\u00b7lich", "fahl", "und", "m\u00fc\u00b7de", "flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seit der goldgelockte Tag erstand.", "tokens": ["Seit", "der", "gold\u00b7ge\u00b7lock\u00b7te", "Tag", "er\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Blumen prangen dort in bunten Farben,", "tokens": ["Blu\u00b7men", "pran\u00b7gen", "dort", "in", "bun\u00b7ten", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die begie\u00dft sie jetzt, da\u00df fort sie bl\u00fchn;", "tokens": ["Die", "be\u00b7gie\u00dft", "sie", "jetzt", ",", "da\u00df", "fort", "sie", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PTKVZ", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn im Herzen schon die Blumen starben,", "tokens": ["Wenn", "im", "Her\u00b7zen", "schon", "die", "Blu\u00b7men", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "L\u00e4\u00dft man gern sie vor den Fenstern gl\u00fchn.", "tokens": ["L\u00e4\u00dft", "man", "gern", "sie", "vor", "den", "Fens\u00b7tern", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Zwischen Rosen, Ampeln, Engelch\u00f6ren", "tokens": ["Zwi\u00b7schen", "Ro\u00b7sen", ",", "Am\u00b7peln", ",", "En\u00b7gelc\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Steht ein Bild der Himmelsk\u00f6nigin;", "tokens": ["Steht", "ein", "Bild", "der", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dort der ew'gen Lampe Gluth zu n\u00e4hren,", "tokens": ["Dort", "der", "ew'\u00b7gen", "Lam\u00b7pe", "Gluth", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bringt sie Oel, wie Vesta's Priesterin!", "tokens": ["Bringt", "sie", "O\u00b7el", ",", "wie", "Ve\u00b7sta's", "Pries\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PWAV", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Neue Blumen geht sie jetzt zu pfl\u00fccken,", "tokens": ["Neu\u00b7e", "Blu\u00b7men", "geht", "sie", "jetzt", "zu", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zwei Gewinde f\u00fcgt sie t\u00e4ndelnd draus,", "tokens": ["Zwei", "Ge\u00b7win\u00b7de", "f\u00fcgt", "sie", "t\u00e4n\u00b7delnd", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einen Kranz, Mariens Haupt zu schm\u00fccken,", "tokens": ["Ei\u00b7nen", "Kranz", ",", "Ma\u00b7ri\u00b7ens", "Haupt", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr sich selbst dann einen Blumenstrau\u00df.", "tokens": ["F\u00fcr", "sich", "selbst", "dann", "ei\u00b7nen", "Blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Scheint's nicht reinstes Hochgef\u00fchl des Weibes,", "tokens": ["Scheint's", "nicht", "reins\u00b7tes", "Hoch\u00b7ge\u00b7f\u00fchl", "des", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das so arglos hier mit Kr\u00e4nzen spielt,", "tokens": ["Das", "so", "arg\u00b7los", "hier", "mit", "Kr\u00e4n\u00b7zen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weil es selbst den Schoo\u00df des eignen Leibes", "tokens": ["Weil", "es", "selbst", "den", "Schoo\u00df", "des", "eig\u00b7nen", "Lei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einen Heiland werth zu tragen f\u00fchlt?", "tokens": ["Ei\u00b7nen", "Hei\u00b7land", "werth", "zu", "tra\u00b7gen", "f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "K\u00fcnstlich schminkt sie nun die blassen Wangen,", "tokens": ["K\u00fcnst\u00b7lich", "schminkt", "sie", "nun", "die", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und doch nenn' ich Schamroth dieses Roth,", "tokens": ["Und", "doch", "nenn'", "ich", "Scham\u00b7roth", "die\u00b7ses", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn sie l\u00e4\u00dft es auf dem Antlitz prangen,", "tokens": ["Denn", "sie", "l\u00e4\u00dft", "es", "auf", "dem", "Ant\u00b7litz", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, aus Scham, da\u00df es so bla\u00df und todt!", "tokens": ["Ach", ",", "aus", "Scham", ",", "da\u00df", "es", "so", "bla\u00df", "und", "todt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Nun das ros'ge Haupt sie la\u00df und lose", "tokens": ["Nun", "das", "ros'\u00b7ge", "Haupt", "sie", "la\u00df", "und", "lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die wei\u00dfen H\u00e4nde niederbeugt,", "tokens": ["In", "die", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de", "nie\u00b7der\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Scheint's nicht eine m\u00fcde Purpurrose,", "tokens": ["Scheint's", "nicht", "ei\u00b7ne", "m\u00fc\u00b7de", "Pur\u00b7pur\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf zwei Nachbarlilien hingeneigt!", "tokens": ["Auf", "zwei", "Nach\u00b7bar\u00b7li\u00b7li\u00b7en", "hin\u00b7ge\u00b7neigt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und so starrt sie schweigend in die Welle,", "tokens": ["Und", "so", "starrt", "sie", "schwei\u00b7gend", "in", "die", "Wel\u00b7l