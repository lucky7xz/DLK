trochaic.penta"}, "line.2": {"text": "Von der Paradiesesrose: Weib,", "tokens": ["Von", "der", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7ro\u00b7se", ":", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00fcmmer nur vom sch\u00f6nsten Heiligthume,", "tokens": ["Tr\u00fcm\u00b7mer", "nur", "vom", "sch\u00f6ns\u00b7ten", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, ein tiefgefallen s\u00fcndig Weib!", "tokens": ["Ach", ",", "ein", "tief\u00b7ge\u00b7fal\u00b7len", "s\u00fcn\u00b7dig", "Weib", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Und doch k\u00f6nnt' ich knieen hier und beten,", "tokens": ["Und", "doch", "k\u00f6nnt'", "ich", "kni\u00b7e\u00b7en", "hier", "und", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie vor Heil'gen beten, weinen hier!", "tokens": ["Wie", "vor", "Heil'\u00b7gen", "be\u00b7ten", ",", "wei\u00b7nen", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVINF", "$,", "PRELS", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Rose liegt am Weg zertreten,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "liegt", "am", "Weg", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "