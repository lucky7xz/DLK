{"textgrid.poem.56840": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun hast auch du, mein Herze,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hast auch du, mein Herze,", "tokens": ["Nun", "hast", "auch", "du", ",", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$,", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dein gro\u00dfes Liebesleid,", "tokens": ["dein", "gro\u00b7\u00dfes", "Lie\u00b7be\u00b7sleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "nun bist auch du vom Schmerze", "tokens": ["nun", "bist", "auch", "du", "vom", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "gesegnet und geweiht.", "tokens": ["ge\u00b7seg\u00b7net", "und", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von heut ab wird dein Klagen", "tokens": ["Von", "heut", "ab", "wird", "dein", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PTKVZ", "VAFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "nicht t\u00e4ndeln mehr wie einst,", "tokens": ["nicht", "t\u00e4n\u00b7deln", "mehr", "wie", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PIS", "KOKOM", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und auch dein sch\u00f6nstes Sagen", "tokens": ["und", "auch", "dein", "sch\u00f6ns\u00b7tes", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wird sein, als ob du weinst.", "tokens": ["wird", "sein", ",", "als", "ob", "du", "weinst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAINF", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}