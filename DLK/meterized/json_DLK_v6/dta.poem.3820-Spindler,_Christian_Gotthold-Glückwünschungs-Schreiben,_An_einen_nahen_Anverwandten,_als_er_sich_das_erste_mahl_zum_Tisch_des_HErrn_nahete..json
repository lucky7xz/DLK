{"dta.poem.3820": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungs-Schreiben,  \n An einen nahen Anverwandten, als er sich das erste mahl zum  \n Tisch des HErrn nahete.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Heut speiset dich, o Freund! der allertreuste", "tokens": ["Heut", "spei\u00b7set", "dich", ",", "o", "Freund", "!", "der", "al\u00b7ler\u00b7treus\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirth,", "tokens": ["Wirth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Heut speiset dich das Lamm, das Lamm ist selbst der", "tokens": ["Heut", "spei\u00b7set", "dich", "das", "Lamm", ",", "das", "Lamm", "ist", "selbst", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hirt;", "tokens": ["Hirt", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Dein JEsus, so dir l\u00e4ngst die Seligkeit erworben,", "tokens": ["Dein", "Je\u00b7sus", ",", "so", "dir", "l\u00e4ngst", "die", "Se\u00b7lig\u00b7keit", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der am verfluchten Holtz vor deine Schuld gestorbe\u0303,", "tokens": ["Der", "am", "ver\u00b7fluch\u00b7ten", "Holtz", "vor", "dei\u00b7ne", "Schuld", "ge\u00b7stor\u00b7b\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Der Heyland speiset dich mit seinem Him\u0303el-Brod,", "tokens": ["Der", "Hey\u00b7land", "spei\u00b7set", "dich", "mit", "sei\u00b7nem", "Him\u0303el\u00b7Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Es tr\u00e4ncket dich sein Blut; Heut ist sein Creutz und", "tokens": ["Es", "tr\u00e4n\u00b7cket", "dich", "sein", "Blut", ";", "Heut", "ist", "sein", "Creutz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Tod.", "tokens": ["Tod", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Freund! deines Hertzens Lust; dich wird kein Feind", "tokens": ["Freund", "!", "dei\u00b7nes", "Hert\u00b7zens", "Lust", ";", "dich", "wird", "kein", "Feind"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "mehr schrecken,", "tokens": ["mehr", "schre\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Wie lieblich mu\u00df dir doch das s\u00fcsse Man\u0303a schmecken!", "tokens": ["Wie", "lieb\u00b7lich", "mu\u00df", "dir", "doch", "das", "s\u00fcs\u00b7se", "Ma\u00f1a", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Sein Blut, sein theures Blut, bringt dir erw\u00fcnsch-", "tokens": ["Sein", "Blut", ",", "sein", "theu\u00b7res", "Blut", ",", "bringt", "dir", "er\u00b7w\u00fcn\u00b7sch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "te Ruh,", "tokens": ["te", "Ruh", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Nunmehro eign es dir im wahren Glauben zu.", "tokens": ["Nun\u00b7meh\u00b7ro", "eign", "es", "dir", "im", "wah\u00b7ren", "Glau\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Heut wird dir JEsu Leib zum erstenmahl gegeben,", "tokens": ["Heut", "wird", "dir", "Je\u00b7su", "Leib", "zum", "ers\u00b7ten\u00b7mahl", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "APPRART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Durch diese neue Krafft erneuerst du dein Leben,", "tokens": ["Durch", "die\u00b7se", "neu\u00b7e", "Krafft", "er\u00b7neu\u00b7erst", "du", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es \u00e4ndert nun sein Blut den bi\u00dfher frechen Geist,", "tokens": ["Es", "\u00e4n\u00b7dert", "nun", "sein", "Blut", "den", "bi\u00df\u00b7her", "fre\u00b7chen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der dich dem S\u00fcnden-Koth der Laster-Brut ent-", "tokens": ["Der", "dich", "dem", "S\u00fcn\u00b7den\u00b7Koth", "der", "Las\u00b7ter\u00b7Brut", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "rei\u00dft.", "tokens": ["rei\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Nun hast du, h\u00f6chst begl\u00fcckt, ein ruhiges Gewissen,", "tokens": ["Nun", "hast", "du", ",", "h\u00f6chst", "be\u00b7gl\u00fcckt", ",", "ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Du must den S\u00fcnden-Lauff bey der Erneurung", "tokens": ["Du", "must", "den", "S\u00fcn\u00b7den\u00b7Lauff", "bey", "der", "Er\u00b7neu\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "schliessen.", "tokens": ["schlies\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Nun fliehst du h\u00f6chst vergn\u00fcgt der S\u00fcnden Dienst-", "tokens": ["Nun", "fliehst", "du", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "der", "S\u00fcn\u00b7den", "Dienst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "barkeit,", "tokens": ["bar\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Nunmehro lebest du in Glaubens-Sicherheit.", "tokens": ["Nun\u00b7meh\u00b7ro", "le\u00b7best", "du", "in", "Glau\u00b7bens\u00b7Si\u00b7cher\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dein JEsus liebet dich mit z\u00e4rtlichen Erbarmen,", "tokens": ["Dein", "Je\u00b7sus", "lie\u00b7bet", "dich", "mit", "z\u00e4rt\u00b7li\u00b7chen", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du ruhst an seiner Brust, in seinen Liebes-Armen.", "tokens": ["Du", "ruhst", "an", "sei\u00b7ner", "Brust", ",", "in", "sei\u00b7nen", "Lie\u00b7bes\u00b7Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nun bist du ja sein Kind, nun deckt dich seine Huld,", "tokens": ["Nun", "bist", "du", "ja", "sein", "Kind", ",", "nun", "deckt", "dich", "sei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nun senckt er in das Meer die grosse S\u00fcnden-", "tokens": ["Nun", "senckt", "er", "in", "das", "Meer", "die", "gros\u00b7se", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Schuld.", "tokens": ["Schuld", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Nur dencke du auch stets an deines JEsu Leiden,", "tokens": ["Nur", "den\u00b7cke", "du", "auch", "stets", "an", "dei\u00b7nes", "Je\u00b7su", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Aus diesem Spiegel lern die freche Bo\u00dfheit meiden,", "tokens": ["Aus", "die\u00b7sem", "Spie\u00b7gel", "lern", "die", "fre\u00b7che", "Bo\u00df\u00b7heit", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Fleuch allen Laster-Pfad, o! s\u00fcndige nicht mehr,", "tokens": ["Fleuch", "al\u00b7len", "Las\u00b7ter\u00b7Pfad", ",", "o", "!", "s\u00fcn\u00b7di\u00b7ge", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "FM", "$.", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des H\u00f6chsten Vaters Zorn entbren\u0303t alsdenn zu sehr;", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Va\u00b7ters", "Zorn", "ent\u00b7bre\u00f1t", "als\u00b7denn", "zu", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn schrecket uns sein Grimm, und sein gerechtes", "tokens": ["Denn", "schre\u00b7cket", "uns", "sein", "Grimm", ",", "und", "sein", "ge\u00b7rech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NE", "$,", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Dr\u00e4uen,", "tokens": ["Dr\u00e4u\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Drum lerne auf der Welt dich von der Welt be-", "tokens": ["Drum", "ler\u00b7ne", "auf", "der", "Welt", "dich", "von", "der", "Welt", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "freyen.", "tokens": ["frey\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Ach folge GOttes Wort und des Erl\u00f6sers Rath,", "tokens": ["Ach", "fol\u00b7ge", "Got\u00b7tes", "Wort", "und", "des", "Er\u00b7l\u00f6\u00b7sers", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja, t\u00e4glich flehe du, wie dorten David that:", "tokens": ["Ja", ",", "t\u00e4g\u00b7lich", "fle\u00b7he", "du", ",", "wie", "dor\u00b7ten", "Da\u00b7vid", "that", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ach! schaffe doch in mir, o GOtt! ein reines Hertze,", "tokens": ["Ach", "!", "schaf\u00b7fe", "doch", "in", "mir", ",", "o", "Gott", "!", "ein", "rei\u00b7nes", "Hert\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Damit ich nicht mein Wohl durchs Zeitliche ver-", "tokens": ["Da\u00b7mit", "ich", "nicht", "mein", "Wohl", "durchs", "Zeit\u00b7li\u00b7che", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "schertze.", "tokens": ["schert\u00b7ze", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "So wachse denn an Zucht und stiller Erbarkeit,", "tokens": ["So", "wach\u00b7se", "denn", "an", "Zucht", "und", "stil\u00b7ler", "Er\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und Christi theures Blut sey stets dein Ehren-Kleid.", "tokens": ["Und", "Chris\u00b7ti", "theu\u00b7res", "Blut", "sey", "stets", "dein", "Eh\u00b7ren\u00b7Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So kan den festesten, den starck und treusten Glauben", "tokens": ["So", "kan", "den", "fes\u00b7tes\u00b7ten", ",", "den", "starck", "und", "treus\u00b7ten", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "$,", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der Satan, nein, die Welt, nein, keine Feinde", "tokens": ["Der", "Sa\u00b7tan", ",", "nein", ",", "die", "Welt", ",", "nein", ",", "kei\u00b7ne", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "ART", "NN", "$,", "PTKANT", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "rauben.", "tokens": ["rau\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.50": {"text": "Er ists, der, was noch fehlt, dir reichlich geben kan,", "