{"textgrid.poem.65457": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "30. Weltlust", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer Buhlerin vergleichbar ist die s\u00fc\u00dfe Lust der Welt,", "tokens": ["Ei\u00b7ner", "Buh\u00b7le\u00b7rin", "ver\u00b7gleich\u00b7bar", "ist", "die", "s\u00fc\u00b7\u00dfe", "Lust", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die in jeder Hand erhoben eine goldne Schlinge h\u00e4lt.", "tokens": ["Die", "in", "je\u00b7der", "Hand", "er\u00b7ho\u00b7ben", "ei\u00b7ne", "gold\u00b7ne", "Schlin\u00b7ge", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVPP", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mit der einen hat sie eben ihren Liebling aufgeh\u00e4ngt,", "tokens": ["Mit", "der", "ei\u00b7nen", "hat", "sie", "e\u00b7ben", "ih\u00b7ren", "Lieb\u00b7ling", "auf\u00b7ge\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "W\u00e4hrend sie mit ihrer andern sich ein neues Herzchen f\u00e4ngt.", "tokens": ["W\u00e4h\u00b7rend", "sie", "mit", "ih\u00b7rer", "an\u00b7dern", "sich", "ein", "neu\u00b7es", "Herz\u00b7chen", "f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}