{"dta.poem.14789": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist zu wehe zum Weinen \u2014", "tokens": ["Mir", "ist", "zu", "we\u00b7he", "zum", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Uhr so gleichf\u00f6rmig pickt,", "tokens": ["Die", "Uhr", "so", "gleich\u00b7f\u00f6r\u00b7mig", "pickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das R\u00e4dleiu, das schnurrt so in einem,", "tokens": ["Das", "R\u00e4d\u00b7lei\u00b7u", ",", "das", "schnurrt", "so", "in", "ei\u00b7nem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "ADV", "APPR", "ART", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir ist, als w\u00e4r' ich verr\u00fcckt. \u2014", "tokens": ["Mir", "ist", ",", "als", "w\u00e4r'", "ich", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}