{"textgrid.poem.56091": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: O was mu\u00df es die Engel gekostet haben,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O was mu\u00df es die Engel gekostet haben,", "tokens": ["O", "was", "mu\u00df", "es", "die", "En\u00b7gel", "ge\u00b7kos\u00b7tet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PPER", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "nicht aufzusingen pl\u00f6tzlich, wie man aufweint,", "tokens": ["nicht", "auf\u00b7zu\u00b7sin\u00b7gen", "pl\u00f6tz\u00b7lich", ",", "wie", "man", "auf\u00b7weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIZU", "ADJD", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "da sie doch wu\u00dften: in dieser Nacht wird dem Knaben", "tokens": ["da", "sie", "doch", "wu\u00df\u00b7ten", ":", "in", "die\u00b7ser", "Nacht", "wird", "dem", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$.", "APPR", "PDAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die Mutter geboren, dem Einen, der bald erscheint.", "tokens": ["die", "Mut\u00b7ter", "ge\u00b7bo\u00b7ren", ",", "dem", "Ei\u00b7nen", ",", "der", "bald", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ART", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Schwingend verschwiegen sie sich und zeigten die Richtung,", "tokens": ["Schwin\u00b7gend", "ver\u00b7schwie\u00b7gen", "sie", "sich", "und", "zeig\u00b7ten", "die", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "wo, allein, das Geh\u00f6ft lag des Joachim,", "tokens": ["wo", ",", "al\u00b7lein", ",", "das", "Ge\u00b7h\u00f6ft", "lag", "des", "Joa\u00b7ch\u00b7im", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "$,", "ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ach, sie f\u00fchlten in sich und im Raum die reine Verdichtung,", "tokens": ["ach", ",", "sie", "f\u00fchl\u00b7ten", "in", "sich", "und", "im", "Raum", "die", "rei\u00b7ne", "Ver\u00b7dich\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "PPER", "VVFIN", "APPR", "PRF", "KON", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "aber es durfte keiner nieder zu ihm.", "tokens": ["a\u00b7ber", "es", "durf\u00b7te", "kei\u00b7ner", "nie\u00b7der", "zu", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "PTKVZ", "APPR", "PPER", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Denn die beiden waren schon so au\u00dfer sich vor Getue.", "tokens": ["Denn", "die", "bei\u00b7den", "wa\u00b7ren", "schon", "so", "au\u00b7\u00dfer", "sich", "vor", "Ge\u00b7tue", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "VAFIN", "ADV", "ADV", "APPR", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Eine Nachbarin kam und klugte und wu\u00dfte nicht wie,", "tokens": ["Ei\u00b7ne", "Nach\u00b7ba\u00b7rin", "kam", "und", "klug\u00b7te", "und", "wu\u00df\u00b7te", "nicht", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "KON", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "und der Alte, vorsichtig, ging und verhielt das Gemuhe", "tokens": ["und", "der", "Al\u00b7te", ",", "vor\u00b7sich\u00b7tig", ",", "ging", "und", "ver\u00b7hielt", "das", "Ge\u00b7mu\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "einer dunkelen Kuh. Denn so war es noch nie.", "tokens": ["ei\u00b7ner", "dun\u00b7ke\u00b7len", "Kuh", ".", "Denn", "so", "war", "es", "noch", "nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}}}}