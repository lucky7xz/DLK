{"textgrid.poem.24527": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Gedancken als einer Doctor Medicin\u00e6 ward", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer klug und m\u00e4\u00dfig lebt/ braucht keiner Artzeney/", "tokens": ["Wer", "klug", "und", "m\u00e4\u00b7\u00dfig", "lebt", "/", "braucht", "kei\u00b7ner", "Art\u00b7ze\u00b7ney", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVFIN", "$(", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Natur an sich hat man nichts einzugeben.", "tokens": ["Und", "der", "Na\u00b7tur", "an", "sich", "hat", "man", "nichts", "ein\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VAFIN", "PIS", "PIS", "VVIZU", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kein Artzt kan in der Welt bey lauter Tugend leben/", "tokens": ["Kein", "Artzt", "kan", "in", "der", "Welt", "bey", "lau\u00b7ter", "Tu\u00b7gend", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Laster machen ihm nur eine Wohnung frey.", "tokens": ["Die", "Las\u00b7ter", "ma\u00b7chen", "ihm", "nur", "ei\u00b7ne", "Woh\u00b7nung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch Fehler lebet er/ und die sind mancherley:", "tokens": ["Durch", "Feh\u00b7ler", "le\u00b7bet", "er", "/", "und", "die", "sind", "man\u00b7cher\u00b7ley", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "KON", "PDS", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Freund/ Er ist gemacht vor welche ", "tokens": ["Mein", "Freund", "/", "Er", "ist", "ge\u00b7macht", "vor", "wel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "VVPP", "APPR", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Den allen stehet Er nunmehro trefflich bey.", "tokens": ["Den", "al\u00b7len", "ste\u00b7het", "Er", "nun\u00b7meh\u00b7ro", "treff\u00b7lich", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jedoch erz\u00fcrne nicht/ sein Leben zuzubringen", "tokens": ["Je\u00b7doch", "er\u00b7z\u00fcr\u00b7ne", "nicht", "/", "sein", "Le\u00b7ben", "zu\u00b7zu\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$(", "PPOSAT", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit welchen/ die verliebt/ die gern was gutes schlingen/", "tokens": ["Mit", "wel\u00b7chen", "/", "die", "ver\u00b7liebt", "/", "die", "gern", "was", "gu\u00b7tes", "schlin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$(", "ART", "VVPP", "$(", "ART", "ADV", "PWS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit der ", "tokens": ["Mit", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Ist ein geschickter Artzt in jeden Fall gerecht.", "tokens": ["Ist", "ein", "ge\u00b7schick\u00b7ter", "Artzt", "in", "je\u00b7den", "Fall", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Vortheil ist nicht klein: aus Krancken und Gesunden", "tokens": ["Der", "Vor\u00b7theil", "ist", "nicht", "klein", ":", "aus", "Kran\u00b7cken", "und", "Ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird theils sein Schatz gesucht/ theils seine Lust gefunden.", "tokens": ["Wird", "theils", "sein", "Schatz", "ge\u00b7sucht", "/", "theils", "sei\u00b7ne", "Lust", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}