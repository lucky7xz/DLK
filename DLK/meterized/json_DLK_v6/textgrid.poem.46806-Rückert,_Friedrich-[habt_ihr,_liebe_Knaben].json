{"textgrid.poem.46806": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[habt ihr, liebe Knaben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr, liebe Knaben,", "tokens": ["Habt", "ihr", ",", "lie\u00b7be", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wohl um eure Gaben", "tokens": ["Wohl", "um", "eu\u00b7re", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zweimal schon gebeten?", "tokens": ["Zwei\u00b7mal", "schon", "ge\u00b7be\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und seid gar betreten,", "tokens": ["Und", "seid", "gar", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df ich schwieg \u2013 unhuldig?", "tokens": ["Da\u00df", "ich", "schwieg", "\u2013", "un\u00b7hul\u00b7dig", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Seid nicht ungeduldig!", "tokens": ["Seid", "nicht", "un\u00b7ge\u00b7dul\u00b7dig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00f6ret, was mich st\u00f6rte:", "tokens": ["H\u00f6\u00b7ret", ",", "was", "mich", "st\u00f6r\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Eben wars' als h\u00f6rte", "tokens": ["E\u00b7ben", "war\u00b7s'", "als", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOKOM", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Ich dort ein Geflister", "tokens": ["Ich", "dort", "ein", "Ge\u00b7flis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Euerer Geschwister,", "tokens": ["Eu\u00b7e\u00b7rer", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Jener, die dort oben", "tokens": ["Je\u00b7ner", ",", "die", "dort", "o\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PDAT", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Mir sind aufgehoben.", "tokens": ["Mir", "sind", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Und ich mu\u00dfte sehen,", "tokens": ["Und", "ich", "mu\u00df\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Was sie mochten flehen;", "tokens": ["Was", "sie", "moch\u00b7ten", "fle\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Doch ich sah an ihnen", "tokens": ["Doch", "ich", "sah", "an", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Nur zufriedne Mienen,", "tokens": ["Nur", "zu\u00b7fried\u00b7ne", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "L\u00e4chelnde Geberde,", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.18": {"text": "Die mich sanft zur Erde", "tokens": ["Die", "mich", "sanft", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Wies zur\u00fcck: Wir haben", "tokens": ["Wies", "zu\u00b7r\u00fcck", ":", "Wir", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Mutter, unsre Gaben;", "tokens": ["Mut\u00b7ter", ",", "uns\u00b7re", "Ga\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Geh und gib nur jenen,", "tokens": ["Geh", "und", "gib", "nur", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVIMP", "ADV", "PDS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Die danach sich sehnen,", "tokens": ["Die", "da\u00b7nach", "sich", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "PRF", "VVINF", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Gieb nur deinen vieren", "tokens": ["Gieb", "nur", "dei\u00b7nen", "vie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Unser Theil zu ihren.", "tokens": ["Un\u00b7ser", "Theil", "zu", "ih\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}