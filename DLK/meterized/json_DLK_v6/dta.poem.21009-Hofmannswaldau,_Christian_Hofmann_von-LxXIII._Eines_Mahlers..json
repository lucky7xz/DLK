{"dta.poem.21009": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIII.  \n Eines Mahlers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kunstrie\u00df meiner Hand ziert manches F\u00fcrsten", "tokens": ["Der", "Kuns\u00b7trie\u00df", "mei\u00b7ner", "Hand", "ziert", "man\u00b7ches", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00e4tze/", "tokens": ["Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Doch f\u00e4llt er durch den Spruch der himmlischen", "tokens": ["Doch", "f\u00e4llt", "er", "durch", "den", "Spruch", "der", "himm\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gesetze.", "tokens": ["Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Taffel frist der Wurm/ mein Mahlwerck frist", "tokens": ["Die", "Taf\u00b7fel", "frist", "der", "Wurm", "/", "mein", "Mahl\u00b7werck", "frist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Zeit/", "tokens": ["die", "Zeit", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Hier wird der Mahler selbst ein Bild der Sterblig-", "tokens": ["Hier", "wird", "der", "Mah\u00b7ler", "selbst", "ein", "Bild", "der", "Ster\u00b7blig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "keit.", "tokens": ["keit", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}}}}