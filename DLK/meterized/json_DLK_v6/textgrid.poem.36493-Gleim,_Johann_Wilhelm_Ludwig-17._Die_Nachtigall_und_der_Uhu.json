{"textgrid.poem.36493": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "17. Die Nachtigall und der Uhu", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall sang ihre sch\u00f6nsten Lieder,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "sang", "ih\u00b7re", "sch\u00f6ns\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem hohen Berg', einmal;", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", "Ber\u00b7g'", ",", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und s\u00fc\u00dfe T\u00f6ne fielen nieder", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "T\u00f6\u00b7ne", "fie\u00b7len", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ein nicht fernes Thal.", "tokens": ["In", "ein", "nicht", "fer\u00b7nes", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in dem hohen Berge wohnte,", "tokens": ["Und", "in", "dem", "ho\u00b7hen", "Ber\u00b7ge", "wohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit langer Zeit, ein weiser Zwerg,", "tokens": ["Seit", "lan\u00b7ger", "Zeit", ",", "ein", "wei\u00b7ser", "Zwerg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Lichtwers", "tokens": ["Den", "Licht\u00b7wers"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "K\u00e4m' er an diesen hohen Berg.", "tokens": ["K\u00e4m'", "er", "an", "die\u00b7sen", "ho\u00b7hen", "Berg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und in dem Thale quakten Fr\u00f6sche!", "tokens": ["Und", "in", "dem", "Tha\u00b7le", "quak\u00b7ten", "Fr\u00f6\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach ein Uhu: Nachtigall, o du,", "tokens": ["Da", "sprach", "ein", "U\u00b7hu", ":", "Nach\u00b7ti\u00b7gall", ",", "o", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "$,", "FM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit deinem t\u00f6nenden Gew\u00e4sche!", "tokens": ["Mit", "dei\u00b7nem", "t\u00f6\u00b7nen\u00b7den", "Ge\u00b7w\u00e4\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Quaken h\u00f6r' ich lieber zu!", "tokens": ["Dem", "Qua\u00b7ken", "h\u00f6r'", "ich", "lie\u00b7ber", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Denker giebst du nichts zu denken,", "tokens": ["Dem", "Den\u00b7ker", "giebst", "du", "nichts", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Lacher nichts zu lachen, du!", "tokens": ["Dem", "La\u00b7cher", "nichts", "zu", "la\u00b7chen", ",", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKZU", "VVINF", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir will ich deine Lieder schenken,", "tokens": ["Dir", "will", "ich", "dei\u00b7ne", "Lie\u00b7der", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Quaken h\u00f6r' ich lieber zu!", "tokens": ["Dem", "Qua\u00b7ken", "h\u00f6r'", "ich", "lie\u00b7ber", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Viel K\u00f6pfe haben viele Sinne,", "tokens": ["Viel", "K\u00f6p\u00b7fe", "ha\u00b7ben", "vie\u00b7le", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ein gelehrter Wiedehopf,", "tokens": ["Sprach", "ein", "ge\u00b7lehr\u00b7ter", "Wie\u00b7de\u00b7hopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Mark zum Denken hat der Spinne,", "tokens": ["Und", "Mark", "zum", "Den\u00b7ken", "hat", "der", "Spin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Raben, und des Esels Kopf.", "tokens": ["Des", "Ra\u00b7ben", ",", "und", "des", "E\u00b7sels", "Kopf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Deswegen mag ihr Urteil gelten,", "tokens": ["Des\u00b7we\u00b7gen", "mag", "ihr", "Ur\u00b7teil", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Uhu! sprach der weise Zwerg;", "tokens": ["Herr", "U\u00b7hu", "!", "sprach", "der", "wei\u00b7se", "Zwerg", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall (Sie merken sich's, Herr Velten!)", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "(", "Sie", "mer\u00b7ken", "sich's", ",", "Herr", "Vel\u00b7ten", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PIS", "$,", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vernahm's und flog sogleich auf einen h\u00f6hern Berg!", "tokens": ["Ver\u00b7nahm's", "und", "flog", "sog\u00b7leich", "auf", "ei\u00b7nen", "h\u00f6\u00b7hern", "Berg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}