{"textgrid.poem.24927": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbso ganz verwandelt du, der beim Orkan", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso ganz verwandelt du, der beim Orkan", "tokens": ["\u00bb", "so", "ganz", "ver\u00b7wan\u00b7delt", "du", ",", "der", "beim", "Or\u00b7kan"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sonst tollk\u00fchn in die Meerflut stach", "tokens": ["Sonst", "toll\u00b7k\u00fchn", "in", "die", "Meer\u00b7flut", "stach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Kiel, da\u00df wir es zitternd sahn,", "tokens": ["Und", "mit", "dem", "Kiel", ",", "da\u00df", "wir", "es", "zit\u00b7ternd", "sahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wogensch\u00e4ume lachend brach?", "tokens": ["Die", "Wo\u00b7gen\u00b7sch\u00e4u\u00b7me", "la\u00b7chend", "brach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sag an, warum du einsam tr\u00e4umst und sinnst,", "tokens": ["Sag", "an", ",", "wa\u00b7rum", "du", "ein\u00b7sam", "tr\u00e4umst", "und", "sinnst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Freunde sag's, Christoforo!", "tokens": ["Dem", "Freun\u00b7de", "sag's", ",", "Chris\u00b7to\u00b7fo\u00b7ro", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sorge scheuch, das eitle Hirngespinst!", "tokens": ["Die", "Sor\u00b7ge", "scheuch", ",", "das", "eit\u00b7le", "Hirn\u00b7ge\u00b7spinst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei neu mit uns beim Ballspiel froh!\u00ab", "tokens": ["Sei", "neu", "mit", "uns", "beim", "Ball\u00b7spiel", "froh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "APPRART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst! Wie viel von Fragen auch best\u00fcrmt,", "tokens": ["Um\u00b7sonst", "!", "Wie", "viel", "von", "Fra\u00b7gen", "auch", "be\u00b7st\u00fcrmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00fcngling bricht das Schweigen nicht:", "tokens": ["Der", "J\u00fcng\u00b7ling", "bricht", "das", "Schwei\u00b7gen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er br\u00fctet, Schriften vor sich aufget\u00fcrmt,", "tokens": ["Er", "br\u00fc\u00b7tet", ",", "Schrif\u00b7ten", "vor", "sich", "auf\u00b7ge\u00b7t\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Morgen bis zum Abendlicht.", "tokens": ["Vom", "Mor\u00b7gen", "bis", "zum", "A\u00b7bend\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Monde schwinden; mit dem Freunde da", "tokens": ["Und", "Mon\u00b7de", "schwin\u00b7den", ";", "mit", "dem", "Freun\u00b7de", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einst ruht er nachts beim Flutgeroll", "tokens": ["Einst", "ruht", "er", "nachts", "beim", "Flut\u00b7ge\u00b7roll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Seegestad der stolzen Genua", "tokens": ["Am", "See\u00b7ge\u00b7stad", "der", "stol\u00b7zen", "Ge\u00b7nua"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und spricht zu ihm geheimnisvoll:", "tokens": ["Und", "spricht", "zu", "ihm", "ge\u00b7heim\u00b7nis\u00b7voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbvernimm! Im leichten Nachen, fern dem Strand,", "tokens": ["\u00bb", "ver\u00b7nimm", "!", "Im", "leich\u00b7ten", "Na\u00b7chen", ",", "fern", "dem", "Strand", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "APPRART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warf mich der Nordsturm j\u00fcngst umher;", "tokens": ["Warf", "mich", "der", "Nord\u00b7sturm", "j\u00fcngst", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ringsum kein Ufer; nur mit j\u00e4hem Rand", "tokens": ["Ring\u00b7sum", "kein", "U\u00b7fer", ";", "nur", "mit", "j\u00e4\u00b7hem", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Stieg eine Klippe aus dem Meer.", "tokens": ["Stieg", "ei\u00b7ne", "Klip\u00b7pe", "aus", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort stand im Nebel, den wie ein Gewand", "tokens": ["Dort", "stand", "im", "Ne\u00b7bel", ",", "den", "wie", "ein", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Nachtwind auf und nieder blies,", "tokens": ["Der", "Nacht\u00b7wind", "auf", "und", "nie\u00b7der", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Riesenbild von Marmor, dessen Hand,", "tokens": ["Ein", "Rie\u00b7sen\u00b7bild", "von", "Mar\u00b7mor", ",", "des\u00b7sen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weit ausgestreckt, nach Westen wies.\u00ab", "tokens": ["Weit", "aus\u00b7ge\u00b7streckt", ",", "nach", "Wes\u00b7ten", "wies", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}