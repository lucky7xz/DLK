{"textgrid.poem.39583": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2. Kritik eines K\u00fcsters", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir K\u00fcster, w\u00fcrd'ger Herr, sind hoch erfreut,", "tokens": ["Wir", "K\u00fcs\u00b7ter", ",", "w\u00fcrd'\u00b7ger", "Herr", ",", "sind", "hoch", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "$,", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Sie so sch\u00f6n der Glocken Lob gesungen;", "tokens": ["Da\u00df", "Sie", "so", "sch\u00f6n", "der", "Glo\u00b7cken", "Lob", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es hat uns fast wie Festgel\u00e4ut geklungen.", "tokens": ["Es", "hat", "uns", "fast", "wie", "Fest\u00b7ge\u00b7l\u00e4ut", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur haben Sie sich etwas weit zerstreut,", "tokens": ["Nur", "ha\u00b7ben", "Sie", "sich", "et\u00b7was", "weit", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und doch dabei den Hauptpunkt \u00fcbergangen:", "tokens": ["Und", "doch", "da\u00b7bei", "den", "Haupt\u00b7punkt", "\u00fc\u00b7ber\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kl\u00f6pfel mein' ich, die darinnen hangen.", "tokens": ["Die", "Kl\u00f6p\u00b7fel", "mein'", "ich", ",", "die", "da\u00b7rin\u00b7nen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn ohne Zung' im Munde, \u2013 mit Respekt", "tokens": ["Denn", "oh\u00b7ne", "Zung'", "im", "Mun\u00b7de", ",", "\u2013", "mit", "Res\u00b7pekt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "APPRART", "NN", "$,", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu sagen, \u2013 m\u00fc\u00dfte ja der Pfarrer selbst verstummen.", "tokens": ["Zu", "sa\u00b7gen", ",", "\u2013", "m\u00fc\u00df\u00b7te", "ja", "der", "Pfar\u00b7rer", "selbst", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "$(", "VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So, wenn kein Kl\u00f6pfel in den Glocken steckt,", "tokens": ["So", ",", "wenn", "kein", "Kl\u00f6p\u00b7fel", "in", "den", "Glo\u00b7cken", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie sehr man auch am Seile zerrt und reckt,", "tokens": ["Wie", "sehr", "man", "auch", "am", "Sei\u00b7le", "zerrt", "und", "reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADV", "APPRART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Man bringt sie nicht zum Bimmeln oder Brummen.", "tokens": ["Man", "bringt", "sie", "nicht", "zum", "Bim\u00b7meln", "o\u00b7der", "Brum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}