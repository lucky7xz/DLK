{"dta.poem.21448": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Im Zecherkreis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht ist\u2019s. Tr\u00fcb\u2019 flackert der Ampeln Licht,", "tokens": ["Nacht", "ist'", "s.", "Tr\u00fcb'", "fla\u00b7ckert", "der", "Am\u00b7peln", "Licht", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Des Mondes Schein durch die Fenster bricht.", "tokens": ["Des", "Mon\u00b7des", "Schein", "durch", "die", "Fens\u00b7ter", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir sitzen im Kreis beim festlichen Mahl,", "tokens": ["Wir", "sit\u00b7zen", "im", "Kreis", "beim", "fest\u00b7li\u00b7chen", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Hand zu Hand geht der duftige Pokal.", "tokens": ["Von", "Hand", "zu", "Hand", "geht", "der", "duf\u00b7ti\u00b7ge", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wild-\u00fcppige Zecher sind wir zumeist,", "tokens": ["Wild\u00b7\u00fcp\u00b7pi\u00b7ge", "Ze\u00b7cher", "sind", "wir", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Manches Witzwort spr\u00fcht von Geist zu Geist.", "tokens": ["Man\u00b7ches", "Witz\u00b7wort", "spr\u00fcht", "von", "Geist", "zu", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dazwischen t\u00f6nt der Dirnen Gelach,", "tokens": ["Da\u00b7zwi\u00b7schen", "t\u00f6nt", "der", "Dir\u00b7nen", "Ge\u00b7lach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Das klingt so gell, das klingt so jach \u2026", "tokens": ["Das", "klingt", "so", "gell", ",", "das", "klingt", "so", "jach", "\u2026"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O tolles Schwelgen im Ueberflu\u00df!", "tokens": ["O", "tol\u00b7les", "Schwel\u00b7gen", "im", "Ue\u00b7berf\u00b7lu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Immer s\u00fc\u00dfer berauscht uns der D\u00e4mon Genu\u00df.", "tokens": ["Im\u00b7mer", "s\u00fc\u00b7\u00dfer", "be\u00b7rauscht", "uns", "der", "D\u00e4\u00b7mon", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ob auch in n\u00e4chster Stunde vielleicht", "tokens": ["Ob", "auch", "in", "n\u00e4chs\u00b7ter", "Stun\u00b7de", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Der Tod \u00fcber unsere H\u00e4upter streicht:", "tokens": ["Der", "Tod", "\u00fc\u00b7ber", "un\u00b7se\u00b7re", "H\u00e4up\u00b7ter", "streicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Uns k\u00fcmmert es nicht. Brust wogend an Brust \u2014", "tokens": ["Uns", "k\u00fcm\u00b7mert", "es", "nicht", ".", "Brust", "wo\u00b7gend", "an", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So lasst uns sterben im Taumel der Lust!", "tokens": ["So", "lasst", "uns", "ster\u00b7ben", "im", "Tau\u00b7mel", "der", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}