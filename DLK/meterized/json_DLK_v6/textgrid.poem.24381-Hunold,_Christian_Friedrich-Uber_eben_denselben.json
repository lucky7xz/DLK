{"textgrid.poem.24381": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber eben denselben", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Losung hie\u00df bey mir: Gott und mein Vaterland.", "tokens": ["Die", "Lo\u00b7sung", "hie\u00df", "bey", "mir", ":", "Gott", "und", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor jenen war der Geist/ vor di\u00df die tapffre Hand.", "tokens": ["Vor", "je\u00b7nen", "war", "der", "Geist", "/", "vor", "di\u00df", "die", "tapf\u00b7fre", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "ART", "NN", "$(", "APPR", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilst du die Missethat und meinen M\u00f6rder wissen?", "tokens": ["Wilst", "du", "die", "Mis\u00b7se\u00b7that", "und", "mei\u00b7nen", "M\u00f6r\u00b7der", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um meiner Mutter Heil hat mich ihr Mann zerrissen.", "tokens": ["Um", "mei\u00b7ner", "Mut\u00b7ter", "Heil", "hat", "mich", "ihr", "Mann", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VAFIN", "PPER",