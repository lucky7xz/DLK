{"textgrid.poem.56213": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie st\u00fcrmte der Knab in das Leben", "tokens": ["Wie", "st\u00fcrm\u00b7te", "der", "Knab", "in", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So feindlich schroff und ergrimmt! \u2013", "tokens": ["So", "feind\u00b7lich", "schroff", "und", "er\u00b7grimmt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Blick in dein klares Auge,", "tokens": ["Ein", "Blick", "in", "dein", "kla\u00b7res", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Blick in den reinen Himmel,", "tokens": ["Ein", "Blick", "in", "den", "rei\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie friedsam ward er gestimmt!", "tokens": ["Wie", "fried\u00b7sam", "ward", "er", "ge\u00b7stimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Er liegt, der Wilde, bes\u00e4nftigt,", "tokens": ["Er", "liegt", ",", "der", "Wil\u00b7de", ",", "be\u00b7s\u00e4nf\u00b7tigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gelassen, besonnen und mild,", "tokens": ["Ge\u00b7las\u00b7sen", ",", "be\u00b7son\u00b7nen", "und", "mild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zu deinen F\u00fc\u00dfen geb\u00e4ndigt,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hebet zitternd die H\u00e4nde", "tokens": ["Und", "he\u00b7bet", "zit\u00b7ternd", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zu dir, du friedliches Bild!", "tokens": ["Zu", "dir", ",", "du", "fried\u00b7li\u00b7ches", "Bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich habe mir einen Garten", "tokens": ["Ich", "ha\u00b7be", "mir", "ei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bestellt nach allem Flei\u00df;", "tokens": ["Be\u00b7stellt", "nach", "al\u00b7lem", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da seh ich die Rosen erbl\u00fchen,", "tokens": ["Da", "seh", "ich", "die", "Ro\u00b7sen", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich h\u00e4rmen und still vergl\u00fchen,", "tokens": ["Sich", "h\u00e4r\u00b7men", "und", "still", "ver\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von denen die Herrin nicht wei\u00df.", "tokens": ["Von", "de\u00b7nen", "die", "Her\u00b7rin", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich hab ein Haus mir erbauet,", "tokens": ["Ich", "hab", "ein", "Haus", "mir", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Begr\u00fcndet es dauerhaft;", "tokens": ["Be\u00b7gr\u00fcn\u00b7det", "es", "dau\u00b7er\u00b7haft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das seh ich so d\u00fcster trauern,", "tokens": ["Das", "seh", "ich", "so", "d\u00fcs\u00b7ter", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil nicht in den \u00f6den Mauern", "tokens": ["Weil", "nicht", "in", "den", "\u00f6\u00b7den", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die segnende Hausfrau schafft.", "tokens": ["Die", "seg\u00b7nen\u00b7de", "Haus\u00b7frau", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich habe von reinem Golde", "tokens": ["Ich", "ha\u00b7be", "von", "rei\u00b7nem", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bestellt mir einen Ring,", "tokens": ["Be\u00b7stellt", "mir", "ei\u00b7nen", "Ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Ring... ich zittre verstummend \u2013", "tokens": ["Den", "Ring", "...", "ich", "zitt\u00b7re", "ver\u00b7stum\u00b7mend", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Ring, du Reine, du Holde,", "tokens": ["Den", "Ring", ",", "du", "Rei\u00b7ne", ",", "du", "Hol\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nimm an den goldenen Ring.", "tokens": ["Nimm", "an", "den", "gol\u00b7de\u00b7nen", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Den Gartenhag und die Rosen,", "tokens": ["Den", "Gar\u00b7ten\u00b7hag", "und", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Haus, des Ringes Zier,", "tokens": ["Das", "Haus", ",", "des", "Rin\u00b7ges", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz und meinen Frieden,", "tokens": ["Mein", "Herz", "und", "mei\u00b7nen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Leben und mein Lieben,", "tokens": ["Mein", "Le\u00b7ben", "und", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die leg ich zu F\u00fc\u00dfen dir.", "tokens": ["Die", "leg", "ich", "zu", "F\u00fc\u00b7\u00dfen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "APPR", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}