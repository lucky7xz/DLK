{"textgrid.poem.32960": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh die Welt, du siehst die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh die Welt, du siehst die Welt,", "tokens": ["Ich", "seh", "die", "Welt", ",", "du", "siehst", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du nennst es Prosa, ich Gedicht,", "tokens": ["du", "nennst", "es", "Pro\u00b7sa", ",", "ich", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "was mir gef\u00e4llt,", "tokens": ["was", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "gef\u00e4llt dir nicht,", "tokens": ["ge\u00b7f\u00e4llt", "dir", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und aus dem n\u00e4mlichen Gesicht", "tokens": ["und", "aus", "dem", "n\u00e4m\u00b7li\u00b7chen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "err\u00e4tst du Freude und ich Trauer,", "tokens": ["er\u00b7r\u00e4tst", "du", "Freu\u00b7de", "und", "ich", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "du nennst es s\u00fc\u00df, ich nenn es sauer ...", "tokens": ["du", "nennst", "es", "s\u00fc\u00df", ",", "ich", "nenn", "es", "sau\u00b7er", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "wir fangen nun an, uns dr\u00fcber zu streiten", "tokens": ["wir", "fan\u00b7gen", "nun", "an", ",", "uns", "dr\u00fc\u00b7ber", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "PAV", "PTKZU", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "und alles uns gr\u00fcndlich zu verleiden.", "tokens": ["und", "al\u00b7les", "uns", "gr\u00fcnd\u00b7lich", "zu", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Dritter kommt dazu und lacht:", "tokens": ["Ein", "Drit\u00b7ter", "kommt", "da\u00b7zu", "und", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Gott, gehabt euch nicht so t\u00f6richt!", "tokens": ["Mein", "Gott", ",", "ge\u00b7habt", "euch", "nicht", "so", "t\u00f6\u00b7richt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAPP", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "im Winter, Kinder, ist es Winter,", "tokens": ["im", "Win\u00b7ter", ",", "Kin\u00b7der", ",", "ist", "es", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und wenn der Mai kommt, wird es Fr\u00fchling,", "tokens": ["und", "wenn", "der", "Mai", "kommt", ",", "wird", "es", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und im Oktober nennt man's Herbst ...", "tokens": ["und", "im", "Ok\u00b7to\u00b7ber", "nennt", "man's", "Herbst", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich meinerseits freu mich nicht minder", "tokens": ["ich", "mei\u00b7ner\u00b7seits", "freu", "mich", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "an Winter, als an Mai und Herbst.", "tokens": ["an", "Win\u00b7ter", ",", "als", "an", "Mai", "und", "Herbst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}