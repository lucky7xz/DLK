{"textgrid.poem.65367": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heilige Fr\u00fchlingswonne,", "tokens": ["O", "hei\u00b7li\u00b7ge", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du sinkest nieder,", "tokens": ["Du", "sin\u00b7kest", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Strahlend und flimmernd", "tokens": ["Strah\u00b7lend", "und", "flim\u00b7mernd"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In himmlischen Schauern,", "tokens": ["In", "himm\u00b7li\u00b7schen", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auf alle Berge,", "tokens": ["Auf", "al\u00b7le", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In alle Th\u00e4ler,", "tokens": ["In", "al\u00b7le", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In jede Menschenbrust!", "tokens": ["In", "je\u00b7de", "Men\u00b7schen\u00b7brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ja, du bist es,", "tokens": ["Ja", ",", "du", "bist", "es", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Geist Gottes,", "tokens": ["Geist", "Got\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Du gie\u00dfest dich aus", "tokens": ["Du", "gie\u00b7\u00dfest", "dich", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "\u00dcber die Welt!", "tokens": ["\u00dc\u00b7ber", "die", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Soll ich auf die sonnige H\u00f6he steigen", "tokens": ["Soll", "ich", "auf", "die", "son\u00b7ni\u00b7ge", "H\u00f6\u00b7he", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.13": {"text": "Und beten?", "tokens": ["Und", "be\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Soll ich in dem dunkeln Thale liegen", "tokens": ["Soll", "ich", "in", "dem", "dun\u00b7keln", "Tha\u00b7le", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und sinnen?", "tokens": ["Und", "sin\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "O tritt sanft, mein Fu\u00df,", "tokens": ["O", "tritt", "sanft", ",", "mein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Da\u00df du den Wurm nicht tretest,", "tokens": ["Da\u00df", "du", "den", "Wurm", "nicht", "tre\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der unter dir", "tokens": ["Der", "un\u00b7ter", "dir"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Sich freuet des sonnigen Lebens!", "tokens": ["Sich", "freu\u00b7et", "des", "son\u00b7ni\u00b7gen", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Und du, hoch schlagende Brust,", "tokens": ["Und", "du", ",", "hoch", "schla\u00b7gen\u00b7de", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Halt' an den Athem,", "tokens": ["Halt'", "an", "den", "A\u00b7them", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Da\u00df du die M\u00fccke", "tokens": ["Da\u00df", "du", "die", "M\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Nicht in dich ziehest,", "tokens": ["Nicht", "in", "dich", "zie\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Die sich wieget im Strahle", "tokens": ["Die", "sich", "wie\u00b7get", "im", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Vor deinem Munde!", "tokens": ["Vor", "dei\u00b7nem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}