{"dta.poem.11795": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer alles Gute liebt, wo er's nur aufgetrieben,", "tokens": ["Wer", "al\u00b7les", "Gu\u00b7te", "liebt", ",", "wo", "er's", "nur", "auf\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "VVFIN", "$,", "PWAV", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darf auch das Gute, das er an sich selbst fand, lieben;", "tokens": ["Darf", "auch", "das", "Gu\u00b7te", ",", "das", "er", "an", "sich", "selbst", "fand", ",", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie einem Kinderfreund, dem lieb die fremden sind,", "tokens": ["Wie", "ei\u00b7nem", "Kin\u00b7der\u00b7freund", ",", "dem", "lieb", "die", "frem\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erlaubt ist, da\u00df ihm lieb auch sei sein eignes Kind.", "tokens": ["Er\u00b7laubt", "ist", ",", "da\u00df", "ihm", "lieb", "auch", "sei", "sein", "eig\u00b7nes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wie ein Vater streng das Kind zieht, das er liebt,", "tokens": ["Doch", "wie", "ein", "Va\u00b7ter", "streng", "das", "Kind", "zieht", ",", "das", "er", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie sein gutes Korn ein Hauswirth flei\u00dfig siebt;", "tokens": ["Und", "wie", "sein", "gu\u00b7tes", "Korn", "ein", "Haus\u00b7wirth", "flei\u00b7\u00dfig", "siebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht minder lieb ist ihm das Kindlein, das er z\u00fcchtigt,", "tokens": ["Nicht", "min\u00b7der", "lieb", "ist", "ihm", "das", "Kin\u00b7dlein", ",", "das", "er", "z\u00fcch\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder werth das Korn, wenn er die Spreu verfl\u00fcchtigt:", "tokens": ["Nicht", "min\u00b7der", "werth", "das", "Korn", ",", "wenn", "er", "die", "Spreu", "ver\u00b7fl\u00fcch\u00b7tigt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So liebe Gutes nur an dir, um es zu bessern,", "tokens": ["So", "lie\u00b7be", "Gu\u00b7tes", "nur", "an", "dir", ",", "um", "es", "zu", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df den schlechten Wein den schlechten Schenkwirth w\u00e4ssern.", "tokens": ["Und", "la\u00df", "den", "schlech\u00b7ten", "Wein", "den", "schlech\u00b7ten", "Schenk\u00b7wirth", "w\u00e4s\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}