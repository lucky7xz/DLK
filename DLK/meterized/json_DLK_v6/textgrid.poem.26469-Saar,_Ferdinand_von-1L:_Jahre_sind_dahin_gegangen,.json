{"textgrid.poem.26469": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jahre sind dahin gegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahre sind dahin gegangen,", "tokens": ["Jah\u00b7re", "sind", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich an K\u00e4mpfen, reich an M\u00fch'n;", "tokens": ["Reich", "an", "K\u00e4mp\u00b7fen", ",", "reich", "an", "M\u00fch'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend And're fr\u00f6hlich sangen,", "tokens": ["W\u00e4h\u00b7rend", "An\u00b7d'\u00b7re", "fr\u00f6h\u00b7lich", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Lie\u00df ich still mein Herz ergl\u00fch'n.", "tokens": ["Lie\u00df", "ich", "still", "mein", "Herz", "er\u00b7gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gro\u00dfen Zielen zugewendet,", "tokens": ["Gro\u00b7\u00dfen", "Zie\u00b7len", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich Gr\u00f6\u00df'res nur bedacht \u2013", "tokens": ["Hab'", "ich", "Gr\u00f6\u00df'\u00b7res", "nur", "be\u00b7dacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach wie wenig ward vollendet,", "tokens": ["Ach", "wie", "we\u00b7nig", "ward", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach wie wenig ward vollbracht.", "tokens": ["Ach", "wie", "we\u00b7nig", "ward", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt doch, bei des Lebens Neige,", "tokens": ["Jetzt", "doch", ",", "bei", "des", "Le\u00b7bens", "Nei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehr' ich in mich selbst zur\u00fcck \u2013", "tokens": ["Kehr'", "ich", "in", "mich", "selbst", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so bl\u00fcht, ihr Liederzweige,", "tokens": ["Und", "so", "bl\u00fcht", ",", "ihr", "Lie\u00b7der\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein letztes Dichtergl\u00fcck!", "tokens": ["Als", "ein", "letz\u00b7tes", "Dich\u00b7ter\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}