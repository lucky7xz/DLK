{"textgrid.poem.59649": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist nun vergangen,", "tokens": ["Der", "Tag", "ist", "nun", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dunkel schl\u00e4ft die Welt,", "tokens": ["Und", "dun\u00b7kel", "schl\u00e4ft", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hellen Sterne prangen", "tokens": ["Die", "hel\u00b7len", "Ster\u00b7ne", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am blauen Himmelszelt;", "tokens": ["Am", "blau\u00b7en", "Him\u00b7mels\u00b7zelt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur in den gr\u00fcnen Zweigen", "tokens": ["Nur", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Singt noch die Nachtigall,", "tokens": ["Singt", "noch", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im weiten, tiefen Schweigen", "tokens": ["Im", "wei\u00b7ten", ",", "tie\u00b7fen", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der einz'ge Lebensschall.", "tokens": ["Der", "einz'\u00b7ge", "Le\u00b7bens\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich aber, Vater, stehe", "tokens": ["Ich", "a\u00b7ber", ",", "Va\u00b7ter", ",", "ste\u00b7he"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meiner H\u00fcttent\u00fcr", "tokens": ["In", "mei\u00b7ner", "H\u00fct\u00b7ten\u00b7t\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schau' hinauf zur H\u00f6he", "tokens": ["Und", "schau'", "hin\u00b7auf", "zur", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schau' hinauf zu dir;", "tokens": ["Und", "schau'", "hin\u00b7auf", "zu", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie gerne m\u00f6cht' ich klingen", "tokens": ["Wie", "ger\u00b7ne", "m\u00f6cht'", "ich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als helle Nachtigall,", "tokens": ["Als", "hel\u00b7le", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dir Preis und Dank zu bringen", "tokens": ["Dir", "Preis", "und", "Dank", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit tiefem Schmerzenschall.", "tokens": ["Mit", "tie\u00b7fem", "Schmer\u00b7zen\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, mit dem Schall der Schmerzen:", "tokens": ["Ja", ",", "mit", "dem", "Schall", "der", "Schmer\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Denn geht die Nacht herauf,", "tokens": ["Denn", "geht", "die", "Nacht", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So springt in meinem Herzen", "tokens": ["So", "springt", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Quell der Tr\u00e4nen auf,", "tokens": ["Ein", "Quell", "der", "Tr\u00e4\u00b7nen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Tr\u00e4nen und der Klagen \u2013", "tokens": ["Der", "Tr\u00e4\u00b7nen", "und", "der", "Kla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du, Vater, wei\u00dft es best,", "tokens": ["Du", ",", "Va\u00b7ter", ",", "wei\u00dft", "es", "best", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was singen nicht und sagen,", "tokens": ["Was", "sin\u00b7gen", "nicht", "und", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was sich nicht sprechen l\u00e4\u00dft.", "tokens": ["Was", "sich", "nicht", "spre\u00b7chen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du kennest meinen Kummer,", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7nen", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der auf gen Himmel blickt,", "tokens": ["Der", "auf", "gen", "Him\u00b7mel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann f\u00fcr den s\u00fc\u00dfen Schlummer", "tokens": ["Wann", "f\u00fcr", "den", "s\u00fc\u00b7\u00dfen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Welt sich schickt,", "tokens": ["Die", "gan\u00b7ze", "Welt", "sich", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Womit so schwer beladen", "tokens": ["Wo\u00b7mit", "so", "schwer", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz nach oben schaut,", "tokens": ["Mein", "Herz", "nach", "o\u00b7ben", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach deinem Born der Gnaden,", "tokens": ["Nach", "dei\u00b7nem", "Born", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Labsal niedertaut.", "tokens": ["Der", "Lab\u00b7sal", "nie\u00b7der\u00b7taut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja, deine s\u00fc\u00dfe Liebe,", "tokens": ["Ja", ",", "dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die tr\u00f6stet mir den Schmerz,", "tokens": ["Die", "tr\u00f6s\u00b7tet", "mir", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, deine s\u00fc\u00dfe Liebe,", "tokens": ["Ja", ",", "dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die stillet mir das Herz,", "tokens": ["Die", "stil\u00b7let", "mir", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die l\u00f6st in hei\u00dfen Tr\u00e4nen", "tokens": ["Die", "l\u00f6st", "in", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Eis des Busens auf", "tokens": ["Das", "Eis", "des", "Bu\u00b7sens", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und stellet Sinn und Sehnen", "tokens": ["Und", "stel\u00b7let", "Sinn", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum hohen Sternenlauf.", "tokens": ["Zum", "ho\u00b7hen", "Ster\u00b7nen\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O la\u00df mich ewig schauen", "tokens": ["O", "la\u00df", "mich", "e\u00b7wig", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im stillen Kindersinn", "tokens": ["Im", "stil\u00b7len", "Kin\u00b7der\u00b7sinn"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu jenen g\u00fcldnen Auen,", "tokens": ["Zu", "je\u00b7nen", "g\u00fcld\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Woher ich kommen bin!", "tokens": ["Wo\u00b7her", "ich", "kom\u00b7men", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O richte Herz und Sinne,", "tokens": ["O", "rich\u00b7te", "Herz", "und", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Vater, f\u00fcr und f\u00fcr", "tokens": ["Mein", "Va\u00b7ter", ",", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu deiner s\u00fc\u00dfen Minne,", "tokens": ["Zu", "dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Himmel hin, zu dir.", "tokens": ["Zum", "Him\u00b7mel", "hin", ",", "zu", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So mag ich froh mich legen", "tokens": ["So", "mag", "ich", "froh", "mich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun mit der Welt zur Ruh',", "tokens": ["Nun", "mit", "der", "Welt", "zur", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Amen und mein Segen,", "tokens": ["Mein", "A\u00b7men", "und", "mein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein W\u00e4chter, das bist du;", "tokens": ["Mein", "W\u00e4ch\u00b7ter", ",", "das", "bist", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mag in deinem Frieden", "tokens": ["So", "mag", "in", "dei\u00b7nem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich fr\u00f6hlich schlafen ein,", "tokens": ["Ich", "fr\u00f6h\u00b7lich", "schla\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort oben und hienieden", "tokens": ["Dort", "o\u00b7ben", "und", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Schlaf und Wachen dein.", "tokens": ["Im", "Schlaf", "und", "Wa\u00b7chen", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}