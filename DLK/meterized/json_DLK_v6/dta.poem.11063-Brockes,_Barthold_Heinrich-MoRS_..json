{"dta.poem.11063": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "MoRS .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend rafft er pl\u00f6tzlich weg; die Alten reist er\nmit sich fort,", "tokens": ["Die", "Ju\u00b7gend", "rafft", "er", "pl\u00f6tz\u00b7lich", "weg", ";", "die", "Al\u00b7ten", "reist", "er", "mit", "sich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wohin er st\u00fcrzet, st\u00fcrzt er rasend; den ganzen Kreis der wei-", "tokens": ["Wo\u00b7hin", "er", "st\u00fcr\u00b7zet", ",", "st\u00fcrzt", "er", "ra\u00b7send", ";", "den", "gan\u00b7zen", "Kreis", "der", "wei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$.", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Durchf\u00e4hrt sein Blitz; er triumphiert allein, selbst blind, mit", "tokens": ["Durch\u00b7f\u00e4hrt", "sein", "Blitz", ";", "er", "tri\u00b7um\u00b7phiert", "al\u00b7lein", ",", "selbst", "blind", ",", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$,", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}