{"dta.poem.203": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie zu ihm sagte: er w\u00e4re zu furchtsahm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin und bleibe so in meinem Courtifiren/", "tokens": ["Ich", "bin", "und", "blei\u00b7be", "so", "in", "mei\u00b7nem", "Cour\u00b7ti\u00b7fi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich die Frucht mag sehn doch nicht zum Munde f\u00fchren/", "tokens": ["Da\u00df", "ich", "die", "Frucht", "mag", "sehn", "doch", "nicht", "zum", "Mun\u00b7de", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wurmenstichig ist und voller Galle steckt/", "tokens": ["Die", "wur\u00b7men\u00b7sti\u00b7chig", "ist", "und", "vol\u00b7ler", "Gal\u00b7le", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die/ ob sie Anfangs s\u00fc\u00df/ hernach verzweiffelt schmeckt.", "tokens": ["Die", "/", "ob", "sie", "An\u00b7fangs", "s\u00fc\u00df", "/", "her\u00b7nach", "ver\u00b7zweif\u00b7felt", "schmeckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "ADJD", "$(", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum kanich auch nichts mehr als ihre Brust ber\u00fchren/", "tokens": ["Drum", "ka\u00b7nich", "auch", "nichts", "mehr", "als", "ih\u00b7re", "Brust", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "PIS", "PIS", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der Gewohnheit nach mit Worten sie ", "tokens": ["Und", "der", "Ge\u00b7wohn\u00b7heit", "nach", "mit", "Wor\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "APPR", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn dieses werde ich niemahlen ", "tokens": ["Denn", "die\u00b7ses", "wer\u00b7de", "ich", "nie\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch g\u00f6nnen/ da\u00df mein Sinn nach ihrer Schoo\u00df sich streckt.", "tokens": ["Noch", "g\u00f6n\u00b7nen", "/", "da\u00df", "mein", "Sinn", "nach", "ih\u00b7rer", "Schoo\u00df", "sich", "streckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin und bleibe so.", "tokens": ["Ich", "bin", "und", "blei\u00b7be", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ich mag den M\u00e4dgens gern aus Lust ", "tokens": ["Ich", "mag", "den", "M\u00e4d\u00b7gens", "gern", "aus", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und so sie g\u00fctig sind zum Spasse was ", "tokens": ["Und", "so", "sie", "g\u00fc\u00b7tig", "sind", "zum", "Spas\u00b7se", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADJD", "VAFIN", "APPRART", "NN", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Alleine von der That mich vielerley abschreckt/", "tokens": ["Al\u00b7lei\u00b7ne", "von", "der", "That", "mich", "vie\u00b7ler\u00b7ley", "ab\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.13": {"text": "Das dem es nicht bewust vers\u00fc\u00dfte Lust erweckt.", "tokens": ["Das", "dem", "es", "nicht", "be\u00b7wust", "ver\u00b7s\u00fc\u00df\u00b7te", "Lust", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "PTKNEG", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum soll auch \u00fcber mich kein M\u00e4dgen ", "tokens": ["Drum", "soll", "auch", "\u00fc\u00b7ber", "mich", "kein", "M\u00e4d\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich bin und bleibe so.", "tokens": ["Ich", "bin", "und", "blei\u00b7be", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}