{"dta.poem.7445": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich bekenn's, die Stimme Gottes ist", "tokens": ["Ja", ",", "ich", "be\u00b7kenn's", ",", "die", "Stim\u00b7me", "Got\u00b7tes", "ist"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "NE", "$,", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Volkes Stimme! und wer ihr vertraut,", "tokens": ["Des", "Vol\u00b7kes", "Stim\u00b7me", "!", "und", "wer", "ihr", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "KON", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der hat sein Haus auf Felsen sich gebaut,", "tokens": ["Der", "hat", "sein", "Haus", "auf", "Fel\u00b7sen", "sich", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Inde\u00df der Zorn des Herrn die Frevler fri\u00dft.", "tokens": ["In\u00b7de\u00df", "der", "Zorn", "des", "Herrn", "die", "Frev\u00b7ler", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem S\u00e4nger Heil, der ihrer nie vergi\u00dft,", "tokens": ["Dem", "S\u00e4n\u00b7ger", "Heil", ",", "der", "ih\u00b7rer", "nie", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem nur des Volkes Schmerz vom Auge thaut,", "tokens": ["Dem", "nur", "des", "Vol\u00b7kes", "Schmerz", "vom", "Au\u00b7ge", "thaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nicht im eignen Jammer sich beschaut", "tokens": ["Der", "nicht", "im", "eig\u00b7nen", "Jam\u00b7mer", "sich", "be\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPRART", "ADJA", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und selbstgef\u00e4llig seine Silben mi\u00dft!", "tokens": ["Und", "selbst\u00b7ge\u00b7f\u00e4l\u00b7lig", "sei\u00b7ne", "Sil\u00b7ben", "mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch sollt' er drum nur ", "tokens": ["Doch", "sollt'", "er", "drum", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PAV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und, wenn es Not thut, reicht ein Schwert hinein?", "tokens": ["Und", ",", "wenn", "es", "Not", "thut", ",", "reicht", "ein", "Schwert", "hin\u00b7ein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der nicht ", "tokens": ["Der", "nicht"], "token_info": ["word", "word"], "pos": ["ART", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und dreimal: Nein! und stimme f\u00fcr ", "tokens": ["Und", "drei\u00b7mal", ":", "Nein", "!", "und", "stim\u00b7me", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PTKANT", "$.", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}