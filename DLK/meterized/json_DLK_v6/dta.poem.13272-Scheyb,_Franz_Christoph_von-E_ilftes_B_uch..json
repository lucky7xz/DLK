{"dta.poem.13272": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Theresia verlieh der Weisheit zwar Geh\u00f6r,", "tokens": ["The\u00b7re\u00b7sia", "ver\u00b7lieh", "der", "Weis\u00b7heit", "zwar", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Allein ihr Angesicht ver\u00e4nderte sich sehr,", "tokens": ["Al\u00b7lein", "ihr", "An\u00b7ge\u00b7sicht", "ver\u00b7\u00e4n\u00b7der\u00b7te", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da man dem guten Volck so wenig Beyfall gabe;", "tokens": ["Da", "man", "dem", "gu\u00b7ten", "Volck", "so", "we\u00b7nig", "Bey\u00b7fall", "ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "540Wie, wann es sich mit nichts, als mit Einbildung labe.", "tokens": [",", "wann", "es", "sich", "mit", "nichts", ",", "als", "mit", "Ein\u00b7bil\u00b7dung", "la\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$,", "PWAV", "PPER", "PRF", "APPR", "PIS", "$,", "KOUS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}