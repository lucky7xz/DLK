{"dta.poem.6059": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen heller Schein", "tokens": ["Der", "Son\u00b7nen", "hel\u00b7ler", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erf\u00fcllet alle Ding allein,", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "al\u00b7le", "Ding", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reichet jeglichen Planeten, auf der Reise,", "tokens": ["Und", "rei\u00b7chet", "jeg\u00b7li\u00b7chen", "Pla\u00b7ne\u00b7ten", ",", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als der, in freyen Raum, in seinem grossen Creise,", "tokens": ["Als", "der", ",", "in", "frey\u00b7en", "Raum", ",", "in", "sei\u00b7nem", "gros\u00b7sen", "Crei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein stetes Gleich-Gewicht", "tokens": ["Sein", "ste\u00b7tes", "Gleich\u00b7Ge\u00b7wicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn Aenderung beh\u00e4lt; der Strahlen helles Licht.", "tokens": ["Ohn", "A\u00b7en\u00b7de\u00b7rung", "be\u00b7h\u00e4lt", ";", "der", "Strah\u00b7len", "hel\u00b7les", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wie sie in vielerley Aspecten stehen,", "tokens": ["Wie", "sie", "in", "vie\u00b7ler\u00b7ley", "A\u00b7spec\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kan man allein durchs Licht der Sonne sehen.", "tokens": ["Kan", "man", "al\u00b7lein", "durchs", "Licht", "der", "Son\u00b7ne", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob man nun spricht, da\u00df es die Erde sey,", "tokens": ["Ob", "man", "nun", "spricht", ",", "da\u00df", "es", "die", "Er\u00b7de", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie oder ein Planet, ist alles einerley.", "tokens": ["Wie", "o\u00b7der", "ein", "Pla\u00b7net", ",", "ist", "al\u00b7les", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "ART", "NN", "$,", "VAFIN", "PIS", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Sie sind aus einerley Materie gemacht,", "tokens": ["Sie", "sind", "aus", "ei\u00b7ner\u00b7ley", "Ma\u00b7te\u00b7rie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Auf eine Art hervorgebracht,", "tokens": ["Auf", "ei\u00b7ne", "Art", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie haben einerley Bewegen,", "tokens": ["Sie", "ha\u00b7ben", "ei\u00b7ner\u00b7ley", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vereinen sich, und stehen sich entgegen", "tokens": ["Ver\u00b7ei\u00b7nen", "sich", ",", "und", "ste\u00b7hen", "sich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KON", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In einerley Beschaffenheit;", "tokens": ["In", "ei\u00b7ner\u00b7ley", "Be\u00b7schaf\u00b7fen\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ja, zu gewisser Zeit", "tokens": ["Ja", ",", "zu", "ge\u00b7wis\u00b7ser", "Zeit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Versp\u00fcren sie gewisse Aehnlichkeit", "tokens": ["Ver\u00b7sp\u00fc\u00b7ren", "sie", "ge\u00b7wis\u00b7se", "A\u00b7ehn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "An Aendrung, und zwar ohne Ruh:", "tokens": ["An", "A\u00b7en\u00b7drung", ",", "und", "zwar", "oh\u00b7ne", "Ruh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Sie nehmen ab, sie nehmen zu.", "tokens": ["Sie", "neh\u00b7men", "ab", ",", "sie", "neh\u00b7men", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}