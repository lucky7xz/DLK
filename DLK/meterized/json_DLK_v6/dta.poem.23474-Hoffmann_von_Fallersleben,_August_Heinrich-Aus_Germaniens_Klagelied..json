{"dta.poem.23474": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus Germaniens Klagelied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich armes Reich, was soll ich endlich machen,", "tokens": ["Was", "soll", "ich", "ar\u00b7mes", "Reich", ",", "was", "soll", "ich", "end\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun mir genommen ist mein Freuen, Lust und Lachen?", "tokens": ["Nun", "mir", "ge\u00b7nom\u00b7men", "ist", "mein", "Freu\u00b7en", ",", "Lust", "und", "La\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kaum bin ich mehr bei Sinnen", "tokens": ["Kaum", "bin", "ich", "mehr", "bei", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser langen Noth.", "tokens": ["In", "die\u00b7ser", "lan\u00b7gen", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was soll ich doch beginnen?", "tokens": ["Was", "soll", "ich", "doch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur w\u00fcnsch' ich mir den Tod.", "tokens": ["Nur", "w\u00fcn\u00b7sch'", "ich", "mir", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Kinder so ich selbst erzeuget sind die Schlangen,", "tokens": ["Die", "Kin\u00b7der", "so", "ich", "selbst", "er\u00b7zeu\u00b7get", "sind", "die", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihre Mutter, mich, zu w\u00fcrgen unterfangen;", "tokens": ["Die", "ih\u00b7re", "Mut\u00b7ter", ",", "mich", ",", "zu", "w\u00fcr\u00b7gen", "un\u00b7ter\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "PPER", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die haben mich zerbissen,", "tokens": ["Die", "ha\u00b7ben", "mich", "zer\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df fast mein ganzer Leib", "tokens": ["Da\u00df", "fast", "mein", "gan\u00b7zer", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In St\u00fccklein ist zerrissen:", "tokens": ["In", "St\u00fcck\u00b7lein", "ist", "zer\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O weh, ich armes Weib!", "tokens": ["O", "weh", ",", "ich", "ar\u00b7mes", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, Lieb' und Treu ist hin, die Gottesfurcht erkaltet;", "tokens": ["Ach", ",", "Lieb'", "und", "Treu", "ist", "hin", ",", "die", "Got\u00b7tes\u00b7furcht", "er\u00b7kal\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glaub' ist abgethan, Best\u00e4ndigkeit veraltet.", "tokens": ["Der", "Glaub'", "ist", "ab\u00b7ge\u00b7than", ",", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das deutsche Blut bed\u00fcnget", "tokens": ["Das", "deut\u00b7sche", "Blut", "be\u00b7d\u00fcn\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So manches sch\u00f6ne Land;", "tokens": ["So", "man\u00b7ches", "sch\u00f6\u00b7ne", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}