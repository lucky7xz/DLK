{"dta.poem.11941": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solang du jung bist, mag es dir villeicht behagen,", "tokens": ["So\u00b7lang", "du", "jung", "bist", ",", "mag", "es", "dir", "vil\u00b7leicht", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAFIN", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um eines Hauptes L\u00e4ng' ob andern aufzuragen.", "tokens": ["Um", "ei\u00b7nes", "Haup\u00b7tes", "L\u00e4ng'", "ob", "an\u00b7dern", "auf\u00b7zu\u00b7ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NE", "KOUS", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn du \u00e4lter wirst, dein Auge bl\u00f6d' und schwach,", "tokens": ["Doch", "wenn", "du", "\u00e4l\u00b7ter", "wirst", ",", "dein", "Au\u00b7ge", "bl\u00f6d'", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint der Vorzug dir villeicht als Ungemach.", "tokens": ["Er\u00b7scheint", "der", "Vor\u00b7zug", "dir", "vil\u00b7leicht", "als", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn nicht den Sternen wirst du darum n\u00e4her gehn,", "tokens": ["Denn", "nicht", "den", "Ster\u00b7nen", "wirst", "du", "da\u00b7rum", "n\u00e4\u00b7her", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VAFIN", "PPER", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch minder deutlich wol am Weg die Gr\u00e4ser sehn.", "tokens": ["Doch", "min\u00b7der", "deut\u00b7lich", "wol", "am", "Weg", "die", "Gr\u00e4\u00b7ser", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann um so tiefer wird dein Haupt sich auf die Brust", "tokens": ["Dann", "um", "so", "tie\u00b7fer", "wird", "dein", "Haupt", "sich", "auf", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Absenken, um zu sehn der Erde gr\u00fcne Lust;", "tokens": ["Ab\u00b7sen\u00b7ken", ",", "um", "zu", "sehn", "der", "Er\u00b7de", "gr\u00fc\u00b7ne", "Lust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Wie jeder Greis es senkt, um noch einmal zu gr\u00fc\u00dfen", "tokens": ["Wie", "je\u00b7der", "Greis", "es", "senkt", ",", "um", "noch", "ein\u00b7mal", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$,", "KOUI", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blumen, die nun bald das Grab ihm h\u00fcten m\u00fc\u00dfen.", "tokens": ["Die", "Blu\u00b7men", ",", "die", "nun", "bald", "das", "Grab", "ihm", "h\u00fc\u00b7ten", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}