{"textgrid.poem.51674": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nihm alles weg, was wir der Seelen unterlegen.", "tokens": ["Nihm", "al\u00b7les", "weg", ",", "was", "wir", "der", "See\u00b7len", "un\u00b7ter\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast die Erste Sach: und kanst die Welt bewegen.", "tokens": ["Du", "hast", "die", "Ers\u00b7te", "Sach", ":", "und", "kanst", "die", "Welt", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}