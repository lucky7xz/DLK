{"dta.poem.21808": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Freund/", "tokens": ["Freund", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Getreuer/", "tokens": ["Ge\u00b7treu\u00b7er", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "denn/ weil im Feuer", "tokens": ["denn", "/", "weil", "im", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "KOUS", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "mein F\u00fcrst Apollo scheint/", "tokens": ["mein", "F\u00fcrst", "A\u00b7pol\u00b7lo", "scheint", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ich werd' unendlich lieben.", "tokens": ["ich", "werd'", "un\u00b7end\u00b7lich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimm hier f\u00fcr alle Gunst", "tokens": ["Nimm", "hier", "f\u00fcr", "al\u00b7le", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "so du lang' an mir erwiesen/", "tokens": ["so", "du", "lang'", "an", "mir", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "die Zeilen meiner schlechten Kunst/", "tokens": ["die", "Zei\u00b7len", "mei\u00b7ner", "schlech\u00b7ten", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "mit Venus Schwanen-kiel geschrieben/", "tokens": ["mit", "Ve\u00b7nus", "Schwa\u00b7nen\u00b7kiel", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "die Zeilen/ von der Jugend nur gepriesen.", "tokens": ["die", "Zei\u00b7len", "/", "von", "der", "Ju\u00b7gend", "nur", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zwar \u00fcberzeuget mich mein Sinn", "tokens": ["Zwar", "\u00fc\u00b7berz\u00b7eu\u00b7get", "mich", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "da\u00df ich dir mehr verpflichtet binn:", "tokens": ["da\u00df", "ich", "dir", "mehr", "ver\u00b7pflich\u00b7tet", "binn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "was aber/ ist in meinen H\u00e4nden?", "tokens": ["was", "a\u00b7ber", "/", "ist", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "der G\u00f6tter \u00fcberheuffter Grimm", "tokens": ["der", "G\u00f6t\u00b7ter", "\u00fc\u00b7berh\u00b7euff\u00b7ter", "Grimm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "f\u00fchrt mich im Elend noch her\u00fcm", "tokens": ["f\u00fchrt", "mich", "im", "E\u00b7lend", "noch", "he\u00b7r\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "ohn anzulenden.", "tokens": ["ohn", "an\u00b7zu\u00b7len\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "VVIZU", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Ich wei\u00df/ dz dein Gem\u00fcht'", "tokens": ["Ich", "wei\u00df", "/", "dz", "dein", "Ge\u00b7m\u00fcht'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "auff ein getreues Herze sieht/", "tokens": ["auff", "ein", "ge\u00b7treu\u00b7es", "Her\u00b7ze", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "das steht/ mein Herzensfreund/ dir offe\u0303.", "tokens": ["das", "steht", "/", "mein", "Her\u00b7zens\u00b7freund", "/", "dir", "off\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PPOSAT", "NN", "$(", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein ander mag auff Gold und Sch\u00e4zze/", "tokens": ["Ein", "an\u00b7der", "mag", "auff", "Gold", "und", "Sch\u00e4z\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "auff Augen-schein und eitle Gaben hoffen/", "tokens": ["auff", "Au\u00b7gen\u00b7schein", "und", "eit\u00b7le", "Ga\u00b7ben", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "der wehrten Freundschaft-bund helt and' weit Geseze", "tokens": ["der", "wehr\u00b7ten", "Freund\u00b7schaft\u00b7bund", "helt", "and'", "weit", "Ge\u00b7se\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Mit diesen Zeilen wil ich dir mein Herz un\u0303 mich erbeige\u0303 geben.", "tokens": ["Mit", "die\u00b7sen", "Zei\u00b7len", "wil", "ich", "dir", "mein", "Herz", "u\u00f1", "mich", "erb\u00b7eig\u1ebd", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "KON", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.24": {"text": "ein merers schenk ich/ wird mich Gott zuk\u00fcnftig u. dich lasse\u0303 lebe\u0303.", "tokens": ["ein", "me\u00b7rers", "schenk", "ich", "/", "wird", "mich", "Gott", "zu\u00b7k\u00fcnf\u00b7tig", "u.", "dich", "lass\u1ebd", "le\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "NN", "ADJD", "APPR", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}}}}}