{"dta.poem.5913": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und gnug erwiesen bleibt; so mu\u00df von K\u00fcgelein", "tokens": ["Und", "gnug", "er\u00b7wie\u00b7sen", "bleibt", ";", "so", "mu\u00df", "von", "K\u00fc\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "$.", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zwischen-Raum, in Eile,", "tokens": ["Der", "Zwi\u00b7schen\u00b7Raum", ",", "in", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Best\u00e4ndig angef\u00fcllet seyn", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "seyn"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die beweglichen und kleine Theile", "tokens": ["Durch", "die", "be\u00b7weg\u00b7li\u00b7chen", "und", "klei\u00b7ne", "Thei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Des Cubus, welcher sich ger\u00fcndet,", "tokens": ["Des", "Cu\u00b7bus", ",", "wel\u00b7cher", "sich", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als dessen zarte Kleinigkeit,", "tokens": ["Als", "des\u00b7sen", "zar\u00b7te", "Klei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ohn\u2019 alle Hindrung, allezeit", "tokens": ["Ohn'", "al\u00b7le", "Hin\u00b7drung", ",", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Leicht einen Eingang findet.", "tokens": ["Leicht", "ei\u00b7nen", "Ein\u00b7gang", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Theilchen nun, die v\u00f6llig nicht so klein,", "tokens": ["Die", "Theil\u00b7chen", "nun", ",", "die", "v\u00f6l\u00b7lig", "nicht", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und etwas gr\u00f6ber seyn,", "tokens": ["Und", "et\u00b7was", "gr\u00f6\u00b7ber", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Sind nicht so starck bewegt, und bleiben", "tokens": ["Sind", "nicht", "so", "starck", "be\u00b7wegt", ",", "und", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In ihrer Theilung nicht so gleich und glatt,", "tokens": ["In", "ih\u00b7rer", "Thei\u00b7lung", "nicht", "so", "gleich", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sie haben Falten, Kr\u00fcmmen, Ecken,", "tokens": ["Sie", "ha\u00b7ben", "Fal\u00b7ten", ",", "Kr\u00fcm\u00b7men", ",", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die eine stets sich in die andre stecken,", "tokens": ["Die", "ei\u00b7ne", "stets", "sich", "in", "die", "and\u00b7re", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PRF", "APPR", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die sich zusammen h\u00e4ckeln, fassen,", "tokens": ["Die", "sich", "zu\u00b7sam\u00b7men", "h\u00e4c\u00b7keln", ",", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und in einander treiben lassen.", "tokens": ["Und", "in", "ein\u00b7an\u00b7der", "trei\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}