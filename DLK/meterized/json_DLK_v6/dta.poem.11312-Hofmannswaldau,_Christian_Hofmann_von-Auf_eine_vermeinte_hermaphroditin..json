{"dta.poem.11312": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine vermeinte hermaphroditin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Selbst Hercules wagt sich nicht leicht an zwey:", "tokens": ["Selbst", "Her\u00b7cu\u00b7les", "wagt", "sich", "nicht", "leicht", "an", "zwey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "APPR", "CARD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Drum, Fulvia! sprich nicht, da\u00df ich zu furchtsam sey;", "tokens": ["Drum", ",", "Ful\u00b7via", "!", "sprich", "nicht", ",", "da\u00df", "ich", "zu", "furcht\u00b7sam", "sey", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NE", "$.", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weil unter deinem hemd, ach! solt\u2019 ich nicht erschrecken?", "tokens": ["Weil", "un\u00b7ter", "dei\u00b7nem", "hemd", ",", "ach", "!", "solt'", "ich", "nicht", "er\u00b7schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "ITJ", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein mann und weib zugleich verborgen stecken.", "tokens": ["Ein", "mann", "und", "weib", "zu\u00b7gleich", "ver\u00b7bor\u00b7gen", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}