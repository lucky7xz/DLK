{"dta.poem.19460": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied Chastelard's.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Sehnsucht ist Qual!", "tokens": ["Sehn\u00b7sucht", "ist", "Qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Herrin wag ich's nicht zu sagen,", "tokens": ["Der", "Her\u00b7rin", "wag", "ich's", "nicht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will's den dunkeln Eichen klagen", "tokens": ["Ich", "will's", "den", "dun\u00b7keln", "Ei\u00b7chen", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im gr\u00fcnen Thal:", "tokens": ["Im", "gr\u00fc\u00b7nen", "Thal", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sehnsucht ist Qual.", "tokens": ["Sehn\u00b7sucht", "ist", "Qual", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mein Leib vergeht", "tokens": ["Mein", "Leib", "ver\u00b7geht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie schmelzend Eis in bleichen Farben,", "tokens": ["Wie", "schmel\u00b7zend", "Eis", "in", "blei\u00b7chen", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sieht mich dursten, lechzen, darben,", "tokens": ["Sie", "sieht", "mich", "durs\u00b7ten", ",", "lech\u00b7zen", ",", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "VVINF", "$,", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt unerfleht \u2014", "tokens": ["Bleibt", "un\u00b7er\u00b7fleht"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Leib vergeht.", "tokens": ["Mein", "Leib", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch mag es sein,", "tokens": ["Doch", "mag", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df sie an ihrer Macht sich weide!", "tokens": ["Da\u00df", "sie", "an", "ih\u00b7rer", "Macht", "sich", "wei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ergetzt sie grausam sich an meinem Leide;", "tokens": ["Er\u00b7getzt", "sie", "grau\u00b7sam", "sich", "an", "mei\u00b7nem", "Lei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So denkt sie mein \u2014", "tokens": ["So", "denkt", "sie", "mein"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Drum mag es sein.", "tokens": ["Drum", "mag", "es", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sehnsucht ist Qual!", "tokens": ["Sehn\u00b7sucht", "ist", "Qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dem K\u00fchnsten macht die Folter bange,", "tokens": ["Dem", "K\u00fchns\u00b7ten", "macht", "die", "Fol\u00b7ter", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Grab, darin ich nichts verlange,", "tokens": ["Ein", "Grab", ",", "da\u00b7rin", "ich", "nichts", "ver\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb mir, o Thal!", "tokens": ["Gieb", "mir", ",", "o", "Thal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sehnsucht ist Qual.", "tokens": ["Sehn\u00b7sucht", "ist", "Qual", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}