{"dta.poem.11846": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil das Vergn\u00fcgen, das man bannen will, entweicht,", "tokens": ["Weil", "das", "Ver\u00b7gn\u00fc\u00b7gen", ",", "das", "man", "ban\u00b7nen", "will", ",", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und oft die Lustpartie dir keinen Lustpart reicht;", "tokens": ["Und", "oft", "die", "Lust\u00b7par\u00b7tie", "dir", "kei\u00b7nen", "Lust\u00b7part", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So geh nur dran, wie an ein andres Tagsgesch\u00e4fte,", "tokens": ["So", "geh", "nur", "dran", ",", "wie", "an", "ein", "and\u00b7res", "Tags\u00b7ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An das du eben heut willst setzen Zeit und Kr\u00e4fte.", "tokens": ["An", "das", "du", "e\u00b7ben", "heut", "willst", "set\u00b7zen", "Zeit", "und", "Kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und war's nun ein Gesch\u00e4ft, so hast du's abgethan;", "tokens": ["Und", "wa\u00b7r's", "nun", "ein", "Ge\u00b7sch\u00e4ft", ",", "so", "hast", "du's", "ab\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und war die Lust dabei, so schreib zu Dank sie an.", "tokens": ["Und", "war", "die", "Lust", "da\u00b7bei", ",", "so", "schreib", "zu", "Dank", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PAV", "$,", "ADV", "ADJD", "PTKZU", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}