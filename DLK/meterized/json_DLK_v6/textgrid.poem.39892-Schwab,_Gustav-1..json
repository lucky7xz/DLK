{"textgrid.poem.39892": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Henker mit dem Beile", "tokens": ["Der", "Hen\u00b7ker", "mit", "dem", "Bei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Herzog Alba tritt:", "tokens": ["Vor", "Her\u00b7zog", "Al\u00b7ba", "tritt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu liebest, Herr, die Eile,", "tokens": ["\u00bb", "du", "lie\u00b7best", ",", "Herr", ",", "die", "Ei\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Beil war scharf, es schnitt.", "tokens": ["Mein", "Beil", "war", "scharf", ",", "es", "schnitt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schnitt dem starren Alten", "tokens": ["Es", "schnitt", "dem", "star\u00b7ren", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch's kn\u00f6cherne Genick;", "tokens": ["Durch's", "kn\u00f6\u00b7cher\u00b7ne", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Junge wollt' nicht halten,", "tokens": ["Der", "Jun\u00b7ge", "wollt'", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn zwang der Knechte Strick.", "tokens": ["Ihn", "zwang", "der", "Knech\u00b7te", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Frau \u2013 den kleinen Knaben", "tokens": ["Die", "Frau", "\u2013", "den", "klei\u00b7nen", "Kna\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft von der Brust sie nicht \u2013", "tokens": ["L\u00e4\u00dft", "von", "der", "Brust", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kommt, sie will es haben,", "tokens": ["Sie", "kommt", ",", "sie", "will", "es", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt gleich vor dein Gesicht.\u00ab", "tokens": ["Jetzt", "gleich", "vor", "dein", "Ge\u00b7sicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und vor des Herzogs Augen", "tokens": ["Und", "vor", "des", "Her\u00b7zogs", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trug sie die Mutterbrust,", "tokens": ["Trug", "sie", "die", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie lie\u00df das Kindlein saugen,", "tokens": ["Sie", "lie\u00df", "das", "Kin\u00b7dlein", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie blickt es an mit Lust.", "tokens": ["Sie", "blickt", "es", "an", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Weib sprach ohne Beben:", "tokens": ["Das", "Weib", "sprach", "oh\u00b7ne", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbmein Kind ist noch nicht satt,", "tokens": ["\u00bb", "mein", "Kind", "ist", "noch", "nicht", "satt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich so lange leben,", "tokens": ["La\u00df", "mich", "so", "lan\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis es getrunken hat.", "tokens": ["Bis", "es", "ge\u00b7trun\u00b7ken", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es liegt auf weichen Pf\u00fchlen", "tokens": ["Es", "liegt", "auf", "wei\u00b7chen", "Pf\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem s\u00fc\u00dfen Born.", "tokens": ["An", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Born", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, k\u00f6nntest du das f\u00fchlen,", "tokens": ["Ja", ",", "k\u00f6nn\u00b7test", "du", "das", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So legte sich dein Zorn!\u00ab", "tokens": ["So", "leg\u00b7te", "sich", "dein", "Zorn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Herzog sprach mit Hohne:", "tokens": ["Der", "Her\u00b7zog", "sprach", "mit", "Hoh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbwerd' ich ein S\u00e4ugling \u2013 gut!", "tokens": ["\u00bb", "werd'", "ich", "ein", "S\u00e4ug\u00b7ling", "\u2013", "gut", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann hoffet, da\u00df ich schone;", "tokens": ["Dann", "hof\u00b7fet", ",", "da\u00df", "ich", "scho\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr jetzt will ich dein Blut!\u00ab", "tokens": ["F\u00fcr", "jetzt", "will", "ich", "dein", "Blut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als drauf der Diener fa\u00dfte", "tokens": ["Als", "drauf", "der", "Die\u00b7ner", "fa\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Kind mit rauher Hand,", "tokens": ["Das", "Kind", "mit", "rau\u00b7her", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter erst erbla\u00dfte,", "tokens": ["Die", "Mut\u00b7ter", "erst", "er\u00b7bla\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mutter erst entbrannt'.", "tokens": ["Die", "Mut\u00b7ter", "erst", "ent\u00b7brannt'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es hob in wilden Wellen", "tokens": ["Es", "hob", "in", "wil\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich ihre blo\u00dfe Brust,", "tokens": ["Sich", "ih\u00b7re", "blo\u00b7\u00dfe", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ward zu Feuerquellen", "tokens": ["Es", "ward", "zu", "Feu\u00b7er\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Augen stille Lust.", "tokens": ["Der", "Au\u00b7gen", "stil\u00b7le", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie rief: \u00bbO s\u00fc\u00df ist Sterben,", "tokens": ["Sie", "rief", ":", "\u00bb", "O", "s\u00fc\u00df", "ist", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn eins vom Hiebe stirbt!", "tokens": ["Wenn", "eins", "vom", "Hie\u00b7be", "stirbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, Herzog, sollt verderben,", "tokens": ["Du", ",", "Her\u00b7zog", ",", "sollt", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie welkend man verdirbt!", "tokens": ["Wie", "wel\u00b7kend", "man", "ver\u00b7dirbt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nach Leben sollst du trachten", "tokens": ["Nach", "Le\u00b7ben", "sollst", "du", "trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sollst, wie unentw\u00f6hnt", "tokens": ["Und", "sollst", ",", "wie", "un\u00b7ent\u00b7w\u00f6hnt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Kindlein dort, verschmachten,", "tokens": ["Mein", "Kin\u00b7dlein", "dort", ",", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nach der Mutter st\u00f6hnt!\u00ab", "tokens": ["Das", "nach", "der", "Mut\u00b7ter", "st\u00f6hnt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Herzog hat's vernommen,", "tokens": ["Der", "Her\u00b7zog", "hat's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er h\u00f6rt ihr schweigend zu;", "tokens": ["Er", "h\u00f6rt", "ihr", "schwei\u00b7gend", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Henker l\u00e4\u00dft er kommen", "tokens": ["Den", "Hen\u00b7ker", "l\u00e4\u00dft", "er", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schaffet bald sich Ruh.", "tokens": ["Und", "schaf\u00b7fet", "bald", "sich", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er l\u00e4\u00dft im Tod sie trotzen,", "tokens": ["Er", "l\u00e4\u00dft", "im", "Tod", "sie", "trot\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sitzt im Purpurglanz,", "tokens": ["Er", "sitzt", "im", "Pur\u00b7pur\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Leben f\u00fchlt er strotzen", "tokens": ["Sein", "Le\u00b7ben", "f\u00fchlt", "er", "strot\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Mark des Niederlands.", "tokens": ["Vom", "Mark", "des", "Nie\u00b7der\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}