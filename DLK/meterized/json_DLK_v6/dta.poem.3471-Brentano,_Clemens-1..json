{"dta.poem.3471": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach meiner Lieb viel hundert Knaben trachten,               ", "tokens": ["Nach", "mei\u00b7ner", "Lieb", "viel", "hun\u00b7dert", "Kna\u00b7ben", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der, den ich lieb, will mein nicht achten,", "tokens": ["Al\u00b7lein", "der", ",", "den", "ich", "lieb", ",", "will", "mein", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "ADJD", "$,", "VMFIN", "PPOSAT", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach weh mir armen Maid, vor Leid mu\u00df ich verschmachten.", "tokens": ["Ach", "weh", "mir", "ar\u00b7men", "Maid", ",", "vor", "Leid", "mu\u00df", "ich", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jeder begehrt zu mir sich zu verpflichten,", "tokens": ["Je\u00b7der", "be\u00b7gehrt", "zu", "mir", "sich", "zu", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Allein der, den ich lieb, thut mich vernichten,", "tokens": ["Al\u00b7lein", "der", ",", "den", "ich", "lieb", ",", "thut", "mich", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "ADJD", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach weh mir armen Maid, was soll ich dann anrichten.", "tokens": ["Ach", "weh", "mir", "ar\u00b7men", "Maid", ",", "was", "soll", "ich", "dann", "an\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "All andre thun mir Gutes viel verjehen,", "tokens": ["All", "and\u00b7re", "thun", "mir", "Gu\u00b7tes", "viel", "ver\u00b7je\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der, den ich lieb, mag mich nicht sehen,", "tokens": ["Al\u00b7lein", "der", ",", "den", "ich", "lieb", ",", "mag", "mich", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "ADJD", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Ach weh mir armen Maid, wie mu\u00df mir dann geschehen.", "tokens": ["Ach", "weh", "mir", "ar\u00b7men", "Maid", ",", "wie", "mu\u00df", "mir", "dann", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von allen keiner mag mir widerstreben,", "tokens": ["Von", "al\u00b7len", "kei\u00b7ner", "mag", "mir", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201eallein der, den ich lieb, will sich nicht geben,", "tokens": ["\u201e", "al\u00b7lein", "der", ",", "den", "ich", "lieb", ",", "will", "sich", "nicht", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "$,", "PRELS", "PPER", "ADJD", "$,", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach weh mir armen Maid, was soll mir dann das Leben.", "tokens": ["Ach", "weh", "mir", "ar\u00b7men", "Maid", ",", "was", "soll", "mir", "dann", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich wollt, da\u00df der verhindert mich", "tokens": ["Ich", "wollt", ",", "da\u00df", "der", "ver\u00b7hin\u00b7dert", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meinem Gl\u00fcck, sollt halten sich", "tokens": ["An", "mei\u00b7nem", "Gl\u00fcck", ",", "sollt", "hal\u00b7ten", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Jahr nach meinem Willen,", "tokens": ["Ein", "Jahr", "nach", "mei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wollt ihm gar in kurzer Zeit, all seinen Hochmuth               ", "tokens": ["Ich", "wollt", "ihm", "gar", "in", "kur\u00b7zer", "Zeit", ",", "all", "sei\u00b7nen", "Hoch\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich wollt, da\u00df der mein jezund spott,", "tokens": ["Ich", "wollt", ",", "da\u00df", "der", "mein", "je\u00b7zund", "spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jahr sollt halten mein Gebot,", "tokens": ["Ein", "Jahr", "sollt", "hal\u00b7ten", "mein", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er w\u00fcrd dermassen b\u00fcssen,", "tokens": ["Er", "w\u00fcrd", "der\u00b7mas\u00b7sen", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihm gewi\u00df in Tagen kurz, seins Lebens sollt ver-               ", "tokens": ["Da\u00df", "ihm", "ge\u00b7wi\u00df", "in", "Ta\u00b7gen", "kurz", ",", "seins", "Le\u00b7bens", "sollt", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "ADJD", "$,", "PPOSAT", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich bin gen Baden zogen,", "tokens": ["Ich", "bin", "gen", "Ba\u00b7den", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu l\u00f6schen meine Brunst,", "tokens": ["Zu", "l\u00f6\u00b7schen", "mei\u00b7ne", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So find ich mich betrogen,", "tokens": ["So", "find", "ich", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn es ist gar umsunst,", "tokens": ["Denn", "es", "ist", "gar", "um\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer kann das Feuer kennen,", "tokens": ["Wer", "kann", "das", "Feu\u00b7er", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mir mein Herz thut brennen!", "tokens": ["Das", "mir", "mein", "Herz", "thut", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich thu mich vielmals w\u00e4schen", "tokens": ["Ich", "thu", "mich", "viel\u00b7mals", "w\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Wasser kalt und hei\u00df,", "tokens": ["Mit", "Was\u00b7ser", "kalt", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kann doch nicht erl\u00f6schen,", "tokens": ["Und", "kann", "doch", "nicht", "er\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja mein kein Rath mehr wei\u00df,", "tokens": ["Ja", "mein", "kein", "Rath", "mehr", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kann nicht das Feuer kennen,", "tokens": ["Kann", "nicht", "das", "Feu\u00b7er", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mir im Herz thut brennen.", "tokens": ["Das", "mir", "im", "Herz", "thut", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn ich den ganzen Tag", "tokens": ["Wenn", "ich", "den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gef\u00fchrt hab meine Klag,", "tokens": ["Ge\u00b7f\u00fchrt", "hab", "mei\u00b7ne", "Klag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So giebts mir noch zu schaffen", "tokens": ["So", "giebts", "mir", "noch", "zu", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey Nacht, wann ich soll schlafen.", "tokens": ["Bey", "Nacht", ",", "wann", "ich", "soll", "schla\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Traum mit gro\u00dfem Schrecken", "tokens": ["Ein", "Traum", "mit", "gro\u00b7\u00dfem", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Thut mich gar oft aufwecken.", "tokens": ["Thut", "mich", "gar", "oft", "auf\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Schlaf seh ich den Schein", "tokens": ["Im", "Schlaf", "seh", "ich", "den", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Allerliebsten mein,", "tokens": ["Des", "Al\u00b7ler\u00b7liebs\u00b7ten", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem starken Bogen,", "tokens": ["Mit", "ei\u00b7nem", "star\u00b7ken", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darauf viel Pfeil gezogen,", "tokens": ["Da\u00b7rauf", "viel", "Pfeil", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit will er mich heben", "tokens": ["Da\u00b7mit", "will", "er", "mich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Aus diesem schweren Leben.", "tokens": ["Aus", "die\u00b7sem", "schwe\u00b7ren", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zu solchem Schreckgesicht", "tokens": ["Zu", "sol\u00b7chem", "Schreck\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kann ich stillschweigen nicht,", "tokens": ["Kann", "ich", "still\u00b7schwei\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schrei mit lauter Stimmen:", "tokens": ["Ich", "schrei", "mit", "lau\u00b7ter", "Stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eo Knabe la\u00df dein Grimmen,", "tokens": ["\u201e", "o", "Kna\u00b7be", "la\u00df", "dein", "Grim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201enicht wollst, weil ich thu schlafen,", "tokens": ["\u201e", "nicht", "wollst", ",", "weil", "ich", "thu", "schla\u00b7fen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ejezt brauchen deine Waffen.\u201c", "tokens": ["\u201e", "jezt", "brau\u00b7chen", "dei\u00b7ne", "Waf\u00b7fen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ach hartes Herz, la\u00df dich doch eins erweichen,", "tokens": ["Ach", "har\u00b7tes", "Herz", ",", "la\u00df", "dich", "doch", "eins", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mich zu deiner Huld doch noch gereichen;", "tokens": ["La\u00df", "mich", "zu", "dei\u00b7ner", "Huld", "doch", "noch", "ge\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iam