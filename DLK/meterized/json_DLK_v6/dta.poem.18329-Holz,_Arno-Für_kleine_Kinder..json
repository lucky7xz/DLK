{"dta.poem.18329": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr kleine Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der lehrte mich das Dichten:", "tokens": ["Der", "lehr\u00b7te", "mich", "das", "Dich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Volk und ein St\u00fcckchen Marzipan", "tokens": ["Ein", "Volk", "und", "ein", "St\u00fcck\u00b7chen", "Mar\u00b7zi\u00b7pan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bestehn aus zweierlei Schichten.", "tokens": ["Be\u00b7stehn", "aus", "zwei\u00b7er\u00b7lei", "Schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die eine schl\u00fcrft Austern und baut sich Kohl", "tokens": ["Die", "ei\u00b7ne", "schl\u00fcrft", "Aus\u00b7tern", "und", "baut", "sich", "Kohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "NN", "KON", "VVFIN", "PRF", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und macht in Vaterlandstreue", "tokens": ["Und", "macht", "in", "Va\u00b7ter\u00b7land\u00b7streu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und f\u00fchlt sich kannibalisch wohl", "tokens": ["Und", "f\u00fchlt", "sich", "kan\u00b7ni\u00b7ba\u00b7lisch", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Goethes f\u00fcnfhundert S\u00e4ue.", "tokens": ["Wie", "Goe\u00b7thes", "f\u00fcnf\u00b7hun\u00b7dert", "S\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "CARD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die andere spielt tagt\u00e4glich ", "tokens": ["Die", "an\u00b7de\u00b7re", "spielt", "tag\u00b7t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und kleidet sich in Lappen", "tokens": ["Und", "klei\u00b7det", "sich", "in", "Lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fchrt ihr ganzes Lebenlang", "tokens": ["Und", "f\u00fchrt", "ihr", "gan\u00b7zes", "Le\u00b7ben\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen Hungerknochen im Wappen!", "tokens": ["Ei\u00b7nen", "Hun\u00b7ger\u00b7kno\u00b7chen", "im", "Wap\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}