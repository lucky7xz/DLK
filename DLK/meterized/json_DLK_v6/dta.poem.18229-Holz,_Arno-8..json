{"dta.poem.18229": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Liegt die goldne Zeit,", "tokens": ["Liegt", "die", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo mein Herz von tausend Liedern schwoll!", "tokens": ["Wo", "mein", "Herz", "von", "tau\u00b7send", "Lie\u00b7dern", "schwoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "CARD", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nun ist stumm mein Mund", "tokens": ["Nun", "ist", "stumm", "mein", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und mein Herz so wund", "tokens": ["Und", "mein", "Herz", "so", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ist von Thr\u00e4nen, nur von Thr\u00e4nen voll!", "tokens": ["Ist", "von", "Thr\u00e4\u00b7nen", ",", "nur", "von", "Thr\u00e4\u00b7nen", "voll", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O was g\u00e4b ich drum,", "tokens": ["O", "was", "g\u00e4b", "ich", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00e4r ich nicht so stumm,", "tokens": ["W\u00e4r", "ich", "nicht", "so", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Thr\u00e4ne f\u00e4nde ihren Lauf!", "tokens": ["Und", "die", "Thr\u00e4\u00b7ne", "f\u00e4n\u00b7de", "ih\u00b7ren", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber Lied wie Schmerz,", "tokens": ["A\u00b7ber", "Lied", "wie", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00fctet stumm das Herz,", "tokens": ["H\u00fc\u00b7tet", "stumm", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und wer kommt und schiebt den Riegel auf?", "tokens": ["Und", "wer", "kommt", "und", "schiebt", "den", "Rie\u00b7gel", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Junger Liebe Gl\u00fcck,", "tokens": ["Jun\u00b7ger", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kehrst du nie zur\u00fcck?", "tokens": ["Kehrst", "du", "nie", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, das Herz mir noch das Herz zerbricht!", "tokens": ["Ach", ",", "das", "Herz", "mir", "noch", "das", "Herz", "zer\u00b7bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein Funkelstern,", "tokens": ["Wie", "ein", "Fun\u00b7kels\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "O so ewig fern,", "tokens": ["O", "so", "e\u00b7wig", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gl\u00e4nzt die goldne Zeit im goldnen Licht!", "tokens": ["Gl\u00e4nzt", "die", "gold\u00b7ne", "Zeit", "im", "gold\u00b7nen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}