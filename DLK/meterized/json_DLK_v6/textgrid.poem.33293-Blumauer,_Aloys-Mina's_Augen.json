{"textgrid.poem.33293": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Mina's Augen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Augen sind's, aus deren Blicken", "tokens": ["Zwei", "Au\u00b7gen", "sin\u00b7d's", ",", "aus", "de\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne selbst ihr Feuer stahl,", "tokens": ["Die", "Son\u00b7ne", "selbst", "ihr", "Feu\u00b7er", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht, M\u00e4nnerherzen, gleich den M\u00fccken,", "tokens": ["Seht", ",", "M\u00e4n\u00b7ner\u00b7her\u00b7zen", ",", "gleich", "den", "M\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreh'n taumelnd sich in ihrem Strahl.", "tokens": ["Dreh'n", "tau\u00b7melnd", "sich", "in", "ih\u00b7rem", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O sonnt' ich doch in diesen Augen,", "tokens": ["O", "sonnt'", "ich", "doch", "in", "die\u00b7sen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den M\u00fccken gleich, mein Angesicht,", "tokens": ["Den", "M\u00fc\u00b7cken", "gleich", ",", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O d\u00fcrft ich Lieb' aus ihnen saugen,", "tokens": ["O", "d\u00fcrft", "ich", "Lieb'", "aus", "ih\u00b7nen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4rmen mich an ihrem Licht!", "tokens": ["Und", "w\u00e4r\u00b7men", "mich", "an", "ih\u00b7rem", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}