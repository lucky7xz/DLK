{"textgrid.poem.32662": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Alten", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es leben die Alten,", "tokens": ["Es", "le\u00b7ben", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die M\u00e4dchen und Wein", "tokens": ["Die", "M\u00e4d\u00b7chen", "und", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "F\u00fcr Mittel gehalten,", "tokens": ["F\u00fcr", "Mit\u00b7tel", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sich weislich zu freun!", "tokens": ["Sich", "weis\u00b7lich", "zu", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie \u00fcbten die Pflichten", "tokens": ["Sie", "\u00fcb\u00b7ten", "die", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des Biedermanns aus,", "tokens": ["Des", "Bie\u00b7der\u00b7manns", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und lachten in Z\u00fcchten", "tokens": ["Und", "lach\u00b7ten", "in", "Z\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Beim n\u00e4chtlichen Schmaus.", "tokens": ["Beim", "n\u00e4cht\u00b7li\u00b7chen", "Schmaus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Da lud man die Jugend", "tokens": ["Da", "lud", "man", "die", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Mahle mit ein,", "tokens": ["Zum", "Mah\u00b7le", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und predigte Tugend", "tokens": ["Und", "pre\u00b7dig\u00b7te", "Tu\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Durch Thaten allein;", "tokens": ["Durch", "Tha\u00b7ten", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Man r\u00fchmte die Gro\u00dfen,", "tokens": ["Man", "r\u00fchm\u00b7te", "die", "Gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die, tapfer und gut,", "tokens": ["Die", ",", "tap\u00b7fer", "und", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Kein andres vergossen,", "tokens": ["Kein", "and\u00b7res", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Als feindliches Blut.", "tokens": ["Als", "feind\u00b7li\u00b7ches", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dem Lande zu Ehren", "tokens": ["Dem", "Lan\u00b7de", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nahm jeder sein Glas;", "tokens": ["Nahm", "je\u00b7der", "sein", "Glas", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Vergn\u00fcgen half's leeren,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "ha\u00b7lf's", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch hielten sie Ma\u00df,", "tokens": ["Doch", "hiel\u00b7ten", "sie", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und lachten sich n\u00fcchtern,", "tokens": ["Und", "lach\u00b7ten", "sich", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und sangen, in Ruh,", "tokens": ["Und", "san\u00b7gen", ",", "in", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Von fr\u00f6hlichen Dichtern", "tokens": ["Von", "fr\u00f6h\u00b7li\u00b7chen", "Dich\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ein Liedchen dazu.", "tokens": ["Ein", "Lied\u00b7chen", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Um Mitternacht schieden", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "schie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie k\u00fcssend vom Schmaus,", "tokens": ["Sie", "k\u00fcs\u00b7send", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und kehrten in Frieden", "tokens": ["Und", "kehr\u00b7ten", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum Weibchen nach Haus.", "tokens": ["Zum", "Weib\u00b7chen", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es leben die Alten!", "tokens": ["Es", "le\u00b7ben", "die", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wir folgen dem Brauch,", "tokens": ["Wir", "fol\u00b7gen", "dem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Auf den sie gehalten,", "tokens": ["Auf", "den", "sie", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und freuen uns auch.", "tokens": ["Und", "freu\u00b7en", "uns", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}