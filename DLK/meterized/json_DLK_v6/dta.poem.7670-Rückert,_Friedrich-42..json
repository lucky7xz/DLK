{"dta.poem.7670": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, du k\u00f6nnest dich nicht mit der Welt vertragen,", "tokens": ["Du", "klagst", ",", "du", "k\u00f6n\u00b7nest", "dich", "nicht", "mit", "der", "Welt", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht der Geselligkeit Beschr\u00e4nkungen ertragen.", "tokens": ["Nicht", "der", "Ge\u00b7sel\u00b7lig\u00b7keit", "Be\u00b7schr\u00e4n\u00b7kun\u00b7gen", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zur Wildnis fliehest du, dem Menschen zu entfliehn;", "tokens": ["Zur", "Wild\u00b7nis", "flie\u00b7hest", "du", ",", "dem", "Men\u00b7schen", "zu", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du tr\u00e4gst ihn mit an dir, und kannst ihn aus nicht ziehn.", "tokens": ["Du", "tr\u00e4gst", "ihn", "mit", "an", "dir", ",", "und", "kannst", "ihn", "aus", "nicht", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$,", "KON", "VMFIN", "PPER", "APPR", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn aber du dich selbst ertragen mu\u00dft und leiden;", "tokens": ["Wenn", "a\u00b7ber", "du", "dich", "selbst", "er\u00b7tra\u00b7gen", "mu\u00dft", "und", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von deinem Ebenbild warum willst du dich scheiden?", "tokens": ["Von", "dei\u00b7nem", "E\u00b7ben\u00b7bild", "wa\u00b7rum", "willst", "du", "dich", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du f\u00fchlst mit der Natur dich mehr in Eintracht nur,", "tokens": ["Du", "f\u00fchlst", "mit", "der", "Na\u00b7tur", "dich", "mehr", "in", "Ein\u00b7tracht", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du nicht ihrem Gang vorzeichnest deine Spur;", "tokens": ["Weil", "du", "nicht", "ih\u00b7rem", "Gang", "vor\u00b7zeich\u00b7nest", "dei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Menschen aber willst du deine Wege zeigen,", "tokens": ["Den", "Men\u00b7schen", "a\u00b7ber", "willst", "du", "dei\u00b7ne", "We\u00b7ge", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedenklos da\u00df, wie du, auch jeder ist sein eigen.", "tokens": ["Be\u00b7den\u00b7klos", "da\u00df", ",", "wie", "du", ",", "auch", "je\u00b7der", "ist", "sein", "ei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "$,", "PWAV", "PPER", "$,", "ADV", "PIS", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Tr\u00e4gst du ohn' Ungeduld Frost, Regen, Sturm und Wind?", "tokens": ["Tr\u00e4gst", "du", "ohn'", "Un\u00b7ge\u00b7duld", "Frost", ",", "Re\u00b7gen", ",", "Sturm", "und", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Menschenunbestand ist dir zu ungelind?", "tokens": ["Nur", "Men\u00b7schen\u00b7un\u00b7be\u00b7stand", "ist", "dir", "zu", "un\u00b7ge\u00b7lind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Mann, der vor dem Zwang des Lebens nimmt die Flucht,", "tokens": ["Der", "Mann", ",", "der", "vor", "dem", "Zwang", "des", "Le\u00b7bens", "nimmt", "die", "Flucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wie der Knabe, der entl\u00e4uft der Eltern Zucht,", "tokens": ["Ist", "wie", "der", "Kna\u00b7be", ",", "der", "ent\u00b7l\u00e4uft", "der", "El\u00b7tern", "Zucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der sich bequemen will ehr allem Unbequemen,", "tokens": ["Der", "sich", "be\u00b7que\u00b7men", "will", "ehr", "al\u00b7lem", "Un\u00b7be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "VMFIN", "ADV", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Rache, wie er meint, nur an der Zucht zu nehmen.", "tokens": ["Um", "Ra\u00b7che", ",", "wie", "er", "meint", ",", "nur", "an", "der", "Zucht", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der rechte Mann erkennt und ehrt des Lebens Schranken,", "tokens": ["Der", "rech\u00b7te", "Mann", "er\u00b7kennt", "und", "ehrt", "des", "Le\u00b7bens", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Erkenntnis wird er seine Freiheit danken.", "tokens": ["Und", "der", "Er\u00b7kennt\u00b7nis", "wird", "er", "sei\u00b7ne", "Frei\u00b7heit", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sein Innres ist sein Thun, das strebt er zu vermehren;", "tokens": ["Sein", "Inn\u00b7res", "ist", "sein", "Thun", ",", "das", "strebt", "er", "zu", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von au\u00dfen leidet er, das strebt er abzuwehren.", "tokens": ["Von", "au\u00b7\u00dfen", "lei\u00b7det", "er", ",", "das", "strebt", "er", "ab\u00b7zu\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und selbst sein Leiden wei\u00df in Thun er zu verwandeln,", "tokens": ["Und", "selbst", "sein", "Lei\u00b7den", "wei\u00df", "in", "Thun", "er", "zu", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn menschlich handelnd er lehrt Menschen menschlich handeln.", "tokens": ["Wenn", "menschlich", "han\u00b7delnd", "er", "lehrt", "Men\u00b7schen", "menschlich", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "PPER", "VVFIN", "NN", "ADJD", "VVINF", "$."], "meter": "-+---++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Denn uneins unter sich macht Menschen Leidenschaft,", "tokens": ["Denn", "un\u00b7eins", "un\u00b7ter", "sich", "macht", "Men\u00b7schen", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur in der Vernunft ist ihrer Einheit Kraft.", "tokens": ["Und", "nur", "in", "der", "Ver\u00b7nunft", "ist", "ih\u00b7rer", "Ein\u00b7heit", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Des Menschen Aufgab' ist Erziehung und Entwildung", "tokens": ["Des", "Men\u00b7schen", "Auf\u00b7gab'", "ist", "Er\u00b7zie\u00b7hung", "und", "Ent\u00b7wil\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des menschlichen Geschlechts und eigne Menschheitsbildung.", "tokens": ["Des", "menschli\u00b7chen", "Ge\u00b7schlechts", "und", "eig\u00b7ne", "Menschheits\u00b7bil\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}