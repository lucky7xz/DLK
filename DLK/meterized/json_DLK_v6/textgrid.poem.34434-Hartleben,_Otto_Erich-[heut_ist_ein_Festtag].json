{"textgrid.poem.34434": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[heut ist ein Festtag]", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut ist ein Festtag,", "tokens": ["Heut", "ist", "ein", "Fest\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ellen, ein Freudentag!", "tokens": ["El\u00b7len", ",", "ein", "Freu\u00b7den\u00b7tag", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Weisst du? Den ersten Kuss", "tokens": ["Weisst", "du", "?", "Den", "ers\u00b7ten", "Kuss"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "raubt ich dir heut.", "tokens": ["raubt", "ich", "dir", "heut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ach, warst du b\u00f6se", "tokens": ["Ach", ",", "warst", "du", "b\u00f6\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u2013 als es geschehen war;", "tokens": ["\u2013", "als", "es", "ge\u00b7sche\u00b7hen", "war", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "riebst dir die Lippen wund,", "tokens": ["riebst", "dir", "die", "Lip\u00b7pen", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "weintest beinah!", "tokens": ["wein\u00b7test", "bei\u00b7nah", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Sage doch, Ellen:", "tokens": ["Sa\u00b7ge", "doch", ",", "El\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "hast dich doch kaum gewehrt,", "tokens": ["hast", "dich", "doch", "kaum", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "als ich dein K\u00f6pfchen nahm \u2013", "tokens": ["als", "ich", "dein", "K\u00f6pf\u00b7chen", "nahm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "hast mich wohl lieb?", "tokens": ["hast", "mich", "wohl", "lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}