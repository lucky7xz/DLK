{"dta.poem.7540": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Feind ist zweierlei, ein Feind der B\u00f6ses that", "tokens": ["Dein", "Feind", "ist", "zwei\u00b7er\u00b7lei", ",", "ein", "Feind", "der", "B\u00f6\u00b7ses", "that"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "$,", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dir, und einer der's von dir erlitten hat.", "tokens": ["An", "dir", ",", "und", "ei\u00b7ner", "der's", "von", "dir", "er\u00b7lit\u00b7ten", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "ART", "PIS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mu\u00dft du um Hilf' in Noth den einen von den beiden", "tokens": ["Mu\u00dft", "du", "um", "Hil\u00b7f'", "in", "Noth", "den", "ei\u00b7nen", "von", "den", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NE", "APPR", "NN", "ART", "ART", "APPR", "ART", "PIAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Anrufen, sei's der that, nicht der es mu\u00dfte leiden.", "tokens": ["An\u00b7ru\u00b7fen", ",", "sei's", "der", "that", ",", "nicht", "der", "es", "mu\u00df\u00b7te", "lei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "VVFIN", "$,", "PTKNEG", "ART", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn jenem steht nun zu, da\u00df gut er's wieder mache,", "tokens": ["Denn", "je\u00b7nem", "steht", "nun", "zu", ",", "da\u00df", "gut", "er's", "wie\u00b7der", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "ADJD", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dieser sinnet nur auf des Erlittnen Rache.", "tokens": ["Doch", "die\u00b7ser", "sin\u00b7net", "nur", "auf", "des", "Er\u00b7litt\u00b7nen", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}