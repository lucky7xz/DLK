{"textgrid.poem.37691": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebe Furcht ist Fackel meiner Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebe Furcht ist Fackel meiner Liebe", "tokens": ["Der", "Lie\u00b7be", "Furcht", "ist", "Fa\u00b7ckel", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meinen Traum mit Strahlen Nachts erfreut,", "tokens": ["Die", "mei\u00b7nen", "Traum", "mit", "Strah\u00b7len", "Nachts", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit mich nicht die Einsamkeit betr\u00fcbe,", "tokens": ["Da\u00b7mit", "mich", "nicht", "die", "Ein\u00b7sam\u00b7keit", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir Sterne auf die dunkle Erde streut,", "tokens": ["Mir", "Ster\u00b7ne", "auf", "die", "dunk\u00b7le", "Er\u00b7de", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und meiner Liebe Flamme h\u00f6her treibt,", "tokens": ["Und", "mei\u00b7ner", "Lie\u00b7be", "Flam\u00b7me", "h\u00f6\u00b7her", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df dir ein Zeichen bleibt.", "tokens": ["Da\u00df", "dir", "ein", "Zei\u00b7chen", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Liebesfurcht ich seh die Wolken jagen", "tokens": ["In", "Lie\u00b7bes\u00b7furcht", "ich", "seh", "die", "Wol\u00b7ken", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort \u00fcber'm Mond, da\u00df er zu wanken scheint.", "tokens": ["Dort", "\u00fc\u00b7ber'm", "Mond", ",", "da\u00df", "er", "zu", "wan\u00b7ken", "scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohin, wohin will euch der Sturmwind tragen?,", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "will", "euch", "der", "Sturm\u00b7wind", "tra\u00b7gen", "?", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "$,", "PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu meinem Lieben, der es treulich meint!", "tokens": ["Zu", "mei\u00b7nem", "Lie\u00b7ben", ",", "der", "es", "treu\u00b7lich", "meint", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Blume Bl\u00e4tter warf ich in den Wind,", "tokens": ["Der", "Blu\u00b7me", "Bl\u00e4t\u00b7ter", "warf", "ich", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er bringt sie dir geschwind.", "tokens": ["Er", "bringt", "sie", "dir", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Liebe Furcht durchbebet mich so sachte,", "tokens": ["Der", "Lie\u00b7be", "Furcht", "durch\u00b7be\u00b7bet", "mich", "so", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu schauen, ob mein Kind noch athmen kann,", "tokens": ["Zu", "schau\u00b7en", ",", "ob", "mein", "Kind", "noch", "ath\u00b7men", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es sah mich an, und drehte sich und lachte.", "tokens": ["Es", "sah", "mich", "an", ",", "und", "dreh\u00b7te", "sich", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah es schon wie dich, wenn es ein Mann;", "tokens": ["Ich", "sah", "es", "schon", "wie", "dich", ",", "wenn", "es", "ein", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schauet aus der Liebe \u00f6dem Haus", "tokens": ["So", "schau\u00b7et", "aus", "der", "Lie\u00b7be", "\u00f6\u00b7dem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein frommer Geist voraus.", "tokens": ["Ein", "from\u00b7mer", "Geist", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wird Liebe Furcht, so la\u00df die Furcht mich lieben,", "tokens": ["Wird", "Lie\u00b7be", "Furcht", ",", "so", "la\u00df", "die", "Furcht", "mich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$,", "ADV", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und liebe mich, dieweil ich furchtsam bin,", "tokens": ["Und", "lie\u00b7be", "mich", ",", "die\u00b7weil", "ich", "furcht\u00b7sam", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So kann die Furcht die Liebe nie betr\u00fcben,", "tokens": ["So", "kann", "die", "Furcht", "die", "Lie\u00b7be", "nie", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Furcht und Liebe haben gleichen Sinn,", "tokens": ["Und", "Furcht", "und", "Lie\u00b7be", "ha\u00b7ben", "glei\u00b7chen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es w\u00e4chst die Furcht der Liebe zum Gewinn", "tokens": ["Es", "w\u00e4chst", "die", "Furcht", "der", "Lie\u00b7be", "zum", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In deiner Liebe Sinn.", "tokens": ["In", "dei\u00b7ner", "Lie\u00b7be", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}