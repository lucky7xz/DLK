{"textgrid.poem.36341": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Uralte Riesenzeiten,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uralte Riesenzeiten,", "tokens": ["Ur\u00b7al\u00b7te", "Rie\u00b7sen\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Helden Wunderstreiten,", "tokens": ["Der", "Hel\u00b7den", "Wun\u00b7der\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schlang all' die \u00d6d' hinab.", "tokens": ["Schlang", "all'", "die", "\u00d6d'", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verschollen ist die Klage,", "tokens": ["Ver\u00b7schol\u00b7len", "ist", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verstummt die graue Sage,", "tokens": ["Ver\u00b7stummt", "die", "grau\u00b7e", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es deckt uns all' ein Grab.", "tokens": ["Es", "deckt", "uns", "all'", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom Winterschlaf umwunden,", "tokens": ["Vom", "Win\u00b7ter\u00b7schlaf", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel tausend Jahr' gebunden,", "tokens": ["Viel", "tau\u00b7send", "Jahr'", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "D\u00e4mmert der Mensch so fort.", "tokens": ["D\u00e4m\u00b7mert", "der", "Mensch", "so", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gebannt in engem Kreise,", "tokens": ["Ge\u00b7bannt", "in", "en\u00b7gem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "M\u00fchsam die ird'sche Reise,", "tokens": ["M\u00fch\u00b7sam", "die", "ird'\u00b7sche", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Erstirbt zuletzt das Wort.", "tokens": ["Er\u00b7stirbt", "zu\u00b7letzt", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn Morgenrot erscheinet,", "tokens": ["Wenn", "Mor\u00b7gen\u00b7rot", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nacht uns der Braut vereinet,", "tokens": ["Nacht", "uns", "der", "Braut", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Neu gr\u00fcnt der alte Bund;", "tokens": ["Neu", "gr\u00fcnt", "der", "al\u00b7te", "Bund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In heil'ge Flut versenket,", "tokens": ["In", "heil'\u00b7ge", "Flut", "ver\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Geist die Wunder denket,", "tokens": ["Der", "Geist", "die", "Wun\u00b7der", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00d6ffnet sich bald der Mund.", "tokens": ["\u00d6ff\u00b7net", "sich", "bald", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In Fr\u00fchlings Glut und Schatten,", "tokens": ["In", "Fr\u00fch\u00b7lings", "Glut", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Lieb' und Tod sich gatten,", "tokens": ["Wo", "Lieb'", "und", "Tod", "sich", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erwacht die k\u00fchne Lust;", "tokens": ["Er\u00b7wacht", "die", "k\u00fch\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da brechen hohe Lieder,", "tokens": ["Da", "bre\u00b7chen", "ho\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die alten Quellen wieder,", "tokens": ["Die", "al\u00b7ten", "Quel\u00b7len", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus der befreiten Brust.", "tokens": ["Aus", "der", "be\u00b7frei\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun \u00f6ffnen sich die Zeichen;", "tokens": ["Nun", "\u00f6ff\u00b7nen", "sich", "die", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es mag das Licht erreichen,", "tokens": ["Es", "mag", "das", "Licht", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den keine Fessel h\u00e4lt.", "tokens": ["Den", "kei\u00b7ne", "Fes\u00b7sel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde bl\u00fcht verwandelt,", "tokens": ["Die", "Er\u00b7de", "bl\u00fcht", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der trunkne Dichter wandelt", "tokens": ["Der", "trunk\u00b7ne", "Dich\u00b7ter", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In sel'ger Geisterwelt.", "tokens": ["In", "sel'\u00b7ger", "Geis\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erstaunt ob dem Gesange,", "tokens": ["Er\u00b7staunt", "ob", "dem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Horchet dem Fremdlingsklange,", "tokens": ["Hor\u00b7chet", "dem", "Fremd\u00b7lings\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vergessend Leid und Schmach,", "tokens": ["Ver\u00b7ges\u00b7send", "Leid", "und", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nun frei der Mensch von Schmerzen,", "tokens": ["Nun", "frei", "der", "Mensch", "von", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zieht in tiefem Herzen", "tokens": ["Und", "zieht", "in", "tie\u00b7fem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem mag'schen Strome nach.", "tokens": ["Dem", "mag'\u00b7schen", "Stro\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch bald ist der verklungen,", "tokens": ["Doch", "bald", "ist", "der", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie brausend er geschwungen,", "tokens": ["Wie", "brau\u00b7send", "er", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder stumm das Grab.", "tokens": ["Und", "wie\u00b7der", "stumm", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es flammt das Lied vergebens,", "tokens": ["Es", "flammt", "das", "Lied", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der w\u00fcste Sturm des Lebens", "tokens": ["Der", "w\u00fcs\u00b7te", "Sturm", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Rei\u00dft es in \u00d6d' herab.", "tokens": ["Rei\u00dft", "es", "in", "\u00d6d'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Das sind die alten Kl\u00e4nge,", "tokens": ["Das", "sind", "die", "al\u00b7ten", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Helden- und Klagges\u00e4nge", "tokens": ["Hel\u00b7den", "und", "Klag\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Aus ferner Riesenzeit.", "tokens": ["Aus", "fer\u00b7ner", "Rie\u00b7sen\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Liede mu\u00df gelingen,", "tokens": ["Dem", "Lie\u00b7de", "mu\u00df", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie wieder uns zu bringen,", "tokens": ["Sie", "wie\u00b7der", "uns", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Retter ist nicht weit.", "tokens": ["Der", "Ret\u00b7ter", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Fr\u00fchling wird erstehen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wird", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es mu\u00df noch einst geschehen,", "tokens": ["Es", "mu\u00df", "noch", "einst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was alle prophezeit.", "tokens": ["Was", "al\u00b7le", "pro\u00b7phe\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}