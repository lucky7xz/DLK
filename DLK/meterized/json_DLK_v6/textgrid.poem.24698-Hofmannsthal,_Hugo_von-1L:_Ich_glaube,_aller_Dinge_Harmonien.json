{"textgrid.poem.24698": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich glaube, aller Dinge Harmonien", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube, aller Dinge Harmonien", "tokens": ["Ich", "glau\u00b7be", ",", "al\u00b7ler", "Din\u00b7ge", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was von Sch\u00f6nheit auf dem Leben ruht,", "tokens": ["Und", "was", "von", "Sch\u00f6n\u00b7heit", "auf", "dem", "Le\u00b7ben", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der Dichter ausgegoss'nes Blut", "tokens": ["Das", "ist", "der", "Dich\u00b7ter", "aus\u00b7ge\u00b7goss'\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6nheit, die ihr Sinn der Welt geliehen:", "tokens": ["Und", "Sch\u00f6n\u00b7heit", ",", "die", "ihr", "Sinn", "der", "Welt", "ge\u00b7lie\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da schufen die aus ihres Innern Glut", "tokens": ["Da", "schu\u00b7fen", "die", "aus", "ih\u00b7res", "In\u00b7nern", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Ringens und des Lebens Poesien, \u2013", "tokens": ["Des", "Rin\u00b7gens", "und", "des", "Le\u00b7bens", "Poe\u00b7si\u00b7en", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jene stillen Leidens leises Ziehen", "tokens": ["Und", "je\u00b7ne", "stil\u00b7len", "Lei\u00b7dens", "lei\u00b7ses", "Zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verkl\u00e4rend, was beklemmend auf uns ruht.", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", ",", "was", "be\u00b7klem\u00b7mend", "auf", "uns", "ruht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wo des Abends zitternd zarte T\u00f6ne", "tokens": ["Doch", "wo", "des", "A\u00b7bends", "zit\u00b7ternd", "zar\u00b7te", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unnennbar schmerzlich singen vom Entsagen,", "tokens": ["Un\u00b7nenn\u00b7bar", "schmerz\u00b7lich", "sin\u00b7gen", "vom", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo die Dinge, die verschwimmen, tragen", "tokens": ["Und", "wo", "die", "Din\u00b7ge", ",", "die", "ver\u00b7schwim\u00b7men", ",", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die r\u00fchrendste, die n\u00e4chstverwandte Sch\u00f6ne:", "tokens": ["Die", "r\u00fch\u00b7rends\u00b7te", ",", "die", "n\u00e4chst\u00b7ver\u00b7wand\u00b7te", "Sch\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und glaube, da\u00df du sie geschaffen. Amen.", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "du", "sie", "ge\u00b7schaf\u00b7fen", ".", "A\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}