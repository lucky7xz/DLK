{"textgrid.poem.40228": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Jesus in Gethsemane", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lautlos ruhet, starr der Dom der Palmen,", "tokens": ["Laut\u00b7los", "ru\u00b7het", ",", "starr", "der", "Dom", "der", "Pal\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schaun zum Mond die Schatten aus den Halmen,", "tokens": ["schaun", "zum", "Mond", "die", "Schat\u00b7ten", "aus", "den", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "weint die blauen Thr\u00e4nen still die Nacht.", "tokens": ["weint", "die", "blau\u00b7en", "Thr\u00e4\u00b7nen", "still", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nur von hei\u00dfen Menschenlauten schauert", "tokens": ["Nur", "von", "hei\u00b7\u00dfen", "Men\u00b7schen\u00b7lau\u00b7ten", "schau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "dumpf der stumme starre Hain \u2013 und schauert:", "tokens": ["dumpf", "der", "stum\u00b7me", "star\u00b7re", "Hain", "\u2013", "und", "schau\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "ADJA", "NN", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "einsam kniet und bebt und trauert", "tokens": ["ein\u00b7sam", "kniet", "und", "bebt", "und", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "dort ein Mann und st\u00f6hnt mit weher Macht.", "tokens": ["dort", "ein", "Mann", "und", "st\u00f6hnt", "mit", "we\u00b7her", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6re, h\u00f6re, Geist der Wahrheit,", "tokens": ["H\u00f6\u00b7re", ",", "h\u00f6\u00b7re", ",", "Geist", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Reue, meine dunkle Schuld:", "tokens": ["mei\u00b7ne", "Reu\u00b7e", ",", "mei\u00b7ne", "dunk\u00b7le", "Schuld", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "der in Kampf ich wandelte und Starrheit,", "tokens": ["der", "in", "Kampf", "ich", "wan\u00b7del\u00b7te", "und", "Star\u00b7rheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, ein Baum des Lichtes wollt' ich leben", "tokens": ["Ach", ",", "ein", "Baum", "des", "Lich\u00b7tes", "wollt'", "ich", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00fcberm\u00e4chtig der Natur:", "tokens": ["\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ach, sie sahn nicht Jesu ", "tokens": ["ach", ",", "sie", "sahn", "nicht", "Je\u00b7su"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["XY", "$,", "PPER", "VVFIN", "PTKNEG", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "sahn die ", "tokens": ["sahn", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Uebermenschlich hab' ich mich ", "tokens": ["Ue\u00b7ber\u00b7menschlich", "hab'", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Einer, Er nur wu\u00dfte \u2013 \u2013 Judas! Freund!", "tokens": ["Ei\u00b7ner", ",", "Er", "nur", "wu\u00df\u00b7te", "\u2013", "\u2013", "Ju\u00b7das", "!", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PPER", "ADV", "VVFIN", "$(", "$(", "NE", "$.", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "warum \u2013 hast du \u2013 mich \u2013 verraten?!", "tokens": ["wa\u00b7rum", "\u2013", "hast", "du", "\u2013", "mich", "\u2013", "ver\u00b7ra\u00b7ten", "?!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$(", "VAFIN", "PPER", "$(", "PPER", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "k\u00f6nnt' ich ", "tokens": ["k\u00f6nnt'", "ich"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Menschen menschlich ", "tokens": ["Men\u00b7schen", "menschlich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "meiner ", "tokens": ["mei\u00b7ner"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Und zum Mond die Arme wild gebreitet", "tokens": ["Und", "zum", "Mond", "die", "Ar\u00b7me", "wild", "ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und das Auge qualentief geweitet,", "tokens": ["und", "das", "Au\u00b7ge", "qua\u00b7len\u00b7tief", "ge\u00b7wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "seine hei\u00dfen Blicke schmachtend irr'n.", "tokens": ["sei\u00b7ne", "hei\u00b7\u00dfen", "Bli\u00b7cke", "schmach\u00b7tend", "irr'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und herab die bleiche Bahn der Strahlen", "tokens": ["Und", "her\u00b7ab", "die", "blei\u00b7che", "Bahn", "der", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "sieht er ", "tokens": ["sieht", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "durch die Schatten ihm die fahlen", "tokens": ["durch", "die", "Schat\u00b7ten", "ihm", "die", "fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dolche bohrend in die gl\u00fchende Stirn.", "tokens": ["Dol\u00b7che", "boh\u00b7rend", "in", "die", "gl\u00fc\u00b7hen\u00b7de", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Wehe, wehe, Geist der Liebe:", "tokens": ["We\u00b7he", ",", "we\u00b7he", ",", "Geist", "der", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ganz in Reinheit thronst du klar und hoch, \u2013", "tokens": ["ganz", "in", "Rein\u00b7heit", "thronst", "du", "klar", "und", "hoch", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und ich ", "tokens": ["und", "ich"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Schwerter stie\u00df ich in die weichsten Herzen:", "tokens": ["Schwer\u00b7ter", "stie\u00df", "ich", "in", "die", "weichs\u00b7ten", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "doch die Mutter ringet drob in Schmerzen", "tokens": ["doch", "die", "Mut\u00b7ter", "rin\u00b7get", "drob", "in", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "und mit sehnsuchtwundem Herzen", "tokens": ["und", "mit", "sehn\u00b7sucht\u00b7wun\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "weint um mich die Magdalenerin!", "tokens": ["weint", "um", "mich", "die", "Mag\u00b7da\u00b7le\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Nackt und blo\u00df, und nur ein Menschensohn,", "tokens": ["Nackt", "und", "blo\u00df", ",", "und", "nur", "ein", "Men\u00b7schen\u00b7sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wollt' ich sammeln all mein arm Geschlecht;", "tokens": ["wollt'", "ich", "sam\u00b7meln", "all", "mein", "arm", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PIAT", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "doch: im Mitleid schl\u00e4ft die ", "tokens": ["doch", ":", "im", "Mit\u00b7leid", "schl\u00e4ft", "die"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPRART", "NN", "VVFIN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "weh: ", "tokens": ["weh", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Judas, Judas, kommst du mich zu ", "tokens": ["Ju\u00b7das", ",", "Ju\u00b7das", ",", "kommst", "du", "mich", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "PPER", "PRF", "APPR"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "hei\u00dft ", "tokens": ["hei\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Mu\u00df denn diese Welt sich erst vernichten,", "tokens": ["Mu\u00df", "denn", "die\u00b7se", "Welt", "sich", "erst", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDAT", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "der Erl\u00f6sung Reich emporzurichten?", "tokens": ["der", "Er\u00b7l\u00f6\u00b7sung", "Reich", "em\u00b7por\u00b7zu\u00b7rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Freiheit, lebst du im Gewissen blo\u00df?!", "tokens": ["Frei\u00b7heit", ",", "lebst", "du", "im", "Ge\u00b7wis\u00b7sen", "blo\u00df", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und aufs Antlitz zagend hingezwungen,", "tokens": ["Und", "aufs", "Ant\u00b7litz", "za\u00b7gend", "hin\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "f\u00fchlt er heftiger die Anfechtungen,", "tokens": ["f\u00fchlt", "er", "hef\u00b7ti\u00b7ger", "die", "An\u00b7fech\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und von Schwei\u00df die Dulderstirne trieft.", "tokens": ["und", "von", "Schwei\u00df", "die", "Dul\u00b7der\u00b7stir\u00b7ne", "trieft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und ins Gras wie Blut die gro\u00dfen Tropfen", "tokens": ["Und", "ins", "Gras", "wie", "Blut", "die", "gro\u00b7\u00dfen", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KOKOM", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "f\u00fchlt er brennend von den Schl\u00e4fen tropfen,", "tokens": ["f\u00fchlt", "er", "bren\u00b7nend", "von", "den", "Schl\u00e4\u00b7fen", "trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "an den Leib der Erde klopfen", "tokens": ["an", "den", "Leib", "der", "Er\u00b7de", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "seine Seele furcht-und-angstgepr\u00fcft.", "tokens": ["sei\u00b7ne", "See\u00b7le", "furcht\u00b7un\u00b7dangst\u00b7ge\u00b7pr\u00fcft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Geist des Lebens: Klarheit, Klarheit!", "tokens": ["Geist", "des", "Le\u00b7bens", ":", "Klar\u00b7heit", ",", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wird denn Sieg um Opfer nur gew\u00e4hrt?!", "tokens": ["wird", "denn", "Sieg", "um", "Op\u00b7fer", "nur", "ge\u00b7w\u00e4hrt", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, es kommt der ", "tokens": ["Sieh", ",", "es", "kommt", "der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Selig, meiner Inbrunst mich zu t\u00f6ten,", "tokens": ["Se\u00b7lig", ",", "mei\u00b7ner", "In\u00b7brunst", "mich", "zu", "t\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "eine Lebensleuchte wollt' ich stehn, \u2013", "tokens": ["ei\u00b7ne", "Le\u00b7bens\u00b7leuch\u00b7te", "wollt'", "ich", "stehn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "aber jetzt in Todesn\u00f6ten", "tokens": ["a\u00b7ber", "jetzt", "in