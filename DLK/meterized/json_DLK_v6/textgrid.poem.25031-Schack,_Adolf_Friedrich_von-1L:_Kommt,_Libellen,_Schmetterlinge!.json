{"textgrid.poem.25031": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt, Libellen, Schmetterlinge!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Libellen, Schmetterlinge!", "tokens": ["Kommt", ",", "Li\u00b7bel\u00b7len", ",", "Schmet\u00b7ter\u00b7lin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Goldig, rot und blau von Schwinge,", "tokens": ["Gol\u00b7dig", ",", "rot", "und", "blau", "von", "Schwin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiegt euch in der Sommerluft,", "tokens": ["Wiegt", "euch", "in", "der", "Som\u00b7mer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin von Kelch zu Kelche gaukelt,", "tokens": ["Hin", "von", "Kelch", "zu", "Kel\u00b7che", "gau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Windgeschaukelt,", "tokens": ["Wind\u00b7ge\u00b7schau\u00b7kelt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Um mich her im Bl\u00fctenduft.", "tokens": ["Um", "mich", "her", "im", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seid die Seelen ihr von Stunden,", "tokens": ["Seid", "die", "See\u00b7len", "ihr", "von", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir s\u00fc\u00df dahingeschwunden?", "tokens": ["Die", "mir", "s\u00fc\u00df", "da\u00b7hin\u00b7ge\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ihr aus der Gruft euch hebt,", "tokens": ["Wie", "ihr", "aus", "der", "Gruft", "euch", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Alle kenn' ich sie, die holden,", "tokens": ["Al\u00b7le", "kenn'", "ich", "sie", ",", "die", "hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche golden", "tokens": ["Wel\u00b7che", "gol\u00b7den"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mich in sel'ger Zeit umschwebt.", "tokens": ["Mich", "in", "sel'\u00b7ger", "Zeit", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stunden, in geliebten Armen", "tokens": ["Stun\u00b7den", ",", "in", "ge\u00b7lieb\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst vertr\u00e4umt, indes von warmen", "tokens": ["Einst", "ver\u00b7tr\u00e4umt", ",", "in\u00b7des", "von", "war\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lippen mich der Hauch umquoll,", "tokens": ["Lip\u00b7pen", "mich", "der", "Hauch", "um\u00b7quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu mir wie Himmelslieder", "tokens": ["Und", "zu", "mir", "wie", "Him\u00b7mels\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sanft hernieder", "tokens": ["Sanft", "her\u00b7nie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Eine s\u00fc\u00dfe Stimme scholl.", "tokens": ["Ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", "scholl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ihr leicht, ihr fl\u00fcgelschnellen", "tokens": ["Wie", "ihr", "leicht", ",", "ihr", "fl\u00fc\u00b7gel\u00b7schnel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmetterlinge und Libellen,", "tokens": ["Schmet\u00b7ter\u00b7lin\u00b7ge", "und", "Li\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Um mich schwebt im Morgenschein,", "tokens": ["Um", "mich", "schwebt", "im", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber aus des Grabes Banden", "tokens": ["Sel\u00b7ber", "aus", "des", "Gra\u00b7bes", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon erstanden,", "tokens": ["Schon", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Glaub' ich, so wie ihr, zu sein.", "tokens": ["Glaub'", "ich", ",", "so", "wie", "ihr", ",", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}