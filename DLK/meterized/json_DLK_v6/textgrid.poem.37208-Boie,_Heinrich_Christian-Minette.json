{"textgrid.poem.37208": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Minette", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Minettens helle Blicke fodern", "tokens": ["Mi\u00b7net\u00b7tens", "hel\u00b7le", "Bli\u00b7cke", "fo\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebietrisch auf zur Huldigung,", "tokens": ["Ge\u00b7bie\u00b7trisch", "auf", "zur", "Hul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sie reizet, lockt, hat nie genug,", "tokens": ["Sie", "rei\u00b7zet", ",", "lockt", ",", "hat", "nie", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als bis ihr aller Herzen lodern.", "tokens": ["Als", "bis", "ihr", "al\u00b7ler", "Her\u00b7zen", "lo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Witz, der wie das Auge gl\u00fcht,", "tokens": ["Ihr", "Witz", ",", "der", "wie", "das", "Au\u00b7ge", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Blitze schie\u00dft, nur Funken spr\u00fcht,", "tokens": ["Nur", "Blit\u00b7ze", "schie\u00dft", ",", "nur", "Fun\u00b7ken", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erwartet Lob von jeder Zunge.", "tokens": ["Er\u00b7war\u00b7tet", "Lob", "von", "je\u00b7der", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr Amor ist ein wilder Junge", "tokens": ["Ihr", "A\u00b7mor", "ist", "ein", "wil\u00b7der", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der mit der Fackel, die er tr\u00e4gt,", "tokens": ["Der", "mit", "der", "Fa\u00b7ckel", ",", "die", "er", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mutwillig ins Gesicht euch schl\u00e4gt.", "tokens": ["Mut\u00b7wil\u00b7lig", "ins", "Ge\u00b7sicht", "euch", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}