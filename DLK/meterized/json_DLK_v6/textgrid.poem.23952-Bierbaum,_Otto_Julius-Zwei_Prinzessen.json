{"textgrid.poem.23952": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Prinzessen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Prinzessen f\u00e4hrt zum Hochzeitsfest", "tokens": ["Die", "Prin\u00b7zes\u00b7sen", "f\u00e4hrt", "zum", "Hoch\u00b7zeits\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vier Schimmel am Wagen,", "tokens": ["Vier", "Schim\u00b7mel", "am", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit rotem Kragen", "tokens": ["Mit", "ro\u00b7tem", "Kra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Kutscher und silberbetre\u00dft.", "tokens": ["Die", "Kut\u00b7scher", "und", "sil\u00b7ber\u00b7be\u00b7tre\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Trara!", "tokens": ["Tra\u00b7ra", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Hell schmettern Trompeten und Trompetinen.", "tokens": ["Hell", "schmet\u00b7tern", "Trom\u00b7pe\u00b7ten", "und", "Trom\u00b7pe\u00b7ti\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Prinze\u00dflein sitzt da mit s\u00fc\u00dfen Mienen", "tokens": ["Prin\u00b7ze\u00df\u00b7lein", "sitzt", "da", "mit", "s\u00fc\u00b7\u00dfen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "In Galatoilette und Gloria.", "tokens": ["In", "Ga\u00b7la\u00b7to\u00b7i\u00b7let\u00b7te", "und", "Glo\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Menge verneigt sich und hebt den Hut;", "tokens": ["Die", "Men\u00b7ge", "ver\u00b7neigt", "sich", "und", "hebt", "den", "Hut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie prunkt die Karosse!", "tokens": ["Wie", "prunkt", "die", "Ka\u00b7ros\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir stehn in der Gosse ...", "tokens": ["Wir", "stehn", "in", "der", "Gos\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u00bbach Gott, so eine hats gut ...\u00ab", "tokens": ["\u00bb", "ach", "Gott", ",", "so", "ei\u00b7ne", "hats", "gut", "...", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADV", "ART", "VAFIN", "ADJD", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Trara!", "tokens": ["Tra\u00b7ra", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Hell schmettern Trompeten und Trompetinen.", "tokens": ["Hell", "schmet\u00b7tern", "Trom\u00b7pe\u00b7ten", "und", "Trom\u00b7pe\u00b7ti\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Eine Kleine sagts mit sauren Mienen", "tokens": ["Ei\u00b7ne", "Klei\u00b7ne", "sagts", "mit", "sau\u00b7ren", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und gl\u00e4nzt doch in Sch\u00f6nheit und Gloria.", "tokens": ["Und", "gl\u00e4nzt", "doch", "in", "Sch\u00f6n\u00b7heit", "und", "Glo\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Die Prinzessin hab ich nicht mehr gesehn,", "tokens": ["Die", "Prin\u00b7zes\u00b7sin", "hab", "ich", "nicht", "mehr", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah nur die feine,", "tokens": ["Ich", "sah", "nur", "die", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die liebe Kleine", "tokens": ["Die", "lie\u00b7be", "Klei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im wollenen R\u00f6ckchen stehn.", 