{"textgrid.poem.30723": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel dienen dem Altar;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel dienen dem Altar;", "tokens": ["Viel", "die\u00b7nen", "dem", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich la\u00df es bleiben wahr;", "tokens": ["Ich", "la\u00df", "es", "blei\u00b7ben", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch d\u00fcnckt mich gleichwol auch,", "tokens": ["Doch", "d\u00fcnckt", "mich", "gleich\u00b7wol", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Altar sey manchmal Bauch.", "tokens": ["Al\u00b7tar", "sey", "manch\u00b7mal", "Bauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}