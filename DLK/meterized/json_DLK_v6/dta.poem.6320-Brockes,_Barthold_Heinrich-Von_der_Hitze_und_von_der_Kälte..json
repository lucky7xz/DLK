{"dta.poem.6320": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Handlung nun von beyden Seiten", "tokens": ["Die", "Hand\u00b7lung", "nun", "von", "bey\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist n\u00f6thig: denn des Feuers Theile", "tokens": ["Ist", "n\u00f6\u00b7thig", ":", "denn", "des", "Feu\u00b7ers", "Thei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bestreben sich in steter Eile", "tokens": ["Be\u00b7stre\u00b7ben", "sich", "in", "ste\u00b7ter", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich zu entfernen, wegzufliegen,", "tokens": ["Sich", "zu", "ent\u00b7fer\u00b7nen", ",", "weg\u00b7zu\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Daher, indem sie sich im steten Creise biegen,", "tokens": ["Da\u00b7her", ",", "in\u00b7dem", "sie", "sich", "im", "ste\u00b7ten", "Crei\u00b7se", "bie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie sich zu uns im Circkel wieder f\u00fcgen.", "tokens": ["Sie", "sich", "zu", "uns", "im", "Cir\u00b7ckel", "wie\u00b7der", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPER", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}