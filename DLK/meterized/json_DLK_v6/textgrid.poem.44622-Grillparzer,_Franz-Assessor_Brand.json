{"textgrid.poem.44622": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Assessor Brand", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allzu eifrigem Studieren", "tokens": ["Von", "all\u00b7zu", "eif\u00b7ri\u00b7gem", "Stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PTKA", "ADJA", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ruft h\u00f6hnisch l\u00e4chelnd der Assessor Brand,", "tokens": ["Ruft", "h\u00f6h\u00b7nisch", "l\u00e4\u00b7chelnd", "der", "As\u00b7ses\u00b7sor", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verlor der alte Hofrat den Verstand!", "tokens": ["Ver\u00b7lor", "der", "al\u00b7te", "Hof\u00b7rat", "den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie kann das einem klugen Mann passieren?", "tokens": ["Wie", "kann", "das", "ei\u00b7nem", "klu\u00b7gen", "Mann", "pas\u00b7sie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sitze zwanzig Jahre bei Gericht", "tokens": ["Ich", "sit\u00b7ze", "zwan\u00b7zig", "Jah\u00b7re", "bei", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mir geschah nie so etwas! \u2013 Ich zweifle nicht,", "tokens": ["Und", "mir", "ge\u00b7schah", "nie", "so", "et\u00b7was", "!", "\u2013", "Ich", "zweif\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$.", "$(", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was man nie hatte, kann man nicht verlieren!", "tokens": ["Was", "man", "nie", "hat\u00b7te", ",", "kann", "man", "nicht", "ver\u00b7lie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VAFIN", "$,", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}