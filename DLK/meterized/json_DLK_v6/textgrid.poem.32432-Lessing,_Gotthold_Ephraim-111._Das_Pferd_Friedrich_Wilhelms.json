{"textgrid.poem.32432": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "111. Das Pferd Friedrich Wilhelms", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr bleibet vor Verwundrung stehn,", "tokens": ["Ihr", "blei\u00b7bet", "vor", "Ver\u00b7wund\u00b7rung", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zweifelt doch an meinem Leben?", "tokens": ["Und", "zwei\u00b7felt", "doch", "an", "mei\u00b7nem", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft meinen Reiter mir die Ferse geben:", "tokens": ["La\u00dft", "mei\u00b7nen", "Rei\u00b7ter", "mir", "die", "Fer\u00b7se", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sollt ihr sehn!", "tokens": ["So", "sollt", "ihr", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}