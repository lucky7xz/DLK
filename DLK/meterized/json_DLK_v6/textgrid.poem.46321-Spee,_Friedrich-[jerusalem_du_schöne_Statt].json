{"textgrid.poem.46321": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[jerusalem du sch\u00f6ne Statt]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jerusalem du sch\u00f6ne Statt,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "du", "sch\u00f6\u00b7ne", "Statt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann ich zu dir gedencke;", "tokens": ["Wann", "ich", "zu", "dir", "ge\u00b7den\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur stund in tieffen z\u00e4hren bad", "tokens": ["Zur", "stund", "in", "tief\u00b7fen", "z\u00e4h\u00b7ren", "bad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beid augen ich versencke:", "tokens": ["Beid", "au\u00b7gen", "ich", "ver\u00b7sen\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Sonnen-liecht: mehr scheine nicht,", "tokens": ["Ach", "Son\u00b7nen\u00b7liecht", ":", "mehr", "schei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00f6sch ab die fewrig strolen,", "tokens": ["L\u00f6sch", "ab", "die", "few\u00b7rig", "stro\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "All glantz vnd schein: all flammen dein", "tokens": ["All", "glantz", "vnd", "schein", ":", "all", "flam\u00b7men", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PTKVZ", "$.", "PIAT", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Acht ich wie schwartze kolen.", "tokens": ["Acht", "ich", "wie", "schwart\u00b7ze", "ko\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PPER", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein sch\u00f6nes liecht ist Gott allein,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "liecht", "ist", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leucht im Himmel droben:", "tokens": ["So", "leucht", "im", "Him\u00b7mel", "dro\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sonn, Mon, vnd Stern, vnd hiesig schein,", "tokens": ["Sonn", ",", "Mon", ",", "vnd", "Stern", ",", "vnd", "hie\u00b7sig", "schein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "NN", "$,", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halt ich f\u00fcr schlechte gaben.", "tokens": ["Halt", "ich", "f\u00fcr", "schlech\u00b7te", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seynd gegen Gott: nur Kinderspott;", "tokens": ["Seynd", "ge\u00b7gen", "Gott", ":", "nur", "Kin\u00b7der\u00b7spott", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$.", "ADV", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nie m\u00f6gens ihn erreichen:", "tokens": ["Nie", "m\u00f6\u00b7gens", "ihn", "er\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur dunckel gantz: ist all ihr glantz", "tokens": ["Nur", "dun\u00b7ckel", "gantz", ":", "ist", "all", "ihr", "glantz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "$.", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd endlich m\u00fcssens weichen.", "tokens": ["Vnd", "end\u00b7lich", "m\u00fcs\u00b7sens", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch frewd vnd wollust diser welt,", "tokens": ["Auch", "frewd", "vnd", "wol\u00b7lust", "di\u00b7ser", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "VMFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der pomp vnd pracht imgleichen,", "tokens": ["Der", "pomp", "vnd", "pracht", "im\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir gegen Gott so gar mi\u00dffelt,", "tokens": ["Mir", "ge\u00b7gen", "Gott", "so", "gar", "mi\u00df\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit koth ichs thu vergleichen.", "tokens": ["Mit", "koth", "ichs", "thu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja weltlich schertz: ist mir ein schmertz;", "tokens": ["Ja", "welt\u00b7lich", "schertz", ":", "ist", "mir", "ein", "schmertz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADJD", "$.", "VAFIN", "PPER", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr lauter qual ichs achte:", "tokens": ["F\u00fcr", "lau\u00b7ter", "qual", "ichs", "ach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frewd dunckt mir sein: fast lauter pein,", "tokens": ["Frewd", "dunckt", "mir", "sein", ":", "fast", "lau\u00b7ter", "pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VAINF", "$.", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wans recht bey mir betrachte.", "tokens": ["Wans", "recht", "bey", "mir", "be\u00b7trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ad\u00e8, Ad\u00e8, zu guter nacht", "tokens": ["Ad\u00e8", ",", "Ad\u00e8", ",", "zu", "gu\u00b7ter", "nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sonn, Mon, vnd Himmels-sterne;", "tokens": ["Sonn", ",", "Mon", ",", "vnd", "Him\u00b7mels\u00b7ster\u00b7ne", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ade, ade, verg\u00e4nglich pracht,", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "ver\u00b7g\u00e4ng\u00b7lich", "pracht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Euch la\u00df ich also gerne:", "tokens": ["Euch", "la\u00df", "ich", "al\u00b7so", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nichts mag doch sein: als Gott allein,", "tokens": ["Nichts", "mag", "doch", "sein", ":", "als", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VAINF", "$.", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darauff wir sicher leinen:", "tokens": ["Dar\u00b7auff", "wir", "si\u00b7cher", "lei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "All ander ding: seind mir zu ring;", "tokens": ["All", "an\u00b7der", "ding", ":", "seind", "mir", "zu", "ring", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "VAFIN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Gott, sollt ich nit weinen!", "tokens": ["O", "Gott", ",", "sollt", "ich", "nit", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu dir mein augen spritzen auff,", "tokens": ["Zu", "dir", "mein", "au\u00b7gen", "sprit\u00b7zen", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich kein trost mehr finde:", "tokens": ["Weil", "ich", "kein", "trost", "mehr", "fin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu dir mein seufftzer gehn zu hauff,", "tokens": ["Zu", "dir", "mein", "seufft\u00b7zer", "gehn", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVINF", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die st\u00e4ts ich bla\u00df in winde:", "tokens": ["Die", "st\u00e4ts", "ich", "bla\u00df", "in", "win\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach wan wird sein: das ich erschein,", "tokens": ["Ach", "wan", "wird", "sein", ":", "das", "ich", "er\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "VAINF", "$.", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deinen l\u00fcsten droben!", "tokens": ["In", "dei\u00b7nen", "l\u00fcs\u00b7ten", "dro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach wan werd ich: ergreiffen dich,", "tokens": ["Ach", "wan", "werd", "ich", ":", "er\u00b7greif\u00b7fen", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu deinem Thron erhoben!", "tokens": ["Zu", "dei\u00b7nem", "Thron", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der brullend Hirsch nie schwinder sprang,", "tokens": ["Der", "brul\u00b7lend", "Hirsch", "nie", "schwin\u00b7der", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In durst vnd hei\u00dfer Sonnen,", "tokens": ["In", "durst", "vnd", "hei\u00b7\u00dfer", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wan er vernam den wasser-klang", "tokens": ["Wan", "er", "ver\u00b7nam", "den", "was\u00b7ser\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vom fall der k\u00fchlen Brunnen:", "tokens": ["Vom", "fall", "der", "k\u00fch\u00b7len", "Brun\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als ich nach dir: lauff mit begier,", "tokens": ["Als", "ich", "nach", "dir", ":", "lauff", "mit", "be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$.", "APPR", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gottes Statt du sch\u00f6ne.", "tokens": ["O", "Got\u00b7tes", "Statt", "du", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur tag vnd nacht: nach dir ich tracht,", "tokens": ["Nur", "tag", "vnd", "nacht", ":", "nach", "dir", "ich", "tracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$.", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "All frewd ich sonst verh\u00f6ne.", "tokens": ["All", "frewd", "ich", "sonst", "ver\u00b7h\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Jerusalem, du sch\u00f6ne Statt,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", ",", "du", "sch\u00f6\u00b7ne", "Statt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wan ich zu dir gedencke,", "tokens": ["Wan", "ich", "zu", "dir", "ge\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr lauter lieb ich fast ermatt,", "tokens": ["F\u00fcr", "lau\u00b7ter", "lieb", "ich", "fast", "er\u00b7matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr lust ich schier erkr\u00e4ncke.", "tokens": ["F\u00fcr", "lust", "ich", "schier", "er\u00b7kr\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Hertz sich bald: in st\u00fcck zerspalt:", "tokens": ["Mein", "Hertz", "sich", "bald", ":", "in", "st\u00fcck", "zer\u00b7spalt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "$.", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach wee! wie lang sols werden?", "tokens": ["Ach", "wee", "!", "wie", "lang", "sols", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PWAV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie lang soll doch: ich bleiben noch,", "tokens": ["Wie", "lang", "soll", "doch", ":", "ich", "blei\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So weit von dir auff erden.", "tokens": ["So", "weit", "von", "dir", "auff", "er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}