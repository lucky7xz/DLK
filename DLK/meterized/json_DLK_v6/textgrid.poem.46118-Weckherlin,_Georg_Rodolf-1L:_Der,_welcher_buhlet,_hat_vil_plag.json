{"textgrid.poem.46118": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der, welcher buhlet, hat vil plag", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der, welcher buhlet, hat vil plag", "tokens": ["Der", ",", "wel\u00b7cher", "buh\u00b7let", ",", "hat", "vil", "plag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und der nicht buhlet, hat all tag", "tokens": ["und", "der", "nicht", "buh\u00b7let", ",", "hat", "all", "tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "PTKNEG", "VVFIN", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auch m\u00fch gnug, sein herz zu verdrie\u00dfen;", "tokens": ["auch", "m\u00fch", "gnug", ",", "sein", "herz", "zu", "ver\u00b7drie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "der aber hat mehr pein und reu,", "tokens": ["der", "a\u00b7ber", "hat", "mehr", "pein", "und", "reu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PIAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der nach bewehrter lieb und treu", "tokens": ["der", "nach", "be\u00b7wehr\u00b7ter", "lieb", "und", "treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "kan seiner diensten nicht genie\u00dfen.", "tokens": ["kan", "sei\u00b7ner", "diens\u00b7ten", "nicht", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lehr, adel, tugend, k\u00fchnheit, zucht", "tokens": ["Lehr", ",", "a\u00b7del", ",", "tu\u00b7gend", ",", "k\u00fchn\u00b7heit", ",", "zucht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "seind zu der lieb nun gar ohn frucht,", "tokens": ["seind", "zu", "der", "lieb", "nun", "gar", "ohn", "frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJD", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "den k\u00fcnsten die leut nichts nachfragen;", "tokens": ["den", "k\u00fcns\u00b7ten", "die", "leut", "nichts", "nach\u00b7fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und die jungfrauen diser zeit", "tokens": ["und", "die", "jung\u00b7frau\u00b7en", "di\u00b7ser", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(schier alle feil) erheben weit", "tokens": ["(", "schier", "al\u00b7le", "feil", ")", "er\u00b7he\u00b7ben", "weit"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "PIAT", "NN", "$(", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die, so am mehrsten geld zutragen.", "tokens": ["die", ",", "so", "am", "mehrs\u00b7ten", "geld", "zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O da\u00df der j\u00e4merlich verderb", "tokens": ["O", "da\u00df", "der", "j\u00e4\u00b7mer\u00b7lich", "ver\u00b7derb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und greulich sterb und wider sterb,", "tokens": ["und", "greu\u00b7lich", "sterb", "und", "wi\u00b7der", "sterb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der das gold erstlich hat erfunden!", "tokens": ["der", "das", "gold", "erst\u00b7lich", "hat", "er\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "dardurch verblindet die blutsfreind,", "tokens": ["dar\u00b7durch", "ver\u00b7blin\u00b7det", "die", "bluts\u00b7freind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mehr, dan nat\u00fcrliche todsfeind,", "tokens": ["mehr", ",", "dan", "na\u00b7t\u00fcr\u00b7li\u00b7che", "tods\u00b7feind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sich hassen, von geiz \u00fcberwunden.", "tokens": ["sich", "has\u00b7sen", ",", "von", "geiz", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Daher entspringet alle not,", "tokens": ["Da\u00b7her", "ent\u00b7sprin\u00b7get", "al\u00b7le", "not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verdru\u00df, neid, zwietracht, krieg und tod,", "tokens": ["ver\u00b7dru\u00df", ",", "neid", ",", "zwiet\u00b7racht", ",", "krieg", "und", "tod", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "angst, trauren, sorgen und mistrauen.", "tokens": ["angst", ",", "trau\u00b7ren", ",", "sor\u00b7gen", "und", "mis\u00b7trau\u00b7en", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVINF", "$,", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "darum, ihr j\u00fcngling, seid doch weis,", "tokens": ["da\u00b7rum", ",", "ihr", "j\u00fcng\u00b7ling", ",", "seid", "doch", "weis", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "zu h\u00fcten euch mit allem flei\u00df", "tokens": ["zu", "h\u00fc\u00b7ten", "euch", "mit", "al\u00b7lem", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "f\u00fcr allen geizigen jungfrauen.", "tokens": ["f\u00fcr", "al\u00b7len", "gei\u00b7zi\u00b7gen", "jung\u00b7frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}