{"textgrid.poem.44092": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erhole dich, bedr\u00e4ngtes Herze,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhole dich, bedr\u00e4ngtes Herze,", "tokens": ["Er\u00b7ho\u00b7le", "dich", ",", "be\u00b7dr\u00e4ng\u00b7tes", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schoo\u00dfe der Gela\u00dfenheit;", "tokens": ["Im", "Schoo\u00b7\u00dfe", "der", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verzagen hilft uns nicht vom Schmerze,", "tokens": ["Ver\u00b7za\u00b7gen", "hilft", "uns", "nicht", "vom", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unschuld Gram erg\u00f6zt den Neid", "tokens": ["Der", "Un\u00b7schuld", "Gram", "er\u00b7g\u00f6zt", "den", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und macht durch ihren Thr\u00e4nenbach", "tokens": ["Und", "macht", "durch", "ih\u00b7ren", "Thr\u00e4\u00b7nen\u00b7bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kraft zum \u00dcberwinden schwach.", "tokens": ["Die", "Kraft", "zum", "\u00dc\u00b7berw\u00b7in\u00b7den", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Elend ist ja auf der Erden", "tokens": ["Das", "E\u00b7lend", "ist", "ja", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So allgemein als Luft und Fall.", "tokens": ["So", "all\u00b7ge\u00b7mein", "als", "Luft", "und", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald wir kaum zu Menschen werden,", "tokens": ["So\u00b7bald", "wir", "kaum", "zu", "Men\u00b7schen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So prophezeit der Jammerschall", "tokens": ["So", "pro\u00b7phe\u00b7zeit", "der", "Jam\u00b7mer\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch in der Wiege schon die Noth,", "tokens": ["Auch", "in", "der", "Wie\u00b7ge", "schon", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die unserm ganzen Leben droht.", "tokens": ["Die", "un\u00b7serm", "gan\u00b7zen", "Le\u00b7ben", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man wandelt zwischen Furcht und Sp\u00f6ttern,", "tokens": ["Man", "wan\u00b7delt", "zwi\u00b7schen", "Furcht", "und", "Sp\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man wird verschnidten und gedr\u00fcckt", "tokens": ["Man", "wird", "ver\u00b7schnid\u00b7ten", "und", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVINF", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bey den gr\u00f6sten Ungl\u00fcckswettern", "tokens": ["Und", "bey", "den", "gr\u00f6s\u00b7ten", "Un\u00b7gl\u00fccks\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch gar von Freunden umger\u00fcckt,", "tokens": ["Auch", "gar", "von", "Freun\u00b7den", "um\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Freunden, die des Gl\u00fcckes List", "tokens": ["Von", "Freun\u00b7den", ",", "die", "des", "Gl\u00fc\u00b7ckes", "List"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit angenehmen Blicken k\u00fcst.", "tokens": ["Mit", "an\u00b7ge\u00b7neh\u00b7men", "Bli\u00b7cken", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Geist ist willig, recht zu leben,", "tokens": ["Der", "Geist", "ist", "wil\u00b7lig", ",", "recht", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4re gern der M\u00e4ngel los;", "tokens": ["Und", "w\u00e4\u00b7re", "gern", "der", "M\u00e4n\u00b7gel", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein des Fleisches Widerstreben", "tokens": ["Al\u00b7lein", "des", "Flei\u00b7sches", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist allemahl so starck und gro\u00df,", "tokens": ["Ist", "al\u00b7le\u00b7mahl", "so", "starck", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df auch der allerbeste Christ", "tokens": ["Da\u00df", "auch", "der", "al\u00b7ler\u00b7bes\u00b7te", "Christ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum \u00f6ftern Rath und Trost vermi\u00dft.", "tokens": ["Zum", "\u00f6f\u00b7tern", "Rath", "und", "Trost", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was aber sollt ich weichlich klagen?", "tokens": ["Was", "a\u00b7ber", "sollt", "ich", "weich\u00b7lich", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich weis, da\u00df mein Erl\u00f6ser lebt;", "tokens": ["Ich", "weis", ",", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lebt, er zehlet meine Plagen,", "tokens": ["Er", "lebt", ",", "er", "zeh\u00b7let", "mei\u00b7ne", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worin mich viel Gefahr begr\u00e4bt,", "tokens": ["Wo\u00b7rin", "mich", "viel", "Ge\u00b7fahr", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er lebt, er siegt und theilt den Raub", "tokens": ["Er", "lebt", ",", "er", "siegt", "und", "theilt", "den", "Raub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tritt, was mich verfolgt, in Staub.", "tokens": ["Und", "tritt", ",", "was", "mich", "ver\u00b7folgt", ",", "in", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach k\u00f6nt es doch ein jeder fa\u00dfen,", "tokens": ["Ach", "k\u00f6nt", "es", "doch", "ein", "je\u00b7der", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gl\u00fccklich so ein Leiden heist,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "so", "ein", "Lei\u00b7den", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey dem wir uns auf den verla\u00dfen,", "tokens": ["Bey", "dem", "wir", "uns", "auf", "den", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPR", "ART", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der endlich unser Joch zerschmeist", "tokens": ["Der", "end\u00b7lich", "un\u00b7ser", "Joch", "zer\u00b7schmeist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und de\u00dfen v\u00e4terliche Zucht", "tokens": ["Und", "de\u00b7\u00dfen", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wohlfahrt unsrer Seele sucht.", "tokens": ["Die", "Wohl\u00b7fahrt", "uns\u00b7rer", "See\u00b7le", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Indem ich die Versichrung habe", "tokens": ["In\u00b7dem", "ich", "die", "Ver\u00b7sich\u00b7rung", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und v\u00f6llig \u00fcberzeuget bin,", "tokens": ["Und", "v\u00f6l\u00b7lig", "\u00fc\u00b7berz\u00b7eu\u00b7get", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seh ich t\u00e4glich nach dem Grabe", "tokens": ["So", "seh", "ich", "t\u00e4g\u00b7lich", "nach", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lust und Sehnsuchtsblicken hin", "tokens": ["Mit", "Lust", "und", "Sehn\u00b7suchts\u00b7bli\u00b7cken", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00fcntsche bald mit hei\u00dfem Flehn,", "tokens": ["Und", "w\u00fcnt\u00b7sche", "bald", "mit", "hei\u00b7\u00dfem", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch de\u00dfen Nacht das Licht zu sehn,", "tokens": ["Durch", "de\u00b7\u00dfen", "Nacht", "das", "Licht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Das Licht, in dem der Vater wohnet,", "tokens": ["Das", "Licht", ",", "in", "dem", "der", "Va\u00b7ter", "woh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort, wo Verdru\u00df und Angst und Weh", "tokens": ["Dort", ",", "wo", "Ver\u00b7dru\u00df", "und", "Angst", "und", "Weh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Auserwehlten ewig schonet,", "tokens": ["Der", "Au\u00b7ser\u00b7wehl\u00b7ten", "e\u00b7wig", "scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey denen ich in Hofnung steh.", "tokens": ["Bey", "de\u00b7nen", "ich", "in", "Hof\u00b7nung", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Glaube k\u00e4mpft, Gott spricht ein Wort,", "tokens": ["Mein", "Glau\u00b7be", "k\u00e4mpft", ",", "Gott", "spricht", "ein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Halt an, mein Geist, dein Sieg ist dort.", "tokens": ["Halt", "an", ",", "mein", "Geist", ",", "dein", "Sieg", "ist", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Fallt hin, ihr abgekr\u00e4nckten Glieder,", "tokens": ["Fallt", "hin", ",", "ihr", "ab\u00b7ge\u00b7kr\u00e4nck\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mengt euch unter Asch und Sand;", "tokens": ["Und", "mengt", "euch", "un\u00b7ter", "Asch", "und", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr werdet mir gewislich wieder", "tokens": ["Ihr", "wer\u00b7det", "mir", "ge\u00b7wis\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit neuer Klarheit zugewand.", "tokens": ["Mit", "neu\u00b7er", "Klar\u00b7heit", "zu\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verschleus, du abgetragnes Kleid,", "tokens": ["Ver\u00b7schleus", ",", "du", "ab\u00b7ge\u00b7trag\u00b7nes", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwesung bringt Vollkommenheit.", "tokens": ["Ver\u00b7we\u00b7sung", "bringt", "Voll\u00b7kom\u00b7men\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da werd ich in dem Gottheitsspiegel", "tokens": ["Da", "werd", "ich", "in", "dem", "Got\u00b7theits\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Abgrund hoher Wei\u00dfheit schaun,", "tokens": ["Den", "Ab\u00b7grund", "ho\u00b7her", "Wei\u00df\u00b7heit", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird mir auch des Lammes Siegel", "tokens": ["Da", "wird", "mir", "auch", "des", "Lam\u00b7mes", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sch\u00f6nsten Brautschmuck anvertraun,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Brautsc\u00b7hmuck", "an\u00b7ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da werd ich wider alle stehn,", "tokens": ["Da", "werd", "ich", "wi\u00b7der", "al\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wider mich zu Schaden gehn.", "tokens": ["Die", "wi\u00b7der", "mich", "zu", "Scha\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}