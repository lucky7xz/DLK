{"dta.poem.11088": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er schicket ihr perlen.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey diesen perlen liegt zugleich mein armes hertze.", "tokens": ["Bey", "die\u00b7sen", "per\u00b7len", "liegt", "zu\u00b7gleich", "mein", "ar\u00b7mes", "hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil perlen sonsten nichts als harte thr\u00e4nen sind;", "tokens": ["Weil", "per\u00b7len", "sons\u00b7ten", "nichts", "als", "har\u00b7te", "thr\u00e4\u00b7nen", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VMFIN", "PIS", "KOKOM", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So zeuget es dadurch von seinem grossen schmertze,", "tokens": ["So", "zeu\u00b7get", "es", "da\u00b7durch", "von", "sei\u00b7nem", "gros\u00b7sen", "schmert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da bey den thr\u00e4nen sichs in der gesellschafft findt.", "tokens": ["Da", "bey", "den", "thr\u00e4\u00b7nen", "sichs", "in", "der", "ge\u00b7sell\u00b7schafft", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie gleichen dir und mir: An h\u00e4rte deinen sinnen;", "tokens": ["Sie", "glei\u00b7chen", "dir", "und", "mir", ":", "An", "h\u00e4r\u00b7te", "dei\u00b7nen", "sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "$.", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An bl\u00e4sse kommen sie fast meinen wangen bey.", "tokens": ["An", "bl\u00e4s\u00b7se", "kom\u00b7men", "sie", "fast", "mei\u00b7nen", "wan\u00b7gen", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn wo die sehnsucht kan die oberhand gewinnen,", "tokens": ["Denn", "wo", "die", "sehn\u00b7sucht", "kan", "die", "o\u00b7ber\u00b7hand", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da mu\u00df das angesicht in solche liberey.", "tokens": ["Da", "mu\u00df", "das", "an\u00b7ge\u00b7sicht", "in", "sol\u00b7che", "li\u00b7be\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch ihre bl\u00e4sse kan auch aus der furcht entspringen,", "tokens": ["Doch", "ih\u00b7re", "bl\u00e4s\u00b7se", "kan", "auch", "aus", "der", "furcht", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der eyfer hat vielleicht denselben beygebracht,", "tokens": ["Der", "ey\u00b7fer", "hat", "viel\u00b7leicht", "den\u00b7sel\u00b7ben", "bey\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ihrem strahl bey dir es werde mi\u00dfgelingen,", "tokens": ["Da\u00df", "ih\u00b7rem", "strahl", "bey", "dir", "es", "wer\u00b7de", "mi\u00df\u00b7ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil deines halses schnee ihr ei\u00df zu nichte macht.", "tokens": ["Weil", "dei\u00b7nes", "hal\u00b7ses", "schnee", "ihr", "ei\u00df", "zu", "nich\u00b7te", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch allen ungeacht, wohin mein geist sich neiget,", "tokens": ["Doch", "al\u00b7len", "un\u00b7ge\u00b7acht", ",", "wo\u00b7hin", "mein", "geist", "sich", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da m\u00fcssen sie auch hin, wo mein\u2019, ist ihre ruh,", "tokens": ["Da", "m\u00fcs\u00b7sen", "sie", "auch", "hin", ",", "wo", "mein'", ",", "ist", "ih\u00b7re", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die morgen-r\u00f6the hat sie durch den thau gezeuget,", "tokens": ["Die", "mor\u00b7gen\u00b7r\u00f6\u00b7the", "hat", "sie", "durch", "den", "thau", "ge\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Darum geh\u00f6ren sie auch einer sonne zu.", "tokens": ["Da\u00b7rum", "ge\u00b7h\u00f6\u00b7ren", "sie", "auch", "ei\u00b7ner", "son\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nimm sie erfreuet an, so schwinden meine schmertzen,", "tokens": ["Nimm", "sie", "er\u00b7freu\u00b7et", "an", ",", "so", "schwin\u00b7den", "mei\u00b7ne", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nimm sie und auch zugleich derselben art an dich;", "tokens": ["Nimm", "sie", "und", "auch", "zu\u00b7gleich", "der\u00b7sel\u00b7ben", "art", "an", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "ADV", "ADV", "PDAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du kennest diese ja, sie st\u00e4rckt die matten hertzen,", "tokens": ["Du", "ken\u00b7nest", "die\u00b7se", "ja", ",", "sie", "st\u00e4rckt", "die", "mat\u00b7ten", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ach st\u00e4rcke, Doris! auch mit deinen perlen mich!", "tokens": ["Ach", "st\u00e4r\u00b7cke", ",", "Do\u00b7ris", "!", "auch", "mit", "dei\u00b7nen", "per\u00b7len", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "NE", "$.", "ADV", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dergleichen perlen sind vor mein geschlecht erkohren,", "tokens": ["Derg\u00b7lei\u00b7chen", "per\u00b7len", "sind", "vor", "mein", "ge\u00b7schlecht", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VAFIN", "APPR", "PPOSAT", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der himmel machte sie, da er an uns gedacht;", "tokens": ["Der", "him\u00b7mel", "mach\u00b7te", "sie", ",", "da", "er", "an", "uns", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und die hat die natur vor Evens volck gebohren,", "tokens": ["Und", "die", "hat", "die", "na\u00b7tur", "vor", "E\u00b7vens", "volck", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und also den verlust der ersten gleich gemacht.", "tokens": ["Und", "al\u00b7so", "den", "ver\u00b7lust", "der", "ers\u00b7ten", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "VVFIN", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mein hertze sehnet sich nun nach den perlen-tr\u00e4ncken,", "tokens": ["Mein", "hert\u00b7ze", "seh\u00b7net", "sich", "nun", "nach", "den", "per\u00b7len\u00b7tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es stellt sich abgematt bey deinen sch\u00e4tzen ein.", "tokens": ["Es", "stellt", "sich", "ab\u00b7ge\u00b7matt", "bey", "dei\u00b7nen", "sch\u00e4t\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wilst du vor perlen ihm auch was von perlen schencken;", "tokens": ["Wilst", "du", "vor", "per\u00b7len", "ihm", "auch", "was", "von", "per\u00b7len", "schen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "ADV", "PWS", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So la\u00df den perlen-ku\u00df davon den anfang seyn!", "tokens": ["So", "la\u00df", "den", "per\u00b7len\u00b7ku\u00df", "da\u00b7von", "den", "an\u00b7fang", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PAV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}