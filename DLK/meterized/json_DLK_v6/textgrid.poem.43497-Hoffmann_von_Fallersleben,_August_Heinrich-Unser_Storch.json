{"textgrid.poem.43497": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unser Storch", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr ihn noch nicht vernommen?", "tokens": ["Habt", "ihr", "ihn", "noch", "nicht", "ver\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Dache sitzt er schon.", "tokens": ["Auf", "dem", "Da\u00b7che", "sitzt", "er", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Storch ist heimgekommen,", "tokens": ["Un\u00b7ser", "Storch", "ist", "heim\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt doch! h\u00f6rt den frohen Ton!", "tokens": ["H\u00f6rt", "doch", "!", "h\u00f6rt", "den", "fro\u00b7hen", "Ton", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Klappre du, klappre du klapp klapp klapp!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Klapp klapp klapp!", "tokens": ["Klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Klappre du, klappre du immerzu!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ja, du bist nun eingetroffen", "tokens": ["Ja", ",", "du", "bist", "nun", "ein\u00b7ge\u00b7trof\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach so langer Winternacht,", "tokens": ["Nach", "so", "lan\u00b7ger", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast erf\u00fcllet unser Hoffen", "tokens": ["Hast", "er\u00b7f\u00fcl\u00b7let", "un\u00b7ser", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Fr\u00fchling mitgebracht.", "tokens": ["Und", "den", "Fr\u00fch\u00b7ling", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Klappre du, klappre du klapp klapp klapp!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Klapp klapp klapp!", "tokens": ["Klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Klappre du, klappre du immerzu!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach, wie t\u00f6nt in unsre Ohren", "tokens": ["Ach", ",", "wie", "t\u00f6nt", "in", "uns\u00b7re", "Oh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch so s\u00fc\u00df der frohe Ton:", "tokens": ["Doch", "so", "s\u00fc\u00df", "der", "fro\u00b7he", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, wir sind wie neugeboren,", "tokens": ["Ja", ",", "wir", "sind", "wie", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "KOKOM", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Winter ist entfloh'n.", "tokens": ["Denn", "der", "Win\u00b7ter", "ist", "ent\u00b7floh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "VAFIN", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Klappre du, klappre du klapp klapp klapp!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Klapp klapp klapp!", "tokens": ["Klapp", "klapp", "klapp", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Klappre du, klappre du immerzu!", "tokens": ["Klapp\u00b7re", "du", ",", "klapp\u00b7re", "du", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}