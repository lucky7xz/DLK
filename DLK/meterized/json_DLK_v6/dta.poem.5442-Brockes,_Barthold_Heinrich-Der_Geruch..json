{"dta.poem.5442": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wie durch den Schall die Luft bald harmonisch", "tokens": ["So", "wie", "durch", "den", "Schall", "die", "Luft", "bald", "har\u00b7mo\u00b7nisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "zugericht,", "tokens": ["zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bald zu klugen W\u00f6rtern wird; wie sie hell wird durch", "tokens": ["Bald", "zu", "klu\u00b7gen", "W\u00f6r\u00b7tern", "wird", ";", "wie", "sie", "hell", "wird", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VAFIN", "$.", "PWAV", "PPER", "ADJD", "VAFIN", "APPR"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ein Licht;", "tokens": ["ein", "Licht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So wird durch den reinen Balsam, der aus bunten Blu-", "tokens": ["So", "wird", "durch", "den", "rei\u00b7nen", "Bal\u00b7sam", ",", "der", "aus", "bun\u00b7ten", "Blu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "TRUNC"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "men steiget,", "tokens": ["men", "stei\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie, durch ein unsichtbar Licht, hell. Ich h\u00f6r\u2019, da\u00df etwas", "tokens": ["Sie", ",", "durch", "ein", "un\u00b7sicht\u00b7bar", "Licht", ",", "hell", ".", "Ich", "h\u00f6r'", ",", "da\u00df", "et\u00b7was"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "ADJD", "NN", "$,", "ADJD", "$.", "PPER", "VVFIN", "$,", "KOUS", "PIS"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "spricht,", "tokens": ["spricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und ein s\u00fc\u00df, harmonisch Wesen meiner Seele deutlich zeiget,", "tokens": ["Und", "ein", "s\u00fc\u00df", ",", "har\u00b7mo\u00b7nisch", "We\u00b7sen", "mei\u00b7ner", "See\u00b7le", "deut\u00b7lich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "$,", "ADJD", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Welches mich durch Lust zur Andacht, sonderlich zum Dan-", "tokens": ["Wel\u00b7ches", "mich", "durch", "Lust", "zur", "An\u00b7dacht", ",", "son\u00b7der\u00b7lich", "zum", "Dan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "APPRART", "NN", "$,", "ADJD", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "cken, treibt", "tokens": ["cken", ",", "treibt"], "token_info": ["word", "punct", "word"], "pos": ["VVINF", "$,", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Und, mit bunten Lettern, gleichsam die\u00df in meine Seele", "tokens": ["Und", ",", "mit", "bun\u00b7ten", "Let\u00b7tern", ",", "gleich\u00b7sam", "die\u00df", "in", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "ADJD", "PDS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "schreibt:", "tokens": ["schreibt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Unser Geist kan im Empfinden, zu des grossen Sch\u00f6pfers", "tokens": ["Un\u00b7ser", "Geist", "kan", "im", "Emp\u00b7fin\u00b7den", ",", "zu", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Ehren,", "tokens": ["Eh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Wenn sie nur bedachtsam riecht, deutlich diese weisen Lehren,", "tokens": ["Wenn", "sie", "nur", "be\u00b7dacht\u00b7sam", "riecht", ",", "deut\u00b7lich", "die\u00b7se", "wei\u00b7sen", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADJD", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Aus der Blumen leisen Sprache, wie aus allen Dingen, h\u00f6ren.", "tokens": ["Aus", "der", "Blu\u00b7men", "lei\u00b7sen", "Spra\u00b7che", ",", "wie", "aus", "al\u00b7len", "Din\u00b7gen", ",", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "APPR", "PIAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Liest die Seele durchs Gesicht; stellet sie sich durch das Ohr", "tokens": ["Liest", "die", "See\u00b7le", "durchs", "Ge\u00b7sicht", ";", "stel\u00b7let", "sie", "sich", "durch", "das", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Wenn sie treue Lehrer h\u00f6ret: ", "tokens": ["Wenn", "sie", "treu\u00b7e", "Leh\u00b7rer", "h\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "vor;", "tokens": ["vor", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Riecht sie eben die\u00df in Blumen. Denn die wolgemischten", "tokens": ["Riecht", "sie", "e\u00b7ben", "die\u00df", "in", "Blu\u00b7men", ".", "Denn", "die", "wol\u00b7ge\u00b7mischten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "APPR", "NN", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "S\u00e4fte", "tokens": ["S\u00e4f\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und des k\u00fcnstlich-edlen Werckzeugs unserer Nasen scharf-", "tokens": ["Und", "des", "k\u00fcnst\u00b7lich\u00b7ed\u00b7len", "Wer\u00b7ck\u00b7zeugs", "un\u00b7se\u00b7rer", "Na\u00b7sen", "scha\u00b7rf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN", "TRUNC"], "meter": "--+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "fe Kr\u00e4fte", "tokens": ["fe", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Zeigen, wenn man es erweget, ja so deut-als lieblich an,", "tokens": ["Zei\u00b7gen", ",", "wenn", "man", "es", "er\u00b7we\u00b7get", ",", "ja", "so", "deut\u00b7als", "lieb\u00b7lich", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Da\u00df vom Sch\u00f6pfer alles stammet, aus sich selbst nichts", "tokens": ["Da\u00df", "vom", "Sch\u00f6p\u00b7fer", "al\u00b7les", "stam\u00b7met", ",", "aus", "sich", "selbst", "nichts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PIS", "VVFIN", "$,", "APPR", "PRF", "ADV", "PIS"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.27": {"text": "werden kann.", "tokens": ["wer\u00b7den", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VAINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}