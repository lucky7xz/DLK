{"textgrid.poem.49309": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An den dichten Wald", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spielet sanft, ihr schlanken Zweige,", "tokens": ["Spie\u00b7let", "sanft", ",", "ihr", "schlan\u00b7ken", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielet, weil der Tag ist hell,", "tokens": ["Spie\u00b7let", ",", "weil", "der", "Tag", "ist", "hell", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' auch Ph\u00f6bus l\u00e4uft zur Neige,", "tokens": ["Eh'", "auch", "Ph\u00f6\u00b7bus", "l\u00e4uft", "zur", "Nei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielet meiner Florabell;", "tokens": ["Spie\u00b7let", "mei\u00b7ner", "Flo\u00b7ra\u00b7bell", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfet mir, mit s\u00fc\u00dfen Weisen", "tokens": ["Hel\u00b7fet", "mir", ",", "mit", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Menscheng\u00f6ttin preisen.", "tokens": ["Die\u00b7se", "Men\u00b7schen\u00b7g\u00f6t\u00b7tin", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zweiglein sauset in die Wette", "tokens": ["Zwei\u00b7glein", "sau\u00b7set", "in", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gleichsam einen Lobgesang,", "tokens": ["Gleich\u00b7sam", "ei\u00b7nen", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eh' die Sch\u00f6nste geht zu Bette", "tokens": ["Eh'", "die", "Sch\u00f6ns\u00b7te", "geht", "zu", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und man h\u00f6rt der Fr\u00f6sche Klang;", "tokens": ["Und", "man", "h\u00f6rt", "der", "Fr\u00f6\u00b7sche", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn wil ich die Stimm' auch schwingen,", "tokens": ["Denn", "wil", "ich", "die", "Stimm'", "auch", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Florabellen zu besingen.", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7len", "zu", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Neiget euch doch bald, ihr Eichen,", "tokens": ["Nei\u00b7get", "euch", "doch", "bald", ",", "ihr", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gar zur Erden mit Begier!", "tokens": ["Gar", "zur", "Er\u00b7den", "mit", "Be\u00b7gier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Florabellen seh' ich schleichen", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7len", "seh'", "ich", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hinter jenem Busch herf\u00fcr.", "tokens": ["Hin\u00b7ter", "je\u00b7nem", "Busch", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ach, was trag' ich gro\u00df Verlangen,", "tokens": ["Ach", ",", "was", "trag'", "ich", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Florabellen zu empfangen!", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7len", "zu", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O du sch\u00f6nster Tag im Lenzen,", "tokens": ["O", "du", "sch\u00f6ns\u00b7ter", "Tag", "im", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ph\u00f6bus machet dich zwar rein,", "tokens": ["Ph\u00f6\u00b7bus", "ma\u00b7chet", "dich", "zwar", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Nun ich aber sehe gl\u00e4nzen", "tokens": ["Nun", "ich", "a\u00b7ber", "se\u00b7he", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Florabellen Aeugelein,", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7len", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Wirst du sch\u00f6ner als die Sonne;", "tokens": ["Wirst", "du", "sch\u00f6\u00b7ner", "als", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Eile doch, mein Herz und Wonne!", "tokens": ["Ei\u00b7le", "doch", ",", "mein", "Herz", "und", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ja, sie komt mit sanften Schritten!", "tokens": ["Ja", ",", "sie", "komt", "mit", "sanf\u00b7ten", "Schrit\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denket, welch ein' Himmelspracht!", "tokens": ["Den\u00b7ket", ",", "welch", "ein'", "Him\u00b7mel\u00b7spracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Dicker Wald, la\u00df dich erbitten,", "tokens": ["Di\u00b7cker", "Wald", ",", "la\u00df", "dich", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Oeffne dich mit ganzer Macht,", "tokens": ["Oeff\u00b7ne", "dich", "mit", "gan\u00b7zer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Denn so wil ich n\u00e4her treten,", "tokens": ["Denn", "so", "wil", "ich", "n\u00e4\u00b7her", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Florabellen anzubeten.", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7len", "an\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Florabella, la\u00df mich knieen", "tokens": ["Flo\u00b7ra\u00b7bel\u00b7la", ",", "la\u00df", "mich", "kni\u00b7e\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVFIN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.32": {"text": "Nur f\u00fcr deiner Majestat!", "tokens": ["Nur", "f\u00fcr", "dei\u00b7ner", "Ma\u00b7jes\u00b7tat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Daphnis wil nur vollenziehen,", "tokens": ["Daph\u00b7nis", "wil", "nur", "vol\u00b7len\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Was er dir versprochen hat;", "tokens": ["Was", "er", "dir", "ver\u00b7spro\u00b7chen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "La\u00df ihn, sol er nicht verderben,", "tokens": ["La\u00df", "ihn", ",", "sol", "er", "nicht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Sch\u00f6nste, deinen Schlaven sterben.", "tokens": ["Sch\u00f6ns\u00b7te", ",", "dei\u00b7nen", "Schla\u00b7ven", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}