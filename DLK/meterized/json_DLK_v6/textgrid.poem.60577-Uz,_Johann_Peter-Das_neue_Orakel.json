{"textgrid.poem.60577": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Orakel", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Propheten unsrer Zeit,", "tokens": ["Pro\u00b7phe\u00b7ten", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zigeuner, alte Weiber,", "tokens": ["Zi\u00b7geu\u00b7ner", ",", "al\u00b7te", "Wei\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer ihr alle seyd,", "tokens": ["Und", "wer", "ihr", "al\u00b7le", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Neugier Zeitvertreiber!", "tokens": ["Der", "Neu\u00b7gier", "Zeit\u00b7ver\u00b7trei\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun ists um euch gethan:", "tokens": ["Nun", "ists", "um", "euch", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer wird euch k\u00fcnftig fragen?", "tokens": ["Wer", "wird", "euch", "k\u00fcnf\u00b7tig", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Coffeesatz f\u00e4ngt an", "tokens": ["Der", "Cof\u00b7fee\u00b7satz", "f\u00e4ngt", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In Bildern wahrzusagen.", "tokens": ["In", "Bil\u00b7dern", "wahr\u00b7zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Phantasie erkl\u00e4rt", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "er\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Ausspruchs finstre Z\u00fcge.", "tokens": ["Des", "Aus\u00b7spruchs", "finst\u00b7re", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sieht, wann ihrs begehrt,", "tokens": ["Die", "sieht", ",", "wann", "ihrs", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Schlamme Baar und Wiege:", "tokens": ["Im", "Schlam\u00b7me", "Baar", "und", "Wie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie, wer den \u00f6den Wald", "tokens": ["Wie", ",", "wer", "den", "\u00f6\u00b7den", "Wald"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um Mitternacht durchziehet,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "durch\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald eines Geists Gestalt,", "tokens": ["Bald", "ei\u00b7nes", "Geists", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bald helle Sch\u00e4tze siehet.", "tokens": ["Bald", "hel\u00b7le", "Sch\u00e4t\u00b7ze", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch mich versichert sie", "tokens": ["Auch", "mich", "ver\u00b7si\u00b7chert", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit ernstlicher Geberde,", "tokens": ["Mit", "ernst\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich, nach kurzer M\u00fch,", "tokens": ["Da\u00df", "ich", ",", "nach", "kur\u00b7zer", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein M\u00e4dchen schn\u00e4beln werde.", "tokens": ["Mein", "M\u00e4d\u00b7chen", "schn\u00e4\u00b7beln", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Auge t\u00e4uscht sie nicht:", "tokens": ["Ihr", "Au\u00b7ge", "t\u00e4uscht", "sie", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da schn\u00e4beln sich zwo Tauben;", "tokens": ["Da", "schn\u00e4\u00b7beln", "sich", "zwo", "Tau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was sie mir verspricht,", "tokens": ["Und", "was", "sie", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hilft mir die Liebe glauben.", "tokens": ["Hilft", "mir", "die", "Lie\u00b7be", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sey gl\u00e4ubig, spr\u00f6des Kind!", "tokens": ["Sey", "gl\u00e4u\u00b7big", ",", "spr\u00f6\u00b7des", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und komm und la\u00df dich k\u00fcssen.", "tokens": ["Und", "komm", "und", "la\u00df", "dich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Sterblicher entrinnt", "tokens": ["Kein", "Sterb\u00b7li\u00b7cher", "ent\u00b7rinnt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Schicksals festen Schl\u00fcssen.", "tokens": ["Des", "Schick\u00b7sals", "fes\u00b7ten", "Schl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn Tauben stehen da,", "tokens": ["Denn", "Tau\u00b7ben", "ste\u00b7hen", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kann ich sie gleich nicht sehen.", "tokens": ["Kann", "ich", "sie", "gleich", "nicht", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was Bileam nicht sah,", "tokens": ["Was", "Bi\u00b7leam", "nicht", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sah doch sein Esel stehen.", "tokens": ["Sah", "doch", "sein", "E\u00b7sel", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}