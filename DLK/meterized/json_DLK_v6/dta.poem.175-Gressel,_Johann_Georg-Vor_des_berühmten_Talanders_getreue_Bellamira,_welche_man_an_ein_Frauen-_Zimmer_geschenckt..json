{"dta.poem.175": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vor des ber\u00fchmten  Talanders  getreue  \n  Bellamira,  welche man an ein Frauen-  \n Zimmer geschenckt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So kr\u00f6hnt Best\u00e4ndigkeit/ nach ausgestandnen Proben/", "tokens": ["So", "kr\u00b7\u00f6hnt", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "nach", "aus\u00b7ge\u00b7stand\u00b7nen", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zuletzt den treuen Sinn mit einem Sieges-Krantz/", "tokens": ["Zu\u00b7letzt", "den", "treu\u00b7en", "Sinn", "mit", "ei\u00b7nem", "Sie\u00b7ges\u00b7Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wie ein Fels im Meer bleibt unzerst\u00fcckt und gantz/", "tokens": ["Denn", "wie", "ein", "Fels", "im", "Meer", "bleibt", "un\u00b7zer\u00b7st\u00fcckt", "und", "gantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "ADJD", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob gleich des ", "tokens": ["Ob", "gleich", "des"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So bleibt Best\u00e4ndigkeit trotz allen Wettern oben/", "tokens": ["So", "bleibt", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "trotz", "al\u00b7len", "Wet\u00b7tern", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn gleich des Reiches-Sonn verehrte ihren Glantz/", "tokens": ["Wenn", "gleich", "des", "Rei\u00b7ches\u00b7Sonn", "ver\u00b7ehr\u00b7te", "ih\u00b7ren", "Glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier f\u00fchrt der Printz zuletzt die treue Braut zum Tantz/", "tokens": ["Hier", "f\u00fchrt", "der", "Printz", "zu\u00b7letzt", "die", "treu\u00b7e", "Braut", "zum", "Tantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nachdem des Nebels-Dunst und Ungl\u00fccks-Lufft verstoben.", "tokens": ["Nach\u00b7dem", "des", "Ne\u00b7bels\u00b7Dunst", "und", "Un\u00b7gl\u00fccks\u00b7Lufft", "ver\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier lerne ", "tokens": ["Hier", "ler\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Da\u00df dich kein Ungl\u00fccks-Wind mag von der Stelle schieben.", "tokens": ["Da\u00df", "dich", "kein", "Un\u00b7gl\u00fccks\u00b7Wind", "mag", "von", "der", "Stel\u00b7le", "schie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich werde trotz den Neid/ trotz allen Donner-Wettern", "tokens": ["Ich", "wer\u00b7de", "trotz", "den", "Neid", "/", "trotz", "al\u00b7len", "Don\u00b7ner\u00b7Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In meiner Liebe stehn/ und in der Treue Schrancken", "tokens": ["In", "mei\u00b7ner", "Lie\u00b7be", "stehn", "/", "und", "in", "der", "Treu\u00b7e", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$(", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gar wohl gesichert seyn/ bey deren Lorber-Bl\u00e4ttern/", "tokens": ["Gar", "wohl", "ge\u00b7si\u00b7chert", "seyn", "/", "bey", "de\u00b7ren", "Lor\u00b7ber\u00b7Bl\u00e4t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$(", "APPR", "PRELAT", "NN", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Nur lasse du mein Kind die feste Treu nicht wancken.", "tokens": ["Nur", "las\u00b7se", "du", "mein", "Kind", "die", "fes\u00b7te", "Treu", "nicht", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}