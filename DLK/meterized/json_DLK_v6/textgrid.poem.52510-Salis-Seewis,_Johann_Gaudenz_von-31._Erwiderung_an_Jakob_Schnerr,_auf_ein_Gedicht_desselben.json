{"textgrid.poem.52510": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "31. Erwiderung an Jakob Schnerr, auf ein Gedicht desselben", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Ossian aus Selmas Felsenhallen", "tokens": ["Wie", "Os\u00b7si\u00b7an", "aus", "Sel\u00b7mas", "Fel\u00b7sen\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah manches W\u00f6lkchen ich vor\u00fcbergleiten;", "tokens": ["Sah", "man\u00b7ches", "W\u00f6lk\u00b7chen", "ich", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch lichtere besuchten mich zu Zeiten,", "tokens": ["Auch", "lich\u00b7te\u00b7re", "be\u00b7such\u00b7ten", "mich", "zu", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um fl\u00fcchtig stumm im Blauen zu verwallen.", "tokens": ["Um", "fl\u00fcch\u00b7tig", "stumm", "im", "Blau\u00b7en", "zu", "ver\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch t\u00f6nte Wohllaut, wie von Harfensaiten;", "tokens": ["Doch", "t\u00f6n\u00b7te", "Wohl\u00b7laut", ",", "wie", "von", "Har\u00b7fen\u00b7sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie leises Echo ferner Nachtigallen,", "tokens": ["Wie", "lei\u00b7ses", "E\u00b7cho", "fer\u00b7ner", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernimmt der Greis mit heiterm Wohlgefallen", "tokens": ["Ver\u00b7nimmt", "der", "Greis", "mit", "hei\u00b7term", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Freundesgru\u00df, den ihm die Geister weihten.", "tokens": ["Den", "Freun\u00b7des\u00b7gru\u00df", ",", "den", "ihm", "die", "Geis\u00b7ter", "weih\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als zart verh\u00fcllt im Silberw\u00f6lkchen-Schleier", "tokens": ["Als", "zart", "ver\u00b7h\u00fcllt", "im", "Sil\u00b7ber\u00b7w\u00f6lk\u00b7chen\u00b7Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "J\u00fcngst deine Muse freundlich mich besucht,", "tokens": ["J\u00fcngst", "dei\u00b7ne", "Mu\u00b7se", "freund\u00b7lich", "mich", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erhob mein Herz sich freudiger und freier.", "tokens": ["Er\u00b7hob", "mein", "Herz", "sich", "freu\u00b7di\u00b7ger", "und", "frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O hemme, rief ich, W\u00f6lkchen, deine Flucht;", "tokens": ["O", "hem\u00b7me", ",", "rief", "ich", ",", "W\u00f6lk\u00b7chen", ",", "dei\u00b7ne", "Flucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dieses Alpenthales \u00f6der Bucht", "tokens": ["In", "die\u00b7ses", "Al\u00b7pen\u00b7tha\u00b7les", "\u00f6\u00b7der", "Bucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ehrt deinen Sender man in stiller Feier!", "tokens": ["Ehrt", "dei\u00b7nen", "Sen\u00b7der", "man", "in", "stil\u00b7ler", "Fei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}