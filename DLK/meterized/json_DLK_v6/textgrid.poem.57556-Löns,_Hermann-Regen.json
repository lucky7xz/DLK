{"textgrid.poem.57556": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Regen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel h\u00e4lt gro\u00dfe W\u00e4sche heut", "tokens": ["Der", "Him\u00b7mel", "h\u00e4lt", "gro\u00b7\u00dfe", "W\u00e4\u00b7sche", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und s\u00e4ubert die staubige Erde,", "tokens": ["Und", "s\u00e4u\u00b7bert", "die", "stau\u00b7bi\u00b7ge", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Damit so gl\u00e4nzend und rein ihr Gewand", "tokens": ["Da\u00b7mit", "so", "gl\u00e4n\u00b7zend", "und", "rein", "ihr", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie am ersten Maitage werde.", "tokens": ["Wie", "am", "ers\u00b7ten", "Mai\u00b7ta\u00b7ge", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das donnert und blitzt und prasselt und klatscht", "tokens": ["Das", "don\u00b7nert", "und", "blitzt", "und", "pras\u00b7selt", "und", "klatscht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hernieder auf Zweige und Bl\u00e4tter,", "tokens": ["Her\u00b7nie\u00b7der", "auf", "Zwei\u00b7ge", "und", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der durstige Rasen trinkt sich satt", "tokens": ["Der", "durs\u00b7ti\u00b7ge", "Ra\u00b7sen", "trinkt", "sich", "satt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach langem, trockenem Wetter.", "tokens": ["Nach", "lan\u00b7gem", ",", "tro\u00b7cke\u00b7nem", "Wet\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich wollt, von dem k\u00fchlend erfrischenden Gu\u00df", "tokens": ["Ich", "wollt", ",", "von", "dem", "k\u00fch\u00b7lend", "er\u00b7fri\u00b7schen\u00b7den", "Gu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "W\u00fcrd' auch mein Herz getroffen,", "tokens": ["W\u00fcrd'", "auch", "mein", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinweggesp\u00fclt wie der Staub vom Gras", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7sp\u00fclt", "wie", "der", "Staub", "vom", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00fcrd' Glaube, Liebe und Hoffen.", "tokens": ["W\u00fcrd'", "Glau\u00b7be", ",", "Lie\u00b7be", "und", "Hof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine wei\u00dfe Rose vor mir steht,", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "Ro\u00b7se", "vor", "mir", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ge\u00f6ffnet ist das Fenster \u2013", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "ist", "das", "Fens\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinaus damit, verschwindet jetzt", "tokens": ["Hin\u00b7aus", "da\u00b7mit", ",", "ver\u00b7schwin\u00b7det", "jetzt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "PAV", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sentimentalen Gespenster.", "tokens": ["Ihr", "sen\u00b7ti\u00b7men\u00b7ta\u00b7len", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Das St\u00fcck von \u00bbDu und Ich\u00ab ist aus,", "tokens": ["Das", "St\u00fcck", "von", "\u00bb", "Du", "und", "Ich", "\u00ab", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "PPER", "KON", "PPER", "$(", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vorhang wird geschlossen,", "tokens": ["Der", "Vor\u00b7hang", "wird", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lebenszeit zu kostbar ist", "tokens": ["Die", "Le\u00b7bens\u00b7zeit", "zu", "kost\u00b7bar", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKA", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr solche Narrenspossen.", "tokens": ["F\u00fcr", "sol\u00b7che", "Nar\u00b7rens\u00b7pos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Klar mu\u00df der Geist und n\u00fcchtern sein", "tokens": ["Klar", "mu\u00df", "der", "Geist", "und", "n\u00fcch\u00b7tern", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "KON", "ADJD", "VAINF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Im frischen Arbeitsgetriebe \u2013", "tokens": ["Im", "fri\u00b7schen", "Ar\u00b7beits\u00b7ge\u00b7trie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du uraltsch\u00f6ne Allnatur,", "tokens": ["Du", "ur\u00b7alt\u00b7sch\u00f6\u00b7ne", "All\u00b7na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibst meine letzte Liebe!", "tokens": ["Du", "bleibst", "mei\u00b7ne", "letz\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}