{"textgrid.poem.24485": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ist dem Himmel nah; du Leser bist auf Erden/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist dem Himmel nah; du Leser bist auf Erden/", "tokens": ["Der", "ist", "dem", "Him\u00b7mel", "nah", ";", "du", "Le\u00b7ser", "bist", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$.", "PPER", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stehst doch \u00fcber ihn. Wie er die Fahne dreht/", "tokens": ["Und", "stehst", "doch", "\u00fc\u00b7ber", "ihn", ".", "Wie", "er", "die", "Fah\u00b7ne", "dreht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$.", "PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schwenckst du die Grillen rum. Wenn ihn sein Amt erh\u00f6ht/", "tokens": ["Schwenckst", "du", "die", "Gril\u00b7len", "rum", ".", "Wenn", "ihn", "sein", "Amt", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht dich die Thorheit gro\u00df. Was wirds nun endlich werden?", "tokens": ["Macht", "dich", "die", "Thor\u00b7heit", "gro\u00df", ".", "Was", "wirds", "nun", "end\u00b7lich", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "$.", "PWS", "VAFIN", "ADV", "ADV", "VAINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Dein Ehrgeitz mag ein Schiefer-Decker seyn/", "tokens": ["Dein", "Ehr\u00b7geitz", "mag", "ein", "Schie\u00b7fer\u00b7De\u00b7cker", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sey gro\u00df in deiner Luft/ auf Erden bist du klein.", "tokens": ["Sey", "gro\u00df", "in", "dei\u00b7ner", "Luft", "/", "auf", "Er\u00b7den", "bist", "du", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPOSAT", "NN", "$(", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}