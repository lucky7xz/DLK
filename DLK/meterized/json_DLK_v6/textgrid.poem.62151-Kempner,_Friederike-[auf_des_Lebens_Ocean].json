{"textgrid.poem.62151": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[auf des Lebens Ocean]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Lebens Ocean", "tokens": ["Auf", "des", "Le\u00b7bens", "O\u00b7cean"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00e4hrt der Dampfer stolz geehrt,", "tokens": ["F\u00e4hrt", "der", "Damp\u00b7fer", "stolz", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher, ach, im schwanken Kahn", "tokens": ["Man\u00b7cher", ",", "ach", ",", "im", "schwan\u00b7ken", "Kahn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "ITJ", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehend jeder Welle wehrt.", "tokens": ["Ste\u00b7hend", "je\u00b7der", "Wel\u00b7le", "wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seh't das Schiff mit sichrer Hast", "tokens": ["Seh't", "das", "Schiff", "mit", "sich\u00b7rer", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fast im Hafen liegt \u2013", "tokens": ["Fast", "im", "Ha\u00b7fen", "liegt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Nachen ohne Rast", "tokens": ["Und", "der", "Na\u00b7chen", "oh\u00b7ne", "Rast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Wogen fliegt.", "tokens": ["Auf", "den", "Wo\u00b7gen", "fliegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Himmelhoch und abgrundstief", "tokens": ["Him\u00b7mel\u00b7hoch", "und", "ab\u00b7grund\u00b7stief"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Gott, der Mensch zu Hilfe rief \u2013", "tokens": ["\u2013", "Gott", ",", "der", "Mensch", "zu", "Hil\u00b7fe", "rief", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich er vor Anker lief!", "tokens": ["End\u00b7lich", "er", "vor", "An\u00b7ker", "lief", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es rennt des Dampfers Last", "tokens": ["Doch", "es", "rennt", "des", "Damp\u00b7fers", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Klippen m\u00e4cht'gen Ast", "tokens": ["An", "der", "Klip\u00b7pen", "m\u00e4cht'\u00b7gen", "Ast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zerschellet Kiel und Mast. \u2013", "tokens": ["Und", "zer\u00b7schel\u00b7let", "Kiel", "und", "Mast", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}