{"textgrid.poem.52152": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die kleine Blanche", "genre": "verse", "period": "N.A.", "pub_year": 1874, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem kleinen Hofe von Navarra", "tokens": ["An", "dem", "klei\u00b7nen", "Ho\u00b7fe", "von", "Na\u00b7var\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War das Leben eine lose Fabel,", "tokens": ["War", "das", "Le\u00b7ben", "ei\u00b7ne", "lo\u00b7se", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine drohnde oder heitre Maske,", "tokens": ["Ei\u00b7ne", "drohn\u00b7de", "o\u00b7der", "heit\u00b7re", "Mas\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine \u00fcberraschende Novelle,", "tokens": ["Ei\u00b7ne", "\u00fc\u00b7berr\u00b7a\u00b7schen\u00b7de", "No\u00b7vel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Ein phantastisch wahrheitloses Schauspiel \u2013", "tokens": ["Ein", "phan\u00b7tas\u00b7tisch", "wahr\u00b7heit\u00b7lo\u00b7ses", "Schau\u00b7spiel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der am Hofe war auf kurzen Urlaub,", "tokens": ["Der", "am", "Ho\u00b7fe", "war", "auf", "kur\u00b7zen", "Ur\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Hauptmann Duplessis sa\u00df vor der B\u00fchne,", "tokens": ["Haupt\u00b7mann", "Du\u00b7ples\u00b7sis", "sa\u00df", "vor", "der", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Drauf ein M\u00e4dchen an verratner Liebe", "tokens": ["Drauf", "ein", "M\u00e4d\u00b7chen", "an", "ver\u00b7rat\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Starb. Im letzten Akte lag sie marmorn", "tokens": ["Starb", ".", "Im", "letz\u00b7ten", "Ak\u00b7te", "lag", "sie", "mar\u00b7morn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Auf dem Grabmal als ihr eigen Bildnis,", "tokens": ["Auf", "dem", "Grab\u00b7mal", "als", "ihr", "ei\u00b7gen", "Bild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Schluchzend rang die H\u00e4nde der Verr\u00e4ter,", "tokens": ["Schluch\u00b7zend", "rang", "die", "H\u00e4n\u00b7de", "der", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sieh! da hob sie sachte sich und lebte.", "tokens": ["Sieh", "!", "da", "hob", "sie", "sach\u00b7te", "sich", "und", "leb\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Andern Tages wandelte der Hauptmann", "tokens": ["An\u00b7dern", "Ta\u00b7ges", "wan\u00b7del\u00b7te", "der", "Haupt\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "In des Schlosses irrsam dunkeln G\u00e4rten,", "tokens": ["In", "des", "Schlos\u00b7ses", "irr\u00b7sam", "dun\u00b7keln", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "An die zarte kleine Blanche denkend,", "tokens": ["An", "die", "zar\u00b7te", "klei\u00b7ne", "Blan\u00b7che", "den\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Die er schnell gek\u00fc\u00dft und schnell verraten \u2013", "tokens": ["Die", "er", "schnell", "ge\u00b7k\u00fc\u00dft", "und", "schnell", "ver\u00b7ra\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Etwas sieht er schimmern durch Zypressen:", "tokens": ["Et\u00b7was", "sieht", "er", "schim\u00b7mern", "durch", "Zyp\u00b7res\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Auf dem Grabmal liegt die kleine Blanche", "tokens": ["Auf", "dem", "Grab\u00b7mal", "liegt", "die", "klei\u00b7ne", "Blan\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Marmorn. An dem Sockel ist zu lesen:", "tokens": ["Mar\u00b7morn", ".", "An", "dem", "So\u00b7ckel", "ist", "zu", "le\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u00bbblanche schlummert nach verratner Liebe.\u00ab", "tokens": ["\u00bb", "blan\u00b7che", "schlum\u00b7mert", "nach", "ver\u00b7rat\u00b7ner", "Lie\u00b7be", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "\u00bbheb dich, kleine Blanche!\u00ab ruft der Hauptmann.", "tokens": ["\u00bb", "heb", "dich", ",", "klei\u00b7ne", "Blan\u00b7che", "!", "\u00ab", "ruft", "der", "Haupt\u00b7mann", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "\u00bbwickle dich aus deinen wei\u00dfen T\u00fcchern!", "tokens": ["\u00bb", "wick\u00b7le", "dich", "aus", "dei\u00b7nen", "wei\u00b7\u00dfen", "T\u00fc\u00b7chern", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Spiel nicht mit dem Tode, kleine Blanche!\u00ab", "tokens": ["Spiel", "nicht", "mit", "dem", "To\u00b7de", ",", "klei\u00b7ne", "Blan\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Doch der Marmor f\u00fchlte nichts. Es f\u00fchlte", "tokens": ["Doch", "der", "Mar\u00b7mor", "f\u00fchl\u00b7te", "nichts", ".", "Es", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Nichts, die drunter schl\u00e4ft. Sie starb im Ernste.", "tokens": ["Nichts", ",", "die", "drun\u00b7ter", "schl\u00e4ft", ".", "Sie", "starb", "im", "Erns\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PAV", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}