{"textgrid.poem.45928": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie oft ich dir gesungen]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft ich dir gesungen,", "tokens": ["Wie", "oft", "ich", "dir", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00dft besser du als ich;", "tokens": ["Wei\u00dft", "bes\u00b7ser", "du", "als", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie manchen Kranz geschlungen,", "tokens": ["Wie", "man\u00b7chen", "Kranz", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00dft besser du als ich.", "tokens": ["Wei\u00dft", "bes\u00b7ser", "du", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die hohen Sterne schwanden", "tokens": ["Die", "ho\u00b7hen", "Ster\u00b7ne", "schwan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So d\u00fcster heut in dir,", "tokens": ["So", "d\u00fcs\u00b7ter", "heut", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schwanden die Gedanken", "tokens": ["Es", "schwan\u00b7den", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So d\u00fcster heut in mir.", "tokens": ["So", "d\u00fcs\u00b7ter", "heut", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dir schickt die Blumenkette", "tokens": ["Dir", "schickt", "die", "Blu\u00b7men\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Ameley,", "tokens": ["Die", "sch\u00f6\u00b7ne", "A\u00b7me\u00b7ley", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O helfe mir erretten", "tokens": ["O", "hel\u00b7fe", "mir", "er\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6ne Ameley.", "tokens": ["Die", "sch\u00f6\u00b7ne", "A\u00b7me\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie froh mein Herz geschlagen,", "tokens": ["Wie", "froh", "mein", "Herz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00dft besser du als ich;", "tokens": ["Wei\u00dft", "bes\u00b7ser", "du", "als", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ich mein Leid soll klagen,", "tokens": ["Wie", "ich", "mein", "Leid", "soll", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00dft besser du als ich.", "tokens": ["Wei\u00dft", "bes\u00b7ser", "du", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du gabst mir in den Wellen", "tokens": ["Du", "gabst", "mir", "in", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Ameley,", "tokens": ["Die", "sch\u00f6\u00b7ne", "A\u00b7me\u00b7ley", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wolle mir gesellen", "tokens": ["O", "wol\u00b7le", "mir", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6ne Ameley.", "tokens": ["Die", "sch\u00f6\u00b7ne", "A\u00b7me\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gute Nacht, tu dich bedenken,", "tokens": ["Gu\u00b7te", "Nacht", ",", "tu", "dich", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir das beste sei;", "tokens": ["Was", "mir", "das", "bes\u00b7te", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tu in dem Traum mir schenken", "tokens": ["Tu", "in", "dem", "Traum", "mir", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die sch\u00f6ne Ameley.", "tokens": ["Die", "sch\u00f6\u00b7ne", "A\u00b7me\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}