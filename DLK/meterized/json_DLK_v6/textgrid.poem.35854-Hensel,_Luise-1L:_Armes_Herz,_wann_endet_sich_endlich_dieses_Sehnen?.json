{"textgrid.poem.35854": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Armes Herz, wann endet sich endlich dieses Sehnen?", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armes Herz, wann endet sich endlich dieses Sehnen?", "tokens": ["Ar\u00b7mes", "Herz", ",", "wann", "en\u00b7det", "sich", "end\u00b7lich", "die\u00b7ses", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PRF", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wann, o! wann versiegen sie, diese bittern Thr\u00e4nen?", "tokens": ["Wann", ",", "o", "!", "wann", "ver\u00b7sie\u00b7gen", "sie", ",", "die\u00b7se", "bit\u00b7tern", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "$.", "PWAV", "VVFIN", "PPER", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie am schw\u00fclen Sommertag seufzt nach Thau die Rose,", "tokens": ["Wie", "am", "schw\u00fc\u00b7len", "Som\u00b7mer\u00b7tag", "seufzt", "nach", "Thau", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie das kranke Kind sich sehnt nach der Mutter Schoo\u00dfe,", "tokens": ["Wie", "das", "kran\u00b7ke", "Kind", "sich", "sehnt", "nach", "der", "Mut\u00b7ter", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "So verlangt mein Herz nach Dir, einzig Holder, Lieber!", "tokens": ["So", "ver\u00b7langt", "mein", "Herz", "nach", "Dir", ",", "ein\u00b7zig", "Hol\u00b7der", ",", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "ADJD", "NN", "$,", "ADJD", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ach, das Leben ohne Dich wird mir t\u00e4glich tr\u00fcber.", "tokens": ["Ach", ",", "das", "Le\u00b7ben", "oh\u00b7ne", "Dich", "wird", "mir", "t\u00e4g\u00b7lich", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn das Morgenroth erscheint, ruf' ich: Ach, erscheine!", "tokens": ["Wenn", "das", "Mor\u00b7gen\u00b7roth", "er\u00b7scheint", ",", "ruf'", "ich", ":", "Ach", ",", "er\u00b7schei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$.", "ITJ", "$,", "VVFIN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kommt der Mond, Dein sanftes Bild, sitz' ich noch und weine,", "tokens": ["Kommt", "der", "Mond", ",", "Dein", "sanf\u00b7tes", "Bild", ",", "sitz'", "ich", "noch", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich Dich, o g\u00f6ttlich Herz! ach! so sp\u00e4t erkannte,", "tokens": ["Da\u00df", "ich", "Dich", ",", "o", "g\u00f6tt\u00b7lich", "Herz", "!", "ach", "!", "so", "sp\u00e4t", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "FM", "ADJD", "NN", "$.", "XY", "$.", "ADV", "ADJD", "VVFIN", "$,"], "meter": "--+-+-++-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df in dieser s\u00fcnd'gen Brust fremdes Feuer brannte,", "tokens": ["Da\u00df", "in", "die\u00b7ser", "s\u00fcn\u00b7d'\u00b7gen", "Brust", "frem\u00b7des", "Feu\u00b7er", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da\u00df ich Dich durch meine Schuld lie\u00df am Kreuz erblassen,", "tokens": ["Da\u00df", "ich", "Dich", "durch", "mei\u00b7ne", "Schuld", "lie\u00df", "am", "Kreuz", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Da\u00df ich noch im Todeskampf treulos Dich verlassen!", "tokens": ["Da\u00df", "ich", "noch", "im", "To\u00b7des\u00b7kampf", "treu\u00b7los", "Dich", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "O, nun mu\u00df auch ich Dir nach, auch mit Dir nun sterben!", "tokens": ["O", ",", "nun", "mu\u00df", "auch", "ich", "Dir", "nach", ",", "auch", "mit", "Dir", "nun", "ster\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "ADV", "PPER", "PPER", "PTKVZ", "$,", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimmer kann hienieden ich Ruh' und Trost erwerben.", "tokens": ["Nim\u00b7mer", "kann", "hien\u00b7ie\u00b7den", "ich", "Ruh'", "und", "Trost", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "F\u00fchl' ich meine Wange kalt, meine Brust beklommen,", "tokens": ["F\u00fchl'", "ich", "mei\u00b7ne", "Wan\u00b7ge", "kalt", ",", "mei\u00b7ne", "Brust", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADJD", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sag' ich: Freue dich, mein Herz, bald nun wird Er kommen.", "tokens": ["Sag'", "ich", ":", "Freu\u00b7e", "dich", ",", "mein", "Herz", ",", "bald", "nun", "wird", "Er", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Seh' ich nur ein welkend Blatt fallen und erbleichen,", "tokens": ["Seh'", "ich", "nur", "ein", "wel\u00b7kend", "Blatt", "fal\u00b7len", "und", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "L\u00e4chl' ich still und sage mir: Bald wirst du ihm gleichen.", "tokens": ["L\u00e4chl'", "ich", "still", "und", "sa\u00b7ge", "mir", ":", "Bald", "wirst", "du", "ihm", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Doch so lang' dein Odem weht, sollst als Magd du dienen,", "tokens": ["Doch", "so", "lang'", "dein", "O\u00b7dem", "weht", ",", "sollst", "als", "Magd", "du", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob es dir gelingen mag, deine Schuld zu s\u00fchnen;", "tokens": ["Ob", "es", "dir", "ge\u00b7lin\u00b7gen", "mag", ",", "dei\u00b7ne", "Schuld", "zu", "s\u00fch\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deiner Thr\u00e4nen herbe Fluth mu\u00df noch reicher flie\u00dfen,", "tokens": ["Dei\u00b7ner", "Thr\u00e4\u00b7nen", "her\u00b7be", "Fluth", "mu\u00df", "noch", "rei\u00b7cher", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und du mu\u00dft, o b\u00f6ses Herz, hier noch h\u00e4rter b\u00fc\u00dfen.", "tokens": ["Und", "du", "mu\u00dft", ",", "o", "b\u00f6\u00b7ses", "Herz", ",", "hier", "noch", "h\u00e4r\u00b7ter", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "FM", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Endlich r\u00fchrt Ihn doch dein Leid, reget Sein Erbarmen,", "tokens": ["End\u00b7lich", "r\u00fchrt", "Ihn", "doch", "dein", "Leid", ",", "re\u00b7get", "Sein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und du ruhst in Ewigkeit in des Ew'gen Armen.", "tokens": ["Und", "du", "ruhst", "in", "E\u00b7wig\u00b7keit", "in", "des", "Ew'\u00b7gen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}