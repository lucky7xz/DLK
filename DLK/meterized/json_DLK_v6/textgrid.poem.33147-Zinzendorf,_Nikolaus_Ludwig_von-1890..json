{"textgrid.poem.33147": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1890.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gemeine, mein Herz! ich habe wol schmerz, und f\u00fchle ein leid bey der uns verordneten abreise-zeit;", "tokens": ["Ge\u00b7mei\u00b7ne", ",", "mein", "Herz", "!", "ich", "ha\u00b7be", "wol", "schmerz", ",", "und", "f\u00fch\u00b7le", "ein", "leid", "bey", "der", "uns", "ver\u00b7ord\u00b7ne\u00b7ten", "ab\u00b7rei\u00b7se\u00b7zeit", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ART", "ADJD", "APPR", "PRELS", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+-+-+-+---+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Alleine die gnad, im flammenden rad gef\u00fchret zu seyn zu sonst einem theil der unsichtbarn Gemein;", "tokens": ["Al\u00b7lei\u00b7ne", "die", "gnad", ",", "im", "flam\u00b7men\u00b7den", "rad", "ge\u00b7f\u00fch\u00b7ret", "zu", "seyn", "zu", "sonst", "ei\u00b7nem", "theil", "der", "un\u00b7sicht\u00b7barn", "Ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "PTKZU", "VAINF", "PTKA", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Das selige gl\u00fck, ein ziemliches st\u00fck in eures lichts schein, in diesen gewenden gewallet zu seyn;", "tokens": ["Das", "se\u00b7li\u00b7ge", "gl\u00fck", ",", "ein", "ziem\u00b7li\u00b7ches", "st\u00fck", "in", "eu\u00b7res", "lichts", "schein", ",", "in", "die\u00b7sen", "ge\u00b7wen\u00b7den", "ge\u00b7wal\u00b7let", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "APPR", "PDAT", "ADJA", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+--+-+--+-+-+--+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Mir ist in der hohl so inniglich wohl, darinnen ihr sitzt, vor aller umstellung der s\u00fcnde besch\u00fctzt.", "tokens": ["Mir", "ist", "in", "der", "hohl", "so", "in\u00b7nig\u00b7lich", "wohl", ",", "da\u00b7rin\u00b7nen", "ihr", "sitzt", ",", "vor", "al\u00b7ler", "um\u00b7stel\u00b7lung", "der", "s\u00fcn\u00b7de", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJD", "ADV", "ADJD", "ADV", "$,", "ADV", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+----+-+--+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Dank opfere ich, und heilige mich im blute des Herrn zu eurem geselln in der n\u00e4he und fern.", "tokens": ["Dank", "op\u00b7fe\u00b7re", "ich", ",", "und", "hei\u00b7li\u00b7ge", "mich", "im", "blu\u00b7te", "des", "Herrn", "zu", "eu\u00b7rem", "ge\u00b7selln", "in", "der", "n\u00e4\u00b7he", "und", "fern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "KON", "ADJA", "PPER", "APPRART", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJD", "APPR", "ART", "VVFIN", "KON", "ADJD", "$."], "meter": "+--+--+--+-+--+-+--+--+--+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ihr sch\u00e4felein all im lieblichen stall, die blutige pracht hat euch zu der sehenden wunder gemacht.", "tokens": ["Ihr", "sch\u00e4\u00b7fe\u00b7lein", "all", "im", "lieb\u00b7li\u00b7chen", "stall", ",", "die", "blu\u00b7ti\u00b7ge", "pracht", "hat", "euch", "zu", "der", "se\u00b7hen\u00b7den", "wun\u00b7der", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Bleib, Jesus-kind' bleib dem heiligen leib der s\u00fcndergemein sein alles in allen, ja alles allein.", "tokens": ["Bleib", ",", "Je\u00b7sus\u00b7kind'", "bleib", "dem", "hei\u00b7li\u00b7gen", "leib", "der", "s\u00fcn\u00b7der\u00b7ge\u00b7mein", "sein", "al\u00b7les", "in", "al\u00b7len", ",", "ja", "al\u00b7les", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJD", "VAINF", "PIS", "APPR", "PIAT", "$,", "ADV", "PIS", "ADV", "$."], "meter": "+---+-+--+-+--+-+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}}}}}