{"textgrid.poem.42280": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Vorgef\u00fchl der Gesundheit", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4uschet ihr mit euerm Wechseltanze,", "tokens": ["T\u00e4u\u00b7schet", "ihr", "mit", "eu\u00b7erm", "Wech\u00b7sel\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du, o Wunsch, und du, o Hoffnung, mich?", "tokens": ["Du", ",", "o", "Wunsch", ",", "und", "du", ",", "o", "Hoff\u00b7nung", ",", "mich", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "KON", "PPER", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oder naht im Purpurnelkenkranze", "tokens": ["O\u00b7der", "naht", "im", "Pur\u00b7pur\u00b7nel\u00b7ken\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frohen Trittes die Gesundheit sich?", "tokens": ["Fro\u00b7hen", "Trit\u00b7tes", "die", "Ge\u00b7sund\u00b7heit", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Will sie von dem D\u00e4mon mich erl\u00f6sen,", "tokens": ["Will", "sie", "von", "dem", "D\u00e4\u00b7mon", "mich", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Welcher meine Kraft gefangen nahm?", "tokens": ["Wel\u00b7cher", "mei\u00b7ne", "Kraft", "ge\u00b7fan\u00b7gen", "nahm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Soll ich wiederum zu Dem genesen,", "tokens": ["Soll", "ich", "wie\u00b7de\u00b7rum", "zu", "Dem", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der ich der Natur vom Busen kam?", "tokens": ["Der", "ich", "der", "Na\u00b7tur", "vom", "Bu\u00b7sen", "kam", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df mich dir mein Vorgef\u00fchl verk\u00fcnden,", "tokens": ["La\u00df", "mich", "dir", "mein", "Vor\u00b7ge\u00b7f\u00fchl", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Boie, alter, trauter Herzensfreund!", "tokens": ["Boi\u00b7e", ",", "al\u00b7ter", ",", "trau\u00b7ter", "Her\u00b7zens\u00b7freund", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wonniglich wirst du es mit empfinden,", "tokens": ["Won\u00b7nig\u00b7lich", "wirst", "du", "es", "mit", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "APPR", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wann der Dulder fessellos erscheint;", "tokens": ["Wann", "der", "Dul\u00b7der", "fes\u00b7sel\u00b7los", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wann er mit der angebornen St\u00e4rke", "tokens": ["Wann", "er", "mit", "der", "an\u00b7ge\u00b7bor\u00b7nen", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jugendlich Apollons Bogen spannt,", "tokens": ["Ju\u00b7gend\u00b7lich", "A\u00b7pol\u00b7lons", "Bo\u00b7gen", "spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Oder r\u00fcstig zu Athenens Werke", "tokens": ["O\u00b7der", "r\u00fcs\u00b7tig", "zu", "A\u00b7the\u00b7nens", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NE", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Unter der \u00c4gide sich ermannt.", "tokens": ["Un\u00b7ter", "der", "\u00c4\u00b7gi\u00b7de", "sich", "er\u00b7mannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ha, dein Freund, einst mehr als halb verloren,", "tokens": ["Ha", ",", "dein", "Freund", ",", "einst", "mehr", "als", "halb", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "ADV", "PIAT", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keck verh\u00f6hnt von schn\u00f6dem \u00dcbermut,", "tokens": ["Keck", "ver\u00b7h\u00f6hnt", "von", "schn\u00f6\u00b7dem", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "War zum lahmen Schw\u00e4chling nicht geboren;", "tokens": ["War", "zum", "lah\u00b7men", "Schw\u00e4ch\u00b7ling", "nicht", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihn durchflo\u00df kein tr\u00e4ges feiges Blut.", "tokens": ["Ihn", "durch\u00b7flo\u00df", "kein", "tr\u00e4\u00b7ges", "fei\u00b7ges", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Das bezeugen ihm des Pindus W\u00fcrden,", "tokens": ["Das", "be\u00b7zeu\u00b7gen", "ihm", "des", "Pin\u00b7dus", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die er in der Ohnmacht noch erwarb,", "tokens": ["Die", "er", "in", "der", "Ohn\u00b7macht", "noch", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und die Kraft, die unter allen B\u00fcrden", "tokens": ["Und", "die", "Kraft", ",", "die", "un\u00b7ter", "al\u00b7len", "B\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nicht in zwanzig Jahren ganz erstarb.", "tokens": ["Nicht", "in", "zwan\u00b7zig", "Jah\u00b7ren", "ganz", "er\u00b7starb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "CARD", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Heil ihm! Leichter f\u00fchlt er schon die Glieder;", "tokens": ["Heil", "ihm", "!", "Leich\u00b7ter", "f\u00fchlt", "er", "schon", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Genius, der in ihm strebt,", "tokens": ["Und", "der", "Ge\u00b7nius", ",", "der", "in", "ihm", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00fcttelt freier, st\u00e4rker das Gefieder,", "tokens": ["Sch\u00fct\u00b7telt", "frei\u00b7er", ",", "st\u00e4r\u00b7ker", "das", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das dem schweren Nebel ihn enthebt.", "tokens": ["Das", "dem", "schwe\u00b7ren", "Ne\u00b7bel", "ihn", "ent\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Erde, dich mit allen deinen Bergen,", "tokens": ["Er\u00b7de", ",", "dich", "mit", "al\u00b7len", "dei\u00b7nen", "Ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Allem lastenden Metall darin,", "tokens": ["Al\u00b7lem", "las\u00b7ten\u00b7den", "Me\u00b7tall", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Allen Riesen drauf und allen Zwergen,", "tokens": ["Al\u00b7len", "Rie\u00b7sen", "drauf", "und", "al\u00b7len", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Haucht er bald, wie Flaum, vor sich dahin.", "tokens": ["Haucht", "er", "bald", ",", "wie", "Flaum", ",", "vor", "sich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWAV", "NN", "$,", "APPR", "PRF", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Edle Rache beut er dann der Schande,", "tokens": ["Ed\u00b7le", "Ra\u00b7che", "beut", "er", "dann", "der", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die er \u00fcber sein Verschulden trug,", "tokens": ["Die", "er", "\u00fc\u00b7ber", "sein", "Ver\u00b7schul\u00b7den", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seit der Hypochonder dumpfe Bande", "tokens": ["Seit", "der", "Hy\u00b7po\u00b7chon\u00b7der", "dump\u00b7fe", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um die rein gestimmten Nerven schlug,", "tokens": ["Um", "die", "rein", "ge\u00b7stimm\u00b7ten", "Ner\u00b7ven", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wann es heller um der Wahrheit Seher,", "tokens": ["Wann", "es", "hel\u00b7ler", "um", "der", "Wahr\u00b7heit", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "W\u00e4rmer um der Sch\u00f6nheit Pfleger tagt,", "tokens": ["W\u00e4r\u00b7mer", "um", "der", "Sch\u00f6n\u00b7heit", "Pfle\u00b7ger", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und er glorreich eines Hauptes h\u00f6her", "tokens": ["Und", "er", "glor\u00b7reich", "ei\u00b7nes", "Haup\u00b7tes", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als zehntausend Alltagsmenschen ragt.", "tokens": ["Als", "zehn\u00b7tau\u00b7send", "All\u00b7tags\u00b7men\u00b7schen", "ragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Mag es Riese dann und Drache wagen,", "tokens": ["Mag", "es", "Rie\u00b7se", "dann", "und", "Dra\u00b7che", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADV", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gegen ihn zum Kampf heran zu gehn!", "tokens": ["Ge\u00b7gen", "ihn", "zum", "Kampf", "he\u00b7ran", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mag das Gl\u00fcck ihn auf den Armen tragen,", "tokens": ["Mag", "das", "Gl\u00fcck", "ihn", "auf", "den", "Ar\u00b7men", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder Er auf eignen F\u00fc\u00dfen stehn!", "tokens": ["O\u00b7der", "Er", "auf", "eig\u00b7nen", "F\u00fc\u00b7\u00dfen", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Neu ger\u00fcstet mit den G\u00f6tterwaffen,", "tokens": ["Neu", "ge\u00b7r\u00fcs\u00b7tet", "mit", "den", "G\u00f6t\u00b7ter\u00b7waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die er mit gest\u00e4hltem Arme f\u00fchrt,", "tokens": ["Die", "er", "mit", "ge\u00b7st\u00e4hl\u00b7tem", "Ar\u00b7me", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wird er sich nach Heldenrecht verschaffen,", "tokens": ["Wird", "er", "sich", "nach", "Hel\u00b7den\u00b7recht", "ver\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was sein Wunsch bedarf und ihm geb\u00fchrt. \u2013", "tokens": ["Was", "sein", "Wunsch", "be\u00b7darf", "und", "ihm", "ge\u00b7b\u00fchrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Herr des Lebens, willst du mich erhalten,", "tokens": ["Herr", "des", "Le\u00b7bens", ",", "willst", "du", "mich", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O so gib nur Eins, \u2013 Gesundheit mir!", "tokens": ["O", "so", "gib", "nur", "Eins", ",", "\u2013", "Ge\u00b7sund\u00b7heit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "ADV", "NN", "$,", "$(", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dankend will ich dir die H\u00e4nde falten,", "tokens": ["Dan\u00b7kend", "will", "ich", "dir", "die", "H\u00e4n\u00b7de", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber bitten weiter nichts von dir.", "tokens": ["A\u00b7ber", "bit\u00b7ten", "wei\u00b7ter", "nichts", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00fchn durch Klippen, Strudel, Ungeheuer", "tokens": ["K\u00fchn", "durch", "Klip\u00b7pen", ",", "Stru\u00b7del", ",", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lenk' ich, allgenugsam mir, alsdann", "tokens": ["Lenk'", "ich", ",", "all\u00b7ge\u00b7nug\u00b7sam", "mir", ",", "als\u00b7dann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "PPER", "$,", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Auf des Lebens Ozean mein Steuer.", "tokens": ["Auf", "des", "Le\u00b7bens", "O\u00b7ze\u00b7an", "mein", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Selbst sein Gott ist ein gesunder Mann.", "tokens": ["Selbst", "sein", "Gott", "ist", "ein", "ge\u00b7sun\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}