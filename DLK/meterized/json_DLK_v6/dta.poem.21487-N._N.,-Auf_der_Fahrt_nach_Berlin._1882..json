{"dta.poem.21487": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Fahrt nach Berlin.  \n 1882.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Von Westen kam ich, \u2014 schwerer Haideduft", "tokens": ["Von", "Wes\u00b7ten", "kam", "ich", ",", "schwe\u00b7rer", "Hai\u00b7de\u00b7duft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umflo\u00df mich noch, vor meinen Augen hoben", "tokens": ["Um\u00b7flo\u00df", "mich", "noch", ",", "vor", "mei\u00b7nen", "Au\u00b7gen", "ho\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich wei\u00dfe Birken in die klare Luft,", "tokens": ["Sich", "wei\u00b7\u00dfe", "Bir\u00b7ken", "in", "die", "kla\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von lauten Schw\u00e4rmen Kr\u00e4henvolks umstoben,", "tokens": ["Von", "lau\u00b7ten", "Schw\u00e4r\u00b7men", "Kr\u00e4\u00b7hen\u00b7volks", "um\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weit, weit die Haide, H\u00fcgel gelben Sand\u2019s,", "tokens": ["Weit", ",", "weit", "die", "Hai\u00b7de", ",", "H\u00fc\u00b7gel", "gel\u00b7ben", "San\u00b7d's", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "NE", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und binsen\u00fcberwachs\u2019ne Wasserkolke,", "tokens": ["Und", "bin\u00b7sen\u00b7\u00fcber\u00b7wachs'\u00b7ne", "Was\u00b7ser\u00b7kol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Fern zieht ein Sch\u00e4fer in des Sonnenbrand\u2019s", "tokens": ["Fern", "zieht", "ein", "Sch\u00e4\u00b7fer", "in", "des", "Son\u00b7nen\u00b7bran\u00b7d's"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Braungl\u00fchendem Reich vertr\u00e4umt mit seinem Volke.", "tokens": ["Braun\u00b7gl\u00fc\u00b7hen\u00b7dem", "Reich", "ver\u00b7tr\u00e4umt", "mit", "sei\u00b7nem", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von Westen kam ich und mein Geist umspann", "tokens": ["Von", "Wes\u00b7ten", "kam", "ich", "und", "mein", "Geist", "um\u00b7spann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weichm\u00fcthig rasch entschwund\u2019ne Jugendtage,", "tokens": ["Weich\u00b7m\u00fct\u00b7hig", "rasch", "ent\u00b7schwun\u00b7d'\u00b7ne", "Ju\u00b7gend\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "++-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "War\u2019s eine Thr\u00e4ne, die vom Aug\u2019 mir rann,", "tokens": ["Wa\u00b7r's", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "die", "vom", "Aug'", "mir", "rann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Klang\u2019s von dem Mund wie sehnsuchtsbange Klage? \u2026", "tokens": ["Klang's", "von", "dem", "Mund", "wie", "sehn\u00b7suchts\u00b7ban\u00b7ge", "Kla\u00b7ge", "?", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "$.", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Von Westen kam ich und mein Geist entflog", "tokens": ["Von", "Wes\u00b7ten", "kam", "ich", "und", "mein", "Geist", "ent\u00b7flog"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Voran und weit in dunkle Zukunftstunden \u2026", "tokens": ["Vo\u00b7ran", "und", "weit", "in", "dunk\u00b7le", "Zu\u00b7kunft\u00b7stun\u00b7den", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wohl hob er m\u00e4chtig sich, sein Flug war hoch,", "tokens": ["Wohl", "hob", "er", "m\u00e4ch\u00b7tig", "sich", ",", "sein", "Flug", "war", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Schlachten sah er, Drang und blut\u2019ge Wunden.", "tokens": ["Und", "Schlach\u00b7ten", "sah", "er", ",", "Drang", "und", "blut'\u00b7ge", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vorbei die Spiele, durch den Nebelschwall", "tokens": ["Vor\u00b7bei", "die", "Spie\u00b7le", ",", "durch", "den", "Ne\u00b7bel\u00b7schwall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des grauenden Septembermorgens jagen", "tokens": ["Des", "grau\u00b7en\u00b7den", "Sep\u00b7tem\u00b7ber\u00b7mor\u00b7gens", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Des Zuges R\u00e4der, und vom dumpfen Schall", "tokens": ["Des", "Zu\u00b7ges", "R\u00e4\u00b7der", ",", "und", "vom", "dum\u00b7pfen", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "St\u00f6hnt, dr\u00f6hnt und saust\u2019s im engen Eisenwagen \u2026", "tokens": ["St\u00f6hnt", ",", "dr\u00f6hnt", "und", "saust's", "im", "en\u00b7gen", "Ei\u00b7sen\u00b7wa\u00b7gen", "\u2026"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerzauste Wolken, winddurchw\u00fchlter Wald", "tokens": ["Zer\u00b7zaus\u00b7te", "Wol\u00b7ken", ",", "wind\u00b7durch\u00b7w\u00fchl\u00b7ter", "Wald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und braune Felsen schie\u00dfen wirr vor\u00fcber,", "tokens": ["Und", "brau\u00b7ne", "Fel\u00b7sen", "schie\u00b7\u00dfen", "wirr", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dort graut die Havel, und das Wasser schwallt,", "tokens": ["Dort", "graut", "die", "Ha\u00b7vel", ",", "und", "das", "Was\u00b7ser", "schwallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Br\u00fccke, hei! dumpf braust der Zug hin\u00fcber.", "tokens": ["Die", "Br\u00fc\u00b7cke", ",", "hei", "!", "dumpf", "braust", "der", "Zug", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Fenster auf! Dort dr\u00fcben liegt Berlin!", "tokens": ["Die", "Fens\u00b7ter", "auf", "!", "Dort", "dr\u00fc\u00b7ben", "liegt", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dampf wallt empor und Qualm, in schwarzen Schleiern", "tokens": ["Dampf", "wallt", "em\u00b7por", "und", "Qualm", ",", "in", "schwar\u00b7zen", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "H\u00e4ngt tief und steif die Wolke dr\u00fcber hin,", "tokens": ["H\u00e4ngt", "tief", "und", "steif", "die", "Wol\u00b7ke", "dr\u00fc\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die bleiche Luft dr\u00fcckt schwer und liegt wie bleiern \u2026", "tokens": ["Die", "blei\u00b7che", "Luft", "dr\u00fcckt", "schwer", "und", "liegt", "wie", "blei\u00b7ern", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "KOKOM", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Flammenheerd darunter \u2014 ein Vulkan,", "tokens": ["Ein", "Flam\u00b7men\u00b7heerd", "da\u00b7run\u00b7ter", "ein", "Vul\u00b7kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$(", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Millionen Feuerbr\u00e4nden lodernd, \u2026", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "Feu\u00b7er\u00b7br\u00e4n\u00b7den", "lo\u00b7dernd", ",", "\u2026"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ein Paradies, ein s\u00fc\u00dfes Kanaan, \u2014", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies", ",", "ein", "s\u00fc\u00b7\u00dfes", "Ka\u00b7naan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ein H\u00f6llenreich und Schatten bleich vermodernd.", "tokens": ["Ein", "H\u00f6l\u00b7len\u00b7reich", "und", "Schat\u00b7ten", "bleich", "ver\u00b7mo\u00b7dernd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hindonnernd rollt der Zug! Es saust die Luft,", "tokens": ["Hin\u00b7don\u00b7nernd", "rollt", "der", "Zug", "!", "Es", "saust", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein anderer rast dumpfrasselnd risch vor\u00fcber,", "tokens": ["Ein", "an\u00b7de\u00b7rer", "rast", "dum\u00b7pf\u00b7ras\u00b7selnd", "risch", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fabriken rauchgeschw\u00e4rzt, im Wasserduft", "tokens": ["Fab\u00b7ri\u00b7ken", "rauch\u00b7ge\u00b7schw\u00e4rzt", ",", "im", "Was\u00b7ser\u00b7duft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Gl\u00e4nzt Flamm\u2019 um Flamme, d\u00fcster, tr\u00fcb\u2019 und tr\u00fcber,", "tokens": ["Gl\u00e4nzt", "Flamm'", "um", "Flam\u00b7me", ",", "d\u00fcs\u00b7ter", ",", "tr\u00fcb'", "und", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Engbr\u00fcst\u2019ge H\u00e4user, Fenster schmal und klein,", "tokens": ["Eng\u00b7br\u00fcst'\u00b7ge", "H\u00e4u\u00b7ser", ",", "Fens\u00b7ter", "schmal", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bald braust es dumpf durch dunkle Br\u00fcckenbogen,", "tokens": ["Bald", "braust", "es", "dumpf", "durch", "dunk\u00b7le", "Br\u00fc\u00b7cken\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald blitzt es unter uns wie grauer Wasserschein,", "tokens": ["Bald", "blitzt", "es", "un\u00b7ter", "uns", "wie", "grau\u00b7er", "Was\u00b7ser\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unter K\u00e4hnen wandeln m\u00fcd\u2019 die Wogen.", "tokens": ["Und", "un\u00b7ter", "K\u00e4h\u00b7nen", "wan\u00b7deln", "m\u00fcd'", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vorbei, vor\u00fcber! und ein geller Pfiff!", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7\u00fc\u00b7ber", "!", "und", "ein", "gel\u00b7ler", "Pfiff", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$.", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wei\u00df fliegt der Dampf, \u2026 ein Knirschen an den Schienen!", "tokens": ["Wei\u00df", "fliegt", "der", "Dampf", ",", "\u2026", "ein", "Knir\u00b7schen", "an", "den", "Schie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bremse st\u00f6hnt laut unter starkem Griff \u2026", "tokens": ["Die", "Brem\u00b7se", "st\u00f6hnt", "laut", "un\u00b7ter", "star\u00b7kem", "Griff", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Langsamer nun! Es gl\u00e4nzt in Aller Mienen!", "tokens": ["Lang\u00b7sa\u00b7mer", "nun", "!", "Es", "gl\u00e4nzt", "in", "Al\u00b7ler", "Mie\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Glashallen \u00fcber uns, rings Menschenwirr\u2019n, \u2026", "tokens": ["Glas\u00b7hal\u00b7len", "\u00fc\u00b7ber", "uns", ",", "rings", "Men\u00b7schen\u00b7wirr'n", ",", "\u2026"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADV", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Halt! Und \u201eBerlin!\u201c Hinaus aus engem Wagen!", "tokens": ["Halt", 