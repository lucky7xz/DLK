{"textgrid.poem.52304": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbja, Kinder, ja!\u00ab seggt K\u00f6ster Suhr,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.57", "et:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbja, Kinder, ja!\u00ab seggt K\u00f6ster Suhr,", "tokens": ["\u00bb", "ja", ",", "Kin\u00b7der", ",", "ja", "!", "\u00ab", "seggt", "K\u00f6s\u00b7ter", "Suhr", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$,", "ADV", "$.", "$(", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbja, Kinder, ja! Glaubt mich das nur!", "tokens": ["\u00bb", "ja", ",", "Kin\u00b7der", ",", "ja", "!", "Glaubt", "mich", "das", "nur", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$,", "ADV", "$.", "VVFIN", "PPER", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht, uns're Welt, das is 'ne Welt,", "tokens": ["Seht", ",", "un\u00b7s'\u00b7re", "Welt", ",", "das", "is", "'ne", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "FM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie's nahrens eine gibt hier in der Welt,", "tokens": ["Wie's", "nah\u00b7rens", "ei\u00b7ne", "gibt", "hier", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich w\u00e4r' schon weit herumgekommen", "tokens": ["Ich", "w\u00e4r'", "schon", "weit", "her\u00b7um\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meine Wanderschaft, als ich noch Schneider w\u00e4r,", "tokens": ["Auf", "mei\u00b7ne", "Wan\u00b7der\u00b7schaft", ",", "als", "ich", "noch", "Schnei\u00b7der", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch h\u00e4tt' ich niemals nich vernommen,", "tokens": ["Doch", "h\u00e4tt'", "ich", "nie\u00b7mals", "nich", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df's eine bess're gebe mehr;", "tokens": ["Da\u00df's", "ei\u00b7ne", "bess'\u00b7re", "ge\u00b7be", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das hei\u00dft, den Himmel ausgenommen,", "tokens": ["Das", "hei\u00dft", ",", "den", "Him\u00b7mel", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das kann jedwederein insehn.", "tokens": ["Das", "kann", "jed\u00b7we\u00b7de\u00b7rein", "in\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ne, unsere Welt und all die Sachen,", "tokens": ["Ne", ",", "un\u00b7se\u00b7re", "Welt", "und", "all", "die", "Sa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "PIAT", "ART", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Die in ihr s\u00fcnd, die s\u00fcnd so sch\u00f6n,", "tokens": ["Die", "in", "ihr", "s\u00fcnd", ",", "die", "s\u00fcnd", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJD", "$,", "PRELS", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df ich sie selbst nicht k\u00f6nnte besser machen.", "tokens": ["Da\u00df", "ich", "sie", "selbst", "nicht", "k\u00f6nn\u00b7te", "bes\u00b7ser", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die ganze Welt is wunderbor", "tokens": ["Die", "gan\u00b7ze", "Welt", "is", "wun\u00b7der\u00b7bor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Un klug un hellschen weis' inricht,", "tokens": ["Un", "klug", "un", "hell\u00b7schen", "weis'", "in\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "FM", "VVINF", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Un eigentlich is nicks nich slicht.", "tokens": ["Un", "ei\u00b7gent\u00b7lich", "is", "nicks", "nich", "slicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "FM", "FM", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das stimmet allens up en Hor,", "tokens": ["Das", "stim\u00b7met", "al\u00b7lens", "up", "en", "Hor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Un weislich is dat ausgesunn'n", "tokens": ["Un", "weis\u00b7lich", "is", "dat", "aus\u00b7ge\u00b7sunn'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "FM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un stimmt genau up Stick un Stun'n.", "tokens": ["Un", "stimmt", "ge\u00b7nau", "up", "Stick", "un", "Stun'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Gott schuf den Menschen un den Affen;", "tokens": ["Gott", "schuf", "den", "Men\u00b7schen", "un", "den", "Af\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Worum hat er denn woll das Jahr erschaffen?", "tokens": ["Wo\u00b7rum", "hat", "er", "denn", "woll", "das", "Jahr", "er\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Wer wei\u00df't? Besinnt euch noch en beten! \u2013", "tokens": ["Wer", "wei\u00df't", "?", "Be\u00b7sinnt", "euch", "noch", "en", "be\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "FM", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ihr dummen Jungs, ihr wi\u00dft das nicht? \u2013", "tokens": ["Ihr", "dum\u00b7men", "Jungs", ",", "ihr", "wi\u00dft", "das", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PDS", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Seht! Das is dorum so inricht,", "tokens": ["Seht", "!", "Das", "is", "do\u00b7rum", "so", "in\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "FM", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Da\u00df jeder Knecht un jedes M\u00e4ten", "tokens": ["Da\u00df", "je\u00b7der", "Knecht", "un", "je\u00b7des", "M\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "FM", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zu rechter Zeit sein Jahrlohn krigt", "tokens": ["Zu", "rech\u00b7ter", "Zeit", "sein", "Jahr\u00b7lohn", "krigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un unserein sein bitschen Geld,", "tokens": ["Un", "un\u00b7ser\u00b7ein", "sein", "bit\u00b7schen", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df doch die Kinder, wenn der Snee denn f\u00f6llt,", "tokens": ["Da\u00df", "doch", "die", "Kin\u00b7der", ",", "wenn", "der", "Snee", "denn", "f\u00f6llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Nich 's Winters laufen brauchen barst;", "tokens": ["Nich", "'s", "Win\u00b7ters", "lau\u00b7fen", "brau\u00b7chen", "barst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "NN", "VVINF", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.30": {"text": "Un denn das Korn auch auf'n Harst. \u2013", "tokens": ["Un", "denn", "das", "Korn", "auch", "auf'n", "Harst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ART", "NN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Un denn die Monat! Seht, wie wunderbor!", "tokens": ["Un", "denn", "die", "Mo\u00b7nat", "!", "Seht", ",", "wie", "wun\u00b7der\u00b7bor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Zw\u00f6lf Monat hat ein jedes Johr,", "tokens": ["Zw\u00f6lf", "Mo\u00b7nat", "hat", "ein", "je\u00b7des", "Johr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Un jeder Monat drei\u00dfig Tag',", "tokens": ["Un", "je\u00b7der", "Mo\u00b7nat", "drei\u00b7\u00dfig", "Tag'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Un etliche noch einen mehr.", "tokens": ["Un", "et\u00b7li\u00b7che", "noch", "ei\u00b7nen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADV", "ART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Kl\u00e4nhamels Jehann J\u00f6ching, sag',", "tokens": ["Kl\u00e4n\u00b7ha\u00b7mels", "Je\u00b7hann", "J\u00f6\u00b7ching", ",", "sag'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wo k\u00f6mmt denn diese Sach' wohl her?\u00ab", "tokens": ["Wo", "k\u00f6mmt", "denn", "die\u00b7se", "Sach'", "wohl", "her", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PDAT", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "\u00bbvon die Karninkens k\u00f6mmt das her,", "tokens": ["\u00bb", "von", "die", "Kar\u00b7nin\u00b7kens", "k\u00f6mmt", "das", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wil di denn \u00fcmmer jungen dauhn.\u00ab", "tokens": ["Wil", "di", "denn", "\u00fcm\u00b7mer", "jun\u00b7gen", "dauhn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NE", "KON", "ADV", "ADJA", "VVINF", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.39": {"text": "\u00bbganz richtig! Seht, da k\u00f6nnt ihr's schaun!", "tokens": ["\u00bb", "ganz", "rich\u00b7tig", "!", "Seht", ",", "da", "k\u00f6nnt", "ih\u00b7r's", "schaun", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$.", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.40": {"text": "Von die Karninkens k\u00f6mmt das her.\u00ab", "tokens": ["Von", "die", "Kar\u00b7nin\u00b7kens", "k\u00f6mmt", "das", "her", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PDS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "\u00bbwo is das aber, Meister Suhr,", "tokens": ["\u00bb", "wo", "is", "das", "a\u00b7ber", ",", "Meis\u00b7ter", "Suhr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "FM", "ART", "ADV", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Da\u00df achtunzwanzig Dag' doch nur", "tokens": ["Da\u00df", "acht\u00b7unz\u00b7wan\u00b7zig", "Dag'", "doch", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Monat Februari hat?\u00ab", "tokens": ["Der", "Mo\u00b7nat", "Feb\u00b7ru\u00b7a\u00b7ri", "hat", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "\u00bbauch das ist ganz nach Gottes Rat", "tokens": ["\u00bb", "auch", "das", "ist", "ganz", "nach", "Got\u00b7tes", "Rat"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PDS", "VAFIN", "ADV", "APPR", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.45": {"text": "Verst\u00e4nnig un sehr klug inricht,", "tokens": ["Ver\u00b7st\u00e4n\u00b7nig", "un", "sehr", "klug", "in\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "FM", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Sonst g\u00fcng das mit den Klennje nicht.", "tokens": ["Sonst", "g\u00fcng", "das", "mit", "den", "Klenn\u00b7je", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Seht diesen kleinen Klennje an,", "tokens": ["Seht", "die\u00b7sen", "klei\u00b7nen", "Klenn\u00b7je", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Der hier an dieser Wand tut h\u00e4ngen.", "tokens": ["Der", "hier", "an", "die\u00b7ser", "Wand", "tut", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Was sollt denn woll der arme Mann,", "tokens": ["Was", "sollt", "denn", "woll", "der", "ar\u00b7me", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der ihn gemachet hat, anf\u00e4ngen,", "tokens": ["Der", "ihn", "ge\u00b7ma\u00b7chet", "hat", ",", "an\u00b7f\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Wenn wir nu in den Februwor", "tokens": ["Wenn", "wir", "nu", "in", "den", "Feb\u00b7ru\u00b7wor"], "token_info": ["word", "