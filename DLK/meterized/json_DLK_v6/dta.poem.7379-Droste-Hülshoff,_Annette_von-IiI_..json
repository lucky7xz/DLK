{"dta.poem.7379": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Zehn Jahre! \u2014 und Mancher der keck umher", "tokens": ["Zehn", "Jah\u00b7re", "!", "und", "Man\u00b7cher", "der", "keck", "um\u00b7her"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "$(", "KON", "NN", "ART", "ADJD", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die funkelnden Blicke geschossen,", "tokens": ["Die", "fun\u00b7keln\u00b7den", "Bli\u00b7cke", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der schl\u00e4gt sie heute zu Boden schwer,", "tokens": ["Der", "schl\u00e4gt", "sie", "heu\u00b7te", "zu", "Bo\u00b7den", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Mancher hat sie geschlossen.", "tokens": ["Und", "Man\u00b7cher", "hat", "sie", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Am Hafendamme geht eine Frau,", "tokens": ["Am", "Ha\u00b7fen\u00b7dam\u00b7me", "geht", "ei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u2014 Mich d\u00fcnkt, wir m\u00fcssen sie kennen,", "tokens": ["Mich", "d\u00fcnkt", ",", "wir", "m\u00fcs\u00b7sen", "sie", "ken\u00b7nen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Ihr Haar einst schwarz, nun schillerndes Grau,", "tokens": ["Ihr", "Haar", "einst", "schwarz", ",", "nun", "schil\u00b7lern\u00b7des", "Grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und hohl die Wangen ihr brennen.", "tokens": ["Und", "hohl", "die", "Wan\u00b7gen", "ihr", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Topfe tr\u00e4gt sie den Honigwab,", "tokens": ["Im", "Top\u00b7fe", "tr\u00e4gt", "sie", "den", "Ho\u00b7nig\u00b7wab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zergehend in Julius-Hitze;", "tokens": ["Zer\u00b7ge\u00b7hend", "in", "Ju\u00b7li\u00b7us\u00b7Hit\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Tr\u00e4gerin trocknet den Schwei\u00df sich ab,", "tokens": ["Die", "Tr\u00e4\u00b7ge\u00b7rin", "trock\u00b7net", "den", "Schwei\u00df", "sich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und ruft dem hinkenden Spitze.", "tokens": ["Und", "ruft", "dem", "hin\u00b7ken\u00b7den", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der sie bestellte, den Schiffspatron,", "tokens": ["Der", "sie", "be\u00b7stell\u00b7te", ",", "den", "Schiffs\u00b7pat\u00b7ron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sieht \u00fcber die Planke sie kommen;", "tokens": ["Sieht", "\u00fc\u00b7ber", "die", "Plan\u00b7ke", "sie", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Wird er ihr k\u00fcmmern den kargen Lohn?", "tokens": ["Wird", "er", "ihr", "k\u00fcm\u00b7mern", "den", "kar\u00b7gen", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Gertrude denkt es beklommen.", "tokens": ["Ger\u00b7tru\u00b7de", "denkt", "es", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch nein, \u2014 wo sich die Matrosen geschaart,", "tokens": ["Doch", "nein", ",", "wo", "sich", "die", "Mat\u00b7ro\u00b7sen", "ge\u00b7schaart", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "$(", "PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Strande sieht sie ihn schreiten,", "tokens": ["Zum", "Stran\u00b7de", "sieht", "sie", "ihn", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sch\u00fcttelt das Haupt, er streicht den Bart,", "tokens": ["Er", "sch\u00fct\u00b7telt", "das", "Haupt", ",", "er", "streicht", "den", "Bart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und scheint auf die Welle zu deuten.", "tokens": ["Und", "scheint", "auf", "die", "Wel\u00b7le", "zu", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und schau den Spitz! er schnuppert am Grund \u2014", "tokens": ["Und", "schau", "den", "Spitz", "!", "er", "schnup\u00b7pert", "am", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u201ewas suchst du denn in den Gleisen?", "tokens": ["\u201e", "was", "suchst", "du", "denn", "in", "den", "Glei\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Fidel, Fidel!\u201c fort strauchelt der Hund,", "tokens": ["Fi\u00b7del", ",", "Fi\u00b7del", "!", "\u201c", "fort", "strau\u00b7chelt", "der", "Hund", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "$(", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und heulet wie W\u00f6lfe im Eisen.", "tokens": ["Und", "heu\u00b7let", "wie", "W\u00f6l\u00b7fe", "im", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Barmherziger Himmel! ihr wird so bang,", "tokens": ["Barm\u00b7her\u00b7zi\u00b7ger", "Him\u00b7mel", "!", "ihr", "wird", "so", "bang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie watet im brennenden Sande,", "tokens": ["Sie", "wa\u00b7tet", "im", "bren\u00b7nen\u00b7den", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wieder erhebt sich so hohl und lang", "tokens": ["Und", "wie\u00b7der", "er\u00b7hebt", "sich", "so", "hohl", "und", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des Hundes Geheul vom Strande.", "tokens": ["Des", "Hun\u00b7des", "Ge\u00b7heul", "vom", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O Gott, eine triefende Leich' im Kies,", "tokens": ["O", "Gott", ",", "ei\u00b7ne", "trie\u00b7fen\u00b7de", "Leich'", "im", "Kies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Eine Leich' mit dem Auge des Stieres!", "tokens": ["Ei\u00b7ne", "Leich'", "mit", "dem", "Au\u00b7ge", "des", "Stie\u00b7res", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und dr\u00fcber kreucht das zottige Vlies", "tokens": ["Und", "dr\u00fc\u00b7ber", "kreucht", "das", "zot\u00b7ti\u00b7ge", "Vlies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Des lahmen wimmernden Thieres.", "tokens": ["Des", "lah\u00b7men", "wim\u00b7mern\u00b7den", "Thie\u00b7res", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gertrude steht, sie starret herab,", "tokens": ["Ger\u00b7tru\u00b7de", "steht", ",", "sie", "star\u00b7ret", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit Blicken irrer und irrer,", "tokens": ["Mit", "Bli\u00b7cken", "ir\u00b7rer", "und", "ir\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "KON", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann beugt sie \u00fcber die Leiche hinab,", "tokens": ["Dann", "beugt", "sie", "\u00fc\u00b7ber", "die", "Lei\u00b7che", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit L\u00e4cheln wirrer und wirrer,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "wir\u00b7rer", "und", "wir\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "KON", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie wiegt das Haupt bald so bald so,", "tokens": ["Sie", "wiegt", "das", "Haupt", "bald", "so", "bald", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie fl\u00fcstert mit zuckendem Munde,", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "mit", "zu\u00b7cken\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und eh die zweite Minute entfloh,", "tokens": ["Und", "eh", "die", "zwei\u00b7te", "Mi\u00b7nu\u00b7te", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da liegt sie kniend am Grunde.", "tokens": ["Da", "liegt", "sie", "kni\u00b7end", "am", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Sie fa\u00dft der Todten geschwollene Hand,", "tokens": ["Sie", "fa\u00dft", "der", "Tod\u00b7ten", "ge\u00b7schwol\u00b7le\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Haar voll Muscheln und Tange,", "tokens": ["Ihr", "Haar", "voll", "Mu\u00b7scheln", "und", "Tan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie fa\u00dft ihr triefend zerlumptes Gewand,", "tokens": ["Sie", "fa\u00dft", "ihr", "trie\u00b7fend", "zer\u00b7lump\u00b7tes", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und s\u00e4ubert von Kiese die Wange;", "tokens": ["Und", "s\u00e4u\u00b7bert", "von", "Kie\u00b7se", "die", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dann sachte schiebt sie das Tuch zur\u00fcck,", "tokens": ["Dann", "sach\u00b7te", "schiebt", "sie", "das", "Tuch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Recht wo die Schultern sich runden,", "tokens": ["Recht", "wo", "die", "Schul\u00b7tern", "sich", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "So stier und bohrend verweilt ihr Blick,", "tokens": ["So", "stier", "und", "boh\u00b7rend", "ver\u00b7weilt", "ihr", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als habe sie Etwas gefunden.", "tokens": ["Als", "ha\u00b7be", "sie", "Et\u00b7was", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Nun zuckt sie auf, erhebt sich jach,", "tokens": ["Nun", "zuckt", "sie", "auf", ",", "er\u00b7hebt", "sich", "jach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6\u00dft ein wimmernd Gest\u00f6hne,", "tokens": ["Und", "st\u00f6\u00dft", "ein", "wim\u00b7mernd", "Ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Grad eben als der Matrose sprach:", "tokens": ["Grad", "e\u00b7ben", "als", "der", "Mat\u00b7ro\u00b7se", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.