{"dta.poem.5432": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Undancks Wust, und aus demselben ein Laster- und", "tokens": ["Des", "Un\u00b7dancks", "Wust", ",", "und", "aus", "dem\u00b7sel\u00b7ben", "ein", "Las\u00b7ter", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "APPR", "PDAT", "ART", "TRUNC", "KON"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "ein Ungl\u00fccks-Heer;", "tokens": ["ein", "Un\u00b7gl\u00fccks\u00b7Heer", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ach, so gewehnt euch, liebste Menschen, zur Lust, zum", "tokens": ["Ach", ",", "so", "ge\u00b7wehnt", "euch", ",", "liebs\u00b7te", "Men\u00b7schen", ",", "zur", "Lust", ",", "zum"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ADV", "VVPP", "PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "APPRART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Danck, je mehr und mehr!", "tokens": ["Danck", ",", "je", "mehr", "und", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn, stimmt mit hier empfangnen G\u00fctern ein frohes", "tokens": ["Denn", ",", "stimmt", "mit", "hier", "emp\u00b7fang\u00b7nen", "G\u00fc\u00b7tern", "ein", "fro\u00b7hes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "APPR", "ADV", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dancken \u00fcberein;", "tokens": ["Dan\u00b7cken", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wird GOtt und N\u00e4chster recht geliebet, du hier vergn\u00fcgt,", "tokens": ["Wird", "Gott", "und", "N\u00e4chs\u00b7ter", "recht", "ge\u00b7lie\u00b7bet", ",", "du", "hier", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "VVPP", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "dort seelig seyn.", "tokens": ["dort", "see\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}