{"dta.poem.2588": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. F.   C. P.   in K. u. J. A. E. K. Hochzeit/  \n den 9. Julii 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weg mit der Einsamkeit/ was soll der leere Stand?", "tokens": ["Weg", "mit", "der", "Ein\u00b7sam\u00b7keit", "/", "was", "soll", "der", "lee\u00b7re", "Stand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Kein Mensch lebt auf der Welt/ der sich kan selber lieben.", "tokens": ["Kein", "Mensch", "lebt", "auf", "der", "Welt", "/", "der", "sich", "kan", "sel\u00b7ber", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist das Weib erbaut aus einer Seiten-Rieben/", "tokens": ["Es", "ist", "das", "Weib", "er\u00b7baut", "aus", "ei\u00b7ner", "Sei\u00b7ten\u00b7Rie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Adam/ wie er nur erwachte/ fertig fand.", "tokens": ["Die", "A\u00b7dam", "/", "wie", "er", "nur", "er\u00b7wach\u00b7te", "/", "fer\u00b7tig", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "ADV", "VVFIN", "$(", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df war sein erster Schatz/ an dem er sich ergetzte/", "tokens": ["Di\u00df", "war", "sein", "ers\u00b7ter", "Schatz", "/", "an", "dem", "er", "sich", "er\u00b7getz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die er ein Paradis im Paradise sch\u00e4tzte.", "tokens": ["Die", "er", "ein", "Pa\u00b7ra\u00b7dis", "im", "Pa\u00b7ra\u00b7di\u00b7se", "sch\u00e4tz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er sah verwundernde der Augen Sternen an/", "tokens": ["Er", "sah", "ver\u00b7wun\u00b7dern\u00b7de", "der", "Au\u00b7gen", "Ster\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Lichter konten ihn mehr als die Fackeln blenden/", "tokens": ["Die", "Lich\u00b7ter", "kon\u00b7ten", "ihn", "mehr", "als", "die", "Fa\u00b7ckeln", "blen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So uns bey stiller Nacht der Monden pflegt zu senden/", "tokens": ["So", "uns", "bey", "stil\u00b7ler", "Nacht", "der", "Mon\u00b7den", "pflegt", "zu", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn er sein g\u00fcldnes Heer f\u00fchrt auf die blaue Bahn.", "tokens": ["Wenn", "er", "sein", "g\u00fcld\u00b7nes", "Heer", "f\u00fchrt", "auf", "die", "blau\u00b7e", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Strahlen zogen ihn/ die Blicke wurden Blitze/", "tokens": ["Die", "Strah\u00b7len", "zo\u00b7gen", "ihn", "/", "die", "Bli\u00b7cke", "wur\u00b7den", "Blit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und er empfand bey sich ein\u2019 ungemeine Hitze.", "tokens": ["Und", "er", "emp\u00b7fand", "bey", "sich", "ein'", "un\u00b7ge\u00b7mei\u00b7ne", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ob schon die gantze Welt zu seinen F\u00fcssen lag/", "tokens": ["Ob", "schon", "die", "gant\u00b7ze", "Welt", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Flora unter ihm den sch\u00f6nsten Teppicht machte/", "tokens": ["Und", "Flo\u00b7ra", "un\u00b7ter", "ihm", "den", "sch\u00f6ns\u00b7ten", "Tep\u00b7picht", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Feld gr\u00fcn/ wie Smaragd/ der Wald von Anmuth lachte/", "tokens": ["Das", "Feld", "gr\u00fcn", "/", "wie", "Sma\u00b7ragd", "/", "der", "Wald", "von", "An\u00b7muth", "lach\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "KOKOM", "NN", "$(", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Der Sonnen g\u00fcldnes Rad verl\u00e4ngerte den Tag/", "tokens": ["Der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Rad", "ver\u00b7l\u00e4n\u00b7ger\u00b7te", "den", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Lufft wie Demant hell/ die Brunnen wie Crystallen/", "tokens": ["Die", "Lufft", "wie", "De\u00b7mant", "hell", "/", "die", "Brun\u00b7nen", "wie", "Crys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "ADJD", "$(", "ART", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit welchem sich vermischt der Thon der Nachtigallen.", "tokens": ["Mit", "wel\u00b7chem", "sich", "ver\u00b7mischt", "der", "Thon", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So r\u00fchrt ihn doch nicht so das grosse Kunst-Geb\u00e4u/", "tokens": ["So", "r\u00fchrt", "ihn", "doch", "nicht", "so", "das", "gros\u00b7se", "Kunst\u00b7Ge\u00b7b\u00e4u", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als ihn die Lilien auf der Geliebten Wangen/", "tokens": ["Als", "ihn", "die", "Li\u00b7li\u00b7en", "auf", "der", "Ge\u00b7lieb\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So sind die Rosen nie im Fr\u00fchling aufgegangen/", "tokens": ["So", "sind", "die", "Ro\u00b7sen", "nie", "im", "Fr\u00fch\u00b7ling", "auf\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit solchen Blumen prangt nicht der besternte May/", "tokens": ["Mit", "sol\u00b7chen", "Blu\u00b7men", "prangt", "nicht", "der", "bes\u00b7tern\u00b7te", "May", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Als seiner Eva Mund/ der/ wenn er sich nur regte/", "tokens": ["Als", "sei\u00b7ner", "E\u00b7va", "Mund", "/", "der", "/", "wenn", "er", "sich", "nur", "reg\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$(", "ART", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den Zunder gr\u00f6\u00dfrer Glut in feine Seele legte.", "tokens": ["Den", "Zun\u00b7der", "gr\u00f6\u00df\u00b7rer", "Glut", "in", "fei\u00b7ne", "See\u00b7le", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er sah die Glieder an/ sein eigen Fleisch und Blut/", "tokens": ["Er", "sah", "die", "Glie\u00b7der", "an", "/", "sein", "ei\u00b7gen", "Fleisch", "und", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein solches Helffenbein/ das Indien nicht schicket/", "tokens": ["Ein", "sol\u00b7ches", "Helf\u00b7fen\u00b7bein", "/", "das", "In\u00b7di\u00b7en", "nicht", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da mit Saphiren war der Aderu Grund durchst\u00fccket/", "tokens": ["Da", "mit", "Sa\u00b7phi\u00b7ren", "war", "der", "A\u00b7de\u00b7ru", "Grund", "durch\u00b7st\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Er siht den Schnee der Brust/ da w\u00e4chset ihm der Muth.", "tokens": ["Er", "siht", "den", "Schnee", "der", "Brust", "/", "da", "w\u00e4ch\u00b7set", "ihm", "der", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df auf der weissen H\u00f6h Granaten-Knospen bl\u00fchen/", "tokens": ["Da\u00df", "auf", "der", "weis\u00b7sen", "H\u00f6h", "Gra\u00b7na\u00b7ten\u00b7Knos\u00b7pen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und die Rubinen die Narcissen \u00fcberziehen.", "tokens": ["Und", "die", "Ru\u00b7bi\u00b7nen", "die", "Nar\u00b7cis\u00b7sen", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Woll\u2019 ist nicht so zart/ die Seide nicht so weich/", "tokens": ["Die", "Woll'", "ist", "nicht", "so", "zart", "/", "die", "Sei\u00b7de", "nicht", "so", "weich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$(", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als der formirte Leib au\u00df schlechtem Thon und Erden.", "tokens": ["Als", "der", "for\u00b7mir\u00b7te", "Leib", "au\u00df", "schlech\u00b7tem", "Thon", "und", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es mu\u00df ihm jedes Glied zu Lust-Magneten werden/", "tokens": ["Es", "mu\u00df", "ihm", "je\u00b7des", "Glied", "zu", "Lust\u00b7Ma\u00b7gne\u00b7ten", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPR", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und sie ist K\u00f6nigin in seinem K\u00f6nigreich.", "tokens": ["Und", "sie", "ist", "K\u00f6\u00b7ni\u00b7gin", "in", "sei\u00b7nem", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er theilt mit ihr sein Hertz/ und gibt ihr zu verstehen/", "tokens": ["Er", "theilt", "mit", "ihr", "sein", "Hertz", "/", "und", "gibt", "ihr", "zu", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df diese W\u00fcrckung komm au\u00df den gestirnten H\u00f6hen.", "tokens": ["Da\u00df", "die\u00b7se", "W\u00fcr\u00b7ckung", "komm", "au\u00df", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie hatte kaum den Ku\u00df/ den heissen Ku\u00df geschmeckt/", "tokens": ["Sie", "hat\u00b7te", "kaum", "den", "Ku\u00df", "/", "den", "heis\u00b7sen", "Ku\u00df", "ge\u00b7schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dem sich kein Zimmet-Oel und Amber-Kuchen gleichet/", "tokens": ["Dem", "sich", "kein", "Zim\u00b7met\u00b7O\u00b7el", "und", "Am\u00b7ber\u00b7Ku\u00b7chen", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PIAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dem Ambrosiner-Marck und Safft des Nectars weichet/", "tokens": ["Dem", "Am\u00b7bro\u00b7si\u00b7ner\u00b7Ma\u00b7rck", "und", "Safft", "des", "Nec\u00b7tars", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "Als sie ihn gantz verliebt de\u00df Hertzens Grund entdeckt/", "tokens": ["Als", "sie", "ihn", "gantz", "ver\u00b7liebt", "de\u00df", "Hert\u00b7zens", "Grund", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-