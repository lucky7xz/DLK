{"textgrid.poem.58237": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "275. Suchen und Finden sind zwei Dinge", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft dich nicht zu sehr an Form und Weisen binden,", "tokens": ["Du", "mu\u00dft", "dich", "nicht", "zu", "sehr", "an", "Form", "und", "Wei\u00b7sen", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "PTKA", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man suchet Gott nicht stets, man mu\u00df ihn ja auch finden;", "tokens": ["Man", "su\u00b7chet", "Gott", "nicht", "stets", ",", "man", "mu\u00df", "ihn", "ja", "auch", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "PTKNEG", "ADV", "$,", "PIS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer noch im Suchen ist, der l\u00e4uft und wirket viel,", "tokens": ["Wer", "noch", "im", "Su\u00b7chen", "ist", ",", "der", "l\u00e4uft", "und", "wir\u00b7ket", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer ihn gefunden hat, genie\u00dfet und ist still.", "tokens": ["Wer", "ihn", "ge\u00b7fun\u00b7den", "hat", ",", "ge\u00b7nie\u00b7\u00dfet", "und", "ist", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}