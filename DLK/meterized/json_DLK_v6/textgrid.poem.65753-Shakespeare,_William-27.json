{"textgrid.poem.65753": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "27", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich eil' ins Bett, erm\u00fcdet von Beschwer,", "tokens": ["Ich", "eil'", "ins", "Bett", ",", "er\u00b7m\u00fc\u00b7det", "von", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur holden Ruhstatt weitgereister Glieder:", "tokens": ["Zur", "hol\u00b7den", "Ruh\u00b7statt", "weit\u00b7ge\u00b7reis\u00b7ter", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch auf den Weg macht sich das Haupt nunmehr,", "tokens": ["Doch", "auf", "den", "Weg", "macht", "sich", "das", "Haupt", "nun\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wach wird die Seele, sinkt der Leib darnieder.", "tokens": ["Wach", "wird", "die", "See\u00b7le", ",", "sinkt", "der", "Leib", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn jetzo suchen die Gedanken dich,", "tokens": ["Denn", "jet\u00b7zo", "su\u00b7chen", "die", "Ge\u00b7dan\u00b7ken", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus weiter Fern' auf frommer Pilgerschaft;", "tokens": ["Aus", "wei\u00b7ter", "Fern'", "auf", "from\u00b7mer", "Pil\u00b7ger\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weit offen halten Augenlider sich,", "tokens": ["Weit", "of\u00b7fen", "hal\u00b7ten", "Au\u00b7gen\u00b7li\u00b7der", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Ich blick' in Dunkel, wie ein Blinder gafft.", "tokens": ["Ich", "blick'", "in", "Dun\u00b7kel", ",", "wie", "ein", "Blin\u00b7der", "gafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nur meines Geistes Aug' einbildsamlich", "tokens": ["Nur", "mei\u00b7nes", "Geis\u00b7tes", "Aug'", "ein\u00b7bild\u00b7sam\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Stellt dein Phantom unsehenden Augen dar:", "tokens": ["Stellt", "dein", "Phan\u00b7tom", "un\u00b7se\u00b7hen\u00b7den", "Au\u00b7gen", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Dort h\u00e4ngt's in N\u00e4chten ein Juwel f\u00fcr mich,", "tokens": ["Dort", "h\u00e4ngt's", "in", "N\u00e4ch\u00b7ten", "ein", "Ju\u00b7wel", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Verkl\u00e4rt das alte Dunkel wunderbar.", "tokens": ["Ver\u00b7kl\u00e4rt", "das", "al\u00b7te", "Dun\u00b7kel", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sieh, wie am Tag den Leib, nachts das Gem\u00fct,", "tokens": ["Sieh", ",", "wie", "am", "Tag", "den", "Leib", ",", "nachts", "das", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Um dich und mich, ersehnte Ruhe flieht!", "tokens": ["Um", "dich", "und", "mich", ",", "er\u00b7sehn\u00b7te", "Ru\u00b7he", "flieht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "KON", "PPER", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}