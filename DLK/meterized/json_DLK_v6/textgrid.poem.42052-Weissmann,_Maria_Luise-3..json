{"textgrid.poem.42052": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber es h\u00e4ngt vor allem Fr\u00fchling", "tokens": ["A\u00b7ber", "es", "h\u00e4ngt", "vor", "al\u00b7lem", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIS", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein sanfter Schleier wie Herbst.", "tokens": ["Ein", "sanf\u00b7ter", "Schlei\u00b7er", "wie", "Herbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Oder wurden meine Augen grau?", "tokens": ["O\u00b7der", "wur\u00b7den", "mei\u00b7ne", "Au\u00b7gen", "grau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nie blendet mich der Tag.", "tokens": ["Nie", "blen\u00b7det", "mich", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ward ich der blassen Erde zart vertraut,", "tokens": ["Ward", "ich", "der", "blas\u00b7sen", "Er\u00b7de", "zart", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oh unsrer Liebe nahe Bitternis!", "tokens": ["Oh", "uns\u00b7rer", "Lie\u00b7be", "na\u00b7he", "Bit\u00b7ter\u00b7nis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einmal werd ich der tiefste Schatten sein,", "tokens": ["Ein\u00b7mal", "werd", "ich", "der", "tiefs\u00b7te", "Schat\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der sie befiel.", "tokens": ["Der", "sie", "be\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}