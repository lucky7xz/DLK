{"textgrid.poem.54111": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gesto\u00dfener Seufzer", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kreuzt mir die Lustjacht in der Badewanne?", "tokens": ["Kreuzt", "mir", "die", "Lust\u00b7jacht", "in", "der", "Ba\u00b7de\u00b7wan\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Knirscht mir das Auto auf dem gelben Kies?", "tokens": ["Knirscht", "mir", "das", "Au\u00b7to", "auf", "dem", "gel\u00b7ben", "Kies", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Br\u00e4unt mir das Ro\u00dfb\u00fcff in der Kupferpfanne?", "tokens": ["Br\u00e4unt", "mir", "das", "Ro\u00df\u00b7b\u00fcff", "in", "der", "Kup\u00b7fer\u00b7pfan\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blitzt mir am Hemd der Diamant-T\u00fcrkis?", "tokens": ["Blitzt", "mir", "am", "Hemd", "der", "Di\u00b7a\u00b7mant\u00b7T\u00fcr\u00b7kis", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Hin hauch ich einen Seufzer des Verzichts:", "tokens": ["Hin", "hauch", "ich", "ei\u00b7nen", "Seuf\u00b7zer", "des", "Ver\u00b7zichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ich brings zu nichts.", "tokens": ["ich", "brings", "zu", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, was das ist und wie ichs treibe . . .", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "das", "ist", "und", "wie", "ichs", "trei\u00b7be", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PDS", "VAFIN", "KON", "PWAV", "PIS", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich spare manchen vordatierten Scheck.", "tokens": ["Ich", "spa\u00b7re", "man\u00b7chen", "vor\u00b7da\u00b7tier\u00b7ten", "Scheck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dann naht Lottchen mit dem Lotterleibe,", "tokens": ["Und", "dann", "naht", "Lott\u00b7chen", "mit", "dem", "Lot\u00b7ter\u00b7lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und dann ist alles wieder weg.", "tokens": ["und", "dann", "ist", "al\u00b7les", "wie\u00b7der", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Infolge ihres Liebesunterrichts . . .", "tokens": ["In\u00b7fol\u00b7ge", "ih\u00b7res", "Lie\u00b7bes\u00b7un\u00b7ter\u00b7richts", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich brings zu nichts.", "tokens": ["Ich", "brings", "zu", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die andern h\u00e4ufen so Verm\u00f6gen auf Verm\u00f6gen.", "tokens": ["Die", "an\u00b7dern", "h\u00e4u\u00b7fen", "so", "Ver\u00b7m\u00f6\u00b7gen", "auf", "Ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern wandeln durch das Goldportal.", "tokens": ["Die", "an\u00b7dern", "wan\u00b7deln", "durch", "das", "Gold\u00b7por\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber kann mir nichts nach hinten legen;", "tokens": ["Ich", "a\u00b7ber", "kann", "mir", "nichts", "nach", "hin\u00b7ten", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PIS", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich hab noch nie \u2013 und m\u00f6chte auch einmal.", "tokens": ["ich", "hab", "noch", "nie", "\u2013", "und", "m\u00f6ch\u00b7te", "auch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$(", "KON", "VMFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Reichtum ist der Lohn des B\u00f6sewichts.", "tokens": ["Der", "Reich\u00b7tum", "ist", "der", "Lohn", "des", "B\u00f6\u00b7se\u00b7wichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich brings zu nichts.", "tokens": ["Ich", "brings", "zu", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "So lern doch endlich von den andern Knaben", "tokens": ["So", "lern", "doch", "end\u00b7lich", "von", "den", "an\u00b7dern", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die einzig brauchbare Philosophie:", "tokens": ["die", "ein\u00b7zig", "brauch\u00b7ba\u00b7re", "Phi\u00b7lo\u00b7so\u00b7phie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es g'n\u00fcgt nicht nur, Verh\u00e4ltnisse zu haben \u2013", "tokens": ["Es", "g'\u00b7n\u00fcgt", "nicht", "nur", ",", "Ver\u00b7h\u00e4lt\u00b7nis\u00b7se", "zu", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "PTKZU", "VAINF", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "sie leben alle \u00fcber sie.", "tokens": ["sie", "le\u00b7ben", "al\u00b7le", "\u00fc\u00b7ber", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trink aus der Nachbarin Champagnerglas!", "tokens": ["Trink", "aus", "der", "Nach\u00b7ba\u00b7rin", "Cham\u00b7pag\u00b7ner\u00b7glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Bleib schuldig Miete, Liebe, Arzt und Gas!", "tokens": ["Bleib", "schul\u00b7dig", "Mie\u00b7te", ",", "Lie\u00b7be", ",", "Arzt", "und", "Gas", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bezahl den Apfel \u2013 fri\u00df die Ananas!", "tokens": ["Be\u00b7zahl", "den", "Ap\u00b7fel", "\u2013", "fri\u00df", "die", "A\u00b7na\u00b7nas", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer also handelt, bringts zu was.", "tokens": ["Wer", "al\u00b7so", "han\u00b7delt", ",", "bringts", "zu", "was", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "PRELS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}