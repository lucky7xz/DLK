{"textgrid.poem.33129": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "27. Auf den ehrw\u00fcrdigen Greis, den Landes-Aeltesten von Schweinitz", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater, soll ich dich von denen Tempel-Stufen,", "tokens": ["Mein", "Va\u00b7ter", ",", "soll", "ich", "dich", "von", "de\u00b7nen", "Tem\u00b7pel\u00b7Stu\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PRF", "APPR", "PRELS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dein erfreuter Geist als im Triumph besteigt,", "tokens": ["Die", "dein", "er\u00b7freu\u00b7ter", "Geist", "als", "im", "Tri\u00b7umph", "be\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In diese Zeitlichkeit best\u00fcrzt zur\u00fckke rufen,", "tokens": ["In", "die\u00b7se", "Zeit\u00b7lich\u00b7keit", "be\u00b7st\u00fcrzt", "zu\u00b7r\u00fck\u00b7ke", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo du, gar Lebens-satt, das graue Haupt geneigt?", "tokens": ["Wo", "du", ",", "gar", "Le\u00b7bens\u00b7satt", ",", "das", "grau\u00b7e", "Haupt", "ge\u00b7neigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nein, nein! Doch rufe ich, auf die erhabnen Zinnen,", "tokens": ["Nein", ",", "nein", "!", "Doch", "ru\u00b7fe", "ich", ",", "auf", "die", "er\u00b7hab\u00b7nen", "Zin\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "KON", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo, bey des Lammes Stuhl, dein Lammes-Wesen steht,", "tokens": ["Wo", ",", "bey", "des", "Lam\u00b7mes", "Stuhl", ",", "dein", "Lam\u00b7mes\u00b7We\u00b7sen", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir nach: Du eilst zu fr\u00fch und auch zu sp\u00e4t von hinnen!", "tokens": ["Dir", "nach", ":", "Du", "eilst", "zu", "fr\u00fch", "und", "auch", "zu", "sp\u00e4t", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PTKA", "ADJD", "KON", "ADV", "PTKA", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir deinem Sohn zu fr\u00fch; der Sehnsucht viel zu sp\u00e4t.", "tokens": ["Mir", "dei\u00b7nem", "Sohn", "zu", "fr\u00fch", ";", "der", "Sehn\u00b7sucht", "viel", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKA", "ADJD", "$.", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist der erste Freund, den ich in diesem Lande,", "tokens": ["Du", "bist", "der", "ers\u00b7te", "Freund", ",", "den", "ich", "in", "die\u00b7sem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als einen edlen Knecht von unserm Herrn gegr\u00fc\u00dft:", "tokens": ["Als", "ei\u00b7nen", "ed\u00b7len", "Knecht", "von", "un\u00b7serm", "Herrn", "ge\u00b7gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du, eine rechte Zier dem adelichen Stande,", "tokens": ["Du", ",", "ei\u00b7ne", "rech\u00b7te", "Zier", "dem", "a\u00b7de\u00b7li\u00b7chen", "Stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihm, dem das Christenthum so wunder-seltsam ist.", "tokens": ["Ihm", ",", "dem", "das", "Chris\u00b7ten\u00b7thum", "so", "wun\u00b7der\u00b7selt\u00b7sam", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du warst schon Kammerherr bey Churf\u00fcrst Hans Georgen,", "tokens": ["Du", "warst", "schon", "Kam\u00b7mer\u00b7herr", "bey", "Chur\u00b7f\u00fcrst", "Hans", "Ge\u00b7or\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Hofmann ehemals, dann Landes-Aeltester;", "tokens": ["Ein", "Hof\u00b7mann", "e\u00b7he\u00b7mals", ",", "dann", "Lan\u00b7des\u00b7Ael\u00b7tes\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Es denkt das ganze Land noch deiner Vater-Sorgen,", "tokens": ["Es", "denkt", "das", "gan\u00b7ze", "Land", "noch", "dei\u00b7ner", "Va\u00b7ter\u00b7Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und rechnet deinen Ruhm der sp\u00e4ten Nachwelt her.", "tokens": ["Und", "rech\u00b7net", "dei\u00b7nen", "Ruhm", "der", "sp\u00e4\u00b7ten", "Nach\u00b7welt", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Worinnen stande nun dein dauerhaftes Gl\u00fckke?", "tokens": ["Wo\u00b7rin\u00b7nen", "stan\u00b7de", "nun", "dein", "dau\u00b7er\u00b7haf\u00b7tes", "Gl\u00fck\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht im bey Hofe seyn, du flohest bald nach Haus:", "tokens": ["Nicht", "im", "bey", "Ho\u00b7fe", "seyn", ",", "du", "flo\u00b7hest", "bald", "nach", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "APPR", "NN", "VAINF", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom Landes-Amte rief dein Alter dich zur\u00fckke:", "tokens": ["Vom", "Lan\u00b7des\u00b7Am\u00b7te", "rief", "dein", "Al\u00b7ter", "dich", "zu\u00b7r\u00fck\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht in der Ehe selbst, Du lebtest alles aus.", "tokens": ["Nicht", "in", "der", "E\u00b7he", "selbst", ",", "Du", "leb\u00b7test", "al\u00b7les", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Werke sinds allein, die sind dir nachgefahren;", "tokens": ["Die", "Wer\u00b7ke", "sinds", "al\u00b7lein", ",", "die", "sind", "dir", "nach\u00b7ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PRELS", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch durch Werke wird ja kein Verdienst geschafft,", "tokens": ["Je\u00b7doch", "durch", "Wer\u00b7ke", "wird", "ja", "kein", "Ver\u00b7dienst", "ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Glaube kan allein zur Seligkeit bewahren;", "tokens": ["Der", "Glau\u00b7be", "kan", "al\u00b7lein", "zur", "Se\u00b7lig\u00b7keit", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich meyne auch das Werk des Glaubens in der Kraft.", "tokens": ["Ich", "mey\u00b7ne", "auch", "das", "Werk", "des", "Glau\u00b7bens", "in", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wohlan, der Alten eins, vor unsers Lammes Throne,", "tokens": ["Wo\u00b7hlan", ",", "der", "Al\u00b7ten", "eins", ",", "vor", "un\u00b7sers", "Lam\u00b7mes", "Thro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "PIS", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh, liebes graues Haupt! geh hin, um auszuruhn:", "tokens": ["Geh", ",", "lie\u00b7bes", "grau\u00b7es", "Haupt", "!", "geh", "hin", ",", "um", "aus\u00b7zu\u00b7ruhn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "NN", "$.", "VVFIN", "PTKVZ", "$,", "KOUI", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geh, wirf, mit matter Hand, die angebotne Krone", "tokens": ["Geh", ",", "wirf", ",", "mit", "mat\u00b7ter", "Hand", ",", "die", "an\u00b7ge\u00b7bot\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem K\u00f6nige zu Fu\u00df: Entschlaf im Frieden nun.", "tokens": ["Dem", "K\u00f6\u00b7ni\u00b7ge", "zu", "Fu\u00df", ":", "Ent\u00b7schlaf", "im", "Frie\u00b7den", "nun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dein Angedenken soll in meinem Leben gr\u00fcnen,", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7ken", "soll", "in", "mei\u00b7nem", "Le\u00b7ben", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der treue Gottes-Knecht, mein lieber Rothe, wird", "tokens": ["Der", "treu\u00b7e", "Got\u00b7tes\u00b7Knecht", ",", "mein", "lie\u00b7ber", "Ro\u00b7the", ",", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir zur Erinnerung von deiner Liebe dienen,", "tokens": ["Mir", "zur", "E\u00b7rin\u00b7ne\u00b7rung", "von", "dei\u00b7ner", "Lie\u00b7be", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Er, deiner Enkelgen so treu erfundner Hirt.", "tokens": ["Er", ",", "dei\u00b7ner", "En\u00b7kel\u00b7gen", "so", "treu", "er\u00b7fund\u00b7ner", "Hirt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein Sohn und Tochter sind an deine Stelle kommen:", "tokens": ["Dein", "Sohn", "und", "Toch\u00b7ter", "sind", "an", "dei\u00b7ne", "Stel\u00b7le", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So v\u00e4terlich ich dich geehrt; so br\u00fcderlich", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "ich", "dich", "ge\u00b7ehrt", ";", "so", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVPP", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin ich in ihre Gunst und Freundschaft aufgenommen,", "tokens": ["Bin", "ich", "in", "ih\u00b7re", "Gunst", "und", "Freund\u00b7schaft", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Evangelio zu wandeln w\u00fcrdiglich.", "tokens": ["Dem", "E\u00b7van\u00b7ge\u00b7lio", "zu", "wan\u00b7deln", "w\u00fcr\u00b7dig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKZU", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.10": {"line.1": {"text": "Dein Einfalts-Wandel soll viel andre Seelen r\u00fchren,", "tokens": ["Dein", "Ein\u00b7falts\u00b7Wan\u00b7del", "soll", "viel", "and\u00b7re", "See\u00b7len", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Kinder gehn dieselbe Strasse mit;", "tokens": ["Und", "dei\u00b7ne", "Kin\u00b7der", "gehn", "die\u00b7sel\u00b7be", "Stras\u00b7se", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was will ich also noch viel panegyrisiren?", "tokens": ["Was", "will", "ich", "al\u00b7so", "noch", "viel", "pa\u00b7ne\u00b7gy\u00b7ri\u00b7si\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sohn bekennet dich dem Vater.", "tokens": ["Der", "Sohn", "be\u00b7ken\u00b7net", "dich", "dem", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}