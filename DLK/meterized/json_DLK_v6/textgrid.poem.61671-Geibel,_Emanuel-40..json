{"textgrid.poem.61671": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt bei schlimmer Zeit in engen Schranken", "tokens": ["Ihr", "habt", "bei", "schlim\u00b7mer", "Zeit", "in", "en\u00b7gen", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewahrt die Summe christlicher Gedanken;", "tokens": ["Be\u00b7wahrt", "die", "Sum\u00b7me", "christ\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch diese engen Schranken sind noch drum", "tokens": ["Doch", "die\u00b7se", "en\u00b7gen", "Schran\u00b7ken", "sind", "noch", "drum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "VAFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Kirche nicht und nicht das Christentum.", "tokens": ["Die", "Kir\u00b7che", "nicht", "und", "nicht", "das", "Chris\u00b7ten\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}