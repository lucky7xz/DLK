{"textgrid.poem.60006": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, Freund! in Deinem Sans Souci,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Freund! in Deinem Sans Souci,", "tokens": ["Ja", ",", "Freund", "!", "in", "Dei\u00b7nem", "Sans", "Sou\u00b7ci", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo, bey der Musen Harmonie,", "tokens": ["Wo", ",", "bey", "der", "Mu\u00b7sen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die finstere Philosophie,", "tokens": ["Die", "fins\u00b7te\u00b7re", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Lied und Scherz und Ku\u00df gew\u00f6hnet,", "tokens": ["An", "Lied", "und", "Scherz", "und", "Ku\u00df", "ge\u00b7w\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Huldg\u00f6ttinnen sich vers\u00f6hnet,", "tokens": ["Mit", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "sich", "ver\u00b7s\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo neben Dir Dein Amor sitzt,", "tokens": ["Wo", "ne\u00b7ben", "Dir", "Dein", "A\u00b7mor", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und spielend einen Plato schnitzt", "tokens": ["Und", "spie\u00b7lend", "ei\u00b7nen", "Pla\u00b7to", "schnitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da lassen Dich erhabne Freuden", "tokens": ["Da", "las\u00b7sen", "Dich", "er\u00b7hab\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Kein f\u00fcrstlich Sans Souci beneiden;", "tokens": ["Kein", "f\u00fcrst\u00b7lich", "Sans", "Sou\u00b7ci", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da ruft den unget\u00e4uschten Blick", "tokens": ["Da", "ruft", "den", "un\u00b7ge\u00b7t\u00e4uschten", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Von der Pal\u00e4ste stolzen Mauern", "tokens": ["Von", "der", "Pa\u00b7l\u00e4s\u00b7te", "stol\u00b7zen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Weisheit freundschaftlich zur\u00fcck,", "tokens": ["Die", "Weis\u00b7heit", "freund\u00b7schaft\u00b7lich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und lehrt Dich, K\u00f6nige bedauern.", "tokens": ["Und", "lehrt", "Dich", ",", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie scherzen nicht mit uns im Hain,", "tokens": ["Sie", "scher\u00b7zen", "nicht", "mit", "uns", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie ladet nicht der Rasen ein;", "tokens": ["Sie", "la\u00b7det", "nicht", "der", "Ra\u00b7sen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Kaum sehen sie das Veilchen bl\u00fchen,", "tokens": ["Kaum", "se\u00b7hen", "sie", "das", "Veil\u00b7chen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Sonne hinter Bergen gl\u00fchen,", "tokens": ["Die", "Son\u00b7ne", "hin\u00b7ter", "Ber\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Den H\u00fcgel, den Aurora malt,", "tokens": ["Den", "H\u00fc\u00b7gel", ",", "den", "Au\u00b7ro\u00b7ra", "malt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und wie der Mond auf Teiche stralt.", "tokens": ["Und", "wie", "der", "Mond", "auf", "Tei\u00b7che", "stralt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Kein Vogel singt f\u00fcr sie Ges\u00e4nge;", "tokens": ["Kein", "Vo\u00b7gel", "singt", "f\u00fcr", "sie", "Ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die kleine Philomele schweigt,", "tokens": ["Die", "klei\u00b7ne", "Phi\u00b7lo\u00b7me\u00b7le", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wenn sich in rauschendem Gepr\u00e4nge", "tokens": ["Wenn", "sich", "in", "rau\u00b7schen\u00b7dem", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der Herr von ihren W\u00e4ldern zeigt.", "tokens": ["Der", "Herr", "von", "ih\u00b7ren", "W\u00e4l\u00b7dern", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mit unterbrochnen T\u00f6nen steigt", "tokens": ["Mit", "un\u00b7ter\u00b7broch\u00b7nen", "T\u00f6\u00b7nen", "steigt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die Lerche, wo der Frohsinn weicht,", "tokens": ["Die", "Ler\u00b7che", ",", "wo", "der", "Froh\u00b7sinn", "weicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und bang, mit leisem Murmeln schleicht", "tokens": ["Und", "bang", ",", "mit", "lei\u00b7sem", "Mur\u00b7meln", "schleicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der ungegr\u00fc\u00dfte Bach vor\u00fcber.", "tokens": ["Der", "un\u00b7ge\u00b7gr\u00fc\u00df\u00b7te", "Bach", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Echo sagt erschrocken nach,", "tokens": ["Der", "E\u00b7cho", "sagt", "er\u00b7schro\u00b7cken", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Was ein Monarch im Purpur sprach,", "tokens": ["Was", "ein", "Mon\u00b7arch", "im", "Pur\u00b7pur", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Und h\u00f6rt des Hirten Stimme lieber.", "tokens": ["Und", "h\u00f6rt", "des", "Hir\u00b7ten", "Stim\u00b7me", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Uns, bester Gleim, uns liebt das Thal;", "tokens": ["Uns", ",", "bes\u00b7ter", "Gleim", ",", "uns", "liebt", "das", "Thal", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort, wo wir seine Rosen pfl\u00fccken,", "tokens": ["Dort", ",", "wo", "wir", "sei\u00b7ne", "Ro\u00b7sen", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und den gef\u00fcllten Becher schm\u00fccken,", "tokens": ["Und", "den", "ge\u00b7f\u00fcll\u00b7ten", "Be\u00b7cher", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verachten wir Lucullus Mahl.", "tokens": ["Ver\u00b7ach\u00b7ten", "wir", "Lu\u00b7cul\u00b7lus", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es trank aus goldenem Pokale", "tokens": ["Es", "trank", "aus", "gol\u00b7de\u00b7nem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur selten die Zufriedenheit;", "tokens": ["Nur", "sel\u00b7ten", "die", "Zu\u00b7frie\u00b7den\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur selten wohnt im Marmorsaale", "tokens": ["Nur", "sel\u00b7ten", "wohnt", "im", "Mar\u00b7mor\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gl\u00fcck der wahren Z\u00e4rtlichkeit.", "tokens": ["Das", "Gl\u00fcck", "der", "wah\u00b7ren", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr F\u00fcrsten! sah man, unter K\u00fcssen,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", "!", "sah", "man", ",", "un\u00b7ter", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PIS", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von euern Wangen Thr\u00e4nen flie\u00dfen?", "tokens": ["Von", "eu\u00b7ern", "Wan\u00b7gen", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "F\u00fcr uns als G\u00f6tter aufgestellt,", "tokens": ["F\u00fcr", "uns", "als", "G\u00f6t\u00b7ter", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vom Diadem das Haupt umwunden,", "tokens": ["Vom", "Di\u00b7a\u00b7dem", "das", "Haupt", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Was hilft euch eine ganze Welt,", "tokens": ["Was", "hilft", "euch", "ei\u00b7ne", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In der ihr keinen Freund gefunden?", "tokens": ["In", "der", "ihr", "kei\u00b7nen", "Freund", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur dann, wenn am verla\u00dfnen Herd", "tokens": ["Nur", "dann", ",", "wenn", "am", "ver\u00b7la\u00df\u00b7nen", "Herd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Unschuld ihre H\u00e4nde ringet,", "tokens": ["Die", "Un\u00b7schuld", "ih\u00b7re", "H\u00e4n\u00b7de", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zum Palast die Stimme dringet,", "tokens": ["Bis", "zum", "Pa\u00b7last", "die", "Stim\u00b7me", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch V\u00e4ter nennt und Schutz begehrt:", "tokens": ["Euch", "V\u00e4\u00b7ter", "nennt", "und", "Schutz", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann seyd ihr uns des Neides werth.", "tokens": ["Dann", "seyd", "ihr", "uns", "des", "Nei\u00b7des", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nein! von unz\u00e4hlbaren Sch\u00e4tzen", "tokens": ["Doch", "nein", "!", "von", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Raub der Bosheit zu ersetzen,", "tokens": ["Den", "Raub", "der", "Bos\u00b7heit", "zu", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist das ein himmlisches Erg\u00f6tzen,", "tokens": ["Ist", "das", "ein", "himm\u00b7li\u00b7sches", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist das der Tugend h\u00f6chster Ruhm?", "tokens": ["Ist", "das", "der", "Tu\u00b7gend", "h\u00f6chs\u00b7ter", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was wir, o Freund! der Armuth geben", "tokens": ["Was", "wir", ",", "o", "Freund", "!", "der", "Ar\u00b7muth", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "$,", "FM", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von unserm kleinen Eigenthum,", "tokens": ["Von", "un\u00b7serm", "klei\u00b7nen", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+