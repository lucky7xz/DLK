{"textgrid.poem.40748": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Ein Feenm\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schritt ein Dichter durch die Schattenhage,", "tokens": ["Schritt", "ein", "Dich\u00b7ter", "durch", "die", "Schat\u00b7ten\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An das Waldesherz legt' er die Frage:", "tokens": ["An", "das", "Wal\u00b7des\u00b7herz", "legt'", "er", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbwie du rauschest, Wald, gewitterbange,", "tokens": ["\u00bb", "wie", "du", "rau\u00b7schest", ",", "Wald", ",", "ge\u00b7wit\u00b7ter\u00b7ban\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVFIN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zittert mir die Seel' im Liederdrange!", "tokens": ["Zit\u00b7tert", "mir", "die", "Seel'", "im", "Lie\u00b7der\u00b7dran\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rauschest du nur deines Pflanzers Ehre,", "tokens": ["Rau\u00b7schest", "du", "nur", "dei\u00b7nes", "Pflan\u00b7zers", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder auch von seiner Aexte Schwere?", "tokens": ["O\u00b7der", "auch", "von", "sei\u00b7ner", "A\u00b7ex\u00b7te", "Schwe\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sei mein Lied die Lerche thaubesoldet,", "tokens": ["Sei", "mein", "Lied", "die", "Ler\u00b7che", "thau\u00b7be\u00b7sol\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Flaum und Sang von Sonnenschmelz vergoldet,", "tokens": ["Flaum", "und", "Sang", "von", "Son\u00b7nen\u00b7schmelz", "ver\u00b7gol\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Oder sei's der Aar im Wappenbild,", "tokens": ["O\u00b7der", "sei's", "der", "Aar", "im", "Wap\u00b7pen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "F\u00fchrend seines Landes Schwert und Schild?", "tokens": ["F\u00fch\u00b7rend", "sei\u00b7nes", "Lan\u00b7des", "Schwert", "und", "Schild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sei mein Lied die weiche Philomele,", "tokens": ["Sei", "mein", "Lied", "die", "wei\u00b7che", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Der die Liebe s\u00e4nftigt Herz und Kehle,", "tokens": ["Der", "die", "Lie\u00b7be", "s\u00e4nf\u00b7tigt", "Herz", "und", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Oder sei's der Falk' im raschen Sto\u00df,", "tokens": ["O\u00b7der", "sei's", "der", "Fal\u00b7k'", "im", "ra\u00b7schen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Den der Zorn verwandelt zum Gescho\u00df?", "tokens": ["Den", "der", "Zorn", "ver\u00b7wan\u00b7delt", "zum", "Ge\u00b7scho\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "F\u00fchr' ich nicht das eigne Lied zum Tode,", "tokens": ["F\u00fchr'", "ich", "nicht", "das", "eig\u00b7ne", "Lied", "zum", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wenn ich's n\u00e4hrte mit des Marktes Brode?", "tokens": ["Wenn", "ich's", "n\u00e4hr\u00b7te", "mit", "des", "Mark\u00b7tes", "Bro\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Stirbt des Ew'gen Kind nicht schon an T\u00f6nen,", "tokens": ["Stirbt", "des", "Ew'\u00b7gen", "Kind", "nicht", "schon", "an", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Die verst\u00e4ndlich nur der Stunde S\u00f6hnen?", "tokens": ["Die", "ver\u00b7st\u00e4nd\u00b7lich", "nur", "der", "Stun\u00b7de", "S\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Sing' ein Dichter, wie die Biene sticht,", "tokens": ["Sing'", "ein", "Dich\u00b7ter", ",", "wie", "die", "Bie\u00b7ne", "sticht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Deren Leben mit dem Stachel bricht?\u00ab \u2013 \u2013", "tokens": ["De\u00b7ren", "Le\u00b7ben", "mit", "dem", "Sta\u00b7chel", "bricht", "?", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Doch der Waldesmund steht ihm nicht Rede,", "tokens": ["Doch", "der", "Wal\u00b7des\u00b7mund", "steht", "ihm", "nicht", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.22": {"text": "L\u00e4\u00dft ihm ungel\u00f6st die inn're Fehde,", "tokens": ["L\u00e4\u00dft", "ihm", "un\u00b7ge\u00b7l\u00f6st", "die", "inn'\u00b7re", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Wenn's nicht Antwort ist, da\u00df er ihm eben", "tokens": ["Wenn's", "nicht", "Ant\u00b7wort", "ist", ",", "da\u00df", "er", "ihm", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Ein alt M\u00e4rchen lie\u00df im Sinne schweben:", "tokens": ["Ein", "alt", "M\u00e4r\u00b7chen", "lie\u00df", "im", "Sin\u00b7ne", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Vor dem diamantnen Feenschlo\u00df", "tokens": ["Vor", "dem", "di\u00b7a\u00b7mant\u00b7nen", "Feen\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Scharrt gez\u00e4umt das wei\u00dfe Elfenro\u00df.", "tokens": ["Scharrt", "ge\u00b7z\u00e4umt", "das", "wei\u00b7\u00dfe", "El\u00b7fen\u00b7ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Feenk\u00f6n'gin spricht zum holden Knaben:", "tokens": ["Feen\u00b7k\u00f6n'\u00b7gin", "spricht", "zum", "hol\u00b7den", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbzieh' dahin, die Erde zu durchtraben,", "tokens": ["\u00bb", "zieh'", "da\u00b7hin", ",", "die", "Er\u00b7de", "zu", "durch\u00b7tra\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "S\u00fc\u00dfer Liebling, habe deinen Willen,", "tokens": ["S\u00fc\u00b7\u00dfer", "Lieb\u00b7ling", ",", "ha\u00b7be", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Magst dein unerkl\u00e4rbar Sehnen stillen!", "tokens": ["Magst", "dein", "un\u00b7er\u00b7kl\u00e4r\u00b7bar", "Seh\u00b7nen", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Todeswiege, freudenlose Erde,", "tokens": ["To\u00b7des\u00b7wie\u00b7ge", ",", "freu\u00b7den\u00b7lo\u00b7se", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bittre Trift der kranken Menschenheerde,", "tokens": ["Bitt\u00b7re", "Trift", "der", "kran\u00b7ken", "Men\u00b7schen\u00b7heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ach, sie h\u00e4tte l\u00e4ngst dich schon begraben,", "tokens": ["Ach", ",", "sie", "h\u00e4t\u00b7te", "l\u00e4ngst", "dich", "schon", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Feiten dich nicht ew'ger Jugend Gaben!", "tokens": ["Fei\u00b7ten", "dich", "nicht", "ew'\u00b7ger", "Ju\u00b7gend", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mein Umarmen soll dein Todenschrein", "tokens": ["Mein", "Um\u00b7ar\u00b7men", "soll", "dein", "To\u00b7den\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Und mein Mund sein schlie\u00dfend Siegel sein,", "tokens": ["Und", "mein", "Mund", "sein", "schlie\u00b7\u00dfend", "Sie\u00b7gel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Meine Locken sind dein Sterbelinnen", "tokens": ["Mei\u00b7ne", "Lo\u00b7cken", "sind", "dein", "Ster\u00b7be\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und dein Modern hei\u00dft unsterblich Minnen.", "tokens": ["Und", "dein", "Mo\u00b7dern", "hei\u00dft", "uns\u00b7terb\u00b7lich", "Min\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Warst ein K\u00f6niglein den Staubgebor'nen,", "tokens": ["Warst", "ein", "K\u00f6\u00b7nig\u00b7lein", "den", "Staub\u00b7ge\u00b7bor'\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Bist ein K\u00f6nig nun uns Lichterkornen;", "tokens": ["Bist", "ein", "K\u00f6\u00b7nig", "nun", "uns", "Lich\u00b7ter\u00b7kor\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Nur der Erdenstaub droht dir Gef\u00e4hrde,", "tokens": ["Nur", "der", "Er\u00b7dens\u00b7taub", "droht", "dir", "Ge\u00b7f\u00e4hr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Nimmer drum betritt die schn\u00f6de Erde!", "tokens": ["Nim\u00b7mer", "drum", "be\u00b7tritt", "die", "schn\u00f6\u00b7de", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Auf gefeitem Pferd durchspreng' die Lande,", "tokens": ["Auf", "ge\u00b7fei\u00b7tem", "Pferd", "durch\u00b7spreng'", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Unverletzt kehrst du zum Feenstrande;", "tokens": ["Un\u00b7ver\u00b7letzt", "kehrst", "du", "zum", "Feen\u00b7stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Denk', es sei die\u00df Ro\u00df das Musenpferd,", "tokens": ["Denk'", ",", "es", "sei", "die\u00df", "Ro\u00df", "das", "Mu\u00b7sen\u00b7pferd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VAFIN", "PDS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Drauf ein Seher durch die R\u00e4ume f\u00e4hrt:", "tokens": ["Drauf", "ein", "Se\u00b7her", "durch", "die", "R\u00e4u\u00b7me", "f\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Steigt er nieder zu gemeinem Staube,", "tokens": ["Steigt", "er", "nie\u00b7der", "zu", "ge\u00b7mei\u00b7nem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Weh, gemeinem Loos f\u00e4llt er zum Raube!", "tokens": ["Weh", ",", "ge\u00b7mei\u00b7nem", "Loos", "f\u00e4llt", "er", "zum", "Rau\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Nimmer drum betritt den Grund der Erde,", "tokens": ["Nim\u00b7mer", "drum", "be\u00b7tritt", "den", "Grund", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Erdenstaub allein bringt dir Gef\u00e4hrde.\u00ab \u2013", "tokens": ["Er\u00b7dens\u00b7taub", "al\u00b7lein", "bringt", "dir", "Ge\u00b7f\u00e4hr\u00b7de", ".", "\u00ab