{"textgrid.poem.52942": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bl\u00e4tter rieseln von den B\u00e4umen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "rie\u00b7seln", "von", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch kahle Stoppeln bl\u00e4st der Wind,", "tokens": ["Durch", "kah\u00b7le", "Stop\u00b7peln", "bl\u00e4st", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lange noch willst du dich s\u00e4umen", "tokens": ["Wie", "lan\u00b7ge", "noch", "willst", "du", "dich", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deinen Fahrten, armes Kind?", "tokens": ["Auf", "dei\u00b7nen", "Fahr\u00b7ten", ",", "ar\u00b7mes", "Kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Tal bist du zu Tal gegangen,", "tokens": ["Von", "Tal", "bist", "du", "zu", "Tal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jede H\u00fctte lugtest du,", "tokens": ["In", "je\u00b7de", "H\u00fct\u00b7te", "lug\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mit dir ging, war dein Verlangen,", "tokens": ["Was", "mit", "dir", "ging", ",", "war", "dein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was nirgends weilte, deine Ruh.", "tokens": ["Was", "nir\u00b7gends", "weil\u00b7te", ",", "dei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sahst du neue Berge blauen,", "tokens": ["Und", "sahst", "du", "neu\u00b7e", "Ber\u00b7ge", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob noch so fern, ob noch so hoch,", "tokens": ["Ob", "noch", "so", "fern", ",", "ob", "noch", "so", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du mu\u00dftest doch hin\u00fcberschauen,", "tokens": ["Du", "mu\u00df\u00b7test", "doch", "hin\u00b7\u00fc\u00b7bersc\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du dachtest: Dr\u00fcben find ich's noch!", "tokens": ["Du", "dach\u00b7test", ":", "Dr\u00fc\u00b7ben", "find", "ich's", "noch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verloren hast du sch\u00f6ne Jahre,", "tokens": ["Ver\u00b7lo\u00b7ren", "hast", "du", "sch\u00f6\u00b7ne", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergeudet manchen sch\u00f6nren Traum,", "tokens": ["Ver\u00b7geu\u00b7det", "man\u00b7chen", "sch\u00f6n\u00b7ren", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Haupte falln die Haare,", "tokens": ["Von", "dei\u00b7nem", "Haup\u00b7te", "falln", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wie die Bl\u00e4tter dort vom Baum.", "tokens": ["So", "wie", "die", "Bl\u00e4t\u00b7ter", "dort", "vom", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch deine Seele kalt und schneidend", "tokens": ["Durch", "dei\u00b7ne", "See\u00b7le", "kalt", "und", "schnei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weht der Erfahrung b\u00f6ser Ost,", "tokens": ["Weht", "der", "Er\u00b7fah\u00b7rung", "b\u00f6\u00b7ser", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die letzte Hoffnung kr\u00fcmmt sich leidend", "tokens": ["Die", "letz\u00b7te", "Hoff\u00b7nung", "kr\u00fcmmt", "sich", "lei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schauernd vor dem Winterfrost.", "tokens": ["Und", "schau\u00b7ernd", "vor", "dem", "Win\u00b7ter\u00b7frost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die St\u00f6rche ziehen froh von hinnen,", "tokens": ["Die", "St\u00f6r\u00b7che", "zie\u00b7hen", "froh", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft noch nicht, wohin du gehst;", "tokens": ["Du", "wei\u00dft", "noch", "nicht", ",", "wo\u00b7hin", "du", "gehst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihnen kannst du nicht entrinnen,", "tokens": ["Mit", "ih\u00b7nen", "kannst", "du", "nicht", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So falle nieder, wo du stehst.", "tokens": ["So", "fal\u00b7le", "nie\u00b7der", ",", "wo", "du", "stehst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00e4rm dich an fremder Menschen Herde,", "tokens": ["W\u00e4rm", "dich", "an", "frem\u00b7der", "Men\u00b7schen", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn einen eignen hast du nicht,", "tokens": ["Denn", "ei\u00b7nen", "eig\u00b7nen", "hast", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4ume eine Heimats-Erde,", "tokens": ["Und", "tr\u00e4u\u00b7me", "ei\u00b7ne", "Hei\u00b7mats\u00b7Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man in fremden Zungen spricht.", "tokens": ["Wo", "man", "in", "frem\u00b7den", "Zun\u00b7gen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gleichgiltig dr\u00fcck dich in die Ecke", "tokens": ["Gleich\u00b7gil\u00b7tig", "dr\u00fcck", "dich", "in", "die", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und stimm in ihren Alltags-Scherz,", "tokens": ["Und", "stimm", "in", "ih\u00b7ren", "All\u00b7tags\u00b7Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der Entsagung Leichendecke", "tokens": ["Und", "der", "Ent\u00b7sa\u00b7gung", "Lei\u00b7chen\u00b7de\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zieh fester um dein starres Herz.", "tokens": ["Zieh", "fes\u00b7ter", "um", "dein", "star\u00b7res", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du hast's gewollt! Du darfst nicht grollen,", "tokens": ["Du", "hast's", "ge\u00b7wollt", "!", "Du", "darfst", "nicht", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VMPP", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn du noch so elend bist;", "tokens": ["Und", "wenn", "du", "noch", "so", "e\u00b7lend", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ach! du h\u00e4ttest ahnen sollen,", "tokens": ["Denn", "ach", "!", "du", "h\u00e4t\u00b7test", "ah\u00b7nen", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VAFIN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es nicht ewig Fr\u00fchling ist.", "tokens": ["Da\u00df", "es", "nicht", "e\u00b7wig", "Fr\u00fch\u00b7ling", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}