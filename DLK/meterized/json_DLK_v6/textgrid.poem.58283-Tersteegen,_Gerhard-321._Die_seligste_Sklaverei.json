{"textgrid.poem.58283": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "321. Die seligste Sklaverei", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfe Sklaverei, in Liebe sein gefangen,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Skla\u00b7ve\u00b7rei", ",", "in", "Lie\u00b7be", "sein", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Jesu sanfter Wink ist unsres Willens Band", "tokens": ["Da", "Je\u00b7su", "sanf\u00b7ter", "Wink", "ist", "uns\u00b7res", "Wil\u00b7lens", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alle Liebe ganz in ihn ist angewandt!", "tokens": ["Und", "al\u00b7le", "Lie\u00b7be", "ganz", "in", "ihn", "ist", "an\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df, ein solcher wird nicht frei zu sein verlangen.", "tokens": ["Ich", "wei\u00df", ",", "ein", "sol\u00b7cher", "wird", "nicht", "frei", "zu", "sein", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "PIAT", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}