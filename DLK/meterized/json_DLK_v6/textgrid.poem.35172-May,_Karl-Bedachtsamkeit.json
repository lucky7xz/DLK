{"textgrid.poem.35172": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Bedachtsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei ruhig; st\u00fcrme, st\u00fcrme nicht!", "tokens": ["Sei", "ru\u00b7hig", ";", "st\u00fcr\u00b7me", ",", "st\u00fcr\u00b7me", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "VVFIN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum sollst du dich \u00fcberst\u00fcrzen?", "tokens": ["Wa\u00b7rum", "sollst", "du", "dich", "\u00fc\u00b7bers\u00b7t\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Thu recht und billig deine Pflicht;", "tokens": ["Thu", "recht", "und", "bil\u00b7lig", "dei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kannst die Zeit doch nicht verk\u00fcrzen.", "tokens": ["Du", "kannst", "die", "Zeit", "doch", "nicht", "ver\u00b7k\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei ruhig; dr\u00e4ng dich nicht voran!", "tokens": ["Sei", "ru\u00b7hig", ";", "dr\u00e4ng", "dich", "nicht", "vo\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gilt, die edle Kraft zu sparen.", "tokens": ["Es", "gilt", ",", "die", "ed\u00b7le", "Kraft", "zu", "spa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer diese Kraft nicht z\u00fcgeln kann,", "tokens": ["Wer", "die\u00b7se", "Kraft", "nicht", "z\u00fc\u00b7geln", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird mit ihr nicht gl\u00fccklich fahren.", "tokens": ["Der", "wird", "mit", "ihr", "nicht", "gl\u00fcck\u00b7lich", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei ruhig, doch vers\u00e4ume nichts!", "tokens": ["Sei", "ru\u00b7hig", ",", "doch", "ver\u00b7s\u00e4u\u00b7me", "nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es darf sich keine L\u00fccke zeigen.", "tokens": ["Es", "darf", "sich", "kei\u00b7ne", "L\u00fc\u00b7cke", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du empor zum Quell des Lichts,", "tokens": ["Willst", "du", "em\u00b7por", "zum", "Quell", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du behutsam aufzusteigen.", "tokens": ["Hast", "du", "be\u00b7hut\u00b7sam", "auf\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei ruhig, immer unbeirrt!", "tokens": ["Sei", "ru\u00b7hig", ",", "im\u00b7mer", "un\u00b7be\u00b7irrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dich von Andern nicht beth\u00f6ren;", "tokens": ["La\u00df", "dich", "von", "An\u00b7dern", "nicht", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer sich selber untreu wird,", "tokens": ["Denn", "wer", "sich", "sel\u00b7ber", "un\u00b7treu", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist von ihnen leicht zu st\u00f6ren.", "tokens": ["Der", "ist", "von", "ih\u00b7nen", "leicht", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sei ruhig, wenn das Ende naht!", "tokens": ["Sei", "ru\u00b7hig", ",", "wenn", "das", "En\u00b7de", "naht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du nicht zaghaft wie so Viele,", "tokens": ["Bist", "du", "nicht", "zag\u00b7haft", "wie", "so", "Vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KOKOM", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So bringt die letzte, schwerste That", "tokens": ["So", "bringt", "die", "letz\u00b7te", ",", "schwers\u00b7te", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Engelsschwingen dich zum Ziele.", "tokens": ["Auf", "En\u00b7gels\u00b7schwin\u00b7gen", "dich", "zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}