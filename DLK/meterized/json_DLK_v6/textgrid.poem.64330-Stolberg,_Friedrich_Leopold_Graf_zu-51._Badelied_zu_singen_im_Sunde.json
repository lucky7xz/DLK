{"textgrid.poem.64330": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "51. Badelied zu singen im Sunde", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lockten mich nimmer", "tokens": ["Es", "lock\u00b7ten", "mich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die milderen Schimmer", "tokens": ["Die", "mil\u00b7de\u00b7ren", "Schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Sonne so sehr!", "tokens": ["Der", "Son\u00b7ne", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Abendluft hauchet!", "tokens": ["Die", "A\u00b7ben\u00b7dluft", "hau\u00b7chet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auf, J\u00fcnglinge, tauchet", "tokens": ["Auf", ",", "J\u00fcng\u00b7lin\u00b7ge", ",", "tau\u00b7chet"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Glieder ins Meer!", "tokens": ["Die", "Glie\u00b7der", "ins", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Hier, wo sich zwei Meere", "tokens": ["Hier", ",", "wo", "sich", "zwei", "Mee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "CARD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Begegnen wie Heere,", "tokens": ["Be\u00b7geg\u00b7nen", "wie", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "St\u00fcrz' ich mich hinab!", "tokens": ["St\u00fcrz'", "ich", "mich", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mich Sterblichen gr\u00fc\u00dfen", "tokens": ["Mich", "Sterb\u00b7li\u00b7chen", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Nymphen, und k\u00fcssen", "tokens": ["Die", "Nym\u00b7phen", ",", "und", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Hitze mir ab!", "tokens": ["Die", "Hit\u00b7ze", "mir", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Seht Titan, er sinket", "tokens": ["Seht", "Ti\u00b7tan", ",", "er", "sin\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ins Weltmeer, und winket", "tokens": ["Ins", "Welt\u00b7meer", ",", "und", "win\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Noch flammend uns her!", "tokens": ["Noch", "flam\u00b7mend", "uns", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schamr\u00f6tend erhebet", "tokens": ["Scham\u00b7r\u00f6\u00b7tend", "er\u00b7he\u00b7bet"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sich Luna, und bebet", "tokens": ["Sich", "Lu\u00b7na", ",", "und", "be\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "NE", "$,", "KON", "VVFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Auf \u00f6stlichem Meer!", "tokens": ["Auf", "\u00f6st\u00b7li\u00b7chem", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "O r\u00fchmliche Wonne,", "tokens": ["O", "r\u00fchm\u00b7li\u00b7che", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Mond und mit Sonne", "tokens": ["Mit", "Mond", "und", "mit", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu baden im Meer!", "tokens": ["Zu", "ba\u00b7den", "im", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die wallenden Glutten", "tokens": ["Die", "wal\u00b7len\u00b7den", "Glut\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der purpurnen Fluten", "tokens": ["Der", "pur\u00b7pur\u00b7nen", "Flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So rund um uns her!", "tokens": ["So", "rund", "um", "uns", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}