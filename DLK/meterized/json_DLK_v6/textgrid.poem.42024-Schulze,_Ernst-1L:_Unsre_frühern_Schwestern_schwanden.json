{"textgrid.poem.42024": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unsre fr\u00fchern Schwestern schwanden", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre fr\u00fchern Schwestern schwanden", "tokens": ["Uns\u00b7re", "fr\u00fc\u00b7hern", "Schwes\u00b7tern", "schwan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon im Hain,", "tokens": ["Schon", "im", "Hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und wir armen Kinder standen", "tokens": ["Und", "wir", "ar\u00b7men", "Kin\u00b7der", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz allein.", "tokens": ["Ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schwarz war unser Thal umzogen", "tokens": ["Schwarz", "war", "un\u00b7ser", "Thal", "um\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie das Grab,", "tokens": ["Wie", "das", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und es rann in Wetterwogen", "tokens": ["Und", "es", "rann", "in", "Wet\u00b7ter\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kalt herab.", "tokens": ["Kalt", "her\u00b7ab", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ach, da zagten wir im tr\u00fcben", "tokens": ["Ach", ",", "da", "zag\u00b7ten", "wir", "im", "tr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturmesdrohn,", "tokens": ["Stur\u00b7mes\u00b7drohn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Weinten still, da\u00df alle Lieben", "tokens": ["Wein\u00b7ten", "still", ",", "da\u00df", "al\u00b7le", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns entflohn;", "tokens": ["Uns", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Unsre Kl\u00f6cklein sanken schmerzlich", "tokens": ["Uns\u00b7re", "Kl\u00f6c\u00b7klein", "san\u00b7ken", "schmerz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief in's Gr\u00fcn,", "tokens": ["Tief", "in's", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und wir w\u00fcnschten hei\u00df und herzlich", "tokens": ["Und", "wir", "w\u00fcnschten", "hei\u00df", "und", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu verbl\u00fchn.", "tokens": ["Zu", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Da, du freundliches Gebilde,", "tokens": ["Da", ",", "du", "freund\u00b7li\u00b7ches", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rten wir,", "tokens": ["H\u00f6r\u00b7ten", "wir", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mitleid wohn' und Huld und Milde", "tokens": ["Mit\u00b7leid", "wohn'", "und", "Huld", "und", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets bei dir;", "tokens": ["Stets", "bei", "dir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ewig heitre Sonne scheine", "tokens": ["E\u00b7wig", "heit\u00b7re", "Son\u00b7ne", "schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um dich her,", "tokens": ["Um", "dich", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und wenn du ihm l\u00e4chelst, weine", "tokens": ["Und", "wenn", "du", "ihm", "l\u00e4\u00b7chelst", ",", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Keiner mehr.", "tokens": ["Kei\u00b7ner", "mehr", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Drum sind wir zu dir gekommen,", "tokens": ["Drum", "sind", "wir", "zu", "dir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Retterin!", "tokens": ["Ret\u00b7te\u00b7rin", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nimm die friedlichen, die frommen", "tokens": ["Nimm", "die", "fried\u00b7li\u00b7chen", ",", "die", "from\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "ADJA", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcmlein hin.", "tokens": ["Bl\u00fcm\u00b7lein", "hin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wenn der Kelch bei Sturmesschweben", "tokens": ["Wenn", "der", "Kelch", "bei", "Stur\u00b7mes\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch erliegt,", "tokens": ["Auch", "er\u00b7liegt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Lang noch rinnt das duft'ge Leben", "tokens": ["Lang", "noch", "rinnt", "das", "duft'\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unversiegt.", "tokens": ["Un\u00b7ver\u00b7siegt", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}