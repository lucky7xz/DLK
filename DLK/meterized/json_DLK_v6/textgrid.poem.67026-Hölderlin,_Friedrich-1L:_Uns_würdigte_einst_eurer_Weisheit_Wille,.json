{"textgrid.poem.67026": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Uns w\u00fcrdigte einst eurer Weisheit Wille,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns w\u00fcrdigte einst eurer Weisheit Wille,", "tokens": ["Uns", "w\u00fcr\u00b7dig\u00b7te", "einst", "eu\u00b7rer", "Weis\u00b7heit", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kirche Dienst auch uns zu weihn,", "tokens": ["Der", "Kir\u00b7che", "Dienst", "auch", "uns", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer, Br\u00fcder, s\u00e4umt, da\u00df er die Schuld des Danks erf\u00fclle,", "tokens": ["Wer", ",", "Br\u00fc\u00b7der", ",", "s\u00e4umt", ",", "da\u00df", "er", "die", "Schuld", "des", "Danks", "er\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wir uns solcher Gnade freun?", "tokens": ["Die", "wir", "uns", "sol\u00b7cher", "Gna\u00b7de", "freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Froh eilt der Wanderer, durch dunkle W\u00e4lder,", "tokens": ["Froh", "eilt", "der", "Wan\u00b7de\u00b7rer", ",", "durch", "dunk\u00b7le", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch W\u00fcsten, die von Hitze gl\u00fchn,", "tokens": ["Durch", "W\u00fcs\u00b7ten", ",", "die", "von", "Hit\u00b7ze", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erblickt er nur von fern des Lands begl\u00fcckte Felder,", "tokens": ["Er\u00b7blickt", "er", "nur", "von", "fern", "des", "Lands", "be\u00b7gl\u00fcck\u00b7te", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Ruh und Friede bl\u00fchn.", "tokens": ["Wo", "Ruh", "und", "Frie\u00b7de", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So k\u00f6nnen wir die frohe Bahn durcheilen,", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "die", "fro\u00b7he", "Bahn", "durc\u00b7hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil schon das hohe Ziel uns lacht", "tokens": ["Weil", "schon", "das", "ho\u00b7he", "Ziel", "uns", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der Bestimmung Sporn, ein Feind von tr\u00e4gen Weilen,", "tokens": ["Und", "der", "Be\u00b7stim\u00b7mung", "Sporn", ",", "ein", "Feind", "von", "tr\u00e4\u00b7gen", "Wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns froh und emsig macht.", "tokens": ["Uns", "froh", "und", "em\u00b7sig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, dieses Gl\u00fcck, das, gro\u00dfe M\u00e4cenaten,", "tokens": ["Ja", ",", "die\u00b7ses", "Gl\u00fcck", ",", "das", ",", "gro\u00b7\u00dfe", "M\u00e4\u00b7ce\u00b7na\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "$,", "PDS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr schenkt, soll nie ein tr\u00e4ger Sinn", "tokens": ["Ihr", "schenkt", ",", "soll", "nie", "ein", "tr\u00e4\u00b7ger", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei uns verdunkeln, nein! verehren Flei\u00df und Taten,", "tokens": ["Bei", "uns", "ver\u00b7dun\u00b7keln", ",", "nein", "!", "ver\u00b7eh\u00b7ren", "Flei\u00df", "und", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "PTKANT", "$.", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Tugend immerhin.", "tokens": ["Und", "Tu\u00b7gend", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Euch aber kr\u00f6ne Ruhm und hohe Ehre,", "tokens": ["Euch", "a\u00b7ber", "kr\u00f6\u00b7ne", "Ruhm", "und", "ho\u00b7he", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dem Verdienste stets geb\u00fchrt,", "tokens": ["Die", "dem", "Ver\u00b7diens\u00b7te", "stets", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder k\u00fcnftge Tag erh\u00f6he und vermehre", "tokens": ["Und", "je\u00b7der", "k\u00fcnft\u00b7ge", "Tag", "er\u00b7h\u00f6\u00b7he", "und", "ver\u00b7meh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Glanz, der euch schon ziert.", "tokens": ["Den", "Glanz", ",", "der", "euch", "schon", "ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und was ist wohl f\u00fcr euch die sch\u00f6nste Krone?", "tokens": ["Und", "was", "ist", "wohl", "f\u00fcr", "euch", "die", "sch\u00f6ns\u00b7te", "Kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der Kirche und des Staates Wohl,", "tokens": ["Der", "Kir\u00b7che", "und", "des", "Staa\u00b7tes", "Wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stets eurer Sorgen Ziel. Wohlan, der Himmel lohne", "tokens": ["Stets", "eu\u00b7rer", "Sor\u00b7gen", "Ziel", ".", "Wo\u00b7hlan", ",", "der", "Him\u00b7mel", "loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$.", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Euch stets mit ihrem Wohl.", "tokens": ["Euch", "stets", "mit", "ih\u00b7rem", "Wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}