{"textgrid.poem.58505": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "543. Wo nichts zu sehn, da macht's Gott sch\u00f6n", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' mit Wohlgefall'n das Sch\u00f6ne andrer Kinder,", "tokens": ["Ich", "seh'", "mit", "Wohl\u00b7ge\u00b7fall'n", "das", "Sch\u00f6\u00b7ne", "an\u00b7drer", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den eignen Schmutz und Fehl seh' ich mit Scham nicht minder;", "tokens": ["Den", "eig\u00b7nen", "Schmutz", "und", "Fehl", "seh'", "ich", "mit", "Scham", "nicht", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies Sehen siehet Gott mit Wohlgefall'n in mir", "tokens": ["Dies", "Se\u00b7hen", "sie\u00b7het", "Gott", "mit", "Wohl\u00b7ge\u00b7fall'n", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVFIN", "NN", "APPR", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und spricht: Weil du so blo\u00df, so nimm mein Sch\u00f6nes dir,", "tokens": ["Und", "spricht", ":", "Weil", "du", "so", "blo\u00df", ",", "so", "nimm", "mein", "Sch\u00f6\u00b7nes", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur nimm es ohne Nehmen", "tokens": ["Nur", "nimm", "es", "oh\u00b7ne", "Neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dann komm ohne Sch\u00e4men!", "tokens": ["Und", "dann", "komm", "oh\u00b7ne", "Sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}