{"textgrid.poem.61075": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen Zimmern hast du j\u00fcngst gewohnt,", "tokens": ["In", "die\u00b7sen", "Zim\u00b7mern", "hast", "du", "j\u00fcngst", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Treppen hat dein sch\u00f6ner Fu\u00df betreten,", "tokens": ["Die", "Trep\u00b7pen", "hat", "dein", "sch\u00f6\u00b7ner", "Fu\u00df", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch diese Wipfel schautest du den Mond", "tokens": ["Durch", "die\u00b7se", "Wip\u00b7fel", "schau\u00b7test", "du", "den", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sahst den Sommer bl\u00fchn auf diesen Beeten.", "tokens": ["Und", "sahst", "den", "Som\u00b7mer", "bl\u00fchn", "auf", "die\u00b7sen", "Bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dort an jenem Fenster sa\u00dfest du,", "tokens": ["Und", "dort", "an", "je\u00b7nem", "Fens\u00b7ter", "sa\u00b7\u00dfest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und alter Zeit gedachtest du im Herzen,", "tokens": ["Und", "al\u00b7ter", "Zeit", "ge\u00b7dach\u00b7test", "du", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dort entschliefst du, wenn zu tiefer Ruh'", "tokens": ["Und", "dort", "ent\u00b7schliefst", "du", ",", "wenn", "zu", "tie\u00b7fer", "Ruh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKA", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Nachtgebet besprochen alle Schmerzen.", "tokens": ["Dein", "Nacht\u00b7ge\u00b7bet", "be\u00b7spro\u00b7chen", "al\u00b7le", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, da du fortzogst, mu\u00dft' es jedem sein,", "tokens": ["Ach", ",", "da", "du", "fort\u00b7zogst", ",", "mu\u00dft'", "es", "je\u00b7dem", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ob der Engel dieses Hauses schiede;", "tokens": ["Als", "ob", "der", "En\u00b7gel", "die\u00b7ses", "Hau\u00b7ses", "schie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber trat an deiner Statt herein,", "tokens": ["Ich", "a\u00b7ber", "trat", "an", "dei\u00b7ner", "Statt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein wilder Gast mit meinem wilden Liede.", "tokens": ["Ein", "wil\u00b7der", "Gast", "mit", "mei\u00b7nem", "wil\u00b7den", "Lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun ist mir oft, als w\u00fc\u00dften sie von dir", "tokens": ["Nun", "ist", "mir", "oft", ",", "als", "w\u00fc\u00df\u00b7ten", "sie", "von", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00fc\u00dften reden diese stummen W\u00e4nde,", "tokens": ["Und", "m\u00fc\u00df\u00b7ten", "re\u00b7den", "die\u00b7se", "stum\u00b7men", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als schwebt' um Garten, Wald und Blumen hier", "tokens": ["Als", "schwebt'", "um", "Gar\u00b7ten", ",", "Wald", "und", "Blu\u00b7men", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein still Verm\u00e4chtnis, das ich nicht verst\u00e4nde.", "tokens": ["Ein", "still", "Ver\u00b7m\u00e4cht\u00b7nis", ",", "das", "ich", "nicht", "ver\u00b7st\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und doch, verst\u00e4nd' ich's, m\u00f6cht' es mir - wer wei\u00df! -", "tokens": ["Und", "doch", ",", "ver\u00b7st\u00e4nd'", "ich's", ",", "m\u00f6cht'", "es", "mir", "wer", "wei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "VVFIN", "PIS", "$,", "VMFIN", "PPER", "PPER", "$(", "PWS", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Busen w\u00e4lzen eine Last von Kummer", "tokens": ["Vom", "Bu\u00b7sen", "w\u00e4l\u00b7zen", "ei\u00b7ne", "Last", "von", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Wimper m\u00fcd und fieberhei\u00df", "tokens": ["Und", "die\u00b7se", "Wim\u00b7per", "m\u00fcd", "und", "fie\u00b7ber\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Tr\u00e4nen wieder segnen und mit Schlummer.", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "wie\u00b7der", "seg\u00b7nen", "und", "mit", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "W\u00fc\u00dft' ich das ", "tokens": ["W\u00fc\u00dft'", "ich", "das"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Die Rast mir nimmt und mir verst\u00f6rt das Leben,", "tokens": ["Die", "Rast", "mir", "nimmt", "und", "mir", "ver\u00b7st\u00f6rt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und, wenn du's tatest, ob du mir vergeben?", "tokens": ["Und", ",", "wenn", "du's", "ta\u00b7test", ",", "ob", "du", "mir", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}