{"textgrid.poem.55335": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Typus", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nichts in der Haut,", "tokens": ["Es", "ist", "nichts", "in", "der", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was nicht im Knochen ist.", "tokens": ["Was", "nicht", "im", "Kno\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor schlechtem Gebilde jedem graut,", "tokens": ["Vor", "schlech\u00b7tem", "Ge\u00b7bil\u00b7de", "je\u00b7dem", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ein Augenschmerz ihm ist.", "tokens": ["Das", "ein", "Au\u00b7gen\u00b7schmerz", "ihm", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was freut denn jeden? Bl\u00fchen zu sehn,", "tokens": ["Was", "freut", "denn", "je\u00b7den", "?", "Bl\u00fc\u00b7hen", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PIAT", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das von innen schon gut gestaltet;", "tokens": ["Das", "von", "in\u00b7nen", "schon", "gut", "ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Au\u00dfen mag's in Gl\u00e4tte, mag in Farben gehn,", "tokens": ["Au\u00b7\u00dfen", "mag's", "in", "Gl\u00e4t\u00b7te", ",", "mag", "in", "Far\u00b7ben", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "$,", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Es ist ihm schon voran gewaltet.", "tokens": ["Es", "ist", "ihm", "schon", "vo\u00b7ran", "ge\u00b7wal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}