{"textgrid.poem.48452": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "23. Als H. G\u00f6rg Wilhelm P\u00f6hmer von ihm aus Moskaw nach Deutschland verreisete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis hieher war dein Ziel. Nun zeuchst du, s\u00fc\u00dfer Freund,", "tokens": ["Bis", "hie\u00b7her", "war", "dein", "Ziel", ".", "Nun", "zeuchst", "du", ",", "s\u00fc\u00b7\u00dfer", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PAV", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit Ehren wieder heim, wo deine Sonne scheint", "tokens": ["mit", "Eh\u00b7ren", "wie\u00b7der", "heim", ",", "wo", "dei\u00b7ne", "Son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und meine Hoffnung bl\u00fcht. Zwar, w\u00e4re W\u00fcndschen K\u00f6nnen,", "tokens": ["und", "mei\u00b7ne", "Hoff\u00b7nung", "bl\u00fcht", ".", "Zwar", ",", "w\u00e4\u00b7re", "W\u00fcnd\u00b7schen", "K\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "$,", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und w\u00e4re Wollen Tun, du soltest nicht von hinnen", "tokens": ["und", "w\u00e4\u00b7re", "Wol\u00b7len", "Tun", ",", "du", "sol\u00b7test", "nicht", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "NE", "$,", "PPER", "VMFIN", "PTKNEG", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und so den R\u00fcckzug tun. Ich l\u00e4ge dir stets an,", "tokens": ["und", "so", "den", "R\u00fcck\u00b7zug", "tun", ".", "Ich", "l\u00e4\u00b7ge", "dir", "stets", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "damit mein langer Weg nicht w\u00fcrd' ohn' dich getan,", "tokens": ["da\u00b7mit", "mein", "lan\u00b7ger", "Weg", "nicht", "w\u00fcrd'", "ohn'", "dich", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "o du mein Pylades! Mein Ph\u00f6bus w\u00fcrde br\u00fcnstig,", "tokens": ["o", "du", "mein", "Py\u00b7la\u00b7des", "!", "Mein", "Ph\u00f6\u00b7bus", "w\u00fcr\u00b7de", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPER", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "s\u00e4ng' h\u00f6her als zuvor, weil du ihm w\u00e4rest g\u00fcnstig,", "tokens": ["s\u00e4ng'", "h\u00f6\u00b7her", "als", "zu\u00b7vor", ",", "weil", "du", "ihm", "w\u00e4\u00b7rest", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "an dem es ihm sonst fehlt. Kein Ekel, kein Verdru\u00df", "tokens": ["an", "dem", "es", "ihm", "sonst", "fehlt", ".", "Kein", "E\u00b7kel", ",", "kein", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$.", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "des Reisens stie\u00df' mich an, den ich sonst f\u00fcrchten mu\u00df.", "tokens": ["des", "Rei\u00b7sens", "stie\u00df'", "mich", "an", ",", "den", "ich", "sonst", "f\u00fcrch\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zeuch aber, wie du tust, weil ja der Rat der Sternen", "tokens": ["Zeuch", "a\u00b7ber", ",", "wie", "du", "tust", ",", "weil", "ja", "der", "Rat", "der", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "dich hei\u00dft zur\u00fccke gehn! Der mu\u00df mehr sehn und lernen,", "tokens": ["dich", "hei\u00dft", "zu\u00b7r\u00fc\u00b7cke", "gehn", "!", "Der", "mu\u00df", "mehr", "sehn", "und", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$.", "PDS", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der noch nicht gnugsam kan. Du, dieser Jahre Pracht,", "tokens": ["der", "noch", "nicht", "gnug\u00b7sam", "kan", ".", "Du", ",", "die\u00b7ser", "Jah\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "VMFIN", "$.", "PPER", "$,", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "hast die Vollkommenheit selbselbsten leer gemacht.", "tokens": ["hast", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "selb\u00b7selbs\u00b7ten", "leer", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein weises Tun tut kund, worzu du bist geboren:", "tokens": ["Dein", "wei\u00b7ses", "Tun", "tut", "kund", ",", "wor\u00b7zu", "du", "bist", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "zu nichts, als was dir gleicht. Hast keine Zeit verloren", "tokens": ["zu", "nichts", ",", "als", "was", "dir", "gleicht", ".", "Hast", "kei\u00b7ne", "Zeit", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "KOUS", "PRELS", "PPER", "VVFIN", "$.", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "beim Spiel' und um ein Glas, nicht, wie wol mancher pflegt,", "tokens": ["beim", "Spiel'", "und", "um", "ein", "Glas", ",", "nicht", ",", "wie", "wol", "man\u00b7cher", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,", "PTKNEG", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der mehr Herz' in dem Mund', als Mund im Herzen tr\u00e4gt,", "tokens": ["der", "mehr", "Her\u00b7z'", "in", "dem", "Mund'", ",", "als", "Mund", "im", "Her\u00b7zen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "der eh' nicht ist ein Man, als bis er k\u00f6mmt zu Frauen.", "tokens": ["der", "eh'", "nicht", "ist", "ein", "Man", ",", "als", "bis", "er", "k\u00f6mmt", "zu", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PTKNEG", "VAFIN", "ART", "PIS", "$,", "KOUS", "APPR", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bist jung gegangen hin, um nicht nur anzuschauen,", "tokens": ["Bist", "jung", "ge\u00b7gan\u00b7gen", "hin", ",", "um", "nicht", "nur", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "PTKVZ", "$,", "KOUI", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "was Feind mit Feinden tut, bist selbst gebrochen raus,", "tokens": ["was", "Feind", "mit", "Fein\u00b7den", "tut", ",", "bist", "selbst", "ge\u00b7bro\u00b7chen", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "hast deinen Gegenteil voran gefordert aus,", "tokens": ["hast", "dei\u00b7nen", "Ge\u00b7gen\u00b7teil", "vo\u00b7ran", "ge\u00b7for\u00b7dert", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das Kriegern r\u00fchmlich ist. Der neulich gro\u00dfe K\u00f6nig", "tokens": ["das", "Krie\u00b7gern", "r\u00fchm\u00b7lich", "ist", ".", "Der", "neu\u00b7lich", "gro\u00b7\u00dfe", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "war dir nach W\u00fcrden hold. So zeugst du auch nicht wenig,", "tokens": ["war", "dir", "nach", "W\u00fcr\u00b7den", "hold", ".", "So", "zeugst", "du", "auch", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "wie wol du deine Zeit bei dem hast angelegt,", "tokens": ["wie", "wol", "du", "dei\u00b7ne", "Zeit", "bei", "dem", "hast", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "APPR", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "der unsers Landes Last auf seinen Achseln tr\u00e4gt,", "tokens": ["der", "un\u00b7sers", "Lan\u00b7des", "Last", "auf", "sei\u00b7nen", "Ach\u00b7seln", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "ein Atlas dieser Zeit. Die Bl\u00fcte deiner Jugend", "tokens": ["ein", "At\u00b7las", "die\u00b7ser", "Zeit", ".", "Die", "Bl\u00fc\u00b7te", "dei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "streut von sich weit und breit den Ruch von aller Tugend.", "tokens": ["streut", "von", "sich", "weit", "und", "breit", "den", "Ruch", "von", "al\u00b7ler", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADJD", "KON", "ADJD", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kein sch\u00f6nes Buch geht aus, du wei\u00dft es, eh' es k\u00f6mmt,", "tokens": ["Kein", "sch\u00f6\u00b7nes", "Buch", "geht", "aus", ",", "du", "wei\u00dft", "es", ",", "eh'", "es", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und kanst es, eh' es der kaum in die H\u00e4nde nimmt.", "tokens": ["und", "kanst", "es", ",", "eh'", "es", "der", "kaum", "in", "die", "H\u00e4n\u00b7de", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "des ", "tokens": ["des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Und solle gleich durch Neid ", "tokens": ["Und", "sol\u00b7le", "gleich", "durch", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "so w\u00fcrd' er doch durch dich hinwieder ganz dastehn:", "tokens": ["so", "w\u00fcrd'", "er", "doch", "durch", "dich", "hin\u00b7wie\u00b7der", "ganz", "das\u00b7tehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "so fertig bist du sein. Du liebst, was wert zu lieben,", "tokens": ["so", "fer\u00b7tig", "bist", "du", "sein", ".", "Du", "liebst", ",", "was", "wert", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VAINF", "$.", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "und setzest es in dich. Was ", "tokens": ["und", "set\u00b7zest", "es", "in", "dich", ".", "Was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$.", "PWS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "was unser ", "tokens": ["was", "un\u00b7ser"], "token_info": ["word", "word"], "pos": ["PWS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "und sagst es ohne Buch auf einen Nagel her,", "tokens": ["und", "sagst", "es", "oh\u00b7ne", "Buch", "auf", "ei\u00b7nen", "Na\u00b7gel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "das Keiner von uns kan. Ich wunder' mich der Gaben.", "tokens": ["das", "Kei\u00b7ner", "von", "uns", "kan", ".", "Ich", "wun\u00b7der'", "mich", "der", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "VMFIN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Denn was wir Andern halb, ja kaum nur einzeln haben,", "tokens": ["Denn", "was", "wir", "An\u00b7dern", "halb", ",", "ja", "kaum", "nur", "ein\u00b7zeln", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJA", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ged\u00e4chtn\u00fc\u00df, Wissenschaft, Beredsamkeit, Verstand,", "tokens": ["Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", ",", "Wis\u00b7sen\u00b7schaft", ",", "Be\u00b7red\u00b7sam\u00b7keit", ",", "Ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"