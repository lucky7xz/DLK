{"textgrid.poem.23974": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gottesdienst", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf steiler H\u00f6he stand ich schauend.", "tokens": ["Auf", "stei\u00b7ler", "H\u00f6\u00b7he", "stand", "ich", "schau\u00b7end", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Auge trank in tiefen, gro\u00dfen", "tokens": ["Mein", "Au\u00b7ge", "trank", "in", "tie\u00b7fen", ",", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Z\u00fcgen die Sch\u00f6nheit.", "tokens": ["Z\u00fc\u00b7gen", "die", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Weit in graue, webende Fernen", "tokens": ["Weit", "in", "grau\u00b7e", ",", "we\u00b7ben\u00b7de", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Schweifte der Blick auf fr\u00f6hlichen Fittichen,", "tokens": ["Schweif\u00b7te", "der", "Blick", "auf", "fr\u00f6h\u00b7li\u00b7chen", "Fit\u00b7ti\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Holte die schimmernde Sch\u00f6nheit mir,", "tokens": ["Hol\u00b7te", "die", "schim\u00b7mern\u00b7de", "Sch\u00f6n\u00b7heit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Bettete tief sie ins Herz mir ein.", "tokens": ["Bet\u00b7te\u00b7te", "tief", "sie", "ins", "Herz", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Rotes Moor in schmalen Strichen,", "tokens": ["Ro\u00b7tes", "Moor", "in", "schma\u00b7len", "Stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lilafarbener Sammt lockerer Fr\u00fchlingsackererde", "tokens": ["Li\u00b7la\u00b7far\u00b7be\u00b7ner", "Sammt", "lo\u00b7cke\u00b7rer", "Fr\u00fch\u00b7lings\u00b7a\u00b7cker\u00b7er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Weich dazwischen gebreitet;", "tokens": ["Weich", "da\u00b7zwi\u00b7schen", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Junges, lachendes Wiesengr\u00fcn", "tokens": ["Jun\u00b7ges", ",", "la\u00b7chen\u00b7des", "Wie\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wellig hineingeschlungen:", "tokens": ["Wel\u00b7lig", "hin\u00b7ein\u00b7ge\u00b7schlun\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Freudebanner der jubelnden Hoffnung", "tokens": ["Freu\u00b7de\u00b7ban\u00b7ner", "der", "ju\u00b7beln\u00b7den", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In des Keimdrangs br\u00e4utlich leuchtender,", "tokens": ["In", "des", "Keim\u00b7drangs", "br\u00e4ut\u00b7lich", "leuch\u00b7ten\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Lustiger Farbe.", "tokens": ["Lus\u00b7ti\u00b7ger", "Far\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcssig glitzerbewegtes Silber", "tokens": ["Fl\u00fcs\u00b7sig", "glit\u00b7zer\u00b7be\u00b7weg\u00b7tes", "Sil\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hurtig eilenden Wassers blinkt", "tokens": ["Hur\u00b7tig", "ei\u00b7len\u00b7den", "Was\u00b7sers", "blinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In weiten Windungen bogengeschlungen:", "tokens": ["In", "wei\u00b7ten", "Win\u00b7dun\u00b7gen", "bo\u00b7gen\u00b7ge\u00b7schlun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ich dich liebe mit jauchzender Seele,", "tokens": ["Wie", "ich", "dich", "lie\u00b7be", "mit", "jauch\u00b7zen\u00b7der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Oh du frische, rauschende, fr\u00f6hliche,", "tokens": ["Oh", "du", "fri\u00b7sche", ",", "rau\u00b7schen\u00b7de", ",", "fr\u00f6h\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Tummelnde Freiheit!", "tokens": ["Tum\u00b7meln\u00b7de", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gr\u00fcnbehauchte Weiherspiegel", "tokens": ["Gr\u00fcn\u00b7be\u00b7hauch\u00b7te", "Wei\u00b7her\u00b7spie\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinnen tiefen, stillen Traum", "tokens": ["Sin\u00b7nen", "tie\u00b7fen", ",", "stil\u00b7len", "Traum"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mitten in der \u00fcberm\u00fctigen Farbenheiterkeit.", "tokens": ["Mit\u00b7ten", "in", "der", "\u00fc\u00b7berm\u00b7\u00fc\u00b7ti\u00b7gen", "Far\u00b7ben\u00b7hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Dunkle, trotzige W\u00e4ldermassen,", "tokens": ["Dunk\u00b7le", ",", "trot\u00b7zi\u00b7ge", "W\u00e4l\u00b7der\u00b7mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Braun,", "tokens": ["Braun", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Breit,", "tokens": ["Breit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Br\u00fcten gewaltigen Ernst und das dunkle", "tokens": ["Br\u00fc\u00b7ten", "ge\u00b7wal\u00b7ti\u00b7gen", "Ernst", "und", "das", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "KON", "ART", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Geheimnis wipfelumrauschter Einsamkeit.", "tokens": ["Ge\u00b7heim\u00b7nis", "wip\u00b7fel\u00b7um\u00b7rauschter", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Zwischenhinein hellrote D\u00e4cher,", "tokens": ["Zwi\u00b7schen\u00b7hin\u00b7ein", "hell\u00b7ro\u00b7te", "D\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+---++-+-", "measure": "dactylic.init"}, "line.16": {"text": "Bl\u00e4ulich wirbelnder Rauch daraus;", "tokens": ["Bl\u00e4u\u00b7lich", "wir\u00b7beln\u00b7der", "Rauch", "da\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PAV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Blitzende Fenster von Menschenh\u00e4usern", "tokens": ["Blit\u00b7zen\u00b7de", "Fens\u00b7ter", "von", "Men\u00b7schen\u00b7h\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Leuchten wie lachende Augen.", "tokens": ["Leuch\u00b7ten", "wie", "la\u00b7chen\u00b7de", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Aber weit, weit dr\u00fcber hinweg,", "tokens": ["A\u00b7ber", "weit", ",", "weit", "dr\u00fc\u00b7ber", "hin\u00b7weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "PAV", "APZR", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Weit, in duftiger blauender Ferne,", "tokens": ["Weit", ",", "in", "duf\u00b7ti\u00b7ger", "blau\u00b7en\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weit, oh weit \u00fcber dem Kleingespiel,", "tokens": ["Weit", ",", "oh", "weit", "\u00fc\u00b7ber", "dem", "Klein\u00b7ge\u00b7spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Starr,", "tokens": ["Starr", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Gewaltig,", "tokens": ["Ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Mit rissigen Schroffen,", "tokens": ["Mit", "ris\u00b7si\u00b7gen", "Schrof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In Schnee und Eis krystallen geh\u00fcllt,", "tokens": ["In", "Schnee", "und", "Eis", "krys\u00b7tal\u00b7len", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ragen die Alpen.", "tokens": ["Ra\u00b7gen", "die", "Al\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Stille, Stille \u00fcber dem Riesenrund.", "tokens": ["Stil\u00b7le", ",", "Stil\u00b7le", "\u00fc\u00b7ber", "dem", "Rie\u00b7sen\u00b7rund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ueber mir", "tokens": ["Ue\u00b7ber", "mir"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hoch in den L\u00fcften", "tokens": ["Hoch", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schreit ein Falke,", "tokens": ["Schreit", "ein", "Fal\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Langsam kreisend durch das tiefe L\u00fcfteblau.", "tokens": ["Lang\u00b7sam", "krei\u00b7send", "durch", "das", "tie\u00b7fe", "L\u00fcf\u00b7teb\u00b7lau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Stille, Stille .... die schweigende Sch\u00f6nheit", "tokens": ["Stil\u00b7le", ",", "Stil\u00b7le", "....", "die", "schwei\u00b7gen\u00b7de", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Atmet leise, voll. \u2013 Da schwebt", "tokens": ["At\u00b7met", "lei\u00b7se", ",", "voll", ".", "\u2013", "Da", "schwebt"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NE", "ADJD", "$,", "ADJD", "$.", "$(", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Tiefe der kleinen Stadt", "tokens": ["Aus", "der", "Tie\u00b7fe", "der", "klei\u00b7nen", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hell ein Singen empor, es klingt:", "tokens": ["Hell", "ein", "Sin\u00b7gen", "em\u00b7por", ",", "es", "klingt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "\u00bbder Mai ist gekommen\u00ab ....", "tokens": ["\u00bb", "der", "Mai", "ist", "ge\u00b7kom\u00b7men", "\u00ab", "...."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von Kinderlippen.", "tokens": ["Von", "Kin\u00b7der\u00b7lip\u00b7pen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "In enger Stube sitzen die Kleinen.", "tokens": ["In", "en\u00b7ger", "Stu\u00b7be", "sit\u00b7zen", "die", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sehe im Geiste die frischen roten", "tokens": ["Ich", "se\u00b7he", "im", "Geis\u00b7te", "die", "fri\u00b7schen", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "M\u00e4ulerchen sich gleichm\u00e4\u00dfig \u00f6ffnen,", "tokens": ["M\u00e4u\u00b7ler\u00b7chen", "sich", "gleich\u00b7m\u00e4\u00b7\u00dfig", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sehe den Lehrer die Fiedel streichen,", "tokens": ["Se\u00b7he", "den", "Leh\u00b7rer", "die", "Fie\u00b7del", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sehe die lustig mitsingenden Augen, \u2013", "tokens": ["Se\u00b7he", "die", "lus\u00b7tig", "mit\u00b7sin\u00b7gen\u00b7den", "Au\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Kindheit, Kindheit,", "tokens": ["Kind\u00b7heit", ",", "Kind\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Fr\u00f6hliche, frische,", "tokens": ["Fr\u00f6h\u00b7li\u00b7che", ",", "fri\u00b7sche", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Singende Unschuld!", "tokens": ["Sin\u00b7