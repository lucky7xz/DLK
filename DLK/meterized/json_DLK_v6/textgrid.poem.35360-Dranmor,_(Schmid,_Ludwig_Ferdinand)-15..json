{"textgrid.poem.35360": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar sprach ich oft zu mir: Sowie der kecke", "tokens": ["Zwar", "sprach", "ich", "oft", "zu", "mir", ":", "So\u00b7wie", "der", "ke\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch kluge Schiffer in des Sturmes Toben,", "tokens": ["Doch", "klu\u00b7ge", "Schif\u00b7fer", "in", "des", "Stur\u00b7mes", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Wind und Flut sich gegen ihn erhoben", "tokens": ["Wenn", "Wind", "und", "Flut", "sich", "ge\u00b7gen", "ihn", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn verdr\u00e4ngen wollen vom Verdecke,", "tokens": ["Und", "ihn", "ver\u00b7dr\u00e4n\u00b7gen", "wol\u00b7len", "vom", "Ver\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich zornig gegen solchen Anprall steift", "tokens": ["Sich", "zor\u00b7nig", "ge\u00b7gen", "sol\u00b7chen", "An\u00b7prall", "steift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wassertriefend, fr\u00f6stelnd und verdrie\u00dflich", "tokens": ["Und", "was\u00b7ser\u00b7trie\u00b7fend", ",", "fr\u00f6s\u00b7telnd", "und", "ver\u00b7drie\u00df\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich an den Mastbaum binden l\u00e4\u00dft und schlie\u00dflich", "tokens": ["Sich", "an", "den", "Mast\u00b7baum", "bin\u00b7den", "l\u00e4\u00dft", "und", "schlie\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Siegreich die Stricke wieder von sich streift;", "tokens": ["Sieg\u00b7reich", "die", "Stri\u00b7cke", "wie\u00b7der", "von", "sich", "streift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "So, statt des Lebens St\u00fcrme zu bejammern,", "tokens": ["So", ",", "statt", "des", "Le\u00b7bens", "St\u00fcr\u00b7me", "zu", "be\u00b7jam\u00b7mern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Statt unbeh\u00fclflich \u00fcber Bord zu fliegen,", "tokens": ["Statt", "un\u00b7be\u00b7h\u00fcl\u00b7flich", "\u00fc\u00b7ber", "Bord", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Will ich fortan mein schwaches Herz bekriegen", "tokens": ["Will", "ich", "for\u00b7tan", "mein", "schwa\u00b7ches", "Herz", "be\u00b7krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und mich an der Erfahrung Mastbaum klammern.", "tokens": ["Und", "mich", "an", "der", "Er\u00b7fah\u00b7rung", "Mast\u00b7baum", "klam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "So will ich, ein verwitterter Matrose,", "tokens": ["So", "will", "ich", ",", "ein", "ver\u00b7wit\u00b7ter\u00b7ter", "Mat\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der gegen Ungemach sich kr\u00e4ftig stemmt,", "tokens": ["Der", "ge\u00b7gen", "Un\u00b7ge\u00b7mach", "sich", "kr\u00e4f\u00b7tig", "stemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bevor die gro\u00dfe Flut mich \u00fcberschwemmt,", "tokens": ["Be\u00b7vor", "die", "gro\u00b7\u00dfe", "Flut", "mich", "\u00fc\u00b7bersc\u00b7hwemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Dem Regen trotzen und der Wasserhose", "tokens": ["Dem", "Re\u00b7gen", "trot\u00b7zen", "und", "der", "Was\u00b7ser\u00b7ho\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und Freund wie Feind die Stirne bieten, starr", "tokens": ["Und", "Freund", "wie", "Feind", "die", "Stir\u00b7ne", "bie\u00b7ten", ",", "starr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "KOKOM", "NN", "ART", "NN", "VVINF", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und teilnahmslos, nicht Leidenschaften d\u00e4mpfend,", "tokens": ["Und", "teil\u00b7nahms\u00b7los", ",", "nicht", "Lei\u00b7den\u00b7schaf\u00b7ten", "d\u00e4mp\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Nur den Impuls, den innern Drang bek\u00e4mpfend,", "tokens": ["Nur", "den", "Im\u00b7puls", ",", "den", "in\u00b7nern", "Drang", "be\u00b7k\u00e4mp\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Damit es nicht mehr hei\u00dfe: Sei kein Narr!", "tokens": ["Da\u00b7mit", "es", "nicht", "mehr", "hei\u00b7\u00dfe", ":", "Sei", "kein", "Narr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$.", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "O Schw\u00e4rmer mit dem z\u00e4rtlichen Gem\u00fcte,", "tokens": ["O", "Schw\u00e4r\u00b7mer", "mit", "dem", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Dem P\u00f6bel, niedrig oder hoch geboren,", "tokens": ["Dem", "P\u00f6\u00b7bel", ",", "nied\u00b7rig", "o\u00b7der", "hoch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der gegen Deinesgleichen sich verschworen,", "tokens": ["Der", "ge\u00b7gen", "Dei\u00b7nes\u00b7glei\u00b7chen", "sich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Dem gibst du deines Geistes beste Bl\u00fcte?", "tokens": ["Dem", "gibst", "du", "dei\u00b7nes", "Geis\u00b7tes", "bes\u00b7te", "Bl\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die deiner spotten, liebst du wie Geschwister?", "tokens": ["Die", "dei\u00b7ner", "spot\u00b7ten", ",", "liebst", "du", "wie", "Ge\u00b7schwis\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "$,", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Den Mann, den du gekleidet und gespeist,", "tokens": ["Den", "Mann", ",", "den", "du", "ge\u00b7klei\u00b7det", "und", "ge\u00b7speist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und der zum Lohne dir die Z\u00e4hne weist?", "tokens": ["Und", "der", "zum", "Loh\u00b7ne", "dir", "die", "Z\u00e4h\u00b7ne", "weist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und dann die K\u00fchlen, Frommen, die Philister,", "tokens": ["Und", "dann", "die", "K\u00fch\u00b7len", ",", "From\u00b7men", ",", "die", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die Neider, die Verfolger, die Ver\u00e4chter", "tokens": ["Die", "Nei\u00b7der", ",", "die", "Ver\u00b7fol\u00b7ger", ",", "die", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und Jeden, der, wenn dich des Schicksals B\u00fcrde", "tokens": ["Und", "Je\u00b7den", ",", "der", ",", "wenn", "dich", "des", "Schick\u00b7sals", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "$,", "PRELS", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Zu Boden dr\u00fcckte, dich mit Hohngel\u00e4chter", "tokens": ["Zu", "Bo\u00b7den", "dr\u00fcck\u00b7te", ",", "dich", "mit", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Hinunter in die Tiefe sto\u00dfen w\u00fcrde?", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Tie\u00b7fe", "sto\u00b7\u00dfen", "w\u00fcr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ich habe solche Stimmen nicht mi\u00dfachtet;", "tokens": ["Ich", "ha\u00b7be", "sol\u00b7che", "Stim\u00b7men", "nicht", "mi\u00df\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Sie sagten Unleugbares, l\u00e4ngst Geahntes;", "tokens": ["Sie", "sag\u00b7ten", "Un\u00b7leug\u00b7ba\u00b7res", ",", "l\u00e4ngst", "Ge\u00b7ahn\u00b7tes", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Sie haben, unter Schmerzen Angebahntes", "tokens": ["Sie", "ha\u00b7ben", ",", "un\u00b7ter", "Schmer\u00b7zen", "An\u00b7ge\u00b7bahn\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Vollendend, mein Gem\u00fct mit Gram umnachtet.", "tokens": ["Voll\u00b7en\u00b7dend", ",", "mein", "Ge\u00b7m\u00fct", "mit", "Gram", "um\u00b7nach\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und ich zerbrach den leuchtenden Altar,", "tokens": ["Und", "ich", "zer\u00b7brach", "den", "leuch\u00b7ten\u00b7den", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Vor dem ich falscher Br\u00fcder Schmerz gehuldigt;", "tokens": ["Vor", "dem", "ich", "fal\u00b7scher", "Br\u00fc\u00b7der", "Schmerz", "ge\u00b7hul\u00b7digt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ich, den man blinden Mitgef\u00fchls beschuldigt,", "tokens": ["Ich", ",", "den", "man", "blin\u00b7den", "Mit\u00b7ge\u00b7f\u00fchls", "be\u00b7schul\u00b7digt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Bin nicht der gute Junge, der ich war;", "tokens": ["Bin", "nicht", "der", "gu\u00b7te", "Jun\u00b7ge", ",", "der", "ich", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Denn, wenn ich meine Arme ausgebreitet,", "tokens": ["Denn", ",", "wenn", "ich", "mei\u00b7ne", "Ar\u00b7me", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Die ganze Menschheit liebend zu umfangen,", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "lie\u00b7bend", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "Hat stets ein Judas-Ku\u00df auf meinen Wangen", "tokens": ["Hat", "stets", "ein", "Ju\u00b7das\u00b7Ku\u00df", "auf", "mei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Gebrannt und auf Verrat mich vorbereitet.", "tokens": ["Ge\u00b7brannt", "und", "auf", "Ver\u00b7rat", "mich", "vor\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Es ist gewitzigt nun, was ihr mi\u00dfhandelt,", "tokens": ["Es", "ist", "ge\u00b7wit\u00b7zigt", "nun", ",", "was", "ihr", "mi\u00df\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Des Dichters Herz, das keiner unterjocht,", "tokens": ["Des", "Dich\u00b7ters", "Herz", ",", "das", "kei\u00b7ner", "un\u00b7ter\u00b7jocht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Seit es gepanzert euch entgegenpocht,", "tokens": ["Seit", "es", "ge\u00b7pan\u00b7zert", "euch", "ent\u00b7ge\u00b7gen\u00b7pocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Seit es, wie eure, sich in Erz verwandelt.", "tokens": ["Seit", "es", ",", "wie", "eu\u0