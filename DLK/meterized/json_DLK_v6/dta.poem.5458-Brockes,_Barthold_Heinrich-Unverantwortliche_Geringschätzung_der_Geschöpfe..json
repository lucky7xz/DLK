{"dta.poem.5458": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unverantwortliche Geringsch\u00e4tzung  \n der Gesch\u00f6pfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lange bist du doch in deiner Lebens-Zeit", "tokens": ["Wie", "lan\u00b7ge", "bist", "du", "doch", "in", "dei\u00b7ner", "Le\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seh\u2019nden Augen blind, und bleibst der Eitelkeit,", "tokens": ["Mit", "seh'n\u00b7den", "Au\u00b7gen", "blind", ",", "und", "bleibst", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Uebermuths und der Gewohnheit Knecht?", "tokens": ["Des", "Ue\u00b7ber\u00b7muths", "und", "der", "Ge\u00b7wohn\u00b7heit", "Knecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es scheint ein Bl\u00fcmchen dir zu schlecht,", "tokens": ["Es", "scheint", "ein", "Bl\u00fcm\u00b7chen", "dir", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bl\u00e4ttgen scheint dir zu geringe,", "tokens": ["Ein", "Bl\u00e4tt\u00b7gen", "scheint", "dir", "zu", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kn\u00f6spchen scheinet dir zu klein,", "tokens": ["Ein", "Kn\u00f6spc\u00b7hen", "schei\u00b7net", "dir", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Gr\u00e4schen deines Geists und Denckens wehrt zu seyn;", "tokens": ["Kein", "Gr\u00e4sc\u00b7hen", "dei\u00b7nes", "Geists", "und", "Den\u00b7ckens", "wehrt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da doch dem allerkleinsten Dinge,", "tokens": ["Da", "doch", "dem", "al\u00b7ler\u00b7kleins\u00b7ten", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn man es mit Vernunft erwegt,", "tokens": ["Wenn", "man", "es", "mit", "Ver\u00b7nunft", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein kr\u00e4ftiger Beweis vom Sch\u00f6pfer eingepr\u00e4gt.", "tokens": ["Ein", "kr\u00e4f\u00b7ti\u00b7ger", "Be\u00b7weis", "vom", "Sch\u00f6p\u00b7fer", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "L\u00e4\u00dft jegliches Gew\u00e4chs nun unsre Seelen,", "tokens": ["L\u00e4\u00dft", "jeg\u00b7li\u00b7ches", "Ge\u00b7w\u00e4chs", "nun", "uns\u00b7re", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn man nur ", "tokens": ["Wenn", "man", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So kann es folglich auch nicht fehlen", "tokens": ["So", "kann", "es", "folg\u00b7lich", "auch", "nicht", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr mu\u00df aus jeglichem, Lust, Lieb\u2019 und Lob entstehn.", "tokens": ["Ihr", "mu\u00df", "aus", "jeg\u00b7li\u00b7chem", ",", "Lust", ",", "Lieb'", "und", "Lob", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Indem ein sich also betragendes Gem\u00fcth", "tokens": ["In\u00b7dem", "ein", "sich", "al\u00b7so", "be\u00b7tra\u00b7gen\u00b7des", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PRF", "ADV", "ADJA", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Darinnen \u00fcberall ein G\u00f6ttlich Licht entdecket,", "tokens": ["Da\u00b7rin\u00b7nen", "\u00fc\u00b7be\u00b7rall", "ein", "G\u00f6tt\u00b7lich", "Licht", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In allem seine Macht und weise Liebe sieht,", "tokens": ["In", "al\u00b7lem", "sei\u00b7ne", "Macht", "und", "wei\u00b7se", "Lie\u00b7be", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seine Freundlichkeit in allem schmecket.", "tokens": ["Und", "sei\u00b7ne", "Freund\u00b7lich\u00b7keit", "in", "al\u00b7lem", "schme\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}