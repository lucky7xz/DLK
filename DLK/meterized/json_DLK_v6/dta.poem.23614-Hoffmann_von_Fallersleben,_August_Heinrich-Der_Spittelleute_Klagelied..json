{"dta.poem.23614": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Spittelleute Klagelied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wir m\u00fcssen Morgens fr\u00fch aufstehn,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "Mor\u00b7gens", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn wir das Gebet gesprochen,", "tokens": ["Und", "wenn", "wir", "das", "Ge\u00b7bet", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwei Eimer Wasser holen gehn", "tokens": ["Zwei", "Ei\u00b7mer", "Was\u00b7ser", "ho\u00b7len", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unsre Morgensuppe kochen.", "tokens": ["Und", "uns\u00b7re", "Mor\u00b7gen\u00b7sup\u00b7pe", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dann m\u00fcssen wir um halber zehn", "tokens": ["Dann", "m\u00fcs\u00b7sen", "wir", "um", "hal\u00b7ber", "zehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "APPO", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An unser Tagewerk gleich schreiten,", "tokens": ["An", "un\u00b7ser", "Ta\u00b7ge\u00b7werk", "gleich", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wiedrum an dem Heerde stehn", "tokens": ["Und", "wie\u00b7drum", "an", "dem", "Heer\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unser Mittagsmahl bereiten.", "tokens": ["Und", "un\u00b7ser", "Mit\u00b7tags\u00b7mahl", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Kaum ist das Mahl genommen ein,", "tokens": ["Kaum", "ist", "das", "Mahl", "ge\u00b7nom\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum kann man sich des Schlafs erwehren,", "tokens": ["Kaum", "kann", "man", "sich", "des", "Schlafs", "er\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich mu\u00df man wieder munter sein,", "tokens": ["Gleich", "mu\u00df", "man", "wie\u00b7der", "mun\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Vesperbr\u00f6tchen zu verzehren.", "tokens": ["Das", "Ves\u00b7per\u00b7br\u00f6t\u00b7chen", "zu", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ist nun auch endlich das geschehn,", "tokens": ["Ist", "nun", "auch", "end\u00b7lich", "das", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ART", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird es Abend unterdessen,", "tokens": ["So", "wird", "es", "A\u00b7bend", "un\u00b7ter\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00f6chten gern zu Bette gehn,", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "gern", "zu", "Bet\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00fcssen noch zu Nacht erst essen.", "tokens": ["Und", "m\u00fcs\u00b7sen", "noch", "zu", "Nacht", "erst", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gottlob! bald endigt sich die Noth!", "tokens": ["Gott\u00b7lob", "!", "bald", "en\u00b7digt", "sich", "die", "Noth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So denkt man wohl, o ja \u2014 mit Nichten!", "tokens": ["So", "denkt", "man", "wohl", ",", "o", "ja", "mit", "Nich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "FM", "ADV", "$(", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir m\u00fcssen nach dem Abendbrot", "tokens": ["Wir", "m\u00fcs\u00b7sen", "nach", "dem", "A\u00b7bend\u00b7brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst unsre Andacht noch verrichten.", "tokens": ["Erst", "uns\u00b7re", "An\u00b7dacht", "noch", "ver\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir armen Spittelleute,", "tokens": ["Wir", "ar\u00b7men", "Spit\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was haben ", "tokens": ["Was", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nun ist es doch zum Ausruhn Zeit!", "tokens": ["Nun", "ist", "es", "doch", "zum", "Aus\u00b7ruhn", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O nein! wir d\u00fcrfen noch nicht schlafen;", "tokens": ["O", "nein", "!", "wir", "d\u00fcr\u00b7fen", "noch", "nicht", "schla\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Spittelmeister l\u00e4rmt und schreit:", "tokens": ["Der", "Spit\u00b7tel\u00b7meis\u00b7ter", "l\u00e4rmt", "und", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst reinigt Teller, Krug und Hafen!", "tokens": ["Erst", "rei\u00b7nigt", "Tel\u00b7ler", ",", "Krug", "und", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}