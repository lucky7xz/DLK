{"textgrid.poem.30355": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob meine Sinngetichte mit Tausenden gleich gehen,", "tokens": ["Ob", "mei\u00b7ne", "Sinn\u00b7ge\u00b7tich\u00b7te", "mit", "Tau\u00b7sen\u00b7den", "gleich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So dencke, wie viel Tausend der Augen gegen stehen!", "tokens": ["So", "den\u00b7cke", ",", "wie", "viel", "Tau\u00b7send", "der", "Au\u00b7gen", "ge\u00b7gen", "ste\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PIAT", "NN", "ART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich lasse mir gen\u00fcgen, ob ihrer viel gleich fallen,", "tokens": ["Ich", "las\u00b7se", "mir", "ge\u00b7n\u00fc\u00b7gen", ",", "ob", "ih\u00b7rer", "viel", "gleich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo nur noch Platz behalten die t\u00fcchtigsten von allen.", "tokens": ["Wo", "nur", "noch", "Platz", "be\u00b7hal\u00b7ten", "die", "t\u00fcch\u00b7tigs\u00b7ten", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NN", "VVFIN", "ART", "ADJA", "APPR", "PIAT", "$."], "meter": "-+-+-+--++--+-", "measure": "iambic.hexa.relaxed"}}}}}