{"dta.poem.11632": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn meine Mutter Frau Kaiserin w\u00e4r',", "tokens": ["Und", "wenn", "mei\u00b7ne", "Mut\u00b7ter", "Frau", "Kai\u00b7se\u00b7rin", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN", "NE", "VAFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4tt' ich Kleider und seidene Schuh',", "tokens": ["H\u00e4tt'", "ich", "Klei\u00b7der", "und", "sei\u00b7de\u00b7ne", "Schuh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ich g\u00e4b' doch den herzigen Jungen nicht her,", "tokens": ["Ich", "g\u00e4b'", "doch", "den", "her\u00b7zi\u00b7gen", "Jun\u00b7gen", "nicht", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "G\u00e4b' ihm Kron' und Zepter dazu.", "tokens": ["G\u00e4b'", "ihm", "Kron'", "und", "Zep\u00b7ter", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}