{"dta.poem.5308": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gelbe Mah.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bl\u00fchest du nun auch in deiner g\u00fcldnen Pracht,", "tokens": ["So", "bl\u00fc\u00b7hest", "du", "nun", "auch", "in", "dei\u00b7ner", "g\u00fcld\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, der so dich, als uns, und alle Welt gemacht,", "tokens": ["Dem", ",", "der", "so", "dich", ",", "als", "uns", ",", "und", "al\u00b7le", "Welt", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "PPER", "$,", "KOUS", "PPER", "$,", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch hier, zum ersten mahl, zum Preis und Ruhme,", "tokens": ["Auch", "hier", ",", "zum", "ers\u00b7ten", "mahl", ",", "zum", "Preis", "und", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "ADJA", "ADV", "$,", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gold-gelber Mah, Bewunderns-wehrte Blume,", "tokens": ["Gold\u00b7gel\u00b7ber", "Mah", ",", "Be\u00b7wun\u00b7derns\u00b7wehr\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die du vor kurtzer Zeit auf der Chineser Gr\u00e4ntzen", "tokens": ["Die", "du", "vor", "kurt\u00b7zer", "Zeit", "auf", "der", "Chi\u00b7ne\u00b7ser", "Gr\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die glatten Bl\u00e4tter liessest gl\u00e4ntzen!", "tokens": ["Die", "glat\u00b7ten", "Bl\u00e4t\u00b7ter", "lies\u00b7sest", "gl\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du, die noch nie ein Aug in Teutschland jemahls sah!", "tokens": ["Du", ",", "die", "noch", "nie", "ein", "Aug", "in", "Teutschland", "je\u00b7mahls", "sah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Da du vorhin so fern, bist mir anjetzt so nah!", "tokens": ["Da", "du", "vor\u00b7hin", "so", "fern", ",", "bist", "mir", "an\u00b7jetzt", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Auf welche wunderbahre Weise", "tokens": ["Auf", "wel\u00b7che", "wun\u00b7der\u00b7bah\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vollf\u00fchrtest du solch\u2019 eine lange Reise?", "tokens": ["Voll\u00b7f\u00fchr\u00b7test", "du", "solch'", "ei\u00b7ne", "lan\u00b7ge", "Rei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nie gnug gepriesner ", "tokens": ["Nie", "gnug", "ge\u00b7pri\u00b7es\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Dem, an Erfahrung, Geist und Kunst, fast keiner", "tokens": ["Dem", ",", "an", "Er\u00b7fah\u00b7rung", ",", "Geist", "und", "Kunst", ",", "fast", "kei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "gleich,", "tokens": ["gleich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Durch deiner edlen Neu-Begier", "tokens": ["Durch", "dei\u00b7ner", "ed\u00b7len", "Neu\u00b7Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Vern\u00fcnft\u2019gen Trieb, erblicken wir", "tokens": ["Ver\u00b7n\u00fcnft'\u00b7gen", "Trieb", ",", "er\u00b7bli\u00b7cken", "wir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nunmehr auch hier", "tokens": ["Nun\u00b7mehr", "auch", "hier"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Des gelben Mah Betrachtungs-wehrte Zier.", "tokens": ["Des", "gel\u00b7ben", "Mah", "Be\u00b7trach\u00b7tungs\u00b7wehr\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er hat mit Achtsamkeit dich dorten bl\u00fchen sehn,", "tokens": ["Er", "hat", "mit", "Acht\u00b7sam\u00b7keit", "dich", "dor\u00b7ten", "bl\u00fc\u00b7hen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geliebte Blum\u2019, er fand dein frembdes Wesen sch\u00f6n", "tokens": ["Ge\u00b7lieb\u00b7te", "Blum'", ",", "er", "fand", "dein", "fremb\u00b7des", "We\u00b7sen", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wehrt, da\u00df dein Geschlechte", "tokens": ["Und", "wehrt", ",", "da\u00df", "dein", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Von andern V\u00f6lckern auch gesehen werden m\u00f6gte.", "tokens": ["Von", "an\u00b7dern", "V\u00f6l\u00b7ckern", "auch", "ge\u00b7se\u00b7hen", "wer\u00b7den", "m\u00f6g\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum nahm er, da es nicht dein zartes Wesen litt,", "tokens": ["Drum", "nahm", "er", ",", "da", "es", "nicht", "dein", "zar\u00b7tes", "We\u00b7sen", "litt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dich selbst, in deinem Flor, mit sich zu nehmen,", "tokens": ["Dich", "selbst", ",", "in", "dei\u00b7nem", "Flor", ",", "mit", "sich", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Ur-Kraft, in dem Saamen, mit", "tokens": ["Die", "Ur\u00b7Kraft", ",", "in", "dem", "Saa\u00b7men", ",", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}}}}