{"dta.poem.18309": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An den's gerichtet ist!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und h\u00e4tt' ich bei Hofe Credit,", "tokens": ["Und", "h\u00e4tt'", "ich", "bei", "Ho\u00b7fe", "Cre\u00b7dit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ich g\u00e4be dir f\u00fcr dein gro\u00dfes Maul", "tokens": ["Ich", "g\u00e4\u00b7be", "dir", "f\u00fcr", "dein", "gro\u00b7\u00dfes", "Maul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Orden ", "tokens": ["Den", "Or\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und doch; vergeblich dein Ringen nach Ruhm,", "tokens": ["Und", "doch", ";", "ver\u00b7geb\u00b7lich", "dein", "Rin\u00b7gen", "nach", "Ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADJD", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Nebel verbleicht dein Glanz", "tokens": ["Zum", "Ne\u00b7bel", "ver\u00b7bleicht", "dein", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor dem Sigl'schen Mauldreckschleuderthum", "tokens": ["Vor", "dem", "Sigl'\u00b7schen", "Maul\u00b7dreck\u00b7schleu\u00b7der\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Des \u201eBairischen Vaterlands.\u201c", "tokens": ["Des", "\u201e", "Bai\u00b7ri\u00b7schen", "Va\u00b7ter\u00b7lands", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}