{"textgrid.poem.38598": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6ttlicher J\u00fcngling, umleuchtet vom sterbenden Scheine des Abends,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttlicher J\u00fcngling, umleuchtet vom sterbenden Scheine des Abends,", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", ",", "um\u00b7leuch\u00b7tet", "vom", "ster\u00b7ben\u00b7den", "Schei\u00b7ne", "des", "A\u00b7bends", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Weilst du im einsamen Thal, unter des Felsens Gestr\u00e4uch;", "tokens": ["Weilst", "du", "im", "ein\u00b7sa\u00b7men", "Thal", ",", "un\u00b7ter", "des", "Fel\u00b7sens", "Ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Senkst den flammenden Blick, voll heiliger Stille des Geistes,", "tokens": ["Senkst", "den", "flam\u00b7men\u00b7den", "Blick", ",", "voll", "hei\u00b7li\u00b7ger", "Stil\u00b7le", "des", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Auf des fliehenden Bachs d\u00fcsterbeschattete Fluth!", "tokens": ["Auf", "des", "flie\u00b7hen\u00b7den", "Bachs", "d\u00fcs\u00b7ter\u00b7be\u00b7schat\u00b7te\u00b7te", "Fluth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Himmelserscheinung auf Erden! im Irrgang' des wechselnden Schiksals", "tokens": ["Him\u00b7mel\u00b7ser\u00b7schei\u00b7nung", "auf", "Er\u00b7den", "!", "im", "Irr\u00b7gang'", "des", "wech\u00b7seln\u00b7den", "Schik\u00b7sals"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Str\u00f6m' \u00fcberwindende Kraft, Ahndung und Ruh' mir ins Herz!", "tokens": ["Str\u00f6m'", "\u00fc\u00b7berw\u00b7in\u00b7den\u00b7de", "Kraft", ",", "Ahn\u00b7dung", "und", "Ruh'", "mir", "ins", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "KON", "NN", "PPER", "APPRART", "NN", "$."], "meter": "----+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sieh'! ich eilte dir zu, die Seele voll st\u00fcrmenden Unmuths;", "tokens": ["Sieh'", "!", "ich", "eil\u00b7te", "dir", "zu", ",", "die", "See\u00b7le", "voll", "st\u00fcr\u00b7men\u00b7den", "Un\u00b7muths", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Stiller und besser durch dich kehr' ich zur Heimath zur\u00fcck!", "tokens": ["Stil\u00b7ler", "und", "bes\u00b7ser", "durch", "dich", "kehr'", "ich", "zur", "Hei\u00b7math", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}