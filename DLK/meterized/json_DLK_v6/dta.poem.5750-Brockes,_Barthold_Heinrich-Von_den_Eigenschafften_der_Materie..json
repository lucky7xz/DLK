{"dta.poem.5750": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind blos die \u00e4ussre Fl\u00e4ch der C\u00f6rper, welche sich", "tokens": ["Sind", "blos", "die", "\u00e4uss\u00b7re", "Fl\u00e4ch", "der", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rings um dieselbe legen:", "tokens": ["Rings", "um", "die\u00b7sel\u00b7be", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Woraus man billig schliesst,", "tokens": ["Wo\u00b7raus", "man", "bil\u00b7lig", "schliesst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir es recht erwegen,", "tokens": ["Wenn", "wir", "es", "recht", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df alles angef\u00fcllt. Und eben hieraus fliesst,", "tokens": ["Da\u00df", "al\u00b7les", "an\u00b7ge\u00b7f\u00fcllt", ".", "Und", "e\u00b7ben", "hier\u00b7aus", "fliesst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$.", "KON", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df, wenn der Geist gedenckt auf eine Weite,", "tokens": ["Da\u00df", ",", "wenn", "der", "Geist", "ge\u00b7denckt", "auf", "ei\u00b7ne", "Wei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die unerm\u00e4\u00dflich ist; er anders nichts gedencke,", "tokens": ["Die", "un\u00b7er\u00b7m\u00e4\u00df\u00b7lich", "ist", ";", "er", "an\u00b7ders", "nichts", "ge\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$.", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als da\u00df blos die Materie", "tokens": ["Als", "da\u00df", "blos", "die", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohn\u2019 End ins unerm\u00e4\u00dfliche", "tokens": ["Ohn'", "End", "ins", "un\u00b7er\u00b7m\u00e4\u00df\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Sich dehne, senck und lencke,", "tokens": ["Sich", "deh\u00b7ne", ",", "senck", "und", "len\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PDS", "$,", "VVIMP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und aller Orten sich verbreite.", "tokens": ["Und", "al\u00b7ler", "Or\u00b7ten", "sich", "ver\u00b7brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}