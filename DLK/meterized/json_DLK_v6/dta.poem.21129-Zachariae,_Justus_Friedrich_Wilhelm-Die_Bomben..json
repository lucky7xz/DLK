{"dta.poem.21129": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Bomben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, schrecklich flieht sie dahin die alles zerschmet-\nternde Bombe!", "tokens": ["Sieh", ",", "schreck\u00b7lich", "flieht", "sie", "da\u00b7hin", "die", "al\u00b7les", "zer\u00b7schmet", "tern\u00b7de", "Bom\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "PPER", "PAV", "ART", "PIS", "TRUNC", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie spr\u00fcht Verderben und Tod aus ihrem entz\u00fcndeten", "tokens": ["Sie", "spr\u00fcht", "Ver\u00b7der\u00b7ben", "und", "Tod", "aus", "ih\u00b7rem", "ent\u00b7z\u00fcn\u00b7de\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus ihrem Bauche schwingt sich die ungeheure Ver-", "tokens": ["Aus", "ih\u00b7rem", "Bau\u00b7che", "schwingt", "sich", "die", "un\u00b7ge\u00b7heu\u00b7re", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jhr Athem t\u00f6dtet, wie die Pest.", "tokens": ["Ihr", "A\u00b7them", "t\u00f6d\u00b7tet", ",", "wie", "die", "Pest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So st\u00fcrmt sie grausam und wild in nie eroberte St\u00e4d-", "tokens": ["So", "st\u00fcrmt", "sie", "grau\u00b7sam", "und", "wild", "in", "nie", "er\u00b7o\u00b7ber\u00b7te", "St\u00e4d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Donnern der Mitternacht gleich zertr\u00fcmmert sie", "tokens": ["Den", "Don\u00b7nern", "der", "Mit\u00b7ter\u00b7nacht", "gleich", "zer\u00b7tr\u00fcm\u00b7mert", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Streut Flammen \u00fcber die Stadt; verw\u00fcstet heilige", "tokens": ["Streut", "Flam\u00b7men", "\u00fc\u00b7ber", "die", "Stadt", ";", "ver\u00b7w\u00fcs\u00b7tet", "hei\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$.", "VVFIN", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und st\u00fcrzet Schl\u00f6sser in den Staub.", "tokens": ["Und", "st\u00fcr\u00b7zet", "Schl\u00f6s\u00b7ser", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entflammend w\u00fchlt sie sich ietzt in Vorrathsh\u00e4user von", "tokens": ["Ent\u00b7flam\u00b7mend", "w\u00fchlt", "sie", "sich", "ietzt", "in", "Vor\u00b7raths\u00b7h\u00e4u\u00b7ser", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Steine, Funken und Rauch, und wilde schmettern-", "tokens": ["Und", "Stei\u00b7ne", ",", "Fun\u00b7ken", "und", "Rauch", ",", "und", "wil\u00b7de", "schmet\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verbreiten gleich Blitzen den Tod; und eine Nacht der", "tokens": ["Ver\u00b7brei\u00b7ten", "gleich", "Blit\u00b7zen", "den", "Tod", ";", "und", "ei\u00b7ne", "Nacht", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "ART", "NN", "$.", "KON", "ART", "NN", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bedeckt mit Schutt und Graus die Stadt.", "tokens": ["Be\u00b7deckt", "mit", "Schutt", "und", "Graus", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So machen Sterbliche sich zu himmelst\u00fcrmenden", "tokens": ["So", "ma\u00b7chen", "Sterb\u00b7li\u00b7che", "sich", "zu", "him\u00b7mel\u00b7st\u00fcr\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie rauben der r\u00e4chenden Hand des Himmels die stra-", "tokens": ["Sie", "rau\u00b7ben", "der", "r\u00e4\u00b7chen\u00b7den", "Hand", "des", "Him\u00b7mels", "die", "stra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und w\u00fcten wider sich selbst mit Flammen des schwar-", "tokens": ["Und", "w\u00fc\u00b7ten", "wi\u00b7der", "sich", "selbst", "mit", "Flam\u00b7men", "des", "schwa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADV", "APPR", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wafnen sich mit H\u00f6llenblitz.", "tokens": ["Und", "waf\u00b7nen", "sich", "mit", "H\u00f6l\u00b7len\u00b7blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}