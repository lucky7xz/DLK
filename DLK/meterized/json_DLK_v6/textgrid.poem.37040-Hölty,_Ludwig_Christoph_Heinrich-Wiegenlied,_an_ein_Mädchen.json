{"textgrid.poem.37040": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied, an ein M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch schlinget dich die s\u00fc\u00dfe Ruh", "tokens": ["Noch", "schlin\u00b7get", "dich", "die", "s\u00fc\u00b7\u00dfe", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren Arm. Vergn\u00fcgt,", "tokens": ["In", "ih\u00b7ren", "Arm", ".", "Ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein kleines P\u00fcppchen, schlummerst du,", "tokens": ["Mein", "klei\u00b7nes", "P\u00fcpp\u00b7chen", ",", "schlum\u00b7merst", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dich die Amme wiegt.", "tokens": ["Wenn", "dich", "die", "Am\u00b7me", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf deinen Wangen keimet schon", "tokens": ["Auf", "dei\u00b7nen", "Wan\u00b7gen", "kei\u00b7met", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sanftes Morgenroth,", "tokens": ["Ein", "sanf\u00b7tes", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, wenn 12 Lenze dir entflohn,", "tokens": ["Das", ",", "wenn", "12", "Len\u00b7ze", "dir", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "number", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "CARD", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit sch\u00f6nen Fe\u00dfeln droht.", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Fe\u00b7\u00df\u00b7eln", "droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Um deine jungen Blicke schwebt", "tokens": ["Um", "dei\u00b7ne", "jun\u00b7gen", "Bli\u00b7cke", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein L\u00e4cheln, welches bald", "tokens": ["Ein", "L\u00e4\u00b7cheln", ",", "wel\u00b7ches", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Stutzer goldne Netze webt,", "tokens": ["Dem", "Stut\u00b7zer", "gold\u00b7ne", "Net\u00b7ze", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir entgegenwallt.", "tokens": ["Der", "dir", "ent\u00b7ge\u00b7gen\u00b7wallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann \u00f6fnen hundert Fenster sich,", "tokens": ["Dann", "\u00f6f\u00b7nen", "hun\u00b7dert", "Fens\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du am Fenster stehst,", "tokens": ["Wenn", "du", "am", "Fens\u00b7ter", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Blick auf Blick verfolget dich,", "tokens": ["Und", "Blick", "auf", "Blick", "ver\u00b7fol\u00b7get", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du zur Kirche gehst.", "tokens": ["Wenn", "du", "zur", "Kir\u00b7che", "gehst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Man lobt, von warmer Lieb entbrannt,", "tokens": ["Man", "lobt", ",", "von", "war\u00b7mer", "Lieb", "ent\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald deinen kleinen Fu\u00df,", "tokens": ["Bald", "dei\u00b7nen", "klei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald dein Gesicht, bald deine Hand,", "tokens": ["Bald", "dein", "Ge\u00b7sicht", ",", "bald", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht sich deinen Ku\u00df.", "tokens": ["Und", "w\u00fcnscht", "sich", "dei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du aber, holder als der May,", "tokens": ["Du", "a\u00b7ber", ",", "hol\u00b7der", "als", "der", "May", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich in Bl\u00fcthen h\u00fcllt,", "tokens": ["Der", "sich", "in", "Bl\u00fc\u00b7then", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein P\u00fcppchen, bleib der Unschuld treu,", "tokens": ["Mein", "P\u00fcpp\u00b7chen", ",", "bleib", "der", "Un\u00b7schuld", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die jetzt dein Herz erf\u00fcllt.", "tokens": ["Die", "jetzt", "dein", "Herz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es wimmre der Insektenchor", "tokens": ["Es", "wimm\u00b7re", "der", "In\u00b7sek\u00b7ten\u00b7chor"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Stutzervolks sich heisch!", "tokens": ["Des", "Stut\u00b7zer\u00b7volks", "sich", "heisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leih nie dein jungfr\u00e4uliches Ohr", "tokens": ["Leih", "nie", "dein", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dem summenden Ger\u00e4usch.", "tokens": ["Dem", "sum\u00b7men\u00b7den", "Ge\u00b7r\u00e4usch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Tugend, die der Himmel minnt,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "der", "Him\u00b7mel", "minnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00e4tz \u00fcber Gold und Rang.", "tokens": ["Sch\u00e4tz", "\u00fc\u00b7ber", "Gold", "und", "Rang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann sing ich dir, mein sch\u00f6nes Kind,", "tokens": ["Dann", "sing", "ich", "dir", ",", "mein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einst den Brautgesang.", "tokens": ["Noch", "einst", "den", "Braut\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}