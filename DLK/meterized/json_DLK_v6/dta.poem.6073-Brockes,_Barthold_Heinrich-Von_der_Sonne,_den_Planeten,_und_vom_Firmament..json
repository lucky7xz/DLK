{"dta.poem.6073": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht im Firmament ein Spiel bald nah, bald ferne.", "tokens": ["Man", "sieht", "im", "Fir\u00b7ma\u00b7ment", "ein", "Spiel", "bald", "nah", ",", "bald", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die C\u00f6rper \u00e4ndern stets f\u00fcr uns Gestalt und Weise.", "tokens": ["Die", "C\u00f6r\u00b7per", "\u00e4n\u00b7dern", "stets", "f\u00fcr", "uns", "Ge\u00b7stalt", "und", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir, die wir in dem dritten Creise", "tokens": ["Wir", ",", "die", "wir", "in", "dem", "drit\u00b7ten", "Crei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Best\u00e4ndig zwischen ihnen gehn,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "zwi\u00b7schen", "ih\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da unser Gang von ihrem sehr verschieden;", "tokens": ["Da", "un\u00b7ser", "Gang", "von", "ih\u00b7rem", "sehr", "ver\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So m\u00fcssen wir sie stets ver\u00e4ndert sehn.", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "sie", "stets", "ver\u00b7\u00e4n\u00b7dert", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sind alle C\u00f6rper gleich auf eine Art gef\u00fchret.", "tokens": ["Sind", "al\u00b7le", "C\u00f6r\u00b7per", "gleich", "auf", "ei\u00b7ne", "Art", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird etwan einst von uns versp\u00fcret,", "tokens": ["Wird", "et\u00b7wan", "einst", "von", "uns", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df einer in des Firmamentes H\u00f6h,", "tokens": ["Da\u00df", "ei\u00b7ner", "in", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In seinem Wege,", "tokens": ["In", "sei\u00b7nem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "An selbem Orte bleib\u2019 und sich nicht rege;", "tokens": ["An", "sel\u00b7bem", "Or\u00b7te", "bleib'", "und", "sich", "nicht", "re\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "PRF", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So bilden wir uns ein, als ob er stille steh,", "tokens": ["So", "bil\u00b7den", "wir", "uns", "ein", ",", "als", "ob", "er", "stil\u00b7le", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Daher wir ihn ", "tokens": ["Da\u00b7her", "wir", "ihn"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Wenn wir von ihm nun selber vorwerts rennen,", "tokens": ["Wenn", "wir", "von", "ihm", "nun", "sel\u00b7ber", "vor\u00b7werts", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "So bildet blos der Schein,", "tokens": ["So", "bil\u00b7det", "blos", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Als ob er r\u00fcckwerts eilt, uns ein,", "tokens": ["Als", "ob", "er", "r\u00fcck\u00b7werts", "eilt", ",", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dahero sie so dann den Nahmen", "tokens": ["Da\u00b7he\u00b7ro", "sie", "so", "dann", "den", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Sie scheinen offt geschwind, offt langsam fortzugehn.", "tokens": ["Sie", "schei\u00b7nen", "offt", "ge\u00b7schwind", ",", "offt", "lang\u00b7sam", "fort\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nachdem wir sie von andren Seiten sehn;", "tokens": ["Nach\u00b7dem", "wir", "sie", "von", "an\u00b7dren", "Sei\u00b7ten", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "So gl\u00e4ntzt ihr funckelnd Heer", "tokens": ["So", "gl\u00e4ntzt", "ihr", "fun\u00b7ckelnd", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Von der erborgten Gluht, bald weniger, bald mehr.", "tokens": ["Von", "der", "er\u00b7borg\u00b7ten", "Gluht", ",", "bald", "we\u00b7ni\u00b7ger", ",", "bald", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}