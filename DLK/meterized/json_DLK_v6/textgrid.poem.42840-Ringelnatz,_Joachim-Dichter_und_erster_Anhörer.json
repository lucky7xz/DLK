{"textgrid.poem.42840": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Dichter und erster Anh\u00f6rer", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie trugen zwei Sardellen", "tokens": ["Sie", "tru\u00b7gen", "zwei", "Sar\u00b7del\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Zu Grabe. \u2013 \u2013 \u00bbWer?\u00ab", "tokens": ["Zu", "Gra\u00b7be", ".", "\u2013", "\u2013", "\u00bb", "Wer", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "$(", "$(", "$(", "PWS", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Wellen,", "tokens": ["Die", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sie trugen sie vor sich her.", "tokens": ["Sie", "tru\u00b7gen", "sie", "vor", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwieso zu Grabe? Wohin denn?\u00ab", "tokens": ["\u00bb", "wie\u00b7so", "zu", "Gra\u00b7be", "?", "Wo\u00b7hin", "denn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "NN", "$.", "PWAV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Grabe, zur ewigen Ruh!", "tokens": ["Zu", "Gra\u00b7be", ",", "zur", "e\u00b7wi\u00b7gen", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbwohin?\u00ab \u2013 \u2013 Nun je nach den Winden,", "tokens": ["\u00bb", "wo\u00b7hin", "?", "\u00ab", "\u2013", "\u2013", "Nun", "je", "nach", "den", "Win\u00b7den", ","], "token_info": ["punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "$(", "$(", "$(", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vielleicht nach Afrika zu.", "tokens": ["Viel\u00b7leicht", "nach", "Af\u00b7ri\u00b7ka", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sie murmelten Weisen der Trauer", "tokens": ["Sie", "mur\u00b7mel\u00b7ten", "Wei\u00b7sen", "der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wegweit, tagaus und tagein.", "tokens": ["Weg\u00b7weit", ",", "ta\u00b7gaus", "und", "ta\u00b7gein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbda werden sie auf die Dauer", "tokens": ["\u00bb", "da", "wer\u00b7den", "sie", "auf", "die", "Dau\u00b7er"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl heiser geworden sein.\u00ab", "tokens": ["Wohl", "hei\u00b7ser", "ge\u00b7wor\u00b7den", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAPP", "VAINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Schwarz winkte am fernen Gestade", "tokens": ["Schwarz", "wink\u00b7te", "am", "fer\u00b7nen", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Grab \u2013 \u2013 \u2013 \u00bbUnd der Abend sinkt,", "tokens": ["Ein", "Grab", "\u2013", "\u2013", "\u2013", "\u00bb", "Und", "der", "A\u00b7bend", "sinkt", ","], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "$(", "$(", "$(", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und deine Sardellenballade,", "tokens": ["Und", "dei\u00b7ne", "Sar\u00b7del\u00b7len\u00b7bal\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(ganz offen gesprochen) die stinkt.\u00ab", "tokens": ["(", "ganz", "of\u00b7fen", "ge\u00b7spro\u00b7chen", ")", "die", "stinkt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VVPP", "$(", "ART", "VVFIN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}