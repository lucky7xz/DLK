{"dta.poem.631": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Cleliens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Porsenna r\u00fchmet mich/ und ehret meine That/", "tokens": ["Por\u00b7sen\u00b7na", "r\u00fch\u00b7met", "mich", "/", "und", "eh\u00b7ret", "mei\u00b7ne", "That", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Hat ein verwegnes Weib durch seine That vernichtet/", "tokens": ["Hat", "ein", "ver\u00b7weg\u00b7nes", "Weib", "durch", "sei\u00b7ne", "That", "ver\u00b7nich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es bringt die B\u00fcrgen durch und liefert sie der Stadl.", "tokens": ["Es", "bringt", "die", "B\u00fcr\u00b7gen", "durch", "und", "lie\u00b7fert", "sie", "der", "Stadl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}