{"textgrid.poem.48959": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Trinklied bei Hermanns Siegesschmaus", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, setzt euch um den Barden her,", "tokens": ["Kommt", ",", "setzt", "euch", "um", "den", "Bar\u00b7den", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jeder mit der tiefsten Schale;", "tokens": ["Ein", "je\u00b7der", "mit", "der", "tiefs\u00b7ten", "Scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es gilt auf k\u00fcnft'ge Siegesmahle!", "tokens": ["Es", "gilt", "auf", "k\u00fcnft'\u00b7ge", "Sie\u00b7ges\u00b7mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt, setzt euch um mich her.", "tokens": ["Kommt", ",", "setzt", "euch", "um", "mich", "her", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Singt freudig und aus voller Brust.", "tokens": ["Singt", "freu\u00b7dig", "und", "aus", "vol\u00b7ler", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rom liegt, zertreten vom Verderben:", "tokens": ["Rom", "liegt", ",", "zer\u00b7tre\u00b7ten", "vom", "Ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wir sind seiner F\u00e4sser Erben!", "tokens": ["Doch", "wir", "sind", "sei\u00b7ner", "F\u00e4s\u00b7ser", "Er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was fehlt uns noch zur Lust?", "tokens": ["Was", "fehlt", "uns", "noch", "zur", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schenkt ein den Wein, der sie gelabt!", "tokens": ["Schenkt", "ein", "den", "Wein", ",", "der", "sie", "ge\u00b7labt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind nicht kundig ihn zu halten;", "tokens": ["Wir", "sind", "nicht", "kun\u00b7dig", "ihn", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er k\u00f6nnte sauern und veralten:", "tokens": ["Er", "k\u00f6nn\u00b7te", "sau\u00b7ern", "und", "ver\u00b7al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum trinket, weil ihr habt.", "tokens": ["Drum", "trin\u00b7ket", ",", "weil", "ihr", "habt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Thor schickt den Sturm aus; der zerbricht", "tokens": ["Thor", "schickt", "den", "Sturm", "aus", ";", "der", "zer\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsern W\u00e4ldern Stamm und \u00c4ste:", "tokens": ["In", "un\u00b7sern", "W\u00e4l\u00b7dern", "Stamm", "und", "\u00c4s\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald winkt er ihn zur\u00fcck, und Weste", "tokens": ["Bald", "winkt", "er", "ihn", "zu\u00b7r\u00fcck", ",", "und", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebkosen dem Gesicht.", "tokens": ["Lieb\u00b7ko\u00b7sen", "dem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es w\u00fcrgt der aufgereizte B\u00e4r", "tokens": ["Es", "w\u00fcrgt", "der", "auf\u00b7ge\u00b7reiz\u00b7te", "B\u00e4r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die St\u00f6renfriede seines Schlummers:", "tokens": ["Die", "St\u00f6\u00b7ren\u00b7frie\u00b7de", "sei\u00b7nes", "Schlum\u00b7mers", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann h\u00fcpft der Siegestanz des Brummers", "tokens": ["Dann", "h\u00fcpft", "der", "Sie\u00b7ge\u00b7stanz", "des", "Brum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Feld und Wald daher.", "tokens": ["Durch", "Feld", "und", "Wald", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wir, wir sollen tr\u00e4ge sein?", "tokens": ["Und", "wir", ",", "wir", "sol\u00b7len", "tr\u00e4\u00b7ge", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ha! la\u00dft in Roms verwaisten Mauern", "tokens": ["Ha", "!", "la\u00dft", "in", "Roms", "ver\u00b7wais\u00b7ten", "Mau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00fctter und die Br\u00e4ute trauern:", "tokens": ["Die", "M\u00fct\u00b7ter", "und", "die", "Br\u00e4u\u00b7te", "trau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben ihren Wein!", "tokens": ["Wir", "ha\u00b7ben", "ih\u00b7ren", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch soll ihr Wein, so wie man sagt,", "tokens": ["Auch", "soll", "ihr", "Wein", ",", "so", "wie", "man", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mann erfreun, den Greis erneuern,", "tokens": ["Den", "Mann", "er\u00b7freun", ",", "den", "Greis", "er\u00b7neu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den w\u00e4rmsten Barden mehr befeuern:", "tokens": ["Den", "w\u00e4rms\u00b7ten", "Bar\u00b7den", "mehr", "be\u00b7feu\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und es ist, wie man sagt!", "tokens": ["Und", "es", "ist", ",", "wie", "man", "sagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, ich verschmachte! gebet mir", "tokens": ["Ach", ",", "ich", "ver\u00b7schmach\u00b7te", "!", "ge\u00b7bet", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von diesem Wein, ihr Freiheitsr\u00e4cher!", "tokens": ["Von", "die\u00b7sem", "Wein", ",", "ihr", "Frei\u00b7heits\u00b7r\u00e4\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrs Vaterland den ersten Becher;", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "den", "ers\u00b7ten", "Be\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den andern, Hermann dir!", "tokens": ["Den", "an\u00b7dern", ",", "Her\u00b7mann", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}