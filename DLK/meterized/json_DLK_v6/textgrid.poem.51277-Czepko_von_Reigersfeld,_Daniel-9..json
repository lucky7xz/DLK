{"textgrid.poem.51277": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was suchst du M\u00e4chtiger, umb gro\u00dfes Lob zu haben,", "tokens": ["Was", "suchst", "du", "M\u00e4ch\u00b7ti\u00b7ger", ",", "umb", "gro\u00b7\u00dfes", "Lob", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "KOUI", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df Himmel an zu baun, bi\u00df H\u00f6llen ab zu graben?", "tokens": ["Bi\u00df", "Him\u00b7mel", "an", "zu", "baun", ",", "bi\u00df", "H\u00f6l\u00b7len", "ab", "zu", "gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$,", "KOUS", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umbsonst. Egypten bringt die Sa\u00fclen in die Lufft,", "tokens": ["Um\u00b7bsonst", ".", "E\u00b7gyp\u00b7ten", "bringt", "die", "Sa\u00fc\u00b7len", "in", "die", "Lufft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Rhodis sein Sonnenbild: Mausolus seine Grufft,", "tokens": ["Rho\u00b7dis", "sein", "Son\u00b7nen\u00b7bild", ":", "Mau\u00b7so\u00b7lus", "sei\u00b7ne", "Grufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "NE", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und sagen dir: Umbsonst! Die gro\u00dfen Wunderwercke", "tokens": ["Und", "sa\u00b7gen", "dir", ":", "Um\u00b7bsonst", "!", "Die", "gro\u00b7\u00dfen", "Wun\u00b7der\u00b7wer\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreyn deine Arbeit an, und lachen deiner St\u00e4rcke.", "tokens": ["Schreyn", "dei\u00b7ne", "Ar\u00b7beit", "an", ",", "und", "la\u00b7chen", "dei\u00b7ner", "St\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelehrter! Wie da\u00df du dich also sehr bem\u00fchst,", "tokens": ["Ge\u00b7lehr\u00b7ter", "!", "Wie", "da\u00df", "du", "dich", "al\u00b7so", "sehr", "be\u00b7m\u00fchst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOKOM", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Zustand aller Welt viel schreibest, redst, und siehst.", "tokens": ["Vom", "Zu\u00b7stand", "al\u00b7ler", "Welt", "viel", "schrei\u00b7best", ",", "redst", ",", "und", "siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "ADV", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur auch umbsonst! Das Gifft wird Sacraten dir weisen,", "tokens": ["Nur", "auch", "um\u00b7bsonst", "!", "Das", "Gifft", "wird", "Sa\u00b7cra\u00b7ten", "dir", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Bad den Seneca, den Cicero das Eysen:", "tokens": ["Das", "Bad", "den", "Se\u00b7ne\u00b7ca", ",", "den", "Ci\u00b7ce\u00b7ro", "das", "Ey\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NE", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sagen: da\u00df, wie scharff und spitzig ihr Verstand,", "tokens": ["Die", "sa\u00b7gen", ":", "da\u00df", ",", "wie", "scharff", "und", "spit\u00b7zig", "ihr", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$.", "KOUS", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie, wie die Welt denn lohnt, was spitzigers erkannt.", "tokens": ["Sie", ",", "wie", "die", "Welt", "denn", "lohnt", ",", "was", "spit\u00b7zi\u00b7gers", "er\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "NN", "KON", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was wilt du, Siegender, dich trotzig unterwinden,", "tokens": ["Was", "wilt", "du", ",", "Sie\u00b7gen\u00b7der", ",", "dich", "trot\u00b7zig", "un\u00b7ter\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$,", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gantzen Welt das Joch und dein Lob anzubinden?", "tokens": ["Der", "gant\u00b7zen", "Welt", "das", "Joch", "und", "dein", "Lob", "an\u00b7zu\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umbsonst! Der Grieche bringt den Alexander f\u00fcr:", "tokens": ["Um\u00b7bsonst", "!", "Der", "Grie\u00b7che", "bringt", "den", "A\u00b7lex\u00b7an\u00b7der", "f\u00fcr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "ART", "NE", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den C\u00e6sar zeigt sein Rom, die Persen ihren Cyr,", "tokens": ["Den", "C\u00e6sar", "zeigt", "sein", "Rom", ",", "die", "Per\u00b7sen", "ih\u00b7ren", "Cyr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Es sagen auch: Umbsonst! die m\u00e4chtigen Monarchen:", "tokens": ["Es", "sa\u00b7gen", "auch", ":", "Um\u00b7bsonst", "!", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Mon\u00b7ar\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Grab zeigt deines dir, in dem du aus kanst schnarchen.", "tokens": ["Ihr", "Grab", "zeigt", "dei\u00b7nes", "dir", ",", "in", "dem", "du", "aus", "kanst", "schnar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "PPER", "$,", "APPR", "PRELS", "PPER", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey m\u00e4chtig, sey gelehrt, sey siegreich: Mensch, umbsonst,", "tokens": ["Sey", "m\u00e4ch\u00b7tig", ",", "sey", "ge\u00b7lehrt", ",", "sey", "sieg\u00b7reich", ":", "Mensch", ",", "um\u00b7bsonst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "VVPP", "$,", "VAFIN", "ADJD", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Schlo\u00df, dein Witz, dein Harnsch, ist Rauch, ist Dampff, ist Dunst.", "tokens": ["Dein", "Schlo\u00df", ",", "dein", "Witz", ",", "dein", "Harnsch", ",", "ist", "Rauch", ",", "ist", "Dampff", ",", "ist", "Dunst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer in ihm selber wohnt, sich selber kan ergr\u00fcnden,", "tokens": ["Wer", "in", "ihm", "sel\u00b7ber", "wohnt", ",", "sich", "sel\u00b7ber", "kan", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "VVFIN", "$,", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Hertze selber wei\u00df in Ihm zu \u00fcberwinden:", "tokens": ["Sein", "Hert\u00b7ze", "sel\u00b7ber", "wei\u00df", "in", "Ihm", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist m\u00e4chtig, ist gelehrt, ist siegreich umb und an,", "tokens": ["Ist", "m\u00e4ch\u00b7tig", ",", "ist", "ge\u00b7lehrt", ",", "ist", "sieg\u00b7reich", "umb", "und", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "VVPP", "$,", "VAFIN", "ADJD", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hat vielmehr verbracht, als der voll Stolz und Wahn", "tokens": ["Und", "hat", "viel\u00b7mehr", "ver\u00b7bracht", ",", "als", "der", "voll", "Stolz", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "ART", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Lufft erf\u00fcllen wil durch felsichte Pal\u00e4ste,", "tokens": ["Die", "Lufft", "er\u00b7f\u00fcl\u00b7len", "wil", "durch", "fel\u00b7sich\u00b7te", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch B\u00fccher alle Welt, durch Krieg die Himmels Feste.", "tokens": ["Durch", "B\u00fc\u00b7cher", "al\u00b7le", "Welt", ",", "durch", "Krieg", "die", "Him\u00b7mels", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}