{"textgrid.poem.57589": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Gewitter", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfmutter Natur im Lehnstuhl sitzt \u2013", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "Na\u00b7tur", "im", "Lehn\u00b7stuhl", "sitzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie langeweilig ist es heute,", "tokens": ["Wie", "lan\u00b7ge\u00b7wei\u00b7lig", "ist", "es", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie g\u00e4hnt, ganz unertr\u00e4glich sind sie heute,", "tokens": ["Sie", "g\u00e4hnt", ",", "ganz", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "sind", "sie", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sonst so lustigen Leute:", "tokens": ["Die", "sonst", "so", "lus\u00b7ti\u00b7gen", "Leu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die B\u00e4ume brummen so geistlos und fad,", "tokens": ["Die", "B\u00e4u\u00b7me", "brum\u00b7men", "so", "geist\u00b7los", "und", "fad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die B\u00e4chlein schwatzen so weise,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "schwat\u00b7zen", "so", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Wind ist erk\u00e4ltet und stark verschnupft \u2013", "tokens": ["Der", "Wind", "ist", "er\u00b7k\u00e4l\u00b7tet", "und", "stark", "ver\u00b7schnupft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "ADJD", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die Gro\u00dfmutter l\u00e4chelt leise.", "tokens": ["Die", "Gro\u00df\u00b7mut\u00b7ter", "l\u00e4\u00b7chelt", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das L\u00e4cheln flackert als rotes Licht", "tokens": ["Das", "L\u00e4\u00b7cheln", "fla\u00b7ckert", "als", "ro\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Himmelsrande empor \u2013", "tokens": ["Am", "Him\u00b7mels\u00b7ran\u00b7de", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dem Winde f\u00e4llt etwas Lustiges ein,", "tokens": ["Dem", "Win\u00b7de", "f\u00e4llt", "et\u00b7was", "Lus\u00b7ti\u00b7ges", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er sagt es den B\u00e4umen ins Ohr,", "tokens": ["Er", "sagt", "es", "den", "B\u00e4u\u00b7men", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die B\u00e4ume nicken verst\u00e4ndnisvoll,", "tokens": ["Die", "B\u00e4u\u00b7me", "ni\u00b7cken", "ver\u00b7st\u00e4nd\u00b7nis\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erz\u00e4hlen dem B\u00e4chlein es weiter,", "tokens": ["Er\u00b7z\u00e4h\u00b7len", "dem", "B\u00e4ch\u00b7lein", "es", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Das B\u00e4chlein prustet lautlachend los \u2013", "tokens": ["Das", "B\u00e4ch\u00b7lein", "prus\u00b7tet", "laut\u00b7la\u00b7chend", "los", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Die Gro\u00dfmama wird jetzt heiter.", "tokens": ["Die", "Gro\u00df\u00b7ma\u00b7ma", "wird", "jetzt", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Gro\u00dfmutter ein uraltes Witzchen erz\u00e4hlt \u2013", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "ein", "ur\u00b7al\u00b7tes", "Witz\u00b7chen", "er\u00b7z\u00e4hlt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein Blitzschlag f\u00e4hrt herunter!", "tokens": ["Ein", "Blitz\u00b7schlag", "f\u00e4hrt", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gro\u00dfm\u00fctterchen kichert \u2013 der Donner rollt!", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7ter\u00b7chen", "ki\u00b7chert", "\u2013", "der", "Don\u00b7ner", "rollt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die Tafelrunde wird munter \u2013", "tokens": ["Die", "Ta\u00b7fel\u00b7run\u00b7de", "wird", "mun\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es toasten die B\u00e4ume, der Bach wird berauscht,", "tokens": ["Es", "toas\u00b7ten", "die", "B\u00e4u\u00b7me", ",", "der", "Bach", "wird", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Wind ist vollkommen bezecht,", "tokens": ["Der", "Wind", "ist", "voll\u00b7kom\u00b7men", "be\u00b7zecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Gro\u00dfm\u00fctterchen witzelt und kichert wie toll \u2013", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7ter\u00b7chen", "wit\u00b7zelt", "und", "ki\u00b7chert", "wie", "toll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "KOKOM", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "So ist ihr die Tischstimmung recht.", "tokens": ["So", "ist", "ihr", "die", "Tischs\u00b7tim\u00b7mung", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}