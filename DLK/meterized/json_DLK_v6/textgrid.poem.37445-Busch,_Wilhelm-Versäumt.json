{"textgrid.poem.37445": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vers\u00e4umt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Arbeit ist kein Bub geschaffen,", "tokens": ["Zur", "Ar\u00b7beit", "ist", "kein", "Bub", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lernen findet er nicht sch\u00f6n;", "tokens": ["Das", "Ler\u00b7nen", "fin\u00b7det", "er", "nicht", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er m\u00f6chte tr\u00e4umen, m\u00f6chte gaffen", "tokens": ["Er", "m\u00f6ch\u00b7te", "tr\u00e4u\u00b7men", ",", "m\u00f6ch\u00b7te", "gaf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vogelnester suchen gehn.", "tokens": ["Und", "Vo\u00b7gel\u00b7nes\u00b7ter", "su\u00b7chen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er liebt es, lang im Bett zu liegen.", "tokens": ["Er", "liebt", "es", ",", "lang", "im", "Bett", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie es halt im Leben geht:", "tokens": ["Und", "wie", "es", "halt", "im", "Le\u00b7ben", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Grad zu den fr\u00fchen Morgenz\u00fcgen", "tokens": ["Grad", "zu", "den", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt man am leichtesten zu sp\u00e4t.", "tokens": ["Kommt", "man", "am", "leich\u00b7tes\u00b7ten", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}