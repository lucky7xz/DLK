{"textgrid.poem.41369": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Natter und der Aal", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu der Natter sprach der Aal:", "tokens": ["Zu", "der", "Nat\u00b7ter", "sprach", "der", "Aal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Geschick ist zu bedauren,", "tokens": ["Mein", "Ge\u00b7schick", "ist", "zu", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil auf mich fast allemal,", "tokens": ["Weil", "auf", "mich", "fast", "al\u00b7le\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht auf dich, die Leute lauren.", "tokens": ["Nicht", "auf", "dich", ",", "die", "Leu\u00b7te", "lau\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruh' und Unschuld sch\u00fctzt mich nicht,", "tokens": ["Ruh'", "und", "Un\u00b7schuld", "sch\u00fctzt", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil mir jeder Netze flicht.", "tokens": ["Weil", "mir", "je\u00b7der", "Net\u00b7ze", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vetter, fiel die Natter ein,", "tokens": ["Vet\u00b7ter", ",", "fiel", "die", "Nat\u00b7ter", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unschuld wird dich nicht befrein;", "tokens": ["Un\u00b7schuld", "wird", "dich", "nicht", "be\u00b7fr\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber ich kann Z\u00e4hne weisen,", "tokens": ["A\u00b7ber", "ich", "kann", "Z\u00e4h\u00b7ne", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deren Bi\u00df die Feinde scheun.", "tokens": ["De\u00b7ren", "Bi\u00df", "die", "Fein\u00b7de", "scheun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}