{"textgrid.poem.36158": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rose, Rose, wie dem Sturm gefallen!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose, Rose, wie dem Sturm gefallen!", "tokens": ["Ro\u00b7se", ",", "Ro\u00b7se", ",", "wie", "dem", "Sturm", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie so pl\u00f6tzlich! Duft \u2013 und lebenleer", "tokens": ["Wie", "so", "pl\u00f6tz\u00b7lich", "!", "Duft", "\u2013", "und", "le\u00b7ben\u00b7leer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$.", "NN", "$(", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schmachtest du im Staube. Traurig wallen", "tokens": ["Schmach\u00b7test", "du", "im", "Stau\u00b7be", ".", "Trau\u00b7rig", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine welken Bl\u00e4tter um dich her.", "tokens": ["Dei\u00b7ne", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und du warst so lieblich! warst so milde!", "tokens": ["Und", "du", "warst", "so", "lieb\u00b7lich", "!", "warst", "so", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$.", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herrlich perlt' auf dir der Morgenthau!", "tokens": ["Herr\u00b7lich", "perlt'", "auf", "dir", "der", "Mor\u00b7gent\u00b7hau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine Rose glich dir im Gefilde!", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", "glich", "dir", "im", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Keine Blum' auf weiter Blumenau!", "tokens": ["Kei\u00b7ne", "Blum'", "auf", "wei\u00b7ter", "Blu\u00b7me\u00b7nau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dein D\u00fcfte wallten durch den Garten,", "tokens": ["Dein", "D\u00fcf\u00b7te", "wall\u00b7ten", "durch", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hauchten Wohlger\u00fcche durch die Luft.", "tokens": ["Hauch\u00b7ten", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wandrer, die vor\u00fcbergingen, harrten,", "tokens": ["Wand\u00b7rer", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7gin\u00b7gen", ",", "harr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Athmeten der Blume s\u00fc\u00dfen Duft.", "tokens": ["Ath\u00b7me\u00b7ten", "der", "Blu\u00b7me", "s\u00fc\u00b7\u00dfen", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Freundlich that der Sonne Strahl ihr kosen,", "tokens": ["Freund\u00b7lich", "that", "der", "Son\u00b7ne", "Strahl", "ihr", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Aurora weine fr\u00fch und spat", "tokens": ["Und", "Au\u00b7ro\u00b7ra", "wei\u00b7ne", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf die sch\u00f6nste, r\u00f6theste der Rosen,", "tokens": ["Auf", "die", "sch\u00f6ns\u00b7te", ",", "r\u00f6\u00b7thes\u00b7te", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die der Sturm nun abgebrochen hat.", "tokens": ["Die", "der", "Sturm", "nun", "ab\u00b7ge\u00b7bro\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wild und grausam hat er sie entbl\u00e4ttert,", "tokens": ["Wild", "und", "grau\u00b7sam", "hat", "er", "sie", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre Krone hat er abgerast.", "tokens": ["Ih\u00b7re", "Kro\u00b7ne", "hat", "er", "ab\u00b7ge\u00b7rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alle ihre Zier ist abgewettert.", "tokens": ["Al\u00b7le", "ih\u00b7re", "Zier", "ist", "ab\u00b7ge\u00b7wet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alle ihre Farb' und Frisch' erblass't.", "tokens": ["Al\u00b7le", "ih\u00b7re", "Fa\u00b7rb'", "und", "Frisch'", "erb\u00b7lass'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "XY", "XY", "XY"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "All' ihr s\u00fc\u00dfer Duft ist ausgegossen.", "tokens": ["All'", "ihr", "s\u00fc\u00b7\u00dfer", "Duft", "ist", "aus\u00b7ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre Bl\u00e4tter wehn im Wind' einher \u2013", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", "wehn", "im", "Wind'", "ein\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zwar den Mutterstamm, dem sie entsprossen,", "tokens": ["Zwar", "den", "Mut\u00b7ter\u00b7stamm", ",", "dem", "sie", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schm\u00fccken noch der edeln Knospen mehr;", "tokens": ["Schm\u00fc\u00b7cken", "noch", "der", "e\u00b7deln", "Knos\u00b7pen", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber von den zarten Knospen allen,", "tokens": ["A\u00b7ber", "von", "den", "zar\u00b7ten", "Knos\u00b7pen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War die fr\u00fchste, reifste, liebste sie!", "tokens": ["War", "die", "fr\u00fchs\u00b7te", ",", "reifs\u00b7te", ",", "liebs\u00b7te", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Eine ist dem Sturm gefallen!", "tokens": ["Und", "die", "Ei\u00b7ne", "ist", "dem", "Sturm", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "O du W\u00fcther, warum? warum die?", "tokens": ["O", "du", "W\u00fct\u00b7her", ",", "wa\u00b7rum", "?", "wa\u00b7rum", "die", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PWAV", "$.", "PWAV", "ART", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Zwar, es ist das Loos der Erdensch\u00f6ne,", "tokens": ["Zwar", ",", "es", "ist", "das", "Loos", "der", "Er\u00b7den\u00b7sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu verfliegen, wie der Thau verfliegt,", "tokens": ["Zu", "ver\u00b7flie\u00b7gen", ",", "wie", "der", "Thau", "ver\u00b7fliegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Den die Sonne weg leckt, wie die Thr\u00e4ne", "tokens": ["Den", "die", "Son\u00b7ne", "weg", "leckt", ",", "wie", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In den Staub, darein sie sank, versiegt.", "tokens": ["In", "den", "Staub", ",", "da\u00b7rein", "sie", "sank", ",", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und was wegwelkt aus den Erdenthalen", "tokens": ["Und", "was", "weg\u00b7welkt", "aus", "den", "Er\u00b7dent\u00b7ha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwindet darum nicht aus Gottes Welt.", "tokens": ["Schwin\u00b7det", "da\u00b7rum", "nicht", "aus", "Got\u00b7tes", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKNEG", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht des Morgenroths verstrahlte Strahlen,", "tokens": ["Nicht", "des", "Mor\u00b7gen\u00b7roths", "ver\u00b7strahl\u00b7te", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht die Blume, die zu Staub zerf\u00e4llt,", "tokens": ["Nicht", "die", "Blu\u00b7me", ",", "die", "zu", "Staub", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Nicht die Asche ausgebrannter Sonnen,", "tokens": ["Nicht", "die", "A\u00b7sche", "aus\u00b7ge\u00b7brann\u00b7ter", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht die D\u00fcfte, die der Ros' entwehn,", "tokens": ["Nicht", "die", "D\u00fcf\u00b7te", ",", "die", "der", "Ros'", "ent\u00b7wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht das F\u00e4dchen, das vom Wurm gesponnen", "tokens": ["Nicht", "das", "F\u00e4d\u00b7chen", ",", "das", "vom", "Wurm", "ge\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der Luft verflattert, mag vergehn!", "tokens": ["In", "der", "Luft", "ver\u00b7flat\u00b7tert", ",", "mag", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Mag nicht schwinden aus dem gro\u00dfen Alle!", "tokens": ["Mag", "nicht", "schwin\u00b7den", "aus", "dem", "gro\u00b7\u00dfen", "Al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geht nicht gar verlohren! \u2013 Klein und gro\u00df,", "tokens": ["Geht", "nicht", "gar", "ver\u00b7loh\u00b7ren", "!", "\u2013", "Klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVPP", "$.", "$(", "NE", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hauch des W\u00fcrmchens, Staub der Weltenballe", "tokens": ["Hauch", "des", "W\u00fcrm\u00b7chens", ",", "Staub", "der", "Wel\u00b7ten\u00b7bal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wahret Gott in seinem treuen Schoo\u00df.", "tokens": ["Wah\u00b7ret", "Gott", "in", "sei\u00b7nem", "treu\u00b7en", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Und o Wonn'! o Tr\u00f6stung! Jedes Staubes", "tokens": ["Und", "o", "Wonn'", "!", "o", "Tr\u00f6s\u00b7tung", "!", "Je\u00b7des", "Stau\u00b7bes"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "NN", "$.", "FM", "NN", "$.", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Harrt ein gro\u00dfer Wiederbringungstag!", "tokens": ["Harrt", "ein", "gro\u00b7\u00dfer", "Wie\u00b7der\u00b7brin\u00b7gungs\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Frischer Saft durchstr\u00f6mt des welken Laubes", "tokens": ["Fri\u00b7scher", "Saft", "durch\u00b7str\u00f6mt", "des", "wel\u00b7ken", "Lau\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eingeschrumpfte R\u00f6hren. D\u00fcrr Gebein wird wach!", "tokens": ["Ein\u00b7ge\u00b7schrumpf\u00b7te", "R\u00f6h\u00b7ren", ".", "D\u00fcrr", "Ge\u00b7bein", "wird", "wach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.13": {"line.1": {"text": "Morgenr\u00f6then, welche hier verstrahlen,", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7then", ",", "wel\u00b7che", "hier", "ver\u00b7strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bl\u00fch'n in sch\u00f6ner'n Welten sch\u00f6ner au