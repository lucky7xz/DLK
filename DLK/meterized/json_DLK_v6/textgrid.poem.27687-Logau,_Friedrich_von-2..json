{"textgrid.poem.27687": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihrer viel sind zwar beflissen,", "tokens": ["Ih\u00b7rer", "viel", "sind", "zwar", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im Helicon zu wissen.", "tokens": ["Sich", "im", "He\u00b7li\u00b7con", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie gleich nun ziehn und ziehn,", "tokens": ["Ob", "sie", "gleich", "nun", "ziehn", "und", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommen langsam sie doch hin;", "tokens": ["Kom\u00b7men", "lang\u00b7sam", "sie", "doch", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ihr bestes Pferd ist heuer", "tokens": ["Denn", "ihr", "bes\u00b7tes", "Pferd", "ist", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Viel zu seltsam und zu theuer.", "tokens": ["Viel", "zu", "selt\u00b7sam", "und", "zu", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}