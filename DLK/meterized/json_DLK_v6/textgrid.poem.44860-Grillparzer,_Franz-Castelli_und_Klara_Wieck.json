{"textgrid.poem.44860": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Castelli und Klara Wieck", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachsicht, die die Welt dir schenkt als dein,", "tokens": ["Die", "Nach\u00b7sicht", ",", "die", "die", "Welt", "dir", "schenkt", "als", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "KOKOM", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Willst gegen andre du so hart verleugnen.", "tokens": ["Willst", "ge\u00b7gen", "and\u00b7re", "du", "so", "hart", "ver\u00b7leug\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sei, um in K\u00fcnsten streng zu sein,", "tokens": ["Sei", ",", "um", "in", "K\u00fcns\u00b7ten", "streng", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUI", "APPR", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Streng gegen dich vorerst in deiner eignen.", "tokens": ["Streng", "ge\u00b7gen", "dich", "vor\u00b7erst", "in", "dei\u00b7ner", "eig\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}