{"textgrid.poem.62118": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[die Sonne gehet strahlend unter]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne gehet strahlend unter,", "tokens": ["Die", "Son\u00b7ne", "ge\u00b7het", "strah\u00b7lend", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur scheinbar, Freund, nicht in der Tat \u2013", "tokens": ["Nur", "schein\u00b7bar", ",", "Freund", ",", "nicht", "in", "der", "Tat", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vorhang f\u00e4llt so rasch herunter, \u2013", "tokens": ["Der", "Vor\u00b7hang", "f\u00e4llt", "so", "rasch", "her\u00b7un\u00b7ter", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man nur ihn gesehen hat. \u2013", "tokens": ["Da\u00df", "man", "nur", "ihn", "ge\u00b7se\u00b7hen", "hat", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}