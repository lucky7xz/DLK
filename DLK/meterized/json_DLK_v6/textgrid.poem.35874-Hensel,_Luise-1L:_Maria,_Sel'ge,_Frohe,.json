{"textgrid.poem.35874": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Maria, Sel'ge, Frohe,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria, Sel'ge, Frohe,", "tokens": ["Ma\u00b7ria", ",", "Sel'\u00b7ge", ",", "Fro\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du mildes Mutterherz!", "tokens": ["Du", "mil\u00b7des", "Mut\u00b7ter\u00b7herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh her auf mich, Du Hohe!", "tokens": ["Sieh", "her", "auf", "mich", ",", "Du", "Ho\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "PPER", "$,", "PPER", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schaue himmelw\u00e4rts.", "tokens": ["Ich", "schau\u00b7e", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Du hast f\u00fcr mich gelitten", "tokens": ["Du", "hast", "f\u00fcr", "mich", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei Deines Sohnes Tod,", "tokens": ["Bei", "Dei\u00b7nes", "Soh\u00b7nes", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du woll'st auch f\u00fcr mich bitten", "tokens": ["Du", "woll'st", "auch", "f\u00fcr", "mich", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Dein- und meinem Gott.", "tokens": ["Bei", "Dein", "und", "mei\u00b7nem", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast Ihn mir geboren,", "tokens": ["Du", "hast", "Ihn", "mir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Weihnacht mir geschenkt;", "tokens": ["Zur", "Weih\u00b7nacht", "mir", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' Ihn oft verloren,", "tokens": ["Ich", "hab'", "Ihn", "oft", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlassen und gekr\u00e4nkt.", "tokens": ["Ver\u00b7las\u00b7sen", "und", "ge\u00b7kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Welt spricht zu den Sinnen,", "tokens": ["Die", "Welt", "spricht", "zu", "den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sinne h\u00f6ren's gern;", "tokens": ["Die", "Sin\u00b7ne", "h\u00f6\u00b7ren's", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Denken, mein Beginnen", "tokens": ["Mein", "Den\u00b7ken", ",", "mein", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt mich von Dir noch fern.", "tokens": ["H\u00e4lt", "mich", "von", "Dir", "noch", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch ist mir etwas blieben", "tokens": ["Doch", "ist", "mir", "et\u00b7was", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im tiefsten Herzen drin,", "tokens": ["Im", "tiefs\u00b7ten", "Her\u00b7zen", "drin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das will so gern Dich lieben,", "tokens": ["Das", "will", "so", "gern", "Dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das zieht zu Dir mich hin.", "tokens": ["Das", "zieht", "zu", "Dir", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Maria! das vermehre,", "tokens": ["Ma\u00b7ria", "!", "das", "ver\u00b7meh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Das l\u00e4ut're, st\u00e4rke Du,", "tokens": ["Das", "l\u00e4ut'\u00b7re", ",", "st\u00e4r\u00b7ke", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es die Welt nicht st\u00f6re,", "tokens": ["Da\u00df", "es", "die", "Welt", "nicht", "st\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df es Dir schwebe zu.", "tokens": ["Da\u00df", "es", "Dir", "schwe\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O, zieh mich n\u00e4her, n\u00e4her", "tokens": ["O", ",", "zieh", "mich", "n\u00e4\u00b7her", ",", "n\u00e4\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Freude und durch Harm", "tokens": ["Durch", "Freu\u00b7de", "und", "durch", "Harm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heb mich h\u00f6her, h\u00f6her", "tokens": ["Und", "heb", "mich", "h\u00f6\u00b7her", ",", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit treuem Mutterarm.", "tokens": ["Mit", "treu\u00b7em", "Mut\u00b7ter\u00b7arm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, nimm mich zum Geschenke,", "tokens": ["O", ",", "nimm", "mich", "zum", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So unwerth ich auch bin,", "tokens": ["So", "un\u00b7werth", "ich", "auch", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! und dann lenke", "tokens": ["Ma\u00b7ria", "!", "und", "dann", "len\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Kind zum Vater hin!", "tokens": ["Dein", "Kind", "zum", "Va\u00b7ter", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}