{"textgrid.poem.26490": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder die ersten sonnigen Hauche,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder die ersten sonnigen Hauche,", "tokens": ["Wie\u00b7der", "die", "ers\u00b7ten", "son\u00b7ni\u00b7gen", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lockend hinaus vor die d\u00fcstere Stadt;", "tokens": ["Lo\u00b7ckend", "hin\u00b7aus", "vor", "die", "d\u00fcs\u00b7te\u00b7re", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wieder am zitternden, treibenden Strauche", "tokens": ["Wie\u00b7der", "am", "zit\u00b7tern\u00b7den", ",", "trei\u00b7ben\u00b7den", "Strau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ersten Knospen, das erste Blatt.", "tokens": ["Die", "ers\u00b7ten", "Knos\u00b7pen", ",", "das", "ers\u00b7te", "Blatt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wieder auf leis' ergr\u00fcnenden H\u00e4ngen", "tokens": ["Wie\u00b7der", "auf", "leis'", "er\u00b7gr\u00fc\u00b7nen\u00b7den", "H\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ersten Veilchens lieblicher Fund;", "tokens": ["Ers\u00b7ten", "Veil\u00b7chens", "lieb\u00b7li\u00b7cher", "Fund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wieder mit ersten Jubelges\u00e4ngen", "tokens": ["Wie\u00b7der", "mit", "ers\u00b7ten", "Ju\u00b7bel\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hebt sich die Lerche vom scholligen Grund.", "tokens": ["Hebt", "sich", "die", "Ler\u00b7che", "vom", "schol\u00b7li\u00b7gen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Werdenden Fr\u00fchlings verk\u00fcndende Zeichen,", "tokens": ["Wer\u00b7den\u00b7den", "Fr\u00fch\u00b7lings", "ver\u00b7k\u00fcn\u00b7den\u00b7de", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Alte Genossen von Lust und Schmerz,", "tokens": ["Al\u00b7te", "Ge\u00b7nos\u00b7sen", "von", "Lust", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach, wie entz\u00fcckt ihr, ihr ewig Gleichen,", "tokens": ["Ach", ",", "wie", "ent\u00b7z\u00fcckt", "ihr", ",", "ihr", "e\u00b7wig", "Glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ewig auf's neue das Menschenherz!", "tokens": ["E\u00b7wig", "auf's", "neu\u00b7e", "das", "Men\u00b7schen\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}