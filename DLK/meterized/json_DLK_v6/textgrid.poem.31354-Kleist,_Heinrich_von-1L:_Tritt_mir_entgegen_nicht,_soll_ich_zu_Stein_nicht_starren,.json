{"textgrid.poem.31354": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tritt mir entgegen nicht, soll ich zu Stein nicht starren,", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt mir entgegen nicht, soll ich zu Stein nicht starren,", "tokens": ["Tritt", "mir", "ent\u00b7ge\u00b7gen", "nicht", ",", "soll", "ich", "zu", "Stein", "nicht", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "PTKNEG", "$,", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Auf M\u00e4rkten, oder sonst, wo Menschen atmend gehn,", "tokens": ["Auf", "M\u00e4rk\u00b7ten", ",", "o\u00b7der", "sonst", ",", "wo", "Men\u00b7schen", "at\u00b7mend", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADV", "$,", "PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich will ich nur am Styx, bei marmorwei\u00dfen Scharen,", "tokens": ["Dich", "will", "ich", "nur", "am", "Styx", ",", "bei", "mar\u00b7mor\u00b7wei\u00b7\u00dfen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Leonidas, Armin und Tell, den Geistern, sehn.", "tokens": ["Leo\u00b7ni\u00b7das", ",", "Ar\u00b7min", "und", "Tell", ",", "den", "Geis\u00b7tern", ",", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$,", "ART", "NN", "$,", "VVINF", "$."], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du Held, der, gleich dem Fels, das Haupt erh\u00f6ht zur Sonnen,", "tokens": ["Du", "Held", ",", "der", ",", "gleich", "dem", "Fels", ",", "das", "Haupt", "er\u00b7h\u00f6ht", "zur", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "$,", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Fu\u00df versenkt in Nacht, des Stromes Wut gewehrt,", "tokens": ["Den", "Fu\u00df", "ver\u00b7senkt", "in", "Nacht", ",", "des", "Stro\u00b7mes", "Wut", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der stinkend wie die Pest, der H\u00f6lle wie entronnen,", "tokens": ["Der", "stin\u00b7kend", "wie", "die", "Pest", ",", "der", "H\u00f6l\u00b7le", "wie", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "$,", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Bau sechs festlicher Jahrtausende zerst\u00f6rt!", "tokens": ["Den", "Bau", "sechs", "fest\u00b7li\u00b7cher", "Jahr\u00b7tau\u00b7sen\u00b7de", "zer\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dir lie\u00df ich, hei\u00df wie Glut, ein Lied zum Himmel dringen,", "tokens": ["Dir", "lie\u00df", "ich", ",", "hei\u00df", "wie", "Glut", ",", "ein", "Lied", "zum", "Him\u00b7mel", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhabner, h\u00e4ttest du Geringeres getan.", "tokens": ["Er\u00b7hab\u00b7ner", ",", "h\u00e4t\u00b7test", "du", "Ge\u00b7rin\u00b7ge\u00b7res", "ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch was der Ebro sah, kann keine Leier singen,", "tokens": ["Doch", "was", "der", "Eb\u00b7ro", "sah", ",", "kann", "kei\u00b7ne", "Lei\u00b7er", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NE", "VVFIN", "$,", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in dem Tempel still, h\u00e4ng ich sie wieder an.", "tokens": ["Und", "in", "dem", "Tem\u00b7pel", "still", ",", "h\u00e4ng", "ich", "sie", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}