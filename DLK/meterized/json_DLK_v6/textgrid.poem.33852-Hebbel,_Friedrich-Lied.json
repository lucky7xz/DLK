{"textgrid.poem.33852": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, wir wollen Erdbeer'n pfl\u00fccken,", "tokens": ["Komm", ",", "wir", "wol\u00b7len", "Erd\u00b7beer'n", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es doch nicht weit zum Wald,", "tokens": ["Ist", "es", "doch", "nicht", "weit", "zum", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen junge Rosen brechen,", "tokens": ["Wol\u00b7len", "jun\u00b7ge", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verwelken ja so bald!", "tokens": ["Sie", "ver\u00b7wel\u00b7ken", "ja", "so", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Droben jene Wetterwolke,", "tokens": ["Dro\u00b7ben", "je\u00b7ne", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich \u00e4ngstigt, f\u00fcrcht' ich nicht;", "tokens": ["Die", "dich", "\u00e4ngs\u00b7tigt", ",", "f\u00fcrcht'", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, sie ist mir sehr willkommen,", "tokens": ["Nein", ",", "sie", "ist", "mir", "sehr", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Mittagssonne sticht.", "tokens": ["Denn", "die", "Mit\u00b7tags\u00b7son\u00b7ne", "sticht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "All die sengend-hei\u00dfen Stralen,", "tokens": ["All", "die", "sen\u00b7gen\u00b7dhei\u00b7\u00dfen", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die uns drohen, l\u00f6scht sie aus,", "tokens": ["Die", "uns", "dro\u00b7hen", ",", "l\u00f6scht", "sie", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn sie sich selbst entladet,", "tokens": ["Und", "wenn", "sie", "sich", "selbst", "ent\u00b7la\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Sind wir lange schon zu Haus!", "tokens": ["Sind", "wir", "lan\u00b7ge", "schon", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "T\u00e4ndelnd flecht' ich dann die Rosen", "tokens": ["T\u00e4n\u00b7delnd", "flecht'", "ich", "dann", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dein dunkelbraunes Haar,", "tokens": ["In", "dein", "dun\u00b7kel\u00b7brau\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du bietest Beer' um Beere", "tokens": ["Und", "du", "bie\u00b7test", "Beer'", "um", "Bee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen durst'gen Lippen dar.", "tokens": ["Mei\u00b7nen", "dur\u00b7st'\u00b7gen", "Lip\u00b7pen", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}