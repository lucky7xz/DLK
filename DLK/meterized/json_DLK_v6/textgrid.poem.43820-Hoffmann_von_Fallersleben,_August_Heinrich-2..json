{"textgrid.poem.43820": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gute Nacht!", "tokens": ["Nun", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du hast f\u00fcr heut genug gelacht,", "tokens": ["Du", "hast", "f\u00fcr", "heut", "ge\u00b7nug", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hast du auch geweint gar sehr,", "tokens": ["Doch", "hast", "du", "auch", "ge\u00b7weint", "gar", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob dir Leids geschehen w\u00e4r'.", "tokens": ["Als", "ob", "dir", "Leids", "ge\u00b7sche\u00b7hen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das kann nicht sein!", "tokens": ["Das", "kann", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Drum, liebes Kind, schlaf' ruhig ein.", "tokens": ["Drum", ",", "lie\u00b7bes", "Kind", ",", "schlaf'", "ru\u00b7hig", "ein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was deiner Mutter widerfuhr,", "tokens": ["Was", "dei\u00b7ner", "Mut\u00b7ter", "wi\u00b7der\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war dein ganzes Leiden nur.", "tokens": ["Das", "war", "dein", "gan\u00b7zes", "Lei\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schlaf' denn ein!", "tokens": ["So", "schlaf'", "denn", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Wieg' ist deine Welt allein,", "tokens": ["Die", "Wieg'", "ist", "dei\u00b7ne", "Welt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drin Sonn' und Mond nicht untergeh'n,", "tokens": ["Drin", "Sonn'", "und", "Mond", "nicht", "un\u00b7ter\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Wolken zieh'n und Winde weh'n.", "tokens": ["Noch", "Wol\u00b7ken", "zieh'n", "und", "Win\u00b7de", "weh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "NN", "VVFIN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das kennst noch nicht,", "tokens": ["Das", "kennst", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und kennst, mein Kind, gar vieles nicht;", "tokens": ["Und", "kennst", ",", "mein", "Kind", ",", "gar", "vie\u00b7les", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wei\u00dft genug, wenn Eins du wei\u00dft,", "tokens": ["Doch", "wei\u00dft", "ge\u00b7nug", ",", "wenn", "Eins", "du", "wei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Vater und was Mutter hei\u00dft.", "tokens": ["Was", "Va\u00b7ter", "und", "was", "Mut\u00b7ter", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}