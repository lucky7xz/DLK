{"textgrid.poem.67723": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Triumphlied der G\u00f6tter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun l\u00e4cheln die Himmel in Sicherheit", "tokens": ["Nun", "l\u00e4\u00b7cheln", "die", "Him\u00b7mel", "in", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nach dem Ungewitter des Kampfs", "tokens": ["Nach", "dem", "Un\u00b7ge\u00b7wit\u00b7ter", "des", "Kampfs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sich auf! \u2013 Meine Zunge, die Donnergluth trank,", "tokens": ["Sich", "auf", "!", "\u2013", "Mei\u00b7ne", "Zun\u00b7ge", ",", "die", "Don\u00b7nerg\u00b7luth", "trank", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "L\u00f6sch' jetzt bestrittener Nektar!", "tokens": ["L\u00f6sch'", "jetzt", "be\u00b7strit\u00b7te\u00b7ner", "Nek\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nun trinkt, Ihr G\u00f6tter! Ihr tief Geheul", "tokens": ["Nun", "trinkt", ",", "Ihr", "G\u00f6t\u00b7ter", "!", "Ihr", "tief", "Ge\u00b7heul"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$.", "PPER", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus dem Erdbauch brauset Triumph!", "tokens": ["Aus", "dem", "Erd\u00b7bauch", "brau\u00b7set", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Triumph! Meinen Namen, den Donnerer, ruft", "tokens": ["Tri\u00b7umph", "!", "Mei\u00b7nen", "Na\u00b7men", ",", "den", "Don\u00b7ne\u00b7rer", ",", "ruft"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Bebend Aetna, der rauchende Siegsberg! \u2013", "tokens": ["Be\u00b7bend", "A\u00b7et\u00b7na", ",", "der", "rau\u00b7chen\u00b7de", "Siegs\u00b7berg", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "NE", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Den G\u00f6ttern Evoe! Mein eppichumlorbeert Haar", "tokens": ["Den", "G\u00f6t\u00b7tern", "E\u00b7voe", "!", "Mein", "ep\u00b7pic\u00b7hum\u00b7lor\u00b7beert", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Rauscht! Weg, blutiger Panzer!", "tokens": ["Rauscht", "!", "Weg", ",", "blu\u00b7ti\u00b7ger", "Pan\u00b7zer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ganymed, Dein Kelch umkr\u00e4nzet sich mir! \u2013", "tokens": ["Ga\u00b7ny\u00b7med", ",", "Dein", "Kelch", "um\u00b7kr\u00e4n\u00b7zet", "sich", "mir", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "PPER", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Tanzt, M\u00e4naden, Silenen, und Du,", "tokens": ["Tanzt", ",", "M\u00e4\u00b7na\u00b7den", ",", "Si\u00b7le\u00b7nen", ",", "und", "Du", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "$,", "KON", "PPER", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Alter Silen, Deinen Esel tr\u00e4nk in Nektar!", "tokens": ["Al\u00b7ter", "Si\u00b7len", ",", "Dei\u00b7nen", "E\u00b7sel", "tr\u00e4nk", "in", "Nek\u00b7tar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "Er durstet, der l\u00f6wengleich br\u00fcllte", "tokens": ["Er", "durs\u00b7tet", ",", "der", "l\u00f6\u00b7wen\u00b7gleich", "br\u00fcll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Jene weibisch pochende Ungeheuer zur\u00fcck!", "tokens": ["Je\u00b7ne", "wei\u00b7bisch", "po\u00b7chen\u00b7de", "Un\u00b7ge\u00b7heu\u00b7er", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und jauchzt, Ihr G\u00f6tter und Faunen! Ich gl\u00fch'!", "tokens": ["Und", "jauchzt", ",", "Ihr", "G\u00f6t\u00b7ter", "und", "Fau\u00b7nen", "!", "Ich", "gl\u00fch'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "So gl\u00fchte der Wange der Gott das Feuer!", "tokens": ["So", "gl\u00fch\u00b7te", "der", "Wan\u00b7ge", "der", "Gott", "das", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}