{"dta.poem.19966": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Liebesklage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwarzes Band, o du mein Leben!", "tokens": ["Schwar\u00b7zes", "Band", ",", "o", "du", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh' auf meinem Herzen warm;", "tokens": ["Ruh'", "auf", "mei\u00b7nem", "Her\u00b7zen", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe hat dich mir gegeben,", "tokens": ["Lie\u00b7be", "hat", "dich", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne dich, wie w\u00e4r' ich arm!", "tokens": ["Oh\u00b7ne", "dich", ",", "wie", "w\u00e4r'", "ich", "arm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fragt man mich, warum ich trage", "tokens": ["Fragt", "man", "mich", ",", "wa\u00b7rum", "ich", "tra\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses schwarze schlechte Band,", "tokens": ["Die\u00b7ses", "schwar\u00b7ze", "schlech\u00b7te", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ich's nicht vor Weinen sagen:", "tokens": ["Kann", "ich's", "nicht", "vor", "Wei\u00b7nen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es kommt von Liebeshand.", "tokens": ["Denn", "es", "kommt", "von", "Lie\u00b7bes\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So ich sollte ruhig schlafen,", "tokens": ["So", "ich", "soll\u00b7te", "ru\u00b7hig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Bettlein, kann's nicht seyn;", "tokens": ["In", "dem", "Bet\u00b7tlein", ",", "kann's", "nicht", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe stets mit dir zu schaffen,", "tokens": ["Ha\u00b7be", "stets", "mit", "dir", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarzes Band! du liebe Pein!", "tokens": ["Schwar\u00b7zes", "Band", "!", "du", "lie\u00b7be", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So ich sollte zu mir nehmen,", "tokens": ["So", "ich", "soll\u00b7te", "zu", "mir", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas Speise oder Trank,", "tokens": ["Et\u00b7was", "Spei\u00b7se", "o\u00b7der", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ich nicht vor lauter Gr\u00e4men", "tokens": ["Kann", "ich", "nicht", "vor", "lau\u00b7ter", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagen Dank: denn ich bin krank.", "tokens": ["Sa\u00b7gen", "Dank", ":", "denn", "ich", "bin", "krank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Krank seyn, es nicht d\u00fcrfen klagen,", "tokens": ["Krank", "seyn", ",", "es", "nicht", "d\u00fcr\u00b7fen", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist wohl eine schwere Pein;", "tokens": ["Ist", "wohl", "ei\u00b7ne", "schwe\u00b7re", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieben, es nicht d\u00fcrfen sagen,", "tokens": ["Lie\u00b7ben", ",", "es", "nicht", "d\u00fcr\u00b7fen", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ein hartes Lieben seyn!", "tokens": ["Mu\u00df", "ein", "har\u00b7tes", "Lie\u00b7ben", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}