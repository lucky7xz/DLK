{"textgrid.poem.62777": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein edler brittischer Wallach,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edler brittischer Wallach,", "tokens": ["Ein", "ed\u00b7ler", "brit\u00b7ti\u00b7scher", "Wal\u00b7lach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der auf dem Eis ein Bein zerbrach,", "tokens": ["Der", "auf", "dem", "Eis", "ein", "Bein", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kroch martervoll nach seinem Stalle,", "tokens": ["Kroch", "mar\u00b7ter\u00b7voll", "nach", "sei\u00b7nem", "Stal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem ein rundes F\u00fcllen fra\u00df.", "tokens": ["In", "dem", "ein", "run\u00b7des", "F\u00fcl\u00b7len", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ey, guter Oheim, was ist das?", "tokens": ["Ey", ",", "gu\u00b7ter", "O\u00b7heim", ",", "was", "ist", "das", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PWS", "VAFIN", "PDS", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Rief es, wie kamst du denn zu Falle?", "tokens": ["Rief", "es", ",", "wie", "kamst", "du", "denn", "zu", "Fal\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So rasch ich bin, so ist doch mir,", "tokens": ["So", "rasch", "ich", "bin", ",", "so", "ist", "doch", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gottlob, der Fu\u00df noch nie geglitten.", "tokens": ["Gott\u00b7lob", ",", "der", "Fu\u00df", "noch", "nie", "ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ganz wohl, versetzt das arme Thier,", "tokens": ["Ganz", "wohl", ",", "ver\u00b7setzt", "das", "ar\u00b7me", "Thier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein du liefst noch nie im Schlitten.", "tokens": ["Al\u00b7lein", "du", "liefst", "noch", "nie", "im", "Schlit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So, Freund, ist oft die Heiligkeit,", "tokens": ["So", ",", "Freund", ",", "ist", "oft", "die", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Womit sich kleine Seelen bl\u00e4hen,", "tokens": ["Wo\u00b7mit", "sich", "klei\u00b7ne", "See\u00b7len", "bl\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Blos Mangel an Gelegenheit,", "tokens": ["Blos", "Man\u00b7gel", "an", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fehler andrer zu begehen.", "tokens": ["Die", "Feh\u00b7ler", "an\u00b7drer", "zu", "be\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}