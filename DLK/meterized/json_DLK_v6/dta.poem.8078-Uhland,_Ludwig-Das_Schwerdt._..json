{"dta.poem.8078": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Schwerdt. .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Schmiede ging ein junger Held,", "tokens": ["Zur", "Schmie\u00b7de", "ging", "ein", "jun\u00b7ger", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hatt\u2019 ein gutes Schwerdt bestellt.", "tokens": ["Er", "hatt'", "ein", "gu\u00b7tes", "Schwerdt", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch als er\u2019s wog in freier Hand,", "tokens": ["Doch", "als", "er's", "wog", "in", "frei\u00b7er", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schwerdt er viel zu schwer erfand.", "tokens": ["Das", "Schwerdt", "er", "viel", "zu", "schwer", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der alte Schmied den Bart sich streicht:", "tokens": ["Der", "al\u00b7te", "Schmied", "den", "Bart", "sich", "streicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edas Schwerdt ist nicht zu schwer noch leicht,", "tokens": ["\u201e", "das", "Schwerdt", "ist", "nicht", "zu", "schwer", "noch", "leicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKNEG", "PTKA", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu schwach ist Euer Arm, ich mein\u2019,", "tokens": ["Zu", "schwach", "ist", "Eu\u00b7er", "Arm", ",", "ich", "mein'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch morgen soll geholfen seyn.\u201c", "tokens": ["Doch", "mor\u00b7gen", "soll", "ge\u00b7hol\u00b7fen", "seyn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201enein, heut! bei aller Ritterschaft!", "tokens": ["\u201e", "nein", ",", "heut", "!", "bei", "al\u00b7ler", "Rit\u00b7ter\u00b7schaft", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "$.", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch meine, nicht durch Feuers Kraft.\u201c", "tokens": ["Durch", "mei\u00b7ne", ",", "nicht", "durch", "Feu\u00b7ers", "Kraft", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "$,", "PTKNEG", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling spricht\u2019s, ihn Kraft durchdringt,", "tokens": ["Der", "J\u00fcng\u00b7ling", "spricht's", ",", "ihn", "Kraft", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schwerdt er hoch in L\u00fcften schwingt.", "tokens": ["Das", "Schwerdt", "er", "hoch", "in", "L\u00fcf\u00b7ten", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}