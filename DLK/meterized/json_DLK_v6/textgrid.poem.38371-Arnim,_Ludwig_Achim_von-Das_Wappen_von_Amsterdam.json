{"textgrid.poem.38371": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Wappen von Amsterdam", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gieng einmal nach Amsterdam,", "tokens": ["Ich", "gieng", "ein\u00b7mal", "nach", "A\u00b7mster\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+++--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Auf der Faullenzer Stra\u00dfe,", "tokens": ["Auf", "der", "Faul\u00b7len\u00b7zer", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Man fragt mich, ob ich faullenzen kann,", "tokens": ["Man", "fragt", "mich", ",", "ob", "ich", "faul\u00b7len\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich sagte nein, und meint doch ja,", "tokens": ["Ich", "sag\u00b7te", "nein", ",", "und", "meint", "doch", "ja", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKANT", "$,", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sezt mich nieder und faullenzt da,", "tokens": ["Ich", "sezt", "mich", "nie\u00b7der", "und", "faul\u00b7lenzt", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es war wohl tausend Gulden werth,", "tokens": ["Es", "war", "wohl", "tau\u00b7send", "Gul\u00b7den", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Daf\u00fcr kauft ich ein sch\u00f6nes Pferd,", "tokens": ["Da\u00b7f\u00fcr", "kauft", "ich", "ein", "sch\u00f6\u00b7nes", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wars kein junges, wars ein alts,", "tokens": ["Wars", "kein", "jun\u00b7ges", ",", "wars", "ein", "alts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "$,", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohne Kopf und ohne Hals,", "tokens": ["Oh\u00b7ne", "Kopf", "und", "oh\u00b7ne", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ohne Schenkel, ohne Bein,", "tokens": ["Oh\u00b7ne", "Schen\u00b7kel", ",", "oh\u00b7ne", "Bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf dem Pferd ritt ich allein,", "tokens": ["Auf", "dem", "Pferd", "ritt", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf dem Pferd ritt ich so lang,", "tokens": ["Auf", "dem", "Pferd", "ritt", "ich", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Bis ihm gar der Bauch zersprang.", "tokens": ["Bis", "ihm", "gar", "der", "Bauch", "zer\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Flog heraus ein G\u00f6ckerlein,", "tokens": ["Flog", "he\u00b7raus", "ein", "G\u00f6\u00b7cker\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Kr\u00e4hte grob und kr\u00e4hte fein,", "tokens": ["Kr\u00e4h\u00b7te", "grob", "und", "kr\u00e4h\u00b7te", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Hatt auf seinem Kopf ein Kamm,", "tokens": ["Hatt", "auf", "sei\u00b7nem", "Kopf", "ein", "Kamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Drauf stand das Wappen von Amsterdam.", "tokens": ["Drauf", "stand", "das", "Wap\u00b7pen", "von", "A\u00b7mster\u00b7dam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}