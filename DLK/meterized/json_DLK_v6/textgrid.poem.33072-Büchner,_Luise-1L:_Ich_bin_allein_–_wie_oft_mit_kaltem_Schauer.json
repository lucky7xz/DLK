{"textgrid.poem.33072": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin allein \u2013 wie oft mit kaltem Schauer", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin allein \u2013 wie oft mit kaltem Schauer", "tokens": ["Ich", "bin", "al\u00b7lein", "\u2013", "wie", "oft", "mit", "kal\u00b7tem", "Schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KOKOM", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trifft mich dies Wort, mit namenloser Trauer \u2013", "tokens": ["Trifft", "mich", "dies", "Wort", ",", "mit", "na\u00b7men\u00b7lo\u00b7ser", "Trau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob sich auch laut das Leben um mich regt;", "tokens": ["Ob", "sich", "auch", "laut", "das", "Le\u00b7ben", "um", "mich", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein \u2013 mit meinem Streben und Bem\u00fchen,", "tokens": ["Al\u00b7lein", "\u2013", "mit", "mei\u00b7nem", "Stre\u00b7ben", "und", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Allein \u2013 wenn eine andre Brust durchgl\u00fchen", "tokens": ["Al\u00b7lein", "\u2013", "wenn", "ei\u00b7ne", "and\u00b7re", "Brust", "durch\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich m\u00f6cht', mit dem, was Meine sch\u00f6n bewegt.", "tokens": ["Ich", "m\u00f6cht'", ",", "mit", "dem", ",", "was", "Mei\u00b7ne", "sch\u00f6n", "be\u00b7wegt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ART", "$,", "PRELS", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O, so allein ist nicht des S\u00fcdens Pflanze,", "tokens": ["O", ",", "so", "al\u00b7lein", "ist", "nicht", "des", "S\u00fc\u00b7dens", "Pflan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "VAFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die einzeln steht in nord'scher Blumen Kranze,", "tokens": ["Die", "ein\u00b7zeln", "steht", "in", "nord'\u00b7scher", "Blu\u00b7men", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gr\u00fc\u00dft sie hier wie dort der Sonne Ku\u00df;", "tokens": ["Es", "gr\u00fc\u00dft", "sie", "hier", "wie", "dort", "der", "Son\u00b7ne", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So einsam nicht auf weitem Feld die Eiche,", "tokens": ["So", "ein\u00b7sam", "nicht", "auf", "wei\u00b7tem", "Feld", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das sehnsuchtsvolle Rauschen ihrer Zweige,", "tokens": ["Das", "sehn\u00b7suchts\u00b7vol\u00b7le", "Rau\u00b7schen", "ih\u00b7rer", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erwiedert hold der V\u00f6gel lauter Gru\u00df.", "tokens": ["Er\u00b7wie\u00b7dert", "hold", "der", "V\u00f6\u00b7gel", "lau\u00b7ter", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl einmal auch, zwei kurze sch\u00f6ne Stunden,", "tokens": ["Wohl", "ein\u00b7mal", "auch", ",", "zwei", "kur\u00b7ze", "sch\u00f6\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hab' ich der Seele s\u00fc\u00dfen Hauch empfunden,", "tokens": ["Hab'", "ich", "der", "See\u00b7le", "s\u00fc\u00b7\u00dfen", "Hauch", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die geistverwandt mit mir die Schwinge regt;", "tokens": ["Die", "geist\u00b7ver\u00b7wandt", "mit", "mir", "die", "Schwin\u00b7ge", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch sie entschwand in endlos weite Ferne,", "tokens": ["Doch", "sie", "ent\u00b7schwand", "in", "end\u00b7los", "wei\u00b7te", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich schau' ihm nach, dem gl\u00e4nzend sch\u00f6nen Sterne,", "tokens": ["Ich", "schau'", "ihm", "nach", ",", "dem", "gl\u00e4n\u00b7zend", "sch\u00f6\u00b7nen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von milder Schwermuth wundersam bewegt.", "tokens": ["Von", "mil\u00b7der", "Schwer\u00b7muth", "wun\u00b7der\u00b7sam", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So flieht mein Leben einsam still von hinnen,", "tokens": ["So", "flieht", "mein", "Le\u00b7ben", "ein\u00b7sam", "still", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Quell, der bang im Sande mu\u00df verrinnen,", "tokens": ["Ein", "Quell", ",", "der", "bang", "im", "San\u00b7de", "mu\u00df", "ver\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nie in einen stolzen Strom sich gie\u00dft;", "tokens": ["Und", "nie", "in", "ei\u00b7nen", "stol\u00b7zen", "Strom", "sich", "gie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Epheu, der bestaubt am Boden lieget:", "tokens": ["Ein", "E\u00b7pheu", ",", "der", "be\u00b7staubt", "am", "Bo\u00b7den", "lie\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Baum, daran er sich vertrauend schmieget,", "tokens": ["Kein", "Baum", ",", "da\u00b7ran", "er", "sich", "ver\u00b7trau\u00b7end", "schmie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PAV", "PPER", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um den er liebend seine Arme schlie\u00dft!", "tokens": ["Um", "den", "er", "lie\u00b7bend", "sei\u00b7ne", "Ar\u00b7me", "schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}