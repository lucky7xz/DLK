{"dta.poem.5288": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zwo Fr\u00fchlings-Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den bunt-bebl\u00fchmten Feldern,", "tokens": ["Auf", "den", "bunt\u00b7be\u00b7bl\u00fchm\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Schatten-reichen W\u00e4ldern", "tokens": ["In", "den", "Schat\u00b7ten\u00b7rei\u00b7chen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrscht, in stiller Einsamkeit,", "tokens": ["Herrscht", ",", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unschuld und Zufriedenheit.", "tokens": ["Un\u00b7schuld", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern vom st\u00e4dtischen Get\u00fcmmel,", "tokens": ["Fern", "vom", "st\u00e4d\u00b7ti\u00b7schen", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als in einem ird\u2019schen Himmel,", "tokens": ["Als", "in", "ei\u00b7nem", "ird'\u00b7schen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Find\u2019 ich hier die g\u00fcldne Zeit.", "tokens": ["Find'", "ich", "hier", "die", "g\u00fcld\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Stille, die den Wald erf\u00fcllt,", "tokens": ["Die", "Stil\u00b7le", ",", "die", "den", "Wald", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der holden Unschuld sanftes Bild,", "tokens": ["Der", "hol\u00b7den", "Un\u00b7schuld", "sanf\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nicht von froher Anmuth leer.", "tokens": ["Ist", "nicht", "von", "fro\u00b7her", "An\u00b7muth", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleinen V\u00f6gel muntres Heer", "tokens": ["Der", "klei\u00b7nen", "V\u00f6\u00b7gel", "mun\u00b7tres", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4sst tausend s\u00fcsse T\u00f6n\u2019 erklingen.", "tokens": ["L\u00e4sst", "tau\u00b7send", "s\u00fcs\u00b7se", "T\u00f6n'", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So kann auch ein gelassnes Hertz", "tokens": ["So", "kann", "auch", "ein", "ge\u00b7lass\u00b7nes", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Recht, bey zugelassnem Schertz,", "tokens": ["Mit", "Recht", ",", "bey", "zu\u00b7ge\u00b7lass\u00b7nem", "Schertz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gesellig lachen, fr\u00f6lich singen;", "tokens": ["Ge\u00b7sel\u00b7lig", "la\u00b7chen", ",", "fr\u00f6\u00b7lich", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Weil sonst die Tugend gr\u00e4mlich w\u00e4r.", "tokens": ["Weil", "sonst", "die", "Tu\u00b7gend", "gr\u00e4m\u00b7lich", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}