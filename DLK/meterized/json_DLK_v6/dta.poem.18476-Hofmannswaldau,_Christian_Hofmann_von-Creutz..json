{"dta.poem.18476": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Creutz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Trag mit gedult des creutzes last/ wirff sie nicht hin zu deinen", "tokens": ["Trag", "mit", "ge\u00b7dult", "des", "creut\u00b7zes", "last", "/", "wirff", "sie", "nicht", "hin", "zu", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVFIN", "$(", "VVIMP", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "f\u00fcssen/", "tokens": ["f\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die sind am schlechtsten dran/ die nicht ihr leiden zu verbergen", "tokens": ["Die", "sind", "am", "schlechts\u00b7ten", "dran", "/", "die", "nicht", "ihr", "lei\u00b7den", "zu", "ver\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "PAV", "$(", "PRELS", "PTKNEG", "PPER", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "wissen.", "tokens": ["wis\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der h\u00f6chste grad der thorheit ist: Da\u00dfelbe tollk\u00fchu \u00e4ndern wollen/", "tokens": ["Der", "h\u00f6chs\u00b7te", "grad", "der", "thor\u00b7heit", "ist", ":", "Da\u00b7\u00dfel\u00b7be", "toll\u00b7k\u00fc\u00b7hu", "\u00e4n\u00b7dern", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "$.", "PDAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Was keiner/ als die rechte zeit/ wird \u00e4ndern k\u00f6nnen/ oder sollen.", "tokens": ["Was", "kei\u00b7ner", "/", "als", "die", "rech\u00b7te", "zeit", "/", "wird", "\u00e4n\u00b7dern", "k\u00f6n\u00b7nen", "/", "o\u00b7der", "sol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "$(", "KOUS", "ART", "ADJA", "NN", "$(", "VAFIN", "VVINF", "VMINF", "$(", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}