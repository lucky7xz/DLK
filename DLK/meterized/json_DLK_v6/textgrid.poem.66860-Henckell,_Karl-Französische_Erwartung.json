{"textgrid.poem.66860": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Franz\u00f6sische Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ihrer Chaiselongue schnellt", "tokens": ["Von", "ih\u00b7rer", "Chai\u00b7se\u00b7lon\u00b7gue", "schnellt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Knute greift sie, frohlockt und schellt:", "tokens": ["Zur", "Knu\u00b7te", "greift", "sie", ",", "froh\u00b7lockt", "und", "schellt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gekommen ist der gepriesene Tag,", "tokens": ["Ge\u00b7kom\u00b7men", "ist", "der", "ge\u00b7prie\u00b7se\u00b7ne", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun d\u00fcrfen wir gar nicht knacken,", "tokens": ["Nun", "d\u00fcr\u00b7fen", "wir", "gar", "nicht", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir geben ein riesiges Festgelag", "tokens": ["Wir", "ge\u00b7ben", "ein", "rie\u00b7si\u00b7ges", "Fest\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Unsern lieben Herren Kosaken.", "tokens": ["Un\u00b7sern", "lie\u00b7ben", "Her\u00b7ren", "Ko\u00b7sa\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun tut ihnen jegliche Reverenz", "tokens": ["Nun", "tut", "ih\u00b7nen", "jeg\u00b7li\u00b7che", "Re\u00b7ve\u00b7renz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und unserm Besch\u00fctzer, dem Zaren,", "tokens": ["Und", "un\u00b7serm", "Be\u00b7sch\u00fct\u00b7zer", ",", "dem", "Za\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun soll die franz\u00f6sische Intelligenz", "tokens": ["Nun", "soll", "die", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "In\u00b7tel\u00b7li\u00b7genz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit dem Freiheits-Kantschu sich paaren.", "tokens": ["Mit", "dem", "Frei\u00b7heits\u00b7Kant\u00b7schu", "sich", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Zujauchzt dem s\u00fc\u00dfen sibirischen Ku\u00df,", "tokens": ["Zu\u00b7jauchzt", "dem", "s\u00fc\u00b7\u00dfen", "si\u00b7bi\u00b7ri\u00b7schen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Duftend nach Schmierfett und K\u00e4se,", "tokens": ["Duf\u00b7tend", "nach", "Schmier\u00b7fett", "und", "K\u00e4\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In meinem Busen der Genius", "tokens": ["In", "mei\u00b7nem", "Bu\u00b7sen", "der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "O selige Zeit, o goldene Zeit!", "tokens": ["O", "se\u00b7li\u00b7ge", "Zeit", ",", "o", "gol\u00b7de\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu Kronstadt ging's auf die Freite,", "tokens": ["Zu", "Kron\u00b7stadt", "ging's", "auf", "die", "Frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da ward unsre Freundschaft eingeweiht,", "tokens": ["Da", "ward", "uns\u00b7re", "Freund\u00b7schaft", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir besahn uns von jeder Seite.", "tokens": ["Wir", "be\u00b7sahn", "uns", "von", "je\u00b7der", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir gaben die Hand uns tiefger\u00fchrt,", "tokens": ["Wir", "ga\u00b7ben", "die", "Hand", "uns", "tief\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Champagner ward liebend getrunken,", "tokens": ["Cham\u00b7pag\u00b7ner", "ward", "lie\u00b7bend", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir haben den B\u00e4rentanz aufgef\u00fchrt", "tokens": ["Wir", "ha\u00b7ben", "den", "B\u00e4\u00b7ren\u00b7tanz", "auf\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sind in Verz\u00fcckung versunken.", "tokens": ["Und", "sind", "in", "Ver\u00b7z\u00fc\u00b7ckung", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und die Liebe bl\u00fchte, die Freundschaft schwoll,", "tokens": ["Und", "die", "Lie\u00b7be", "bl\u00fch\u00b7te", ",", "die", "Freund\u00b7schaft", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Verh\u00e4ltnis steht auf dem Gipfel,", "tokens": ["Das", "Ver\u00b7h\u00e4lt\u00b7nis", "steht", "auf", "dem", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vor der Zottelm\u00fctze baumelt toll", "tokens": ["Vor", "der", "Zot\u00b7tel\u00b7m\u00fct\u00b7ze", "bau\u00b7melt", "toll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner phrygischen M\u00fctze Zipfel.", "tokens": ["Mei\u00b7ner", "phry\u00b7gi\u00b7schen", "M\u00fct\u00b7ze", "Zip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und die elys\u00e4ischen Felder!", "tokens": ["Und", "die", "e\u00b7ly\u00b7s\u00e4\u00b7i\u00b7schen", "Fel\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am liebsten w\u00fcrd' ich vor Wollust verr\u00fcckt", "tokens": ["Am", "liebs\u00b7ten", "w\u00fcrd'", "ich", "vor", "Wol\u00b7lust", "ver\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und opferte all meine Gelder.", "tokens": ["Und", "op\u00b7fer\u00b7te", "all", "mei\u00b7ne", "Gel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Der Liebe ist ja kein Opfer zu gro\u00df,", "tokens": ["Der", "Lie\u00b7be", "ist", "ja", "kein", "Op\u00b7fer", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie m\u00f6chte alles gew\u00e4hren,", "tokens": ["Sie", "m\u00f6ch\u00b7te", "al\u00b7les", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Sehnsucht brennende Z\u00e4hren.", "tokens": ["Der", "Sehn\u00b7sucht", "bren\u00b7nen\u00b7de", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Die Tore auf! Wie ein Paradies", "tokens": ["Die", "To\u00b7re", "auf", "!", "Wie", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PWAV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll Paris zur Parade sich kleiden \u2013", "tokens": ["Soll", "Pa\u00b7ris", "zur", "Pa\u00b7ra\u00b7de", "sich", "klei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPRART", "NN", "PRF", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn morgen schon l\u00e4\u00dft man sich scheiden!\u00ab", "tokens": ["Denn", "mor\u00b7gen", "schon", "l\u00e4\u00dft", "man", "sich", "schei\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PIS", "PRF", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}