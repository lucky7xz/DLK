{"dta.poem.13023": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia sagte mir: \u201eDie r\u00fchrt die st\u00e4rcksten Geister;", "tokens": ["Tha\u00b7lia", "sag\u00b7te", "mir", ":", "\u201e", "Die", "r\u00fchrt", "die", "st\u00e4rcks\u00b7ten", "Geis\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201ees ist ", "tokens": ["\u201e", "es", "ist"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u201esie deckt die Falschheit auf, so die mit Worten f\u00e4rbt;", "tokens": ["\u201e", "sie", "deckt", "die", "Falschheit", "auf", ",", "so", "die", "mit", "Wor\u00b7ten", "f\u00e4rbt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201esie stellt aufrichtig her, was die mit List verderbt.", "tokens": ["\u201e", "sie", "stellt", "auf\u00b7rich\u00b7tig", "her", ",", "was", "die", "mit", "List", "ver\u00b7derbt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PRELS", "ART", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "135\u201dDer traut man nur mit Furcht, weil ihre Wort' und Thaten", "tokens": ["\"", "Der", "traut", "man", "nur", "mit", "Furcht", ",", "weil", "ih\u00b7re", "Wort'", "und", "Tha\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "PIS", "ADV", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201esich wiedersprechend seynd, und oft in Zwist gerathen;", "tokens": ["\u201e", "sich", "wie\u00b7der\u00b7spre\u00b7chend", "seynd", ",", "und", "oft", "in", "Zwist", "ge\u00b7ra\u00b7then", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "VVPP", "VAFIN", "$,", "KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewo jene nur verspricht, was gleich geschehen soll:", "tokens": ["\u201e", "wo", "je\u00b7ne", "nur", "ver\u00b7spricht", ",", "was", "gleich", "ge\u00b7sche\u00b7hen", "soll", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PDS", "ADV", "VVPP", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edaher bezeigen sie einander einen Groll.", "tokens": ["\u201e", "da\u00b7her", "be\u00b7zei\u00b7gen", "sie", "ein\u00b7an\u00b7der", "ei\u00b7nen", "Groll", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eder beyden Meinungen seynd selten zu vergleichen,", "tokens": ["\u201e", "der", "bey\u00b7den", "Mei\u00b7nun\u00b7gen", "seynd", "sel\u00b7ten", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "140\u201dSie m\u00fcssen sich sehr oft in den Gesch\u00e4fften weichen.", "tokens": ["\"", "Sie", "m\u00fcs\u00b7sen", "sich", "sehr", "oft", "in", "den", "Ge\u00b7sch\u00e4ff\u00b7ten", "wei\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}