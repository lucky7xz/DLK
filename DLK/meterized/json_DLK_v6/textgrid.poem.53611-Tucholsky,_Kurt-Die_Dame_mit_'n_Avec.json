{"textgrid.poem.53611": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Dame mit 'n Avec", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle k\u00f6nn sie mir, k\u00f6nn sie mir, k\u00f6nn sie mir!", "tokens": ["Al\u00b7le", "k\u00f6nn", "sie", "mir", ",", "k\u00f6nn", "sie", "mir", ",", "k\u00f6nn", "sie", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "$,", "VMFIN", "PPER", "PPER", "$,", "VMFIN", "PPER", "PPER", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Huch, die M\u00e4nner!", "tokens": ["Huch", ",", "die", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sie sind alle hier, alle hier, alle hier", "tokens": ["Sie", "sind", "al\u00b7le", "hier", ",", "al\u00b7le", "hier", ",", "al\u00b7le", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "PIS", "ADV", "$,", "PIS", "ADV"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "nischt wie Penner!", "tokens": ["nischt", "wie", "Pen\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Erst da tun sie m\u00e4chtig fein,", "tokens": ["Erst", "da", "tun", "sie", "m\u00e4ch\u00b7tig", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "laden mich zum Abend ein.", "tokens": ["la\u00b7den", "mich", "zum", "A\u00b7bend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich kann mich dann nicht halten,", "tokens": ["Und", "ich", "kann", "mich", "dann", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "seh ich des Monokels Glanz \u2013:", "tokens": ["seh", "ich", "des", "Mo\u00b7no\u00b7kels", "Glanz", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "sag den Jungen und den Alten", "tokens": ["sag", "den", "Jun\u00b7gen", "und", "den", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "grad heraus beim Foxtrott-Tanz:", "tokens": ["grad", "he\u00b7raus", "beim", "Fox\u00b7trot\u00b7tTanz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00bbich hab nu mal den Schwung", "tokens": ["\u00bb", "ich", "hab", "nu", "mal", "den", "Schwung"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "ins Ordin\u00e4re!", "tokens": ["ins", "Or\u00b7di\u00b7n\u00e4\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ick bin die richtige", "tokens": ["Ick", "bin", "die", "rich\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "berliner Beere!", "tokens": ["ber\u00b7li\u00b7ner", "Bee\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.15": {"text": "Und bei der Liebe hopps ick jrade wie bein Zeck", "tokens": ["Und", "bei", "der", "Lie\u00b7be", "hopps", "ick", "jra\u00b7de", "wie", "bein", "Zeck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nur \u00fcbern Rinnstein, Rinnstein, Rinnstein", "tokens": ["nur", "\u00fc\u00b7bern", "Rinn\u00b7stein", ",", "Rinn\u00b7stein", ",", "Rinn\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "---+-+-++", "measure": "unknown.measure.tetra"}, "line.17": {"text": "mit 'n Avec!\u00ab", "tokens": ["mit", "'n", "A\u00b7vec", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NE", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Uffn Koppenplatz, Koppenplatz, Koppenplatz", "tokens": ["Uffn", "Kop\u00b7pen\u00b7platz", ",", "Kop\u00b7pen\u00b7platz", ",", "Kop\u00b7pen\u00b7platz"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "lief ick lange.", "tokens": ["lief", "ick", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mitn Sabberlatz, Sabberlatz, Sabberlatz \u2013", "tokens": ["Mitn", "Sab\u00b7ber\u00b7latz", ",", "Sab\u00b7ber\u00b7latz", ",", "Sab\u00b7ber\u00b7latz", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "NE", "$,", "NE", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "'ck wah 'ne Range \u2013!", "tokens": ["'ck", "wah", "'ne", "Ran\u00b7ge", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und mit vierzehn Jahren schon", "tokens": ["Und", "mit", "vier\u00b7zehn", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ging ich bei die Konfektion.", "tokens": ["ging", "ich", "bei", "die", "Kon\u00b7fek\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Das war eine feine Lehre", "tokens": ["Das", "war", "ei\u00b7ne", "fei\u00b7ne", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "in dem gro\u00dfen Modenhaus;", "tokens": ["in", "dem", "gro\u00b7\u00dfen", "Mo\u00b7den\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "und ich machte rasch Karriere,", "tokens": ["und", "ich", "mach\u00b7te", "rasch", "Kar\u00b7rie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "aber manchmal kommt es raus \u2013 \u2013", "tokens": ["a\u00b7ber", "manch\u00b7mal", "kommt", "es", "raus", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ich hab nu mal den Schwung", "tokens": ["Ich", "hab", "nu", "mal", "den", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "ins Ordin\u00e4re!", "tokens": ["ins", "Or\u00b7di\u00b7n\u00e4\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ick bin die richtige", "tokens": ["Ick", "bin", "die", "rich\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "berliner Beere!", "tokens": ["ber\u00b7li\u00b7ner", "Bee\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.15": {"text": "Und bei der Liebe hopps ick jrade wie bein Zeck", "tokens": ["Und", "bei", "der", "Lie\u00b7be", "hopps", "ick", "jra\u00b7de", "wie", "bein", "Zeck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nur \u00fcbern Rinnstein, Rinnstein, Rinnstein", "tokens": ["nur", "\u00fc\u00b7bern", "Rinn\u00b7stein", ",", "Rinn\u00b7stein", ",", "Rinn\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "---+-+-++", "measure": "unknown.measure.tetra"}, "line.17": {"text": "mit 'n Avec!", "tokens": ["mit", "'n", "A\u00b7vec", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Fahr ick viere lang, viere lang, viere lang", "tokens": ["Fahr", "ick", "vie\u00b7re", "lang", ",", "vie\u00b7re", "lang", ",", "vie\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ADJD", "$,", "PIS", "ADJD", "$,", "PIS", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Eklipage,", "tokens": ["Ek\u00b7li\u00b7pa\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "sitz ich ersten Rang, ersten Rang, ersten Rang", "tokens": ["sitz", "ich", "ers\u00b7ten", "Rang", ",", "ers\u00b7ten", "Rang", ",", "ers\u00b7ten", "Rang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "in Kleidage:", "tokens": ["in", "Klei\u00b7da\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Alle Leute drehn sich rum \u2013", "tokens": ["Al\u00b7le", "Leu\u00b7te", "drehn", "sich", "rum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "CARD", "PRF", "ADV", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Donner! die ist gar nicht dumm!", "tokens": ["Don\u00b7ner", "!", "die", "ist", "gar", "nicht", "dumm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcngelnd sitzt bei mir mein H\u00fcndchen.", "tokens": ["Z\u00fcn\u00b7gelnd", "sitzt", "bei", "mir", "mein", "H\u00fcnd\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Autsch! wie mein Brillantschmuck blitzt \u2013", "tokens": ["Autsch", "!", "wie", "mein", "Bril\u00b7lant\u00b7schmuck", "blitzt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Aber spitze ich mein M\u00fcndchen,", "tokens": ["A\u00b7ber", "spit\u00b7ze", "ich", "mein", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "wei\u00dfte gleich, wer vor dir sitzt \u2013!", "tokens": ["wei\u00df\u00b7te", "gleich", ",", "wer", "vor", "dir", "sitzt", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "APPR", "PPER", "VVFIN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ich hab nu mal den Schwung", "tokens": ["Ich", "hab", "nu", "mal", "den", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "ins Ordin\u00e4re!", "tokens": ["ins", "Or\u00b7di\u00b7n\u00e4\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ick bin die richtige", "tokens": ["Ick", "bin", "die", "rich\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "berliner Beere!", "tokens": ["ber\u00b7li\u00b7ner", "Bee\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.15": {"text": "Und bei der Liebe hopps ick jrade wie bein Zeck", "tokens": ["Und", "bei", "der", "Lie\u00b7be", "hopps", "ick", "jra\u00b7de", "wie", "bein", "Zeck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nur \u00fcbern Rinnstein, Rinnstein, Rinnstein", "tokens": ["nur", "\u00fc\u00b7bern", "Rinn\u00b7stein", ",", "Rinn\u00b7stein", ",", "Rinn\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "---+-+-++", "measure": "unknown.measure.tetra"}, "line.17": {"text": "mit 'n Avec!", "tokens": ["mit", "'n", "A\u00b7vec", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}