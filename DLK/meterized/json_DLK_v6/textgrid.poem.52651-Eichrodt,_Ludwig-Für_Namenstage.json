{"textgrid.poem.52651": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Namenstage", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es nicht Verwegenheit", "tokens": ["Ist", "es", "nicht", "Ver\u00b7we\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Namenstag zu feiern,", "tokens": ["Ei\u00b7nen", "Na\u00b7mens\u00b7tag", "zu", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ein Gedicht herunterleiern", "tokens": ["Ein", "Ge\u00b7dicht", "her\u00b7un\u00b7ter\u00b7lei\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Selbstgen\u00fcgsamkeit", "tokens": ["Und", "mit", "Selbst\u00b7ge\u00b7n\u00fcg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Antheil drin betheuern?", "tokens": ["Sei\u00b7nen", "An\u00b7theil", "drin", "be\u00b7theu\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freilich ist es viel gewagt.", "tokens": ["Frei\u00b7lich", "ist", "es", "viel", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn was soll an solchen Tagen", "tokens": ["Denn", "was", "soll", "an", "sol\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Anders mehr der Kluge sagen,", "tokens": ["An\u00b7ders", "mehr", "der", "Klu\u00b7ge", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als da\u00df nun der Tag getagt,", "tokens": ["Als", "da\u00df", "nun", "der", "Tag", "ge\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Kalender nachzuschlagen?", "tokens": ["Im", "Ka\u00b7len\u00b7der", "nach\u00b7zu\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wahrlich, namenlose Pein", "tokens": ["Wahr\u00b7lich", ",", "na\u00b7men\u00b7lo\u00b7se", "Pein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Namenstag zu singen,", "tokens": ["Auf", "den", "Na\u00b7mens\u00b7tag", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm Gedanken abzuringen!", "tokens": ["Ihm", "Ge\u00b7dan\u00b7ken", "ab\u00b7zu\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum in den Tag hinein", "tokens": ["Da\u00b7rum", "in", "den", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "NN", "APZR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mu\u00df der gute S\u00e4nger singen.", "tokens": ["Mu\u00df", "der", "gu\u00b7te", "S\u00e4n\u00b7ger", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und er merket sich den Rath,", "tokens": ["Und", "er", "mer\u00b7ket", "sich", "den", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er mag hier ohne Zaudern,", "tokens": ["Und", "er", "mag", "hier", "oh\u00b7ne", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonder Etiketteschaudern,", "tokens": ["Son\u00b7der", "E\u00b7ti\u00b7ket\u00b7te\u00b7schau\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er auf dem Herzen hat", "tokens": ["Was", "er", "auf", "dem", "Her\u00b7zen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Gelegenheit verplaudern.", "tokens": ["Mit", "Ge\u00b7le\u00b7gen\u00b7heit", "ver\u00b7plau\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So zum Beispiel: sch\u00f6ne Frau,", "tokens": ["So", "zum", "Bei\u00b7spiel", ":", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freund, o bester, Sie verzeihen,", "tokens": ["Freund", ",", "o", "bes\u00b7ter", ",", "Sie", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJD", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Theures M\u00e4dchen, Ihnen weihen", "tokens": ["Theu\u00b7res", "M\u00e4d\u00b7chen", ",", "Ih\u00b7nen", "wei\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht ich \u2013 Freund, ja mir vertrau,", "tokens": ["M\u00f6cht", "ich", "\u2013", "Freund", ",", "ja", "mir", "ver\u00b7trau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Willst du mir zwei Hundert leihen?", "tokens": ["Willst", "du", "mir", "zwei", "Hun\u00b7dert", "lei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}