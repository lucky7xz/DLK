{"textgrid.poem.31424": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? solt wol unsre Noht den Allmachts Arm selbst binden?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? solt wol unsre Noht den Allmachts Arm selbst binden?", "tokens": ["Wie", "?", "solt", "wol", "uns\u00b7re", "Noht", "den", "All\u00b7machts", "Arm", "selbst", "bin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ADV", "PPOSAT", "NN", "ART", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist sie zu lang' und hoch / da\u00df dieser nicht hinreicht /", "tokens": ["ist", "sie", "zu", "lang'", "und", "hoch", "/", "da\u00df", "die\u00b7ser", "nicht", "hin\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "KON", "ADJD", "$(", "KOUS", "PDS", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der doch die Meer-Abgr\u00fcnd' und Sternen-Krei\u00df durchstreicht?", "tokens": ["der", "doch", "die", "Meer\u00b7Ab\u00b7gr\u00fcnd'", "und", "Ster\u00b7nen\u00b7Krei\u00df", "durch\u00b7streicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er kan die Tr\u00fcbsal auch / Allherrschend \u00fcberwinden", "tokens": ["Er", "kan", "die", "Tr\u00fcb\u00b7sal", "auch", "/", "All\u00b7herr\u00b7schend", "\u00fc\u00b7berw\u00b7in\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "$(", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein sagt mir / was sich w\u00fcrd' ohn sein' Erhaltung finden /", "tokens": ["Mein", "sagt", "mir", "/", "was", "sich", "w\u00fcrd'", "ohn", "sein'", "Er\u00b7hal\u00b7tung", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$(", "PWS", "PRF", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dem iedes Ding gehorcht / auch iede Sache weicht?", "tokens": ["dem", "ie\u00b7des", "Ding", "ge\u00b7horcht", "/", "auch", "ie\u00b7de", "Sa\u00b7che", "weicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$(", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "das sch\u00fctzen ist Ihm so / wie das erschaffen / leicht.", "tokens": ["das", "sch\u00fct\u00b7zen", "ist", "Ihm", "so", "/", "wie", "das", "er\u00b7schaf\u00b7fen", "/", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "PPER", "ADV", "$(", "KOKOM", "PDS", "VVINF", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der alles werden heist / macht alles auch verschwinden.", "tokens": ["der", "al\u00b7les", "wer\u00b7den", "heist", "/", "macht", "al\u00b7les", "auch", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAINF", "VAFIN", "$(", "VVFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dir / dir befihl ich mich / du Allerschaffungs Hand /", "tokens": ["Dir", "/", "dir", "be\u00b7fihl", "ich", "mich", "/", "du", "Al\u00b7ler\u00b7schaf\u00b7fungs", "Hand", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "VVFIN", "PPER", "PRF", "$(", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit dir getraut' ich mir auch \u00fcbermenschte Thaten", "tokens": ["mit", "dir", "ge\u00b7traut'", "ich", "mir", "auch", "\u00fc\u00b7ber\u00b7menschte", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "zu \u00fcben / ob sie auch schon \u00fcber Krafft und Stand.", "tokens": ["zu", "\u00fc\u00b7ben", "/", "ob", "sie", "auch", "schon", "\u00fc\u00b7ber", "Krafft", "und", "Stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das was du segnest / geht (tobt H\u00f6ll und Welt) von statten.", "tokens": ["das", "was", "du", "seg\u00b7nest", "/", "geht", "(", "tobt", "H\u00f6ll", "und", "Welt", ")", "von", "stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "NN", "KON", "NN", "$(", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich zweifle nun nicht mehr (kan ich schon nichts) an mir.", "tokens": ["Ich", "zweif\u00b7le", "nun", "nicht", "mehr", "(", "kan", "ich", "schon", "nichts", ")", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$(", "VMFIN", "PPER", "ADV", "PIS", "$(", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein ganze Kunst ist die / geleitet seyn von dir.", "tokens": ["Mein", "gan\u00b7ze", "Kunst", "ist", "die", "/", "ge\u00b7lei\u00b7tet", "seyn", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "$(", "VVPP", "VAINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}