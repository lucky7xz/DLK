{"textgrid.poem.26128": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mit den Armen nackt wie ihr Gewissen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit den Armen nackt, wie ihr Gewissen,", "tokens": ["Mit", "den", "Ar\u00b7men", "nackt", ",", "wie", "ihr", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liegt die Liebste in den Kissen, in den wei\u00dfen.", "tokens": ["Liegt", "die", "Liebs\u00b7te", "in", "den", "Kis\u00b7sen", ",", "in", "den", "wei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Fr\u00fchling hat die Fenster aufgerissen,", "tokens": ["Fr\u00fch\u00b7ling", "hat", "die", "Fens\u00b7ter", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sonne rollt den Leib den fr\u00fchlingshei\u00dfen.", "tokens": ["Son\u00b7ne", "rollt", "den", "Leib", "den", "fr\u00fch\u00b7lings\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit der Lust von sch\u00f6nen wilden Tieren", "tokens": ["Mit", "der", "Lust", "von", "sch\u00f6\u00b7nen", "wil\u00b7den", "Tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kommt die Sonne breit auf allen Vieren,", "tokens": ["Kommt", "die", "Son\u00b7ne", "breit", "auf", "al\u00b7len", "Vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sonne hat f\u00fcr meine Liebste Zeit;", "tokens": ["Son\u00b7ne", "hat", "f\u00fcr", "mei\u00b7ne", "Liebs\u00b7te", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie die Katzen liegen sie beisammen,", "tokens": ["Wie", "die", "Kat\u00b7zen", "lie\u00b7gen", "sie", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wie die Katzen, deren Haare Funken flammen.", "tokens": ["Wie", "die", "Kat\u00b7zen", ",", "de\u00b7ren", "Haa\u00b7re", "Fun\u00b7ken", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}