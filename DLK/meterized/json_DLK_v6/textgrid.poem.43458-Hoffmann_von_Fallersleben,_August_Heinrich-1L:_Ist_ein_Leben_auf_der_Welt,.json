{"textgrid.poem.43458": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist ein Leben auf der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist ein Leben auf der Welt,", "tokens": ["Ist", "ein", "Le\u00b7ben", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das vor allem mir gef\u00e4llt,", "tokens": ["Das", "vor", "al\u00b7lem", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es das Studentenleben,", "tokens": ["Ist", "es", "das", "Stu\u00b7den\u00b7ten\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Weil's von lauter Lust umgeben.", "tokens": ["Weil's", "von", "lau\u00b7ter", "Lust", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lustig ist das Commersieren,", "tokens": ["Lus\u00b7tig", "ist", "das", "Com\u00b7mer\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Musicieren und Spatzieren,", "tokens": ["Mu\u00b7si\u00b7cie\u00b7ren", "und", "Spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lustig ist auch das Studieren.", "tokens": ["Lus\u00b7tig", "ist", "auch", "das", "Stu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Heute lustig, morgen froh,", "tokens": ["Heu\u00b7te", "lus\u00b7tig", ",", "mor\u00b7gen", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcbermorgen wieder so,", "tokens": ["\u00dc\u00b7ber\u00b7mor\u00b7gen", "wie\u00b7der", "so", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Immer, immer frisch, frei, froh,", "tokens": ["Im\u00b7mer", ",", "im\u00b7mer", "frisch", ",", "frei", ",", "froh", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Juchhei\u00dfa! hei\u00dfa! ho hoho!", "tokens": ["Juch\u00b7hei\u00b7\u00dfa", "!", "hei\u00b7\u00dfa", "!", "ho", "ho\u00b7ho", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "XY", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Lebt der Bruder Studio.", "tokens": ["Lebt", "der", "Bru\u00b7der", "Stu\u00b7dio", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist ein Leben auf der Welt,", "tokens": ["Ist", "ein", "Le\u00b7ben", "auf", "der", "Welt", ","], "tok