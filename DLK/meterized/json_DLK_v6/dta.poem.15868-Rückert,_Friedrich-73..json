{"dta.poem.15868": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ich gangen bin aufs Land und sitzen blieben", "tokens": ["Wa\u00b7rum", "ich", "gan\u00b7gen", "bin", "aufs", "Land", "und", "sit\u00b7zen", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "APPRART", "NN", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim ersten Haus, nicht weit mit euch mich umgetrieben?", "tokens": ["Beim", "ers\u00b7ten", "Haus", ",", "nicht", "weit", "mit", "euch", "mich", "um\u00b7ge\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Kukuk, meinen Freund, wollt' ich nur h\u00f6ren schrein;", "tokens": ["Den", "Ku\u00b7kuk", ",", "mei\u00b7nen", "Freund", ",", "wollt'", "ich", "nur", "h\u00f6\u00b7ren", "schrein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier schreit er mir hell genug ins Haus herein.", "tokens": ["Und", "hier", "schreit", "er", "mir", "hell", "ge\u00b7nug", "ins", "Haus", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "