{"textgrid.poem.34763": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ragt ins Meer der Runenstein,", "tokens": ["Es", "ragt", "ins", "Meer", "der", "Ru\u00b7nen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitz ich mit meinen Tr\u00e4umen.", "tokens": ["Da", "sitz", "ich", "mit", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es pfeift der Wind, die M\u00f6wen schrein,", "tokens": ["Es", "pfeift", "der", "Wind", ",", "die", "M\u00f6\u00b7wen", "schrein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wellen, die wandern und sch\u00e4umen.", "tokens": ["Die", "Wel\u00b7len", ",", "die", "wan\u00b7dern", "und", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe geliebt manch sch\u00f6nes Kind", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7liebt", "manch", "sch\u00f6\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und manchen guten Gesellen \u2013", "tokens": ["Und", "man\u00b7chen", "gu\u00b7ten", "Ge\u00b7sel\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo sind sie hin? Es pfeift der Wind,", "tokens": ["Wo", "sind", "sie", "hin", "?", "Es", "pfeift", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sch\u00e4umen und wandern die Wellen.", "tokens": ["Es", "sch\u00e4u\u00b7men", "und", "wan\u00b7dern", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}