{"dta.poem.2964": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wirkte gro\u00df und wirkt, kann in sich seyn nicht nichtig:", "tokens": ["Was", "wirk\u00b7te", "gro\u00df", "und", "wirkt", ",", "kann", "in", "sich", "seyn", "nicht", "nich\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "KON", "VVFIN", "$,", "VMFIN", "APPR", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang es dis dir scheint, sahst du es noch nicht richtig.", "tokens": ["So\u00b7lang", "es", "dis", "dir", "scheint", ",", "sahst", "du", "es", "noch", "nicht", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch richtig siehst du nie, wo du dich selbst verblendest,", "tokens": ["Doch", "rich\u00b7tig", "siehst", "du", "nie", ",", "wo", "du", "dich", "selbst", "ver\u00b7blen\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts erkennest du, wo du dich stolz abwendest.", "tokens": ["Und", "nichts", "er\u00b7ken\u00b7nest", "du", ",", "wo", "du", "dich", "stolz", "ab\u00b7wen\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Komm, Sohn, und la\u00df uns unbefangen, ohne voran", "tokens": ["Komm", ",", "Sohn", ",", "und", "la\u00df", "uns", "un\u00b7be\u00b7fan\u00b7gen", ",", "oh\u00b7ne", "vo\u00b7ran"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "ADJD", "$,", "KOUI", "PTKVZ"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Abzuurtheilen, auch urtheilen \u00fcbern Koran.", "tokens": ["Ab\u00b7zu\u00b7urt\u00b7hei\u00b7len", ",", "auch", "ur\u00b7thei\u00b7len", "\u00fc\u00b7bern", "Ko\u00b7ran", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "VVFIN", "NE", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Wol eine Zauberkraft mu\u00df seyn in dem, woran", "tokens": ["Wol", "ei\u00b7ne", "Zau\u00b7ber\u00b7kraft", "mu\u00df", "seyn", "in", "dem", ",", "wo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPOSAT", "APPR", "ART", "$,", "PWAV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bezaubert eine Welt so h\u00e4ngt wie am Koran.", "tokens": ["Be\u00b7zau\u00b7bert", "ei\u00b7ne", "Welt", "so", "h\u00e4ngt", "wie", "am", "Ko\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}