{"dta.poem.1264": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was zwingt die Liebe nicht? Cupideus List und", "tokens": ["Was", "zwingt", "die", "Lie\u00b7be", "nicht", "?", "Cu\u00b7pi\u00b7deus", "List", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "$.", "NE", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Macht", "tokens": ["Macht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Hat manchen Jupiter in seine Netze bracht.", "tokens": ["Hat", "man\u00b7chen", "Ju\u00b7pi\u00b7ter", "in", "sei\u00b7ne", "Net\u00b7ze", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gradivens k\u00fchner Leib in Stahl geschlossen ein", "tokens": ["Gra\u00b7di\u00b7vens", "k\u00fch\u00b7ner", "Leib", "in", "Stahl", "ge\u00b7schlos\u00b7sen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "VVPP", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Kan f\u00fcr den Waffen nicht der Venus sicher seyn.", "tokens": ["Kan", "f\u00fcr", "den", "Waf\u00b7fen", "nicht", "der", "Ve\u00b7nus", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Sch\u00f6nheit brennend Glantz verst\u00e4rckt das helle", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "bren\u00b7nend", "Glantz", "ver\u00b7st\u00e4rckt", "das", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Licht/", "tokens": ["Licht", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Im fall sein Gegen-Schein auff festen Stahl gericht/", "tokens": ["Im", "fall", "sein", "Ge\u00b7gen\u00b7Schein", "auff", "fes\u00b7ten", "Stahl", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein Hertze findet sich so eisenhart und kalt/", "tokens": ["Kein", "Hert\u00b7ze", "fin\u00b7det", "sich", "so", "ei\u00b7sen\u00b7hart", "und", "kalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie bildet in ihm ab die liebliche Gestalt", "tokens": ["Sie", "bil\u00b7det", "in", "ihm", "ab", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Sch\u00f6nen Angesichts/ und heget/ wo nicht Brunst/", "tokens": ["Des", "Sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sichts", "/", "und", "he\u00b7get", "/", "wo", "nicht", "Brunst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "KON", "VVFIN", "$(", "PWAV", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch eine stille Glutt und zugethane Gunst.", "tokens": ["Doch", "ei\u00b7ne", "stil\u00b7le", "Glutt", "und", "zu\u00b7ge\u00b7tha\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Menschen Eigenschafft/ des Menschen Sinn und", "tokens": ["Des", "Men\u00b7schen", "Ei\u00b7gen\u00b7schafft", "/", "des", "Men\u00b7schen", "Sinn", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Stand/", "tokens": ["Stand", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Die Ordnung der Natur bringt mit sich solchen Brand/", "tokens": ["Die", "Ord\u00b7nung", "der", "Na\u00b7tur", "bringt", "mit", "sich", "sol\u00b7chen", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PRF", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den ein verliebter Geist in allen Adern f\u00fchlt/", "tokens": ["Den", "ein", "ver\u00b7lieb\u00b7ter", "Geist", "in", "al\u00b7len", "A\u00b7dern", "f\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit steter Gegenwart zu neuem Zunder k\u00fchlt.", "tokens": ["Mit", "ste\u00b7ter", "Ge\u00b7gen\u00b7wart", "zu", "neu\u00b7em", "Zun\u00b7der", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nicht lieben/ was man doch f\u00fcr Liebens w\u00fcrdig h\u00e4lt/", "tokens": ["Nicht", "lie\u00b7ben", "/", "was", "man", "doch", "f\u00fcr", "Lie\u00b7bens", "w\u00fcr\u00b7dig", "h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$(", "PWS", "PIS", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist eine Sache/ die zu schwer dem Willen f\u00e4llt;", "tokens": ["Ist", "ei\u00b7ne", "Sa\u00b7che", "/", "die", "zu", "schwer", "dem", "Wil\u00b7len", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "ART", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dem Willen/ welcher di\u00df zu haben ist bedacht/", "tokens": ["Dem", "Wil\u00b7len", "/", "wel\u00b7cher", "di\u00df", "zu", "ha\u00b7ben", "ist", "be\u00b7dacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PDS", "PTKZU", "VAINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was ihm der Sinnen Schlu\u00df als liebbar vorgebracht.", "tokens": ["Was", "ihm", "der", "Sin\u00b7nen", "Schlu\u00df", "als", "lieb\u00b7bar", "vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Di\u00df Lieben/ was sich uns als unser Bildni\u00df zeigt/", "tokens": ["Di\u00df", "Lie\u00b7ben", "/", "was", "sich", "uns", "als", "un\u00b7ser", "Bild\u00b7ni\u00df", "zeigt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "$(", "PWS", "PRF", "PPER", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja n\u00e4her als ein Bild zu unsrer Gleichheit neigt/", "tokens": ["Ja", "n\u00e4\u00b7her", "als", "ein", "Bild", "zu", "uns\u00b7rer", "Gleich\u00b7heit", "neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist unsre von Natur verpflichte Schuldigkeit/", "tokens": ["Ist", "uns\u00b7re", "von", "Na\u00b7tur", "ver\u00b7pflich\u00b7te", "Schul\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die uns/ und was uns gleicht/ zu lieben selbst gebeut.", "tokens": ["Die", "uns", "/", "und", "was", "uns", "gleicht", "/", "zu", "lie\u00b7ben", "selbst", "ge\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KON", "PWS", "PPER", "VVFIN", "$(", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu dem noch die Gewalt des Gegenstandes k\u00fcmmt/", "tokens": ["Zu", "dem", "noch", "die", "Ge\u00b7walt", "des", "Ge\u00b7gen\u00b7stan\u00b7des", "k\u00fcmmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die durch verborgnen Zug uns unsre Freyheit nimmt/", "tokens": ["Die", "durch", "ver\u00b7borg\u00b7nen", "Zug", "uns", "uns\u00b7re", "Frey\u00b7heit", "nimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und mit dem Wercke selbst bezeugt vor iedermann:", "tokens": ["Und", "mit", "dem", "Wer\u00b7cke", "selbst", "be\u00b7zeugt", "vor", "ie\u00b7der\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Frauen-Liebe sey der M\u00e4nner ihr Tyrann.", "tokens": ["Die", "Frau\u00b7en\u00b7Lie\u00b7be", "sey", "der", "M\u00e4n\u00b7ner", "ihr", "Ty\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}