{"dta.poem.10652": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma,   auff die Statt Bre\u00dflaw.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Als Themis an\u00df der Welt zu ziehn jhr vorgenommen/", "tokens": ["Als", "The\u00b7mis", "an\u00df", "der", "Welt", "zu", "ziehn", "jhr", "vor\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll vnderweges sie auch sein nach Bre\u00dflaw kommen/", "tokens": ["Soll", "vn\u00b7der\u00b7we\u00b7ges", "sie", "auch", "sein", "nach", "Bre\u00df\u00b7law", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "PPER", "ADV", "PPOSAT", "APPR", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd weil sie hat vermeint/ sie sey nun allbereit", "tokens": ["Vnd", "weil", "sie", "hat", "ver\u00b7meint", "/", "sie", "sey", "nun", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "VVPP", "$(", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Himmel/ ist sie da noch bi\u00df auff diese Zeit.", "tokens": ["Im", "Him\u00b7mel", "/", "ist", "sie", "da", "noch", "bi\u00df", "auff", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word"