{"dta.poem.7703": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wanns an zu d\u00e4mmern f\u00e4ngt, so ist der Tag nicht ferne;", "tokens": ["Wanns", "an", "zu", "d\u00e4m\u00b7mern", "f\u00e4ngt", ",", "so", "ist", "der", "Tag", "nicht", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des tr\u00f6st' ich mich, wann ich was schwerbegriffnes lerne.", "tokens": ["Des", "tr\u00f6st'", "ich", "mich", ",", "wann", "ich", "was", "schwer\u00b7be\u00b7griff\u00b7nes", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur eines ist, woran mein Unmuth oft erlag:", "tokens": ["Nur", "ei\u00b7nes", "ist", ",", "wo\u00b7ran", "mein", "Un\u00b7muth", "oft", "er\u00b7lag", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nach der D\u00e4mmerung kommt ein so grauer Tag.", "tokens": ["Da\u00df", "nach", "der", "D\u00e4m\u00b7me\u00b7rung", "kommt", "ein", "so", "grau\u00b7er", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "w