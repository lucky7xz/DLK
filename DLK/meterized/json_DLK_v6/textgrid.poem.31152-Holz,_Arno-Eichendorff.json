{"textgrid.poem.31152": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Eichendorff", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ferndr\u00fcben hinter den B\u00e4umen", "tokens": ["Fern\u00b7dr\u00fc\u00b7ben", "hin\u00b7ter", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist eben ein Gl\u00f6cklein verhallt,", "tokens": ["Ist", "e\u00b7ben", "ein", "Gl\u00f6c\u00b7klein", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun will ich hier liegen und tr\u00e4umen", "tokens": ["Nun", "will", "ich", "hier", "lie\u00b7gen", "und", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Mittag im stillen Wald.", "tokens": ["Den", "Mit\u00b7tag", "im", "stil\u00b7len", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hoch \u00fcber mir rauschen die Wipfel", "tokens": ["Hoch", "\u00fc\u00b7ber", "mir", "rau\u00b7schen", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und k\u00fchl herweht's aus der Kluft,", "tokens": ["Und", "k\u00fchl", "her\u00b7weht's", "aus", "der", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und fernhin verschwimmen die Gipfel", "tokens": ["Und", "fern\u00b7hin", "ver\u00b7schwim\u00b7men", "die", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Berge in bl\u00e4ulichem Duft.", "tokens": ["Der", "Ber\u00b7ge", "in", "bl\u00e4u\u00b7li\u00b7chem", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Verschlafen zwitschern und nicken", "tokens": ["Ver\u00b7schla\u00b7fen", "zwit\u00b7schern", "und", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die V\u00f6gel im gr\u00fcnen Tann,", "tokens": ["Die", "V\u00f6\u00b7gel", "im", "gr\u00fc\u00b7nen", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wie verzaubert blicken", "tokens": ["Und", "wie", "ver\u00b7zau\u00b7bert", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wilden Rosen mich an.", "tokens": ["Die", "wil\u00b7den", "Ro\u00b7sen", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Nun wird mir vor Weh und vor Wonne", "tokens": ["Nun", "wird", "mir", "vor", "Weh", "und", "vor", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das Herz so weit, so weit!", "tokens": ["Das", "Herz", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich denk an die goldene Sonne", "tokens": ["Und", "ich", "denk", "an", "die", "gol\u00b7de\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Der sch\u00f6nen Jugendzeit.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sang ich so lustige Weisen", "tokens": ["Da", "sang", "ich", "so", "lus\u00b7ti\u00b7ge", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und ward es doch nimmer m\u00fcd,", "tokens": ["Und", "ward", "es", "doch", "nim\u00b7mer", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn herrlich ist es zu reisen,", "tokens": ["Denn", "herr\u00b7lich", "ist", "es", "zu", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu reisen im sonnigen S\u00fcd!", "tokens": ["Zu", "rei\u00b7sen", "im", "son\u00b7ni\u00b7gen", "S\u00fcd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Dort raunen die Brunnen und rauschen", "tokens": ["Dort", "rau\u00b7nen", "die", "Brun\u00b7nen", "und", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verschlafen die ganze Nacht,", "tokens": ["Ver\u00b7schla\u00b7fen", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Marmorbilder lauschen,", "tokens": ["Und", "Mar\u00b7mor\u00b7bil\u00b7der", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn die Sternlein am Himmel erwacht.", "tokens": ["Wenn", "die", "Stern\u00b7lein", "am", "Him\u00b7mel", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Dann singen die Mandolinen", "tokens": ["Dann", "sin\u00b7gen", "die", "Man\u00b7do\u00b7li\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das alte Lied von den Zwei'n,", "tokens": ["Das", "al\u00b7te", "Lied", "von", "den", "Zwei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und in sinkende Tempelruinen", "tokens": ["Und", "in", "sin\u00b7ken\u00b7de", "Tem\u00b7pel\u00b7ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Spinnt silbern der Mond sich ein.", "tokens": ["Spinnt", "sil\u00b7bern", "der", "Mond", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Von einer Vigne zur andern,", "tokens": ["Von", "ei\u00b7ner", "Vig\u00b7ne", "zur", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dahin \u00fcber Th\u00e4ler und H\u00f6hn,", "tokens": ["Da\u00b7hin", "\u00fc\u00b7ber", "Th\u00e4\u00b7ler", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie tr\u00e4umend sang ich im Wandern:", "tokens": ["Wie", "tr\u00e4u\u00b7mend", "sang", "ich", "im", "Wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Welschland, wie bist du doch sch\u00f6n!", "tokens": ["O", "Wel\u00b7schland", ",", "wie", "bist", "du", "doch", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch, Herz, h\u00f6r auf zu tr\u00e4umen,", "tokens": ["Doch", ",", "Herz", ",", "h\u00f6r", "auf", "zu", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn dahin ist die alte Zeit,", "tokens": ["Denn", "da\u00b7hin", "ist", "die", "al\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber dir rauscht in den B\u00e4umen", "tokens": ["Und", "\u00fc\u00b7ber", "dir", "rauscht", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die gr\u00fcne Einsamkeit.", "tokens": ["Die", "gr\u00fc\u00b7ne", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So manche seiner Flocken", "tokens": ["So", "man\u00b7che", "sei\u00b7ner", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blies mir der Winter aufs Haupt,", "tokens": ["Blies", "mir", "der", "Win\u00b7ter", "aufs", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und meine braunen Locken", "tokens": ["Und", "mei\u00b7ne", "brau\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind alle schon grau verstaubt.", "tokens": ["Sind", "al\u00b7le", "schon", "grau", "ver\u00b7staubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Nur du, mein Herz, bliebst das alte", "tokens": ["Nur", "du", ",", "mein", "Herz", ",", "bliebst", "das", "al\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Und schl\u00e4gst noch so s\u00fcss, so s\u00fcss \u2013", "tokens": ["Und", "schl\u00e4gst", "noch", "so", "s\u00fcss", ",", "so", "s\u00fcss", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O, dass dich dein Herrgott erhalte:", "tokens": ["O", ",", "dass", "dich", "dein", "Herr\u00b7gott", "er\u00b7hal\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Gott gr\u00fcss dich, mein Herz, Gott gr\u00fcss!", "tokens": ["Gott", "gr\u00fcss", "dich", ",", "mein", "Herz", ",", "Gott", "gr\u00fcss", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}