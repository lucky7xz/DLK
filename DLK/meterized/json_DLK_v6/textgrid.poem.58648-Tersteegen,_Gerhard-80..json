{"textgrid.poem.58648": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lebst und schwebest stets in Gott, drum such ihn ja nicht ferne,", "tokens": ["Du", "lebst", "und", "schwe\u00b7best", "stets", "in", "Gott", ",", "drum", "such", "ihn", "ja", "nicht", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Er sitzt f\u00fcrwahr nicht eingesperrt hoch \u00fcber Mond und Sterne;", "tokens": ["Er", "sitzt", "f\u00fcr\u00b7wahr", "nicht", "ein\u00b7ge\u00b7sperrt", "hoch", "\u00fc\u00b7ber", "Mond", "und", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "W\u00e4rst du nur selber nicht so weit,", "tokens": ["W\u00e4rst", "du", "nur", "sel\u00b7ber", "nicht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermannigfaltigt und zerstreut,", "tokens": ["Ver\u00b7man\u00b7nig\u00b7fal\u00b7tigt", "und", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "K\u00e4mst du recht zu dir selber, da", "tokens": ["K\u00e4mst", "du", "recht", "zu", "dir", "sel\u00b7ber", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$,", "KOUS"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "W\u00fcrd'st du im Geist Gott finden nah.", "tokens": ["W\u00fcrd'st", "du", "im", "Geist", "Gott", "fin\u00b7den", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du nahes Wesen, bring mich nah, da\u00df ich in deinem Lichte", "tokens": ["Du", "na\u00b7hes", "We\u00b7sen", ",", "bring", "mich", "nah", ",", "da\u00df", "ich", "in", "dei\u00b7nem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "In Ehrfurcht wandle, wo ich bin, vor deinem Angesichte;", "tokens": ["In", "Ehr\u00b7furcht", "wand\u00b7le", ",", "wo", "ich", "bin", ",", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ich will mich nicht verdecken, nein,", "tokens": ["Ich", "will", "mich", "nicht", "ver\u00b7de\u00b7cken", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Herz liegt blo\u00df, schau, wie ich's mein,", "tokens": ["Mein", "Herz", "liegt", "blo\u00df", ",", "schau", ",", "wie", "ich's", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PTKVZ", "$,", "PWAV", "PIS", "PPOSAT", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Du f\u00fcllest Erd' und Himmel, Herr,", "tokens": ["Du", "f\u00fcl\u00b7lest", "Erd'", "und", "Him\u00b7mel", ",", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "La\u00df doch mein armes Herz nicht leer!", "tokens": ["La\u00df", "doch", "mein", "ar\u00b7mes", "Herz", "nicht", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}