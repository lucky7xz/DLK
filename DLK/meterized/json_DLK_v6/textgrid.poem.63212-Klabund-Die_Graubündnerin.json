{"textgrid.poem.63212": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Graub\u00fcndnerin", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolke h\u00e4ngt sich m\u00fcde in die Miene", "tokens": ["Die", "Wol\u00b7ke", "h\u00e4ngt", "sich", "m\u00fc\u00b7de", "in", "die", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des herbstlich schon ergrauten Tannenwalds.", "tokens": ["Des", "herbst\u00b7lich", "schon", "er\u00b7grau\u00b7ten", "Tan\u00b7nen\u00b7walds", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wasserfall gleicht einer Mandoline.", "tokens": ["Der", "Was\u00b7ser\u00b7fall", "gleicht", "ei\u00b7ner", "Man\u00b7do\u00b7li\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein roter Vogel zwitschert auf der Balz.", "tokens": ["Ein", "ro\u00b7ter", "Vo\u00b7gel", "zwit\u00b7schert", "auf", "der", "Balz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Steinbruch t\u00f6nt ein nagendes Geh\u00e4mmer.", "tokens": ["Vom", "Stein\u00b7bruch", "t\u00f6nt", "ein", "na\u00b7gen\u00b7des", "Ge\u00b7h\u00e4m\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lore f\u00e4hrt mit Felsenfracht zu Tal.", "tokens": ["Die", "Lo\u00b7re", "f\u00e4hrt", "mit", "Fel\u00b7sen\u00b7fracht", "zu", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch dieses Nachmittages Waldesd\u00e4mmer", "tokens": ["Durch", "die\u00b7ses", "Nach\u00b7mit\u00b7ta\u00b7ges", "Wal\u00b7des\u00b7d\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleitet ein papageienhafter Schal.", "tokens": ["Glei\u00b7tet", "ein", "pa\u00b7pa\u00b7gei\u00b7en\u00b7haf\u00b7ter", "Schal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Es zucken matt im Anhauch rauher Winde", "tokens": ["Es", "zu\u00b7cken", "matt", "im", "An\u00b7hauch", "rau\u00b7her", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schmalen Schultern der Graub\u00fcndnerin.", "tokens": ["Die", "schma\u00b7len", "Schul\u00b7tern", "der", "Grau\u00b7b\u00fcnd\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.3": {"text": "Um Hals und Nacken schlingt sich eine Binde", "tokens": ["Um", "Hals", "und", "Na\u00b7cken", "schlingt", "sich", "ei\u00b7ne", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und st\u00fctzt das fast entfallene Totenkinn.", "tokens": ["Und", "st\u00fctzt", "das", "fast", "ent\u00b7fal\u00b7le\u00b7ne", "To\u00b7ten\u00b7kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}