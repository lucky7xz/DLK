{"textgrid.poem.32553": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An Doris nach einem kleinen Scharm\u00fctzel.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich von Sanct Peters Kirche,", "tokens": ["W\u00e4r", "ich", "von", "Sanct", "Pe\u00b7ters", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "VVFIN", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Glaube, da\u00df ich dann gewi\u00df,", "tokens": ["Glau\u00b7be", ",", "da\u00df", "ich", "dann", "ge\u00b7wi\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich zu meiner Liebling Heil'gen", "tokens": ["Dich", "zu", "mei\u00b7ner", "Lieb\u00b7ling", "Heil'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Magdlena malen lie\u00df.", "tokens": ["Als", "Magd\u00b7le\u00b7na", "ma\u00b7len", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVINF", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen, nie sah ich dich sch\u00f6ner,", "tokens": ["M\u00e4d\u00b7chen", ",", "nie", "sah", "ich", "dich", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als da Deine weiche Hand", "tokens": ["Als", "da", "Dei\u00b7ne", "wei\u00b7che", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kunstlos statt des Modekopfstaats,", "tokens": ["Kunst\u00b7los", "statt", "des", "Mo\u00b7de\u00b7kopf\u00b7staats", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Blo\u00df ein Tuch der Stirn umwand.", "tokens": ["Blo\u00df", "ein", "Tuch", "der", "Stirn", "um\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schalkhaft kuckte nie ", "tokens": ["Schalk\u00b7haft", "kuck\u00b7te", "nie"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Neben dem beringten Ohr,", "tokens": ["Ne\u00b7ben", "dem", "be\u00b7ring\u00b7ten", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ein Probchen Haar zu zeigen", "tokens": ["Um", "ein", "Prob\u00b7chen", "Haar", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterm seidnen Tuch hervor.", "tokens": ["Un\u00b7term", "seid\u00b7nen", "Tuch", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dreuster funkelte Dein Auge,", "tokens": ["Dreus\u00b7ter", "fun\u00b7kel\u00b7te", "Dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfer schien Dein wei\u00df Gesicht,", "tokens": ["Wei\u00b7\u00dfer", "schien", "Dein", "wei\u00df", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Purpur flo\u00df um Deine Wangen,", "tokens": ["Pur\u00b7pur", "flo\u00df", "um", "Dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner gl\u00fcht Aurora nicht.", "tokens": ["Sch\u00f6\u00b7ner", "gl\u00fcht", "Au\u00b7ro\u00b7ra", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie der Magdalena Busen", "tokens": ["Wie", "der", "Mag\u00b7da\u00b7le\u00b7na", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Na\u00df von Thr\u00e4nen reuig stieg;", "tokens": ["Na\u00df", "von", "Thr\u00e4\u00b7nen", "reu\u00b7ig", "stieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4dchen so hob Deinen Busen", "tokens": ["M\u00e4d\u00b7chen", "so", "hob", "Dei\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hofnung auf den sch\u00f6nsten Sieg.", "tokens": ["Hof\u00b7nung", "auf", "den", "sch\u00f6ns\u00b7ten", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie die Rose wenn des Morgens", "tokens": ["Wie", "die", "Ro\u00b7se", "wenn", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thau auf ihren Bl\u00e4ttern steht;", "tokens": ["Thau", "auf", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So Dein R\u00f6\u00dfchen, da\u00df an Sch\u00f6nheit", "tokens": ["So", "Dein", "R\u00f6\u00df\u00b7chen", ",", "da\u00df", "an", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Rosen \u00fcbergeht.", "tokens": ["Al\u00b7le", "Ro\u00b7sen", "\u00fc\u00b7ber\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Holde Sehnsucht warmer Liebe,", "tokens": ["Hol\u00b7de", "Sehn\u00b7sucht", "war\u00b7mer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach Dein zauberischer Blick,", "tokens": ["Sprach", "Dein", "zau\u00b7be\u00b7ri\u00b7scher", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter tausend kleinen Seufzern,", "tokens": ["Un\u00b7ter", "tau\u00b7send", "klei\u00b7nen", "Seuf\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Theilest Du mit mir mein Gl\u00fcck.", "tokens": ["Thei\u00b7lest", "Du", "mit", "mir", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Warum hielt'st Du doch dem Auge,", "tokens": ["Wa\u00b7rum", "hielt'st", "Du", "doch", "dem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Wollust luft'ger Flor", "tokens": ["Wenn", "der", "Wol\u00b7lust", "luft'\u00b7ger", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es bezog, oft wenn es lachte,", "tokens": ["Es", "be\u00b7zog", ",", "oft", "wenn", "es", "lach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Hand mi\u00dfg\u00fcnstig vor?", "tokens": ["Dei\u00b7ne", "Hand", "mi\u00df\u00b7g\u00fcns\u00b7tig", "vor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sch\u00e4m' Dich nicht des sanften Schauers,", "tokens": ["Sch\u00e4m'", "Dich", "nicht", "des", "sanf\u00b7ten", "Schau\u00b7ers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch alle Nerven schie\u00dft,", "tokens": ["Der", "durch", "al\u00b7le", "Ner\u00b7ven", "schie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der milde Thau der Wollust", "tokens": ["Wenn", "der", "mil\u00b7de", "Thau", "der", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Rosenmuschel flie\u00dft.", "tokens": ["Aus", "der", "Ro\u00b7sen\u00b7mu\u00b7schel", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "La\u00df das sch\u00f6ngebrochne Auge,", "tokens": ["La\u00df", "das", "sch\u00f6n\u00b7ge\u00b7broch\u00b7ne", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df der Zunge t\u00e4ndelnd Spiel", "tokens": ["La\u00df", "der", "Zun\u00b7ge", "t\u00e4n\u00b7delnd", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagen, ob der kleine Zweykampf", "tokens": ["Sa\u00b7gen", ",", "ob", "der", "klei\u00b7ne", "Zwey\u00b7kampf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir so sehr als mir gefiel.", "tokens": ["Dir", "so", "sehr", "als", "mir", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00df mich alles alles sehen,", "tokens": ["La\u00df", "mich", "al\u00b7les", "al\u00b7les", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der hei\u00dfte Ku\u00df Dich fr\u00e4gt:", "tokens": ["Wenn", "der", "hei\u00df\u00b7te", "Ku\u00df", "Dich", "fr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob Dein Herz auch treu wird bleiben,", "tokens": ["Ob", "Dein", "Herz", "auch", "treu", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Obs auch jetzt f\u00fcr mich nur schl\u00e4gt?", "tokens": ["Obs", "auch", "jetzt", "f\u00fcr", "mich", "nur", "schl\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}