{"textgrid.poem.57317": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verkent denn euer Vaterland,", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verkent denn euer Vaterland,", "tokens": ["Ver\u00b7kent", "denn", "eu\u00b7er", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Undeutsche Deutsche! steht und gaft,", "tokens": ["Un\u00b7deut\u00b7sche", "Deut\u00b7sche", "!", "steht", "und", "gaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit bl\u00f6der Bewundrung grossem Auge,", "tokens": ["Mit", "bl\u00f6\u00b7der", "Be\u00b7wund\u00b7rung", "gros\u00b7sem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Ausland an!", "tokens": ["Das", "Aus\u00b7land", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wettstreitet, wer am lautsten staunt!", "tokens": ["Wett\u00b7strei\u00b7tet", ",", "wer", "am", "lauts\u00b7ten", "staunt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdorret ist des Siegers Kranz!", "tokens": ["Ver\u00b7dor\u00b7ret", "ist", "des", "Sie\u00b7gers", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir rufens euch zu; doch ihr bet\u00e4ubt euch,", "tokens": ["Wir", "ru\u00b7fens", "euch", "zu", ";", "doch", "ihr", "be\u00b7t\u00e4ubt", "euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und streitet fort.", "tokens": ["Und", "strei\u00b7tet", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wir spotten eures Kampfes nicht;", "tokens": ["Wir", "spot\u00b7ten", "eu\u00b7res", "Kamp\u00b7fes", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist des Mitleids Sprache nicht.", "tokens": ["Das", "ist", "des", "Mit\u00b7leids", "Spra\u00b7che", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ungl\u00fcckliche sind uns heilig! Traut uns,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7che", "sind", "uns", "hei\u00b7lig", "!", "Traut", "uns", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$.", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir spotten nicht.", "tokens": ["Wir", "spot\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dem Fremden, den ihr vorzieht, kams", "tokens": ["Dem", "Frem\u00b7den", ",", "den", "ihr", "vor\u00b7zieht", ",", "kams"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie ein, den Fremden vorzuziehn:", "tokens": ["Nie", "ein", ",", "den", "Frem\u00b7den", "vor\u00b7zu\u00b7ziehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hasst die Empfindung dieser Kriechsucht!", "tokens": ["Er", "hasst", "die", "Emp\u00b7fin\u00b7dung", "die\u00b7ser", "Kriech\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verachtet euch,", "tokens": ["Ver\u00b7ach\u00b7tet", "euch", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Weil ihr ihn vorzieht! Fasst ihr nun,", "tokens": ["Weil", "ihr", "ihn", "vor\u00b7zieht", "!", "Fasst", "ihr", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dass wir auf euch voll Mitleid sehn?", "tokens": ["Dass", "wir", "auf", "euch", "voll", "Mit\u00b7leid", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ergr\u00fcndet ihr nun, dass ihr ungl\u00fccklich,", "tokens": ["Er\u00b7gr\u00fcn\u00b7det", "ihr", "nun", ",", "dass", "ihr", "un\u00b7gl\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns heilig seyd?", "tokens": ["Uns", "hei\u00b7lig", "seyd", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}