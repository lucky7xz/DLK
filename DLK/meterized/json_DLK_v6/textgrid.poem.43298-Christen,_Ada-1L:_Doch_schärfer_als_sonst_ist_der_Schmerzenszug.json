{"textgrid.poem.43298": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch sch\u00e4rfer als sonst ist der Schmerzenszug", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch sch\u00e4rfer als sonst ist der Schmerzenszug", "tokens": ["Doch", "sch\u00e4r\u00b7fer", "als", "sonst", "ist", "der", "Schmer\u00b7zens\u00b7zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Auf Deinem Antlitz ausgepr\u00e4gt.", "tokens": ["Auf", "Dei\u00b7nem", "Ant\u00b7litz", "aus\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du g\u00f6nnest dir nicht Ruh genug,", "tokens": ["Du", "g\u00f6n\u00b7nest", "dir", "nicht", "Ruh", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu treu hast du das Kind gepflegt.", "tokens": ["Zu", "treu", "hast", "du", "das", "Kind", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wei\u00dfer als sonst ist heute Dein Mund", "tokens": ["Doch", "wei\u00b7\u00dfer", "als", "sonst", "ist", "heu\u00b7te", "Dein", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Deine Augen gl\u00e4nzen erregt,", "tokens": ["Und", "Dei\u00b7ne", "Au\u00b7gen", "gl\u00e4n\u00b7zen", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du athmest m\u00fchsam! ... Thu mir kund,", "tokens": ["Du", "ath\u00b7mest", "m\u00fch\u00b7sam", "!", "...", "Thu", "mir", "kund", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob einen Wunsch Deine Seele hegt?", "tokens": ["Ob", "ei\u00b7nen", "Wunsch", "Dei\u00b7ne", "See\u00b7le", "hegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und schwerer als sonst Deine kleine Hand", "tokens": ["Und", "schwe\u00b7rer", "als", "sonst", "Dei\u00b7ne", "klei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich pl\u00f6tzlich auf meinen Scheitel legt;", "tokens": ["Sich", "pl\u00f6tz\u00b7lich", "auf", "mei\u00b7nen", "Schei\u00b7tel", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du wirst so still ... Sag, was entschwand", "tokens": ["Du", "wirst", "so", "still", "...", "Sag", ",", "was", "ent\u00b7schwand"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "NN", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Deinem Aug' wie fortgefegt?", "tokens": ["Aus", "Dei\u00b7nem", "Aug'", "wie", "fort\u00b7ge\u00b7fegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und kalt und starr wird Dein holder Leib,", "tokens": ["Und", "kalt", "und", "starr", "wird", "Dein", "hol\u00b7der", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Herz nimmer sanft an meinem schl\u00e4gt ...", "tokens": ["Dein", "Herz", "nim\u00b7mer", "sanft", "an", "mei\u00b7nem", "schl\u00e4gt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So rede, ... Weib ... mein Weib! ... Mein Weib!!", "tokens": ["So", "re\u00b7de", ",", "...", "Weib", "...", "mein", "Weib", "!", "...", "Mein", "Weib", "!!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "$(", "NN", "$(", "PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herrgott! ....... todt! ......", "tokens": ["Herr\u00b7gott", "!", ".......", "todt", "!", "......"], "token_info": ["word", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "ADJD", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}