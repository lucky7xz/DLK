{"textgrid.poem.48184": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbo tr\u00fcbe diese Tage nicht\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O tr\u00fcbe diese Tage nicht,", "tokens": ["O", "tr\u00fc\u00b7be", "die\u00b7se", "Ta\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind der letzte Sonnenschein,", "tokens": ["Sie", "sind", "der", "letz\u00b7te", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lange, und es lischt das Licht,", "tokens": ["Wie", "lan\u00b7ge", ",", "und", "es", "lischt", "das", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Winter bricht herein.", "tokens": ["Und", "un\u00b7ser", "Win\u00b7ter", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies ist die Zeit, wo jeder Tag", "tokens": ["Dies", "ist", "die", "Zeit", ",", "wo", "je\u00b7der", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Tage gilt in seinem Wert,", "tokens": ["Viel", "Ta\u00b7ge", "gilt", "in", "sei\u00b7nem", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil man's nicht mehr erhoffen mag,", "tokens": ["Weil", "man's", "nicht", "mehr", "er\u00b7hof\u00b7fen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Die Flut des Lebens ist dahin,", "tokens": ["Die", "Flut", "des", "Le\u00b7bens", "ist", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ebbt in seinem Stolz und Reiz,", "tokens": ["Es", "ebbt", "in", "sei\u00b7nem", "Stolz", "und", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieh, es schleicht in unsern Sinn", "tokens": ["Und", "sieh", ",", "es", "schleicht", "in", "un\u00b7sern", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein banger, nie gekannter Geiz;", "tokens": ["Ein", "ban\u00b7ger", ",", "nie", "ge\u00b7kann\u00b7ter", "Geiz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein s\u00fc\u00dfer Geiz, der Stunden z\u00e4hlt", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Geiz", ",", "der", "Stun\u00b7den", "z\u00e4hlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jede pr\u00fcft auf ihren Glanz,", "tokens": ["Und", "je\u00b7de", "pr\u00fcft", "auf", "ih\u00b7ren", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O sorge, da\u00df uns keine fehlt,", "tokens": ["O", "sor\u00b7ge", ",", "da\u00df", "uns", "kei\u00b7ne", "fehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00f6nn uns jede Stunde ", "tokens": ["Und", "g\u00f6nn", "uns", "je\u00b7de", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}