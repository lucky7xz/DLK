{"dta.poem.12094": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am besten w\u00fcrdest du in einen Koffer packen", "tokens": ["Am", "bes\u00b7ten", "w\u00fcr\u00b7dest", "du", "in", "ei\u00b7nen", "Kof\u00b7fer", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich lassen, oder auch im Mantelsack einsacken,", "tokens": ["Dich", "las\u00b7sen", ",", "o\u00b7der", "auch", "im", "Man\u00b7tel\u00b7sack", "ein\u00b7sa\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und so auf Reisen gehn, wenn du nicht Geld gewannst,", "tokens": ["Und", "so", "auf", "Rei\u00b7sen", "gehn", ",", "wenn", "du", "nicht", "Geld", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu fahren ordentlich, und nicht zu Fu\u00df gehn kannst.", "tokens": ["Zu", "fah\u00b7ren", "or\u00b7dent\u00b7lich", ",", "und", "nicht", "zu", "Fu\u00df", "gehn", "kannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,", "KON", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fu\u00dfg\u00e4nger geht und steht, wo, wie und wann er mag;", "tokens": ["Fu\u00df\u00b7g\u00e4n\u00b7ger", "geht", "und", "steht", ",", "wo", ",", "wie", "und", "wann", "er", "mag", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "$,", "PWAV", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft, die ihn durchweht, weckt seines Herzen Schlag.", "tokens": ["Die", "Luft", ",", "die", "ihn", "durch\u00b7weht", ",", "weckt", "sei\u00b7nes", "Her\u00b7zen", "Schlag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er h\u00f6rt und sieht und denkt, bis er ist m\u00fcd geworden,", "tokens": ["Er", "h\u00f6rt", "und", "sieht", "und", "denkt", ",", "bis", "er", "ist", "m\u00fcd", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er den Kopf dann h\u00e4ngt auch an den sch\u00f6nsten Borden.", "tokens": ["Wo", "er", "den", "Kopf", "dann", "h\u00e4ngt", "auch", "an", "den", "sch\u00f6ns\u00b7ten", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wer durchfliegen kann die Welt im eignen Wagen,", "tokens": ["Doch", "wer", "durch\u00b7flie\u00b7gen", "kann", "die", "Welt", "im", "eig\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der f\u00fchlt, ein ganzer Mann, vollkommenes Behagen;", "tokens": ["Der", "f\u00fchlt", ",", "ein", "gan\u00b7zer", "Mann", ",", "voll\u00b7kom\u00b7me\u00b7nes", "Be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Schaut vorwerts und zur\u00fcck, und frei nach allen Seiten,", "tokens": ["Schaut", "vor\u00b7werts", "und", "zu\u00b7r\u00fcck", ",", "und", "frei", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "PTKVZ", "$,", "KON", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4\u00dft wie vom Geschick sich von dem Kutscher leiten.", "tokens": ["Und", "l\u00e4\u00dft", "wie", "vom", "Ge\u00b7schick", "sich", "von", "dem", "Kut\u00b7scher", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Weh aber dem, der, wenn Geld oder Kraft versiecht,", "tokens": ["Weh", "a\u00b7ber", "dem", ",", "der", ",", "wenn", "Geld", "o\u00b7der", "Kraft", "ver\u00b7siecht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "$,", "PRELS", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um fortzukommen nur, in Postlandkutschen kriecht;", "tokens": ["Um", "fort\u00b7zu\u00b7kom\u00b7men", "nur", ",", "in", "Post\u00b7land\u00b7kut\u00b7schen", "kriecht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "ADV", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wo mit viel andern er liegt schichtweis aufgestoppelt,", "tokens": ["Wo", "mit", "viel", "an\u00b7dern", "er", "liegt", "schicht\u00b7weis", "auf\u00b7ge\u00b7stop\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "ADJA", "PPER", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und mit der Fracht ein Paar von d\u00fcrren M\u00e4hren hoppelt.", "tokens": ["Und", "mit", "der", "Fracht", "ein", "Paar", "von", "d\u00fcr\u00b7ren", "M\u00e4h\u00b7ren", "hop\u00b7pelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.9": {"line.1": {"text": "Aussteigt er wann er soll, ein wieder wann er mu\u00df,", "tokens": ["Aus\u00b7steigt", "er", "wann", "er", "soll", ",", "ein", "wie\u00b7der", "wann", "er", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "$,", "ART", "ADV", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von der Fahrt ist nichts als M\u00fchsal sein Genu\u00df.", "tokens": ["Und", "von", "der", "Fahrt", "ist", "nichts", "als", "M\u00fch\u00b7sal", "sein", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PIS", "KOKOM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Vom Wege wird ihm nichts bekannt, als da\u00df er st\u00e4ubt,", "tokens": ["Vom", "We\u00b7ge", "wird", "ihm", "nichts", "be\u00b7kannt", ",", "als", "da\u00df", "er", "st\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIS", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vom Gerassel ist selbst das Gespr\u00e4ch bet\u00e4ubt.", "tokens": ["Und", "vom", "Ge\u00b7ras\u00b7sel", "ist", "selbst", "das", "Ge\u00b7spr\u00e4ch", "be\u00b7t\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Wie duckt er sich und ruckt, wie druckt er sich und zuckt,", "tokens": ["Wie", "duckt", "er", "sich", "und", "ruckt", ",", "wie", "druckt", "er", "sich", "und", "zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis er durchs Fenster spuckt, oder durchs Fenster guckt.", "tokens": ["Bis", "er", "durchs", "Fens\u00b7ter", "spuckt", ",", "o\u00b7der", "durchs", "Fens\u00b7ter", "guckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Von Landschaft hascht er schief bald hier bald dort ein St\u00fcck,", "tokens": ["Von", "Land\u00b7schaft", "hascht", "er", "schief", "bald", "hier", "bald", "dort", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringt kein ganzes Bild davon nach Haus zur\u00fcck.", "tokens": ["Und", "bringt", "kein", "gan\u00b7zes", "Bild", "da\u00b7von", "nach", "Haus", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}