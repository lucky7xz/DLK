{"textgrid.poem.50239": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "17. Auff Fulvus den artigen Fuchsschw\u00e4ntzer", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die Worte ", "tokens": ["Die", "Wor\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dass seine ", "tokens": ["Dass", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Das macht, das jederman ihn gerne bey sich sieht.", "tokens": ["Das", "macht", ",", "das", "je\u00b7der\u00b7man", "ihn", "ger\u00b7ne", "bey", "sich", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PIS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn kan gleich nicht ein Wort von ihm vernommen werden,", "tokens": ["Denn", "kan", "gleich", "nicht", "ein", "Wort", "von", "ihm", "ver\u00b7nom\u00b7men", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was schadt's? Ein ", "tokens": ["Was", "schadt's", "?", "Ein"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "$.", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}