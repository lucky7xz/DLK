{"textgrid.poem.60910": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam ruhst du und ernst und verwildert, o Villa Patrizi;", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam ruhst du und ernst und verwildert, o Villa Patrizi;", "tokens": ["Ein\u00b7sam", "ruhst", "du", "und", "ernst", "und", "ver\u00b7wil\u00b7dert", ",", "o", "Vil\u00b7la", "Pat\u00b7ri\u00b7zi", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "KON", "VVPP", "$,", "FM", "FM", "FM", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Aber die sch\u00f6nste, wiewohl menschlicher Pflege beraubt,", "tokens": ["A\u00b7ber", "die", "sch\u00f6ns\u00b7te", ",", "wie\u00b7wohl", "menschli\u00b7cher", "Pfle\u00b7ge", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ruhst wie ein Kranz, mit dem Lorbeerhain und der schlanken Zypressen", "tokens": ["Ruhst", "wie", "ein", "Kranz", ",", "mit", "dem", "Lor\u00b7beer\u00b7hain", "und", "der", "schlan\u00b7ken", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+-+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "M\u00e4chtigem Gang, stets gr\u00fcn, auf des Posilipo Stirn!", "tokens": ["M\u00e4ch\u00b7ti\u00b7gem", "Gang", ",", "stets", "gr\u00fcn", ",", "auf", "des", "Po\u00b7si\u00b7li\u00b7po", "Stirn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+--++--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ja, hier wandle der Dichter allein, und im Wandel betracht er,", "tokens": ["Ja", ",", "hier", "wand\u00b7le", "der", "Dich\u00b7ter", "al\u00b7lein", ",", "und", "im", "Wan\u00b7del", "be\u00b7tracht", "er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+--++-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Durch die Zypressen hindurch, K\u00fcsten und Meer und Vesuv.", "tokens": ["Durch", "die", "Zyp\u00b7res\u00b7sen", "hin\u00b7durch", ",", "K\u00fcs\u00b7ten", "und", "Meer", "und", "Ve\u00b7suv", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$,", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}