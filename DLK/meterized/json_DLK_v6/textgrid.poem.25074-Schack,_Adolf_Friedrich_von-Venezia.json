{"textgrid.poem.25074": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Venezia", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Strand der Insel, wo Venedigs Tote", "tokens": ["Am", "Strand", "der", "In\u00b7sel", ",", "wo", "Ve\u00b7ne\u00b7digs", "To\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "NE", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf stillem Friedhof beieinander ruhen,", "tokens": ["Auf", "stil\u00b7lem", "Fried\u00b7hof", "bei\u00b7ein\u00b7an\u00b7der", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gelandet war ich j\u00fcngst im leichten Boote.", "tokens": ["Ge\u00b7lan\u00b7det", "war", "ich", "j\u00fcngst", "im", "leich\u00b7ten", "Boo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort, wo ich seit dem Fr\u00fchling oft gesessen,", "tokens": ["Dort", ",", "wo", "ich", "seit", "dem", "Fr\u00fch\u00b7ling", "oft", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun blinkten wei\u00df im Reife des Novembers", "tokens": ["Nun", "blink\u00b7ten", "wei\u00df", "im", "Rei\u00b7fe", "des", "No\u00b7vem\u00b7bers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu H\u00e4upten mir die m\u00e4cht'gen Grabcypressen.", "tokens": ["Zu", "H\u00e4up\u00b7ten", "mir", "die", "m\u00e4cht'\u00b7gen", "Grab\u00b7cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ringsum, gemei\u00dfelt auf die Marmorplatten,", "tokens": ["Ring\u00b7sum", ",", "ge\u00b7mei\u00b7\u00dfelt", "auf", "die", "Mar\u00b7mor\u00b7plat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Entgegen schauten mir die Z\u00fcge derer,", "tokens": ["Ent\u00b7ge\u00b7gen", "schau\u00b7ten", "mir", "die", "Z\u00fc\u00b7ge", "de\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PDS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die drunter sich im Staub gebettet hatten.", "tokens": ["Die", "drun\u00b7ter", "sich", "im", "Staub", "ge\u00b7bet\u00b7tet", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und denkend an Venedigs gro\u00dfe Tage", "tokens": ["Und", "den\u00b7kend", "an", "Ve\u00b7ne\u00b7digs", "gro\u00b7\u00dfe", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NE", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sp\u00e4ht' ich, ob nicht ein Stein der Loredani,", "tokens": ["Sp\u00e4ht'", "ich", ",", "ob", "nicht", "ein", "Stein", "der", "Lo\u00b7re\u00b7da\u00b7ni", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pisani, Barbarigo Namen trage.", "tokens": ["Pi\u00b7sa\u00b7ni", ",", "Bar\u00b7ba\u00b7ri\u00b7go", "Na\u00b7men", "tra\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "VVFIN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Vergebens! Die Geschlechter sind verschollen,", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "Die", "Ge\u00b7schlech\u00b7ter", "sind", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die K\u00f6n'ge einst besiegt; ihr Ruhm lebt einzig", "tokens": ["Die", "K\u00f6n'\u00b7ge", "einst", "be\u00b7siegt", ";", "ihr", "Ruhm", "lebt", "ein\u00b7zig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch in verstaubter Pergamente Rollen.", "tokens": ["Noch", "in", "ver\u00b7staub\u00b7ter", "Per\u00b7ga\u00b7men\u00b7te", "Rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So sinnend neben einem Leichensteine", "tokens": ["So", "sin\u00b7nend", "ne\u00b7ben", "ei\u00b7nem", "Lei\u00b7chen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lehnt' ich, indessen an den h\u00f6chsten Alpen", "tokens": ["Lehnt'", "ich", ",", "in\u00b7des\u00b7sen", "an", "den", "h\u00f6chs\u00b7ten", "Al\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Tag erlosch mit letztem blassen Scheine.", "tokens": ["Der", "Tag", "er\u00b7losch", "mit", "letz\u00b7tem", "blas\u00b7sen", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da kam der Sohn des Gondoliers gesprungen:", "tokens": ["Da", "kam", "der", "Sohn", "des", "Gon\u00b7do\u00b7liers", "ge\u00b7sprun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbschnell! Schwer wird sonst die Heimfahrt. Tiefer Nebel", "tokens": ["\u00bb", "schnell", "!", "Schwer", "wird", "sonst", "die", "Heim\u00b7fahrt", ".", "Tie\u00b7fer", "Ne\u00b7bel"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$.", "ADJD", "VAFIN", "ADV", "ART", "NN", "$.", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4lt schon im S\u00fcden Stadt und Meer umschlungen.\u00ab", "tokens": ["H\u00e4lt", "schon", "im", "S\u00fc\u00b7den", "Stadt", "und", "Meer", "um\u00b7schlun\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er zog mich in die Gondel mit der Rechten,", "tokens": ["Er", "zog", "mich", "in", "die", "Gon\u00b7del", "mit", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zu den Rudern griffen Sohn und Vater,", "tokens": ["Und", "zu", "den", "Ru\u00b7dern", "grif\u00b7fen", "Sohn", "und", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie zur\u00fcck mich nach Venedig br\u00e4chten.", "tokens": ["Da\u00df", "sie", "zu\u00b7r\u00fcck", "mich", "nach", "Ve\u00b7ne\u00b7dig", "br\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Still war das Meer; doch graue Nebel wallten", "tokens": ["Still", "war", "das", "Meer", ";", "doch", "grau\u00b7e", "Ne\u00b7bel", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In langem Zuge rings heran und legten", "tokens": ["In", "lan\u00b7gem", "Zu\u00b7ge", "rings", "he\u00b7ran", "und", "leg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf die Lagune sich in schweren Falten.", "tokens": ["Auf", "die", "La\u00b7gu\u00b7ne", "sich", "in", "schwe\u00b7ren", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die beiden thaten kr\u00e4ft'ge Ruderschl\u00e4ge;", "tokens": ["Die", "bei\u00b7den", "tha\u00b7ten", "kr\u00e4ft'\u00b7ge", "Ru\u00b7der\u00b7schl\u00e4\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lang fuhren wir; allein nicht Stadt noch Ufer", "tokens": ["Lang", "fuh\u00b7ren", "wir", ";", "al\u00b7lein", "nicht", "Stadt", "noch", "U\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "ADV", "PTKNEG", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erschien; das Bot glitt langsam hin und tr\u00e4ge.", "tokens": ["Er\u00b7schien", ";", "das", "Bot", "glitt", "lang\u00b7sam", "hin", "und", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Da vor uns ferne her erschollen Stimmen;", "tokens": ["Da", "vor", "uns", "fer\u00b7ne", "her", "er\u00b7schol\u00b7len", "Stim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesang, im Nachthauch flutend, drang ans Ohr mir,", "tokens": ["Ge\u00b7sang", ",", "im", "Nacht\u00b7hauch", "flu\u00b7tend", ",", "drang", "ans", "Ohr", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Lichter sah ich durch das Dunkel glimmen.", "tokens": ["Und", "Lich\u00b7ter", "sah", "ich", "durch", "das", "Dun\u00b7kel", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und uns entgegen aus dem Nebelflore", "tokens": ["Und", "uns", "ent\u00b7ge\u00b7gen", "aus", "dem", "Ne\u00b7bel\u00b7flo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwamm eine Barke; tief verh\u00fcllte M\u00e4nner,", "tokens": ["Schwamm", "ei\u00b7ne", "Bar\u00b7ke", ";", "tief", "ver\u00b7h\u00fcll\u00b7te", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In H\u00e4nden Fackeln, sangen drin im Chore.", "tokens": ["In", "H\u00e4n\u00b7den", "Fa\u00b7ckeln", ",", "san\u00b7gen", "drin", "im", "Cho\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Inmitten war als wie zur Totenfeier", "tokens": ["In\u00b7mit\u00b7ten", "war", "als", "wie", "zur", "To\u00b7ten\u00b7fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOUS", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Katafalk erbaut, und auf ihm ruhte", "tokens": ["Ein", "Ka\u00b7ta\u00b7falk", "er\u00b7baut", ",", "und", "auf", "ihm", "ruh\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein hohes Weib, umwallt von schwarzem Schleier.", "tokens": ["Ein", "ho\u00b7hes", "Weib", ",", "um\u00b7wallt", "von", "schwar\u00b7zem", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Wohl kannt' ich sie, die, blitzend von Juwelen,", "tokens": ["Wohl", "kannt'", "ich", "sie", ",", "die", ",", "blit\u00b7zend", "von", "Ju\u00b7we\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "$,", "PRELS", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Prachtgewanden ich auf manchem Bilde", "tokens": ["In", "Pracht\u00b7ge\u00b7wan\u00b7den", "ich", "auf", "man\u00b7chem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gesehen in des Dogenschlosses S\u00e4len.", "tokens": ["Ge\u00b7se\u00b7hen", "in", "des", "Do\u00b7gen\u00b7schlos\u00b7ses", "S\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ein matter Schimmer spielte um das bleiche", "tokens": ["Ein", "mat\u00b7ter", "Schim\u00b7mer", "spiel\u00b7te", "um", "das", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesicht der Toten, ihr zu F\u00fc\u00dfen lagen", "tokens": ["Ge\u00b7sicht", "der", "To\u00b7ten", ",", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "la\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Banner drei besiegter K\u00f6nigreiche.", "tokens": ["Die", "Ban\u00b7ner", "drei", "be\u00b7sieg\u00b7ter", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "An meiner Seite sank aufs Knie der Knabe;", "tokens": ["An", "mei\u00b7ner", "Sei\u00b7te", "sank", "aufs", "Knie", "der", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ernst die H\u00e4nde faltend, sprach mein Schiffer:", "tokens": ["Doch", "ernst", "die", "H\u00e4n\u00b7de", "fal\u00b7tend", ",", "sprach", "mein", "Schif\u00b7fer", ":"], "token_info": ["word", "word", "word",