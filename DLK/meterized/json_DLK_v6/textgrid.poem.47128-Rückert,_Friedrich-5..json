{"textgrid.poem.47128": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist sch\u00f6n wie der Fr\u00fchlingstag,", "tokens": ["Sie", "ist", "sch\u00f6n", "wie", "der", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Liebestrahlen zerflossen.", "tokens": ["In", "Lie\u00b7bes\u00b7trah\u00b7len", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie ist sch\u00f6n wie der Rosenhag,", "tokens": ["Sie", "ist", "sch\u00f6n", "wie", "der", "Ro\u00b7sen\u00b7hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In D\u00fcfte der Lieb' ergossen.", "tokens": ["In", "D\u00fcf\u00b7te", "der", "Lieb'", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie ist sch\u00f6n, wie in Eden mag", "tokens": ["Sie", "ist", "sch\u00f6n", ",", "wie", "in", "E\u00b7den", "mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "APPR", "NN", "VMFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Baum des Lebens ersprossen.", "tokens": ["Der", "Baum", "des", "Le\u00b7bens", "ers\u00b7pros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie ist sch\u00f6n, wie die Sch\u00f6pfung lag", "tokens": ["Sie", "ist", "sch\u00f6n", ",", "wie", "die", "Sch\u00f6p\u00b7fung", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Geist des Sch\u00f6pfers beschlossen.", "tokens": ["Im", "Geist", "des", "Sch\u00f6p\u00b7fers", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie ist sch\u00f6n wie die Liebesklag',", "tokens": ["Sie", "ist", "sch\u00f6n", "wie", "die", "Lie\u00b7bes\u00b7klag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Sch\u00f6ner als alles, was ich sag',", "tokens": ["Sch\u00f6\u00b7ner", "als", "al\u00b7les", ",", "was", "ich", "sag'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist, was ich im Herzen verschlossen.", "tokens": ["Ist", ",", "was", "ich", "im", "Her\u00b7zen", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}