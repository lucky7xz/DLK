{"dta.poem.15671": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast es einmal brav gemacht, und meinest nun,", "tokens": ["Du", "hast", "es", "ein\u00b7mal", "brav", "ge\u00b7macht", ",", "und", "mei\u00b7nest", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du k\u00f6nnt'st ein andermal auch etwas minder thun.", "tokens": ["Du", "k\u00f6nnt'st", "ein", "an\u00b7der\u00b7mal", "auch", "et\u00b7was", "min\u00b7der", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mitnichten kauft man sich mit Pflichten los von Pflichten,", "tokens": ["Mit\u00b7nich\u00b7ten", "kauft", "man", "sich", "mit", "Pflich\u00b7ten", "los", "von", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PRF", "APPR", "NN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dft, was du einmal entrichtet, stets entrichten.", "tokens": ["Du", "mu\u00dft", ",", "was", "du", "ein\u00b7mal", "ent\u00b7rich\u00b7tet", ",", "stets", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wers einmal gut gemacht hat f\u00fcrder keine Wahl,", "tokens": ["Wers", "ein\u00b7mal", "gut", "ge\u00b7macht", "hat", "f\u00fcr\u00b7der", "kei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df er besser noch es mach' ein andermal.", "tokens": ["Als", "da\u00df", "er", "bes\u00b7ser", "noch", "es", "mach'", "ein", "an\u00b7der\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ADV", "PPER", "VVFIN", "ART", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}