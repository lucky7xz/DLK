{"textgrid.poem.36177": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbheil dem Mann, der dem Herrn vertraut, der spricht zu dem Herrn, Herrn:", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbheil dem Mann, der dem Herrn vertraut, der spricht zu dem Herrn, Herrn:", "tokens": ["\u00bb", "heil", "dem", "Mann", ",", "der", "dem", "Herrn", "ver\u00b7traut", ",", "der", "spricht", "zu", "dem", "Herrn", ",", "Herrn", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Herr, mein Fels, meine Burg, Herr, mein Erretter, mein Hort,", "tokens": ["Herr", ",", "mein", "Fels", ",", "mei\u00b7ne", "Burg", ",", "Herr", ",", "mein", "Er\u00b7ret\u00b7ter", ",", "mein", "Hort", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schirmender Schild in der Schlacht, Horn unantastbarer St\u00e4rke,", "tokens": ["Schir\u00b7men\u00b7der", "Schild", "in", "der", "Schlacht", ",", "Horn", "un\u00b7an\u00b7tast\u00b7ba\u00b7rer", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,", "NE", "ADJA", "NN", "$,"], "meter": "+--+--++--+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Auf Dich hofft' ich, und nie hast Du den Hoffer get\u00e4uscht!", "tokens": ["Auf", "Dich", "hofft'", "ich", ",", "und", "nie", "hast", "Du", "den", "Hof\u00b7fer", "ge\u00b7t\u00e4uscht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Heil ", "tokens": ["Heil"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ihm gl\u00e4nzt tr\u00f6stend die Sonn', ob auch der Himmel sich schw\u00e4rzt.", "tokens": ["Ihm", "gl\u00e4nzt", "tr\u00f6s\u00b7tend", "die", "Sonn'", ",", "ob", "auch", "der", "Him\u00b7mel", "sich", "schw\u00e4rzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Nicht schreckt ihn das Grauen der Nacht. Nicht trifft ihn des Mittags", "tokens": ["Nicht", "schreckt", "ihn", "das", "Grau\u00b7en", "der", "Nacht", ".", "Nicht", "trifft", "ihn", "des", "Mit\u00b7tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "PTKNEG", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "T\u00f6dtlicher Pfeil. Ihn umgarnt nimmer das lauernde Netz.", "tokens": ["T\u00f6dt\u00b7li\u00b7cher", "Pfeil", ".", "Ihn", "um\u00b7garnt", "nim\u00b7mer", "das", "lau\u00b7ern\u00b7de", "Netz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVPP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Tausend st\u00fcrzen zur Rechten dem Sicheren, tausend zur Linken;", "tokens": ["Tau\u00b7send", "st\u00fcr\u00b7zen", "zur", "Rech\u00b7ten", "dem", "Si\u00b7che\u00b7ren", ",", "tau\u00b7send", "zur", "Lin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Sein nur schont das Gescho\u00df, welches die Tausend erlegt.", "tokens": ["Sein", "nur", "schont", "das", "Ge\u00b7scho\u00df", ",", "wel\u00b7ches", "die", "Tau\u00b7send", "er\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Mein begehrt er und stracks helf' ich ihm aus. Aus den Aengsten", "tokens": ["Mein", "be\u00b7gehrt", "er", "und", "stracks", "hel\u00b7f'", "ich", "ihm", "aus", ".", "Aus", "den", "A\u00b7engs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "PPER", "KON", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Rei\u00df' ich ihn eilig heraus, weil er mit Namen mich nennt.", "tokens": ["Rei\u00df'", "ich", "ihn", "ei\u00b7lig", "he\u00b7raus", ",", "weil", "er", "mit", "Na\u00b7men", "mich", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Rufe mich an in der Noth, und ich will dich erretten; Ich will dich", "tokens": ["Ru\u00b7fe", "mich", "an", "in", "der", "Noth", ",", "und", "ich", "will", "dich", "er\u00b7ret\u00b7ten", ";", "Ich", "will", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$,", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$.", "PPER", "VMFIN", "PRF"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Setzen zu Ehren, ich will hold und gew\u00e4rtig dir seyn.", "tokens": ["Set\u00b7zen", "zu", "Eh\u00b7ren", ",", "ich", "will", "hold", "und", "ge\u00b7w\u00e4r\u00b7tig", "dir", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "PPER", "VAINF", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "S\u00e4ttigen will ich dein Herz mit langem Leben. Ein Beispiel", "tokens": ["S\u00e4t\u00b7ti\u00b7gen", "will", "ich", "dein", "Herz", "mit", "lan\u00b7gem", "Le\u00b7ben", ".", "Ein", "Bei\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Sollst du den Sterblichen seyn seltenen dauernden Heils.\u00ab", "tokens": ["Sollst", "du", "den", "Sterb\u00b7li\u00b7chen", "seyn", "sel\u00b7te\u00b7nen", "dau\u00b7ern\u00b7den", "Heils", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPOSAT", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "W\u00fcrdiger Greis, was so der Prophet hochahnend gesungen,", "tokens": ["W\u00fcr\u00b7di\u00b7ger", "Greis", ",", "was", "so", "der", "Pro\u00b7phet", "hoc\u00b7hah\u00b7nend", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Was er geschauet im Geist, hast Du im Leben erprobt.", "tokens": ["Was", "er", "ge\u00b7schau\u00b7et", "im", "Geist", ",", "hast", "Du", "im", "Le\u00b7ben", "er\u00b7probt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Seltenes ward dir gew\u00e4hrt vom Herrn. Mit dem Manna von oben", "tokens": ["Sel\u00b7te\u00b7nes", "ward", "dir", "ge\u00b7w\u00e4hrt", "vom", "Herrn", ".", "Mit", "dem", "Man\u00b7na", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "APPRART", "NN", "$.", "APPR", "ART", "NN", "APPR", "ADV"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "St\u00e4rkt' er dich, tr\u00e4nkete dich aus dem verj\u00fcngenden Quell.", "tokens": ["St\u00e4rkt'", "er", "dich", ",", "tr\u00e4n\u00b7ke\u00b7te", "dich", "aus", "dem", "ver\u00b7j\u00fcn\u00b7gen\u00b7den", "Quell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Siebenzig wurden beschieden dem Sterblichen, achtzig aufs h\u00f6chste;", "tokens": ["Sie\u00b7ben\u00b7zig", "wur\u00b7den", "be\u00b7schie\u00b7den", "dem", "Sterb\u00b7li\u00b7chen", ",", "acht\u00b7zig", "aufs", "h\u00f6chs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ART", "NN", "$,", "CARD", "APPRART", "ADJA", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ueber die achtzig hinaus ward Dir gemessen das Ma\u00df.", "tokens": ["Ue\u00b7ber", "die", "acht\u00b7zig", "hin\u00b7aus", "ward", "Dir", "ge\u00b7mes\u00b7sen", "das", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "APZR", "VAFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Dreimal sahst du erneuert um Dich die Geschlechter der Menschen.", "tokens": ["Drei\u00b7mal", "sahst", "du", "er\u00b7neu\u00b7ert", "um", "Dich", "die", "Ge\u00b7schlech\u00b7ter", "der", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Welche zugleich mit Dir einstens betraten die Bahn,", "tokens": ["Wel\u00b7che", "zu\u00b7gleich", "mit", "Dir", "eins\u00b7tens", "be\u00b7tra\u00b7ten", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "L\u00e4ngst schon schlafen die Meisten den eisernen Schlaf in der Tiefe.", "tokens": ["L\u00e4ngst", "schon", "schla\u00b7fen", "die", "Meis\u00b7ten", "den", "ei\u00b7ser\u00b7nen", "Schlaf", "in", "der", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Schon auch ruhn, die mit Dir einerlei Mutter geherzt.", "tokens": ["Schon", "auch", "ruhn", ",", "die", "mit", "Dir", "ei\u00b7ner\u00b7lei", "Mut\u00b7ter", "ge\u00b7herzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "PRELS", "APPR", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Du nur schreitest einher mit Kraft, ein unsterblicher J\u00fcngling;", "tokens": ["Du", "nur", "schrei\u00b7test", "ein\u00b7her", "mit", "Kraft", ",", "ein", "uns\u00b7terb\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Nicht ward bl\u00f6der der Blick; z\u00f6gernder ward nicht der Gang.", "tokens": ["Nicht", "ward", "bl\u00f6\u00b7der", "der", "Blick", ";", "z\u00f6\u00b7gern\u00b7der", "ward", "nicht", "der", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ADJA", "ART", "NN", "$.", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "--+--+----+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "R\u00fcstig f\u00f6rderst du Tag vor Tag das beschiedene Tagwerk,", "tokens": ["R\u00fcs\u00b7tig", "f\u00f6r\u00b7derst", "du", "Tag", "vor", "Tag", "das", "be\u00b7schie\u00b7de\u00b7ne", "Tag\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Wegerst der Lasten Dich nicht, welche wol J\u00fcngere scheu'n,", "tokens": ["We\u00b7gerst", "der", "Las\u00b7ten", "Dich", "nicht", ",", "wel\u00b7che", "wol", "J\u00fcn\u00b7ge\u00b7re", "scheu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Schau'st mit Ruhe zur\u00fcck in die l\u00e4ngst versunkene Vorzeit,", "tokens": ["Schau'st", "mit", "Ru\u00b7he", "zu\u00b7r\u00fcck", "in", "die", "l\u00e4ngst", "ver\u00b7sun\u00b7ke\u00b7ne", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Blickst gleichm\u00fcthig hinaus in das Verh\u00e4ngni\u00df, was kommt.", "tokens": ["Blickst", "gleich\u00b7m\u00fct\u00b7hig", "hin\u00b7aus", "in", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "was", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,", "PWS", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Freue dich, w\u00fcrdiger Ohm, des Tags, den Gott Dir beschieden!", "tokens": ["Freu\u00b7e", "dich", ",", "w\u00fcr\u00b7di\u00b7ger", "Ohm", ",", "des", "Tags", ",", "den", "Gott", "Dir", "be\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Viel zwar s\u00e4hen ihn gern; aber sie sehen ihn nicht,", "tokens": ["Viel", "zwar", "s\u00e4\u00b7hen", "ihn", "gern", ";", "a\u00b7ber", "sie", "se\u00b7hen", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jenen belohnenden Tag, der zwanzigtausend der \u00e4ltern", "tokens": ["Je\u00b7nen", "be\u00b7loh\u00b7nen\u00b7den", "Tag", ",", "der", "zwan\u00b7zig\u00b7tau\u00b7send", "der", "\u00e4l\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "CARD", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bl\u00fchenden Br\u00fcder begrub, kr\u00e4ftiglich bl\u00fchend, wie sie.", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Br\u00fc\u00b7der", "be\u00b7grub", ",", "kr\u00e4f\u00b7tig\u00b7lich", "bl\u00fc\u00b7hend", ",", "wie", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,", "PWAV", "PPER", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Schau' auf, w\u00fcrdiger Greis, mit Dank und Vertrau'n zu dem Herrn Herrn,", "tokens": ["Schau'", "auf", ",", "w\u00fcr\u00b7di\u00b7ger", "Greis", ",", "mit"