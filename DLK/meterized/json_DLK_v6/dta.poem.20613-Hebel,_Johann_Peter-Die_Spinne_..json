{"dta.poem.20613": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Spinne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.57", "sv:0.28", "it:0.14"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Nei lueget doch das Spinnli a,               ", "tokens": ["Nei", "lue\u00b7get", "doch", "das", "Spinn\u00b7li", "a", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie \u2019s zarti F\u00e4de zwirne cha!", "tokens": ["wie", "'s", "zar\u00b7ti", "F\u00e4\u00b7de", "zwir\u00b7ne", "cha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bas Gvatter meinsch, chasch\u2019s au ne so?", "tokens": ["Bas", "Gvat\u00b7ter", "meinsch", ",", "chasch's", "au", "ne", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,", "NE", "NE", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De wirsch mers, traui, blibe lo.", "tokens": ["De", "wirsch", "mers", ",", "trau\u00b7i", ",", "bli\u00b7be", "lo", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VVFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es machts so subtil und so nett,", "tokens": ["Es", "machts", "so", "sub\u00b7til", "und", "so", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "i wott nit, a\u00dfi \u2019s z\u2019 hasple h\u00e4tt.", "tokens": ["i", "wott", "nit", ",", "a\u00b7\u00dfi", "'s", "z'", "hasp\u00b7le", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo het\u2019s die fini Riste g\u2019no,", "tokens": ["Wo", "het's", "die", "fi\u00b7ni", "Ris\u00b7te", "g'\u00b7no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "FM", "FM", "FM", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "by wellem Meister hechle lo?", "tokens": ["by", "wel\u00b7lem", "Meis\u00b7ter", "hech\u00b7le", "lo", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meinsch, wemme \u2019s w\u00fc\u00dft, e mengi Frau,", "tokens": ["Meinsch", ",", "wem\u00b7me", "'s", "w\u00fc\u00dft", ",", "e", "men\u00b7gi", "Frau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "PPER", "VVFIN", "$,", "FM", "FM", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "sie w\u00e4r so gscheit, und holti au!", "tokens": ["sie", "w\u00e4r", "so", "gscheit", ",", "und", "hol\u00b7ti", "au", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jez lueg mer, wie \u2019s si F\u00fce\u00dfli sezt,", "tokens": ["Jez", "lu\u00b7eg", "mer", ",", "wie", "'s", "si", "F\u00fce\u00df\u00b7li", "sezt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "PWAV", "PPER", "FM", "FM", "FM", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "und spinne will, und d\u2019 Finger nezt.", "tokens": ["und", "spin\u00b7ne", "will", ",", "und", "d'", "Fin\u00b7ger", "nezt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "$,", "KON", "NE", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es zieht e lange Faden us,", "tokens": ["Es", "zieht", "e", "lan\u00b7ge", "Fa\u00b7den", "us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es spinnt e Bruck ans Nochbers Hus,", "tokens": ["es", "spinnt", "e", "Bruck", "ans", "Noch\u00b7bers", "Hus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es baut e Land-Stro\u00df in der Luft,", "tokens": ["es", "baut", "e", "Lan\u00b7dSt\u00b7ro\u00df", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "morn hangt sie scho voll Morgeduft,", "tokens": ["morn", "hangt", "sie", "scho", "voll", "Mor\u00b7ge\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es baut e Fu\u00dfweg nebe dra,", "tokens": ["es", "baut", "e", "Fu\u00df\u00b7weg", "ne\u00b7be", "dra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "\u2019s isch, a\u00df es ehne dure cha.", "tokens": ["'s", "isch", ",", "a\u00df", "es", "eh\u00b7ne", "du\u00b7re", "cha", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Es spinnt und wandlet uf und ab,", "tokens": ["Es", "spinnt", "und", "wand\u00b7let", "uf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Potz tausig, im Gallop und Trap! \u2014", "tokens": ["Potz", "tau\u00b7sig", ",", "im", "Gal\u00b7lop", "und", "Trap", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "$,", "APPRART", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Jez gohts ring um, was hesch, was gisch!", "tokens": ["Jez", "gohts", "ring", "um", ",", "was", "hesch", ",", "was", "gisch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Siehsch, wie ne Ringli worden isch!", "tokens": ["Sieh\u00b7sch", ",", "wie", "ne", "Ring\u00b7li", "wor\u00b7den", "isch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "VAPP", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Jez schie\u00dft\u2019s die zarte F\u00e4den i.", "tokens": ["Jez", "schie\u00dft's", "die", "zar\u00b7te", "F\u00e4\u00b7den", "i."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirds \u00f6bbe solle gwobe sy?", "tokens": ["Wirds", "\u00f6b\u00b7be", "sol\u00b7le", "gwo\u00b7be", "sy", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es isch verstuunt, es haltet still,", "tokens": ["Es", "isch", "ver\u00b7stu\u00b7unt", ",", "es", "hal\u00b7tet", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "es wei\u00df nit recht, wo \u2019s ane will.", "tokens": ["es", "wei\u00df", "nit", "recht", ",", "wo", "'s", "a\u00b7ne", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PWAV", "PPER", "NE", "VMFIN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "\u2019s goht weger z\u2019ruck, i sieh\u2019s em a;", "tokens": ["'s", "goht", "we\u00b7ger", "z'\u00b7ruck", ",", "i", "sieh's", "em", "a", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2019s mu\u00df n\u00e4umis rechts vergesse ha.", "tokens": ["'s", "mu\u00df", "n\u00e4u\u00b7mis", "rechts", "ver\u00b7ges\u00b7se", "ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u201ezwor, denkt es, sel pressiert io nit,", "tokens": ["\u201e", "zwor", ",", "denkt", "es", ",", "sel", "pres\u00b7siert", "i\u00b7o", "nit", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "i halt mi nummen uf dermit.\u201c", "tokens": ["i", "halt", "mi", "num\u00b7men", "uf", "der\u00b7mit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es spinnt und webt, und het kei Rast,", "tokens": ["Es", "spinnt", "und", "webt", ",", "und", "het", "kei", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so gliichlig, me verluegt si fast", "tokens": ["so", "gliich\u00b7lig", ",", "me", "ver\u00b7lu\u00b7egt", "si", "fast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und \u2019s Pfarers Christoph het no gseit,", "tokens": ["Und", "'s", "Pfa\u00b7rers", "Chris\u00b7toph", "het", "no", "gseit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "NE", "VAFIN", "NE", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "\u2019s seig iede Fade zseme gleit.", "tokens": ["'s", "seig", "ie\u00b7de", "Fa\u00b7de", "zse\u00b7me", "gleit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Es mue\u00df ein guti Auge ha,", "tokens": ["Es", "mu\u00b7e\u00df", "ein", "gu\u00b7ti", "Au\u00b7ge", "ha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wers zehlen und erchenne cha.", "tokens": ["wers", "zeh\u00b7len", "und", "er\u00b7chen\u00b7ne", "cha", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Jez puzt es sini H\u00e4ndli ab,", "tokens": ["Jez", "puzt", "es", "si\u00b7ni", "H\u00e4nd\u00b7li", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "FM", "FM", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es stoht, und haut der Faden ab.", "tokens": ["es", "stoht", ",", "und", "haut", "der", "Fa\u00b7den", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jez sizt es in si Summer-Hus,", "tokens": ["Jez", "sizt", "es", "in", "si", "Sum\u00b7mer\u00b7Hus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und luegt die lange Stro\u00dfen us.", "tokens": ["und", "luegt", "die", "lan\u00b7ge", "Stro\u00b7\u00dfen", "us", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es seit: \u201eMe baut si halber z\u2019 tod,", "tokens": ["Es", "seit", ":", "\u201e", "Me", "baut", "si", "hal\u00b7ber", "z'", "tod", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "doch freuts ein au, wenns H\u00fcsli stoht.\u201c", "tokens": ["doch", "freuts", "ein", "au", ",", "wenns", "H\u00fcs\u00b7li", "stoht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "$,", "KOUS", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "In freie L\u00fcste wogt und schwankts,", "tokens": ["In", "frei\u00b7e", "L\u00fcs\u00b7te", "wogt", "und", "schwankts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und an der liebe Sunne hangts;", "tokens": ["und", "an", "der", "lie\u00b7be", "Sun\u00b7ne", "hangts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie schint em frey dur d\u2019 Beinli dur,", "tokens": ["sie", "schint", "em", "frey", "dur", "d'", "Bein\u00b7li", "dur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und \u2019s isch em wohl. In Feld und Flur", "tokens": ["und", "'s", "isch", "em", "wohl", ".", "In", "Feld", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "ADJA", "ADV", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "sieht \u2019s M\u00fcckli tanze, iung und fei\u00df;", "tokens": ["sieht", "'s", "M\u00fcck\u00b7li", "tan\u00b7ze", ",", "i\u00b7ung", "und", "fei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "VVFIN", "$,", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "\u2019s denkt by nem selber: \u201eH\u00e4tti eis!\u201c", "tokens": ["'s", "denkt", "by", "nem", "sel\u00b7ber", ":", "\u201e", "H\u00e4t\u00b7ti", "eis", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "ADV", "$.", "$(", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "O Thierli, wie hesch mi vertz\u00fcckt!", "tokens": ["O", "Thier\u00b7li", ",", "wie", "hesch", "mi", "vert\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "ADJD", "NE", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie bisch so chlei, und doch so gschickt!", "tokens": ["Wie", "bisch", "so", "chlei", ",", "und", "doch", "so", "gsch